{"dta.poem.16065": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "119.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Abh\u00e4ngig von der Welt mu\u00dft du dich nicht betrachten,", "tokens": ["Ab\u00b7h\u00e4n\u00b7gig", "von", "der", "Welt", "mu\u00dft", "du", "dich", "nicht", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Doch auch nicht gegen deins das Recht der Welt verachten.", "tokens": ["Doch", "auch", "nicht", "ge\u00b7gen", "deins", "das", "Recht", "der", "Welt", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "NE", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht du lebst und die Welt ist todt, nicht lebt die Welt", "tokens": ["Nicht", "du", "lebst", "und", "die", "Welt", "ist", "todt", ",", "nicht", "lebt", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "VVFIN", "KON", "ART", "NN", "VAFIN", "ADJD", "$,", "PTKNEG", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und du bist todt; ihr seid zwei Leben gleichgestellt.", "tokens": ["Und", "du", "bist", "todt", ";", "ihr", "seid", "zwei", "Le\u00b7ben", "gleich\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Magst du dich nun als Mann, sie sich als Weib verhalten;", "tokens": ["Magst", "du", "dich", "nun", "als", "Mann", ",", "sie", "sich", "als", "Weib", "ver\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "KOUS", "NN", "$,", "PPER", "PRF", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag weiblich dein Gem\u00fcth, der Weltgeist m\u00e4nnlich walten:", "tokens": ["Mag", "weib\u00b7lich", "dein", "Ge\u00b7m\u00fcth", ",", "der", "Welt\u00b7geist", "m\u00e4nn\u00b7lich", "wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es sei nun, da\u00df in dir die Welt sich eingebar,", "tokens": ["Es", "sei", "nun", ",", "da\u00df", "in", "dir", "die", "Welt", "sich", "ein\u00b7ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sei, da\u00df du in ihr dich selber stellest dar;", "tokens": ["Es", "sei", ",", "da\u00df", "du", "in", "ihr", "dich", "sel\u00b7ber", "stel\u00b7lest", "dar", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "PRF", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So wirst du hier als Mann ins Weltgetriebe greifen,", "tokens": ["So", "wirst", "du", "hier", "als", "Mann", "ins", "Welt\u00b7ge\u00b7trie\u00b7be", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOUS", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dort in stiller Brust der Welt Geheimnis reifen.", "tokens": ["Und", "dort", "in", "stil\u00b7ler", "Brust", "der", "Welt", "Ge\u00b7heim\u00b7nis", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum soll einander Held und Dichter nicht beneiden,", "tokens": ["Drum", "soll", "ein\u00b7an\u00b7der", "Held", "und", "Dich\u00b7ter", "nicht", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PRF", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nur verschieden ist die Welt verkl\u00e4rt in Beiden.", "tokens": ["Denn", "nur", "ver\u00b7schie\u00b7den", "ist", "die", "Welt", "ver\u00b7kl\u00e4rt", "in", "Bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}