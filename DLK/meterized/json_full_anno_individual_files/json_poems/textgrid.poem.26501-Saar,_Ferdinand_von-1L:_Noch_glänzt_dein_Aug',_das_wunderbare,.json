{"textgrid.poem.26501": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch gl\u00e4nzt dein Aug', das wunderbare,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch gl\u00e4nzt dein Aug', das wunderbare,", "tokens": ["Noch", "gl\u00e4nzt", "dein", "Aug'", ",", "das", "wun\u00b7der\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und deine Stirn ist licht und frei \u2013", "tokens": ["Und", "dei\u00b7ne", "Stirn", "ist", "licht", "und", "frei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ach, es zieh'n, es zieh'n die Jahre", "tokens": ["Doch", "ach", ",", "es", "zieh'n", ",", "es", "zieh'n", "die", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An dir auch spurlos nicht vorbei.", "tokens": ["An", "dir", "auch", "spur\u00b7los", "nicht", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Oft ist's, als w\u00e4re schon verschimmert", "tokens": ["Oft", "ist's", ",", "als", "w\u00e4\u00b7re", "schon", "ver\u00b7schim\u00b7mert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schmelz der Wangen, zart und hold,", "tokens": ["Der", "Schmelz", "der", "Wan\u00b7gen", ",", "zart", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ganz so reich, wie einstens, flimmert", "tokens": ["Und", "ganz", "so", "reich", ",", "wie", "eins\u00b7tens", ",", "flim\u00b7mert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "PWAV", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr der Haare dunkles Gold.", "tokens": ["Nicht", "mehr", "der", "Haa\u00b7re", "dunk\u00b7les", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon blickst du sinnend vor dich nieder,", "tokens": ["Schon", "blickst", "du", "sin\u00b7nend", "vor", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mund umzuckt von leisem Weh';", "tokens": ["Den", "Mund", "um\u00b7zuckt", "von", "lei\u00b7sem", "Weh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwerm\u00fcthig haucht's um deine Glieder", "tokens": ["Schwer\u00b7m\u00fct\u00b7hig", "haucht's", "um", "dei\u00b7ne", "Glie\u00b