{"textgrid.poem.55933": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verzauberte: wie kann der Einklang zweier", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzauberte: wie kann der Einklang zweier", "tokens": ["Ver\u00b7zau\u00b7ber\u00b7te", ":", "wie", "kann", "der", "Ein\u00b7klang", "zwei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "PWAV", "VMFIN", "ART", "NN", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "erw\u00e4hlter Worte je den Reim erreichen,", "tokens": ["er\u00b7w\u00e4hl\u00b7ter", "Wor\u00b7te", "je", "den", "Reim", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der in dir kommt und geht, wie auf ein Zeichen.", "tokens": ["der", "in", "dir", "kommt", "und", "geht", ",", "wie", "auf", "ein", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus deiner Stirne steigen Laub und Leier,", "tokens": ["Aus", "dei\u00b7ner", "Stir\u00b7ne", "stei\u00b7gen", "Laub", "und", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und alles Deine geht schon im Vergleich", "tokens": ["und", "al\u00b7les", "Dei\u00b7ne", "geht", "schon", "im", "Ver\u00b7