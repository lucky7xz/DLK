{"textgrid.poem.55289": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Juni", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinter jenem Berge wohnt", "tokens": ["Hin\u00b7ter", "je\u00b7nem", "Ber\u00b7ge", "wohnt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie, die meine Liebe lohnt.", "tokens": ["Sie", ",", "die", "mei\u00b7ne", "Lie\u00b7be", "lohnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage, Berg, was ist denn das?", "tokens": ["Sa\u00b7ge", ",", "Berg", ",", "was", "ist", "denn", "das", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "VAFIN", "ADV", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mir doch, als w\u00e4rst du Glas", "tokens": ["Ist", "mir", "doch", ",", "als", "w\u00e4rst", "du", "Glas"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich w\u00e4r nicht weit davon;", "tokens": ["Und", "ich", "w\u00e4r", "nicht", "weit", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sie kommt, ich seh es schon,", "tokens": ["Denn", "sie", "kommt", ",", "ich", "seh", "es", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig, denn ich bin nicht da,", "tokens": ["Trau\u00b7rig", ",", "denn", "ich", "bin", "nicht", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "PPER", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd, ja, sie wei\u00df es ja!", "tokens": ["L\u00e4\u00b7chelnd", ",", "ja", ",", "sie", "wei\u00df", "es", "ja", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun stellt sich dazwischen", "tokens": ["Nun", "stellt", "sich", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PAV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein k\u00fchles Tal mit leichten B\u00fcschen,", "tokens": ["Ein", "k\u00fch\u00b7les", "Tal", "mit", "leich\u00b7ten", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "B\u00e4chen, Wiesen und dergleichen,", "tokens": ["B\u00e4\u00b7chen", ",", "Wie\u00b7sen", "und", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fchlen und R\u00e4ndern, den sch\u00f6nsten Zeichen,", "tokens": ["M\u00fch\u00b7len", "und", "R\u00e4n\u00b7dern", ",", "den", "sch\u00f6ns\u00b7ten", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Da\u00df da gleich wird eine Fl\u00e4che kommen,", "tokens": ["Da\u00df", "da", "gleich", "wird", "ei\u00b7ne", "Fl\u00e4\u00b7che", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Weite Felder unbeklommen.", "tokens": ["Wei\u00b7te", "Fel\u00b7der", "un\u00b7be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so immer, immer heraus,", "tokens": ["Und", "so", "im\u00b7mer", ",", "im\u00b7mer", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Bis mir an Garten und Haus!", "tokens": ["Bis", "mir", "an", "Gar\u00b7ten", "und", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Aber wie geschicht's?", "tokens": ["A\u00b7ber", "wie", "ge\u00b7schicht's", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Freut mich das alles nicht \u2013", "tokens": ["Freut", "mich", "das", "al\u00b7les", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PIS", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Freute mich des Gesichts", "tokens": ["Freu\u00b7te", "mich", "des", "Ge\u00b7sichts"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und der zwei \u00c4uglein Glanz,", "tokens": ["Und", "der", "zwei", "\u00c4ug\u00b7lein", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Freute mich des leichten Gangs,", "tokens": ["Freu\u00b7te", "mich", "des", "leich\u00b7ten", "Gangs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wie ich sie seh", "tokens": ["Und", "wie", "ich", "sie", "seh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Vom Zopf zur Zeh!", "tokens": ["Vom", "Zopf", "zur", "Zeh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sie ist fort, ich bin hier,", "tokens": ["Sie", "ist", "fort", ",", "ich", "bin", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin weg, bin bei ihr.", "tokens": ["Ich", "bin", "weg", ",", "bin", "bei", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "$,", "VAFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wandelt sie auf schroffen H\u00fcgeln,", "tokens": ["Wan\u00b7delt", "sie", "auf", "schrof\u00b7fen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Eilet sie das Tal entlang,", "tokens": ["Ei\u00b7let", "sie", "das", "Tal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da erklingt es wie mit Fl\u00fcgeln,", "tokens": ["Da", "er\u00b7klingt", "es", "wie", "mit", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da bewegt sich's wie Gesang.", "tokens": ["Da", "be\u00b7wegt", "sich's", "wie", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und auf diese Jugendf\u00fclle,", "tokens": ["Und", "auf", "die\u00b7se", "Ju\u00b7gend\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Glieder frohe Pracht", "tokens": ["Die\u00b7ser", "Glie\u00b7der", "fro\u00b7he", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harret einer in der Stille,", "tokens": ["Har\u00b7ret", "ei\u00b7ner", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den sie einzig gl\u00fccklich macht.", "tokens": ["Den", "sie", "ein\u00b7zig", "gl\u00fcck\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebe steht ihr gar zu sch\u00f6n,", "tokens": ["Lie\u00b7be", "steht", "ihr", "gar", "zu", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nres hab ich nie gesehn!", "tokens": ["Sch\u00f6n\u00b7res", "hab", "ich", "nie", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bricht ihr doch ein Blumenflor", "tokens": ["Bricht", "ihr", "doch", "ein", "Blu\u00b7men\u00b7flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Herzen leicht hervor.", "tokens": ["Aus", "dem", "Her\u00b7zen", "leicht", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Denk ich: Soll es doch so sein!", "tokens": ["Denk", "ich", ":", "Soll", "es", "doch", "so", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "VMFIN", "PPER", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das erquickt mir Mark und Bein;", "tokens": ["Das", "er\u00b7quickt", "mir", "Mark", "und", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hn ich wohl, wenn sie mich liebt,", "tokens": ["W\u00e4hn", "ich", "wohl", ",", "wenn", "sie", "mich", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es noch was Be\u00dfres gibt?", "tokens": ["Da\u00df", "es", "noch", "was", "Be\u00df\u00b7res", "gibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PWS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und noch sch\u00f6ner ist die Braut,", "tokens": ["Und", "noch", "sch\u00f6\u00b7ner", "ist", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie sich mir ganz vertraut,", "tokens": ["Wenn", "sie", "sich", "mir", "ganz", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie spricht und mir erz\u00e4hlt,", "tokens": ["Wenn", "sie", "spricht", "und", "mir", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sie freut und was sie qu\u00e4lt.", "tokens": ["Was", "sie", "freut", "und", "was", "sie", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie's ihr ist und wie's ihr war,", "tokens": ["Wie's", "ihr", "ist", "und", "wie's", "ihr", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "KON", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kenn ich sie doch ganz und gar.", "tokens": ["Kenn", "ich", "sie", "doch", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer gew\u00e4nn an Seel und Leib", "tokens": ["Wer", "ge\u00b7w\u00e4nn", "an", "Seel", "und", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solch ein Kind und solch ein Weib!", "tokens": ["Solch", "ein", "Kind", "und", "solch", "ein", "Weib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "KON", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Hinter jenem Berge wohnt", "tokens": ["Hin\u00b7ter", "je\u00b7nem", "Ber\u00b7ge", "wohnt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie, die meine Liebe lohnt.", "tokens": ["Sie", ",", "die", "mei\u00b7ne", "Lie\u00b7be", "lohnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage, Berg, was ist denn das?", "tokens": ["Sa\u00b7ge", ",", "Berg", ",", "was", "ist", "denn", "das", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "VAFIN", "ADV", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mir doch, als w\u00e4rst du Glas", "tokens": ["Ist", "mir", "doch", ",", "als", "w\u00e4rst", "du", "Glas"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und ich w\u00e4r nicht weit davon;", "tokens": ["Und", "ich", "w\u00e4r", "nicht", "weit", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sie kommt, ich seh es schon,", "tokens": ["Denn", "sie", "kommt", ",", "ich", "seh", "es", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig, denn ich bin nicht da,", "tokens": ["Trau\u00b7rig", ",", "denn", "ich", "bin", "nicht", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "PPER", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd, ja, sie wei\u00df es ja!", "tokens": ["L\u00e4\u00b7chelnd", ",", "ja", ",", "sie", "wei\u00df", "es", "ja", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", 