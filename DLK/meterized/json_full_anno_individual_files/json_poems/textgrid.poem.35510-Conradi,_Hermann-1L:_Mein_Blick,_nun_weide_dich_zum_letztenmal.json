{"textgrid.poem.35510": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Blick, nun weide dich zum letztenmal", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Blick, nun weide dich zum letztenmal", "tokens": ["Mein", "Blick", ",", "nun", "wei\u00b7de", "dich", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An dieses Fr\u00fchlings satter Bl\u00fctenf\u00fclle!", "tokens": ["An", "die\u00b7ses", "Fr\u00fch\u00b7lings", "sat\u00b7ter", "Bl\u00fc\u00b7ten\u00b7f\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Voll Inbrunst sauge dieser Sonne Strahl \u2013", "tokens": ["Voll", "In\u00b7brunst", "sau\u00b7ge", "die\u00b7ser", "Son\u00b7ne", "Strahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz, sei stille! ...", "tokens": ["Mein", "Herz", ",", "sei", "stil\u00b7le", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "VVFIN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Erschweig bewundernd vor dem Werdedrang!", "tokens": ["Er\u00b7schweig", "be\u00b7wun\u00b7dernd", "vor", "dem", "Wer\u00b7de\u00b7drang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was dich erf\u00fcllt, den Winden gib's zum Raube! ...", "tokens": ["Was", "dich", "er\u00b7f\u00fcllt", ",", "den", "Win\u00b7den", "gib's", "zum", "Rau\u00b7be", "!", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "ART", "NN", "NE", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob dir der Hoffnung goldnes Sieb zersprang \u2013", "tokens": ["Ob", "dir", "der", "Hoff\u00b7nung", "gold\u00b7nes", "Sieb", "zer\u00b7sprang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir blieb der Glaube!", "tokens": ["Dir", "blieb", "der", "Glau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O glaube eine winz'ge Weile nur,", "tokens": ["O", "glau\u00b7be", "ei\u00b7ne", "winz'\u00b7ge", "Wei\u00b7le", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df diese Botschaft auch f\u00fcr dich gebracht ward!", "tokens": ["Da\u00df", "die\u00b7se", "Bot\u00b7schaft", "auch", "f\u00fcr", "dich", "ge\u00b7bracht", "ward", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umfa\u00df noch einmal trunken die Natur,", "tokens": ["Um\u00b7fa\u00df", "noch", "ein\u00b7mal", "trun\u0