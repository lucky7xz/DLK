{"dta.poem.857": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.  Auf den Sontag de\u00df ernehrenden Ver-  \n sorgers/ oder  VII.  Sontag nach dem Fest der  \n H. Dreyeinigkeit/ Marc. 8.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "We&#241; gleich kein mittel wehr/ v&#241; aller Trost verschwinde: ", "tokens": ["We", "&#241;", "gleich", "kein", "mit\u00b7tel", "wehr", "/", "v", "&#241;", "al\u00b7ler", "Trost", "ver\u00b7schwin\u00b7de", ":"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "PIAT", "NN", "ADV", "$(", "XY", "$(", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd ich ohn h\u00fclff vnd trost nur vngepfl\u00fcgtes Land", "tokens": ["Vnd", "ich", "ohn", "h\u00fclff", "vnd", "trost", "nur", "vn\u00b7ge\u00b7pfl\u00fcg\u00b7tes", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd gar nicht fruchtbar Holtz/ vnd \u00f6der w\u00fcsten Sand", "tokens": ["Vnd", "gar", "nicht", "frucht\u00b7bar", "Holtz", "/", "vnd", "\u00f6\u00b7der", "w\u00fcs\u00b7ten", "Sand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "NN", "$(", "KON", "ADJD", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In h\u00f6chster hungers noth f\u00fcr meinen Augen f\u00fcnde:", "tokens": ["In", "h\u00f6chs\u00b7ter", "hun\u00b7gers", "noth", "f\u00fcr", "mei\u00b7nen", "Au\u00b7gen", "f\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So zag\u2019 ich dennoch nicht. Denn k\u00f6nt auch seinem Kinde", "tokens": ["So", "zag'", "ich", "den\u00b7noch", "nicht", ".", "Denn", "k\u00f6nt", "auch", "sei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der vor vier tausend Mann hier Brodt vnd Speise fand", "tokens": ["Der", "vor", "vier", "tau\u00b7send", "Mann", "hier", "Brodt", "vnd", "Spei\u00b7se", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "CARD", "CARD", "NN", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd \u00fcberbleiben lie\u00df: Verschlissen Hertz vnd Hand?", "tokens": ["Vnd", "\u00fc\u00b7berb\u00b7lei\u00b7ben", "lie\u00df", ":", "Ver\u00b7schlis\u00b7sen", "Hertz", "vnd", "Hand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "$.", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drumb ists vmbsonst/ da\u00df ich selbst mit sorgen binde!", "tokens": ["Drumb", "ists", "vmbsonst", "/", "da\u00df", "ich", "selbst", "mit", "sor\u00b7gen", "bin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "$(", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht ohn ists/ Ich bin arm/ vnd mit viel angst beschwert", "tokens": ["Nicht", "ohn", "ists", "/", "Ich", "bin", "arm", "/", "vnd", "mit", "viel", "angst", "be\u00b7schwert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NE", "$(", "PPER", "VAFIN", "ADJD", "$(", "KON", "APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wei\u00df ich: wer nur stets zu Gott die Sinnen kehrt", "tokens": ["Doch", "wei\u00df", "ich", ":", "wer", "nur", "stets", "zu", "Gott", "die", "Sin\u00b7nen", "kehrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "PWS", "ADV", "ADV", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den gantz kein S\u00fcnden Netz/ kein zweifelstrick kan fangen:", "tokens": ["Den", "gantz", "kein", "S\u00fcn\u00b7den", "Netz", "/", "kein", "zwei\u00b7fel\u00b7strick", "kan", "fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "NE", "$(", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der gutt\u2019s zu thnn sich m\u00fcht: der Christum fleissig h\u00f6rt", "tokens": ["Der", "gutt's", "zu", "thnn", "sich", "m\u00fcht", ":", "der", "Chris\u00b7tum", "fleis\u00b7sig", "h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PTKZU", "VVFIN", "PRF", "ADJD", "$.", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd Jhn mit fester trew\u2019 vnd reinem Leben ehrt/", "tokens": ["Vnd", "Jhn", "mit", "fes\u00b7ter", "tre\u00b7w'", "vnd", "rei\u00b7nem", "Le\u00b7ben", "ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wird/ was er darff vnd wil/ mit \u00fcberflu\u00df erlangen.", "tokens": ["Wird", "/", "was", "er", "darff", "vnd", "wil", "/", "mit", "\u00fc\u00b7berf\u00b7lu\u00df", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "PWS", "PPER", "VMFIN", "KON", "VMFIN", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}