{"textgrid.poem.41334": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie edel ist ein Herz, das reich an steter Liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie edel ist ein Herz, das reich an steter Liebe,", "tokens": ["Wie", "e\u00b7del", "ist", "ein", "Herz", ",", "das", "reich", "an", "ste\u00b7ter", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Wohlthun lebhaft ist, aus unerlerntem Triebe!", "tokens": ["Zum", "Wohl\u00b7thun", "leb\u00b7haft", "ist", ",", "aus", "un\u00b7er\u00b7lern\u00b7tem", "Trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VAFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wirkt ein lautrer Bach, der durch zwo Wiesen schleicht,", "tokens": ["So", "wirkt", "ein", "laut\u00b7rer", "Bach", ",", "der", "durch", "zwo", "Wie\u00b7sen", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht heftig schwillt, noch rauscht: dem nie die Kraft entweicht,", "tokens": ["Nicht", "hef\u00b7tig", "schwillt", ",", "noch", "rauscht", ":", "dem", "nie", "die", "Kraft", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "$.", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Ufer fruchtbar macht: an dem, bei jedem Lenzen,", "tokens": ["Die", "U\u00b7fer", "frucht\u00b7bar", "macht", ":", "an", "dem", ",", "bei", "je\u00b7dem", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "APPR", "ART", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Blumen, die er n\u00e4hrt, die Hirten sich umkr\u00e4nzen.", "tokens": ["Mit", "Blu\u00b7men", ",", "die", "er", "n\u00e4hrt", ",", "die", "Hir\u00b7ten", "sich", "um\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein kleines Herz voll Stolz, die Werkstatt schlauer Kunst,", "tokens": ["Ein", "klei\u00b7nes", "Herz", "voll", "Stolz", ",", "die", "Werk\u00b7statt", "schlau\u00b7er", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird tugendhaft und mild, aus Eigennutz der Gunst.", "tokens": ["Wird", "tu\u00b7gend\u00b7haft", "und", "mild", ",", "aus", "Ei\u00b7gen\u00b7nutz", "der", "Gunst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein F\u00fcrst, der, eh' er gibt, zehn Zweifel \u00fcberwindet,", "tokens": ["Ein", "F\u00fcrst", ",", "der", ",", "eh'", "er", "gibt", ",", "zehn", "Zwei\u00b7fel", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "VVFIN", "$,", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bis da\u00df sein Kanzler ihm den Ton zum Jawort findet,", "tokens": ["Bis", "da\u00df", "sein", "Kanz\u00b7ler", "ihm", "den", "Ton", "zum", "Ja\u00b7wort", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ahmt einem Springbrunn nach. Die Kunst macht die Natur", "tokens": ["Ahmt", "ei\u00b7nem", "Spring\u00b7brunn", "nach", ".", "Die", "Kunst", "macht", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verschwendrisch, wo sie kargt; jedoch zu Zeiten nur.", "tokens": ["Ver\u00b7schwend\u00b7risch", ",", "wo", "sie", "kargt", ";", "je\u00b7doch", "zu", "Zei\u00b7ten", "nur", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "VVFIN", "$.", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er wird, so wie ein Sturm, uns Wunderkr\u00e4fte zeigen.", "tokens": ["Er", "wird", ",", "so", "wie", "ein", "Sturm", ",", "uns", "Wun\u00b7der\u00b7kr\u00e4f\u00b7te", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "KOKOM", "ART", "NN", "$,", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Seht seinen starken Strahl bis an die Wolken steigen!", "tokens": ["Seht", "sei\u00b7nen", "star\u00b7ken", "Strahl", "bis", "an", "die", "Wol\u00b7ken", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als unersch\u00f6pflich eilt des Wassers Schatz empor,", "tokens": ["Als", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "eilt", "des", "Was\u00b7sers", "Schatz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und prangt in heller Luft: der Schall bet\u00e4ubt das Ohr:", "tokens": ["Und", "prangt", "in", "hel\u00b7ler", "Luft", ":", "der", "Schall", "be\u00b7t\u00e4ubt", "das", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Auge weidet sich an Farben und an Bildern:", "tokens": ["Das", "Au\u00b7ge", "wei\u00b7det", "sich", "an", "Far\u00b7ben", "und", "an", "Bil\u00b7dern", ":"], "token_info": ["wo