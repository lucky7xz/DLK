Und", "flehn", "euch", ",", "ihn", "ins", "Grab", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "\u2013 So schallt die Klage Tag und Nacht,", "tokens": ["\u2013", "So", "schallt", "die", "Kla\u00b7ge", "Tag", "und", "Nacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So grollt es Connaught durch und Leinster.", "tokens": ["So", "grollt", "es", "Con\u00b7naught", "durch", "und", "Leins\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "APPR", "KON", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Der West hat mir den Schrei gebracht \u2013", "tokens": ["Der", "West", "hat", "mir", "den", "Schrei", "ge\u00b7bracht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er trug ihn schrill bis vor mein Fenster.", "tokens": ["Er", "trug", "ihn", "schrill", "bis", "vor", "mein", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Matt, wie ein angescho\u00dfner Weih,", "tokens": ["Matt", ",", "wie", "ein", "an\u00b7ge\u00b7scho\u00df\u00b7ner", "Weih", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herschwebt' er \u00fcber H\u00f6hn und Sunde \u2013", "tokens": ["Her\u00b7schwebt'", "er", "\u00fc\u00b7ber", "H\u00f6hn", "und", "Sun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Schrei der Not, der Hungerschrei,", "tokens": ["Der", "Schrei", "der", "Not", ",", "der", "Hun\u00b7ger\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Sterbeschrei aus Erins Munde!", "tokens": ["Der", "Ster\u00b7be\u00b7schrei", "aus", "E\u00b7rins", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Erin \u2013 da liegt sie auf den Knien,", "tokens": ["E\u00b7rin", "\u2013", "da", "liegt", "sie", "auf", "den", "Kni\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bleich und entstellt, mit wehndem Haare,", "tokens": ["Bleich", "und", "ent\u00b7stellt", ",", "mit", "wehn\u00b7dem", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und streut des Shamrocks welkend Gr\u00fcn", "tokens": ["Und", "streut", "des", "Sham\u00b7rocks", "wel\u00b7kend", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zitternd auf ihrer Kinder Bahre.", "tokens": ["Zit\u00b7ternd", "auf", "ih\u00b7rer", "Kin\u00b7der", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sie kniet auf ihrer Berge Kronen \u2013", "tokens": ["Sie", "kniet", "auf", "ih\u00b7rer", "Ber\u00b7ge", "Kro\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mehr noch, als Harold-Byrons Rom,", "tokens": ["Mehr", "noch", ",", "als", "Ha\u00b7rold\u00b7By\u00b7rons", "Rom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbdie Niobe der Nationen!\u00ab", "tokens": ["\u00bb", "die", "Ni\u00b7o\u00b7be", "der", "Na\u00b7ti\u00b7o\u00b7nen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}