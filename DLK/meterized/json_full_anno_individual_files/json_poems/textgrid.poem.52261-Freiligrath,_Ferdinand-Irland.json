{"textgrid.poem.52261": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Irland", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An rost'ger Kette liegt das Boot;", "tokens": ["An", "rost'\u00b7ger", "Ket\u00b7te", "liegt", "das", "Boot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Segel tr\u00e4umt, das Ruder lungert.", "tokens": ["Das", "Se\u00b7gel", "tr\u00e4umt", ",", "das", "Ru\u00b7der", "lun\u00b7gert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das macht, der Fischerbub' ist tot;", "tokens": ["Das", "macht", ",", "der", "Fi\u00b7scher\u00b7bub'", "ist", "tot", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das macht, der Fischer ist verhungert!", "tokens": ["Das", "macht", ",", "der", "Fi\u00b7scher", "ist", "ver\u00b7hun\u00b7gert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Irlands Fisch ist Herrenfisch;", "tokens": ["Denn", "Ir\u00b7lands", "Fisch", "ist", "Her\u00b7ren\u00b7fisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Strandherr pra\u00dft vom reichen Fange,", "tokens": ["Der", "Strand\u00b7herr", "pra\u00dft", "vom", "rei\u00b7chen", "Fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Leer aber bleibt des F\u00e4ngers Tisch \u2013", "tokens": ["Leer", "a\u00b7ber", "bleibt", "des", "F\u00e4n\u00b7gers", "Tisch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So starb der Fischer, so sein Range.", "tokens": ["So", "starb", "der", "Fi\u00b7scher", ",", "so", "sein", "Ran\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Herde bl\u00f6kt, die Herde br\u00fcllt;", "tokens": ["Die", "Her\u00b7de", "bl\u00f6kt", ",", "die", "Her\u00b7de", "br\u00fcllt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch ein Gedr\u00e4ng' von K\u00fch'n und Schafen!", "tokens": ["Welch", "ein", "Ge\u00b7dr\u00e4ng'", "von", "K\u00fch'n", "und", "Scha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Hirt, von Lumpen schlecht verh\u00fcllt,", "tokens": ["Der", "Hirt", ",", "von", "Lum\u00b7pen", "schlecht", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Treibt sie ans Meer zum n\u00e4chsten Hafen.", "tokens": ["Treibt", "sie", "ans", "Meer", "zum", "n\u00e4chs\u00b7ten", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Irlands Vieh ist Herrenvieh:", "tokens": ["Denn", "Ir\u00b7lands", "Vieh", "ist", "Her\u00b7ren\u00b7vieh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das gerne Paddys Knochen st\u00e4rkte", "tokens": ["Das", "ger\u00b7ne", "Pad\u00b7dys", "Kno\u00b7chen", "st\u00e4rk\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und seiner Kinder brechen Knie \u2013", "tokens": ["Und", "sei\u00b7ner", "Kin\u00b7der", "bre\u00b7chen", "Knie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Grundherr schickt's auf fremde M\u00e4rkte.", "tokens": ["Der", "Grund\u00b7herr", "schickt's", "auf", "frem\u00b7de", "M\u00e4rk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum ist sein Viehstall ihm ein Born", "tokens": ["Drum", "ist", "sein", "Vieh\u00b7stall", "ihm", "ein", "Born"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der \u00dcppigkeit und des Genusses,", "tokens": ["Der", "\u00dcp\u00b7pig\u00b7keit", "und", "des", "Ge\u00b7nus\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeglich Kuh- und Bullenhorn", "tokens": ["Und", "jeg\u00b7lich", "Kuh", "und", "Bul\u00b7len\u00b7horn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ihm ein Horn des \u00dcberflusses.", "tokens": ["Wird", "ihm", "ein", "Horn", "des", "\u00dc\u00b7berf\u00b7lus\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er l\u00e4\u00dft zu London und Paris", "tokens": ["Er", "l\u00e4\u00dft", "zu", "Lon\u00b7don", "und", "Pa\u00b7ris"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Spieltisch unterm Gold sich biegen; \u2013", "tokens": ["Den", "Spiel\u00b7tisch", "un\u00b7term", "Gold", "sich", "bie\u00b7gen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Volk, das er zu Hause lie\u00df,", "tokens": ["Sein", "Volk", ",", "das", "er", "zu", "Hau\u00b7se", "lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00e4llt unterdes wie Winterfliegen.", "tokens": ["F\u00e4llt", "un\u00b7ter\u00b7des", "wie", "Win\u00b7ter\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hallo, Hallo! Gr\u00fcn-Erins Jagd!", "tokens": ["Hal\u00b7lo", ",", "Hal\u00b7lo", "!", "Gr\u00fcn\u00b7\u00b7E\u00b7rins", "Jagd", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "NN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Paddy, lang' zu! das nenn' ich Ziemer!", "tokens": ["Pad\u00b7dy", ",", "lang'", "zu", "!", "das", "nenn'", "ich", "Zie\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKVZ", "$.", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst auch das wird fortgebracht,", "tokens": ["Um\u00b7sonst", "auch", "das", "wird", "fort\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meer\u00fcber mit dem ersten Steamer!", "tokens": ["Meer\u00b7\u00fc\u00b7ber", "mit", "dem", "ers\u00b7ten", "Stea\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Irlands Wild ist Herrenwild:", "tokens": ["Denn", "Ir\u00b7lands", "Wild", "ist", "Her\u00b7ren\u00b7wild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es f\u00fcllt des Grundherrn Bauch und Taschen \u2013", "tokens": ["Es", "f\u00fcllt", "des", "Grund\u00b7herrn", "Bauch", "und", "Ta\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der bleiche Knecht, des Elends Bild,", "tokens": ["Der", "blei\u00b7che", "Knecht", ",", "des", "E\u00b7lends", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hilf Gott! ist selbst zu matt zum Paschen!", "tokens": ["Hilf", "Gott", "!", "ist", "selbst", "zu", "matt", "zum", "Pa\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "ADV", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So sorgt der Herr, da\u00df Hirsch und Ochs,", "tokens": ["So", "sorgt", "der", "Herr", ",", "da\u00df", "Hirsch", "und", "Ochs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das hei\u00dft: da\u00df ihn sein Bauer m\u00e4ste;", "tokens": ["Das", "hei\u00dft", ":", "da\u00df", "ihn", "sein", "Bau\u00b7er", "m\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "KOUS", "PPER", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Statt auszutrocknen seine Bogs \u2013", "tokens": ["Statt", "aus\u00b7zu\u00b7trock\u00b7nen", "sei\u00b7ne", "Bogs", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr kennt sie ja: Irlands Mor\u00e4ste!", "tokens": ["Ihr", "kennt", "sie", "ja", ":", "Ir\u00b7lands", "Mo\u00b7r\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "NE", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er l\u00e4\u00dft den Boden nutzlos ruhn,", "tokens": ["Er", "l\u00e4\u00dft", "den", "Bo\u00b7den", "nutz\u00b7los", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf Halm an Halm sich wiegen k\u00f6nnte;", "tokens": ["Drauf", "Halm", "an", "Halm", "sich", "wie\u00b7gen", "k\u00f6nn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "APPR", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er l\u00e4\u00dft ihn schn\u00f6d' dem Wasserhuhn,", "tokens": ["Er", "l\u00e4\u00dft", "ihn", "schn\u00f6d'", "dem", "Was\u00b7ser\u00b7huhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Kiebitz und der wilden Ente!", "tokens": ["Dem", "Kie\u00b7bitz", "und", "der", "wil\u00b7den", "En\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja doch, bei Gottes Fluche: \u2013 Sumpf", "tokens": ["Ja", "doch", ",", "bei", "Got\u00b7tes", "Flu\u00b7che", ":", "\u2013", "Sumpf"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["PTKANT", "ADV", "$,", "APPR", "NN", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Wildnis vier Millionen \u00c4cker!", "tokens": ["Und", "Wild\u00b7nis", "vier", "Mil\u00b7lion\u00b7en", "\u00c4\u00b7cker", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "CARD", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr aber seid blasiert und stumpf,", "tokens": ["Ihr", "a\u00b7ber", "seid", "bla\u00b7siert", "und", "stumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Faul und verfault \u2013 euch weckt kein Wecker!", "tokens": ["Faul", "und", "ver\u00b7fault", "\u2013", "euch", "weckt", "kein", "We\u00b7cker", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, irisch Land ist Herrenland:", "tokens": ["O", ",", "i\u00b7risch", "Land", "ist", "Her\u00b7ren\u00b7land", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum stehn die M\u00fctter an den Wegen,", "tokens": ["Drum", "stehn", "die", "M\u00fct\u00b7ter", "an", "den", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den toten S\u00e4ugling im Gewand,", "tokens": ["Den", "to\u00b7ten", "S\u00e4ug\u00b7ling", "im", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und flehn euch, ihn ins Grab zu legen.", "tokens": ["Und", "flehn", "euch", ",", "ihn", "ins", "Grab", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u2013 So schallt die Klage Tag und Nacht,", "tokens": ["\u2013", "So", "schallt", "die", "Kla\u00b7ge", "Tag", "und", "Nacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So grollt es Connaught durch und Leinster.", "tokens": ["So", "grollt", "es", "Con\u00b7naught", "durch", "und", "Leins\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "APPR", "KON", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Der West hat mir den Schrei gebracht \u2013", "tokens": ["Der", "West", "hat", "mir", "den", "Schrei", "ge\u00b7bracht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er trug ihn schrill bis vor mein Fenster.", "tokens": ["Er", "trug", "ihn", "schrill", "bis", "vor", "mein", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "wor