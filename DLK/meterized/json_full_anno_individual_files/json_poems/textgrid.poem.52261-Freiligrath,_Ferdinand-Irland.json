a"}, "line.7": {"text": "Der bleiche Knecht, des Elends Bild,", "tokens": ["Der", "blei\u00b7che", "Knecht", ",", "des", "E\u00b7lends", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hilf Gott! ist selbst zu matt zum Paschen!", "tokens": ["Hilf", "Gott", "!", "ist", "selbst", "zu", "matt", "zum", "Pa\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "ADV", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So sorgt der Herr, da\u00df Hirsch und Ochs,", "tokens": ["So", "sorgt", "der", "Herr", ",", "da\u00df", "Hirsch", "und", "Ochs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das hei\u00dft: da\u00df ihn sein Bauer m\u00e4ste;", "tokens": ["Das", "hei\u00dft", ":", "da\u00df", "ihn", "sein", "Bau\u00b7er", "m\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "KOUS", "PPER", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Statt auszutrocknen seine Bogs \u2013", "tokens": ["Statt", "aus\u00b7zu\u00b7trock\u00b7nen", "sei\u00b7ne", "Bogs", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr kennt sie ja: Irlands Mor\u00e4ste!", "tokens": ["Ihr", "kennt", "sie", "ja", ":", "Ir\u00b7lands", "Mo\u00b7r\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "NE", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er l\u00e4\u00dft den Boden nutzlos ruhn,", "tokens": ["Er", "l\u00e4\u00dft", "den", "Bo\u00b7den", "nutz\u00b7los", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf Halm an Halm sich wiegen k\u00f6nnte;", "tokens": ["Drauf", "Halm", "an", "Halm", "sich", "wie\u00b7gen", "k\u00f6nn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "APPR", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er l\u00e4\u00dft ihn schn\u00f6d' dem Wasserhuhn,", "tokens": ["Er", "l\u00e4\u00dft", "ihn", "schn\u00f6d'", "dem", "Was\u00b7ser\u00b7huhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Kiebitz und der wilden Ente!", "tokens": ["Dem", "Kie\u00b7bitz", "und", "der", "wil\u00b7den", "En\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ja doch, bei Gottes Fluche: \u2013 Sumpf", "tokens": ["Ja", "doch", ",", "bei", "Got\u00b7tes", "Flu\u00b7che", ":", "\u2013", "Sumpf"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["PTKANT", "ADV", "$,", "APPR", "NN", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Wildnis vier Millionen \u00c4cker!", "tokens": [