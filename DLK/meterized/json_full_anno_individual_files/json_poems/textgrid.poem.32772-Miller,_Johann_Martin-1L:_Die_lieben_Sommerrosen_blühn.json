{"textgrid.poem.32772": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die lieben Sommerrosen bl\u00fchn", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die lieben Sommerrosen bl\u00fchn", "tokens": ["Die", "lie\u00b7ben", "Som\u00b7mer\u00b7ro\u00b7sen", "bl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun endlich auch im Garten;", "tokens": ["Nun", "end\u00b7lich", "auch", "im", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ging ich jeden Morgen hin,", "tokens": ["Da", "ging", "ich", "je\u00b7den", "Mor\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und konnt' es kaum erwarten.", "tokens": ["Und", "konnt'", "es", "kaum", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die allersch\u00f6nste dacht' ich mir", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "dacht'", "ich", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aufs Kirmesfest zu pfl\u00fccken,", "tokens": ["Aufs", "Kir\u00b7mes\u00b7fest", "zu", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und meines Wilhelm Hut mit ihr", "tokens": ["Und", "mei\u00b7nes", "Wil\u00b7helm", "Hut", "mit", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NE", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beim Ringeltanz zu schm\u00fccken.", "tokens": ["Beim", "Rin\u00b7gel\u00b7tanz", "zu", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, aber keine pfl\u00fcck' ich ab,", "tokens": ["Ach", ",", "a\u00b7ber", "kei\u00b7ne", "pfl\u00fcck", "ich", "ab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PIAT", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinem Hut zu bl\u00fchen!", "tokens": ["Auf", "sei\u00b7nem", "Hut", "zu", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Rosen, nur auf seinem Grab", "tokens": ["Ihr", "Ro\u00b7sen", ",", "nur", "auf", "sei\u00b7nem", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt ihr ein Kreuz umziehen.", "tokens": ["Sollt", "ihr", "ein", "Kreuz", "um\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du guter Wilhelm, dachten wir", "tokens": ["Du", "gu\u00b7ter", "Wil\u00b7helm", ",", "dach\u00b7ten", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NE", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies noch vor wenig Tagen,", "tokens": ["Dies", "noch", "vor", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als wir den ganzen Abend hier", "tokens": ["Als", "wir", "den", "gan\u00b7zen", "A\u00b7bend", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Schl\u00fcsselblumen lagen?", "tokens": ["Auf", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "la\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Rings um uns wehte frische Luft,", "tokens": ["Rings", "um", "uns", "weh\u00b7te", "fri\u00b7sche", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Vogel sangen munter;", "tokens": ["Die", "Vo\u00b7gel", "san\u00b7gen", "mun\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00fc\u00dfer Apfelbl\u00fctenduft", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Ap\u00b7fel\u00b7bl\u00fc\u00b7ten\u00b7duft"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Go\u00df sich vom Baum herunter!", "tokens": ["Go\u00df", "sich", "vom", "Baum", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir lagen, Hand in Hand, vertraut,", "tokens": ["Wir", "la\u00b7gen", ",", "Hand", "in", "Hand", ",", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00fc\u00dften uns und kannten", "tokens": ["Und", "k\u00fc\u00df\u00b7ten", "uns", "und", "kann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein gr\u00f6\u00dfer Gl\u00fcck, als wenn wir Braut", "tokens": ["Kein", "gr\u00f6\u00b7\u00dfer", "Gl\u00fcck", ",", "als", "wenn", "wir", "Braut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOKOM", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Br\u00e4utigam uns nannten.", "tokens": ["Und", "Br\u00e4u\u00b7ti\u00b7gam", "uns", "nann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann aber tr\u00fcbten nach und nach", "tokens": ["Dann", "a\u00b7ber", "tr\u00fcb\u00b7ten", "nach", "und", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich deine frohen Mienen,", "tokens": ["Sich", "dei\u00b7ne", "fro\u00b7hen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und unbekannte Wehmut sprach", "tokens": ["Und", "un\u00b7be\u00b7kann\u00b7te", "Weh\u00b7mut", "sprach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit bangem Blick aus ihnen;", "tokens": ["Mit", "ban\u00b7gem", "Blick", "aus", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Noch immer seh' ich dein Gesicht,", "tokens": ["Noch", "im\u00b7mer", "seh'", "ich", "dein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie du ein Str\u00e4u\u00dfchen pfl\u00fccktest,", "tokens": ["Wie", "du", "ein", "Str\u00e4u\u00df\u00b7chen", "pfl\u00fcck\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und zitternde Vergi\u00dfmeinnicht", "tokens": ["Und", "zit\u00b7tern\u00b7de", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir in die H\u00e4nde dr\u00fccktest!", "tokens": ["Mir", "in", "die", "H\u00e4n\u00b7de", "dr\u00fcck\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da schlug mein Herz, ich konnte kaum", "tokens": ["Da", "schlug", "mein", "Herz", ",", "ich", "konn\u00b7te", 