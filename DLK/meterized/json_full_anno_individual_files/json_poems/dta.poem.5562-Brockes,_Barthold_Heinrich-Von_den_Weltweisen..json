{"dta.poem.5562": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Aristoteles uns die Natur erkl\u00e4rt,", "tokens": ["Wenn", "A\u00b7ris\u00b7to\u00b7te\u00b7les", "uns", "die", "Na\u00b7tur", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und viel von Elementen lehrt,", "tokens": ["Und", "viel", "von", "E\u00b7le\u00b7men\u00b7ten", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df sie", "tokens": ["Da\u00df", "sie"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Durch wiederw\u00e4rt\u2019ge Harmonie", "tokens": ["Durch", "wie\u00b7der\u00b7w\u00e4rt'\u00b7ge", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich eins ins andre mengen,", "tokens": ["Sich", "eins", "ins", "and\u00b7re", "men\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich trennen bald, und bald zusammen h\u00e4ngen;", "tokens": ["Sich", "tren\u00b7nen", "bald", ",", "und", "bald", "zu\u00b7sam\u00b7men", "h\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "$,", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So setzt er fest: Es sey die Welt von Ewigkeit", "tokens": ["So", "setzt", "er", "fest", ":", "Es", "sey", "die", "Welt", "von", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und seiner Lehre Dunckelheit", "tokens": ["Und", "sei\u00b7ner", "Leh\u00b7re", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verursacht lauter Zanck und Streit.", "tokens": ["Ver\u00b7ur\u00b7sacht", "lau\u00b7ter", "Zanck", "und", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}