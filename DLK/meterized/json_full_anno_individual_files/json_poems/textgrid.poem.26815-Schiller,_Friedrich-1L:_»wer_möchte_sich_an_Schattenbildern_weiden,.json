{"textgrid.poem.26815": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwer m\u00f6chte sich an Schattenbildern weiden,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwer m\u00f6chte sich an Schattenbildern weiden,", "tokens": ["\u00bb", "wer", "m\u00f6ch\u00b7te", "sich", "an", "Schat\u00b7ten\u00b7bil\u00b7dern", "wei\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mit erborgtem Schein das Wesen \u00fcberkleiden,", "tokens": ["Die", "mit", "er\u00b7borg\u00b7tem", "Schein", "das", "We\u00b7sen", "\u00fc\u00b7berk\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit tr\u00fcgrischem Besitz die Hoffnung hintergehn?", "tokens": ["Mit", "tr\u00fcg\u00b7ri\u00b7schem", "Be\u00b7sitz", "die", "Hoff\u00b7nung", "hin\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entbl\u00f6\u00dft mu\u00df ich die Wahrheit sehn.", "tokens": ["Ent\u00b7bl\u00f6\u00dft", "mu\u00df", "ich", "die", "Wahr\u00b7heit", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Soll gleich mit meinem Wahn mein ganzer Himmel schwinden,", "tokens": ["Soll", "gleich", "mit", "mei\u00b7nem", "Wahn", "mein", "gan\u00b7zer", "Him\u00b7mel", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Soll gleich den freien Geist, den der erhabne Flug", "tokens": ["Soll", "gleich", "den", "frei\u00b7en", "Geist", ",", "den", "der", "er\u00b7hab\u00b7ne", "Flug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ins grenzenlose Reich der M\u00f6glichkeiten trug,", "tokens": ["Ins", "gren\u00b7zen\u00b7lo\u00b7se", "Reich", "der", "M\u00f6g\u00b7lich\u00b7kei\u00b7ten", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Gegenwart mit strengen Fesseln binden,", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "mit", "stren\u00b7gen", "Fes\u00b7seln", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er lernt sich selber \u00fcberwinden,", "tokens": ["Er", "lernt", "sich", "sel\u00b7ber", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ihn wird das heilige Gebot", "tokens": ["Ihn", "wird", "das", "hei\u00b7li\u00b7ge", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Pflicht, das furchtbare der Not", "tokens": ["Der", "Pflicht", ",", "das", "furcht\u00b7ba\u00b7re", "der", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nur desto unterw\u00fcrfger finden.", "tokens": ["Nur", "des\u00b7to", "un\u00b7ter\u00b7w\u00fcrf\u00b7ger", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wer schon der Wahrheit milde Herrschaft scheut,", "tokens": ["Wer", "schon", "der", "Wahr\u00b7heit", "mil\u00b7de", "Herr\u00b7schaft", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wie tr\u00e4gt er die Notwendigkeit?\u00ab \u2013", "tokens": ["Wie", "tr\u00e4gt", "er", "die", "Not\u00b7wen\u00b7dig\u00b7keit", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "So rufst du aus und blickst, mein strenger Freund,", "tokens": ["So", "rufst", "du", "aus", "und", "blickst", ",", "mein", "stren\u00b7ger", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus der Erfahrung sicherm Porte", "tokens": ["Aus", "der", "Er\u00b7fah\u00b7rung", "si\u00b7cherm", "Por\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verwerfend hin auf alles, was nur scheint.", "tokens": ["Ver\u00b7wer\u00b7fend", "hin", "auf", "al\u00b7les", ",", "was", "nur", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erschreckt von deinem ernsten Worte", "tokens": ["Er\u00b7schreckt", "von", "dei\u00b7nem", "erns\u00b7ten", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entflieht der Liebesg\u00f6tter Schar,", "tokens": ["Ent\u00b7flieht", "der", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Musen Spiel verstummt, es ruhn der Horen T\u00e4nze,", "tokens": ["Der", "Mu\u00b7sen", "Spiel", "ver\u00b7stummt", ",", "es", "ruhn", "der", "Ho\u00b7ren", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Still traurend nehmen ihre Kr\u00e4nze", "tokens": ["Still", "trau\u00b7rend", "neh\u00b7men", "ih\u00b7re", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Schwesterg\u00f6ttinnen vom sch\u00f6n gelockten Haar,", "tokens": ["Die", "Schwes\u00b7ter\u00b7g\u00f6t\u00b7tin\u00b7nen", "vom", "sch\u00f6n", "ge\u00b7lock\u00b7ten", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Apoll zerbricht die goldne Leier,", "tokens": ["A\u00b7poll", "zer\u00b7bricht", "die", "gold\u00b7ne", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Und Hermes seinen Wunderstab,", "tokens": ["Und", "Her\u00b7mes", "sei\u00b7nen", "Wun\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des Traumes rosenfarbner Schleier", "tokens": ["Des", "Trau\u00b7mes", "ro\u00b7sen\u00b7farb\u00b7ner", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "F\u00e4llt von des Lebens bleichem Antlitz ab,", "tokens": ["F\u00e4llt", "von", "des", "Le\u00b7bens", "blei\u00b7chem", "Ant\u00b7litz", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.13": {"text": "Die Welt scheint, was sie ist, ein Grab.", "tokens": ["Die", "Welt", "scheint", ",", "was", "sie", "ist", ",", "ein", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VAFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von seinen Augen nimmt die zauberische Binde", "tokens": ["Von", "sei\u00b7nen", "Au\u00b7gen", "nimmt", "die", "zau\u00b7be\u00b7ri\u00b7sche", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Cytherens Sohn, die Liebe sieht,", "tokens": ["Cy\u00b7the\u00b7rens", "Sohn", ",", "die", "Lie\u00b7be", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Sie sieht in ihrem G\u00f6tterkinde", "tokens": ["Sie", "sieht", "in", "ih\u00b7rem", "G\u00f6t\u00b7ter\u00b7kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den Sterblichen, erschrickt und flieht,", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", ",", "er\u00b7schrickt", "und", "flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Sch\u00f6nheit Jugendbild veraltet,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Ju\u00b7gend\u00b7bild", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Auf deinen Lippen selbst erkaltet", "tokens": ["Auf", "dei\u00b7nen", "Lip\u00b7pen", "selbst", "er\u00b7kal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der Liebe Ku\u00df, und in der Freude Schwung", "tokens": ["Der", "Lie\u00b7be", "Ku\u00df", ",", "und", "in", "der", "Freu\u00b7de", "Schwung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ergreift dich die Versteinerung.", "tokens": ["Er\u00b7greift", "dich", "die", "Ver\u00b7stei\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbwer m\u00f6chte sich an Schattenbildern weiden,", "tokens": ["\u00bb", "wer", "m\u00f6ch\u00b7te", "sich", "an", "Schat\u00b7ten\u00b7bil\u00b7dern", "wei\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mit erborgtem Schein das Wesen \u00fcberkleiden,", "tokens": ["Die", "mit", "er\u00b7borg\u00b7tem", "Schein", "das", "We\u00b7sen", "\u00fc\u00b7berk\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit tr\u00fcgrischem Besitz die Hoffnung hintergehn?", "tokens": ["Mit", "tr\u00fcg\u00b7ri\u00b7schem", "Be\u00b7sitz", "die", "Hoff\u00b7nung", "hin\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entbl\u00f6\u00dft mu\u00df ich die Wahrheit sehn.", "tokens": ["Ent\u00b7