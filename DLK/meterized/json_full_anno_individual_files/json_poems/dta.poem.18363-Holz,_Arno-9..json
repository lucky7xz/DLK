{"dta.poem.18363": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ums dunkle Dachwerk pfeift der Wind", "tokens": ["Ums", "dunk\u00b7le", "Dach\u00b7werk", "pfeift", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Nachbars Lieschen liegt im Sterben", "tokens": ["Und", "Nach\u00b7bars", "Lie\u00b7schen", "liegt", "im", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre Mutter weint sich blind.", "tokens": ["Und", "ih\u00b7re", "Mut\u00b7ter", "weint", "sich", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Haar gebleicht von tausend Sorgen,", "tokens": ["Das", "Haar", "ge\u00b7bleicht", "von", "tau\u00b7send", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im d\u00fcnnen Kleidchen von Kattun,", "tokens": ["Im", "d\u00fcn\u00b7nen", "Kleid\u00b7chen", "von", "Kat\u00b7tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Erwartet sehnlich sie den Morgen \u2014", "tokens": ["Er\u00b7war\u00b7tet", "sehn\u00b7lich", "sie", "den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Apotheker will nicht borgen,", "tokens": ["Der", "A\u00b7pot\u00b7he\u00b7ker", "will", "nicht", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Doctor hat \u201ezu viel zu thun\u201c! ...", "tokens": ["Der", "Doc\u00b7tor", "hat", "\u201e", "zu", "viel", "zu", "thun", "\u201c", "!", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "PTKA", "PIS", "PTKZU", "VVINF", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der M\u00e4rznacht goldne Sterne scheinen,", "tokens": ["Der", "M\u00e4rz\u00b7nacht", "gold\u00b7ne", "Ster\u00b7ne", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Himmel deckt uns alle zu:", "tokens": ["Ihr", "Him\u00b7mel", "deckt", "uns", "al\u00b7le", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6r auf, du M\u00fctterchen, mit Weinen,", "tokens": ["H\u00f6r", "auf", ",", "du", "M\u00fct\u00b7ter\u00b7chen", ",", "mit", "Wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Kind ist besser dran, als du!", "tokens": ["Dein", "Kind", "ist", "bes\u00b7ser", "dran", ",", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es braucht nicht n\u00e4hend mehr zu sputen", "tokens": ["Es", "braucht", "nicht", "n\u00e4\u00b7hend", "mehr", "zu", "spu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich sp\u00e4t bis in die Nacht hinein,", "tokens": ["Sich", "sp\u00e4t", "bis", "in", "die", "Nacht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn die L\u00fcfte sie umfluthen", "tokens": ["Und", "wenn", "die", "L\u00fcf\u00b7te", "sie", "um\u00b7flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und roth die Rosen wieder bluten,", "tokens": ["Und", "roth", "die", "Ro\u00b7sen", "wie\u00b7der", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Spielt um sein Grab der Sonnenschein!", "tokens": ["Spielt", "um", "sein", "Grab", "der", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Noth im l\u00f6chrigen Gewande", "tokens": ["Die", "Noth", "im", "l\u00f6ch\u00b7ri\u00b7gen", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zertritt die Perle der Moral;", "tokens": ["Zer\u00b7tritt", "die", "Per\u00b7le", "der", "Mo\u00b7ral", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Loos der Armuth ist die Schande,", "tokens": ["Das", "Loos", "der", "Ar\u00b7muth", "ist", "die", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Loos der Schande der Spital!", "tokens": ["Das", "Loos", "der", "Schan\u00b7de", "der", "Spi\u00b7tal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, jede Gro\u00dfstadt ist ein Zwinger,", "tokens": ["Ja", ",", "je\u00b7de", "Gro\u00df\u00b7stadt", "ist", "ein", "Zwin\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der roth von Blut und Thr\u00e4nen dampft;", "tokens": ["Der", "roth", "von", "Blut", "und", "Thr\u00e4\u00b7nen", "dampft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum h\u00fctet euch, ihr armen Dinger,", "tokens": ["Drum", "h\u00fc\u00b7tet", "euch", ",", "ihr", "ar\u00b7men", "Din\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn diese Welt hat schmutzge Finger \u2014", "tokens": ["Denn", "die\u00b7se", "Welt", "hat", "schmutz\u00b7ge", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Weh, wem sie sie ins Herzfleisch krampft!", "tokens": ["Weh", ",", "wem", "sie", "sie", "ins", "Herz\u00b7fleisch", "krampft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da horch! ein langgezognes St\u00f6hnen", "tokens": ["Da", "horch", "!", "ein", "lang\u00b7ge\u00b7zog\u00b7nes", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jetzt ein wilder, geller Schrei!", "tokens": ["Und", "jetzt", "ein", "wil\u00b7der", ",", "gel\u00b7ler", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was thut's? Man mu\u00df sich dran gew\u00f6hnen!", "tokens": ["Was", "thut's", "?", "Man", "mu\u00df", "sich", "dran", "ge\u00b7w\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PIS", "VMFIN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier hie\u00df es wieder mal: \u201eVorbei!\u201c", "tokens": ["Hier", "hie\u00df", "es", "wie\u00b7der", "mal", ":", "\u201e", "Vor\u00b7bei", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "$(", "ADV", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schon \u00fcbermorgen karrt der Racker", "tokens": ["Schon", "\u00fc\u00b7ber\u00b7mor\u00b7gen", "karrt", "der", "Ra\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das arme M\u00e4del vor die Stadt", "tokens": ["Das", "ar\u00b7me", "M\u00e4\u00b7del", "vor", "die", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und niemand kennt den Todtenacker,", "tokens": ["Und", "nie\u00b7mand", "kennt", "den", "Tod\u00b7te\u00b7na\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Darauf beim \u00f6den Sterngeflacker", "tokens": ["Da\u00b7rauf", "beim", "\u00f6\u00b7den", "Stern\u00b7ge\u00b7fla\u00b7cker"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Herz sein Gl\u00fcck gefunden hat!", "tokens": ["Ein", "Herz", "sein", "Gl\u00fcck", "ge\u00b7fun\u00b7den", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}