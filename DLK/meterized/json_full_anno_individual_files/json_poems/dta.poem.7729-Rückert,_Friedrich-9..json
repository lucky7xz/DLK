{"dta.poem.7729": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den heil'gen Weda wenn du liesest in der Nacht", "tokens": ["Den", "heil'\u00b7gen", "We\u00b7da", "wenn", "du", "lie\u00b7sest", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim Schein der Lampe, sei der Lampe Schein bewacht,", "tokens": ["Beim", "Schein", "der", "Lam\u00b7pe", ",", "sei", "der", "Lam\u00b7pe", "Schein", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df er nicht d\u00fcster brenn' und da\u00df er irr nicht flirre,", "tokens": ["Da\u00df", "er", "nicht", "d\u00fcs\u00b7ter", "brenn'", "und", "da\u00df", "er", "irr", "nicht", "flir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "PTKVZ", "KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dir's nicht dunkel sei, und da\u00df dein Sinn nicht irre.", "tokens": ["Da\u00df", "dir's", "nicht", "dun\u00b7kel", "sei", ",", "und", "da\u00df", "dein", "Sinn", "nicht", "ir\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADJD", "VAFIN", "$,", "KON", "KOUS", "PPOSAT", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch sei nach au\u00dfen hin ein Schirm gestellt vors Licht,", "tokens": ["Auch", "sei", "nach", "au\u00b7\u00dfen", "hin", "ein", "Schirm", "ge\u00b7stellt", "vors", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "ADV", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit kein L\u00fcftezug es st\u00f6r' im Gleichgewicht,", "tokens": ["Da\u00b7mit", "kein", "L\u00fcf\u00b7te\u00b7zug", "es", "st\u00f6r'", "im", "Gleich\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Auch n\u00e4cht'ge Fliegen nicht und n\u00e4cht'ge Schmetterlinge,", "tokens": ["Auch", "n\u00e4cht'\u00b7ge", "Flie\u00b7gen", "nicht", "und", "n\u00e4cht'\u00b7ge", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlockt von deinem Licht, versengen ihre Schwinge.", "tokens": ["Ver\u00b7lockt", "von", "dei\u00b7nem", "Licht", ",", "ver\u00b7sen\u00b7gen", "ih\u00b7re", "Schwin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn weil du denkest den, der Leben hat gegeben", "tokens": ["Denn", "weil", "du", "den\u00b7kest", "den", ",", "der", "Le\u00b7ben", "hat", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den Wesen allen, soll verlieren keins das Leben;", "tokens": ["Den", "We\u00b7sen", "al\u00b7len", ",", "soll", "ver\u00b7lie\u00b7ren", "keins", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "VMFIN", "VVINF", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und nie gereichen soll geweihter Flamme Sch\u00fcrung", "tokens": ["Und", "nie", "ge\u00b7rei\u00b7chen", "soll", "ge\u00b7weih\u00b7ter", "Flam\u00b7me", "Sch\u00fc\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Ungeweihter Tod, zu Schwacher Irref\u00fchrung.", "tokens": ["Zu", "Un\u00b7ge\u00b7weih\u00b7ter", "Tod", ",", "zu", "Schwa\u00b7cher", "Ir\u00b7re\u00b7f\u00fch\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}