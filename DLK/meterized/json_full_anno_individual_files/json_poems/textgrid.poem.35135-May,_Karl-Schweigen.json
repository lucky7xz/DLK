{"textgrid.poem.35135": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Schweigen", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh still, geh still durchs Leben hin!", "tokens": ["Geh", "still", ",", "geh", "still", "durchs", "Le\u00b7ben", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ger\u00e4usch wohnt nur im Hohlen, Leeren,", "tokens": ["Ge\u00b7r\u00e4usch", "wohnt", "nur", "im", "Hoh\u00b7len", ",", "Lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nie wird edler Mannessinn", "tokens": ["Und", "nie", "wird", "ed\u00b7ler", "Man\u00b7nes\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich durch Trompetenschall entehren.", "tokens": ["Sich", "durch", "Trom\u00b7pe\u00b7ten\u00b7schall", "en\u00b7teh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schlie\u00dft deines lautern Wortes Gold", "tokens": ["Schlie\u00dft", "dei\u00b7nes", "lau\u00b7tern", "Wor\u00b7tes", "Gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Demant des Gedankens ein,", "tokens": ["Den", "De\u00b7mant", "des", "Ge\u00b7dan\u00b7kens", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "So sei die Sparsamkeit ihm hold", "tokens": ["So", "sei", "die", "Spar\u00b7sam\u00b7keit", "ihm", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und lasse es nicht billig sein.", "tokens": ["Und", "las\u00b7se", "es", "nicht", "bil\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei still, wenn deine Eigenart", "tokens": ["Sei", "still", ",", "wenn", "dei\u00b7ne", "Ei\u00b7gen\u00b7art"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt noch nicht Anerkennung findet.", "tokens": ["Jetzt", "noch", "nicht", "An\u00b7er\u00b7ken\u00b7nung", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wei\u00dft ja, wer die Kr\u00e4nze spart", "tokens": ["Du", "wei\u00dft", "ja", ",", "wer", "die", "Kr\u00e4n\u00b7ze", "spart"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wem die Nachwelt einst sie windet.", "tokens": ["Und", "wem", "die", "Nach\u00b7welt", "einst", "sie", "win\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Allem dann sei still, ganz still,", "tokens": ["Vor", "Al\u00b7lem", "dann", "sei", "still", ",", "ganz", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und geh nicht ein auf niedern Zwist,", "tokens": ["Und", "geh", "nicht", "ein", "auf", "nie\u00b7dern", "Zwist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn dich der Neid befeinden will,", "tokens": ["Wenn", "dich", "der", "Neid", "be\u00b7fein\u00b7den", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil du ihm \u00fcberlegen bist.", "tokens": ["Weil", "du", "ihm", "\u00fc\u00b7berl\u00b7e\u00b7gen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Siehst du dich deines Ziels bewu\u00dft", "tokens": ["Siehst", "du", "dich", "dei\u00b7nes", "Ziels", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00dfts auf gutem Grunde stehen,", "tokens": ["Und", "wei\u00dfts", "auf", "gu\u00b7tem", "Grun\u00b7de", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "