{"dta.poem.14448": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da spricht der Abgrund dunkel: Bist nun meine;", "tokens": ["Da", "spricht", "der", "Ab\u00b7grund", "dun\u00b7kel", ":", "Bist", "nun", "mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$.", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zieht mich hinab an bleiernen Gewichtern,", "tokens": ["Zieht", "mich", "hin\u00b7ab", "an", "blei\u00b7er\u00b7nen", "Ge\u00b7wich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sieht stumm mich an aus steinernen Gesichtern,", "tokens": ["Sieht", "stumm", "mich", "an", "aus", "stei\u00b7ner\u00b7nen", "Ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Herz wird selber zum krystall'nen Steine.", "tokens": ["Das", "Herz", "wird", "sel\u00b7ber", "zum", "kry\u00b7stall'\u00b7nen", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}