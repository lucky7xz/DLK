{"textgrid.poem.39124": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Viel S\u00e4nger singen weit und breit,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel S\u00e4nger singen weit und breit,", "tokens": ["Viel", "S\u00e4n\u00b7ger", "sin\u00b7gen", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie singen in Zorn und Harm,", "tokens": ["Sie", "sin\u00b7gen", "in", "Zorn", "und", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie wollen wecken die tr\u00e4ge Zeit", "tokens": ["Sie", "wol\u00b7len", "we\u00b7cken", "die", "tr\u00e4\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus des Schlummers bleiernem Arm.", "tokens": ["Aus", "des", "Schlum\u00b7mers", "blei\u00b7er\u00b7nem", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Im Schlummer sterben die V\u00f6lker hin,", "tokens": ["Im", "Schlum\u00b7mer", "ster\u00b7ben", "die", "V\u00f6l\u00b7ker", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Banner schl\u00e4f