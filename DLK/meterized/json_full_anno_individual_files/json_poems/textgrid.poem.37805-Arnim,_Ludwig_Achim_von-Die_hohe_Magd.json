{"textgrid.poem.37805": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die hohe Magd", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Magd ist wei\u00df und schone,", "tokens": ["Ein", "Magd", "ist", "wei\u00df", "und", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott f\u00fchrt den h\u00f6chsten Prei\u00df,", "tokens": ["Gott", "f\u00fchrt", "den", "h\u00f6chs\u00b7ten", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die ihm dient, zum Lohne", "tokens": ["Und", "die", "ihm", "dient", ",", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An K\u00fcnsten wird sie reich,", "tokens": ["An", "K\u00fcns\u00b7ten", "wird", "sie", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geht jungfr\u00e4ulich bei Frauen", "tokens": ["Geht", "jung\u00b7fr\u00e4u\u00b7lich", "bei", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dort auf den gr\u00fcnen Auen,", "tokens": ["Dort", "auf", "den", "gr\u00fc\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gl\u00fcck zu mein edler Zweig!", "tokens": ["Gl\u00fcck", "zu", "mein", "ed\u00b7ler", "Zweig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+----+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ihr Leib war angebildet", "tokens": ["Ihr", "Leib", "war", "an\u00b7ge\u00b7bil\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Keuschheit \u00fcbergro\u00df,", "tokens": ["Mit", "Keuschheit", "\u00fc\u00b7ber\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schwang sich in ihren Willen,", "tokens": ["Schwang", "sich", "in", "ih\u00b7ren", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwang sich in ihren Schoo\u00df,", "tokens": ["Schwang", "sich", "in", "ih\u00b7ren", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er war so stark von Kr\u00e4ften,", "tokens": ["Er", "war", "so", "stark", "von", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von meisterlichen Gesch\u00e4ften \u2013", "tokens": ["Von", "meis\u00b7ter\u00b7li\u00b7chen", "Ge\u00b7sch\u00e4f\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gott schuf wohl Himmel und Erd.", "tokens": ["Gott", "schuf", "wohl", "Him\u00b7mel", "und", "Erd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ein Kind nach Adams Weise", "tokens": ["Ein", "Kind", "nach", "A\u00b7dams", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An ihren Br\u00fcsten lag,", "tokens": ["An", "ih\u00b7ren", "Br\u00fcs\u00b7ten", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es war ein alter Greise,", "tokens": ["Es", "war", "ein", "al\u00b7ter", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erschuf den ersten Tag,", "tokens": ["Er\u00b7schuf", "den", "ers\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ward ein starker Ritter,", "tokens": ["Es", "ward", "ein", "star\u00b7ker", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein Leiden ward ihm bitter,", "tokens": ["Sein", "Lei\u00b7den", "ward", "ihm", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erlitt gro\u00df Ungemach.", "tokens": ["Er\u00b7litt", "gro\u00df", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sein Seit ward ihm zerschnitten", "tokens": ["Sein", "Seit", "ward", "ihm", "zer\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Mit einem scharfen Speer,", "tokens": ["Mit", "ei\u00b7nem", "schar\u00b7fen", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit hat er zersplitten", "tokens": ["Da\u00b7mit", "hat", "er", "zer\u00b7split\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die H\u00f6lle samt der Erd.", "tokens": ["Die", "H\u00f6l\u00b7le", "samt", "der", "Erd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gott tr\u00f6stet den Gefangnen,", "tokens": ["Gott", "tr\u00f6s\u00b7tet", "den", "Ge\u00b7fang\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drey W\u00fcnsche waren ihm ergangen", "tokens": ["Drey", "W\u00fcn\u00b7sche", "wa\u00b7ren", "ihm", "er\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gegen diese heilige Zeit.", "tokens": ["Ge\u00b7gen", "die\u00b7se", "hei\u00b7li\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Gott stieg aus seinem Grabe,", "tokens": ["Gott", "stieg", "aus", "sei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein F\u00fcrst war wohlgemuth,", "tokens": ["Ein", "F\u00fcrst", "war", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinem Kreuz und Stabe,", "tokens": ["Mit", "sei\u00b7nem", "Kreuz", "und", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drey F\u00e4hnlein schwenkt er roth,", "tokens": ["Drey", "F\u00e4hn\u00b7lein", "schwenkt", "er", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "That sich gen Himmel kehren,", "tokens": ["That", "sich", "gen", "Him\u00b7mel", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Nach tugendlichen Ehren", "tokens": ["Nach", "tu\u00b7gend\u00b7li\u00b7chen", "Eh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Stand ihm Herz, Muth und Sinn.", "tokens": ["Stand", "ihm", "Herz", ",", "Muth", "und", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "O Stern, o Glanz! o Krone,", "tokens": ["O", "Stern", ",", "o", "Glanz", "!", "o", "Kro\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$.", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Himmel aufgethan!", "tokens": ["O", "Him\u00b7mel", "auf\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was gab ihr Gott zum Lohne,", "tokens": ["Was", "gab", "ihr", "Gott", "zum", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drey Chorengel Lobgesang,", "tokens": ["Drey", "Cho\u00b7ren\u00b7gel", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bekleidet ihn mit Sonne,", "tokens": ["Be\u00b7klei\u00b7det", "ihn", "mit", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Maria war voll Wonne,", "tokens": ["Ma\u00b7ria", "war", "voll", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie hell scheint uns der Mond!", "tokens": ["Wie", "hell", "scheint", "uns", "der", "Mond", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein Magd ist wei\u00df und schone,", "tokens": ["Ein", "Magd", "ist", "wei\u00df", "und", "scho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott f\u00fchrt den h\u00f6chsten Prei\u00df,", "tokens": ["Gott", "f\u00fchrt", "den", "h\u00f6chs\u00b7ten", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die ihm dient, zum Lohne", "tokens": ["Und", "die", "ihm", "dient", ",", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "PPER", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An K\u00fcnsten wird sie reich,", "tokens": ["An", "K\u00fcns\u00b7ten", "wird", "sie", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geht jungfr\u00e4ulich bei Frauen", "tokens": ["Geht", "jung\u00b7fr\u00e4u\u00b7lich", "bei", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Dort auf den gr\u00fcnen Auen,", "tokens": ["Dort", "auf", "den", "gr\u00fc\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gl