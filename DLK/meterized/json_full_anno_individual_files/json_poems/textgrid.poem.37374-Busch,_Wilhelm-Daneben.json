{"textgrid.poem.37374": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Daneben", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stoffel hackte mit dem Beile.", "tokens": ["Stof\u00b7fel", "hack\u00b7te", "mit", "dem", "Bei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dabei tat er sich sehr wehe,", "tokens": ["Da\u00b7bei", "tat", "er", "sich", "sehr", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er traf in aller Eile", "tokens": ["Denn", "er", "traf", "in", "al\u00b7ler", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz genau die gro\u00dfe Zehe.", "tokens": ["Ganz", "ge\u00b7nau", "die", "gro\u00b7\u00dfe", "Ze\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ohne jedes Schmerzgewimmer,", "tokens": ["Oh\u00b7ne", "je\u00b7des", "Schmerz\u00b7ge\u00b7wim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur mit Ruh, mit einer festen,", "tokens": ["Nur", "mit", "Ruh", ",", "mit", "ei\u00b7ner", "fes\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APP