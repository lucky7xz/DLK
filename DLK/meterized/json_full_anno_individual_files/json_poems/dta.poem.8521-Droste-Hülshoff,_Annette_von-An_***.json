{"dta.poem.8521": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An ***", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lebt wohl, es kann nicht anders sein,", "tokens": ["Lebt", "wohl", ",", "es", "kann", "nicht", "an\u00b7ders", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spannt flatternd eure Segel aus!", "tokens": ["Spannt", "flat\u00b7ternd", "eu\u00b7re", "Se\u00b7gel", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft mich in meinem Schlo\u00df allein,", "tokens": ["La\u00dft", "mich", "in", "mei\u00b7nem", "Schlo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem geisterhaften Haus.", "tokens": ["In", "mei\u00b7nem", "geis\u00b7ter\u00b7haf\u00b7ten", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lebt wohl und nehmt mein Herz mit euch", "tokens": ["Lebt", "wohl", "und", "nehmt", "mein", "Herz", "mit", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meinen letzten Sonnenstrahl,", "tokens": ["Und", "mei\u00b7nen", "letz\u00b7ten", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er scheide, scheide nur sogleich,", "tokens": ["Er", "schei\u00b7de", ",", "schei\u00b7de", "nur", "sog\u00b7leich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn scheiden mu\u00df er doch einmal.", "tokens": ["Denn", "schei\u00b7den", "mu\u00df", "er", "doch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00dft mich an meines Seees Bord,", "tokens": ["La\u00dft", "mich", "an", "mei\u00b7nes", "See\u00b7es", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich schaukelnd mit dem Wellenstrich,", "tokens": ["Mich", "schau\u00b7kelnd", "mit", "dem", "Wel\u00b7len\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein mit meinem Zauberwort,", "tokens": ["Al\u00b7lein", "mit", "mei\u00b7nem", "Zau\u00b7ber\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Alpengeist und meinem Ich.", "tokens": ["Dem", "Al\u00b7pen\u00b7geist", "und", "mei\u00b7nem", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verlassen, aber einsam nicht,", "tokens": ["Ver\u00b7las\u00b7sen", ",", "a\u00b7ber", "ein\u00b7sam", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ersch\u00fcttert, aber nicht erdr\u00fcckt,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", ",", "a\u00b7ber", "nicht", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lange noch das heil\u2019ge Licht", "tokens": ["So", "lan\u00b7ge", "noch", "das", "heil'\u00b7ge", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf mich mit Liebesaugen blickt.", "tokens": ["Auf", "mich", "mit", "Lie\u00b7be\u00b7sau\u00b7gen", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So lange mir der frische Wald", "tokens": ["So", "lan\u00b7ge", "mir", "der", "fri\u00b7sche", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesang aus jedem Blatte rauscht,", "tokens": ["Ge\u00b7sang", "aus", "je\u00b7dem", "Blat\u00b7te", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus jeder Klippe, jedem Spalt,", "tokens": ["Aus", "je\u00b7der", "Klip\u00b7pe", ",", "je\u00b7dem", "Spalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befreundet mir der Else lauscht.", "tokens": ["Be\u00b7freun\u00b7det", "mir", "der", "El\u00b7se", "lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So lange sich der Arm mir frei", "tokens": ["So", "lan\u00b7ge", "sich", "der", "Arm", "mir", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PRF", "ART", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und waltend noch zum Aether streckt,", "tokens": ["Und", "wal\u00b7tend", "noch", "zum", "A\u00b7e\u00b7ther", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und jedes wilden Geiers Schrei", "tokens": ["Und", "je\u00b7des", "wil\u00b7den", "Gei\u00b7ers", "Schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In mir die milde Muse weckt.", "tokens": ["In", "mir", "die", "mil\u00b7de", "Mu\u00b7se", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}