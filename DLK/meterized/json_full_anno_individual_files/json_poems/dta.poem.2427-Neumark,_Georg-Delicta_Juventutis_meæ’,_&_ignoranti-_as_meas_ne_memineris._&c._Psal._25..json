{"dta.poem.2427": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Delicta Juventutis me\u00e6\u2019, & ignoranti-  \n as meas ne memineris. &c.  Psal. 25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Du grosser Gnadengott ged\u00e4nk doch nicht der S\u00fcnden", "tokens": ["Du", "gros\u00b7ser", "Gna\u00b7den\u00b7gott", "ge\u00b7d\u00e4nk", "doch", "nicht", "der", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ich hab\u2019 armer Mensch/ von meiner Jugend", "tokens": ["Die", "ich", "hab'", "ar\u00b7mer", "Mensch", "/", "von", "mei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "an", "tokens": ["an"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Des Rechten unbewust offt wider dich gethan;", "tokens": ["Des", "Rech\u00b7ten", "un\u00b7be\u00b7wust", "offt", "wi\u00b7der", "dich", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dekk sie mit Gnaden zu und la\u00df dich g\u00fctig finden.", "tokens": ["Dekk", "sie", "mit", "Gna\u00b7den", "zu", "und", "la\u00df", "dich", "g\u00fc\u00b7tig", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "PTKVZ", "KON", "VVIMP", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich werde quit und lo\u00df von aller S\u00fcndenschuld/", "tokens": ["Ich", "wer\u00b7de", "quit", "und", "lo\u00df", "von", "al\u00b7ler", "S\u00fcn\u00b7den\u00b7schuld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn du mein Gott nur d\u00e4nkkst an deine Vaterhuld.", "tokens": ["Wenn", "du", "mein", "Gott", "nur", "d\u00e4nkkst", "an", "dei\u00b7ne", "Va\u00b7ter\u00b7huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}