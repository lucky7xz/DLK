{"textgrid.poem.54080": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der andre Mann", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lernst ihn in einer Gesellschaft kennen.", "tokens": ["Du", "lernst", "ihn", "in", "ei\u00b7ner", "Ge\u00b7sell\u00b7schaft", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er plaudert. Er ist zu dir nett.", "tokens": ["Er", "plau\u00b7dert", ".", "Er", "ist", "zu", "dir", "nett", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er kann dir alle Tenniscracks nennen.", "tokens": ["Er", "kann", "dir", "al\u00b7le", "Ten\u00b7nis\u00b7cracks", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Er sieht gut aus. Ohne Fett.", "tokens": ["Er", "sieht", "gut", "aus", ".", "Oh\u00b7ne", "Fett", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er tanzt ausgezeichnet. Du siehst ihn dir an . . .", "tokens": ["Er", "tanzt", "aus\u00b7ge\u00b7zeich\u00b7net", ".", "Du", "siehst", "ihn", "dir", "an", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct",