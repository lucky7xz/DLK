: ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Dann kennst du seine Nuancen beim Kosen;", "tokens": ["Dann", "kennst", "du", "sei\u00b7ne", "Nu\u00b7an\u00b7cen", "beim", "Ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dann kennst du ihn in Unterhosen;", "tokens": ["dann", "kennst", "du", "ihn", "in", "Un\u00b7ter\u00b7ho\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "dann wird er satt in deinem Besitze;", "tokens": ["dann", "wird", "er", "satt", "in", "dei\u00b7nem", "Be\u00b7sit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "dann kennst du alle seine Witze.", "tokens": ["dann", "kennst", "du", "al\u00b7le", "sei\u00b7ne", "Wit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dann siehst du ihn in Freude und Zorn,", "tokens": ["Dann", "siehst", "du", "ihn", "in", "Freu\u00b7de", "und", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "von oben und unten, von hinten und vorn . . .", "tokens": ["von", "o\u00b7ben", "und", "un\u