{"textgrid.poem.52708": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Blinde", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, ihr mit offenen Augen", "tokens": ["Ihr", ",", "ihr", "mit", "of\u00b7fe\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr kennet nicht meine Nacht!", "tokens": ["Ihr", "ken\u00b7net", "nicht", "mei\u00b7ne", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da ist Alles so finster so dunkel,", "tokens": ["Da", "ist", "Al\u00b7les", "so", "fins\u00b7ter", "so", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Kein freundliches Sternlein wacht.", "tokens": ["Kein", "freund\u00b7li\u00b7ches", "Stern\u00b7lein", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6ne, die liebliche Erde", "tokens": ["Die", "sch\u00f6\u00b7ne", ",", "die", "lieb\u00b7li\u00b7che", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich habe sie lang nicht erschaut,", "tokens": ["Ich", "ha\u00b7be", "sie", "lang", "nicht", "er\u00b7schaut", ","],