{"textgrid.poem.47383": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ermutigung zur \u00dcbersetzung der Hamasa", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Poesie in allen ihren Zungen", "tokens": ["Die", "Poe\u00b7sie", "in", "al\u00b7len", "ih\u00b7ren", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist dem Geweihten ", "tokens": ["Ist", "dem", "Ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Sprache, die im Paradies erklungen,", "tokens": ["Die", "Spra\u00b7che", ",", "die", "im", "Pa\u00b7ra\u00b7dies", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh' sie verwildert auf der wilden Flur.", "tokens": ["Eh'", "sie", "ver\u00b7wil\u00b7dert", "auf", "der", "wil\u00b7den", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wo sie nun auch sei hervorgedrungen,", "tokens": ["Doch", "wo", "sie", "nun", "auch", "sei", "her\u00b7vor\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von ihrem Ursprung tr\u00e4gt sie noch die Spur;", "tokens": ["Von", "ih\u00b7rem", "Ur\u00b7sprung", "tr\u00e4gt", "sie", "noch", "die", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ob sie dumpf im W\u00fcstenglutwind st\u00f6hne,", "tokens": ["Und", "ob", "sie", "dumpf", "im", "W\u00fcs\u00b7ten\u00b7glut\u00b7wind", "st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es sind auch hier des Paradieses T\u00f6ne.", "tokens": ["Es", "sind", "auch", "hier", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Poesie hat hier ein d\u00fcrft'ges Leben,", "tokens": ["Die", "Poe\u00b7sie", "hat", "hier", "ein", "d\u00fcrft'\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei durst'gen Herden im entbrannten Sand,", "tokens": ["Bei", "dur\u00b7st'\u00b7gen", "Her\u00b7den", "im", "ent\u00b7brann\u00b7ten", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Mit Bl\u00fctenschmuck und Schattenduft umgeben,", "tokens": ["Mit", "Bl\u00fc\u00b7ten\u00b7schmuck", "und", "Schat\u00b7ten\u00b7duft", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Abendtau gel\u00f6scht den Mittagsbrand,", "tokens": ["Mit", "A\u00b7bend\u00b7tau", "ge\u00b7l\u00f6scht", "den", "Mit\u00b7tags\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Versch\u00f6nt, vers\u00f6hnt ein leidenschaftlich Streben", "tokens": ["Ver\u00b7sch\u00f6nt", ",", "ver\u00b7s\u00f6hnt", "ein", "lei\u00b7den\u00b7schaft\u00b7lich", "Stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durchs Hochgef\u00fchl von Sprach- und Stammverband,", "tokens": ["Durchs", "Hoch\u00b7ge\u00b7f\u00fchl", "von", "Sprach", "und", "Stamm\u00b7ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und in das Schlachtgrau'n Liebe selbst gewoben,", "tokens": ["Und", "in", "das", "Schlacht\u00b7grau'n", "Lie\u00b7be", "selbst", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die hier auch ist, wie \u00fcberall, von oben.", "tokens": ["Die", "hier", "auch", "ist", ",", "wie", "\u00fc\u00b7be\u00b7rall", ",", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "$,", "PWAV", "ADV", "$,", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer aber soll die nord'sche Nacht erheitern", "tokens": ["Wer", "a\u00b7ber", "soll", "die", "nord'\u00b7sche", "Nacht", "er\u00b7hei\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit solchem Abglanz von des S\u00fcdens Glut?", "tokens": ["Mit", "sol\u00b7chem", "Ab\u00b7glanz", "von", "des", "S\u00fc\u00b7dens", "Glut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer den Gesichtskreis meines Volks erweitern,", "tokens": ["Wer", "den", "Ge\u00b7sichts\u00b7kreis", "mei\u00b7nes", "Volks", "er\u00b7wei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df seinem Blick auf jene Welt sich thut?", "tokens": ["Da\u00df", "sei\u00b7nem", "Blick", "auf", "je\u00b7ne", "Welt", "sich", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PDAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das enge Leben freilich geht zu scheitern,", "tokens": ["Das", "en\u00b7ge", "Le\u00b7ben", "frei\u00b7lich", "geht", "zu", "schei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Je mehr hereinstr\u00f6mt diese Geisterflut;", "tokens": ["Je", "mehr", "her\u00b7ein\u00b7str\u00f6mt", "die\u00b7se", "Geis\u00b7ter\u00b7flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Doch, soll der Ost einmal zum Westen dringen,", "tokens": ["Doch", ",", "soll", "der", "Ost", "ein\u00b7mal", "zum", "Wes\u00b7ten", "drin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer ist der Mann, ihn ganz heranzubringen?", "tokens": ["Wer", "ist", "der", "Mann", ",", "ihn", "ganz", "her\u00b7an\u00b7zu\u00b7brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Darum nur mutvoll vorw\u00e4rts, auszubeuten", "tokens": ["Da\u00b7rum", "nur", "mut\u00b7voll", "vor\u00b7w\u00e4rts", ",", "aus\u00b7zu\u00b7beu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "ADV", "ADJD", "PTKVZ", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den spr\u00f6den Schacht, den nicht erw\u00fchlt ein Scherz,", "tokens": ["Den", "spr\u00f6\u00b7den", "Schacht", ",", "den", "nicht", "er\u00b7w\u00fchlt", "ein", "Scherz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das fremde Leben deinem Volk zu deuten,", "tokens": ["Das", "frem\u00b7de", "Le\u00b7ben", "dei\u00b7nem", "Volk", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ohne dich ihm bliebe taubes Erz.", "tokens": ["Das", "oh\u00b7ne", "dich", "ihm", "blie\u00b7be", "tau\u00b7bes", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wann erst der Menschheit Glieder, die zerstreuten,", "tokens": ["Wann", "erst", "der", "Menschheit", "Glie\u00b7der", ",", "die", "zer\u00b7streu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gesammelt sind ans europ\u00e4ische Herz,", "tokens": ["Ge\u00b7sam\u00b7melt", "sind", "ans", "eu\u00b7ro\u00b7p\u00e4\u00b7i\u00b7sche", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wird sein ein neues Paradies gewonnen,", "tokens": ["Wird", "sein", "ein", "neu\u00b7es", "Pa\u00b7ra\u00b7dies", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So gut es bl\u00fchn kann unterm Strahl der Sonnen.", "tokens": ["So", "gut", "es", "bl\u00fchn", "kann", "un\u00b7term", "Strahl", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und la\u00df dich nicht im edlen Tagwerk irren", "tokens": ["Und", "la\u00df", "dich", "nicht", "im", "ed\u00b7len", "Tag\u00b7werk", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Sch\u00fclern, die nur meistern meisterlich,", "tokens": ["Von", "Sch\u00fc\u00b7lern", ",", "die", "nur", "meis\u00b7tern", "meis\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die in des Worts zerr\u00fctteten Geschirren", "tokens": ["Die", "in", "des", "Worts", "zer\u00b7r\u00fct\u00b7te\u00b7ten", "Ge\u00b7schir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Geist versch\u00fctten, aber trau' auf mich,", "tokens": ["Den", "Geist", "ver\u00b7sch\u00fct\u00b7ten", ",", "a\u00b7ber", "trau'", "auf", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu sammeln rein den Hauch arabischer Myrrhen,", "tokens": ["Zu", "sam\u00b7meln", "rein", "den", "Hauch", "a\u00b7ra\u00b7bi\u00b7scher", "Myr\u00b7rhen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Geweiht zu meinem Priester hab' ich dich,", "tokens": ["Ge\u00b7weiht", "zu", "mei\u00b7nem", "Pries\u00b7ter", "hab'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Komm, mir im deutschen Pantheon zu r\u00e4uchern", "tokens": ["Komm", ",", "mir", "im", "deut\u00b7schen", "Pan\u00b7the\u00b7on", "zu", "r\u00e4u\u00b7chern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "APPRART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und la\u00df die trockne Spreu den trocknen Keuchern!", "tokens": ["Und", "la\u00df", "die", "trock\u00b7ne", "Spreu", "den", "trock\u00b7nen", "Keuc\u00b7hern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Poesie in allen ihren Zungen", "tokens": ["Die", "Poe\u00b7sie", "in", "al\u00b7len", "ih\u00b7ren", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist dem Geweihten ", "tokens": ["Ist", "dem", "Ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Sprache, die im Paradies erklungen,", "tokens": ["Die", "Spra\u00b7che", ",", "die", "im", "Pa\u00b7ra\u00b7dies", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", 