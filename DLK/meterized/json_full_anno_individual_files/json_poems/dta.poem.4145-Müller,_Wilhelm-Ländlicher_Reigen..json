{"dta.poem.4145": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndlicher Reigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' manch Herz gefangen", "tokens": ["Ich", "hab'", "manch", "Herz", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in dem Erntetanz:", "tokens": ["Wohl", "in", "dem", "Ern\u00b7te\u00b7tanz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "All' Jahr ein frisches Herzchen,", "tokens": ["All'", "Jahr", "ein", "fri\u00b7sches", "Herz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "All' Jahr ein frischer Kranz!", "tokens": ["All'", "Jahr", "ein", "fri\u00b7scher", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wem das nicht behaget,", "tokens": ["Und", "wem", "das", "nicht", "be\u00b7ha\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der seh' dem Tanze zu;", "tokens": ["Der", "seh'", "dem", "Tan\u00b7ze", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mag er's Herz behalten,", "tokens": ["So", "mag", "er's", "Herz", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dazu auch ganze Schuh'.", "tokens": ["Da\u00b7zu", "auch", "gan\u00b7ze", "Schuh'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}