{"dta.poem.15896": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh nur, wer sind sie denn, die nach dem Ziel hier laufen", "tokens": ["Sieh", "nur", ",", "wer", "sind", "sie", "denn", ",", "die", "nach", "dem", "Ziel", "hier", "lau\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schnaufen, da\u00df du dich willst mischen in den Haufen?", "tokens": ["Und", "schnau\u00b7fen", ",", "da\u00df", "du", "dich", "willst", "mi\u00b7schen", "in", "den", "Hau\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sieh nur, wer sind sie denn, die hier den Preis vertheilen,", "tokens": ["Sieh", "nur", ",", "wer", "sind", "sie", "denn", ",", "die", "hier", "den", "Preis", "ver\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was denn ist von Ruhm das Reis, das sie ertheilen?", "tokens": ["Und", "was", "denn", "ist", "von", "Ruhm", "das", "Reis", ",", "das", "sie", "er\u00b7thei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VAFIN", "APPR", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erkenne dich! der Ruhm ist hier, da\u00df du besiegst", "tokens": ["Er\u00b7ken\u00b7ne", "dich", "!", "der", "Ruhm", "ist", "hier", ",", "da\u00df", "du", "be\u00b7siegst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unw\u00fcrd'gen Ehrgeiz und nicht mit Unw\u00fcrd'gen kriegst.", "tokens": ["Un\u00b7w\u00fcrd'\u00b7gen", "Ehr\u00b7geiz", "und", "nicht", "mit", "Un\u00b7w\u00fcrd'\u00b7gen", "kriegst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}