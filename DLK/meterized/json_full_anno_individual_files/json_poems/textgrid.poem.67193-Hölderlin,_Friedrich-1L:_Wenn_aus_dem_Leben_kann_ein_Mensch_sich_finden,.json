{"textgrid.poem.67193": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn aus dem Leben kann ein Mensch sich finden,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus dem Leben kann ein Mensch sich finden,", "tokens": ["Wenn", "aus", "dem", "Le\u00b7ben", "kann", "ein", "Mensch", "sich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und das begreifen, wie das Leben sich empfindet,", "tokens": ["Und", "das", "be\u00b7grei\u00b7fen", ",", "wie", "das", "Le\u00b7ben", "sich", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist es gut; wer aus Gefahr sich windet,", "tokens": ["So", "ist", "es", "gut", ";", "wer", "aus", "Ge\u00b7fahr", "sich", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PWS", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist wie ein Mensch, der kommt aus Sturm und Winden.", "tokens": ["Ist", "wie", "ein", "Mensch", ",", "der", "kommt", "aus", "Sturm", "und", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch besser ists, die Sch\u00f6nheit auch zu kennen,", "tokens": ["Doch", "bes\u00b7ser", "ists", ",", "die", "Sch\u00f6n\u00b7heit", "auch", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einrichtung, die Erhabenheit des ganzen Lebens,", "tokens": ["Ein\u00b7rich\u00b7tung", ",", "die", "Er\u00b7ha\u00b7ben\u00b7heit", "des", "gan\u00b7zen", "Le\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn Freude kommt aus M\u00fche des Bestrebens,", "tokens": ["Wenn", "Freu\u00b7de", "kommt", "aus", "M\u00fc\u00b7he", "des", "Be\u00b7stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie die G\u00fcter all in dieser Zeit sich nennen.", "tokens": ["Und", "wie", "die", "G\u00fc\u00b7ter", "all", "in", "die\u00b7ser", "Zeit", "sich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PIAT", "APPR", "PDAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Baum, der gr\u00fcnt, die Gipfel von Gezweigen,", "tokens": ["Der", "Baum", ",", "der", "gr\u00fcnt", ",", "die", "Gip\u00b7fel", "von", "Ge\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blumen, die des Stammes Rind' umgeben,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "des", "Stam\u00b7mes", "Rind'", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind aus der g\u00f6ttlichen Natur, sie sind ein Leben,", "tokens": ["Sind", "aus", "der", "g\u00f6tt\u00b7li\u00b7chen", "Na\u00b7tur", ",", "sie", "sind", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil \u00fcber dieses sich des Himmels L\u00fcfte neigen.", "tokens": ["Weil", "\u00fc\u00b7ber", "die\u00b7ses", "sich", "des", "Him\u00b7mels", "L\u00fcf\u00b7te", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "PRF", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn aber mich neugierge Menschen fragen,", "tokens": ["Wenn", "a\u00b7ber", "mich", "neu\u00b7gier\u00b7ge", "Men\u00b7schen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was dieses sei, sich f\u00fcr Empfindung wagen,", "tokens": ["Was", "die\u00b7ses", "sei", ",", "sich", "f\u00fcr", "Emp\u00b7fin\u00b7dung", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VAFIN", "$,", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was die Bestimmung sei, das H\u00f6chste, das Gewinnen,", "tokens": ["Was", "die", "Be\u00b7stim\u00b7mung", "sei", ",", "das", "H\u00f6chs\u00b7te", ",", "das", "Ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "ART", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sag ich, das ist es, das Leben, wie das Sinnen.", "tokens": ["So", "sag", "ich", ",", "das", "ist", "es", ",", "das", "Le\u00b7ben", ",", "wie", "das", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Wen die Natur gew\u00f6hnlich, ruhig machet,", "tokens": ["Wen", "die", "Na\u00b7tur", "ge\u00b7w\u00f6hn\u00b7lich", ",", "ru\u00b7hig", "ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er mahnet mich, den Menschen froh zu leben,", "tokens": ["Er", "mah\u00b7net", "mich", ",", "den", "Men\u00b7schen", "froh", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warum? die Klarheit ists, vor der auch Weise beben,", "tokens": ["Wa\u00b7rum", "?", "die", "Klar\u00b7heit", "ists", ",", "vor", "der", "auch", "Wei\u00b7se", "be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "NN", "VAFIN", "$,", "APPR", "PRELS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Freudigkeit ist sch\u00f6n, wenn alles scherzt und lachet.", "tokens": ["Die", "Freu\u00b7dig\u00b7keit", "ist", "sch\u00f6n", ",", "wenn", "al\u00b7les", "scherzt", "und", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PIS", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der M\u00e4nner Ernst, der Sieg und die Gefahren,", "tokens": ["Der", "M\u00e4n\u00b7ner", "Ernst", ",", "der", "Sieg", "und", "die", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kommen aus Gebildetheit, und aus Gewahren,", "tokens": ["Sie", "kom\u00b7men", "aus", "Ge\u00b7bil\u00b7det\u00b7heit", ",", "und", "aus", "Ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es geb ein Ziel; das Hohe von den Besten", "tokens": ["Es", "geb", "ein", "Ziel", ";", "das", "Ho\u00b7he", "von", "den", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erkennt sich an dem Sein, und sch\u00f6nen \u00dcberresten.", "tokens": ["Er\u00b7kennt", "sich", "an", "dem", "Sein", ",", "und", "sch\u00f6\u00b7nen", "\u00dc\u00b7ber\u00b7res\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie selber aber sind, wie Auserw\u00e4hlte,", "tokens": ["Sie", "sel\u00b7ber", "a\u00b7ber", "sind", ",", "wie", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihnen ist das Neue, das Erz\u00e4hlte,", "tokens": ["Von", "ih\u00b7nen", "ist", "das", "Neu\u00b7e", ",", "das", "Er\u00b7z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wirklichkeit der Taten geht nicht unter,", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", "der", "Ta\u00b7ten", "geht", "nicht", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Sterne gl\u00e4nzen, gibts ein Leben gro\u00df und munter.", "tokens": ["Wie", "Ster\u00b7ne", "gl\u00e4n\u00b7zen", ",", "gibts", "ein", "Le\u00b7ben", "gro\u00df", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Das Leben ist aus Taten und verwegen,", "tokens": ["Das", "Le\u00b7ben", "ist", "aus", "Ta\u00b7ten", "und", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein hohes Ziel, gehalteners Bewegen,", "tokens": ["Ein", "ho\u00b7hes", "Ziel", ",", "ge\u00b7hal\u00b7te\u00b7ners", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gang und Schritt, doch Seligkeit aus Tugend", "tokens": ["Der", "Gang", "und", "Schritt", ",", "doch", "Se\u00b7lig\u00b7keit", "aus", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gro\u00dfer Ernst, und dennoch lautre Jugend.", "tokens": ["Und", "gro\u00b7\u00dfer", "Ernst", ",", "und", "den\u00b7noch", "laut\u00b7re", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die Reu, und die Vergangenheit in diesem Leben", "tokens": ["Die", "Reu", ",", "und", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "KON", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind ein verschiednes Sein, die eine gl\u00fccket", "tokens": ["Sind", "ein", "ver\u00b7schied\u00b7nes", "Sein", ",", "die", "ei\u00b7ne", "gl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu Ruhm und Ruh, und allem, was entr\u00fccket,", "tokens": ["Zu", "Ruhm", "und", "Ruh", ",", "und", "al\u00b7lem", ",", "was", "ent\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KON", "PIS", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu hohen Regionen, die gegeben;", "tokens": ["Zu", "ho\u00b7hen", "Re\u00b7gi\u00b7o\u00b7nen", ",", "die", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die andre f\u00fchrt zu Qual, und bittern Schmerzen,", "tokens": ["Die", "and\u00b7re", "f\u00fchrt", "zu", "Qual", ",", "und", "bit\u00b7tern", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Menschen untergehn, die mit dem Leben scherzen,", "tokens": ["Wenn", "Men\u00b7schen", "un\u00b7ter\u00b7gehn", ",", "die", "mit", "dem", "Le\u00b7ben", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und das Gebild und Antlitz sich verwandelt", "tokens": ["Und", "das", "Ge\u00b7bild", "und", "Ant\u00b7litz", "sich", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von einem, der nicht gut und sch\u00f6n gehandelt.", "tokens": ["Von", "ei\u00b7nem", ",", "der", "nicht", "gut", "und", "sch\u00f6n", "ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PTKNEG", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Die Sichtbarkeit lebendiger Gestalt, das W\u00e4hren", "tokens": ["Die", "Sicht\u00b7bar\u00b7keit", "le\u00b7ben\u00b7di\u00b7ger", "Ge\u00b7stalt", ",", "das", "W\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser Zeit, wie Menschen sich ern\u00e4hren,", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "wie", "Men\u00b7schen", "sich", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist fast ein Zwist, der lebet der Empfindung,", "tokens": ["Ist", "fast", "ein", "Zwist", ",", "der", "le\u00b7bet", "der", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der andre strebt nach M\u00fchen und Erfindung.", "tokens": ["Der", "and\u00b7re", "strebt", "nach", "M\u00fc\u00b7hen", "und", "Er\u00b7fin\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wenn aus dem Leben kann ein Mensch sich finden,", "tokens": ["Wenn", "aus", "dem", "Le\u00b7ben", "kann", "ein", "Mensch", "sich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und das begreifen, wie das Leben sich empfindet,", "tokens": ["Und", "das", "be\u00b7grei\u00b7fen", ",", "wie", "das", "Le\u00b7ben", "sich", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist es gut; wer aus Gefahr sich windet,", "tokens": ["So", "ist", "es", "gut", ";", "wer", "aus", "Ge\u00b7fahr", "sich", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PWS", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist wie ein Mensch, der kommt aus Sturm und Winden.", "tokens": ["Ist", "wie", "e