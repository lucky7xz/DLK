{"dta.poem.15932": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "137.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht leicht vergeht ein Tag, an dem nicht was geschah,", "tokens": ["Nicht", "leicht", "ver\u00b7geht", "ein", "Tag", ",", "an", "dem", "nicht", "was", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PTKNEG", "PWS", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das herzlich mich erfreut, wenn ich es recht besah.", "tokens": ["Das", "herz\u00b7lich", "mich", "er\u00b7freut", ",", "wenn", "ich", "es", "recht", "be\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn einer doch vergieng, an dem mir nichts des neuen", "tokens": ["Wenn", "ei\u00b7ner", "doch", "ver\u00b7gieng", ",", "an", "dem", "mir", "nichts", "des", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,", "APPR", "PRELS", "PPER", "PIS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erfreulichen geschehn, da mu\u00df mich altes freuen.", "tokens": ["Er\u00b7freu\u00b7li\u00b7chen", "ge\u00b7schehn", ",", "da", "mu\u00df", "mich", "al\u00b7tes", "freu\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos