{"dta.poem.21174": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zweyte Ode .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum dringt durch die lange Nacht", "tokens": ["Wa\u00b7rum", "dringt", "durch", "die", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein zweifelhafter Stral?", "tokens": ["Ein", "zwei\u00b7fel\u00b7haf\u00b7ter", "Stral", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Hofnung, Hofnung! t\u00e4usche nicht", "tokens": ["O", "Hof\u00b7nung", ",", "Hof\u00b7nung", "!", "t\u00e4u\u00b7sche", "nicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$.", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ungl\u00fcckseligs Herz!", "tokens": ["Ein", "un\u00b7gl\u00fcck\u00b7se\u00b7ligs", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "La\u00df mich in tiefer Traurigkeit,", "tokens": ["La\u00df", "mich", "in", "tie\u00b7fer", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der die Seele stirbt!", "tokens": ["In", "der", "die", "See\u00b7le", "stirbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verzweiflung selbst ist Trost f\u00fcr mich,", "tokens": ["Ver\u00b7zwei\u00b7flung", "selbst", "ist", "Trost", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wofern du mich betr\u00fcgst.", "tokens": ["Wo\u00b7fern", "du", "mich", "be\u00b7tr\u00fcgst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu grausam! \u2014 dennoch lispelst du", "tokens": ["Zu", "grau\u00b7sam", "!", "den\u00b7noch", "lis\u00b7pelst", "du"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$.", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem bangen Herzen ein:", "tokens": ["Dem", "ban\u00b7gen", "Her\u00b7zen", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sey vielleicht \u2014 vielleicht geliebt;", "tokens": ["Ich", "sey", "viel\u00b7leicht", "viel\u00b7leicht", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O niedriger Verrath!", "tokens": ["O", "nied\u00b7ri\u00b7ger", "Ver\u00b7rath", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Meynst du, der schimmernde Betrug", "tokens": ["Meynst", "du", ",", "der", "schim\u00b7mern\u00b7de", "Be\u00b7trug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll Kraft dem Herzen leihn?", "tokens": ["Soll", "Kraft", "dem", "Her\u00b7zen", "leihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mehr gl\u00fccklich war es, ganz durchbohrt,", "tokens": ["Mehr", "gl\u00fcck\u00b7lich", "war", "es", ",", "ganz", "durch\u00b7bohrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz, o Verzweiflung, dein.", "tokens": ["Ganz", ",", "o", "Ver\u00b7zwei\u00b7flung", ",", "dein", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Umsonst, umsonst! \u2014 Voll Grausamkeit", "tokens": ["Um\u00b7sonst", ",", "um\u00b7sonst", "!", "Voll", "Grau\u00b7sam\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "$.", "$(", "ADJD", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Bet\u00e4ubest du den Schmerz.", "tokens": ["Be\u00b7t\u00e4u\u00b7best", "du", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbinde meine Wunden dann,", "tokens": ["Ver\u00b7bin\u00b7de", "mei\u00b7ne", "Wun\u00b7den", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rei\u00df sie blutger auf!", "tokens": ["Und", "rei\u00df", "sie", "blut\u00b7ger", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}