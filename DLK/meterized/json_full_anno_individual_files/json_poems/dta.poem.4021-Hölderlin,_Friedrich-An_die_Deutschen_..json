{"dta.poem.4021": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Deutschen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Spottet ja nicht des Kind's, wenn es mit Peitsch'", "tokens": ["Spot\u00b7tet", "ja", "nicht", "des", "Kin\u00b7d's", ",", "wenn", "es", "mit", "Peit\u00b7sch'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "und Sporn,", "tokens": ["und", "Sporn", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Auf dem Rosse von Holz, muthig und gro\u00df sich", "tokens": ["Auf", "dem", "Ros\u00b7se", "von", "Holz", ",", "mut\u00b7hig", "und", "gro\u00df", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "PRF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "d\u00fcnkt.", "tokens": ["d\u00fcnkt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Denn, ihr Deutschen, auch ihr seyd", "tokens": ["Denn", ",", "ihr", "Deut\u00b7schen", ",", "auch", "ihr", "seyd"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Thatenarm und gedankenvoll.", "tokens": ["Tha\u00b7ten\u00b7arm", "und", "ge\u00b7dan\u00b7ken\u00b7voll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Oder k\u00f6mmt, wie der Stral aus dem Gew\u00f6lke", "tokens": ["O\u00b7der", "k\u00f6mmt", ",", "wie", "der", "Stral", "aus", "dem", "Ge\u00b7w\u00f6l\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "k\u00f6mmt,", "tokens": ["k\u00f6mmt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Aus Gedanken die That? Leben die B\u00fccher bald?", "tokens": ["Aus", "Ge\u00b7dan\u00b7ken", "die", "That", "?", "Le\u00b7ben", "die", "B\u00fc\u00b7cher", "bald", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$.", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "O ihr Lieben! so nehmt mich,", "tokens": ["O", "ihr", "Lie\u00b7ben", "!", "so", "nehmt", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich b\u00fc\u00dfe die L\u00e4sterung!", "tokens": ["Da\u00df", "ich", "b\u00fc\u00b7\u00dfe", "die", "L\u00e4s\u00b7te\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}