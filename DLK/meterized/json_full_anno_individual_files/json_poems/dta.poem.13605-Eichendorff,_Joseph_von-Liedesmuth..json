{"dta.poem.13605": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Liedesmuth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was Lorbeerkranz und Lobestand!", "tokens": ["Was", "Lor\u00b7beer\u00b7kranz", "und", "Lo\u00b7be\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es duftet still die Fr\u00fchlingsnacht", "tokens": ["Es", "duf\u00b7tet", "still", "die", "Fr\u00fch\u00b7lings\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rauscht der Wald vom Felsenrand,", "tokens": ["Und", "rauscht", "der", "Wald", "vom", "Fel\u00b7sen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob's jemand h\u00f6rt, ob niemand wacht.", "tokens": ["Ob's", "je\u00b7mand", "h\u00f6rt", ",", "ob", "nie\u00b7mand", "wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schl\u00e4ft noch alles Menschenkind,", "tokens": ["Es", "schl\u00e4ft", "noch", "al\u00b7les", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da pfeift sein lust'ges Wanderlied", "tokens": ["Da", "pfeift", "sein", "lust'\u00b7ges", "Wan\u00b7der\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon \u00fcber's Feld der Morgenwind", "tokens": ["Schon", "\u00fc\u00b7ber's", "Feld", "der", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fr\u00e4gt nicht erst, wer mit ihm zieht.", "tokens": ["Und", "fr\u00e4gt", "nicht", "erst", ",", "wer", "mit", "ihm", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "PWS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ob ihr all' zu Hause sa\u00dft,", "tokens": ["Und", "ob", "ihr", "all'", "zu", "Hau\u00b7se", "sa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling bl\u00fcht doch, weil er mu\u00df,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "bl\u00fcht", "doch", ",", "weil", "er", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob ihr's les't oder bleiben la\u00dft,", "tokens": ["Und", "ob", "ih\u00b7r's", "les't", "o\u00b7der", "blei\u00b7ben", "la\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich singe doch aus frischer Brust.", "tokens": ["Ich", "sin\u00b7ge", "doch", "aus", "fri\u00b7scher", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}