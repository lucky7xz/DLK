{"textgrid.poem.55323": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Atmosph\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie Welt, sie ist so gro\u00df und breit,", "tokens": ["\u00bb", "die", "Welt", ",", "sie", "ist", "so", "gro\u00df", "und", "breit", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel auch so hehr und weit,", "tokens": ["Der", "Him\u00b7mel", "auch", "so", "hehr", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df das alles mit Augen fassen,", "tokens": ["Ich", "mu\u00df", "das", "al\u00b7les", "mit", "Au\u00b7gen", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Will sich aber nicht recht denken lassen.\u00ab", "tokens": ["Will", "sich", "a\u00b7ber", "nicht", "recht", "den\u00b7ken", "las\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PRF", "ADV", "PTKNEG", "ADJD", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dich im Unendlichen zu finden,", "tokens": ["Dich", "im", "Un\u00b7end\u00b7li\u00b7chen", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mu\u00dft unterscheiden und dann verbinden;", "tokens": ["Mu\u00dft", "un\u00b7ter\u00b7schei\u00b7den", "und", "dann", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drum danket mein befl\u00fcgelt Lied", "tokens": ["Drum", "dan\u00b7ket", "mein", "be\u00b7fl\u00fc\u00b7gelt", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Manne, der Wolken unterschied.", "tokens": ["Dem", "Man\u00b7ne", ",", "der", "Wol\u00b7ken", "un\u00b7ter\u00b7schied", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbdie Welt, sie ist so gro\u00df und breit,", "tokens": ["\u00bb", "die", "Welt", ",", "sie", "ist", "so", "gro\u00df", "und", "breit", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel auch so hehr und weit,", "tokens": ["Der", "Him\u00b7mel", "auch", "so", "hehr", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df das alles mit Augen fassen,", "tokens": ["Ich", "mu\u00df", "das", "al\u00b7les", "mit", "Au\