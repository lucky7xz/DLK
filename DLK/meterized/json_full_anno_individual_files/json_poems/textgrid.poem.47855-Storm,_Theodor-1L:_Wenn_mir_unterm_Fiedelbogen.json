{"textgrid.poem.47855": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn mir unterm Fiedelbogen", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mir unterm Fiedelbogen", "tokens": ["Wenn", "mir", "un\u00b7term", "Fie\u00b7del\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manche Saite auch zersprang,", "tokens": ["Man\u00b7che", "Sai\u00b7te", "auch", "zer\u00b7sprang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neue werden aufgezogen,", "tokens": ["Neu\u00b7e", "wer\u00b7den", "auf\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie geben frischen Klang.", "tokens": ["Und", "sie", "ge\u00b7ben", "fri\u00b7schen", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem Sch\u00fctzenplatz am Tore", "tokens": ["Auf", "dem", "Sch\u00fct\u00b7zen\u00b7platz", "am", "To\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strich ich leis mein Spielwerk an;", "tokens": ["Strich", "ich", "leis", "mein", "Spiel\u00b7werk", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie gleich die K\u00f6pfe wandten,", "tokens": ["Wie", "sie", "gleich", "die", "K\u00f6p\u00b7fe", "wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da ich eben nur begann!", "tokens": ["Da", "ich", "e\u00b7ben", "nur", "be\u00b7gann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es t\u00f6nt und schwillt und rauschet,", "tokens": ["Und", "es", "t\u00f6nt", "und", "schwillt", "und", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im Sturz der Waldesbach;", "tokens": ["Wie", "im", "Sturz", "der", "Wal\u00b7des\u00b7bach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Seele singt die Weise,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "singt", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Geige klingt sie nach.", "tokens": ["Mei\u00b7ne", "Gei\u00b7ge", "klingt", "sie", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Trotzig hadern noch die Burschen;", "tokens": ["Trot\u00b7zig", "ha\u00b7dern", "noch", "die", "Bur\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald doch wird es still im Kreis;", "tokens": ["Bald", "doch", "wird", "es", "still", "im", "Kreis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erst ein Raunen, dann ein Schweigen,", "tokens": ["Erst", "ein", "Rau\u00b7nen", ",", "dann", "ein", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst die B\u00e4ume s\u00e4useln leis.", "tokens": ["Selbst", "die", "B\u00e4u\u00b7me", "s\u00e4u\u00b7seln", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zauber hat sie all befangen;", "tokens": ["Zau\u00b7ber", "hat", "sie", "all", "be\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich wei\u00df, wie das geschah!", "tokens": ["Und", "ich", "wei\u00df", ",", "wie", "das", "ge\u00b7schah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort im Kranz der blonden Frauen", "tokens": ["Dort", "im", "Kranz", "der", "blon\u00b7den", "Frau\u00b7en"], "token_info": ["wor