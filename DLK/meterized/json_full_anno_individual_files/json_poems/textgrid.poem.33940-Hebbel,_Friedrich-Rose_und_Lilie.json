{"textgrid.poem.33940": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Rose und Lilie", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Rose liebt die Lilie,", "tokens": ["Die", "Ro\u00b7se", "liebt", "die", "Li\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie steht zu ihren F\u00fc\u00dfen;", "tokens": ["Sie", "steht", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald l\u00f6s't die Glut ihr sch\u00f6nstes Blatt,", "tokens": ["Bald", "l\u00f6s't", "die", "Glut", "ihr", "sch\u00f6ns\u00b7tes", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es f\u00e4llt, um sie zu gr\u00fc\u00dfen.", "tokens": ["Es", "f\u00e4llt", ",", "um", "sie", "zu", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Lilie bemerkt es wohl,", "tokens": ["Die", "Li\u00b7lie", "be\u00b7merkt", "es", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie h\u00e4tt' das Bl\u00e4ttlein gerne;", "tokens": ["Sie", "h\u00e4tt'", "das", "Bl