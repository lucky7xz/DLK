{"dta.poem.19526": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Venedig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Venedig, einen Winter lebt' ich dort \u2014", "tokens": ["Ve\u00b7ne\u00b7dig", ",", "ei\u00b7nen", "Win\u00b7ter", "lebt'", "ich", "dort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Pal\u00e4ste, Br\u00fccken, der Lagune Duft!", "tokens": ["Pa\u00b7l\u00e4s\u00b7te", ",", "Br\u00fc\u00b7cken", ",", "der", "La\u00b7gu\u00b7ne", "Duft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch hier im harten Licht der Gegenwart", "tokens": ["Doch", "hier", "im", "har\u00b7ten", "Licht", "der", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verd\u00e4mmert m\u00e4lig mir die M\u00e4rchenwelt.", "tokens": ["Ver\u00b7d\u00e4m\u00b7mert", "m\u00e4\u00b7lig", "mir", "die", "M\u00e4r\u00b7chen\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vielleicht verga\u00df ich einen Tizian.", "tokens": ["Viel\u00b7leicht", "ver\u00b7ga\u00df", "ich", "ei\u00b7nen", "Ti\u00b7zi\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Frevel! Jenen doch verga\u00df ich nicht,", "tokens": ["Ein", "Fre\u00b7vel", "!", "Je\u00b7nen", "doch", "ver\u00b7ga\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo \u00fcber einem Sturm von Armen sich", "tokens": ["Wo", "\u00fc\u00b7ber", "ei\u00b7nem", "Sturm", "von", "Ar\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Jungfrau feurig in die Himmel hebt,", "tokens": ["Die", "Jung\u00b7frau", "feu\u00b7rig", "in", "die", "Him\u00b7mel", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So wenig als den andern Tizian \u2014", "tokens": ["So", "we\u00b7nig", "als", "den", "an\u00b7dern", "Ti\u00b7zi\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch kein gemalter war's \u2014 die Wirklichkeit:", "tokens": ["Doch", "kein", "ge\u00b7mal\u00b7ter", "wa\u00b7r's", "die", "Wirk\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VAFIN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Am Quai, dem n\u00e4cht'gen, der Slavonen war's.", "tokens": ["Am", "Qua\u00b7i", ",", "dem", "n\u00e4cht'\u00b7gen", ",", "der", "Sla\u00b7vo\u00b7nen", "wa\u00b7r'", "s."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN", "VAFIN", "NE"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Im Dunkel stand ich. Fenster schimmerten.", "tokens": ["Im", "Dun\u00b7kel", "stand", "ich", ".", "Fens\u00b7ter", "schim\u00b7mer\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zwei d\u00fcrft'ge Frauen kamen hergerannt.", "tokens": ["Zwei", "d\u00fcrft'\u00b7ge", "Frau\u00b7en", "ka\u00b7men", "her\u00b7ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Hart an die Scheibe pre\u00dft' das junge Weib", "tokens": ["Hart", "an", "die", "Schei\u00b7be", "pre\u00dft'", "das", "jun\u00b7ge", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die bleiche Stirn. Was drinnen sie erblickt,", "tokens": ["Die", "blei\u00b7che", "Stirn", ".", "Was", "drin\u00b7nen", "sie", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das sie erstarren machte, wei\u00df ich nicht.", "tokens": ["Das", "sie", "er\u00b7star\u00b7ren", "mach\u00b7te", ",", "wei\u00df", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "(vielleicht den Herzgeliebten, welcher sie", "tokens": ["(", "viel\u00b7leicht", "den", "Herz\u00b7ge\u00b7lieb\u00b7ten", ",", "wel\u00b7cher", "sie"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "An eines andern Weibes Brust verrieth.)", "tokens": ["An", "ei\u00b7nes", "an\u00b7dern", "Wei\u00b7bes", "Brust", "ver\u00b7rieth", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ich aber sah den feinsten M\u00e4dchenkopf", "tokens": ["Ich", "a\u00b7ber", "sah", "den", "feins\u00b7ten", "M\u00e4d\u00b7chen\u00b7kopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Vom Tod entf\u00e4rbt! Ein Antlitz voller Tod!", "tokens": ["Vom", "Tod", "ent\u00b7f\u00e4rbt", "!", "Ein", "Ant\u00b7litz", "vol\u00b7ler", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die Mutter f\u00fchrte weg die Schwankende ...", "tokens": ["Die", "Mut\u00b7ter", "f\u00fchr\u00b7te", "weg", "die", "Schwan\u00b7ken\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.22": {"text": "Die beiden Tiziane blieben mir", "tokens": ["Die", "bei\u00b7den", "Ti\u00b7zi\u00b7a\u00b7ne", "blie\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Stets gegenw\u00e4rtig; l\u00f6schen sie, so lischt", "tokens": ["Stets", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ";", "l\u00f6\u00b7schen", "sie", ",", "so", "lischt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$.", "VVFIN", "PPER", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Die G\u00f6ttin ", "tokens": ["Die", "G\u00f6t\u00b7tin"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}