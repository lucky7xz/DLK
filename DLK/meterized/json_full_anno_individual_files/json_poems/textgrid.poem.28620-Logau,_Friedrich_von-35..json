{"textgrid.poem.28620": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich essen soll und trincken, wie ich mich bekleiden soll,", "tokens": ["Wie", "ich", "es\u00b7sen", "soll", "und", "trin\u00b7cken", ",", "wie", "ich", "mich", "be\u00b7klei\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie ich sonst mein Thun soll richten, sind die Leute Kummers voll;", "tokens": ["Wie", "ich", "sonst", "mein", "Thun", "soll", "rich\u00b7ten", ",", "sind", "die", "Leu\u00b7te", "Kum\u00b7mers", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$,", "VAFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wann ich nicht zu trincken, essen, noch mich zu bekleiden h\u00e4tte,", "tokens": ["Wann", "ich", "nicht", "zu", "trin\u00b7cken", ",", "es\u00b7sen", ",", "noch", "mich", "zu", "be\u00b7klei\u00b7den", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "VVFIN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sonsten auch gar viel nicht gilte, gilt es eine starcke Wette,", "tokens": [