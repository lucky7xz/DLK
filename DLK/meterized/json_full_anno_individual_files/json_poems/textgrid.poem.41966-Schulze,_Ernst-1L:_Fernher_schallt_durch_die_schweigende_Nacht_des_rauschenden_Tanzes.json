{"textgrid.poem.41966": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fernher schallt durch die schweigende Nacht des rauschenden Tanzes", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fernher schallt durch die schweigende Nacht des rauschenden Tanzes", "tokens": ["Fern\u00b7her", "schallt", "durch", "die", "schwei\u00b7gen\u00b7de", "Nacht", "des", "rau\u00b7schen\u00b7den", "Tan\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wilde Musik, und lind schaukelt die T\u00f6ne der West", "tokens": ["Wil\u00b7de", "Mu\u00b7sik", ",", "und", "lind", "schau\u00b7kelt", "die", "T\u00f6\u00b7ne", "der", "West"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Durch den umd\u00e4mmerten Hain zu dem Sehnenden; gaukelnde Ahnung", "tokens": ["Durch", "den", "um\u00b7d\u00e4m\u00b7mer\u00b7ten", "Hain", "zu", "dem", "Seh\u00b7nen\u00b7den", ";", "gau\u00b7keln\u00b7de", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Flattert herbei, und s\u00fc\u00df hallt mir im Herzen der Klang.", "tokens": ["Flat\u00b7tert", "her\u00b7bei", ",", "und", "s\u00fc\u00df", "hallt", "mir", "im", "Her\u00b7zen", "der", "Klang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Laulicher Hauch, was s\u00e4uselst du so? O sink' in der Bl\u00fcthe", "tokens": ["Lau\u00b7li\u00b7cher", "Hauch", ",", "was", "s\u00e4u\u00b7selst", "du", "so", "?", "O", "sink'", "in", "der", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$.", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Duftenden Kelch, und still schlummr' in der Liebenden Schoo\u00df;", "tokens": ["Duf\u00b7ten\u00b7den", "Kelch", ",", "und", "still", "schlumm\u00b7r'", "in", "der", "Lie\u00b7ben\u00b7den", "Schoo\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Schmiegt, ihr fr\u00f6hlichen S\u00e4nger des Hains, in's w\u00e4rmende Nest euch,", "tokens": ["Schmiegt", ",", "ihr", "fr\u00f6h\u00b7li\u00b7chen", "S\u00e4n\u00b7ger", "des", "Hains", ",", "in's", "w\u00e4r\u00b7men\u00b7de", "Nest", "euch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "PPER", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Und euch wiege des Zweigs Beben in Tr\u00e4ume der Lust.", "tokens": ["Und", "euch", "wie\u00b7ge", "des", "Zweigs", "Be\u00b7ben", "in", "Tr\u00e4u\u00b7me", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "------+--+--+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ach, s\u00fc\u00df ist's an dem Busen zu ruhn der err\u00f6thenden Liebe,", "tokens": ["Ach", ",", "s\u00fc\u00df", "ist's", "an", "dem", "Bu\u00b7sen", "zu", "ruhn", "der", "er\u00b7r\u00f6\u00b7then\u00b7den", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "S\u00fc\u00df, wenn das sehnende Herz hei\u00df sich an's sehnende schlie\u00dft,", "tokens": ["S\u00fc\u00df", ",", "wenn", "das", "seh\u00b7nen\u00b7de", "Herz", "hei\u00df", "sich", "an's", "seh\u00b7nen\u00b7de", "schlie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "ADJD", "PRF", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Wenn im ersch\u00fctternden Taumel der Lust lauttobend die Brust klopft,", "tokens": ["Wenn", "im", "er\u00b7sch\u00fct\u00b7tern\u00b7den", "Tau\u00b7mel", "der", "Lust", "laut\u00b7to\u00b7bend", "die", "Brust", "klopft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und in stillen Triumph schweigend die Seele versinkt.", "tokens": ["Und", "in", "stil\u00b7len", "Tri\u00b7umph", "schwei\u00b7gend", "die", "See\u00b7le", "ver\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Komm durch die duftige Nacht, komm, Reizende, leicht wie des Mondlichts", "tokens": ["Komm", "durch", "die", "duf\u00b7ti\u00b7ge", "Nacht", ",", "komm", ",", "Rei\u00b7zen\u00b7de", ",", "leicht", "wie", "des", "Mond\u00b7lichts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "$,", "NN", "$,", "ADJD", "KOKOM", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Zauberisch gaukelnder Tanz, still wie der sinkende Thau,", "tokens": ["Zau\u00b7be\u00b7risch", "gau\u00b7keln\u00b7der", "Tanz", ",", "still", "wie", "der", "sin\u00b7ken\u00b7de", "Thau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Da\u00df nicht neidische Blicke das Fest belauschen und frech dann", "tokens": ["Da\u00df", "nicht", "nei\u00b7di\u00b7sche", "Bli\u00b7cke", "das", "Fest", "be\u00b7lau\u00b7schen", "und", "frech", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJA", "NN", "ART", "NN", "VVINF", "KON", "ADJD", "ADV"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Ihrer eigenen Schmach Fiebergebilde verstreun.", "tokens": ["Ih\u00b7rer", "ei\u00b7ge\u00b7nen", "Schmach", "Fie\u00b7ber\u00b7ge\u00b7bil\u00b7de", "ver\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Mi\u00dfgunst sieht nur K\u00fcsse der Gluth, doch der Grazie Hauch, der", "tokens": ["Mi\u00df\u00b7gunst", "sieht", "nur", "K\u00fcs\u00b7se", "der", "Gluth", ",", "doch", "der", "Gra\u00b7zie", "Hauch", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ADV", "NN", "ART", "NN", "$,", "ADV", "ART", "NN", "NN", "$,", "PRELS"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Ueber uns waltet, erkennt nur der geweihete Sinn.", "tokens": ["Ue\u00b7ber", "uns", "wal\u00b7tet", ",", "er\u00b7kennt", "nur", "der", "ge\u00b7wei\u00b7he\u00b7te", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Schleiche dich fort aus des Tanzes Gew\u00fchl; noch gl\u00fche die Brust, noch", "tokens": ["Schlei\u00b7che", "dich", "fort", "aus", "des", "Tan\u00b7zes", "Ge\u00b7w\u00fchl", ";", "noch", "gl\u00fc\u00b7he", "die", "Brust", ",", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "PTKVZ", "APPR", "ART", "NN", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,", "ADV"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Strahle die Wange vom Rausch fl\u00fcchtig entgleitender Lust.", "tokens": ["Strah\u00b7le", "die", "Wan\u00b7ge", "vom", "Rausch", "fl\u00fcch\u00b7tig", "ent\u00b7glei\u00b7ten\u00b7der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.21": {"text": "K\u00fchl ist die schaurige Nacht, doch lodernd der Athem der Sehnsucht,", "tokens": ["K\u00fchl", "ist", "die", "schau\u00b7ri\u00b7ge", "Nacht", ",", "doch", "lo\u00b7dernd", "der", "A\u00b7them", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Flamm' und Flamme verscheucht sch\u00fctzend den eisigen Hauch.", "tokens": ["Flamm'", "und", "Flam\u00b7me", "ver\u00b7scheucht", "sch\u00fct\u00b7zend", "den", "ei\u00b7si\u00b7gen", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Schmiege dich traulich mir an, und lisple Worte des Zartsinns,", "tokens": ["Schmie\u00b7ge", "dich", "trau\u00b7lich", "mir", "an", ",", "und", "lisp\u00b7le", "Wor\u00b7te", "des", "Zarts\u00b7inns", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--++", "measure": "dactylic.di.plus"}, "line.24": {"text": "Welche die Grazie nicht, welche die Liebe nicht schm\u00e4hn;", "tokens": ["Wel\u00b7che", "die", "Gra\u00b7zie", "nicht", ",", "wel\u00b7che", "die", "Lie\u00b7be", "nicht", "schm\u00e4hn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.25": {"text": "K\u00fcsse mich still, doch wecke dein Ku\u00df nicht frevelnden Aufruhr;", "tokens": ["K\u00fcs\u00b7se", "mich", "still", ",", "doch", "we\u00b7cke", "dein", "Ku\u00df", "nicht", "fre\u00b7veln\u00b7den", "Auf\u00b7ruhr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Sanft in den Armen der Scham ruhe geb\u00e4ndigt die Lust.", "tokens": ["Sanft", "in", "den", "Ar\u00b7men", "der", "Scham", "ru\u00b7he", "ge\u00b7b\u00e4n\u00b7digt", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "NN", "VVPP", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Also entschlummert der grimmige Leu, wenn der himmlische Amor,", "tokens": ["Al\u00b7so", "ent\u00b7schlum\u00b7mert", "der", "grim\u00b7mi\u00b7ge", "Leu", ",", "wenn", "der", "himm\u00b7li\u00b7sche", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NE", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Wenn ihm die Charis mit Mohn friedlich die Schl\u00e4fe bekr\u00e4nzt.", "tokens": ["Wenn", "ihm", "die", "Cha\u00b7ris", "mit", "Mohn", "fried\u00b7lich", "die", "Schl\u00e4\u00b7fe", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Hangen m\u00f6cht' ich am Busen dir stets, in ewiger Sehnsucht", "tokens": ["Han\u00b7gen", "m\u00f6cht'", "ich", "am", "Bu\u00b7sen", "dir", "stets", ",", "in", "e\u00b7wi\u00b7ger", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPRART", "NN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "S\u00fc\u00df hinbangend und doch schwelgend in stetem Genu\u00df,", "tokens": ["S\u00fc\u00df", "hin\u00b7ban\u00b7gend", "und", "doch", "schwel\u00b7gend", "in", "ste\u00b7tem", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "M\u00f6chte mit lauterem Sinne die Schuld ausgleichen durch Unschuld", "tokens": ["M\u00f6ch\u00b7te", "mit", "lau\u00b7te\u00b7rem", "Sin\u00b7ne", "die", "Schuld", "aus\u00b7glei\u00b7chen", "durch", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVIZU", "APPR", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Und in d\u00fcsterer Nacht ehren das z\u00fcchtige Licht.", "tokens": ["Und", "in", "d\u00fcs\u00b7te\u00b7rer", "Nacht", "eh\u00b7ren", "das", "z\u00fcch\u00b7ti\u00b7ge", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Eros und Anteros kommt, zartf\u00fchlende, kommt zu dem Altar,", "tokens": ["E\u00b7ros", "und", "An\u00b7te\u00b7ros", "kommt", ",", "zart\u00b7f\u00fch\u00b7len\u00b7de", ",", "kommt", "zu", "dem", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "$,", "ADJA", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.34": {"text": "Welchen Uranias Hand freundlich mit Lilien schm\u00fcckt.", "tokens": ["Wel\u00b7chen", "U\u00b7ra\u00b7ni\u00b7as", "Hand", "freund\u00b7lich", "mit", "Li\u00b7li\u00b7en", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Fernher schallt durch die schweigende Nacht des rauschenden Tanzes", "tokens": ["Fern\u00b7her", "schallt", "durch", "die", "schwei\u00b7gen\u00b7de", "Nacht", "des", "rau\u00b7schen\u00b7den", "Tan\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wilde Musik, und lind schaukelt die T\u00f6ne der West", "tokens": ["Wil\u00b7de", "Mu\u00b7sik", ",", "und", "lind", "schau\u00b7kelt", "die", "T\u00f6\u00b7ne", "der", "West"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Durch den umd\u00e4mmerten Hain zu dem Sehnenden; gaukelnde Ahnung", "tokens": ["Durch", "den", "um\u00b7d\u00e4m\u00b7mer\u00b7ten", "Hain", "zu", "dem", "Seh\u00b7nen\u00b7den", ";", "gau\u00b7keln\u00b7de", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA