{"textgrid.poem.36247": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Blumen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6nen Farben d\u00fcrfen nicht mehr gl\u00e4nzen,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Far\u00b7ben", "d\u00fcr\u00b7fen", "nicht", "mehr", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man darf den s\u00fc\u00dfen Putz nicht mehr entfalten.", "tokens": ["Man", "darf", "den", "s\u00fc\u00b7\u00dfen", "Putz", "nicht", "mehr", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie ziemt' es auch zu solchen hohen T\u00e4nzen,", "tokens": ["Wie", "ziemt'", "es", "auch", "zu", "sol\u00b7chen", "ho\u00b7hen", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Sterne heilig walten,", "tokens": ["Wo", "Ster\u00b7ne", "hei\u00b7lig", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die das Azur umkr\u00e4nzen,", "tokens": ["Die", "das", "A\u00b7zur", "um\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], 