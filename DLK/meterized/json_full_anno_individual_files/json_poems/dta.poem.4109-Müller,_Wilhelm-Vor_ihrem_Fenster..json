{"dta.poem.4109": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vor ihrem Fenster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor deinem hellen Fenster still zu stehn!", "tokens": ["Vor", "dei\u00b7nem", "hel\u00b7len", "Fens\u00b7ter", "still", "zu", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Vorhang find' ich hoch hinaufgewunden,", "tokens": ["Den", "Vor\u00b7hang", "find'", "ich", "hoch", "hin\u00b7auf\u00b7ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Frei darf mein Blick in seinen Himmel sehn.", "tokens": ["Frei", "darf", "mein", "Blick", "in", "sei\u00b7nen", "Him\u00b7mel", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die Blumen, die sich an die Rahmen schmiegen,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "sich", "an", "die", "Rah\u00b7men", "schmie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umschlingen mir dein Bild mit ihrem Kranz,", "tokens": ["Um\u00b7schlin\u00b7gen", "mir", "dein", "Bild", "mit", "ih\u00b7rem", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meines Odems Hauche \u00fcberfliegen", "tokens": ["Und", "mei\u00b7nes", "O\u00b7dems", "Hau\u00b7che", "\u00fc\u00b7berf\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit tr\u00fcbem Nebelduft der Scheiben Glanz.", "tokens": ["Mit", "tr\u00fc\u00b7bem", "Ne\u00b7bel\u00b7duft", "der", "Schei\u00b7ben", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da sitzest du, so still und unbefangen,", "tokens": ["Da", "sit\u00b7zest", "du", ",", "so", "still", "und", "un\u00b7be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sch\u00f6ne Haupt gest\u00fctzt auf deinen Arm,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Haupt", "ge\u00b7st\u00fctzt", "auf", "dei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ich bin dir so nah mit Lust und Bangen,", "tokens": ["Und", "ich", "bin", "dir", "so", "nah", "mit", "Lust", "und", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit meiner W\u00fcnsche ungest\u00fcmem Schwarm.", "tokens": ["Mit", "mei\u00b7ner", "W\u00fcn\u00b7sche", "un\u00b7ge\u00b7st\u00fc\u00b7mem", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du schauest her: es wissen deine Augen", "tokens": ["Du", "schau\u00b7est", "her", ":", "es", "wis\u00b7sen", "dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom s\u00fc\u00dfen Zauber ihrer Blicke nicht,", "tokens": ["Vom", "s\u00fc\u00b7\u00dfen", "Zau\u00b7ber", "ih\u00b7rer", "Bli\u00b7cke", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie meine sich aus ihnen trunken saugen,", "tokens": ["Wie", "mei\u00b7ne", "sich", "aus", "ih\u00b7nen", "trun\u00b7ken", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "APPR", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hell ergl\u00fchen nur von ihrem Licht.", "tokens": ["Und", "hell", "er\u00b7gl\u00fc\u00b7hen", "nur", "von", "ih\u00b7rem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du ahnest nicht, wie sich mein ganzes Leben", "tokens": ["Du", "ah\u00b7nest", "nicht", ",", "wie", "sich", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich einem Mond um deine Sonne dreht,", "tokens": ["Gleich", "ei\u00b7nem", "Mond", "um", "dei\u00b7ne", "Son\u00b7ne", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der bald sich will auf stolzen Strahlen heben,", "tokens": ["Der", "bald", "sich", "will", "auf", "stol\u00b7zen", "Strah\u00b7len", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald tief gebeugt in Thr\u00e4nen untergeht.", "tokens": ["Bald", "tief", "ge\u00b7beugt", "in", "Thr\u00e4\u00b7nen", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Still, still, mein Herz! Was meint dein wildes", "tokens": ["Still", ",", "still", ",", "mein", "Herz", "!", "Was", "meint", "dein", "wil\u00b7des"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlagen?", "tokens": ["Schla\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Schau \u00fcber dich, der Himmel ist nicht fern;", "tokens": ["Schau", "\u00fc\u00b7ber", "dich", ",", "der", "Him\u00b7mel", "ist", "nicht", "fern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Flammen, die aus Sternen fallen, tragen", "tokens": ["Und", "Flam\u00b7men", ",", "die", "aus", "Ster\u00b7nen", "fal\u00b7len", ",", "tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Menschen Seufzer vor den Thron des Herrn.", "tokens": ["Der", "Men\u00b7schen", "Seuf\u00b7zer", "vor", "den", "Thron", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}