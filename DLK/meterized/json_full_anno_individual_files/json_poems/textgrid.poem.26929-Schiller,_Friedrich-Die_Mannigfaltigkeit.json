{"textgrid.poem.26929": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Mannigfaltigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viele sind gut und verst\u00e4ndig, doch z\u00e4hlen f\u00fcr einen nur alle,", "tokens": ["Vie\u00b7le", "sind", "gut", "und", "ver\u00b7st\u00e4n\u00b7dig", ",", "doch", "z\u00e4h\u00b7len", "f\u00fcr", "ei\u00b7nen", "nur", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "VVFIN", "APPR", "ART", "ADV", "PIS", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Denn sie regiert der Begriff, ach! nicht das liebende Herz.", "tokens": ["Denn", "sie", "re\u00b7giert", "der", "Be\u00b7griff", ",", "ach", "!", "nicht", "das", "lie\u00b7ben\u00b7de", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ITJ", "$.", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Traurig herrscht der Begriff, aus tausendfach spielenden Formen", "tokens": ["Trau\u00b7rig", "herrscht", "der", "Be\u00b7griff", ",", "aus", "tau\u00b7send\u00b7fach", "spie\u00b7len\u00b7den", "For\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "APPR", "NE", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Bringet er d\u00fcrftig und leer immer nur eine hervor.", "tokens": ["Brin\u00b7get", "er", "d\u00fcrf\u00b7tig", "und", "leer", "im\u00b7mer", "nur", "ei\u00b7ne", "her\u00b7vo