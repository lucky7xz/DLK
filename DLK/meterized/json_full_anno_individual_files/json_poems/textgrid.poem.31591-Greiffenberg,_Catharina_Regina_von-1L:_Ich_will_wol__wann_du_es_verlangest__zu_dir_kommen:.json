{"textgrid.poem.31591": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will wol / wann du es verlangest / zu dir kommen:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will wol / wann du es verlangest / zu dir kommen:", "tokens": ["Ich", "will", "wol", "/", "wann", "du", "es", "ver\u00b7lan\u00b7gest", "/", "zu", "dir", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$(", "PWAV", "PPER", "PPER", "VVFIN", "$(", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "doch zieht ein ganzes Heer der Tr\u00fcbsal mit mir ein.", "tokens": ["doch", "zieht", "ein", "gan\u00b7zes", "Heer", "der", "Tr\u00fcb\u00b7sal", "mit", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich und das Vngel\u00fcck / schier unzertrennlich seyn.", "tokens": ["Ich", "und", "das", "Vn\u00b7ge\u00b7l\u00fcck", "/", "schier", "un\u00b7zer\u00b7trenn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$(", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hat ihm grosse Streich' auf mich schon vorgenommen.", "tokens": ["Es", "hat", "ihm", "gros\u00b7se", "Streich'", "auf", "mich", "schon", "vor\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word