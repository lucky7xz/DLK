"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Ja die Drohung macht er laut:", "tokens": ["Ja", "die", "Dro\u00b7hung", "macht", "er", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wenn die K\u00f6nigin sich weigert", "tokens": ["Wenn", "die", "K\u00f6\u00b7ni\u00b7gin", "sich", "wei\u00b7gert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Meiner Kraft sich anzutraun,", "tokens": ["Mei\u00b7ner", "Kraft", "sich", "an\u00b7zu\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Wehe jener Stadt, sie m\u00f6chte", "tokens": ["We\u00b7he", "je\u00b7ner", "Stadt", ",", "sie", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PDAT", "NN", "$,", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Dann verschwinden wie ein Traum!", "tokens": ["Dann", "ver\u00b7schwin\u00b7den", "wie", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Zitternd h\u00f6rt es ganz Karthago,", "tokens": ["Zit\u00b7ternd", "h\u00f6rt", "es", "ganz", "Kar\u00b7tha\u00b7go", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.38": {"text": "Weil er m\u00e4chtig \u00fcberaus,", "tokens": ["Weil", "er", "m\u00e4