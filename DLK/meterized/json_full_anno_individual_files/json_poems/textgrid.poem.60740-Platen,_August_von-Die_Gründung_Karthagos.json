.45": {"text": "Aber ihr im tiefen Busen", "tokens": ["A\u00b7ber", "ihr", "im", "tie\u00b7fen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Steigt ein b\u00f6ser Geist herauf,", "tokens": ["Steigt", "ein", "b\u00f6\u00b7ser", "Geist", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Ob sie freveln soll am Gatten,", "tokens": ["Ob", "sie", "fre\u00b7veln", "soll", "am", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Ob sie, jeder Bitte taub,", "tokens": ["Ob", "sie", ",", "je\u00b7der", "Bit\u00b7te", "taub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Freveln soll an ihrem Volke,", "tokens": ["Fre\u00b7veln", "soll", "an", "ih\u00b7rem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Das an ihre Liebe Glaubt?", "tokens": ["Das", "an", "ih\u00b7re", "Lie\u00b7be", "Glaubt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.51": {"text": "Doch in einer solchen Seele", "tokens": ["Doch", "in", "ei\u00b7ner", "sol\u00b7chen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Ist ein Zweifel wie ein Hauch:", "tokens": ["Ist", "ein", "Zwei\u00b7fel", "wie", "ein", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Nur das Gro\u00dfe kann sie denken,", "tokens": ["Nur", "das", "Gro\u00b7\u00dfe", "kann", "sie", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Nur das Gro\u00dfe f\u00fchrt sie aus.", "tokens": ["Nur", "das", "Gro\u00b7\u00dfe", "f\u00fchrt", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Einen Holzsto\u00df, wie zum Opfer,", "tokens": ["Ei\u00b7nen", "Holz\u00b7sto\u00df", ",", "wie", "zum", "Op\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "L\u00e4\u00dft die K\u00f6nigin erbaun,", "tokens": ["L\u00e4\u00dft", "die", "K\u00f6\u00b7ni\u00b7gin", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "L\u00e4\u00dft um ihn das Volk versammeln,", "tokens": ["L\u00e4\u00dft", "um", "ihn", "das", "Volk", "ver\u00b7sam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Tritt hervor und steigt hinauf:", "tokens": ["Tritt", "her\u00b7vor", "und", "steigt", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Lebe wohl, o mein Karthago,", "tokens": ["Le\u00b7be", "wohl", ",", "o", "mein", "Kar\u00b7tha\u00b7go", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.60": {"text": "Nicht die Feinde sollst du schaun,", "tokens": ["Nicht", "die", "Fein\u00b7de", "sollst", "du", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Bl\u00fchn empor in goldner Freiheit,", "tokens": ["Bl\u00fchn", "em\u00b7por", "in", "gold\u00b7ner", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Nicht vergehn in Schutt und Graus:", "tokens": ["Nicht", "ver\u00b7gehn", "in", "Schutt", "und", "Graus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "O Sich\u00e4us, breite deine", "tokens": ["O", "Sic\u00b7h\u00e4us", ",", "brei\u00b7te", "dei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Schattenarme nach mir aus!", "tokens": ["Schat\u00b7ten\u00b7ar\u00b7me", "nach", "mir", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Diese hohen Worte sprechend", "tokens": ["Die\u00b7se", "ho\u00b7hen", "Wor\u00b7te", "spre\u00b7chend"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Fa\u00dft ein Schwert sie ohne Graun,", "tokens": ["Fa\u00dft", "ein", "Schwert", "sie", "oh\u00b7ne", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "St\u00f6\u00dft es durch den sch\u00f6nsten Busen,", "tokens": ["St\u00f6\u00dft", "es", "durch", "den", "sch\u00f6ns\u00b7ten", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Den die Sonne durfte schaun.", "tokens": ["Den", "die", "Son\u00b7ne", "durf\u00b7te", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "Und im Aschenkrug gesammelt", "tokens": ["Und", "im", "A\u00b7schen\u00b7krug", "ge\u00b7sam\u00b7melt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Ward sofort der edle Staub,", "tokens": ["Ward", "so\u00b7fort", "der", "ed\u00b7le", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.71": {"text": "Ward im Tempel selbst bestattet,", "tokens": ["Ward", "im", "Tem\u00b7pel", "selbst", "be\u00b7stat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Ward bekr\u00e4nzt mit Siegeslaub.", "tokens": ["Ward", "be\u00b7kr\u00e4nzt", "mit", "Sie\u00b7ges\u00b7laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.73": {"text": "K\u00f6nig Jarbas zog von dannen,", "tokens": ["K\u00f6\u00b7nig", "Jar\u00b7bas", "zog", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "St\u00f6rte nicht Karthagos Bau:", "tokens": ["St\u00f6r\u00b7te", "nicht", "Kar\u00b7tha\u00b7gos", "Bau", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NE", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.75": {"text": "Jenen seegewaltigen Freistaat", "tokens": ["Je\u00b7nen", "see\u00b7ge\u00b7wal\u00b7ti\u00b7gen", "Frei\u00b7staat"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+--++", "measure": "trochaic.penta.relaxed"}, "line.76": {"text": "Gr\u00fcndete so die gr\u00f6\u00dfte Frau.", "tokens": ["Gr\u00fcn\u00b7de\u00b7te", "so", "die", "gr\u00f6\u00df\u00b7te", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}