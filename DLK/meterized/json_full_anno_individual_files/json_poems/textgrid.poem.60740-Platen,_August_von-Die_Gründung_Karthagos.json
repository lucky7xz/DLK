{"textgrid.poem.60740": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gr\u00fcndung Karthagos", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor der Goldbegier des Bruders,", "tokens": ["Vor", "der", "Gold\u00b7be\u00b7gier", "des", "Bru\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der nach ihren Sch\u00e4tzen schnaubt,", "tokens": ["Der", "nach", "ih\u00b7ren", "Sch\u00e4t\u00b7zen", "schnaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der in ihres Gatten Busen", "tokens": ["Der", "in", "ih\u00b7res", "Gat\u00b7ten", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein verruchtes Schwert getaucht,", "tokens": ["Sein", "ver\u00b7ruch\u00b7tes", "Schwert", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Flieht hinweg die sch\u00f6ne Dido", "tokens": ["Flieht", "hin\u00b7weg", "die", "sch\u00f6\u00b7ne", "Di\u00b7do"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Aus sidonischen Heimataun,", "tokens": ["Aus", "si\u00b7do\u00b7ni\u00b7schen", "Hei\u00b7ma\u00b7taun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Nimmt mit sich geh\u00e4ufte Sch\u00e4tze,", "tokens": ["Nimmt", "mit", "sich", "ge\u00b7h\u00e4uf\u00b7te", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimmt mit sich des Gatten Staub,", "tokens": ["Nimmt", "mit", "sich", "des", "Gat\u00b7ten", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dem gelobt sie st\u00e4te Treue,", "tokens": ["Dem", "ge\u00b7lobt", "sie", "st\u00e4\u00b7te", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie es ziemt den h\u00f6chsten Fraun;", "tokens": ["Wie", "es", "ziemt", "den", "h\u00f6chs\u00b7ten", "Fraun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn der wahren Witwe Liebe", "tokens": ["Denn", "der", "wah\u00b7ren", "Wit\u00b7we", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gleicht dem Lieben einer Braut.", "tokens": ["Gleicht", "dem", "Lie\u00b7ben", "ei\u00b7ner", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Edle folgen ihr und Knechte,", "tokens": ["Ed\u00b7le", "fol\u00b7gen", "ihr", "und", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPER", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Als sie l\u00f6st den Ankertau,", "tokens": ["Als", "sie", "l\u00f6st", "den", "An\u00b7ker\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Segeln auf den hohen Schiffen", "tokens": ["Se\u00b7geln", "auf", "den", "ho\u00b7hen", "Schif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Durch das tiefe Wogenblau,", "tokens": ["Durch", "das", "tie\u00b7fe", "Wo\u00b7gen\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Bis an afrikanischer K\u00fcste", "tokens": ["Bis", "an", "af\u00b7ri\u00b7ka\u00b7ni\u00b7scher", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Landen alle voll Vertraun.", "tokens": ["Lan\u00b7den", "al\u00b7le", "voll", "Ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Dido l\u00e4\u00dft an sichrer Felsbucht", "tokens": ["Di\u00b7do", "l\u00e4\u00dft", "an", "sich\u00b7rer", "Fels\u00b7bucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "M\u00e4chtig eine Stadt erbaun:", "tokens": ["M\u00e4ch\u00b7tig", "ei\u00b7ne", "Stadt", "er\u00b7baun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Axt an Axt erklingt am Ufer,", "tokens": ["Axt", "an", "Axt", "er\u00b7klingt", "am", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Stein um Stein wird ausgehaun.", "tokens": ["Stein", "um", "Stein", "wird", "aus\u00b7ge\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Bald beschirmen stolze Mauern", "tokens": ["Bald", "be\u00b7schir\u00b7men", "stol\u00b7ze", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Tempel, Hafen, H\u00fctt und Haus;", "tokens": ["Tem\u00b7pel", ",", "Ha\u00b7fen", ",", "H\u00fctt", "und", "Haus", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Drauf als K\u00f6nigin beherrschte", "tokens": ["Drauf", "als", "K\u00f6\u00b7ni\u00b7gin", "be\u00b7herrschte"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "KOUS", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Dido diesen stolzen Raum.", "tokens": ["Di\u00b7do", "die\u00b7sen", "stol\u00b7zen", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Doch der Ruf von ihrer Sch\u00f6nheit", "tokens": ["Doch", "der", "Ruf", "von", "ih\u00b7rer", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Breitet seine Fl\u00fcgel aus:", "tokens": ["Brei\u00b7tet", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "aus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "K\u00f6nig Jarbas wohnt benachbart,", "tokens": ["K\u00f6\u00b7nig", "Jar\u00b7bas", "wohnt", "be\u00b7nach\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "Tapferer M\u00e4nner Oberhaupt;", "tokens": ["Tap\u00b7fe\u00b7rer", "M\u00e4n\u00b7ner", "O\u00b7ber\u00b7haupt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Dieser bietet seine Hand ihr,", "tokens": ["Die\u00b7ser", "bie\u00b7tet", "sei\u00b7ne", "Hand", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Ja die Drohung macht er laut:", "tokens": ["Ja", "die", "Dro\u00b7hung", "macht", "er", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wenn die K\u00f6nigin sich weigert", "tokens": ["Wenn", "die", "K\u00f6\u00b7ni\u00b7gin", "sich", "wei\u00b7gert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Meiner Kraft sich anzutraun,", "tokens": ["Mei\u00b7ner", "Kraft", "sich", "an\u00b7zu\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Wehe jener Stadt, sie m\u00f6chte", "tokens": ["We\u00b7he", "je\u00b7ner", "Stadt", ",", "sie", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PDAT", "NN", "$,", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Dann verschwinden wie ein Traum!", "tokens": ["Dann", "ver\u00b7schwin\u00b7den", "wie", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Zitternd h\u00f6rt es ganz Karthago,", "tokens": ["Zit\u00b7ternd", "h\u00f6rt", "es", "ganz", "Kar\u00b7tha\u00b7go", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.38": {"text": "Weil er m\u00e4chtig \u00fcberaus,", "tokens": ["Weil", "er", "m\u00e4ch\u00b7tig", "\u00fc\u00b7be\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und des Volks ergraute V\u00e4ter", "tokens": ["Und", "des", "Volks", "er\u00b7grau\u00b7te", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Treten vor der F\u00fcrstin auf,", "tokens": ["Tre\u00b7ten", "vor", "der", "F\u00fcrs\u00b7tin", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Flehn sie, jenen Bund zu schlie\u00dfen,", "tokens": ["Flehn", "sie", ",", "je\u00b7nen", "Bund", "zu", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Hinzugeben nicht dem Raub", "tokens": ["Hin\u00b7zu\u00b7ge\u00b7ben", "nicht", "dem", "Raub"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Diese Laren, diese Tempel,", "tokens": ["Die\u00b7se", "La\u00b7ren", ",", "die\u00b7se", "Tem\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Die sie liebend selbst gebaut!", "tokens": ["Die", "sie", "lie\u00b7bend", "selbst", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Aber ihr im tiefen Busen", "tokens": ["A\u00b7ber", "ihr", "im", "tie\u00b7fen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Steigt ein b\u00f6ser Geist herauf,", "tokens": ["Steigt", "ein", "b\u00f6\u00b7ser", "Geist", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Ob sie freveln soll am Gatten,", "tokens": ["Ob", "sie", "fre\u00b7veln", "soll", "am", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Ob sie, jeder Bitte taub,", "tokens": ["Ob", "sie", ",", "je\u00b7der", "Bit\u00b7te", "taub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Freveln soll an ihrem Volke,", "tokens": ["Fre\u00b7veln", "soll", "an", "ih\u00b7rem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Das an ihre Liebe Glaubt?", "tokens": ["Das", "an", "ih\u00b7re", "Lie\u00b7be", "Glaubt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.51": {"text": "Doch in einer solchen Seele", "tokens": ["Doch", "in", "ei\u00b7ner", "sol\u00b7chen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Ist ein Zweifel wie ein Hauch:", "tokens": ["Ist", "ein", "Zwei\u00b7fel", "wie", "ein", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Nur das Gro\u00dfe kann sie denken,", "tokens": ["Nur", "das", "Gro\u00b7\u00dfe", "kann", "sie", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Nur das Gro\u00dfe f\u00fchrt sie aus.", "tokens": ["Nur", "das", "Gro\u00b7\u00dfe", "f\u00fchrt", "sie", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Einen Holzsto\u00df, wie zum Opfer,", "tokens": ["Ei\u00b7nen", "Holz\u00b7sto\u00df", ",", "wie", "zum", "Op\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "L\u00e4\u00dft die K\u00f6nigin erbaun,", "tokens": ["L\u00e4\u00dft", "die", "K\u00f6\u00b7ni\u00b7gin", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "L\u00e4\u00dft um ihn das Volk versammeln,", "tokens": ["L\u00e4\u00dft", "um", "ihn", "das", "Volk", "ver\u00b7sam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Tritt hervor und steigt hinauf:", "tokens": ["Tritt", "her\u00b7vor", "und", "steigt", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Lebe wohl, o mein Karthago,", "tokens": ["Le\u00b7be", "wohl", ",", "o", "mein", "Kar\u00b7tha\u00b7go", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.60": {"text": "Nicht die Feinde sollst du schaun,", "tokens": ["Nicht", "die", "Fei