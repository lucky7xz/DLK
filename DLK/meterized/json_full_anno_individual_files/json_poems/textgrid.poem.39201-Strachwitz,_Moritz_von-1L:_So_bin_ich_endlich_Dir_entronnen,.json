{"textgrid.poem.39201": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So bin ich endlich Dir entronnen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bin ich endlich Dir entronnen,", "tokens": ["So", "bin", "ich", "end\u00b7lich", "Dir", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stadt der Kritik und Politik,", "tokens": ["Stadt", "der", "Kri\u00b7tik", "und", "Po\u00b7li\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mich lockt hinaus der Maienwonnen", "tokens": ["Mich", "lockt", "hin\u00b7aus", "der", "Mai\u00b7en\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unwiderstehliche Musik.", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7li\u00b7che", "Mu\u00b7sik", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fahr' hin, Du L\u00e4rm der Zeitungsbl\u00e4tter,", "tokens": ["Fahr'", "hin", ",", "Du", "L\u00e4rm", "der", "Zei\u00b7tungs\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der widerw\u00e4rtig gellend schallt,", "tokens": ["Der", "wi\u00b7der\u00b7w\u00e4r\u00b7tig", "gel\u00b7lend", "schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir ist, als h\u00f6r' ich Horngeschmetter", "tokens": ["Mir", "ist", ",", "als", "h\u00f6r'", "ich", "Horn\u00b7ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus einem fernen Buchenwald!", "tokens": ["Aus", "ei\u00b7nem", "fer\u00b7nen", "Bu\u00b7chen\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nun, mit heil'gem Morgenstrahle", "tokens": ["Und", "nun", ",", "mit", "heil'\u00b7gem", "Mor\u00b7gen\u00b7strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4rbt sich der Hochwald gr\u00fcn und falb,", "tokens": ["F\u00e4rbt", "sich", "der", "Hoch\u00b7wald", "gr\u00fcn", "und", "falb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zu F\u00fc\u00dfen mir das Gr\u00fcn der Tale,", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "mir", "das", "Gr\u00fcn", "der", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu H\u00e4upten mir das Blau der Alp'.", "tokens": ["Zu", "H\u00e4up\u00b7ten", "mir", "das", "Blau", "der", "Al\u00b7p'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Lerche steigt in Flatterschwingung,", "tokens": ["Die", "Ler\u00b7che", "steigt", "in", "Flat\u00b7ter\u00b7schwin\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stumm ausgebreitet schwimmt der Weih',", "tokens": ["Stumm", "aus\u00b7ge\u00b7brei\u00b7tet", "schwimmt", "der", "Weih'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Reh durchbricht die Laubverschlingung,", "tokens": ["Das", "Reh", "durch\u00b7bricht", "die", "Laub\u00b7ver\u00b7schlin\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und aus dem Strome schaut die Fei.", "tokens": ["Und", "aus", "dem", "Stro\u00b7me", "schaut", "die", "Fei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es spielen dunkelrote Lichter", "tokens": ["Es", "spie\u00b7len", "dun\u00b7kel\u00b7ro\u00b7te", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meines Kelches Purpurnacht,", "tokens": ["In", "mei\u00b7nes", "Kel\u00b7ches", "Pur\u00b7pur\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir sei, o Kaiserin der Dichter,", "tokens": ["Dir", "sei", ",", "o", "Kai\u00b7se\u00b7rin", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Romantik, dieser Trunk gebracht!", "tokens": ["Ro\u00b7man\u00b7tik", ",", "die\u00b7ser", "Trunk", "ge\u00b7bracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Deiner Erde, Deinem Wasser,", "tokens": ["Vor", "Dei\u00b7ner", "Er\u00b7de", ",", "Dei\u00b7nem", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Deiner Luft und Deinem Licht,", "tokens": ["In", "Dei\u00b7ner", "Luft", "und", "Dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo mir kein Mi\u00dflaut Deiner Hasser", "tokens": ["Wo", "mir", "kein", "Mi\u00df\u00b7laut", "Dei\u00b7ner", "Has\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den sel'gen Taumel unterbricht.", "tokens": ["Den", "sel'\u00b7gen", "Tau\u00b7mel", "un\u00b7ter\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Sch\u00fctzerin des heil'gen Grales,", "tokens": ["Du", "Sch\u00fct\u00b7ze\u00b7rin", "des", "heil'\u00b7gen", "Gra\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kriemhilde, die um Siegfried weint,", "tokens": ["Kriem\u00b7hil\u00b7de", ",", "die", "um", "Sieg\u00b7fried", "weint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gespielin Du des Mondenstrahles,", "tokens": ["Ge\u00b7spie\u00b7lin", "Du", "des", "Mon\u00b7dens\u00b7trah\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00fcber Heldengr\u00e4ber scheint.", "tokens": ["Der", "\u00fc\u00b7ber", "Hel\u00b7den\u00b7gr\u00e4\u00b7ber", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist Gesang im Stromgerolle", "tokens": ["Du", "bist", "Ge\u00b7sang", "im", "Strom\u00b7ge\u00b7rol\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Harfensausen in dem Baum,", "tokens": ["Und", "Har\u00b7fen\u00b7sau\u00b7sen", "in", "dem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du zogst zuerst ins Wundervolle", "tokens": ["Du", "zogst", "zu\u00b7erst", "ins", "Wun\u00b7der\u00b7vol\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des ersten Dichters Maientraum.", "tokens": ["Des", "ers\u00b7ten", "Dich\u00b7ters", "Mai\u00b7ent\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du warst Frau Venus dem Tannh\u00e4user", "tokens": ["Du", "warst", "Frau", "Ve\u00b7nus", "dem", "Tann\u00b7h\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "NE", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und Loreley dem alten Rhein,", "tokens": ["Und", "Lo\u00b7re\u00b7ley", "dem", "al\u00b7ten", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schwirrst am Teich durch Zitterreiser", "tokens": ["Du", "schwirrst", "am", "Teich", "durch", "Zit\u00b7ter\u00b7rei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Erlenk\u00f6nigs T\u00f6chterlein.", "tokens": ["Als", "Er\u00b7len\u00b7k\u00f6\u00b7nigs", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seit das Volk, das kampfesblinde,", "tokens": ["Und", "seit", "das", "Volk", ",", "das", "kamp\u00b7fes\u00b7blin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich j\u00fcngst verstie\u00df von seiner Seit',", "tokens": ["Dich", "j\u00fcngst", "ver\u00b7stie\u00df", "von", "sei\u00b7ner", "Seit'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Trinkst Du im Wald die Milch der Hinde,", "tokens": ["Trinkst", "Du", "im", "Wald", "die", "Milch", "der", "Hin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Genoveva unsrer Zeit.", "tokens": ["Die", "Ge\u00b7no\u00b7ve\u00b7va", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und doch, Versto\u00df'ne durch Verblendung,", "tokens": ["Und", "doch", ",", "Ver\u00b7sto\u00df'\u00b7ne", "durch", "Ver\u00b7blen\u00b7dung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bist Du reich, trotz Zeit und Zorn,", "tokens": ["Wie", "bist", "Du", "reich", ",", "trotz", "Zeit", "und", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du leerst in g\u00f6ttlicher Verschwendung", "tokens": ["Du", "leerst", "in", "g\u00f6tt\u00b7li\u00b7cher", "Ver\u00b7schwen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tagt\u00e4glich noch Dein Wunderhorn.", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "noch", "Dein", "Wun\u00b7der\u00b7horn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich gr\u00fc\u00dfe Dich mit frommem Sinne,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "Dich", "mit", "from\u00b7mem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ist Dein Reich so gr\u00fcn und weit!", "tokens": ["Wie", "ist", "Dein", "Reich", "so", "gr\u00fcn", "und", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du F\u00fcrstin vielgetreuer Minne,", "tokens": ["Du", "F\u00fcrs\u00b7tin", "viel\u00b7ge\u00b7treu\u00b7er", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sei tausendmal gebenedeit! \u2013", "tokens": ["Sei", "tau\u00b7send\u00b7mal", "ge\u00b7be\u00b7ne\u00b7deit", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Es schweigt die Welt, die Zweige nicken,", "tokens": ["Es", "schweigt", "die", "Welt", ",", "die", "Zwei\u00b7ge", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und leise atmend pulst der See,", "tokens": ["Und", "lei\u00b7se", "at\u00b7mend", "pulst", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es f\u00e4llt ein m\u00e4rchenhaft Entz\u00fccken", "tokens": ["Es", "f\u00e4llt", "ein", "m\u00e4r\u00b7chen\u00b7haft", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir \u00fcbers Herz wie Bl\u00fctenschnee.", "tokens": ["Mir", "\u00fc\u00b7bers", "Herz", "wie", "Bl\u00fc\u00b7ten\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zur Andacht wird der Bl\u00e4tter Plaudern,", "tokens": ["Zur", "An\u00b7dacht", "wird", "der", "Bl\u00e4t\u00b7ter", "Plau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ehrf\u00fcrchtig liegt die Woge da;", "tokens": ["Ehr\u00b7f\u00fcrch\u00b7tig", "liegt", "die", "Wo\u00b7ge", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ha frommes Ahnen, s\u00fc\u00dfes Schaudern,", "tokens": ["Ha", "from\u00b7mes", "Ah\u00b7nen", ",", "s\u00fc\u00b7\u00dfes", "Schau\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Heil Dir, Romantik, Du bist nah'.", "tokens": ["Heil", "Dir", ",", "Ro\u00b7man\u00b7tik", ",", "Du", "bist", "nah'", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "So bin ich endlich Dir entronnen,", "tokens": ["So", "bin", "ich", "end\u00b7lich", "Dir", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stadt der Kritik und Politik,", "tokens": ["Stadt", "der", "Kri\u00b7tik", "und", "Po\u00b7li\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mich lockt hinaus der Maienwonnen", "tokens": ["Mich", "lockt", "hin\u00b7aus", "der", "Mai\u00b7en\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unwiderstehliche Musik.", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7li\u00b7che", "Mu\u00b7sik", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fahr' hin, Du L\u00e4rm der Zeitungsbl\u00e4tter,", "tokens": ["Fahr'", "hin", ",", "Du", "L\u00e4rm", "der", "Zei\u00b7tungs\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der widerw\u00e4rtig gellend schallt,", "tokens": ["Der", "wi\u00b7der\u00b7w\