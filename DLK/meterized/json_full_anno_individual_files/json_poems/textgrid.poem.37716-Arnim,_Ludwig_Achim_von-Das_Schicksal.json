{"textgrid.poem.37716": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Schicksal", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Muth begr\u00fc\u00dfe schon die Lieb',", "tokens": ["Muth", "be\u00b7gr\u00fc\u00b7\u00dfe", "schon", "die", "Lieb'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz verfinstert pl\u00f6tzlich sich die Sonne,", "tokens": ["Ganz", "ver\u00b7fins\u00b7tert", "pl\u00f6tz\u00b7lich", "sich", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Licht von ihrer Scheibe trieb,", "tokens": ["Licht", "von", "ih\u00b7rer", "Schei\u00b7be", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Muth h\u00e4tt' ohne Lieb' wohl nichts gewonnen;", "tokens": ["Muth", "h\u00e4tt'", "oh\u00b7ne", "Lieb'", "wohl", "nichts", "ge\u00b7won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch durch Lieb' wird's erste Nacht.", "tokens": ["Doch", "durch", "Lieb'", "wird's", "ers\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Sacht, sacht! da\u00df k