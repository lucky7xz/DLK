{"textgrid.poem.35164": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "In Ewigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df uns hinauf zu jenen Bergen steigen,", "tokens": ["La\u00df", "uns", "hin\u00b7auf", "zu", "je\u00b7nen", "Ber\u00b7gen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf denen einst die Macht der Welt gestanden!", "tokens": ["Auf", "de\u00b7nen", "einst", "die", "Macht", "der", "Welt", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst mir ihre starken Burgen zeigen", "tokens": ["Du", "sollst", "mir", "ih\u00b7re", "star\u00b7ken", "Bur\u00b7gen", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was von ihnen heut noch ist vorhanden.", "tokens": ["Und", "was", "von", "ih\u00b7nen", "heut", "noch", "ist", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "ADV", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie legte ihre Faust in jede Wage,", "tokens": ["Sie", "leg\u00b7te", "ih\u00b7re", "Faust", "in", "je\u00b7de", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und was sie that, das machte sie zum Recht.", "tokens": ["Und", "was", "sie", "that", ",", "das", "mach\u00b7te", "sie", "zum", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wurde sie, die Welt, der Welt zur Plage;", "tokens": ["So", "wur\u00b7de", "sie", ",", "die", "Welt", ",", "der", "Welt", "zur", "Pla\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Mensch war nur ihr Sklave, nur ihr \u2013 \u2013 Knecht.", "tokens": ["Der", "Mensch", "war", "nur", "ihr", "Skla\u00b7ve", ",", "nur", "ihr", "\u2013", "\u2013", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "$(", "$(", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch heut? Verschwunden sind die stolzen Vesten;", "tokens": ["Doch", "heut", "?", "Ver\u00b7schwun\u00b7den", "sind", "die", "stol\u00b7zen", "Ves\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Tr\u00fcmmer mahnen an vergangne Zeiten.", "tokens": ["Nur", "Tr\u00fcm\u00b7mer", "mah\u00b7nen", "an", "ver\u00b7gang\u00b7ne", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bisweilen st\u00f6bert unter diesen Resten", "tokens": ["Bis\u00b7wei\u00b7len", "st\u00f6\u00b7bert", "un\u00b7ter", "die\u00b7sen", "Res\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gegenwart nach Sehensw\u00fcrdigkeiten.", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "nach", "Se\u00b7hens\u00b7w\u00fcr\u00b7dig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und was sie findet, immer ists das Eine,", "tokens": ["Und", "was", "sie", "fin\u00b7det", ",", "im\u00b7mer", "ists", "das", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wovon der Himmel t\u00e4glich zu uns spricht.", "tokens": ["Wo\u00b7von", "der", "Him\u00b7mel", "t\u00e4g\u00b7lich", "zu", "uns", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier sagt ers in der Sprache dieser Steine:", "tokens": ["Hier", "sagt", "ers", "in", "der", "Spra\u00b7che", "die\u00b7ser", "Stei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbdie Welt vergeht; sie kennt die Liebe nicht!\u00ab", "tokens": ["\u00bb", "die", "Welt", "ver\u00b7geht", ";", "sie", "kennt", "die", "Lie\u00b7be", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df uns hinauf zu jenen Bergen steigen,", "tokens": ["La\u00df", "uns", "hin\u00b7auf", "zu", "je\u00b7nen", "Ber\u00b7gen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf denen einst die Macht des Herrn gestanden!", "tokens": ["Auf", "de\u00b7nen", "einst", "die", "Macht", "des", "Herrn", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst mir seiner Liebe Wunder zeigen", "tokens": ["Du", "sollst", "mir", "sei\u00b7ner", "Lie\u00b7be", "Wun\u00b7der", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was von ihnen heut noch ist vorhanden.", "tokens": ["Und", "was", "von", "ih\u00b7nen", "heut", "noch", "ist", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "ADV", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie legte ihre Hand auf jedes Leben,", "tokens": ["Sie", "leg\u00b7te", "ih\u00b7re", "Hand", "auf", "je\u00b7des", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um es zu segnen, gnadenreich und lind,", "tokens": ["Um", "es", "zu", "seg\u00b7nen", ",", "gna\u00b7den\u00b7reich", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wer sich ihr zu Eigen wollte geben,", "tokens": ["Und", "wer", "sich", "ihr", "zu", "Ei\u00b7gen", "woll\u00b7te", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den nahm sie freudig auf, der wahr ihr \u2013 \u2013 Kind.", "tokens": ["Den", "nahm", "sie", "freu\u00b7dig", "auf", ",", "der", "wahr", "ihr", "\u2013", "\u2013", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PRELS", "ADJD", "PPOSAT", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und heut? Noch r\u00fchmen ihn der Himmel Heere;", "tokens": ["Und", "heut", "?", "Noch", "r\u00fch\u00b7men", "ihn", "der", "Him\u00b7mel", "Hee\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch wird auf jedem Stern sein Lob gesungen;", "tokens": ["Noch", "wird", "auf", "je\u00b7dem", "Stern", "sein", "Lob", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch preisen ihn die Berge und die Meere;", "tokens": ["Noch", "prei\u00b7sen", "ihn", "die", "Ber\u00b7ge", "und", "die", "Mee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch ist der Dank f\u00fcr ihn nicht ausgeklungen.", "tokens": ["Noch", "ist", "der", "Dank", "f\u00fcr", "ihn", "nicht", "aus\u00b7ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch stehen seiner Kinder selge Schaaren", "tokens": ["Noch", "ste\u00b7hen", "sei\u00b7ner", "Kin\u00b7der", "sel\u00b7ge", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor seinem Angesichte, dankbereit", "tokens": ["Vor", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ",", "dank\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und h\u00f6ren nimmer auf, zu offenbaren:", "tokens": ["Und", "h\u00f6\u00b7ren", "nim\u00b7mer", "auf", ",", "zu", "of\u00b7fen\u00b7ba\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbdie Liebe Gottes bleibt in Ewigkeit!\u00ab", "tokens": ["\u00bb", "die", "Lie\u00b7be", "Got\u00b7tes", "bleibt", "in", "E\u00b7wig\u00b7keit", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "La\u00df uns hinauf zu jenen Bergen steigen,", "tokens": ["La\u00df", "uns", "hin\u00b7auf", "zu", "je\u00b7nen", "Ber\u00b7gen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf denen einst die Macht der Welt gestanden!", "tokens": ["Auf", "de\u00b7nen", "einst", "die", "Macht", "der", "Welt", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst mir ihre starken Burgen zeigen", "tokens": ["Du", "sollst", "mir", "ih\u00b7re", "star\u00b7ken", "Bur\u00b7gen", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was von ihnen heut noch ist vorhanden.", "tokens": ["Und", "was", "von", "ih\u00b7nen", "heut", "noch", "ist", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], 