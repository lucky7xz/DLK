ta"}, "line.7": {"text": "Hier sagt ers in der Sprache dieser Steine:", "tokens": ["Hier", "sagt", "ers", "in", "der", "Spra\u00b7che", "die\u00b7ser", "Stei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbdie Welt vergeht; sie kennt die Liebe nicht!\u00ab", "tokens": ["\u00bb", "die", "Welt", "ver\u00b7geht", ";", "sie", "kennt", "die", "Lie\u00b7be", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "La\u00df uns hinauf zu jenen Bergen steigen,", "tokens": ["La\u00df", "uns", "hin\u00b7auf", "zu", "je\u00b7nen", "Ber\u00b7gen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf denen einst die Macht des Herrn gestanden!", "tokens": ["Auf", "de\u00b7nen", "einst", "die", "Macht", "des", "Herrn", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst mir seiner Liebe Wunder zeigen", "tokens": ["Du", "sollst", "mir", "sei\u00b7ner", "Lie\u00b7be", "Wun\u00b7der", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Un