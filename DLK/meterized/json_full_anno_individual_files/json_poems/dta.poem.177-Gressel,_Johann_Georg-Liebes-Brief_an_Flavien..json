{"dta.poem.177": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an  Flavien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Darf ein verloffner Knecht/ der offt den Eyd gebrochen/", "tokens": ["Darf", "ein", "ver\u00b7loff\u00b7ner", "Knecht", "/", "der", "offt", "den", "Eyd", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$(", "ART", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich bey der Fahnen wol ohn Straffe finden ein?", "tokens": ["Sich", "bey", "der", "Fah\u00b7nen", "wol", "ohn", "Straf\u00b7fe", "fin\u00b7den", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird nicht nach Krieges-Recht das Urtheil ihm gesprochen/", "tokens": ["Wird", "nicht", "nach", "Krie\u00b7ge\u00b7sRecht", "das", "Ur\u00b7theil", "ihm", "ge\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er ein Opffer soll des engen Stranges seyn?", "tokens": ["Da\u00df", "er", "ein", "Opf\u00b7fer", "soll", "des", "en\u00b7gen", "Stran\u00b7ges", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil er zu offte schon/ den festen Eyd zerrissen", "tokens": ["Weil", "er", "zu", "off\u00b7te", "schon", "/", "den", "fes\u00b7ten", "Eyd", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVFIN", "ADV", "$(", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und die Barmhertzigkeit des Richters hat verh\u00f6hnt/", "tokens": ["Und", "die", "Barm\u00b7hert\u00b7zig\u00b7keit", "des", "Rich\u00b7ters", "hat", "ver\u00b7h\u00f6hnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nein! vor den Meyn-Eyd mu\u00df er mit den Leben b\u00fcssen/", "tokens": ["Nein", "!", "vor", "den", "Meyn\u00b7Eyd", "mu\u00df", "er", "mit", "den", "Le\u00b7ben", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So anders ", "tokens": ["So", "an\u00b7ders"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Doch pflegt ein ", "tokens": ["Doch", "pflegt", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Erlassen von der Straff auf seine Wacht zu gehn/", "tokens": ["Er\u00b7las\u00b7sen", "von", "der", "Straff", "auf", "sei\u00b7ne", "Wacht", "zu", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn nach den Rechten selbst der Straffe-Last zerf\u00e4llet/", "tokens": ["Denn", "nach", "den", "Rech\u00b7ten", "selbst", "der", "Straf\u00b7fe\u00b7Last", "zer\u00b7f\u00e4l\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So auf den Trommel-Schlag sich l\u00e4st der S\u00fcnder sehn.", "tokens": ["So", "auf", "den", "Trom\u00b7mel\u00b7Schlag", "sich", "l\u00e4st", "der", "S\u00fcn\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So mein\u2019 ich ", "tokens": ["So", "mein'", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Weil mein verloffner Sinn sich zu den F\u00fcssen legt", "tokens": ["Weil", "mein", "ver\u00b7loff\u00b7ner", "Sinn", "sich", "zu", "den", "F\u00fcs\u00b7sen", "legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die er als seine Fahn mit aller Lust umpfangen/", "tokens": ["Die", "er", "als", "sei\u00b7ne", "Fahn", "mit", "al\u00b7ler", "Lust", "um\u00b7pfan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Eh als mein Unbestand den Meyn-Eyd hat erregt.", "tokens": ["Eh", "als", "mein", "Un\u00b7be\u00b7stand", "den", "Meyn\u00b7Eyd", "hat", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Soll nun mein leichter Si\u00f1 des Meyn-Eyds Strafe schmecken!", "tokens": ["Soll", "nun", "mein", "leich\u00b7ter", "Si\u00f1", "des", "Meyn\u00b7Eyds", "Stra\u00b7fe", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So thue ", "tokens": ["So", "thue"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Mich wird der herbe Tod zu keiner Zeit erschrecken/", "tokens": ["Mich", "wird", "der", "her\u00b7be", "Tod", "zu", "kei\u00b7ner", "Zeit", "er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn von so lieber Hand er mir wird angethan.", "tokens": ["Wenn", "von", "so", "lie\u00b7ber", "Hand", "er", "mir", "wird", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ADV", "NN", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kein Strick noch Schwerdt ist Noht/ die Blitz beseelten Augen/", "tokens": ["Kein", "Strick", "noch", "Schwerdt", "ist", "Noht", "/", "die", "Blitz", "be\u00b7seel\u00b7ten", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "VAFIN", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die k\u00f6nnen ", "tokens": ["Die", "k\u00f6n\u00b7nen"], "token_info": ["word", "word"], "pos": ["PDS", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Schon meines Lebens-Krafft aus meinen Hertzen saugen/", "tokens": ["Schon", "mei\u00b7nes", "Le\u00b7bens\u00b7Krafft", "aus", "mei\u00b7nen", "Hert\u00b7zen", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df mein schon matter Geist f\u00e4llt wie die Blumen hin.", "tokens": ["Da\u00df", "mein", "schon", "mat\u00b7ter", "Geist", "f\u00e4llt", "wie", "die", "Blu\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Kein gifftger ", "tokens": ["Kein", "gifft\u00b7ger"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Ist zur Bestraffung noht/ blo\u00df deiner Augen-Strahl", "tokens": ["Ist", "zur", "Be\u00b7straf\u00b7fung", "noht", "/", "blo\u00df", "dei\u00b7ner", "Au\u00b7gen\u00b7Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "NN", "$(", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vollzieht und h\u00e4lt zugleich das traurge Hals-Gerichte/", "tokens": ["Voll\u00b7zieht", "und", "h\u00e4lt", "zu\u00b7gleich", "das", "traur\u00b7ge", "Hals\u00b7Ge\u00b7rich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und martert meinen Geist mit \u00fcberh\u00e4uffter Quaal.", "tokens": ["Und", "mar\u00b7tert", "mei\u00b7nen", "Geist", "mit", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7ter", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wann aber ", "tokens": ["Wann", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Da\u00df deine Gnaden-Sonn mit hellen Strahl auffgeht/", "tokens": ["Da\u00df", "dei\u00b7ne", "Gna\u00b7den\u00b7Sonn", "mit", "hel\u00b7len", "Strahl", "auff\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So dann mein ", "tokens": ["So", "dann", "mein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.32": {"text": "Und freudig ausgestreckt in vollem Schmucke steht.", "tokens": ["Und", "freu\u00b7dig", "aus\u00b7ge\u00b7streckt", "in", "vol\u00b7lem", "Schmu\u00b7cke", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wilt du nun ", "tokens": ["Wilt", "du", "nun"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "So werd ich Sonnen-Blum ja mehr als ", "tokens": ["So", "werd", "ich", "Son\u00b7nen\u00b7Blum", "ja", "mehr", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "PIAT", "KOKOM"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.35": {"text": "Ich werde seyn begl\u00fcckt vor andern auf der Erden/", "tokens": ["Ich", "wer\u00b7de", "seyn", "be\u00b7gl\u00fcckt", "vor", "an\u00b7dern", "auf", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "VVPP", "APPR", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Solt es den ", "tokens": ["Solt", "es", "den"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Ich werde auf der Welt des Himmels-Vorschmack sp\u00fchren/", "tokens": ["Ich", "wer\u00b7de", "auf", "der", "Welt", "des", "Him\u00b7mels\u00b7Vor\u00b7schmack", "sp\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Ich werde meine Hand nach solchen Fr\u00fcchten f\u00fchren", "tokens": ["Ich", "wer\u00b7de", "mei\u00b7ne", "Hand", "nach", "sol\u00b7chen", "Fr\u00fcch\u00b7ten", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wodurch mein mattes Hertz mit Wollust wird erfrischt.", "tokens": ["Wo\u00b7durch", "mein", "mat\u00b7tes", "Hertz", "mit", "Wol\u00b7lust", "wird", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.42": {"text": "So wird mein matter Geist auf Zucker-Rosen gehn/", "tokens": ["So", "wird", "mein", "mat\u00b7ter", "Geist", "auf", "Zu\u00b7cke\u00b7rRo\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "L\u00e4st dein erz\u00fcrnter Sinn des Zornes-Flammen schwinden?", "tokens": ["L\u00e4st", "dein", "er\u00b7z\u00fcrn\u00b7ter", "Sinn", "des", "Zor\u00b7nes\u00b7Flam\u00b7men", "schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So kan ich nach der Nacht den frohen Morgen sehn.", "tokens": ["So", "kan", "ich", "nach", "der", "Nacht", "den", "fro\u00b7hen", "Mor\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Hier schliesset sich mein Brief hier k\u00e4umet auf mein Hoffen/", "tokens": ["Hier", "schlies\u00b7set", "sich", "mein", "Brief", "hier", "k\u00e4u\u00b7met", "auf", "mein", "Hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df meine ", "tokens": ["Da\u00df", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Gewi\u00df mein bl\u00f6der Geist jetzt hast du es getroffen/", "tokens": ["Ge\u00b7wi\u00df", "mein", "bl\u00f6\u00b7der", "Geist", "jetzt", "hast", "du", "es", "ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Denn meine ", "tokens": ["Denn", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}