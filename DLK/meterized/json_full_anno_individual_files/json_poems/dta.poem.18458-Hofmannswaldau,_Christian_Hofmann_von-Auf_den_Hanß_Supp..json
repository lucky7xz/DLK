{"dta.poem.18458": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Han\u00df Supp.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Han\u00df Supp fuhr auf der see/ und als ein sturm entstand/", "tokens": ["Han\u00df", "Supp", "fuhr", "auf", "der", "see", "/", "und", "als", "ein", "sturm", "ent\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "KOUS", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vermeinte jederman/ das schif w\u00fcrd untersincken/", "tokens": ["Ver\u00b7mein\u00b7te", "je\u00b7der\u00b7man", "/", "das", "schif", "w\u00fcrd", "un\u00b7ter\u00b7sin\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "ART", "ADJD", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum nam der di\u00df/ der das/ han\u00df Supp ergrif zur hand", "tokens": ["Drum", "nam", "der", "di\u00df", "/", "der", "das", "/", "han\u00df", "Supp", "er\u00b7grif", "zur", "hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "PDS", "$(", "ART", "ART", "$(", "NE", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den anker/ und vermeint/ jetzt k\u00f6nt er nicht ertrincken.", "tokens": ["Den", "an\u00b7ker", "/", "und", "ver\u00b7meint", "/", "jetzt", "k\u00f6nt", "er", "nicht", "er\u00b7trin\u00b7cken", "."], "token_info": ["word", "word", "pu