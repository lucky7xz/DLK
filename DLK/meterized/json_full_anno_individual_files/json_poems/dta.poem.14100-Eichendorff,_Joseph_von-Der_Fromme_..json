{"dta.poem.14100": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fromme .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da sieht sie drau\u00dfen Str\u00f6me, hell ergangen,", "tokens": ["Da", "sieht", "sie", "drau\u00b7\u00dfen", "Str\u00f6\u00b7me", ",", "hell", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch zaub'risch Land viel Pilger, S\u00e4nger wallen,", "tokens": ["Durch", "zaub'\u00b7risch", "Land", "viel", "Pil\u00b7ger", ",", "S\u00e4n\u00b7ger", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "PIAT", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchl rauscht der Wald, die lust'gen H\u00f6rner schallen,", "tokens": ["K\u00fchl", "rauscht", "der", "Wald", ",", "die", "lust'\u00b7gen", "H\u00f6r\u00b7ner", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aurora scheint, so weit die Blicke langen. \u2014", "tokens": ["Au\u00b7ro\u00b7ra", "scheint", ",", "so", "weit", "die", "Bli\u00b7cke", "lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}