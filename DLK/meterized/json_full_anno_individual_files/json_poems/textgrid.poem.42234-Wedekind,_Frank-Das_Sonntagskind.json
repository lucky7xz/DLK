{"textgrid.poem.42234": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Das Sonntagskind", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets naht das Gl\u00fcck in lichter Sonnenpracht,", "tokens": ["Stets", "naht", "das", "Gl\u00fcck", "in", "lich\u00b7ter", "Son\u00b7nen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleichg\u00fcltig, kalt vor\u00fcber mir zu wandern.", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", ",", "kalt", "vor\u00b7\u00fc\u00b7ber", "mir", "zu", "wan\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein junges Morgenrot verschlingt die Nacht,", "tokens": ["Mein", "jun\u00b7ges", "Mor\u00b7gen\u00b7rot", "ver\u00b7schlingt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Indes ein heller Freudenschimmer lacht", "tokens": ["In\u00b7des", "ein", "hel\u00b7ler", "Freu\u00b7den\u00b7schim\u00b7mer", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In den verkl\u00e4rten Augen eines andern.", "tokens": ["In", "den", "ver\u00b7kl\u00e4r\u00b7ten", "Au\u00b7gen", "ei\u00b7nes", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Sonntagskind! \u2013 Mir war sie niemals hold,", "tokens": ["Ein", "Sonn\u00b7tags\u00b7kind", "!", "\u2013", "Mir", "war", "sie", "nie\u00b7mals", "hold", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die blinde Dame mit den vollen H\u00e4nden.", "tokens": ["Die", "blin\u00b7de", "Da\u00b7me", "mit", "den", "vol\u00b7len", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So manchen Opferdienst ich ihr gezollt,", "tokens": ["So", "man\u00b7chen", "Op\u00b7fer\u00b7dienst", "ich", "ihr", "ge\u00b7zollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie schwebt dahin, um Gold und Minnesold", "tokens": ["Sie", "schwebt", "da\u00b7hin", ",", "um", "Gold", "und", "Min\u00b7ne\u00b7sold"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUI", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An ihren Gunstbegl\u00fcckten zu verschwenden.", "tokens": ["An", "ih\u00b7ren", "Gunst\u00b7be\u00b7gl\u00fcck\u00b7ten", "zu", "ver\u00b7schwen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O der verruchten Un