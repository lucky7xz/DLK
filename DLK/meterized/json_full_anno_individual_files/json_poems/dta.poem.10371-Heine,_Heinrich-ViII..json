{"dta.poem.10371": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Du sah'st mich oft im Kampf mit jenen Schlingeln,", "tokens": ["Du", "sah'st", "mich", "oft", "im", "Kampf", "mit", "je\u00b7nen", "Schlin\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschminkten Katzen und gebrillten Pudeln,", "tokens": ["Ge\u00b7schmink\u00b7ten", "Kat\u00b7zen", "und", "ge\u00b7brill\u00b7ten", "Pu\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die mir den blanken Namen gern besudeln,", "tokens": ["Die", "mir", "den", "blan\u00b7ken", "Na\u00b7men", "gern", "be\u00b7su\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich so gerne in's Verderben z\u00fcngeln.", "tokens": ["Und", "mich", "so", "ger\u00b7ne", "in's", "Ver\u00b7der\u00b7ben", "z\u00fcn\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du sahest oft, wie mich Pedanten hudeln,", "tokens": ["Du", "sa\u00b7hest", "oft", ",", "wie", "mich", "Pe\u00b7dan\u00b7ten", "hu\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Schellenkappentr\u00e4ger mich umklingeln,", "tokens": ["Wie", "Schel\u00b7len\u00b7kap\u00b7pen\u00b7tr\u00e4\u00b7ger", "mich", "um\u00b7klin\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie gift'ge Schlangen um mein Herz sich ringeln;", "tokens": ["Wie", "gift'\u00b7ge", "Schlan\u00b7gen", "um", "mein", "Herz", "sich", "rin\u00b7geln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du sahst mein Blut aus tausend Wunden sprudeln.", "tokens": ["Du", "sahst", "mein", "Blut", "aus", "tau\u00b7send", "Wun\u00b7den", "spru\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du aber standest fest gleich einem Thurme;", "tokens": ["Du", "a\u00b7ber", "stan\u00b7dest", "fest", "gleich", "ei\u00b7nem", "Thur\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein Leuchtthurm war dein Kopf mir in dem Sturme,", "tokens": ["Ein", "Leucht\u00b7thurm", "war", "dein", "Kopf", "mir", "in", "dem", "Stur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dein treues Herz war mir ein guter Hafen.", "tokens": ["Dein", "treu\u00b7es", "Herz", "war", "mir", "ein", "gu\u00b7ter", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wohl wogt um jenen Hafen wilde Brandung,", "tokens": ["Wohl", "wogt", "um", "je\u00b7nen", "Ha\u00b7fen", "wil\u00b7de", "Bran\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nur wen'ge Schiff' erringen dort die Landung,", "tokens": ["Nur", "wen'\u00b7ge", "Schiff'", "er\u00b7rin\u00b7gen", "dort", "die", "Lan\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Doch ist man dort, so kann man sicher schlafen.", "tokens": ["Doch", "ist", "man", "dort", ",", "so", "kann", "man", "si\u00b7cher", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "$,", "ADV", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}