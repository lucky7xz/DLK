{"textgrid.poem.53070": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mensch klagt jetzt die Nacht, das Meer klagt seine Winde,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch klagt jetzt die Nacht, das Meer klagt seine Winde,", "tokens": ["Der", "Mensch", "klagt", "jetzt", "die", "Nacht", ",", "das", "Meer", "klagt", "sei\u00b7ne", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die es rasend wird vnd auff vnd abwerts steigt,", "tokens": ["Durch", "die", "es", "ra\u00b7send", "wird", "vnd", "auff", "vnd", "ab\u00b7werts", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "VAFIN", "KON", "APPR", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Wald den wilden Frost, wann jhm sein Safft vertreugt,", "tokens": ["Der", "Wald", "den", "wil\u00b7den", "Frost", ",", "wann", "jhm", "sein", "Safft", "ver\u00b7treugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein Laub entfellt, sein Baum tr\u00e4gt nichts als Aest vnd Rinde,", "tokens": ["Sein", "Laub", "ent\u00b7fellt", ",", "sein", "Baum", "tr\u00e4gt", "nichts", "als", "A\u00b7est", "vnd", "Rin\u00b7de", ","], "token_inf