{"dta.poem.15594": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Land der Kindheit lie\u00df ich hinterm R\u00fccken liegen,", "tokens": ["Das", "Land", "der", "Kind\u00b7heit", "lie\u00df", "ich", "hin\u00b7term", "R\u00fc\u00b7cken", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vorw\u00e4rts wie der Schritt begann der Blick zu fliegen.", "tokens": ["Und", "vor\u00b7w\u00e4rts", "wie", "der", "Schritt", "be\u00b7gann", "der", "Blick", "zu", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich hatte Muth und Trieb allein, bergan zu gehn,", "tokens": ["Ich", "hat\u00b7te", "Muth", "und", "Trieb", "al\u00b7lein", ",", "ber\u00b7gan", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "ADV", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keine Lust noch Zeit, einmal zur\u00fcck zu sehn.", "tokens": ["Und", "kei\u00b7ne", "Lust", "noch", "Zeit", ",", "ein\u00b7mal", "zu\u00b7r\u00fcck", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "NN", "$,", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann als ich umschaun wollt' auf halber H\u00f6he droben,", "tokens": ["Dann", "als", "ich", "um\u00b7schaun", "wollt'", "auf", "hal\u00b7ber", "H\u00f6\u00b7he", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da hatt' ein H\u00fcgelland dazwischen sich geschoben.", "tokens": ["Da", "hatt'", "ein", "H\u00fc\u00b7gel\u00b7land", "da\u00b7zwi\u00b7schen", "sich", "ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch als ich angelangt nun auf dem Gipfel war,", "tokens": ["Doch", "als", "ich", "an\u00b7ge\u00b7langt", "nun", "auf", "dem", "Gip\u00b7fel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da lag das sch\u00f6ne Thal in Fernen d\u00e4mmerklar.", "tokens": ["Da", "lag", "das", "sch\u00f6\u00b7ne", "Thal", "in", "Fer\u00b7nen", "d\u00e4m\u00b7mer\u00b7klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was mir im Reisedrang verschwunden war, vergessen,", "tokens": ["Was", "mir", "im", "Rei\u00b7se\u00b7drang", "ver\u00b7schwun\u00b7den", "war", ",", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit sanfter Wehmuth nun erinnr' ich all mich dessen.", "tokens": ["Mit", "sanf\u00b7ter", "Weh\u00b7muth", "nun", "er\u00b7inn\u00b7r'", "ich", "all", "mich", "des\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PIAT", "PRF", "PDS", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Sehnsucht tr\u00fcge gern zum stillen Thal mich wieder,", "tokens": ["Die", "Sehn\u00b7sucht", "tr\u00fc\u00b7ge", "gern", "zum", "stil\u00b7len", "Thal", "mich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein mein Weg geht dort den andern Abhang nieder.", "tokens": ["Al\u00b7lein", "mein", "Weg", "geht", "dort", "den", "an\u00b7dern", "Ab\u00b7hang", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}