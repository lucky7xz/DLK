{"textgrid.poem.45857": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[als hohe in sich selbst verwandte M\u00e4chte]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als hohe in sich selbst verwandte M\u00e4chte", "tokens": ["Als", "ho\u00b7he", "in", "sich", "selbst", "ver\u00b7wand\u00b7te", "M\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "APPR", "PRF", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In heil'ger Ordnung bildend sich gereiht,", "tokens": ["In", "heil'\u00b7ger", "Ord\u00b7nung", "bil\u00b7dend", "sich", "ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entz\u00fcndete im wechslenden Geschlechte", "tokens": ["Ent\u00b7z\u00fcn\u00b7de\u00b7te", "im", "wechs\u00b7len\u00b7den", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe lebende Beweglichkeit,", "tokens": ["Die", "Lie\u00b7be", "le\u00b7ben\u00b7de", "Be\u00b7weg\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und ward im Beten tiefgeheimer N\u00e4chte,", "tokens": ["Und", "ward", "im", "Be\u00b7ten", "tief\u00b7ge\u00b7hei\u00b7mer", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Menschen jene Fremde eingeweiht,", "tokens": ["Dem", "Men\u00b7schen", "je\u00b7ne", "Frem\u00b7de", "ein\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein stilles Heimweh ist mit dir geboren,", "tokens": ["Ein", "stil\u00b7les", "Heim\u00b7weh", "ist", "mit", "dir", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hast du gleich fr\u00fch den Wanderstab verloren.", "tokens": ["Hast", "du", "gleich", "fr\u00fch", "den", "Wan\u00b7der\u00b7stab", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die T\u00f6ne ziehn dich hin, in sanften Wellen,", "tokens": ["Die", "T\u00f6\u00b7ne", "ziehn", "dich", "hin", ",", "in", "sanf\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rauscht leis ihr Strom in Ufern von Kristall,", "tokens": ["Rauscht", "leis", "ihr", "Strom", "in", "U\u00b7fern", "von", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sirenen buhlen mit der Fahrt Gesellen,", "tokens": ["Si\u00b7re\u00b7nen", "buh\u00b7len", "mit", "der", "Fahrt", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Aus Bergestiefen gr\u00fc\u00dft sie das Metall,", "tokens": ["Aus", "Ber\u00b7ges\u00b7tie\u00b7fen", "gr\u00fc\u00dft", "sie", "das", "Me\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Donner betet, ihre Segel schwellen,", "tokens": ["Der", "Don\u00b7ner", "be\u00b7tet", ",", "ih\u00b7re", "Se\u00b7gel", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus Ferne ruft der ernste Widerhall;", "tokens": ["Aus", "Fer\u00b7ne", "ruft", "der", "erns\u00b7te", "Wi\u00b7der\u00b7hall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Wimpeln wehn in bunten Melodien,", "tokens": ["Die", "Wim\u00b7peln", "wehn", "in", "bun\u00b7ten", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "O wolltest du mit in die Fremde ziehen.", "tokens": ["O", "woll\u00b7test", "du", "mit", "in", "die", "Frem\u00b7de", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Farben spannen Netze aus, und winken", "tokens": ["Die", "Far\u00b7ben", "span\u00b7nen", "Net\u00b7ze", "aus", ",", "und", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir mit des Aufgangs lebenstrunknem Blick,", "tokens": ["Dir", "mit", "des", "Auf\u00b7gangs", "le\u00b7bens\u00b7trunk\u00b7nem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "In ihren Strahlen Br\u00fcderschaft zu trinken.", "tokens": ["In", "ih\u00b7ren", "Strah\u00b7len", "Br\u00fc\u00b7der\u00b7schaft", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Berge weilen sie, und sehn zur\u00fcck \u2013", "tokens": ["Am", "Ber\u00b7ge", "wei\u00b7len", "sie", ",", "und", "sehn", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Willst du nicht auch zur Heimat niedersinken?", "tokens": ["Willst", "du", "nicht", "auch", "zur", "Hei\u00b7mat", "nie\u00b7der\u00b7sin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn von den Sternen d\u00e4mmert dein Geschick,", "tokens": ["Denn", "von", "den", "Ster\u00b7nen", "d\u00e4m\u00b7mert", "dein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die fremde Heimat, spricht es, zu ergr\u00fcnden,", "tokens": ["Die", "frem\u00b7de", "Hei\u00b7mat", ",", "spricht", "es", ",", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sollst du des Lichtes S\u00f6hnen dich verb\u00fcnden.", "tokens": ["Sollst", "du", "des", "Lich\u00b7tes", "S\u00f6h\u00b7nen", "dich", "ver\u00b7b\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch magst du leicht das Vaterland erringen,", "tokens": ["Auch", "magst", "du", "leicht", "das", "Va\u00b7ter\u00b7land", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du der Felsen hartes Herz besiegt,", "tokens": ["Hast", "du", "der", "Fel\u00b7sen", "har\u00b7tes", "Herz", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Marmor wird in s\u00fc\u00dfem Schmerz erklingen,", "tokens": ["Der", "Mar\u00b7mor", "wird", "in", "s\u00fc\u00b7\u00dfem", "Schmerz", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der tot und stumm in deinem Wege liegt:", "tokens": ["Der", "tot", "und", "stumm", "in", "dei\u00b7nem", "We\u00b7ge", "liegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn deine Arme gl\u00fchend ihn umschlingen,", "tokens": ["Wenn", "dei\u00b7ne", "Ar\u00b7me", "gl\u00fc\u00b7hend", "ihn", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df er sich deinem Bilde liebend schmiegt;", "tokens": ["Da\u00df", "er", "sich", "dei\u00b7nem", "Bil\u00b7de", "lie\u00b7bend", "schmiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann f\u00fchrt dich gern zu jenen fremden Landen,", "tokens": ["Dann", "f\u00fchrt", "dich", "gern", "zu", "je\u00b7nen", "frem\u00b7den", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein Gott, du selbst, aus ihm und dir erstanden.", "tokens": ["Dein", "Gott", ",", "du", "selbst", ",", "aus", "ihm", "und", "dir", "er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADV", "$,", "APPR", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dich schreckt so stiller Gang, so schwer Bem\u00fchen,", "tokens": ["Dich", "schreckt", "so", "stil\u00b7ler", "Gang", ",", "so", "schwer", "Be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sehnest dich in alle Liebe hin,", "tokens": ["Du", "seh\u00b7nest", "dich", "in", "al\u00b7le", "Lie\u00b7be", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Marmors kalte Lippe will nicht gl\u00fchen,", "tokens": ["Des", "Mar\u00b7mors", "kal\u00b7te", "Lip\u00b7pe", "will", "nicht", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Farbe spottet deiner H\u00e4nde Sinn,", "tokens": ["Die", "Far\u00b7be", "spot\u00b7tet", "dei\u00b7ner", "H\u00e4n\u00b7de", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die T\u00f6ne singen Liebe dir und fliehen,", "tokens": ["Die", "T\u00f6\u00b7ne", "sin\u00b7gen", "Lie\u00b7be", "dir", "und", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gewinnst du nicht, so werde selbst Gewinn,", "tokens": ["Ge\u00b7winnst", "du", "nicht", ",", "so", "wer\u00b7de", "selbst", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Entwickle dich in Form, und Licht, und T\u00f6nen,", "tokens": ["Ent\u00b7wick\u00b7le", "dich", "in", "Form", ",", "und", "Licht", ",", "und", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So wird der Heimat B\u00fcrgerkranz dich kr\u00f6nen.", "tokens": ["So", "wird", "der", "Hei\u00b7mat", "B\u00fcr\u00b7ger\u00b7kranz", "dich", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O freier Geist, du unerfa\u00dflich Leben,", "tokens": ["O", "frei\u00b7er", "Geist", ",", "du", "un\u00b7er\u00b7fa\u00df\u00b7lich", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesang der Farbe, Formenharmonie,", "tokens": ["Ge\u00b7sang", "der", "Far\u00b7be", ",", "For\u00b7men\u00b7har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gestalt des Tons, du hell lebendig Weben,", "tokens": ["Ge\u00b7stalt", "des", "Tons", ",", "du", "hell", "le\u00b7ben\u00b7dig", "We\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Nacht und Tod, in Stummheit Melodie,", "tokens": ["In", "Nacht", "und", "Tod", ",", "in", "Stumm\u00b7heit", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In meines Busens Saiten tonlos Beben,", "tokens": ["In", "mei\u00b7nes", "Bu\u00b7sens", "Sai\u00b7ten", "ton\u00b7los", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ersteh' in meiner Seele Poesie:", "tokens": ["Er\u00b7steh'", "in", "mei\u00b7ner", "See\u00b7le", "Poe\u00b7sie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df mich in ihrer G\u00f6ttin Wort sie gr\u00fc\u00dfen,", "tokens": ["La\u00df", "mich", "in", "ih\u00b7rer", "G\u00f6t\u00b7tin", "Wort", "sie", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sich der Heimat Tore mir erschlie\u00dfen.", "tokens": ["Da\u00df", "sich", "der", "Hei\u00b7mat", "To\u00b7re", "mir", 