{"textgrid.poem.26430": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt, lieblicher und linder Tod!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, lieblicher und linder Tod!", "tokens": ["Kommt", ",", "lieb\u00b7li\u00b7cher", "und", "lin\u00b7der", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umwoge die Welt, heiter nahend, nahend", "tokens": ["Um\u00b7wo\u00b7ge", "die", "Welt", ",", "hei\u00b7ter", "na\u00b7hend", ",", "na\u00b7hend"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Tage, bei Nacht, allen, jedem,", "tokens": ["Am", "Ta\u00b7ge", ",", "bei", "Nacht", ",", "al\u00b7len", ",", "je\u00b7dem", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$,", "PIAT", "$,", "PIS", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fr\u00fch oder sp\u00e4ter, lieblicher Tod.", "tokens": ["Fr\u00fch", "o\u00b7der", "sp\u00e4\u00b7ter", ",", "lieb\u00b7li\u00b7cher", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Gelobt sei das unergr\u00fcndliche All,", "tokens": ["Ge\u00b7lobt", "sei", "das", "un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7che", "All", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr Leben und Lust und f\u00fcr die Dinge wundersam und das Forschen,", "tokens": ["F\u00fcr", "Le\u00b7ben", "und", "Lust", "und", "f\u00fcr", "die", "Din\u00b7ge", "wun\u00b7der\u00b7sam", "und", "das", "For\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPR", "ART", "NN", "ADJD", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und f\u00fcr die Liebe, die s\u00fc\u00dfe Liebe! Doch dreifach gelobt", "tokens": ["Und", "f\u00fcr", "die", "Lie\u00b7be", ",", "die", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", "!", "Doch", "drei\u00b7fach", "ge\u00b7lobt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$.", "KON", "PAV", "VVPP"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die sicher umwindenden Arme des k\u00fchl umfaltenden Todes!", "tokens": ["Die", "si\u00b7cher", "um\u00b7win\u00b7den\u00b7den", "Ar\u00b7me", "des", "k\u00fchl", "um\u00b7fal\u00b7ten\u00b7den", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+--+-", "measure": "amphibrach.tetra.plus"}}, "stanza.3": {"line.1": {"text": "Dunkle Mutter, n\u00e4her gleitend, leisen Fu\u00dfes,", "tokens": ["Dunk\u00b7le", "Mut\u00b7ter", ",", "n\u00e4\u00b7her", "glei\u00b7tend", ",", "lei\u00b7sen", "Fu\u00b7\u00dfes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Hat dir noch niemand ein Lied des Willkommens gesungen?", "tokens": ["Hat", "dir", "noch", "nie\u00b7mand", "ein", "Lied", "des", "Will\u00b7kom\u00b7mens", "ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So singe ich eins! Dich will ich preisen vor allen,", "tokens": ["So", "sin\u00b7ge", "ich", "eins", "!", "Dich", "will", "ich", "prei\u00b7sen", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dir bring ich mein Lied, damit du ohne zu wanken kommst,", "tokens": ["Dir", "bring", "ich", "mein", "Lied", ",", "da\u00b7mit", "du", "oh\u00b7ne", "zu", "wan\u00b7ken", "kommst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "KOUI", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Wenn du kommen mu\u00dft.", "tokens": ["Wenn", "du", "kom\u00b7men", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Komm, starke Erl\u00f6serin!", "tokens": ["Komm", ",", "star\u00b7ke", "Er\u00b7l\u00f6\u00b7se\u00b7rin", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn es so ist, nimmst du sie hin, preise ich freudig die Toten,", "tokens": ["Wenn", "es", "so", "ist", ",", "nimmst", "du", "sie", "hin", ",", "prei\u00b7se", "ich", "freu\u00b7dig", "die", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "----+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die du im Meer deiner gleitenden G\u00fcte", "tokens": ["Die", "du", "im", "Meer", "dei\u00b7ner", "glei\u00b7ten\u00b7den", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Badest in seligen, Fluten des Friedens, o Tod!", "tokens": ["Ba\u00b7dest", "in", "se\u00b7li\u00b7gen", ",", "Flu\u00b7ten", "des", "Frie\u00b7dens", ",", "o", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "$,", "NN", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.5": {"line.1": {"text": "Dir gilt mein fr\u00f6hliches St\u00e4ndchen,", "tokens": ["Dir", "gilt", "mein", "fr\u00f6h\u00b7li\u00b7ches", "St\u00e4nd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "T\u00e4nze schlag ich dir vor, Feste und schm\u00fcckende Zier;", "tokens": ["T\u00e4n\u00b7ze", "schlag", "ich", "dir", "vor", ",", "Fes\u00b7te", "und", "schm\u00fc\u00b7cken\u00b7de", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dir frommt das Lachen der Landschaft, der hochumspannende Himmel,", "tokens": ["Dir", "frommt", "das", "La\u00b7chen", "der", "Land\u00b7schaft", ",", "der", "hoch\u00b7um\u00b7span\u00b7nen\u00b7de", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Frische der Felder und die gro\u00dfe, gedankenvolle Nacht!", "tokens": ["Die", "Fri\u00b7sche", "der", "Fel\u00b7der", "und", "die", "gro\u00b7\u00dfe", ",", "ge\u00b7dan\u00b7ken\u00b7vol\u00b7le", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Meeresstrand mit den raunenden Wellen,", "tokens": ["Der", "Mee\u00b7res\u00b7strand", "mit", "den", "rau\u00b7nen\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Deren Stimme ich kenne,", "tokens": ["De\u00b7ren", "Stim\u00b7me", "ich", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wenn die Seele dir zugewendet,", "tokens": ["Wenn", "die", "See\u00b7le", "dir", "zu\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und der Leib dankbar sich an dich schmiegt,", "tokens": ["Und", "der", "Leib", "dank\u00b7bar", "sich", "an", "dich", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Du weitwirkender, wohlumschleierter Tod!", "tokens": ["Du", "weit\u00b7wir\u00b7ken\u00b7der", ",", "woh\u00b7lum\u00b7schlei\u00b7er\u00b7ter", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "\u00dcber den Baumwipfeln sing ich dir zu,", "tokens": ["\u00dc\u00b7ber", "den", "Baum\u00b7wip\u00b7feln", "sing", "ich", "dir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "\u00dcber den steigenden, fallenden Wogen,", "tokens": ["\u00dc\u00b7ber", "den", "stei\u00b7gen\u00b7den", ",", "fal\u00b7len\u00b7den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "\u00dcber den Ackergefilden und den weiten Pr\u00e4rien,", "tokens": ["\u00dc\u00b7ber", "den", "A\u00b7cker\u00b7ge\u00b7fil\u00b7den", "und", "den", "wei\u00b7ten", "Pr\u00e4\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "\u00dcber den eng gepackten St\u00e4dten, Werften und Wegen,", "tokens": ["\u00dc\u00b7ber", "den", "eng", "ge\u00b7pack\u00b7ten", "St\u00e4d\u00b7ten", ",", "Werf\u00b7ten", "und", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Mit ihrem Waren- und Menschengewimmel,", "tokens": ["Mit", "ih\u00b7rem", "Wa\u00b7ren", "und", "Men\u00b7schen\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Singe ich diesen Lobgesang mit Freuden dir zu, o Tod!", "tokens": ["Sin\u00b7ge", "ich", "die\u00b7sen", "Lob\u00b7ge\u00b7sang", "mit", "Freu\u00b7den", "dir", "zu", ",", "o", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "APPR", "NN", "PPER", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "+--+-+-+-+--+-+", "measure": "iambic.septa.invert"}, "line.7": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.8": {"text": "Zum Gegenklang meiner Seele", "tokens": ["Zum", "Ge\u00b7gen\u00b7klang", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Rief in reinen ruhigen T\u00f6nen", "tokens": ["Rief", "in", "rei\u00b7nen", "ru\u00b7hi\u00b7gen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Laut und gehalten der graubraune Vogel,", "tokens": ["Laut", "und", "ge\u00b7hal\u00b7ten", "der", "grau\u00b7brau\u00b7ne", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Dessen Stimme weithin die Nacht erf\u00fcllt.", "tokens": ["Des\u00b7sen", "Stim\u00b7me", "weit\u00b7hin", "die", "Nacht", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Laut in den F\u00f6hren und Zedern,", "tokens": ["Laut", "in", "den", "F\u00f6h\u00b7ren", "und", "Ze\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Klar in der feuchten Frische, \u00fcber dem Sumpfgeruch \u2013", "tokens": ["Klar", "in", "der", "feuch\u00b7ten", "Fri\u00b7sche", ",", "\u00fc\u00b7ber", "dem", "Sumpf\u00b7ge\u00b7ruch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$("], "meter": "---+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ich mit meinem Gef\u00e4hrten dort in der Nacht.", "tokens": ["Und", "ich", "mit", "mei\u00b7nem", "Ge\u00b7f\u00e4hr\u00b7ten", "dort", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "An meinen ungeschlossenen Augen vorbei", "tokens": ["An", "mei\u00b7nen", "un\u00b7ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7gen", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Zog ein Panorama von Bildern.", "tokens": ["Zog", "ein", "Pa\u00b7no\u00b7ra\u00b7ma", "von", "Bil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sah eine schr\u00e4ge Schlachtordnung;", "tokens": ["Ich", "sah", "ei\u00b7ne", "schr\u00e4\u00b7ge", "Schlach\u00b7tord\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dann, wie im Traum, lautlos, Hunderte von Fahnen", "tokens": ["Dann", ",", "wie", "im", "Traum", ",", "laut\u00b7los", ",", "Hun\u00b7der\u00b7te", "von", "Fah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "$,", "ADJD", "$,", "NN", "APPR", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Durch den Qualm der Gesch\u00fctze getragen,", "tokens": ["Durch", "den", "Qualm", "der", "Ge\u00b7sch\u00fct\u00b7ze", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Durchl\u00f6chert von Kugeln,", "tokens": ["Durch\u00b7l\u00f6\u00b7chert", "von", "Ku\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Hin und her im Dampf, zerrissen und blutig,", "tokens": ["Hin", "und", "her", "im", "Dampf", ",", "zer\u00b7ris\u00b7sen", "und", "blu\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPRART", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Bis zuletzt \u2013 immer noch lautlos \u2013 nur wenige Fetzen", "tokens": ["Bis", "zu\u00b7letzt", "\u2013", "im\u00b7mer", "noch", "laut\u00b7los", "\u2013", "nur", "we\u00b7ni\u00b7ge", "Fet\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "$(", "ADV", "ADV", "ADJD", "$(", "ADV", "PIAT", "NN"], "meter": "---+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Flattern an den zersplitterten Schaften.", "tokens": ["Flat\u00b7tern", "an", "den", "zer\u00b7split\u00b7ter\u00b7ten", "Schaf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und die Gerippe von jungen M\u00e4nnern \u2013 ich sah sie \u2013", "tokens": ["Und", "die", "Ge\u00b7rip\u00b7pe", "von", "jun\u00b7gen", "M\u00e4n\u00b7nern", "\u2013", "ich", "sah", "sie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "$("], "meter": "---+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Tr\u00fcmmer aller Gefallenen des gro\u00dfen Krieges.", "tokens": ["Die", "Tr\u00fcm\u00b7mer", "al\u00b7ler", "Ge\u00b7fal\u00b7le\u00b7nen", "des", "gro\u00b7\u00dfen", "Krie\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber ich sah, da\u00df sie anders waren als man gedacht;", "tokens": ["A\u00b7ber", "ich", "sah", ",", "da\u00df", "sie", "an\u00b7ders", "wa\u00b7ren", "als", "man", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "KOUS", "PIS", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sie selber lagen im Frieden, sie litten nicht,", "tokens": ["Sie", "sel\u00b7ber", "la\u00b7gen", "im", "Frie\u00b7den", ",", "sie", "lit\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die Lebenden blieben und litten \u2013 die M\u00fctter litten,", "tokens": ["Die", "Le\u00b7ben\u00b7den", "blie\u00b7ben", "und", "lit\u00b7ten", "\u2013", "die", "M\u00fct\u00b7ter", "lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Die Frauen und Kinder, der sinnende Waffengef\u00e4hrte,", "tokens": ["Die", "Frau\u00b7en", "und", "Kin\u00b7der", ",", "der", "sin\u00b7nen\u00b7de", "Waf\u00b7fen\u00b7ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.7": {"text": "Und die \u00fcbriggebliebenen Heere \u2013 sie litten.", "tokens": ["Und", "die", "\u00fcb\u00b7rig\u00b7ge\u00b7blie\u00b7be\u00b7nen", "Hee\u00b7re", "\u2013", "sie", "lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.10": {"line.1": {"text": "Kommt, lieblicher und linder Tod!", "tokens": ["Kommt", ",", "lieb\u00b7li\u00b7cher", "und", "lin\u00b7der", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umwoge die Welt, heiter nahend, nahend", "tokens": ["Um\u00b7wo\u00b7ge", "die", "Welt", ",", "hei\u00b7ter", "na\u00b7hend", ",", "na\u00b7hend"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Tage, bei Nacht, allen, jedem,", "tokens": ["A