{"textgrid.poem.52176": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Alte Schrift", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst verlockt' es mich im Abendglimmen", "tokens": ["J\u00fcngst", "ver\u00b7lockt'", "es", "mich", "im", "A\u00b7bend\u00b7glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zum Lombardenturm emporzuklimmen,", "tokens": ["Zum", "Lom\u00b7bar\u00b7den\u00b7turm", "em\u00b7por\u00b7zu\u00b7klim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem verschollnen Herrscher hier im Gaue,", "tokens": ["Dem", "ver\u00b7scholl\u00b7nen", "Herr\u00b7scher", "hier", "im", "Gau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der die Ferne noch beherrscht, die blaue.", "tokens": ["Der", "die", "Fer\u00b7ne", "noch", "be\u00b7herrscht", ",", "die", "blau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In den Mauern bin ich lang geblieben:", "tokens": ["In", "den", "Mau\u00b7ern", "bin", "ich", "lang", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-