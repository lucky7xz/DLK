{"dta.poem.15705": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum ist Pfaffengeist so eng und dumpf und klein?", "tokens": ["Wa\u00b7rum", "ist", "Pfaf\u00b7fen\u00b7geist", "so", "eng", "und", "dumpf", "und", "klein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil geistlich vorzugsweis' er will ausschlie\u00dflich seyn.", "tokens": ["Weil", "geist\u00b7lich", "vor\u00b7zugs\u00b7weis'", "er", "will", "aus\u00b7schlie\u00df\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Lebendig ist der Geist, wo er im Leib verharrt;", "tokens": ["Le\u00b7ben\u00b7dig", "ist", "der", "Geist", ",", "wo", "er", "im", "Leib", "ver\u00b7harrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch, wird er selbst ein Leib, ist er zum Tod erstarrt.", "tokens": ["Doch", ",", "wird", "er", "selbst", "ein", "Leib", ",", "ist", "er", "zum", "Tod", "er\u00b7starrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Sauerteiges kannst du nicht im Brot entbehren,", "tokens": ["Des", "Sau\u00b7er\u00b7tei\u00b7ges", "kannst", "du", "nicht", "im", "Brot", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch magst du nicht allein von Sauerteig dich n\u00e4hren.", "tokens": ["Doch", "magst", "du", "nicht", "al\u00b7lein", "von", "Sau\u00b7er\u00b7teig", "dich", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So ist am Kohlgericht auch wol das Salz ersprie\u00dflich,", "tokens": ["So", "ist", "am", "Kohl\u00b7ge\u00b7richt", "auch", "wol", "das", "Salz", "er\u00b7sprie\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ohne Kohl ein Salzgem\u00fcs' ist ungenie\u00dflich.", "tokens": ["Doch", "oh\u00b7ne", "Kohl", "ein", "Sa\u00b7lz\u00b7ge\u00b7m\u00fcs'", "ist", "un\u00b7ge\u00b7nie\u00df\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn ihr das Salz der Welt und Sauerteig wollt seyn,", "tokens": ["Wenn", "ihr", "das", "Salz", "der", "Welt", "und", "Sau\u00b7er\u00b7teig", "wollt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "KON", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So geht bescheiden als Bestandtheil in sie ein!", "tokens": ["So", "geht", "be\u00b7schei\u00b7den", "als", "Be\u00b7stand\u00b7theil", "in", "sie", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KOKOM", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}