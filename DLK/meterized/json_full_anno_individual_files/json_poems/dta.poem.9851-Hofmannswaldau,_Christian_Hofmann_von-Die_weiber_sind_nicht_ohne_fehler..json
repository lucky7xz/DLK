{"dta.poem.9851": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die weiber sind nicht ohne fehler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein weib sey wie es immer sey/", "tokens": ["Ein", "weib", "sey", "wie", "es", "im\u00b7mer", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "PPER", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird ihr doch was fehlen;", "tokens": ["So", "wird", "ihr", "doch", "was", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6ne die ist selten treu/", "tokens": ["Die", "sch\u00f6\u00b7ne", "die", "ist", "sel\u00b7ten", "treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "VAFIN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die garstige macht qu\u00e4len;", "tokens": ["Die", "gars\u00b7ti\u00b7ge", "macht", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die kluge commandirt zu viel/", "tokens": ["Die", "klu\u00b7ge", "com\u00b7man\u00b7dirt", "zu", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die dumme treibt nur narren-spiel;", "tokens": ["Die", "dum\u00b7me", "treibt", "nur", "nar\u00b7ren\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die junge bringt galans ins haus/", "tokens": ["Die", "jun\u00b7ge", "bringt", "ga\u00b7lans", "ins", "haus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NE", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der alten stinckts zum halse raus;", "tokens": ["Der", "al\u00b7ten", "stinckts", "zum", "hal\u00b7se", "raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die reiche l\u00e4st dirs geld nicht frey/", "tokens": ["Die", "rei\u00b7che", "l\u00e4st", "dirs", "geld", "nicht", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "VVFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die arme wird dirs stehlen.", "tokens": ["Die", "ar\u00b7me", "wird", "dirs", "steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ein weib sey wie es immer sey/", "tokens": ["Ein", "weib", "sey", "wie", "es", "im\u00b7mer", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "PPER", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So wird ihr etwas fehlen.", "tokens": ["So", "wird", "ihr", "et\u00b7was", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}