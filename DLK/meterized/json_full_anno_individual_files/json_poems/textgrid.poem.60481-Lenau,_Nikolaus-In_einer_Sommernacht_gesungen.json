{"textgrid.poem.60481": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "In einer Sommernacht gesungen", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dfet, o Nacht, du Freundin stiller Betrachtung!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "o", "Nacht", ",", "du", "Freun\u00b7din", "stil\u00b7ler", "Be\u00b7trach\u00b7tung", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "NN", "$,", "PPER", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Sei der erwachten Natur erhabnem Genusse geheiligt!", "tokens": ["Sei", "der", "er\u00b7wach\u00b7ten", "Na\u00b7tur", "er\u00b7hab\u00b7nem", "Ge\u00b7nus\u00b7se", "ge\u00b7hei\u00b7ligt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hoch auf luftigen Pfaden im weiten Himmelsgefilde", "tokens": ["Hoch", "auf", "luf\u00b7ti\u00b7gen", "Pfa\u00b7den", "im", "wei\u00b7ten", "Him\u00b7mels\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wandelt der Mond und bescheinet die Flur, die vom Walde begrenzt wird,", "tokens": ["Wan\u00b7delt", "der", "Mond", "und", "be\u00b7schei\u00b7net", "die", "Flur", ",", "die", "vom", "Wal\u00b7de", "be\u00b7grenzt", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+--+--+--+--+---+", "measure": "dactylic.tetra.plus"}, "line.5": {"text": "Der mich schweigend empf\u00e4ngt, und hebet die n\u00e4chtliche Feier.", "tokens": ["Der", "mich", "schwei\u00b7gend", "emp\u00b7f\u00e4ngt", ",", "und", "he\u00b7bet", "die", "n\u00e4cht\u00b7li\u00b7che", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Tiefe Stille ringsum \u2013 nur einsame Laute vernehm ich,", "tokens": ["Tie\u00b7fe", "Stil\u00b7le", "ring\u00b7sum", "\u2013", "nur", "ein\u00b7sa\u00b7me", "Lau\u00b7te", "ver\u00b7nehm", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$(", "ADV", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-++--+--+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Die wie T\u00f6ne des Traums dem schlummernden Walde entschweben,", "tokens": ["Die", "wie", "T\u00f6\u00b7ne", "des", "Traums", "dem", "schlum\u00b7mern\u00b7den", "Wal\u00b7de", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und mit r\u00fchrender Macht, als w\u00e4ren es Sprecher des Himmels,", "tokens": ["Und", "mit", "r\u00fch\u00b7ren\u00b7der", "Macht", ",", "als", "w\u00e4\u00b7ren", "es", "Spre\u00b7cher", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "KOKOM", "VAFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "In die Seele mir dringen und wecken die schlummernde Gottheit.", "tokens": ["In", "die", "See\u00b7le", "mir", "drin\u00b7gen", "und", "we\u00b7cken", "die", "schlum\u00b7mern\u00b7de", "Got\u00b7theit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Nacht! du enthebest das Herz der Macht bet\u00f6render L\u00fcste,", "tokens": ["Nacht", "!", "du", "ent\u00b7he\u00b7best", "das", "Herz", "der", "Macht", "be\u00b7t\u00f6\u00b7ren\u00b7der", "L\u00fcs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Und mit Zaubergewalt entstreifst du dem Auge die Binde,", "tokens": ["Und", "mit", "Zau\u00b7ber\u00b7ge\u00b7walt", "ent\u00b7streifst", "du", "dem", "Au\u00b7ge", "die", "Bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Von der Leidenschaft um ihren Vasallen geschlungen.", "tokens": ["Von", "der", "Lei\u00b7den\u00b7schaft", "um", "ih\u00b7ren", "Va\u00b7sal\u00b7len", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "Einem entheiligten Tempel gleichet die s\u00fcndige Seele,", "tokens": ["Ei\u00b7nem", "en\u00b7thei\u00b7lig\u00b7ten", "Tem\u00b7pel", "glei\u00b7chet", "die", "s\u00fcn\u00b7di\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Der den G\u00f6tzen geweiht, gef\u00fcllet mit Bildern des Wahns ist;", "tokens": ["Der", "den", "G\u00f6t\u00b7zen", "ge\u00b7weiht", ",", "ge\u00b7f\u00fcl\u00b7let", "mit", "Bil\u00b7dern", "des", "Wahns", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "VVPP", "APPR", "NN", "ART", "NN", "VAFIN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Doch dein Ernst, o Nacht! erreget des weiseren Lebens", "tokens": ["Doch", "dein", "Ernst", ",", "o", "Nacht", "!", "er\u00b7re\u00b7get", "des", "wei\u00b7se\u00b7ren", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Kr\u00e4ftigen Keim, das Denken ans letzte Verstummen des Menschen;", "tokens": ["Kr\u00e4f\u00b7ti\u00b7gen", "Keim", ",", "das", "Den\u00b7ken", "ans", "letz\u00b7te", "Ver\u00b7stum\u00b7men", "des", "Men\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Denn vom bewegten Gem\u00fct wird jede Erscheinung gedeutet.", "tokens": ["Denn", "vom", "be\u00b7weg\u00b7ten", "Ge\u00b7m\u00fct", "wird", "je\u00b7de", "Er\u00b7schei\u00b7nung", "ge\u00b7deu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Pl\u00f6tzlich birgt nun der Mond sich hinter die schleierne Wolke,", "tokens": ["Pl\u00f6tz\u00b7lich", "birgt", "nun", "der", "Mond", "sich", "hin\u00b7ter", "die", "schlei\u00b7er\u00b7ne", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.19": {"text": "D\u00e4mmerungslicht verbreitend \u00fcber die waldige Gegend,", "tokens": ["D\u00e4m\u00b7me\u00b7rungs\u00b7licht", "ver\u00b7brei\u00b7tend", "\u00fc\u00b7ber", "die", "wal\u00b7di\u00b7ge", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Gleich dem Lichte Vernunft; auch dies wird vom Schleier gedunkelt,", "tokens": ["Gleich", "dem", "Lich\u00b7te", "Ver\u00b7nunft", ";", "auch", "dies", "wird", "vom", "Schlei\u00b7er", "ge\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$.", "ADV", "PDS", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Der den himmlischen Gast der irdischen H\u00fclle verwahret.", "tokens": ["Der", "den", "himm\u00b7li\u00b7schen", "Gast", "der", "ir\u00b7di\u00b7schen", "H\u00fcl\u00b7le", "ver\u00b7wah\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Weiter verfolg ich den Weg, den gefallene Bl\u00fcten bedecken.", "tokens": ["Wei\u00b7ter", "ver\u00b7folg", "ich", "den", "Weg", ",", "den", "ge\u00b7fal\u00b7le\u00b7ne", "Bl\u00fc\u00b7ten", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.23": {"text": "Lange nicht w\u00e4hret die Bl\u00fcte, es sinkt das sch\u00f6ne Gebilde,", "tokens": ["Lan\u00b7ge", "nicht", "w\u00e4h\u00b7ret", "die", "Bl\u00fc\u00b7te", ",", "es", "sinkt", "das", "sch\u00f6\u00b7ne", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Wenns den \u00e4therischen Duft in die wogenden L\u00fcft verhaucht hat.", "tokens": ["Wenns", "den", "\u00e4t\u00b7he\u00b7ri\u00b7schen", "Duft", "in", "die", "wo\u00b7gen\u00b7den", "L\u00fcft", "ver\u00b7haucht", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "Wie die Bl\u00fcte des Baums mu\u00df sinken die Bl\u00fcte der Sch\u00f6pfung,", "tokens": ["Wie", "die", "Bl\u00fc\u00b7te", "des", "Baums", "mu\u00df", "sin\u00b7ken", "die", "Bl\u00fc\u00b7te", "der", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Sinken der Mensch; \u2013 doch gleicht er in allem der Bl\u00fcte, und wird die", "tokens": ["Sin\u00b7ken", "der", "Mensch", ";", "\u2013", "doch", "gleicht", "er", "in", "al\u00b7lem", "der", "Bl\u00fc\u00b7te", ",", "und", "wird", "die"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "PIS", "ART", "NN", "$,", "KON", "VAFIN", "ART"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Menschliche Seele dereinst der Bl\u00fcte verwehetem Hauch gleich?", "tokens": ["Menschli\u00b7che", "See\u00b7le", "de\u00b7reinst", "der", "Bl\u00fc\u00b7te", "ver\u00b7we\u00b7he\u00b7tem", "Hauch", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.28": {"text": "Oder lebet sie fort und lebt, ohne je zu vergehen,", "tokens": ["O\u00b7der", "le\u00b7bet", "sie", "fort", "und", "lebt", ",", "oh\u00b7ne", "je", "zu", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.29": {"text": "Immer sich weitend und inniger immer die Gottheit erfassend?", "tokens": ["Im\u00b7mer", "sich", "wei\u00b7tend", "und", "in\u00b7ni\u00b7ger", "im\u00b7mer", "die", "Got\u00b7theit", "er\u00b7fas\u00b7send", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "KON", "ADJD", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.30": {"text": "Seligster aller Gedanken! vielleicht gedacht auch vom Ewgen", "tokens": ["Se\u00b7ligs\u00b7ter", "al\u00b7ler", "Ge\u00b7dan\u00b7ken", "!", "viel\u00b7leicht", "ge\u00b7dacht", "auch", "vom", "Ew\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$.", "ADV", "VVPP", "ADV", "APPRART", "NN"], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Und gewecket in mir durch seinen empfindbaren Abdruck,", "tokens": ["Und", "ge\u00b7we\u00b7cket", "in", "mir", "durch", "sei\u00b7nen", "emp\u00b7find\u00b7ba\u00b7ren", "Ab\u00b7druck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Durch die Natur! Doch jetzt hemmt liebliche St\u00f6rung den Fortgang", "tokens": ["Durch", "die", "Na\u00b7tur", "!", "Doch", "jetzt", "hemmt", "lieb\u00b7li\u00b7che", "St\u00f6\u00b7rung", "den", "Fort\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "KON", "ADV", "VVFIN", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.33": {"text": "Meiner Betrachtung, es ist das schmelzende Lied Philomelens.", "tokens": ["Mei\u00b7ner", "Be\u00b7trach\u00b7tung", ",", "es", "ist", "das", "schmel\u00b7zen\u00b7de", "Lied", "Phi\u00b7lo\u00b7me\u00b7lens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Der begeisterte Vogel feiert nun jubelnd sein Dasein.", "tokens": ["Der", "be\u00b7geis\u00b7ter\u00b7te", "Vo\u00b7gel", "fei\u00b7ert", "nun", "ju\u00b7belnd", "sein", "Da\u00b7sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Wie mich der Strom melodischer T\u00f6ne s\u00fc\u00df zwingend dahinrei\u00dft,", "tokens": ["Wie", "mich", "der", "Strom", "me\u00b7lo\u00b7di\u00b7scher", "T\u00f6\u00b7ne", "s\u00fc\u00df", "zwin\u00b7gend", "da\u00b7hin\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJA", "NN", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.36": {"text": "Wiegend die horchende Seel im Wechselschwunge des Wohlklangs.", "tokens": ["Wie\u00b7gend", "die", "hor\u00b7chen\u00b7de", "Seel", "im", "Wech\u00b7sel\u00b7schwun\u00b7ge", "des", "Wohl\u00b7klangs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.37": {"text": "Heil dir, herrlicher S\u00e4nger! als Sch\u00f6pfungsgenosse verwandt mir,", "tokens": ["Heil", "dir", ",", "herr\u00b7li\u00b7cher", "S\u00e4n\u00b7ger", "!", "als", "Sch\u00f6p\u00b7fungs\u00b7ge\u00b7nos\u00b7se", "ver\u00b7wandt", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$.", "KOUS", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+--+--+--+-", "