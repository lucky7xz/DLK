, "ge\u00b7we\u00b7cket", "in", "mir", "durch", "sei\u00b7nen", "emp\u00b7find\u00b7ba\u00b7ren", "Ab\u00b7druck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Durch die Natur! Doch jetzt hemmt liebliche St\u00f6rung den Fortgang", "tokens": ["Durch", "die", "Na\u00b7tur", "!", "Doch", "jetzt", "hemmt", "lieb\u00b7li\u00b7che", "St\u00f6\u00b7rung", "den", "Fort\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "KON", "ADV", "VVFIN", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.33": {"text": "Meiner Betrachtung, es ist das schmelzende Lied Philomelens.", "tokens": ["Mei\u00b7ner", "Be\u00b7trach\u00b7tung", ",", "es", "ist", "das", "schmel\u00b7zen\u00b7de", "Lied", "Phi\u00b7lo\u00b7me\u00b7lens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Der begeisterte Vogel feiert nun jubelnd sein Dasein.", "tokens": ["Der", "be\u00b7geis\u00b7ter\u00b7te", "Vo\u00b7gel", "fei\u00b7ert", "nun", "ju\u00b7belnd", "sein", "Da\u00b7sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Wie mich der Strom melodischer T\u00f6ne s\u00fc\u00df zwingend dahinrei\u00dft,", "tokens": ["Wie", "mich", "der", "Strom", "me\u00b7lo\u00b7di\u00b7scher", "T\u00f6\u00b7ne", "s\u00fc\u00df", "zwin\u00b7gend", "da\u00b7hin\u00b7rei