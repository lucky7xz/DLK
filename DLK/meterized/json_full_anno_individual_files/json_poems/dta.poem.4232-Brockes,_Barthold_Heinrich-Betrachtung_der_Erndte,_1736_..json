{"dta.poem.4232": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung der Erndte,  \n  1736 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Menschen, sehet! sehet!", "tokens": ["Ge\u00b7lieb\u00b7te", "Men\u00b7schen", ",", "se\u00b7het", "!", "se\u00b7het", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie itzt der Segen abgem\u00e4het,", "tokens": ["Wie", "itzt", "der", "Se\u00b7gen", "ab\u00b7ge\u00b7m\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In schwehren Garben aufgeh\u00f6het,", "tokens": ["In", "schweh\u00b7ren", "Gar\u00b7ben", "auf\u00b7ge\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier in so s\u00fcsser Ordnung stehet!", "tokens": ["Hier", "in", "so", "s\u00fcs\u00b7ser", "Ord\u00b7nung", "ste\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es seh\u2019 ihn, wer da sehen kann,", "tokens": ["Es", "seh'", "ihn", ",", "wer", "da", "se\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Lust, doch nicht ohn\u2019 Ehrfurcht, an,", "tokens": ["Mit", "Lust", ",", "doch", "nicht", "ohn'", "Ehr\u00b7furcht", ",", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "PTKNEG", "APPR", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erwegt, was ihr f\u00fcr Menschen w\u00e4ret,", "tokens": ["Er\u00b7wegt", ",", "was", "ihr", "f\u00fcr", "Men\u00b7schen", "w\u00e4\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00fcrd\u2019 euch nicht von des Himmels Gunst,", "tokens": ["W\u00fcrd'", "euch", "nicht", "von", "des", "Him\u00b7mels", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "In der beliebten Land-Bau-Kunst,", "tokens": ["In", "der", "be\u00b7lieb\u00b7ten", "Lan\u00b7dBau\u00b7Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was uns so n\u00f6htig, nicht beschehret!", "tokens": ["Was", "uns", "so", "n\u00f6h\u00b7tig", ",", "nicht", "be\u00b7scheh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Erweget, was dazu geh\u00f6ret,", "tokens": ["Er\u00b7we\u00b7get", ",", "was", "da\u00b7zu", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Eh\u2019 das, was hier itzt aufgeh\u00e4uft,", "tokens": ["Eh'", "das", ",", "was", "hier", "itzt", "auf\u00b7ge\u00b7h\u00e4uft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ges\u00e4\u2019t, gewachsen, sich vermehret,", "tokens": ["Ge\u00b7s\u00e4't", ",", "ge\u00b7wach\u00b7sen", ",", "sich", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und endlich auch nunmehr gereift!", "tokens": ["Und", "end\u00b7lich", "auch", "nun\u00b7mehr", "ge\u00b7reift", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich spreche nicht vom Pfl\u00fcgen, Egen,", "tokens": ["Ich", "spre\u00b7che", "nicht", "vom", "Pfl\u00fc\u00b7gen", ",", "E\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom D\u00fcngen, S\u00e4en und vom M\u00e4h\u2019n,", "tokens": ["Vom", "D\u00fcn\u00b7gen", ",", "S\u00e4\u00b7en", "und", "vom", "M\u00e4h'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Dingen, die von uns geschehn;", "tokens": ["Als", "Din\u00b7gen", ",", "die", "von", "uns", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich spreche von des Himmels Segen,", "tokens": ["Ich", "spre\u00b7che", "von", "des", "Him\u00b7mels", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Sonnen-Schein, vom Thau, vom Regen,", "tokens": ["Vom", "Son\u00b7nen\u00b7Schein", ",", "vom", "Thau", ",", "vom", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erweget doch der Zeiten L\u00e4nge,", "tokens": ["Er\u00b7we\u00b7get", "doch", "der", "Zei\u00b7ten", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die zu dem allen n\u00f6htig war!", "tokens": ["Die", "zu", "dem", "al\u00b7len", "n\u00f6h\u00b7tig", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "PIAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es sind ja fast drey viertel Jahr.", "tokens": ["Es", "sind", "ja", "fast", "drey", "vier\u00b7tel", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "CARD", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erweget der Minuten Menge,", "tokens": ["Er\u00b7we\u00b7get", "der", "Mi\u00b7nu\u00b7ten", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die GOtt euch wollen g\u00fcnstig g\u00f6nnen,", "tokens": ["Die", "Gott", "euch", "wol\u00b7len", "g\u00fcns\u00b7tig", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und die euch alle schaden k\u00f6nnen!", "tokens": ["Und", "die", "euch", "al\u00b7le", "scha\u00b7den", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PIS", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie viele Monat\u2019, mehr noch Wochen,", "tokens": ["Wie", "vie\u00b7le", "Mo\u00b7nat'", ",", "mehr", "noch", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wie mancher Tag, wie manche Nacht,", "tokens": ["Wie", "man\u00b7cher", "Tag", ",", "wie", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sind fr\u00fch und spat herein gebrochen,", "tokens": ["Sind", "fr\u00fch", "und", "spat", "her\u00b7ein", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die immer Segen mitgebracht!", "tokens": ["Die", "im\u00b7mer", "Se\u00b7gen", "mit\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was uns der Himmel wollen g\u00f6nnen,", "tokens": ["Was", "uns", "der", "Him\u00b7mel", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hoffnung von dem ganzen Jahr,", "tokens": ["Die", "Hoff\u00b7nung", "von", "dem", "gan\u00b7zen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4tt\u2019 uns, mit vielerley Gefahr,", "tokens": ["H\u00e4tt'", "uns", ",", "mit", "vie\u00b7ler\u00b7ley", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein\u2019 einz\u2019ge Stunde rauben k\u00f6nnen.", "tokens": ["Ein'", "einz'\u00b7ge", "Stun\u00b7de", "rau\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So aber hat uns GOtt besch\u00fctzet.", "tokens": ["So", "a\u00b7ber", "hat", "uns", "Gott", "be\u00b7sch\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "(ach, w\u00fcrd\u2019 es doch mit Ernst bedacht!)", "tokens": ["(", "ach", ",", "w\u00fcrd'", "es", "doch", "mit", "Ernst", "be\u00b7dacht", "!", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "PPER", "ADV", "APPR", "NE", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es hat zum Schaden nicht geblitzet,", "tokens": ["Es", "hat", "zum", "Scha\u00b7den", "nicht", "ge\u00b7blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Fast gar kein Donner-Strahl gekracht.", "tokens": ["Fast", "gar", "kein", "Don\u00b7ner\u00b7Strahl", "ge\u00b7kracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es hat kein wilder Sturm geschnaubet,", "tokens": ["Es", "hat", "kein", "wil\u00b7der", "Sturm", "ge\u00b7schnau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kein Hagel-Schaur des Feldes Pracht,", "tokens": ["Kein", "Ha\u00b7gel\u00b7Schaur", "des", "Fel\u00b7des", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und, in ihr, unsern Schatz geraubet.", "tokens": ["Und", ",", "in", "ihr", ",", "un\u00b7sern", "Schatz", "ge\u00b7rau\u00b7bet", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPER", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ach, w\u00fcrd\u2019 es doch mit Ernst bedacht!", "tokens": ["Ach", ",", "w\u00fcrd'", "es", "doch", "mit", "Ernst", "be\u00b7dacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Es tr\u00e4nkte Kr\u00e4uter, Klee und Gras,", "tokens": ["Es", "tr\u00e4nk\u00b7te", "Kr\u00e4u\u00b7ter", ",", "Klee", "und", "Gras", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dem glatt- und fetten Vieh zur Weide,", "tokens": ["Dem", "glat\u00b7t", "und", "fet\u00b7ten", "Vieh", "zur", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Ein sanft- und nicht zu oftes Na\u00df,", "tokens": ["Ein", "sanft", "und", "nicht", "zu", "of\u00b7tes", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "PTKNEG", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Sonne reifte das Getrayde.", "tokens": ["Die", "Son\u00b7ne", "reif\u00b7te", "das", "Ge\u00b7tray\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Es ist nunmehr so weit gediehen", "tokens": ["Es", "ist", "nun\u00b7mehr", "so", "weit", "ge\u00b7die\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mit dem, was GOtt uns hat beschehrt,", "tokens": ["Mit", "dem", ",", "was", "Gott", "uns", "hat", "be\u00b7schehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df, was der Sch\u00f6pfer uns verliehen,", "tokens": ["Da\u00df", ",", "was", "der", "Sch\u00f6p\u00b7fer", "uns", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Man nunmehr in die Scheune f\u00e4hrt.", "tokens": ["Man", "nun\u00b7mehr", "in", "die", "Scheu\u00b7ne", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Es k\u00f6nnen grosse Leiter-Wagen,", "tokens": ["Es", "k\u00f6n\u00b7nen", "gros\u00b7se", "Lei\u00b7ter\u00b7Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Womit die Lader sich bem\u00fchn,", "tokens": ["Wo\u00b7mit", "die", "La\u00b7der", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Das raschelnde Gewicht kaum tragen,", "tokens": ["Das", "ra\u00b7scheln\u00b7de", "Ge\u00b7wicht", "kaum", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Die Pferde kaum die B\u00fcrde ziehn."