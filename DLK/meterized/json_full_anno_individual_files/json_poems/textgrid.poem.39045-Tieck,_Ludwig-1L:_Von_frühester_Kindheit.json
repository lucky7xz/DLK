{"textgrid.poem.39045": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von fr\u00fchester Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von fr\u00fchester Kindheit", "tokens": ["Von", "fr\u00fc\u00b7hes\u00b7ter", "Kind\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Stieg bei deinem Namen, altes Pisa,", "tokens": ["Stieg", "bei", "dei\u00b7nem", "Na\u00b7men", ",", "al\u00b7tes", "Pi\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auch Ungolinos Schreckgestalt vor mir auf,", "tokens": ["Auch", "Un\u00b7go\u00b7li\u00b7nos", "Schreck\u00b7ge\u00b7stalt", "vor", "mir", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die Entsetzens-Gruppe des Vaters mit den Kindern,", "tokens": ["Die", "Ent\u00b7set\u00b7zens\u00b7Grup\u00b7pe", "des", "Va\u00b7ters", "mit", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Im finstern einsamen Hungerthurme,", "tokens": ["Im", "fins\u00b7tern", "ein\u00b7sa\u00b7men", "Hun\u00b7ger\u00b7thur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie Gerstenberg uns mit unbegreiflicher St\u00e4rke,", "tokens": ["Wie", "Gers\u00b7ten\u00b7berg", "uns", "mit", "un\u00b7be\u00b7greif\u00b7li\u00b7cher", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und \u00fcbermenschlicher Kraft und Fassung,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7menschli\u00b7cher", "Kraft", "und", "Fas\u00b7sung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Scenen des Grauens geschildert hat.", "tokens": ["Die", "Sce\u00b7nen", "des", "Grau\u00b7ens", "ge\u00b7schil\u00b7dert", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Aber l\u00e4ngst ist Stra\u00dfe wie Thurm vernichtet,", "tokens": ["A\u00b7ber", "l\u00e4ngst", "ist", "Stra\u00b7\u00dfe", "wie", "Thurm", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "KOKOM", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.10": {"text": "Und der Raum der Unthat genommen,", "tokens": ["Und", "der", "Raum", "der", "Un\u00b7that", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die einst den Fluch auf Pisa herabrief,", "tokens": ["Die", "einst", "den", "Fluch", "auf", "Pi\u00b7sa", "he\u00b7ra\u00b7brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und Dantes scheltend ernstes Wort.", "tokens": ["Und", "Dan\u00b7tes", "schel\u00b7tend", "erns\u00b7tes", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Daf\u00fcr besuch' ich den Kirchhof nun,", "tokens": ["Da\u00b7f\u00fcr", "be\u00b7such'", "ich", "den", "Kirch\u00b7hof", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und an den hohen W\u00e4nden der edeln Halle", "tokens": ["Und", "an", "den", "ho\u00b7hen", "W\u00e4n\u00b7den", "der", "e\u00b7deln", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Leuchten mir Benozzo's Lebensbilder", "tokens": ["Leuch\u00b7ten", "mir", "Be\u00b7noz\u00b7zo's", "Le\u00b7bens\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "NE", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In lachenden Farben, in muntrer Tracht,", "tokens": ["In", "la\u00b7chen\u00b7den", "Far\u00b7ben", ",", "in", "mun\u00b7trer", "Tracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "In bedeutsamen Mienen, und mit heiterm Sinn entgegen.", "tokens": ["In", "be\u00b7deut\u00b7sa\u00b7men", "Mie\u00b7nen", ",", "und", "mit", "hei\u00b7term", "Sinn", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Der Mystik der \u00e4ltern Welt gegen\u00fcber,", "tokens": ["Der", "Mys\u00b7tik", "der", "\u00e4l\u00b7tern", "Welt", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dem finstern ernsten Sinn der Vorzeit", "tokens": ["Dem", "fins\u00b7tern", "erns\u00b7ten", "Sinn", "der", "Vor\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sind hier die Geschichten des alten Bundes", "tokens": ["Sind", "hier", "die", "Ge\u00b7schich\u00b7ten", "des", "al\u00b7ten", "Bun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "In wahre menschliche Com\u00f6die verwandelt.", "tokens": ["In", "wah\u00b7re", "menschli\u00b7che", "Co\u00b7m\u00f6\u00b7die", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auch heitre Lust spricht ernsten Sinn aus,", "tokens": ["Auch", "heit\u00b7re", "Lust", "spricht", "erns\u00b7ten", "Sinn", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und des Lebens Glanz", "tokens": ["Und", "des", "Le\u00b7bens", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Von diesen W\u00e4nden auf den Todten-Acker strahlend,", "tokens": ["Von", "die\u00b7sen", "W\u00e4n\u00b7den", "auf", "den", "Tod\u00b7ten\u00b7A\u00b7cker", "strah\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Tr\u00f6stet sie mit heiligen Scherz die Verwesung;", "tokens": ["Tr\u00f6s\u00b7tet", "sie", "mit", "hei\u00b7li\u00b7gen", "Scherz", "die", "Ver\u00b7we\u00b7sung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Und die alte fromme Sage,", "tokens": ["Und", "die", "al\u00b7te", "from\u00b7me", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Weil sie kindlich und menschlich ist,", "tokens": ["Weil", "sie", "kind\u00b7lich", "und", "menschlich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Ertr\u00e4gt des edeln K\u00fcnstlers Laun' und Muthwill,", "tokens": ["Er\u00b7tr\u00e4gt", "des", "e\u00b7deln", "K\u00fcnst\u00b7lers", "Laun'", "und", "Mut\u00b7hwill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und l\u00e4chelt sich selbst an wohlgef\u00e4llig.", "tokens": ["Und", "l\u00e4\u00b7chelt", "sich", "selbst", "an", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "ADJD", "$."], "meter": "-+--+---+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dein wunderbares Bild, Orgagna,", "tokens": ["Dein", "wun\u00b7der\u00b7ba\u00b7res", "Bild", ",", "Or\u00b7gag\u00b7na", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ueberrascht mich, so sehr ich es zu kennen glaubte.", "tokens": ["Ue\u00b7ber\u00b7rascht", "mich", ",", "so", "sehr", "ich", "es", "zu", "ken\u00b7nen", "glaub\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "ADV", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Diese Figur des Todes,", "tokens": ["Die\u00b7se", "Fi\u00b7gur", "des", "To\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht Mann, nicht Weib, nicht alt, nicht jung,", "tokens": ["Nicht", "Mann", ",", "nicht", "Weib", ",", "nicht", "alt", ",", "nicht", "jung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fliegend, bla\u00df, entstellt, im schweren Gewand, \u2013", "tokens": ["Flie\u00b7gend", ",", "bla\u00df", ",", "ent\u00b7stellt", ",", "im", "schwe\u00b7ren", "Ge\u00b7wand", ",", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Warum nicht ward sie festgehalten,", "tokens": ["Wa\u00b7rum", "nicht", "ward", "sie", "fest\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und statt des unbedeutenden Gerippes,", "tokens": ["Und", "statt", "des", "un\u00b7be\u00b7deu\u00b7ten\u00b7den", "Ge\u00b7rip\u00b7pes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In neue Formen von Bildnern gesetzt?", "tokens": ["In", "neu\u00b7e", "For\u00b7men", "von", "Bild\u00b7nern", "ge\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich trete wieder zur Stra\u00dfe hinaus,", "tokens": ["Ich", "tre\u00b7te", "wie\u00b7der", "zur", "Stra\u00b7\u00dfe", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und vor mir neigt sich der schiefe Thurm,", "tokens": ["Und", "vor", "mir", "neigt", "sich", "der", "schie\u00b7fe", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als wollt' er die Reisenden gr\u00fc\u00dfen oder h\u00f6hnen.", "tokens": ["Als", "wollt'", "er", "die", "Rei\u00b7sen\u00b7den", "gr\u00fc\u00b7\u00dfen", "o\u00b7der", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Von fr\u00fchester Kindheit", "tokens": ["Von", "fr\u00fc\u00b7hes\u00b7ter", "Kind\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Stieg bei deinem Namen, altes Pisa,", "tokens": ["Stieg", "bei", "dei\u00b7nem", "Na\u00b7men", ",", "al\u00b7tes", "Pi\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auch Ungolinos Schreckgestalt vor mir auf,", "tokens": ["Auch", "Un\u00b7go\u00b7li\u00b7nos", "Schreck\u00b7ge\u00b7stalt", "vor", "mir", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die Entsetzens-Gruppe des Vaters mit den Kindern,", "tokens": ["Die", "Ent\u00b7set\u00b7zens\u00b7Grup\u00b7pe", "des", "Va\u00b7ters", "mit", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, 