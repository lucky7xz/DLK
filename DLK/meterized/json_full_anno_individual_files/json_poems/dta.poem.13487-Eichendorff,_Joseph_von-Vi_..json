{"dta.poem.13487": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wolken, W\u00e4lderw\u00e4rts gegangen,", "tokens": ["Wol\u00b7ken", ",", "W\u00e4l\u00b7der\u00b7w\u00e4rts", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken, fliegend \u00fcber's Haus,", "tokens": ["Wol\u00b7ken", ",", "flie\u00b7gend", "\u00fc\u00b7ber's", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich an euch fest mich hangen,", "tokens": ["K\u00f6nnt'", "ich", "an", "euch", "fest", "mich", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit euch fliegen weit hinaus!", "tokens": ["Mit", "euch", "flie\u00b7gen", "weit", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Taglang durch die W\u00e4lder schweif' ich,", "tokens": ["Ta\u00b7glang", "durch", "die", "W\u00e4l\u00b7der", "schweif'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Voll Gedanken sitz' ich still,", "tokens": ["Voll", "Ge\u00b7dan\u00b7ken", "sitz'", "ich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Saiten fl\u00fcchtig greif' ich,", "tokens": ["In", "die", "Sai\u00b7ten", "fl\u00fcch\u00b7tig", "greif'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder dann auf einmal still.", "tokens": ["Wie\u00b7der", "dann", "auf", "ein\u00b7mal", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "PTKVZ", "$."], "meter": "------+", "measure": "unknown.measure.single"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ne, r\u00fchrende Geschichten", "tokens": ["Sch\u00f6\u00b7ne", ",", "r\u00fch\u00b7ren\u00b7de", "Ge\u00b7schich\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Fallen ein mir, wo ich steh,", "tokens": ["Fal\u00b7len", "ein", "mir", ",", "wo", "ich", "steh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lustig mu\u00df ich schreiben, dichten,", "tokens": ["Lus\u00b7tig", "mu\u00df", "ich", "schrei\u00b7ben", ",", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mir selber gleich so weh.", "tokens": ["Ist", "mir", "sel\u00b7ber", "gleich", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Manches Lied, das ich geschrieben", "tokens": ["Man\u00b7ches", "Lied", ",", "das", "ich", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl vor manchem langen Jahr,", "tokens": ["Wohl", "vor", "man\u00b7chem", "lan\u00b7gen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die Welt vom treuen Lieben", "tokens": ["Da", "die", "Welt", "vom", "treu\u00b7en", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6n mir \u00fcbergl\u00e4nzet war.", "tokens": ["Sch\u00f6n", "mir", "\u00fc\u00b7ber\u00b7gl\u00e4n\u00b7zet", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Find' ich's wieder jetzt voll Bangen:", "tokens": ["Find'", "ich's", "wie\u00b7der", "jetzt", "voll", "Ban\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd' ich wunderbar ger\u00fchrt,", "tokens": ["Werd'", "ich", "wun\u00b7der\u00b7bar", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn so lang ist das vergangen,", "tokens": ["Denn", "so", "lang", "ist", "das", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mich zu dem Lied verf\u00fchrt.", "tokens": ["Was", "mich", "zu", "dem", "Lied", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diese Wolken ziehen weiter,", "tokens": ["Die\u00b7se", "Wol\u00b7ken", "zie\u00b7hen", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle V\u00f6gel sind erweckt,", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "sind", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Gegend gl\u00e4nzet heiter,", "tokens": ["Und", "die", "Ge\u00b7gend", "gl\u00e4n\u00b7zet", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weit und fr\u00f6hlich aufgedeckt.", "tokens": ["Weit", "und", "fr\u00f6h\u00b7lich", "auf\u00b7ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Regen fl\u00fcchtig abw\u00e4rts gehen,", "tokens": ["Re\u00b7gen", "fl\u00fcch\u00b7tig", "ab\u00b7w\u00e4rts", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheint die Sonne zwischendrein,", "tokens": ["Scheint", "die", "Son\u00b7ne", "zwi\u00b7schen\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Haus, dein Garten stehen", "tokens": ["Und", "dein", "Haus", ",", "dein", "Gar\u00b7ten", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber'm Wald im stillen Schein.", "tokens": ["Ue\u00b7ber'm", "Wald", "im", "stil\u00b7len", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch du harrst nicht mehr mit Schmerzen,", "tokens": ["Doch", "du", "harrst", "nicht", "mehr", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo so lang' dein Liebster sey \u2014", "tokens": ["Wo", "so", "lang'", "dein", "Liebs\u00b7ter", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPOSAT", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich t\u00f6dtet noch im Herzen", "tokens": ["Und", "mich", "t\u00f6d\u00b7tet", "noch", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Schmerzen Zauberei.", "tokens": ["Die\u00b7ser", "Schmer\u00b7zen", "Zau\u00b7be\u00b7rei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}