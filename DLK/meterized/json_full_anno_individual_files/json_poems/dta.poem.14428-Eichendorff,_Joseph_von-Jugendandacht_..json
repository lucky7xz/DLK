{"dta.poem.14428": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Den S\u00e4nger will der Fr\u00fchling gar umspinnen,", "tokens": ["Den", "S\u00e4n\u00b7ger", "will", "der", "Fr\u00fch\u00b7ling", "gar", "um\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er, der Geliebteste, darf nicht entfliehen,", "tokens": ["Er", ",", "der", "Ge\u00b7lieb\u00b7tes\u00b7te", ",", "darf", "nicht", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "F\u00fchlt rings ein Lied durch alle Farben ziehen,", "tokens": ["F\u00fchlt", "rings", "ein", "Lied", "durch", "al\u00b7le", "Far\u00b7ben", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ihn so lockend nimmer l\u00e4\u00dft von hinnen.", "tokens": ["Das", "ihn", "so", "lo\u00b7ckend", "nim\u00b7mer", "l\u00e4\u00dft", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}