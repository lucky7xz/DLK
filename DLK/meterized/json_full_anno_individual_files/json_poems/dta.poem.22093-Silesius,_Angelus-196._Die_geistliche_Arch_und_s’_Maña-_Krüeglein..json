{"dta.poem.22093": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "196. Die geistliche Arch und s\u2019 Man\u0303a-  \n Kr\u00fceglein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch ist dein Hertze Gold/ und deine Seele rein/", "tokens": ["Mensch", "ist", "dein", "Hert\u00b7ze", "Gold", "/", "und", "dei\u00b7ne", "See\u00b7le", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "VVFIN", "NN", "$(", "KON", "PPOSAT", "NN", "ADJD", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "So kanst auch du die Arch/ un\u0303 s\u2019 Man\u0303akr\u00fcglein sein/", "tokens": ["So", "kanst", "auch", "du", "die", "Arch", "/", "u\u00f1", "s'", "Ma\u00f1a\u00b7kr\u00fcg\u00b7lein", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "NN", "$(", "FM", "FM", "NN", "VAINF", "$("], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}}}}}