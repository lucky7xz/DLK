{"dta.poem.3569": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hab Holz\u00e4pfel gehaspelt,               ", "tokens": ["Hab", "Holz\u00b7\u00e4p\u00b7fel", "ge\u00b7has\u00b7pelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Zaunstecken, kein Spitz;", "tokens": ["Kein", "Zaun\u00b7ste\u00b7cken", ",", "kein", "Spitz", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bin oft zu meim Schatz gangen,", "tokens": ["Bin", "oft", "zu", "meim", "Schatz", "gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hats kein Mensch gew\u00fc\u00dft.", "tokens": ["Hats", "kein", "Mensch", "ge\u00b7w\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Klein bin ich, das wei\u00df ich,", "tokens": ["Klein", "bin", "ich", ",", "das", "wei\u00df", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gro\u00df mag ich nit wern;", "tokens": ["Gro\u00df", "mag", "ich", "nit", "wern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ein Sch\u00e4tzel mu\u00df ich haben,", "tokens": ["Ein", "Sch\u00e4t\u00b7zel", "mu\u00df", "ich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ein Ha\u00dfelnu\u00dfkern.", "tokens": ["Wie", "ein", "Ha\u00b7\u00df\u00b7el\u00b7nu\u00df\u00b7kern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich hab ein sch\u00f6ns Sch\u00e4tzlein,", "tokens": ["Ich", "hab", "ein", "sch\u00f6ns", "Sch\u00e4tz\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenns nur auch so bleibt;", "tokens": ["Wenns", "nur", "auch", "so", "bleibt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Stells naus in Krautgarten,", "tokens": ["Stells", "naus", "in", "Kr\u00b7aut\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df es die V\u00f6gel vertreibt!", "tokens": ["Da\u00df", "es", "die", "V\u00f6\u00b7gel", "ver\u00b7treibt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}