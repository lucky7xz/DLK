{"dta.poem.9867": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der herbst-zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du magst den lentz und sommer preisen/", "tokens": ["Du", "magst", "den", "lentz", "und", "som\u00b7mer", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir/ mir gef\u00e4llt des herbstes frucht/", "tokens": ["Mir", "/", "mir", "ge\u00b7f\u00e4llt", "des", "herbs\u00b7tes", "frucht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die man in grossen f\u00e4ssern sucht/", "tokens": ["Die", "man", "in", "gros\u00b7sen", "f\u00e4s\u00b7sern", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In sch\u00f6nen gl\u00e4sern pflegt zu weisen.", "tokens": ["In", "sch\u00f6\u00b7nen", "gl\u00e4\u00b7sern", "pflegt", "zu", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo fr\u00f6liche gem\u00fcther seyn/", "tokens": ["Wo", "fr\u00f6\u00b7li\u00b7che", "ge\u00b7m\u00fc\u00b7ther", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da bist auch du/ o edler wein!", "tokens": ["Da", "bist", "auch", "du", "/", "o", "ed\u00b7ler", "wein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du kanst den helden st\u00e4rcke machen/", "tokens": ["Du", "kanst", "den", "hel\u00b7den", "st\u00e4r\u00b7cke", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sich der feind im felde zeigt/", "tokens": ["Wenn", "sich", "der", "feind", "im", "fel\u00b7de", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn/ ehe man die stadt ersteigt/", "tokens": ["Wenn", "/", "e\u00b7he", "man", "die", "stadt", "er\u00b7steigt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PIS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die m\u00f6rsel und carthaunen krachen.", "tokens": ["Die", "m\u00f6r\u00b7sel", "und", "car\u00b7thau\u00b7nen", "kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wo tapffere soldaten seyn/", "tokens": ["Wo", "tapf\u00b7fe\u00b7re", "sol\u00b7da\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da bist auch du/ o edler wein!", "tokens": ["Da", "bist", "auch", "du", "/", "o", "ed\u00b7ler", "wein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Du kanst durch deine macht erwecken", "tokens": ["Du", "kanst", "durch", "dei\u00b7ne", "macht", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der klugen geister eigenschafft/", "tokens": ["Der", "klu\u00b7gen", "geis\u00b7ter", "ei\u00b7gen\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der unerschrocknen redner krafft/", "tokens": ["Der", "un\u00b7er\u00b7schrock\u00b7nen", "red\u00b7ner", "krafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die nach dem himmel pflegt zu schmecken.", "tokens": ["Die", "nach", "dem", "him\u00b7mel", "pflegt", "zu", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wo fertige poeten seyn/", "tokens": ["Wo", "fer\u00b7ti\u00b7ge", "po\u00b7e\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da bist auch du/ o edler wein!", "tokens": ["Da", "bist", "auch", "du", "/", "o", "ed\u00b7ler", "wein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Du heist die m\u00e4nner l\u00e4nger sitzen", "tokens": ["Du", "heist", "die", "m\u00e4n\u00b7ner", "l\u00e4n\u00b7ger", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "In l\u00f6blicher geschellschaffts-lust:", "tokens": ["In", "l\u00f6b\u00b7li\u00b7cher", "ge\u00b7schell\u00b7schaffts\u00b7lust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wem die melancholey bewust/", "tokens": ["Wem", "die", "me\u00b7lan\u00b7cho\u00b7ley", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Kanst du das kalte blut erhitzen.", "tokens": ["Kanst", "du", "das", "kal\u00b7te", "blut", "er\u00b7hit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wo die verliebten hertzen seyn/", "tokens": ["Wo", "die", "ver\u00b7lieb\u00b7ten", "hert\u00b7zen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Da bist auch du/ o edler wein!", "tokens": ["Da", "bist", "auch", "du", "/", "o", "ed\u00b7ler", "wein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Du bist der beste koch auf erden/", "tokens": ["Du", "bist", "der", "bes\u00b7te", "koch", "auf", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der beste leib artzt in der welt/", "tokens": ["Der", "bes\u00b7te", "leib", "artzt", "in", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der zu gesunden sich gesellt/", "tokens": ["Der", "zu", "ge\u00b7sun\u00b7den", "sich", "ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die schwachen wieder starck l\u00e4st werden.", "tokens": ["Die", "schwa\u00b7chen", "wie\u00b7der", "starck", "l\u00e4st", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Darum soll mir/ o edler wein!", "tokens": ["Da\u00b7rum", "soll", "mir", "/", "o", "ed\u00b7ler", "wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der herbst ein gantzes wein-jahr seyn.", "tokens": ["Der", "herbst", "ein", "gant\u00b7zes", "wein\u00b7jahr", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}