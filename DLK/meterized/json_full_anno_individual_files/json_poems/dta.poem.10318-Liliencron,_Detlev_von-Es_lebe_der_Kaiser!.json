{"dta.poem.10318": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Es lebe der Kaiser!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Gez\u00e4nk und Anspuken, ich bin hindurchgeritten.", "tokens": ["Ge\u00b7z\u00e4nk", "und", "A\u00b7nspu\u00b7ken", ",", "ich", "bin", "hin\u00b7durch\u00b7ge\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gestern ward unser greiser, gro\u00dfer K\u00f6nig Kaiser.", "tokens": ["Ge\u00b7stern", "ward", "un\u00b7ser", "grei\u00b7ser", ",", "gro\u00b7\u00dfer", "K\u00f6\u00b7nig", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df hoch mein jung Soldatenherze schlug in Wonne.", "tokens": ["Da\u00df", "hoch", "mein", "jung", "Sol\u00b7da\u00b7ten\u00b7her\u00b7ze", "schlug", "in", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann bist du sicher vor Franzosen und Baschkiren.", "tokens": ["Dann", "bist", "du", "si\u00b7cher", "vor", "Fran\u00b7zo\u00b7sen", "und", "Baschki\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und Siegesband umflattert unsre Fahnenspitzen.", "tokens": ["Und", "Sie\u00b7ges\u00b7band", "um\u00b7flat\u00b7tert", "uns\u00b7re", "Fah\u00b7nens\u00b7pit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}