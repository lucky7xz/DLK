{"dta.poem.16289": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O \u00e4rgre dich nur nicht, wenn deinen Werth vergi\u00dft,", "tokens": ["O", "\u00e4r\u00b7gre", "dich", "nur", "nicht", ",", "wenn", "dei\u00b7nen", "Werth", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich ein Unw\u00fcrdiger mit seinem Ma\u00dfe mi\u00dft.", "tokens": ["Dich", "ein", "Un\u00b7w\u00fcr\u00b7di\u00b7ger", "mit", "sei\u00b7nem", "Ma\u00b7\u00dfe", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "O \u00e4rgre dich nur nicht! sonst wirst du gleich dich fast", "tokens": ["O", "\u00e4r\u00b7gre", "dich", "nur", "nicht", "!", "sonst", "wirst", "du", "gleich", "dich", "fast"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "ADV", "VAFIN", "PPER", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch \u00e4rger \u00e4rgern, da\u00df du dich ge\u00e4rgert hast.", "tokens": ["Noch", "\u00e4r\u00b7ger", "\u00e4r\u00b7gern", ",", "da\u00df", "du", "dich", "ge\u00b7\u00e4r\u00b7gert", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}