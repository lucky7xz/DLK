{"dta.poem.10577": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Epilog.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wie auf dem Felde die Weizenhalmen,", "tokens": ["Wie", "auf", "dem", "Fel\u00b7de", "die", "Wei\u00b7zen\u00b7hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "So wachsen und wogen im Menschengeist", "tokens": ["So", "wach\u00b7sen", "und", "wo\u00b7gen", "im", "Men\u00b7schen\u00b7geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Gedanken.", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Aber die zarten Gedanken der Liebe", "tokens": ["A\u00b7ber", "die", "zar\u00b7ten", "Ge\u00b7dan\u00b7ken", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Sind wie lustig dazwischenbl\u00fchende,", "tokens": ["Sind", "wie", "lus\u00b7tig", "da\u00b7zwi\u00b7schen\u00b7bl\u00fc\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ADJD", "ADJA", "$,"], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Roth' und blaue Blumen.", "tokens": ["Ro\u00b7th'", "und", "blau\u00b7e", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["XY", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Roth' und blaue Blumen!", "tokens": ["Ro\u00b7th'", "und", "blau\u00b7e", "Blu\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["XY", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der m\u00fcrrische Schnitter verwirft Euch als nutzlos,", "tokens": ["Der", "m\u00fcr\u00b7ri\u00b7sche", "Schnit\u00b7ter", "ver\u00b7wirft", "Euch", "als", "nutz\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "KOUS", "ADJD", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "H\u00f6lzerne Flegel zerdr\u00f6schen Euch h\u00f6hnend,", "tokens": ["H\u00f6l\u00b7zer\u00b7ne", "Fle\u00b7gel", "zer\u00b7dr\u00f6\u00b7schen", "Euch", "h\u00f6h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Sogar der hablose Wanderer,", "tokens": ["So\u00b7gar", "der", "hab\u00b7lo\u00b7se", "Wan\u00b7de\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den Eu'r Anblick erg\u00f6tzt und erquickt,", "tokens": ["Den", "Eu'r", "An\u00b7blick", "er\u00b7g\u00f6tzt", "und", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Sch\u00fcttelt das Haupt,", "tokens": ["Sch\u00fct\u00b7telt", "das", "Haupt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und nennt Euch sch\u00f6nes Unkraut.", "tokens": ["Und", "nennt", "Euch", "sch\u00f6\u00b7nes", "Un\u00b7kraut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aber die l\u00e4ndliche Jungfrau,", "tokens": ["A\u00b7ber", "die", "l\u00e4nd\u00b7li\u00b7che", "Jung\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Die Kr\u00e4nzewinderin,", "tokens": ["Die", "Kr\u00e4n\u00b7ze\u00b7win\u00b7de\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verehrt Euch und pfl\u00fcckt Euch", "tokens": ["Ver\u00b7ehrt", "Euch", "und", "pfl\u00fcckt", "Euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und schm\u00fcckt mit Euch die sch\u00f6nen Locken,", "tokens": ["Und", "schm\u00fcckt", "mit", "Euch", "die", "sch\u00f6\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und also geziert, eilt sie zum Tanzplatz,", "tokens": ["Und", "al\u00b7so", "ge\u00b7ziert", ",", "eilt", "sie", "zum", "Tanz\u00b7platz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo Pfeifen und Geigen lieblich ert\u00f6nen,", "tokens": ["Wo", "Pfei\u00b7fen", "und", "Gei\u00b7gen", "lieb\u00b7lich", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Oder zur stillen Buche,", "tokens": ["O\u00b7der", "zur", "stil\u00b7len", "Bu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Wo die Stimme des Liebsten noch lieblicher t\u00f6nt", "tokens": ["Wo", "die", "Stim\u00b7me", "des", "Liebs\u00b7ten", "noch", "lieb\u00b7li\u00b7cher", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Als Pfeifen und Geigen.", "tokens": ["Als", "Pfei\u00b7fen", "und", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}