{"textgrid.poem.39712": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "[2]", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Novalis, deinen heilgen Seherblikken", "tokens": ["No\u00b7va\u00b7lis", ",", "dei\u00b7nen", "heil\u00b7gen", "Se\u00b7her\u00b7blik\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind aufgeschlossen aller Welten R\u00e4ume,", "tokens": ["Sind", "auf\u00b7ge\u00b7schlos\u00b7sen", "al\u00b7ler", "Wel\u00b7ten", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir offenbahrt sich weihend das Gemeine,", "tokens": ["Dir", "of\u00b7fen\u00b7bahrt", "sich", "wei\u00b7hend", "das", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du schaust es in prophetischem Entz\u00fccken.", "tokens": ["Du", "schaust", "es", "in", "pro\u00b7phe\u00b7ti\u00b7schem", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du siehst der Dinge zukunftsvolle Keime", "tokens": ["Du", "siehst", "der", "Din\u00b7ge", "zu\u00b7kunfts\u00b7vol\u00b7le", "Kei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "me