{"textgrid.poem.39110": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r es ein Ro\u00df, das mit verh\u00e4ngten Z\u00fcgeln", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r es ein Ro\u00df, das mit verh\u00e4ngten Z\u00fcgeln", "tokens": ["W\u00e4r", "es", "ein", "Ro\u00df", ",", "das", "mit", "ver\u00b7h\u00e4ng\u00b7ten", "Z\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erdw\u00e4rts den Reiter schleudert aus den B\u00fcgeln:", "tokens": ["Erd\u00b7w\u00e4rts", "den", "Rei\u00b7ter", "schleu\u00b7dert", "aus", "den", "B\u00fc\u00b7geln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die M\u00e4hne fliegt, die hei\u00dfen N\u00fcstern dampfen,", "tokens": ["Die", "M\u00e4h\u00b7ne", "fliegt", ",", "die", "hei\u00b7\u00dfen", "N\u00fcs\u00b7tern", "damp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Adern bei\u00dft es mit dem eignen Zahn,", "tokens": ["Die", "A\u00b7dern", "bei\u00dft", "es", "mit", "dem", "eig\u00b7nen", "Zahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Rings dr\u00f6hnt das Tal von seiner Hufe Stampfen", "tokens": ["Rings", "dr\u00f6hnt", "das", "Tal", "von", "sei\u00b7ner", "Hu\u00b7fe", "Stamp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und, gleich dem Sturmwind, fliegt es seine Bahn:", "tokens": ["Und", ",", "gleich", "dem", "Sturm\u00b7wind", ",", "fliegt", "es", "sei\u00b7ne", "Bahn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "W\u00e4r es ein Ro\u00df, ihr w\u00fcrdet es nicht wagen,", "tokens": ["W\u00e4r", "es", "ein", "Ro\u00df", ",", "ihr", "w\u00fcr\u00b7det", "es", "nicht", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum Stall zur\u00fcck das freie Tier zu jagen.", "tokens": ["Zum", "Stall", "zu\u00b7r\u00fcck", "das", "frei\u00b7e", "Tier", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "W\u00e4r es ein Sturm, was jetzt die Erde r\u00fcttelt,", "tokens": ["W\u00e4r", "es", "ein", "Sturm", ",", "was", "jetzt", "die", "Er\u00b7de", "r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ehrner Faust der B\u00e4ume Kronen sch\u00fcttelt:", "tokens": ["Mit", "ehr\u00b7ner", "Faust", "der", "B\u00e4u\u00b7me", "Kro\u00b7nen", "sch\u00fct\u00b7telt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da kracht der Wald, da st\u00fcrzen eure Mauern,", "tokens": ["Da", "kracht", "der", "Wald", ",", "da", "st\u00fcr\u00b7zen", "eu\u00b7re", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da sch\u00e4umt der Gie\u00dfbach durch die w\u00fcste Flur,", "tokens": ["Da", "sch\u00e4umt", "der", "Gie\u00df\u00b7bach", "durch", "die", "w\u00fcs\u00b7te", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und atemlos, als wie in Fieberschauern,", "tokens": ["Und", "a\u00b7tem\u00b7los", ",", "als", "wie", "in", "Fie\u00b7ber\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebeugten Knies, lauscht zitternd die Natur:", "tokens": ["Ge\u00b7beug\u00b7ten", "Knies", ",", "lauscht", "zit\u00b7ternd", "die", "Na\u00b7tur", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "W\u00e4r es ein Sturm, mit \u00e4ngstlicher Geb\u00e4rde,", "tokens": ["W\u00e4r", "es", "ein", "Sturm", ",", "mit", "\u00e4ngst\u00b7li\u00b7cher", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Haupt bekreuzend, l\u00e4gt ihr an der Erde.", "tokens": ["Das", "Haupt", "be\u00b7kreu\u00b7zend", ",", "l\u00e4gt", "ihr", "an", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja wenn's ein Feuer w\u00e4re, was hier lodert", "tokens": ["Ja", "wenn's", "ein", "Feu\u00b7er", "w\u00e4\u00b7re", ",", "was", "hier", "lo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unerbittlich seine Opfer fordert:", "tokens": ["Und", "un\u00b7er\u00b7bitt\u00b7lich", "sei\u00b7ne", "Op\u00b7fer", "for\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hoch wallt der Dampf und lust'ge Funken spr\u00fchen,", "tokens": ["Hoch", "wallt", "der", "Dampf", "und", "lust'\u00b7ge", "Fun\u00b7ken", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Beschwingte Drachen, durch die Nacht daher,", "tokens": ["Be\u00b7schwing\u00b7te", "Dra\u00b7chen", ",", "durch", "die", "Nacht", "da\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "APPR", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Erde seht ihr, seht den Himmel gl\u00fchen,", "tokens": ["Die", "Er\u00b7de", "seht", "ihr", ",", "seht", "den", "Him\u00b7mel", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wohin ihr schaut, ein einzig Flammenmeer:", "tokens": ["Wo\u00b7hin", "ihr", "schaut", ",", "ein", "ein\u00b7zig", "Flam\u00b7men\u00b7meer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ja w\u00e4r's ein Feuer, mit gesenkten H\u00e4nden,", "tokens": ["Ja", "w\u00e4r's", "ein", "Feu\u00b7er", ",", "mit", "ge\u00b7senk\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr grimmig Werk lie\u00dft ihr die Glut vollenden. \u2013", "tokens": ["Ihr", "grim\u00b7mig", "Werk", "lie\u00dft", "ihr", "die", "Glut", "voll\u00b7en\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und doch das Ro\u00df ist nur ein Tier, nichts weiter,", "tokens": ["Und", "doch", "das", "Ro\u00df", "ist", "nur", "ein", "Tier", ",", "nichts", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und leicht bezwingt es der gewandte Reiter;", "tokens": ["Und", "leicht", "be\u00b7zwingt", "es", "der", "ge\u00b7wand\u00b7te", "Rei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lahm wird der Sturmwind, mit gebrochnen Schwingen,", "tokens": ["Lahm", "wird", "der", "Sturm\u00b7wind", ",", "mit", "ge\u00b7broch\u00b7nen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein m\u00fcder Vogel, sinkt er in den Sand,", "tokens": ["Ein", "m\u00fc\u00b7der", "Vo\u00b7gel", ",", "sinkt", "er", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn des Wassers linde Quellen springen,", "tokens": ["Und", "wenn", "des", "Was\u00b7sers", "lin\u00b7de", "Quel\u00b7len", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erlischt zuletzt der f\u00fcrchterlichste Brand \u2013:", "tokens": ["Er\u00b7lischt", "zu\u00b7letzt", "der", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7te", "Brand", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die ihr euch feig vor Ro\u00df, Sturm, Feur' bewiesen,", "tokens": ["Die", "ihr", "euch", "feig", "vor", "Ro\u00df", ",", "Sturm", ",", "Feur'", "be\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "APPR", "NN", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "W\u00e4r es ein Ro\u00df, das mit verh\u00e4ngten Z\u00fcgeln", "tokens": ["W\u00e4r", "es", "ein", "Ro\u00df", ",", "das", "mit", "ver\u00b7h\u00e4ng\u00b7ten", "Z\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erdw\u00e4rts den Reiter schleudert aus den B\u00fcgeln:", "tokens": ["Erd\u00b7w\u00e4rts", "den", "Rei\u00b7ter", "schleu\u00b7dert", "aus", "den", "B\u00fc\u00b7geln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die M\u00e4hne fliegt, die hei\u00dfen N\u00fcstern dampfen,", "tokens": ["Die", "M\u00e4h\u00b7ne", "fliegt", ",", "die", "hei\u00b7\u00dfen", "N\u00fcs\u00b7tern", "damp\u00b7