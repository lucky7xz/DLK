{"dta.poem.13307": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So fieng ihr Vortrag an: \u201eWer hat nicht angeh\u00f6rt,", "tokens": ["So", "fi\u00b7eng", "ihr", "Vor\u00b7trag", "an", ":", "\u201e", "Wer", "hat", "nicht", "an\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "$(", "PWS", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u201emit was vor Gnad und Huld die ", "tokens": ["\u201e", "mit", "was", "vor", "Gnad", "und", "Huld", "die"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PRELS", "APPR", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201esie bauet mehr auf uns, als auf die Macht der Waffen;", "tokens": ["\u201e", "sie", "bau\u00b7et", "mehr", "auf", "uns", ",", "als", "auf", "die", "Macht", "der", "Waf\u00b7fen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "280\u201dDurch uns, vertraut sie sich, den L\u00e4ndern Ruh zu schaffen.", "tokens": ["\"", "Durch", "uns", ",", "ver\u00b7traut", "sie", "sich", ",", "den", "L\u00e4n\u00b7dern", "Ruh", "zu", "schaf\u00b7fen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "$,", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eder h\u00f6chst-erleuchte F\u00fcrst, ihr theurer ", "tokens": ["\u201e", "der", "h\u00f6chst\u00b7er\u00b7leuch\u00b7te", "F\u00fcrst", ",", "ihr", "theu\u00b7rer"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ewie grosse Zuversicht tr\u00e4gt er zu diesem Saal?", "tokens": ["\u201e", "wie", "gros\u00b7se", "Zu\u00b7ver\u00b7sicht", "tr\u00e4gt", "er", "zu", "die\u00b7sem", "Saal", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewas bracht ", "tokens": ["\u201e", "was", "bracht"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PWS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "\u201eaus denen Fried und Sieg und Ruhm und Heil entsprossen,", "tokens": ["\u201e", "aus", "de\u00b7nen", "Fried", "und", "Sieg", "und", "Ruhm", "und", "Heil", "ent\u00b7spros\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "285\u201dZu meiner Absicht vor? nur dieser Tugend-Krei\u00df", "tokens": ["\"", "Zu", "mei\u00b7ner", "Ab\u00b7sicht", "vor", "?", "nur", "die\u00b7ser", "Tu\u00b7gen\u00b7dKrei\u00df"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eist Hoffnung, Rath, Vertraun, Entscheidung, Hilff und Prei\u00df.", "tokens": ["\u201e", "ist", "Hoff\u00b7nung", ",", "Rath", ",", "Ver\u00b7traun", ",", "Ent\u00b7schei\u00b7dung", ",", "Hilff", "und", "Prei\u00df", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}