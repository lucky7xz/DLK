{"textgrid.poem.60616": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Maultier, dessen Last ein Sack voll Hafer war", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Maultier, dessen Last ein Sack voll Hafer war", "tokens": ["Ein", "Maul\u00b7tier", ",", "des\u00b7sen", "Last", "ein", "Sack", "voll", "Ha\u00b7fer", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ART", "NN", "ADJD", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zog einst mit einem andern Maultier \u00fcber Feld,", "tokens": ["Zog", "einst", "mit", "ei\u00b7nem", "an\u00b7dern", "Maul\u00b7tier", "\u00fc\u00b7ber", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das gr\u00f6\u00dfre Werte trug: in bar", "tokens": ["Das", "gr\u00f6\u00df\u00b7re", "Wer\u00b7te", "trug", ":", "in", "bar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein h\u00fcbsches S\u00fcmmchen Steuergeld.", "tokens": ["Ein", "h\u00fcb\u00b7sches", "S\u00fcmm\u00b7chen", "Steu\u00b7er\u00b7geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So vornehm schritt dies Tier daher,", "tokens": ["So", "vor\u00b7nehm", "schritt", "dies", "Tier", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PDS", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als ob es hochgeadelt w\u00e4r,", "tokens": ["Als", "ob", "es", "hoch\u00b7gea\u00b7delt", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und lie\u00df voll Stolz sein Gl\u00f6ckchen klingen.", "tokens": ["Und", "lie\u00df", "voll", "Stolz", "sein", "Gl\u00f6ck\u00b7chen", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht lange wandern sie, da springen", "tokens": ["Nicht", "lan\u00b7ge", "wan\u00b7dern", "sie", ",", "da", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verwegne R\u00e4uber vor; das Geld ist ihr Begehr,", "tokens": ["Ver\u00b7weg\u00b7ne", "R\u00e4u\u00b7ber", "vor", ";", "das", "Geld", "ist", "ihr", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und w\u00e4hrend man den Hafer unbeachtet l\u00e4\u00dft,", "tokens": ["Und", "w\u00e4h\u00b7rend", "man", "den", "Ha\u00b7fer", "un\u00b7be\u00b7ach\u00b7tet", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.he