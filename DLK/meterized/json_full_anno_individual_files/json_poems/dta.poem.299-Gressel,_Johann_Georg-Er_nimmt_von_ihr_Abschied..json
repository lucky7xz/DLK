{"dta.poem.299": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er nimmt von ihr Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Margenis mein Licht und Leben!", "tokens": ["Mar\u00b7ge\u00b7nis", "mein", "Licht", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun mu\u00df es geschieden seyn/", "tokens": ["Nun", "mu\u00df", "es", "ge\u00b7schie\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jetzo mu\u00df ich Abschied geben/", "tokens": ["Jet\u00b7zo", "mu\u00df", "ich", "Ab\u00b7schied", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt es gleich so grosse Pein.", "tokens": ["Bringt", "es", "gleich", "so", "gros\u00b7se", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Verh\u00e4ngni\u00df heist mich scheiden/", "tokens": ["Mein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "heist", "mich", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich mu\u00df euch meiden", "tokens": ["Ich", "mu\u00df", "euch", "mei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ach die Stunde bricht schon ein.", "tokens": ["Ach", "die", "Stun\u00b7de", "bricht", "schon", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch gedult/ wer kan das wenden/", "tokens": ["Doch", "ge\u00b7dult", "/", "wer", "kan", "das", "wen\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PWS", "VMFIN", "PDS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was das Schicksahl haben will/", "tokens": ["Was", "das", "Schick\u00b7sahl", "ha\u00b7ben", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man mu\u00df ihm an allen Enden", "tokens": ["Man", "mu\u00df", "ihm", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unverweigert halten still/", "tokens": ["Un\u00b7ver\u00b7wei\u00b7gert", "hal\u00b7ten", "still", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich wird es sich bedencken/", "tokens": ["End\u00b7lich", "wird", "es", "sich", "be\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und meinen Kr\u00e4ncken", "tokens": ["Und", "mei\u00b7nen", "Kr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Gleichfals setzen Maa\u00df und Ziel.", "tokens": ["Gleich\u00b7fals", "set\u00b7zen", "Maa\u00df", "und", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist stets mein Trost gewesen", "tokens": ["Du", "bist", "stets", "mein", "Trost", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein Erg\u00f6tzen/ meine Lust/", "tokens": ["Mein", "Er\u00b7g\u00f6t\u00b7zen", "/", "mei\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt ist nichts denn Angst zu lesen", "tokens": ["Jetzt", "ist", "nichts", "denn", "Angst", "zu", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "KON", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der hart beklumnen Brust", "tokens": ["In", "der", "hart", "be\u00b7kl\u00b7um\u00b7nen", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Was ich h\u00f6chlich sonst begehret/", "tokens": ["Was", "ich", "h\u00f6ch\u00b7lich", "sonst", "be\u00b7ge\u00b7hret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und mit verehret/", "tokens": ["Und", "mit", "ver\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Bleibet mir nun unbewust.", "tokens": ["Blei\u00b7bet", "mir", "nun", "un\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mu\u00df mein Leib gleich von dir reisen/", "tokens": ["Mu\u00df", "mein", "Leib", "gleich", "von", "dir", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele bleibt bey dir/", "tokens": ["Mei\u00b7ne", "See\u00b7le", "bleibt", "bey", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Sch\u00f6nheit wird sie speisen", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "wird", "sie", "spei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Erg\u00f6tzung nach Begier.", "tokens": ["Mit", "Er\u00b7g\u00f6t\u00b7zung", "nach", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich gleich die Lippen missen", "tokens": ["Mu\u00df", "ich", "gleich", "die", "Lip\u00b7pen", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ich doch k\u00fcssen", "tokens": ["Will", "ich", "doch", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "In Gedancken deine Zier.", "tokens": ["In", "Ge\u00b7dan\u00b7cken", "dei\u00b7ne", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gute Nacht geliebte Seele/", "tokens": ["Gu\u00b7te", "Nacht", "ge\u00b7lieb\u00b7te", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute Nacht mein Engels-Bild/", "tokens": ["Gu\u00b7te", "Nacht", "mein", "En\u00b7gels\u00b7Bild", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich mich mit Kummer qu\u00e4hle", "tokens": ["Wenn", "ich", "mich", "mit", "Kum\u00b7mer", "qu\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So sey deine Brust erf\u00fcllt/", "tokens": ["So", "sey", "dei\u00b7ne", "Brust", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Stets mit Lust und frohen Sachen/", "tokens": ["Stets", "mit", "Lust", "und", "fro\u00b7hen", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die gar bald machen/", "tokens": ["Die", "gar", "bald", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df die Traurigkeit sich stllit,", "tokens": ["Da\u00df", "die", "Trau\u00b7rig\u00b7keit", "sich", "stl\u00b7lit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}