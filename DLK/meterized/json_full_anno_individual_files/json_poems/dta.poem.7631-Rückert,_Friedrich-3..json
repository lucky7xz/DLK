{"dta.poem.7631": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein wenig l\u00e4nger noch Geduld und froher Muth,", "tokens": ["Ein", "we\u00b7nig", "l\u00e4n\u00b7ger", "noch", "Ge\u00b7duld", "und", "fro\u00b7her", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hell wird alle Tr\u00fcb' und alles Uebel gut.", "tokens": ["Und", "hell", "wird", "al\u00b7le", "Tr\u00fcb'", "und", "al\u00b7les", "Ue\u00b7bel", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schon ist ein sanfter Stral dem Dunkel eingesprengt,", "tokens": ["Schon", "ist", "ein", "sanf\u00b7ter", "Stral", "dem", "Dun\u00b7kel", "ein\u00b7ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein s\u00fc\u00dfer Vorschmack schon dem Bittern eingemengt.", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Vor\u00b7schmack", "schon", "dem", "Bit\u00b7tern", "ein\u00b7ge\u00b7mengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn ab der Schatten nur, wenn zu das Licht nur nimmt,", "tokens": ["Wenn", "ab", "der", "Schat\u00b7ten", "nur", ",", "wenn", "zu", "das", "Licht", "nur", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie schwer auch jener f\u00e4llt, wie schwach auch dieses glimmt;", "tokens": ["Wie", "schwer", "auch", "je\u00b7ner", "f\u00e4llt", ",", "wie", "schwach", "auch", "die\u00b7ses", "glimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PDS", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein wenig l\u00e4nger noch Geduld und froher Muth,", "tokens": ["Ein", "we\u00b7nig", "l\u00e4n\u00b7ger", "noch", "Ge\u00b7duld", "und", "fro\u00b7her", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hell wird alle Tr\u00fcb' und alles Uebel gut.", "tokens": ["Und", "hell", "wird", "al\u00b7le", "Tr\u00fcb'", "und", "al\u00b7les", "Ue\u00b7bel", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}