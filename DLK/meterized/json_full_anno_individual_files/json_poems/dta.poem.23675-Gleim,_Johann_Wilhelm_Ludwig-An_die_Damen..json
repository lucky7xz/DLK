{"dta.poem.23675": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Damen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es seegn euch der Himmel,", "tokens": ["Es", "se\u00b7egn", "euch", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr w\u00fcrdige Sch\u00f6nen!", "tokens": ["Ihr", "w\u00fcr\u00b7di\u00b7ge", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Seid ewig die Wonne", "tokens": ["Seid", "e\u00b7wig", "die", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Jungen und Alten!", "tokens": ["Der", "Jun\u00b7gen", "und", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Seid ewig, wie heute,", "tokens": ["Seid", "e\u00b7wig", ",", "wie", "heu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Labsal der M\u00e4nner!", "tokens": ["Das", "Lab\u00b7sal", "der", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ihr la\u00dft euch nur sehen,", "tokens": ["Ihr", "la\u00dft", "euch", "nur", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "So h\u00fcpfen schon Herzen.", "tokens": ["So", "h\u00fcp\u00b7fen", "schon", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ihr zwinget die Alten", "tokens": ["Ihr", "zwin\u00b7get", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Zu J\u00fcnglingsgeberden.", "tokens": ["Zu", "J\u00fcng\u00b7lings\u00b7ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "Ihr labet die Jungen.", "tokens": ["Ihr", "la\u00b7bet", "die", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Was ist doch ein Leben,", "tokens": ["Was", "ist", "doch", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Das ihr nicht vers\u00fcsset?", "tokens": ["Das", "ihr", "nicht", "ver\u00b7s\u00fcs\u00b7set", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Befragt nur die M\u00e4nner.", "tokens": ["Be\u00b7fragt", "nur", "die", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}