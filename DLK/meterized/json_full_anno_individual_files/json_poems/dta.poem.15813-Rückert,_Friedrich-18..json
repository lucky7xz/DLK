{"dta.poem.15813": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn, es haben dich die Meister abgewiesen,", "tokens": ["Mein", "Sohn", ",", "es", "ha\u00b7ben", "dich", "die", "Meis\u00b7ter", "ab\u00b7ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die als die ersten sind in ihrer Kunst gepriesen.", "tokens": ["Die", "als", "die", "ers\u00b7ten", "sind", "in", "ih\u00b7rer", "Kunst", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "ADJA", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich mahne dich, da\u00df du dir das zu Herzen nehmest:", "tokens": ["Ich", "mah\u00b7ne", "dich", ",", "da\u00df", "du", "dir", "das", "zu", "Her\u00b7zen", "neh\u00b7mest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "PDS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist besch\u00e4mt, wenn du sie selber nicht besch\u00e4mest.", "tokens": ["Du", "bist", "be\u00b7sch\u00e4mt", ",", "wenn", "du", "sie", "sel\u00b7ber", "nicht", "be\u00b7sch\u00e4\u00b7mest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Beut' auf, was in dir ist, entfalte deine Gaben,", "tokens": ["Beut'", "auf", ",", "was", "in", "dir", "ist", ",", "ent\u00b7fal\u00b7te", "dei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie zur Schande sehn, wen sie verworfen haben.", "tokens": ["Da\u00df", "sie", "zur", "Schan\u00b7de", "sehn", ",", "wen", "sie", "ver\u00b7wor\u00b7fen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "$,", "PWS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}