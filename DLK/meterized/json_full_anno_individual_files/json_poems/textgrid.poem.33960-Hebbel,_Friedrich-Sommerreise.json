{"textgrid.poem.33960": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sommerreise", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Haus, im Waldgehege,", "tokens": ["Dein", "Haus", ",", "im", "Wald\u00b7ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stand auf dem H\u00fcgel frei;", "tokens": ["Stand", "auf", "dem", "H\u00fc\u00b7gel", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich, auf gewund'nem Stege,", "tokens": ["Ich", ",", "auf", "ge\u00b7wun\u00b7d'\u00b7nem", "Ste\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zog hart daran vorbei.", "tokens": ["Zog", "hart", "da\u00b7ran", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du gabst mit treuer Miene", "tokens": ["Du", "gabst", "mit", "treu\u00b7er", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir meinen Gru\u00df zur\u00fcck,", "tokens": ["Mir", "mei\u00b7nen", "Gru\u00df", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die wallende G