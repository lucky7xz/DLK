{"textgrid.poem.48820": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "45. Nachdem sie wieder gesund worden war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmt diesen Dank von mir, ihr milden G\u00f6tter, an,", "tokens": ["Nehmt", "die\u00b7sen", "Dank", "von", "mir", ",", "ihr", "mil\u00b7den", "G\u00f6t\u00b7ter", ",", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die ihr f\u00fcr unser Heil aus treuer Vorsicht wachet.", "tokens": ["die", "ihr", "f\u00fcr", "un\u00b7ser", "Heil", "aus", "treu\u00b7er", "Vor\u00b7sicht", "wa\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ihr ihr habt genutzt, das habt ihr mir getan.", "tokens": ["Was", "ihr", "ihr", "habt", "ge\u00b7nutzt", ",", "das", "habt", "ihr", "mir", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,", "PDS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich auch war krank in ihr; in ihr hab ich geachet,", "tokens": ["Ich", "auch", "war", "krank", "in", "ihr", ";", "in", "ihr", "hab", "ich", "ge\u00b7a\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "APPR", "PPOSAT", "$.", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+----+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und Beide habt ihr nun in ihr gesund gemachet.", "tokens": ["und", "Bei\u00b7de", "habt", "ihr", "nun", "in", "ihr", "ge\u00b7sund", "ge\u00b7ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum danken Beide wir, und denken ewig dran.", "tokens": ["Drum", "dan\u00b7ken", "Bei\u00b7de", "wir", ",", "und", "den\u00b7ken", "e\u00b7wig", "dran", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PPER", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Worte sprach ich ihr bei ihren Schmerzen ein,", "tokens": ["Die", "Wor\u00b7te", "sprach", "ich", "ihr", "bei", "ih\u00b7ren", "Schmer\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die sie mit eigner Hand in dieses Tuch gest\u00fccket", "tokens": ["die", "sie", "mit", "eig\u00b7ner", "Hand", "in", "die\u00b7ses", "Tuch", "ge\u00b7st\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und nach ber\u00fchmter Kunst mit Farben hat gesch