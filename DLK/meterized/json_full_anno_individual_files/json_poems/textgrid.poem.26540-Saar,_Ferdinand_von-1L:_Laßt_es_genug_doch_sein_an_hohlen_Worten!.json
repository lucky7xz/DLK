{"textgrid.poem.26540": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft es genug doch sein an hohlen Worten!", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft es genug doch sein an hohlen Worten!", "tokens": ["La\u00dft", "es", "ge\u00b7nug", "doch", "sein", "an", "hoh\u00b7len", "Wor\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PPOSAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sagt's rund heraus: der Mann da ist gestorben;", "tokens": ["Sagt's", "rund", "he\u00b7raus", ":", "der", "Mann", "da", "ist", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$.", "ART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Dank der Mitwelt hat er nicht erworben,", "tokens": ["Den", "Dank", "der", "Mit\u00b7welt", "hat", "er", "nicht", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hinter ihm geschlossen sind die Pforten.", "tokens": ["Und", "hin\u00b7ter", "ihm", "ge\u00b7schlos\u00b7sen", "sind", "die", "Pfor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hinweg den Lorbeerkranz, den l\u00e4ngst verdorrten!", "tokens": ["Hin\u00b7weg", "den", "Lor\u00b7beer\u00b7kranz", ",", "den", "l\u00e4ngst", "ver\u00b7dorr\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstummt, Posaunen, Fl\u00f6ten und Theorben!", "tokens": ["Ver\u00b7stummt", ",", "Po\u00b7sau\u00b7nen", ",", "Fl\u00f6\u00b7ten", "und", "The\u00b7or\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df an der Ungunst er der Zeit verdorben,", "tokens": ["Da\u00df", "an", "der", "Un\u00b7gunst", "er", "der", "Zeit", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beklagt es nicht, ihr heuchelnden Consorten!", "tokens": ["Be\u00b7klagt", "es", "nicht", ",", "ihr", "heu\u00b7cheln\u00b7den", "Con\u00b7sor\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Indessen euch des Beifalls M\u00fcnzen rollten,", "tokens": ["In\u00b7des\u00b7sen", "euch", "des", "Bei\u00b7falls", "M\u00fcn\u00b7zen", "roll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei Hungerkost sein d\u00fcrftig Liedlein sang er,", "tokens": ["Bei", "Hun\u00b7ger\u00b7kost", "sein", "d\u00fcrf\u00b7tig", "Lied\u00b7lein", "sang", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verachtet still von euch und laut gescholten.", "tokens": ["Ver\u00b7ach\u00b7tet", "still", "von", "euch", "und", "laut", "ge\u00b7schol\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Stellt euch mit ihm nicht selbst jetzt an den Pranger,", "tokens": ["Stellt", "euch", "mit", "ihm", "nicht", "selbst", "jetzt", "an", "den", "Pran\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und da er euch im Leben nichts gegolten,", "tokens": ["Und", "da", "er", "euch", "im", "Le\u00b7ben", "nichts", "ge\u00b7gol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPRART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So la\u00dft ihn auch verfaulen auf dem Anger.", "tokens": ["So", "la\u00dft", "ihn", "auch", "ver\u00b7fau\u00b7len", "auf", "dem", "An\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "La\u00dft es genug doch sein an hohlen Worten!", "tokens": ["La\u00dft", "es", "ge\u00b7nug", "doch", "sein", "an", "hoh\u00b7len", "Wor\