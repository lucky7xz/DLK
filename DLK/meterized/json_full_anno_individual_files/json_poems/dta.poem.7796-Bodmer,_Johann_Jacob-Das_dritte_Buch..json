{"dta.poem.7796": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Saul gabe nun Befehl das Lager aufzuheben,", "tokens": ["Saul", "ga\u00b7be", "nun", "Be\u00b7fehl", "das", "La\u00b7ger", "auf\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wozu im gantzen Heer sie mehr als willig leben,", "tokens": ["Wo\u00b7zu", "im", "gant\u00b7zen", "Heer", "sie", "mehr", "als", "wil\u00b7lig", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "PPER", "PIAT", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es t\u00f6nt die Feldtrompet zum Abzug; Man bricht auf.", "tokens": ["Es", "t\u00f6nt", "die", "Feld\u00b7trom\u00b7pet", "zum", "Ab\u00b7zug", ";", "Man", "bricht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es reiset also hin der Helden froher Hauf,", "tokens": ["Es", "rei\u00b7set", "al\u00b7so", "hin", "der", "Hel\u00b7den", "fro\u00b7her", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein jeder war beb\u00fcrdt mit der Philister Sch\u00e4tzen,", "tokens": ["Ein", "je\u00b7der", "war", "be\u00b7b\u00fcrdt", "mit", "der", "Phi\u00b7lis\u00b7ter", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zum Zeichen seiner Ehr, die Last ist ihr Ergetzen,", "tokens": ["Zum", "Zei\u00b7chen", "sei\u00b7ner", "Ehr", ",", "die", "Last", "ist", "ihr", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie gehen muntrer jezt, als f\u00fcrhin da sie leer.", "tokens": ["Sie", "ge\u00b7hen", "mun\u00b7trer", "jezt", ",", "als", "f\u00fcr\u00b7hin", "da", "sie", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "KOUS", "ADV", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ungewisse Gl\u00fcck beschwerte sie vor mehr", "tokens": ["Das", "un\u00b7ge\u00b7wis\u00b7se", "Gl\u00fcck", "be\u00b7schwer\u00b7te", "sie", "vor", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als nun der schwere Raub. Der David hat zu schaffen,", "tokens": ["Als", "nun", "der", "schwe\u00b7re", "Raub", ".", "Der", "Da\u00b7vid", "hat", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$.", "ART", "NE", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df Goliath sein Haupt, des Schwerdt und grosse Waffen", "tokens": ["Da\u00df", "Go\u00b7li\u00b7ath", "sein", "Haupt", ",", "des", "Schwerdt", "und", "gros\u00b7se", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch kommen mit von da, die\u00df ware seine Beut,", "tokens": ["Auch", "kom\u00b7men", "mit", "von", "da", ",", "die\u00df", "wa\u00b7re", "sei\u00b7ne", "Beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPR", "ADV", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Ursach alles Raubs der andern Kriegesleut.", "tokens": ["Die", "Ur\u00b7sach", "al\u00b7les", "Raubs", "der", "an\u00b7dern", "Krie\u00b7ge\u00b7sleut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Viel anders zog er weg, als er war hergekommen;", "tokens": ["Viel", "an\u00b7ders", "zog", "er", "weg", ",", "als", "er", "war", "her\u00b7ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gleich als ein schlechter Hirt war er hie aufgenommen,", "tokens": ["Gleich", "als", "ein", "schlech\u00b7ter", "Hirt", "war", "er", "hie", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nun aber ehrt man ihn als einen Kriegesheld,", "tokens": ["Nun", "a\u00b7ber", "ehrt", "man", "ihn", "als", "ei\u00b7nen", "Krie\u00b7ge\u00b7sheld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der da nach Hof geh\u00f6rt, und nicht mehr in das Feld.", "tokens": ["Der", "da", "nach", "Hof", "ge\u00b7h\u00f6rt", ",", "und", "nicht", "mehr", "in", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "VVFIN", "$,", "KON", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In diesem neuen Stand hofft er wohl zu behagen,", "tokens": ["In", "die\u00b7sem", "neu\u00b7en", "Stand", "hofft", "er", "wohl", "zu", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Der die da ihm zu lieb wollt Hirtenkleider tragen,", "tokens": ["Der", "die", "da", "ihm", "zu", "lieb", "wollt", "Hir\u00b7ten\u00b7klei\u00b7der", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "KOUS", "PPER", "PTKA", "ADJD", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie ehmahls er vernahm in jener s\u00fcssen Nacht,", "tokens": ["Wie", "eh\u00b7mahls", "er", "ver\u00b7nahm", "in", "je\u00b7ner", "s\u00fcs\u00b7sen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ihm, was er niemahls gehoffet, wissend macht.", "tokens": ["Die", "ihm", ",", "was", "er", "nie\u00b7mahls", "ge\u00b7hof\u00b7fet", ",", "wis\u00b7send", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Es war sein Hertz schon da, dahin die Reise gienge,", "tokens": ["Es", "war", "sein", "Hertz", "schon", "da", ",", "da\u00b7hin", "die", "Rei\u00b7se", "gien\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$,", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Wunsch ist mit vorhin, da\u00df alles wohl gelinge.", "tokens": ["Der", "Wunsch", "ist", "mit", "vor\u00b7hin", ",", "da\u00df", "al\u00b7les", "wohl", "ge\u00b7lin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Des K\u00f6nigs, Jonathans, ja ihr selbst eignes Wort,", "tokens": ["Des", "K\u00f6\u00b7nigs", ",", "Jo\u00b7na\u00b7thans", ",", "ja", "ihr", "selbst", "eig\u00b7nes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "ADV", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "St\u00e4rckt seinen Liebesgeist, treibt alle Zagheit fort.", "tokens": ["St\u00e4rckt", "sei\u00b7nen", "Lie\u00b7bes\u00b7geist", ",", "treibt", "al\u00b7le", "Zag\u00b7heit", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}