{"dta.poem.5285": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es bindet sich in dieser Blume, dem HERRN der Crea-", "tokens": ["Es", "bin\u00b7det", "sich", "in", "die\u00b7ser", "Blu\u00b7me", ",", "dem", "HeRRN", "der", "Crea"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "tur zum Preise,", "tokens": ["tur", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Das allersch\u00f6nste Gelb\u2019 und Roth auf eine gantz besondre", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Gelb'", "und", "Roth", "auf", "ei\u00b7ne", "gantz", "be\u00b7sond\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "ART", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Weise.", "tokens": ["Wei\u00b7se", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Von aussen deckt ein g\u00fcldner Glantz die Bl\u00e4tter, wenn", "tokens": ["Von", "aus\u00b7sen", "deckt", "ein", "g\u00fcld\u00b7ner", "Glantz", "die", "Bl\u00e4t\u00b7ter", ",", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "kein Schnecken Blut", "tokens": ["kein", "Schne\u00b7cken", "Blut"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So roht, als ihre innre Seite. Der funckelnden Rubi-", "tokens": ["So", "roht", ",", "als", "ih\u00b7re", "inn\u00b7re", "Sei\u00b7te", ".", "Der", "fun\u00b7ckeln\u00b7den", "Ru\u00b7bi"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$.", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "nen Glut", "tokens": ["nen", "Glut"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Kann kaum Derselben R\u00f6the gleichen. Daher, wenn et-", "tokens": ["Kann", "kaum", "Der\u00b7sel\u00b7ben", "R\u00f6\u00b7the", "glei\u00b7chen", ".", "Da\u00b7her", ",", "wenn", "et"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VMFIN", "ADV", "PDAT", "NN", "VVINF", "$.", "PAV", "$,", "KOUS", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "wann sich ein Blat", "tokens": ["wann", "sich", "ein", "Blat"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Ein wenig umgeleget hatt\u2019,", "tokens": ["Ein", "we\u00b7nig", "um\u00b7ge\u00b7le\u00b7get", "hatt'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Es \u00f6fters schien,", "tokens": ["Es", "\u00f6f\u00b7ters", "schien", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Als ein im allersch\u00f6nsten Golde mit Flei\u00df gefasseter, Ru-", "tokens": ["Als", "ein", "im", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Gol\u00b7de", "mit", "Flei\u00df", "ge\u00b7fas\u00b7se\u00b7ter", ",", "Ru"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "APPRART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "bin;", "tokens": ["bin", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Oft kam es unserm frohen Blicke Verwundrungs-voll nicht", "tokens": ["Oft", "kam", "es", "un\u00b7serm", "fro\u00b7hen", "Bli\u00b7cke", "Ver\u00b7wun\u00b7drungs\u00b7voll", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "NN", "PTKNEG"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "anders vor", "tokens": ["an\u00b7ders", "vor"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Als ein mit dunckel-rohtem Sammte reich ausgef\u00fctterter", "tokens": ["Als", "ein", "mit", "dun\u00b7ckel\u00b7roh\u00b7tem", "Samm\u00b7te", "reich", "aus\u00b7ge\u00b7f\u00fct\u00b7ter\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "ADJA", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Drap d' Or.", "tokens": ["Drap", "d'", "Or", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Recht mitten in der duncklen R\u00f6the sieht man mit unge-", "tokens": ["Recht", "mit\u00b7ten", "in", "der", "dunck\u00b7len", "R\u00f6\u00b7the", "sieht", "man", "mit", "un\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PIS", "APPR", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "zehlten Spitzen", "tokens": ["zehl\u00b7ten", "Spit\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Ein Rund, gleich einer kleinen Sonne, mit g\u00fcldnen Strah-", "tokens": ["Ein", "Rund", ",", "gleich", "ei\u00b7ner", "klei\u00b7nen", "Son\u00b7ne", ",", "mit", "g\u00fcld\u00b7nen", "Strah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "len lieblich blitzen.", "tokens": ["len", "lieb\u00b7lich", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Ein jedes Blatt war Hertzen f\u00f6rmig. Indem ich nun Der-", "tokens": ["Ein", "je\u00b7des", "Blatt", "war", "Hert\u00b7zen", "f\u00f6r\u00b7mig", ".", "In\u00b7dem", "ich", "nun", "Der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "NN", "ADJD", "$.", "KOUS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "selben zwey", "tokens": ["sel\u00b7ben", "zwey"], "token_info": ["word", "word"], "pos": ["ADV", "CARD"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Mit Lust beysammen liegen sahe, so fiel von ungefehr mir bey:", "tokens": ["Mit", "Lust", "bey\u00b7sam\u00b7men", "lie\u00b7gen", "sa\u00b7he", ",", "so", "fiel", "von", "un\u00b7ge\u00b7fehr", "mir", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "VVFIN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.26": {"text": "Da\u00df sie vom aufgeschnittnen Hertzen, das voller Glut, ein", "tokens": ["Da\u00df", "sie", "vom", "auf\u00b7ge\u00b7schnitt\u00b7nen", "Hert\u00b7zen", ",", "das", "vol\u00b7ler", "Glut", ",", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Bildni\u00df sey.", "tokens": ["Bild\u00b7ni\u00df", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Es kam mir vor, als ob die Glut, die in derselben gleich-", "tokens": ["Es", "kam", "mir", "vor", ",", "als", "ob", "die", "Glut", ",", "die", "in", "der\u00b7sel\u00b7ben", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "KOUS", "ART", "NN", "$,", "PRELS", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.29": {"text": "sam flammte,", "tokens": ["sam", "flamm\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Von einer innern Sonne stammte,", "tokens": ["Von", "ei\u00b7ner", "in\u00b7nern", "Son\u00b7ne", "stamm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ach! dacht ich, m\u00f6cht in unserm Hertzen, vom unerschaf-", "tokens": ["Ach", "!", "dacht", "ich", ",", "m\u00f6cht", "in", "un\u00b7serm", "Hert\u00b7zen", ",", "vom", "un\u00b7er\u00b7schaf"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "APPRART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "nen Sonnen-Schein,", "tokens": ["nen", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Auch einer wahren Audacht Glut best\u00e4ndig angefachet seyn!", "tokens": ["Auch", "ei\u00b7ner", "wah\u00b7ren", "Au\u00b7dacht", "Glut", "be\u00b7st\u00e4n\u00b7dig", "an\u00b7ge\u00b7fa\u00b7chet", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}