", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ITJ", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "da\u00df ich senke diese reifende,", "tokens": ["da\u00df", "ich", "sen\u00b7ke", "die\u00b7se", "rei\u00b7fen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "dem Herzen entquellende Z\u00e4hre", "tokens": ["dem", "Her\u00b7zen", "ent\u00b7quel\u00b7len\u00b7de", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "auf die Viole ... Hat er's geh\u00f6rt?", "tokens": ["auf", "die", "Vi\u00b7o\u00b7le", "...", "Hat", "er's", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "VAFIN", "PIS", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "O des Z\u00e4rtlichen, er hat's geh\u00f6rt \u2013!", "tokens": ["O", "des", "Z\u00e4rt\u00b7li\u00b7chen", ",", "er", "hat's", "ge\u00b7h\u00f6rt", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Murmelt und sch\u00fcttelt \u2013 \u2013 Meine Tr\u00e4ne", "tokens": ["Mur\u00b7melt", "und", "sch\u00fct\u00b7telt", "\u2013", "\u2013", "Mei\u00b7ne", "Tr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "KON", "VVFIN", "$(", "$(", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "gleitet sachte die Wang' herab!", "tokens": ["glei\u00b7tet", "sach\u00b7te", "die", "Wang'", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Ach kein M\u00e4dchen, kein Freund!", "tokens": ["Ach", "kein", "M\u00e4d\u00b7chen", ",", "kein", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "kein z\u00e4rtliches, z\u00e4rtlicher, ach!", "tokens": ["kein", "z\u00e4rt\u00b7li\u00b7ches", ",", "z\u00e4rt\u00b7li\u00b7cher", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "$,", "ITJ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "der ich sie breche,", "tokens": ["der", "ich", "sie", "bre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "dem ich sie gebe,", "tokens": ["dem", "ich", "sie", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "diese dir tr\u00e4nenbetaute Viole!", "tokens": ["die\u00b7se", "dir", "tr\u00e4\u00b7nen\u00b7be\u00b7tau\u00b7te", "Vi\u00b7o\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "PPER", "VVFIN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.14": {"line.1": {"text": "Und so mu\u00df sie einsam welken,", "tokens": ["Und", "so", "mu\u00df", "sie", "ein\u00b7sam", "wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so geschm\u00fcckt mit meiner Wehmut,", "tokens": ["so", "ge\u00b7schm\u00fcckt", "mit", "mei\u00b7ner", "Weh\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sterben, unbetrauert, ungeliebet, ach!", "tokens": ["ster\u00b7ben", ",", "un\u00b7be\u00b7trau\u00b7ert", ",", "un\u00b7ge\u00b7lie\u00b7bet", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ITJ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.15": {"line.1": {"text": "Mag sie doch \u2013 sinken, liegen, im Winde zerstieben!", "tokens": ["Mag", "sie", "doch", "\u2013", "sin\u00b7ken", ",", "lie\u00b7gen", ",", "im", "Win\u00b7de", "zer\u00b7stie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$(", "VVINF", "$,", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Meine Wehmut mit ihr!", "tokens": ["Mei\u00b7ne", "Weh\u00b7mut", "mit", "ihr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bist du doch gl\u00fccklich, Geliebter!", "tokens": ["Bist", "du", "doch", "gl\u00fcck\u00b7lich", ",", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "geliebt am Herzen derer, die meine Seele liebt!", "tokens": ["ge\u00b7liebt", "am", "Her\u00b