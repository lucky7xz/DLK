{"textgrid.poem.49010": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach, sie singet, die br\u00fcnstige Finke,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, sie singet, die br\u00fcnstige Finke,", "tokens": ["Ach", ",", "sie", "sin\u00b7get", ",", "die", "br\u00fcns\u00b7ti\u00b7ge", "Fin\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "breitet den zarten Fl\u00fcgel \u00fcbers vollendete Nestchen,", "tokens": ["brei\u00b7tet", "den", "zar\u00b7ten", "Fl\u00fc\u00b7gel", "\u00fc\u00b7bers", "voll\u00b7en\u00b7de\u00b7te", "Nest\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "zwitschert \u2013 und schlummert zum erstenmal wonniglich ein.", "tokens": ["zwit\u00b7schert", "\u2013", "und", "schlum\u00b7mert", "zum", "ers\u00b7ten\u00b7mal", "won\u00b7nig\u00b7lich", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "VVFIN", "APPRART", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und du, mein Freund, ferne! ferne!", "tokens": ["Und", "du", ",", "mein", "Freund", ",", "fer\u00b7ne", "!", "fer\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$.", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttle den Tau, wehender Nachthauch! ich schaure!", "tokens": ["Sch\u00fctt\u00b7le", "den", "Tau", ",", "we\u00b7hen\u00b7der", "Nacht\u00b7hauch", "!", "ich", "schau\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "sch\u00fcttl', ach sch\u00fcttl' ihn mir,", "tokens": ["sch\u00fcttl'", ",", "ach", "sch\u00fcttl'", "ihn", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ITJ", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "da\u00df ich senke diese reifende,", "tokens": ["da\u00df", "ich", "sen\u00b7ke", "die\u00b7se", "rei\u00b7fen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "dem Herzen entquellende Z\u00e4hre", "tokens": ["dem", "Her\u00b7zen", "ent\u00b7quel\u00b7len\u00b7de", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "auf die Viole ... Hat er's geh\u00f6rt?", "tokens": ["auf", "die", "Vi\u00b7o\u00b7le", "...", "Hat", "er's", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "VAFIN", "PIS", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "O des Z\u00e4rtlichen, er hat's geh\u00f6rt \u2013!", "tokens": ["O", "des", "Z\u00e4rt\u00b7li\u00b7chen", ",", "er", "hat's", "ge\u00b7h\u00f6rt", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Murmelt und sch\u00fcttelt \u2013 \u2013 Meine Tr\u00e4ne", "tokens": ["Mur\u00b7melt", "und", "sch\u00fct\u00b7telt", "\u2013", "\u2013", "Mei\u00b7ne", "Tr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "KON", "VVFIN", "$(", "$(", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "gleitet sachte die Wang' herab!", "tokens": ["glei\u00b7tet", "sach\u00b7te", "die", "Wang'", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ach kein M\u00e4dchen, kein Freund!", "tokens": ["Ach", "kein", "M\u00e4d\u00b7chen", ",", "kein", "Freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "kein z\u00e4rtliches, z\u00e4rtlicher, ach!", "tokens": ["kein", "z\u00e4rt\u00b7li\u00b7ches", ",", "z\u00e4rt\u00b7li\u00b7cher", ",", "ach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "$,", "ITJ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "der ich sie breche,", "tokens": ["der", "ich", "sie", "bre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "dem ich sie gebe,", "tokens": ["dem", "ich", "sie", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "diese dir tr\u00e4nenbetaute Viole!", "tokens": ["die\u00b7se", "dir", "tr\u00e4\u00b7nen\u00b7be\u00b7tau\u00b7te", "Vi\u00b7o\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "PPER", "VVFIN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so mu\u00df sie einsam welken,", "tokens": ["Und", "so", "mu\u00df", "sie", "ein\u00b7sam", "wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "so geschm\u00fcckt mit meiner Wehmut,", "tokens": ["so", "ge\u00b7schm\u00fcckt", "mit", "mei\u00b7ner", "Weh\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sterben, unbetrauert, ungeliebet, ach!", "tokens": ["ster\u00b7ben", ",", "un\u00b7be\u00b7trau\u00b7ert", ",", "un\u00b7ge\u00b7lie\u00b7bet", ",", "ach", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ITJ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Mag sie doch \u2013 sinken, liegen, im Winde zerstieben!", "tokens": ["Mag", "sie", "doch", "\u2013", "sin\u00b7ken", ",", "lie\u00b7gen", ",", "im", "Win\u00b7de", "zer\u00b7stie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$(", "VVINF", "$,", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Meine Wehmut mit ihr!", "tokens": ["Mei\u00b7ne", "Weh\u00b7mut", "mit", "ihr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bist du doch gl\u00fccklich, Geliebter!", "tokens": ["Bist", "du", "doch", "gl\u00fcck\u00b7lich", ",", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "geliebt am Herzen derer, die meine Seele liebt!", "tokens": ["ge\u00b7liebt", "am", "Her\u00b7zen", "de\u00b7rer", ",", "die", "mei\u00b7ne", "See\u00b7le", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "PDS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ha! dies wilde, pochende,", "tokens": ["Ha", "!", "dies", "wil\u00b7de", ",", "po\u00b7chen\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "dies unaufhaltsam fliegende,", "tokens": ["dies", "un\u00b7auf\u00b7halt\u00b7sam", "flie\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "dies \u00e4ngstlich tragende, mitf\u00fchlende Herz!", "tokens": ["dies", "\u00e4ngst\u00b7lich", "tra\u00b7gen\u00b7de", ",", "mit\u00b7f\u00fch\u00b7len\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "das, ungl\u00fcckselig ewig,", "tokens": ["das", ",", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "e\u00b7wig", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "barbarisch immer aufnimmt und tr\u00e4gt!", "tokens": ["bar\u00b7ba\u00b7risch", "im\u00b7mer", "auf\u00b7nimmt", "und", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie's dr\u00e4ngt', wie's tobt! dir vorw\u00e4rts nacheilt,", "tokens": ["Wie's", "dr\u00e4ngt'", ",", "wie's", "tobt", "!", "dir", "vor\u00b7w\u00e4rts", "na\u00b7ch\u00b7eilt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "VVFIN", "$.", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und mich peinigt und qu\u00e4lt,", "tokens": ["und", "mich", "pei\u00b7nigt", "und", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "und meine Sinne zerr\u00fcttet,", "tokens": ["und", "mei\u00b7ne", "Sin\u00b7ne", "zer\u00b7r\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "und mir die Nerven zerrei\u00dft!", "tokens": ["und", "mir", "die", "Ner\u00b7ven", "zer\u00b7rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Wachs't zu ", "tokens": ["Wachs't", "zu"], "token_info": ["word", "word"], "pos": ["NN", "PTKZU"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ach Seligkeit des Himmels", "tokens": ["Ach", "Se\u00b7lig\u00b7keit", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "tr\u00e4ufelt nieder dem,", "tokens": ["tr\u00e4u\u00b7felt", "nie\u00b7der", "dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "der des Geliebten Busen umschlingt!", "tokens": ["der", "des", "Ge\u00b7lieb\u00b7ten", "Bu\u00b7sen", "um\u00b7schlingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "O ich weine, da du, Trunkner,", "tokens": ["O", "ich", "wei\u00b7ne", ",", "da", "du", ",", "Trunk\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "KOUS", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da du, Seliger,", "tokens": ["da", "du", ",", "Se\u00b7li\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "an Stollbergs Busen dich kn\u00fcpfst.", "tokens": ["an", "Stoll\u00b7bergs", "Bu\u00b7sen", "dich", "kn\u00fcpfst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Genie\u00dfe! und gedenke meiner!", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "!", "und", "ge\u00b7den\u00b7ke", "mei\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ha! gedenke meiner, wenn du tr\u00e4nenschauernd", "tokens": ["Ha", "!", "ge\u00b7den\u00b7ke", "mei\u00b7ner", ",", "wenn", "du", "tr\u00e4\u00b7nen\u00b7schau\u00b7ernd"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "$,", "KOUS", "PPER", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "unter der Liebe F\u00fclle versinkst", "tokens": ["un\u00b7ter", "der", "Lie\u00b7be", "F\u00fcl\u00b7le", "ver\u00b7sinkst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "und du am Herzen liegest ", "tokens": ["und", "du", "am", "Her\u00b7zen", "lie\u00b7gest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "o wie soll ich ihn nennen!", "tokens": ["o", "wie", "soll", "ich", "ihn", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "KON", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Vater! Freund! Vater!", "tokens": ["Va\u00b7ter", "!", "Freund", "!", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Wenn du an ihm h\u00e4ngst", "tokens": ["Wenn", "du", "an", "ihm", "h\u00e4ngst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "und herzerdr\u00fcckend, zermalmend", "tokens": ["und", "her\u00b7zer\u00b7dr\u00fc\u00b7ckend", ",", "zer\u00b7mal\u00b7mend"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVPP", "$,", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00fcber dir die Wonne liegt,", "tokens": ["\u00fc\u00b7ber", "dir", "die", "Won\u00b7ne", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "o, dann rei\u00dfe dich auf, atme und schaure", "tokens": ["o", ",", "dann", "rei\u00b7\u00dfe", "dich", "auf", ",", "at\u00b7me", "und", "schau\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "und gedenke deines Einsamen hier;", "tokens": ["und", "ge\u00b7den\u00b7ke", "dei\u00b7nes", "Ein\u00b7sa\u00b7men", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "und, indem du noch einmal", "tokens": ["und", ",", "in\u00b7dem", "du", "noch", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "die geliebte Stirne dr\u00fcckst,", "tokens": ["die", "ge\u00b7lieb\u00b7te", "Stir\u00b7ne", "dr\u00fcckst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "gedenke deines Einsamen hier", "tokens": ["ge\u00b7den\u00b7ke", "dei\u00b7nes", "Ein\u00b7sa\u00b7men", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "und wehm\u00fctig und leise so:", "tokens": ["und", "weh\u00b7m\u00fc\u00b7tig", "und", "lei\u00b7se", "so", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Der, der jetzt noch um mich klaget,", "tokens": ["Der", ",", "der", "jetzt", "noch", "um", "mich", "kla\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "schmachtend den Fr\u00fchling verseufzt,", "tokens": ["schmach\u00b7tend", "den", "Fr\u00fch\u00b7ling", "ver\u00b7seufzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "o, des einsamen J\u00fcnglings \u2013", "tokens": ["o", ",", "des", "ein\u00b7sa\u00b7men", "J\u00fcng\u00b7lings", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "er liebt dich ewig wie ich!", "tokens": ["er", "liebt", "dich", "e\u00b7wig", "wie", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Ach, sie singet, die br\u00fcnstige Finke,", "tokens": ["Ach", ",", "sie", "sin\u00b7get", ",", "die", "br\u00fcns\u00b7ti\u00b7ge", "Fin\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "po