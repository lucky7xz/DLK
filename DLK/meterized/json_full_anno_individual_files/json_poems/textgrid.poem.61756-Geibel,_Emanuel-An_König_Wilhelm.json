{"textgrid.poem.61756": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An K\u00f6nig Wilhelm", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit festlich tiefem Fr\u00fchgel\u00e4ute", "tokens": ["Mit", "fest\u00b7lich", "tie\u00b7fem", "Fr\u00fch\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fc\u00dft dich bei des Morgens Strahl,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "dich", "bei", "des", "Mor\u00b7gens", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begr\u00fc\u00dft, o Herr, in Ehrfurcht heute", "tokens": ["Be\u00b7gr\u00fc\u00dft", ",", "o", "Herr", ",", "in", "Ehr\u00b7furcht", "heu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "FM", "NN", "$,", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich unsre Stadt zum erstenmal;", "tokens": ["Dich", "uns\u00b7re", "Stadt", "zum", "ers\u00b7ten\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem hohen Schirmvogt ihr Willkommen", "tokens": ["Dem", "ho\u00b7hen", "Schirm\u00b7vogt", "ihr", "Will\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Neidlosen Jubels bringt sie dar,", "tokens": ["Neid\u00b7lo\u00b7sen", "Ju\u00b7bels", "bringt", "sie", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die selbst in Zeiten l\u00e4ngst verglommen", "tokens": ["Die", "selbst", "in", "Zei\u00b7ten", "l\u00e4ngst", "ver\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des alten Nordbunds F\u00fcrstin war.", "tokens": ["Des", "al\u00b7ten", "Nord\u00b7bunds", "F\u00fcrs\u00b7tin", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Banner, das in jenen Tagen", "tokens": ["Das", "Ban\u00b7ner", ",", "das", "in", "je\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schwestern all am Ostseestrand", "tokens": ["Den", "Schwes\u00b7tern", "all", "am", "Ost\u00b7see\u00b7strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00fchngemut vorangetragen,", "tokens": ["Sie", "k\u00fchn\u00b7ge\u00b7mut", "vor\u00b7an\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch flattert's nun in deiner Hand,", "tokens": ["Hoch", "flat\u00b7tert's", "nun", "in", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In deiner Hand, die auserkoren", "tokens": ["In", "dei\u00b7ner", "Hand", ",", "die", "au\u00b7ser\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Herrn der Herrn, dem sie vertraut,", "tokens": ["Vom", "Herrn", "der", "Herrn", ",", "dem", "sie", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Heiligtum, das wir verloren,", "tokens": ["Das", "Hei\u00b7lig\u00b7tum", ",", "das", "wir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das deutsche Reich uns wieder baut.", "tokens": ["Das", "deut\u00b7sche", "Reich", "uns", "wie\u00b7der", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon ragt bis zu des Maines Borden", "tokens": ["Schon", "ragt", "bis", "zu", "des", "Mai\u00b7nes", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Werk, darob dein Adler wacht,", "tokens": ["Das", "Werk", ",", "da\u00b7rob", "dein", "Ad\u00b7ler", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versammelnd alle St\u00e4mm' im Norden,", "tokens": ["Ver\u00b7sam\u00b7melnd", "al\u00b7le", "St\u00e4mm'", "im", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Riesenfeste deutscher Macht;", "tokens": ["Die", "Rie\u00b7sen\u00b7fes\u00b7te", "deut\u00b7scher", "Macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie auch wir das Banner pflanzen,", "tokens": ["Und", "wie", "auch", "wir", "das", "Ban\u00b7ner", "pflan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das dreifach prangt in Farbenglut,", "tokens": ["Das", "drei\u00b7fach", "prangt", "in", "Far\u00b7beng\u00b7lut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durchstr\u00f6mt uns im Gef\u00fchl des Ganzen", "tokens": ["Durch\u00b7str\u00f6mt", "uns", "im", "Ge\u00b7f\u00fchl", "des", "Gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verj\u00fcngte Kraft, erneuter Mut.", "tokens": ["Ver\u00b7j\u00fcng\u00b7te", "Kraft", ",", "er\u00b7neu\u00b7ter", "Mut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im engen Bett schlich unser Leben", "tokens": ["Im", "en\u00b7gen", "Bett", "schlich", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vereinzelt wie der Bach im Sand;", "tokens": ["Ver\u00b7ein\u00b7zelt", "wie", "der", "Bach", "im", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hast du uns, was not, gegeben,", "tokens": ["Da", "hast", "du", "uns", ",", "was", "not", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "PWS", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Glauben an ein Vaterland.", "tokens": ["Den", "Glau\u00b7ben", "an", "ein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das sch\u00f6ne Recht, uns selbst zu achten,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Recht", ",", "uns", "selbst", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das uns des Auslands Hohn verschlang,", "tokens": ["Das", "uns", "des", "Aus\u00b7lands", "Hohn", "ver\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hast du im Donner deiner Schlachten", "tokens": ["Hast", "du", "im", "Don\u00b7ner", "dei\u00b7ner", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Uns heimgekauft, o habe Dank!", "tokens": ["Uns", "heim\u00b7ge\u00b7kauft", ",", "o", "ha\u00b7be", "Dank", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun weht von T\u00fcrmen, flaggt von Masten", "tokens": ["Nun", "weht", "von", "T\u00fcr\u00b7men", ",", "flaggt", "von", "Mas\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das deutsche Zeichen allgeehrt;", "tokens": ["Das", "deut\u00b7sche", "Zei\u00b7chen", "all\u00b7ge\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihm geschirmt nun bringt die Lasten", "tokens": ["Von", "ihm", "ge\u00b7schirmt", "nun", "bringt", "die", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schiffer froh zum Heimatsherd.", "tokens": ["Der", "Schif\u00b7fer", "froh", "zum", "Hei\u00b7mats\u00b7herd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun mag am harmlos r\u00fcst'gen Werke", "tokens": ["Nun", "mag", "am", "harm\u00b7los", "r\u00fcst'\u00b7gen", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Kunstflei\u00df schaffen unverzagt,", "tokens": ["Der", "Kunst\u00b7flei\u00df", "schaf\u00b7fen", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn Friedensb\u00fcrgschaft ist die St\u00e4rke,", "tokens": ["Denn", "Frie\u00b7dens\u00b7b\u00fcrg\u00b7schaft", "ist", "die", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Daran kein Feind zu r\u00fchren wagt.", "tokens": ["Da\u00b7ran", "kein", "Feind", "zu", "r\u00fch\u00b7ren", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum Heil mit dir und deinem Throne!", "tokens": ["Drum", "Heil", "mit", "dir", "und", "dei\u00b7nem", "Thro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und flicht als gr\u00fcnes Eichenblatt", "tokens": ["Und", "flicht", "als", "gr\u00fc\u00b7nes", "Ei\u00b7chen\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deine Gold- und Lorbeerkrone", "tokens": ["In", "dei\u00b7ne", "Gold", "und", "