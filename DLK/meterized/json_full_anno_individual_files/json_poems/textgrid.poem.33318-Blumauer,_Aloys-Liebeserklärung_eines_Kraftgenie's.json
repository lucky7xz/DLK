{"textgrid.poem.33318": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Liebeserkl\u00e4rung eines Kraftgenie's", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha, wie rudert meine ganze Seele", "tokens": ["Ha", ",", "wie", "ru\u00b7dert", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nun in der Empfindung Ozean?", "tokens": ["Nun", "in", "der", "Emp\u00b7fin\u00b7dung", "O\u00b7ze\u00b7an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Laute Seufzer sprengen mir die Kehle,", "tokens": ["Lau\u00b7te", "Seuf\u00b7zer", "spren\u00b7gen", "mir", "die", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die man auf zehn Meilen h\u00f6ren kann.", "tokens": ["Die", "man", "auf", "zehn", "Mei\u00b7len", "h\u00f6\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "CARD", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Gleich Kanonenkugeln rollen Thr\u00e4nen", "tokens": ["Gleich", "Ka\u00b7no\u00b7nen\u00b7ku\u00b7geln", "rol\u00b7len", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus den beiden Augenm\u00f6sern mir:", "tokens": ["Aus", "den", "bei\u00b7den", "Au\u00b7gen\u00b7m\u00f6\u00b7sern", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Erd' und Himmel bebt bei meinem St\u00f6hnen,", "tokens": ["Erd'", "und", "Him\u00b7mel", "bebt", "bei", "mei\u00b7nem", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich br\u00fclle schluchzend \u2013 wie ein Stier.", "tokens": ["Und", "ich", "br\u00fcl\u00b7le", "schluch\u00b7zend", "\u2013", "wie", "ein", "Stier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$(", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wetterst\u00fcrme der Empfindung treiben", "tokens": ["Wet\u00b7ter\u00b7st\u00fcr\u00b7me", "der", "Emp\u00b7fin\u00b7dung", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mich oft-, west- und s\u00fcd- und nordenw\u00e4rts:", "tokens": ["Mich", "oft", ",", "west", "und", "s\u00fcd", "und", "nor\u00b7den\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "$,", "TRUNC", "KON", "TRUNC", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Seele hat in mir kein Bleiben,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "hat", "in", "mir", "kein", "Blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und es blitzt und donnert mir das Herz.", "tokens": ["Und", "es", "blitzt", "und", "don\u00b7nert", "mir", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ach! ich mu\u00df, ich mu\u00df im Sturm versinken!", "tokens": ["Ach", "!", "ich", "mu\u00df", ",", "ich", "mu\u00df", "im", "Sturm", "ver\u00b7sin\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rette mich, gro\u00dfm\u00fcth'ge Seele, doch!", "tokens": ["Ret\u00b7te", "mich", ",", "gro\u00df\u00b7m\u00fcth'\u00b7ge", "See\u00b7le", ",", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich beginne schon den Tod zu trinken,", "tokens": ["Ich", "be\u00b7gin\u00b7ne", "schon", "den", "Tod", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sieh, mein Lebensnachen hat ein Loch!", "tokens": ["Sieh", ",", "mein", "Le\u00b7bens\u00b7na\u00b7chen", "hat", "ein", "Loch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "me