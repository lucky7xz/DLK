{"textgrid.poem.49111": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei freudig, liebes Pettergen, du weisst nichts von dem Tage,", "tokens": ["Sei", "freu\u00b7dig", ",", "lie\u00b7bes", "Pet\u00b7ter\u00b7gen", ",", "du", "weisst", "nichts", "von", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-++--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Der doch so freudig ist, dich st\u00f6ret keine Plage.", "tokens": ["Der", "doch", "so", "freu\u00b7dig", "ist", ",", "dich", "st\u00f6\u00b7ret", "kei\u00b7ne", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du liegst in deiner Ruh, du weisst nichts von den Sorgen,", "tokens": ["Du", "liegst", "in", "dei\u00b7ner", "Ruh", ",", "du", "weisst", "nichts", "von", "den", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du denkest nicht was heut', du denkest nicht was morgen", "tokens": ["Du", "den\u00b7kest", "nicht", "was", "heut'", ",", "du", "den\u00b7kest", "nicht", "was", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PWS", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich wohl zutragen kann, doch du bist stets vergn\u00fcgt,", "tokens": ["Sich", "wohl", "zu\u00b7tra\u00b7gen", "kann", ",", "doch", "du", "bist", "stets", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "$,", "KON", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn man mit Singen dich in deiner Wiege wiegt.", "tokens": ["Wenn", "man", "mit", "Sin\u00b7gen", "dich", "in", "dei\u00b7ner", "Wie\u00b7ge", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was soll ich w\u00fcnschen dir, was soll ich bringen dir,", "tokens": ["Was", "soll", "ich", "w\u00fcn\u00b7schen", "dir", ",", "was", "soll", "ich", "brin\u00b7gen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "PPER", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als das der liebe Gott sei bei dir f\u00fcr und f\u00fcr,", "tokens": ["Als", "das", "der", "lie\u00b7be", "Gott", "sei", "bei", "dir", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der segne dich mit Kraft, mit Weisheit und Verstand,", "tokens": ["Der", "seg\u00b7ne", "dich", "mit", "Kraft", ",", "mit", "Weis\u00b7heit", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der halt' dich gn\u00e4diglich in seiner Vaterhand,", "tokens": ["Der", "halt'", "dich", "gn\u00e4\u00b7di\u00b7glich", "in", "sei\u00b7ner", "Va\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So bist du wohl versorgt, so bist du sorgenlos,", "tokens": ["So", "bist", "du", "wohl", "ver\u00b7sorgt", ",", "so", "bist", "du", "sor\u00b7gen\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil dich der treue Gott stets h\u00e4lt in seinem Schooss. \u2013", "tokens": ["Weil", "dich", "der", "treu\u00b7e", "Gott", "stets", "h\u00e4lt", "in", "sei\u00b7nem", "Scho\u00b7oss", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sei freudig, liebes Pettergen, du weisst nichts von dem Tage,", "tok