{"dta.poem.9602": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die scheelen augen.  \n 1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr scheelen augen ihr/ wie wohl ist der daran/", "tokens": ["Ihr", "schee\u00b7len", "au\u00b7gen", "ihr", "/", "wie", "wohl", "ist", "der", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "KOKOM", "ADV", "VAFIN", "ART", "PAV", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der einen seiten-blick vor euch geniessen kan:", "tokens": ["Der", "ei\u00b7nen", "sei\u00b7ten\u00b7blick", "vor", "euch", "ge\u00b7nies\u00b7sen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es gehet euer glantz vor andrer augen-zier;", "tokens": ["Es", "ge\u00b7het", "eu\u00b7er", "glantz", "vor", "an\u00b7drer", "au\u00b7gen\u00b7zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr habt den besten prei\u00df/ ihr scheelen augen ihr.", "tokens": ["Ihr", "habt", "den", "bes\u00b7ten", "prei\u00df", "/", "ihr", "schee\u00b7len", "au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was n\u00fctzet mir ein schein/ dadurch man in verdacht/", "tokens": ["Was", "n\u00fct\u00b7zet", "mir", "ein", "schein", "/", "da\u00b7durch", "man", "in", "ver\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJD", "$(", "PAV", "PIS", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dadurch der liebes-bund wird an das licht gebracht?", "tokens": ["Da\u00b7durch", "der", "lie\u00b7bes\u00b7bund", "wird", "an", "das", "licht", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr aber wist davor gantz heimliche manier/", "tokens": ["Ihr", "a\u00b7ber", "wist", "da\u00b7vor", "gantz", "heim\u00b7li\u00b7che", "ma\u00b7nier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PAV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie ihr lieb\u00e4ugelen solt/ ihr scheelen augen ihr.", "tokens": ["Wie", "ihr", "lie\u00b7b\u00e4u\u00b7ge\u00b7len", "solt", "/", "ihr", "schee\u00b7len", "au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$(", "PPOSAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Manch auserlesnes paar mu\u00df durch verliebten schein/", "tokens": ["Manch", "au\u00b7ser\u00b7les\u00b7nes", "paar", "mu\u00df", "durch", "ver\u00b7lieb\u00b7ten", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenns gleiche strahlen wirfft/ sein selbst verr\u00e4ther seyn/", "tokens": ["Wenns", "glei\u00b7che", "strah\u00b7len", "wirfft", "/", "sein", "selbst", "ver\u00b7r\u00e4\u00b7ther", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$(", "PPOSAT", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es zeiget aller welt die sehnliche begier/", "tokens": ["Es", "zei\u00b7get", "al\u00b7ler", "welt", "die", "sehn\u00b7li\u00b7che", "be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die in dem hertzen steckt/ ihr scheelen augen ihr.", "tokens": ["Die", "in", "dem", "hert\u00b7zen", "steckt", "/", "ihr", "schee\u00b7len", "au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jhr aber/ wenn ihr gleich gerade von euch blitzt/", "tokens": ["Ihr", "a\u00b7ber", "/", "wenn", "ihr", "gleich", "ge\u00b7ra\u00b7de", "von", "euch", "blitzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So geht es jenen an/ der euch zur seiten sitzt/", "tokens": ["So", "geht", "es", "je\u00b7nen", "an", "/", "der", "euch", "zur", "sei\u00b7ten", "sitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKVZ", "$(", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den ihr euch au\u00dferwehlt/ und niemand mercket hier", "tokens": ["Den", "ihr", "euch", "au\u00b7\u00dfer\u00b7wehlt", "/", "und", "nie\u00b7mand", "mer\u00b7cket", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "VVPP", "$(", "KON", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den heimlichen verstand/ ihr scheelen augen ihr.", "tokens": ["Den", "heim\u00b7li\u00b7chen", "ver\u00b7stand", "/", "ihr", "schee\u00b7len", "au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jhr scheelen augen stralt mit g\u00f6tter-gleichem schein/", "tokens": ["Ihr", "schee\u00b7len", "au\u00b7gen", "stralt", "mit", "g\u00f6t\u00b7ter\u00b7glei\u00b7chem", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch Venus wolte selbst also gemahlet seyn.", "tokens": ["Auch", "Ve\u00b7nus", "wol\u00b7te", "selbst", "al\u00b7so", "ge\u00b7mah\u00b7let", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum zweiffle niemand nicht/ da\u00df euch die gleiche zier", "tokens": ["Drum", "zweiff\u00b7le", "nie\u00b7mand", "nicht", "/", "da\u00df", "euch", "die", "glei\u00b7che", "zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "PTKNEG", "$(", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sch\u00f6nsten Venus gleicht/ ihr scheelen augen ihr.", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Ve\u00b7nus", "gleicht", "/", "ihr", "schee\u00b7len", "au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gl\u00fcckseelig wer also verstohlen brennen kan/", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "wer", "al\u00b7so", "ver\u00b7stoh\u00b7len", "bren\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PWS", "ADV", "VVINF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den greifft kein frembder neid in seinem lieben an.", "tokens": ["Den", "greifft", "kein", "fremb\u00b7der", "neid", "in", "sei\u00b7nem", "lie\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein gutes gl\u00fccke geht den andern allen f\u00fcr/", "tokens": ["Sein", "gu\u00b7tes", "gl\u00fc\u00b7cke", "geht", "den", "an\u00b7dern", "al\u00b7len", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "PIAT", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er lebt durch euch begl\u00fcckt/ ihr scheelen augen ihr.", "tokens": ["Er", "lebt", "durch", "euch", "be\u00b7gl\u00fcckt", "/", "ihr", "schee\u00b7len", "au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP", "$(", "PPOSAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}