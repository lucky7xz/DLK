{"dta.poem.19069": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "L.   Bey einer   Visite   von dem itzigen Herrn  \n Abt zu Bergen in Dresden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "O GOtt! der Liebe Wunder-Quell,", "tokens": ["O", "Gott", "!", "der", "Lie\u00b7be", "Wun\u00b7der\u00b7Quell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Mensch in Gnaden, ohne S\u00fcnde,", "tokens": ["Du", "Mensch", "in", "Gna\u00b7den", ",", "oh\u00b7ne", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und unser F\u00fcrst Jmmanuel,", "tokens": ["Und", "un\u00b7ser", "F\u00fcrst", "Jm\u00b7ma\u00b7nu\u00b7el", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Geist der H\u00f6hen und der Gr\u00fcnde,", "tokens": ["Du", "Geist", "der", "H\u00f6\u00b7hen", "und", "der", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es setzt uns deiner Wege Lauf", "tokens": ["Es", "setzt", "uns", "dei\u00b7ner", "We\u00b7ge", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ehrerbietiges Erstaunen,", "tokens": ["In", "ehr\u00b7er\u00b7bie\u00b7ti\u00b7ges", "Er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch thun sich unsre Lippen auf,", "tokens": ["Doch", "thun", "sich", "uns\u00b7re", "Lip\u00b7pen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dir recht muthig zu posaunen,", "tokens": ["Von", "dir", "recht", "mut\u00b7hig", "zu", "po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du K\u00f6nig aller Welt,", "tokens": ["Du", "K\u00f6\u00b7nig", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Du zwey gestammter Held,", "tokens": ["Du", "zwey", "ge\u00b7stamm\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Du unsers Lebens beste Freude,", "tokens": ["Du", "un\u00b7sers", "Le\u00b7bens", "bes\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Du Schrecken der Vernunft,", "tokens": ["Du", "Schre\u00b7cken", "der", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und der verkehrten Zunft,", "tokens": ["Und", "der", "ver\u00b7kehr\u00b7ten", "Zunft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Der Deinen wahre Seelen-Weyde.", "tokens": ["Der", "Dei\u00b7nen", "wah\u00b7re", "See\u00b7len\u00b7Wey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du willst, da\u00df unsre Hertzen dir", "tokens": ["Du", "willst", ",", "da\u00df", "uns\u00b7re", "Hert\u00b7zen", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Lob-Ges\u00e4ngen und mit Liedern,", "tokens": ["Mit", "Lob\u00b7Ge\u00b7s\u00e4n\u00b7gen", "und", "mit", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Creutzes reiches Blut-Revier,", "tokens": ["Ins", "Creut\u00b7zes", "rei\u00b7ches", "Blut\u00b7Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mitten unter unsern Br\u00fcdern,", "tokens": ["Und", "mit\u00b7ten", "un\u00b7ter", "un\u00b7sern", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bey aufgestiegner Fr\u00f6lichkeit,", "tokens": ["Bey", "auf\u00b7ge\u00b7stieg\u00b7ner", "Fr\u00f6\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ersten Fr\u00fcchte zinsen sollen.", "tokens": ["Die", "ers\u00b7ten", "Fr\u00fcch\u00b7te", "zin\u00b7sen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn sie des Lebens kurtze Zeit", "tokens": ["Wenn", "sie", "des", "Le\u00b7bens", "kurt\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins Ewige verwandeln wollen,", "tokens": ["Ins", "E\u00b7wi\u00b7ge", "ver\u00b7wan\u00b7deln", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nimm unsern frohen Sinn,", "tokens": ["Nimm", "un\u00b7sern", "fro\u00b7hen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "In diesem Liede hin,", "tokens": ["In", "die\u00b7sem", "Lie\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und g\u00f6nn uns gar geringen Knechten,", "tokens": ["Und", "g\u00f6nn", "uns", "gar", "ge\u00b7rin\u00b7gen", "Knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df wir um deine Treu,", "tokens": ["Da\u00df", "wir", "um", "dei\u00b7ne", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Mit dir, und mit uns selber rechten.", "tokens": ["Mit", "dir", ",", "und", "mit", "uns", "sel\u00b7ber", "rech\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "APPR", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr Hertzen, die da reine Lieb", "tokens": ["Ihr", "Hert\u00b7zen", ",", "die", "da", "rei\u00b7ne", "Lieb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Christo JEsu fest verkn\u00fcpfet,", "tokens": ["In", "Chris\u00b7to", "Je\u00b7su", "fest", "ver\u00b7kn\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der aufgeregte Liebes-Trieb,", "tokens": ["Der", "auf\u00b7ge\u00b7reg\u00b7te", "Lie\u00b7bes\u00b7Trieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sehr er itzt dem HErren h\u00fcpfet,", "tokens": ["So", "sehr", "er", "itzt", "dem", "Her\u00b7ren", "h\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In JEsu Liebes-Arme ziehet,", "tokens": ["In", "Je\u00b7su", "Lie\u00b7bes\u00b7Ar\u00b7me", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So sehr auch nach dem Vaterland,", "tokens": ["So", "sehr", "auch", "nach", "dem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein sehnliches Verlangen gl\u00fchet;)", "tokens": ["Sein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", "gl\u00fc\u00b7het", ";)"], "token_info": ["word", "word", "word", "word", "emoticon"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So tr\u00e4g erweist er sich,", "tokens": ["So", "tr\u00e4g", "er\u00b7weist", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So wenig ritterlich,", "tokens": ["So", "we\u00b7nig", "rit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wenns an ein rechtes Ringen gehet,", "tokens": ["Wenns", "an", "ein", "rech\u00b7tes", "Rin\u00b7gen", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn unversehner Kampf,", "tokens": ["Wenn", "un\u00b7ver\u00b7seh\u00b7ner", "Kampf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und unbequemer Dampf,", "tokens": ["Und", "un\u00b7be\u00b7que\u00b7mer", "Dampf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Jhm vor den bl\u00f6den Augen stehet.", "tokens": ["Jhm", "vor", "den", "bl\u00f6\u00b7den", "Au\u00b7gen", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir wollen diesen Abend noch", "tokens": ["Wir", "wol\u00b7len", "die\u00b7sen", "A\u00b7bend", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns dieser Tr\u00e4gheit sch\u00e4men lernen.", "tokens": ["Uns", "die\u00b7ser", "Tr\u00e4g\u00b7heit", "sch\u00e4\u00b7men", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und uns von JEsu sanftem Joch", "tokens": ["Und", "uns", "von", "Je\u00b7su", "sanf\u00b7tem", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht einen Augenblick entfernen,", "tokens": ["Nicht", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Hertzen, ach! begreiffet euch,", "tokens": ["Ihr", "Hert\u00b7zen", ",", "ach", "!", "be\u00b7greif\u00b7fet", "euch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der HErr verdienet eure Treue;", "tokens": ["Der", "Herr", "ver\u00b7die\u00b7net", "eu\u00b7re", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Unterthan in seinem Reich,", "tokens": ["Ein", "Un\u00b7ter\u00b7than", "in", "sei\u00b7nem", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Trinckt einst mit JEsu auch das Neue;", "tokens": ["Trinckt", "einst", "mit", "Je\u00b7su", "auch", "das", "Neu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "ADV", "ART", "ADJA", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "So viel nun euer sind,", "tokens": ["So", "viel", "nun", "eu\u00b7er", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die JEsus tr\u00e4ge find,", "tokens": ["Die", "Je\u00b7sus", "tr\u00e4\u00b7ge", "find", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die wecke doch sein theures Leiden\u2019;", "tokens": ["Die", "we\u00b7cke", "doch", "sein", "theu\u00b7res", "Lei\u00b7den'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "O! du der Seelen-Mann,", "tokens": ["O", "!", "du", "der", "See\u00b7len\u00b7Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Nimm unsre Seelen an,", "tokens": ["Nimm", "uns\u00b7re", "See\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "La\u00df sie in deinen Schmertzen weyden.", "tokens": ["La\u00df", "sie", "in", "dei\u00b7nen", "Schmert\u00b7zen", "wey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}