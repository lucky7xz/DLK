{"dta.poem.8734": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Verhehl' auch dies mir nicht,", "tokens": ["Ver\u00b7hehl'", "auch", "dies", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PDS", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was f\u00fcr ein Zauberwesen", "tokens": ["Was", "f\u00fcr", "ein", "Zau\u00b7ber\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus deiner Stimme spricht?", "tokens": ["Aus", "dei\u00b7ner", "Stim\u00b7me", "spricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So viele Worte dringen", "tokens": ["So", "vie\u00b7le", "Wor\u00b7te", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An's Ohr uns ohne Plan,", "tokens": ["An's", "Ohr", "uns", "oh\u00b7ne", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4hrend sie verklingen,", "tokens": ["Und", "w\u00e4h\u00b7rend", "sie", "ver\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist alles abgethan.", "tokens": ["Ist", "al\u00b7les", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch dr\u00e4ngt auch nur von ferne", "tokens": ["Doch", "dr\u00e4ngt", "auch", "nur", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Ton zu mir sich her,", "tokens": ["Dein", "Ton", "zu", "mir", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Behorch' ich ihn so gerne,", "tokens": ["Be\u00b7horch'", "ich", "ihn", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergess' ich ihn so schwer!", "tokens": ["Ver\u00b7ge\u00b7ss'", "ich", "ihn", "so", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bebe dann, entglimme", "tokens": ["Ich", "be\u00b7be", "dann", ",", "ent\u00b7glim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allzurascher Glut:", "tokens": ["Von", "all\u00b7zu\u00b7ra\u00b7scher", "Glut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz und deine Stimme", "tokens": ["Mein", "Herz", "und", "dei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verstehn sich gar zu gut!", "tokens": ["Ver\u00b7stehn", "sich", "gar", "zu", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}