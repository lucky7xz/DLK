{"dta.poem.18482": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Clelia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Clelie/ wenn sie darff k\u00fcssen/", "tokens": ["Cle\u00b7lie", "/", "wenn", "sie", "darff", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den/ zu welchem sie hat lust/", "tokens": ["Den", "/", "zu", "wel\u00b7chem", "sie", "hat", "lust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "APPR", "PRELS", "PPER", "VAFIN", "VVFIN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "So wil sie das essen missen/", "tokens": ["So", "wil", "sie", "das", "es\u00b7sen", "mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "VVINF", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es soll helffen f\u00fcr die kost/", "tokens": ["Es", "soll", "helf\u00b7fen", "f\u00fcr", "die", "kost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kom zu mix/ ich wil dich speisen/", "tokens": ["Kom", "zu", "mix", "/", "ich", "wil", "dich", "spei\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$(", "PPER", "VMFIN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Speise du denn wieder mich.", "tokens": ["Spei\u00b7se", "du", "denn", "wie\u00b7der", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es wird sich alsdenn schon weisen/", "tokens": ["Es", "wird", "sich", "als\u00b7denn", "schon", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ob es drauf l\u00e4\u00dft fasten sich.", "tokens": ["Ob", "es", "drauf", "l\u00e4\u00dft", "fas\u00b7ten", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}