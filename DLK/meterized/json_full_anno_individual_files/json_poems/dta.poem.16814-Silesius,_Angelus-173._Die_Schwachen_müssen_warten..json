{"dta.poem.16814": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "173.  Die Schwachen m\u00fcssen warten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du armes V\u00f6gelein/ kanstu nicht selber fliegen/", "tokens": ["Du", "ar\u00b7mes", "V\u00f6\u00b7ge\u00b7lein", "/", "kans\u00b7tu", "nicht", "sel\u00b7ber", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibe mit Geduld bi\u00df du meyr krafft hast ligen.", "tokens": ["So", "blei\u00b7be", "mit", "Ge\u00b7duld", "bi\u00df", "du", "meyr", "krafft", "hast", "li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}