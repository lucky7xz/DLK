{"textgrid.poem.33548": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht jenes Zaubernetz, gesponnen", "tokens": ["Nicht", "je\u00b7nes", "Zau\u00b7ber\u00b7netz", ",", "ge\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "PDAT", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deinem sch\u00f6nen Lockenhaar,", "tokens": ["Aus", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Lo\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch nicht dein leuchtend Augenpaar", "tokens": ["Auch", "nicht", "dein", "leuch\u00b7tend", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat so mein Herz f\u00fcr dich gewonnen,", "tokens": ["Hat", "so", "mein", "Herz", "f\u00fcr", "dich", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, eine Sch\u00f6nheit h\u00f6h'rer Art,", "tokens": ["Nein", ",", "ei\u00b7ne", "Sch\u00f6n\u00b7heit", "h\u00f6h'\u00b7rer", "Art", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die immer mehr sich offenbart.", "tokens": ["Die", "im\u00b7mer", "mehr", "sich", "of\u00b7fen\u00b7bart"