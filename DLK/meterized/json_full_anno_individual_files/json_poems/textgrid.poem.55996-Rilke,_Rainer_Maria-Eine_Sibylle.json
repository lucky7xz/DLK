{"textgrid.poem.55996": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Eine Sibylle", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst, vor Zeiten, nannte man sie alt.", "tokens": ["Einst", ",", "vor", "Zei\u00b7ten", ",", "nann\u00b7te", "man", "sie", "alt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "VVFIN", "PIS", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch sie blieb und kam dieselbe Stra\u00dfe", "tokens": ["Doch", "sie", "blieb", "und", "kam", "die\u00b7sel\u00b7be", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "t\u00e4glich. Und man \u00e4nderte die Ma\u00dfe,", "tokens": ["t\u00e4g\u00b7lich", ".", "Und", "man", "\u00e4n\u00b7der\u00b7te", "die", "Ma\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und man z\u00e4hlte sie wie einen Wald", "tokens": ["und", "man", "z\u00e4hl\u00b7te", "sie", "wie", "ei\u00b7nen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "nach Jahrhunderten. Sie aber stand", "tokens": ["nach", "Jahr\u00b7hun\u00b7der\u00b7ten", ".", "Sie", "a\u00b7ber", "stand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "PPER", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "jeden Abend auf derselben Stelle,", "tokens": ["je\u00b7den", "A\u00b7bend", "auf", "der\u00b7sel\u00b7ben", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "schwarz wie eine alte Citadelle", "tokens": ["schwarz", "wie", "ei\u00b7ne", "al\u00b7te", "Ci\u00b7ta\u00b7del\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "hoch und hohl und ausgebrannt;", "tokens": ["hoch", "und", "hohl", "und", "aus\u00b7ge\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "von den Worten, die sich unbewacht", "tokens": ["von", "den", "Wor\u00b7ten", ",", "die", "sich", "un\u00b7be\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wider ihren Willen in ihr mehrten,", "tokens": ["wi\u00b7der", "ih\u00b7ren", "Wil\u00b7len", "in", "ihr", "mehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "immerfort umschrieen und umflogen,", "tokens": ["im\u00b7mer\u00b7fort", "um\u00b7schri\u00b7een", "und", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "w\u00e4hrend die schon wieder heimgekehrten", "tokens": ["w\u00e4h\u00b7rend", "die", "schon", "wie\u00b7der", "heim\u00b7ge\u00b7kehr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "dunkel unter ihren Augenbogen", "tokens": ["dun\u00b7kel", "un\u00b7ter", "ih\u00b7ren", "Au\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "sa\u00dfen, fertig f\u00fcr die Nacht.", "tokens": ["sa\u00b7\u00dfen", ",", "fer\u00b7tig", "f\u00fcr", "die", "Nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einst, vor Zeiten, nannte man sie alt.", "tokens": ["Einst", ",", "vor", "Zei\u00b7ten", ",", "nann\u00b7te", "man", "sie", "alt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "VVFIN", "PIS", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch sie blieb und kam dieselbe Stra\u00dfe", "tokens": ["Doch", "sie", "blieb", "und", "kam", "die\u00b7sel\u00b7be", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "t\u00e4glich. Und man \u00e4nderte die Ma\u00dfe,", "tokens": ["t\u00e4g\u00b7lich", ".", "Und", "man", "\u00e4n\u00b7der\u00b7te", "die", "Ma\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "KON", "PIS", "VVFIN", "ART", "N