{"textgrid.poem.26865": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein bl\u00fchend Kind, von Grazien und Scherzen", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein bl\u00fchend Kind, von Grazien und Scherzen", "tokens": ["Ein", "bl\u00fc\u00b7hend", "Kind", ",", "von", "Gra\u00b7zi\u00b7en", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umh\u00fcpft \u2013 so, Lotte, spielt um dich die Welt,", "tokens": ["Um\u00b7h\u00fcpft", "\u2013", "so", ",", "Lot\u00b7te", ",", "spielt", "um", "dich", "die", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "ADV", "$,", "NE", "$,", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch so, wie sie sich malt in deinem Herzen,", "tokens": ["Doch", "so", ",", "wie", "sie", "sich", "malt", "in", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deiner Seele sch\u00f6nen Spiegel f\u00e4llt,", "tokens": ["In", "dei\u00b7ner", "See\u00b7le", "sch\u00f6\u00b7nen", "Spie\u00b7gel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So ist sie doch nicht. Die Eroberungen,", "tokens": ["So", "ist", "sie", "doch", "nicht", ".", "Die", "Er\u00b7o\u00b7be\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die jeder deiner Blicke siegreich z\u00e4hlt,", "tokens": ["Die", "je\u00b7der", "dei\u00b7ner", "Bli\u00b7cke", "sieg\u00b7reich", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die deine sanfte Seele dir erzwungen,", "tokens": ["Die", "dei\u00b7ne", "sanf\u00b7te", "See\u00b7le", "dir", "er\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Statuen, die \u2013 dein Gef\u00fchl beseelt,", "tokens": ["Die", "Sta\u00b7tu\u00b7en", ",", "die", "\u2013", "dein", "Ge\u00b7f\u00fchl", "be\u00b7seelt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$(", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Herzen, die dein eignes dir errungen,", "tokens": ["Die", "Her\u00b7zen", ",", "die", "dein", "eig\u00b7nes", "dir", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Wunder, die du selbst getan,", "tokens": ["Die", "Wun\u00b7der", ",", "die", "du", "selbst", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Reize, die dein Dasein ihm gegeben,", "tokens": ["Die", "Rei\u00b7ze", ",", "die", "dein", "Da\u00b7sein", "ihm", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Die rechnest du f\u00fcr Sch\u00e4tze diesem Leben,", "tokens": ["Die", "rech\u00b7nest", "du", "f\u00fcr", "Sch\u00e4t\u00b7ze", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "F\u00fcr Tugenden uns Erdenb\u00fcrgern an.", "tokens": ["F\u00fcr", "Tu\u00b7gen\u00b7den", "uns", "Er\u00b7den\u00b7b\u00fcr\u00b7gern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dem holden Zauber nie entweihter Jugend,", "tokens": ["Dem", "hol\u00b7den", "Zau\u00b7ber", "nie", "ent\u00b7weih\u00b7ter", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Engelg\u00fcte m\u00e4chtgem Talisman,", "tokens": ["Der", "En\u00b7gel\u00b7g\u00fc\u00b7te", "m\u00e4cht\u00b7gem", "Ta\u00b7lis\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Majest\u00e4t der Unschuld und der Tugend,", "tokens": ["Der", "Ma\u00b7jes\u00b7t\u00e4t", "der", "Un\u00b7schuld", "und", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Den will ich sehn \u2013 der diesen trotzen kann.", "tokens": ["Den", "will", "ich", "sehn", "\u2013", "der", "die\u00b7sen", "trot\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$(", "ART", "PDAT", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Froh taumelst du im s\u00fc\u00dfen \u00dcberz\u00e4hlen", "tokens": ["Froh", "tau\u00b7melst", "du", "im", "s\u00fc\u00b7\u00dfen", "\u00dc\u00b7berz\u00b7\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Gl\u00fccklichen, die du gemacht, der Seelen,", "tokens": ["Der", "Gl\u00fcck\u00b7li\u00b7chen", ",", "die", "du", "ge\u00b7macht", ",", "der", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die du gewonnen hast, dahin.", "tokens": ["Die", "du", "ge\u00b7won\u00b7nen", "hast", ",", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sei gl\u00fccklich in dem lieblichen Betruge,", "tokens": ["Sei", "gl\u00fcck\u00b7lich", "in", "dem", "lieb\u00b7li\u00b7chen", "Be\u00b7tru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Nie st\u00fcrze von des Traumes stolzem Fluge", "tokens": ["Nie", "st\u00fcr\u00b7ze", "von", "des", "Trau\u00b7mes", "stol\u00b7zem", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ein trauriges Erwachen dich herab.", "tokens": ["Ein", "trau\u00b7ri\u00b7ges", "Er\u00b7wa\u00b7chen", "dich", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Den Blumen gleich, die deine Beete schm\u00fccken,", "tokens": ["Den", "Blu\u00b7men", "gleich", ",", "die", "dei\u00b7ne", "Bee\u00b7te", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "So pflanze sie \u2013 nur den entfernten Blicken,", "tokens": ["So", "pflan\u00b7ze", "sie", "\u2013", "nur", "den", "ent\u00b7fern\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Betrachte sie! \u2013 doch pfl\u00fccke sie nicht ab!", "tokens": ["Be\u00b7trach\u00b7te", "sie", "!", "\u2013", "doch", "pfl\u00fc\u00b7cke", "sie", "nicht", "ab", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punc