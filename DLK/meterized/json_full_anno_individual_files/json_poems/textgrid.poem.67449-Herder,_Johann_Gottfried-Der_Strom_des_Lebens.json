{"textgrid.poem.67449": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Strom des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flie\u00dfe, des Lebens Strom! Du gehst in Wellen vor\u00fcber,", "tokens": ["Flie\u00b7\u00dfe", ",", "des", "Le\u00b7bens", "Strom", "!", "Du", "gehst", "in", "Wel\u00b7len", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wo mit wechselnder H\u00f6h eine die andre begr\u00e4bt.", "tokens": ["Wo", "mit", "wech\u00b7seln\u00b7der", "H\u00f6h", "ei\u00b7ne", "die", "and\u00b7re", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "ART", "PIS", "VVFIN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "M\u00fche folget der M\u00fche; doch kenn' ich s\u00fc\u00dfere Freuden", "tokens": ["M\u00fc\u00b7he", "fol\u00b7get", "der", "M\u00fc\u00b7he", ";", "doch", "kenn'", "ich", "s\u00fc\u00b7\u00dfe\u00b7re", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Als besiegte Gefahr oder vollendete M\u00fch?", "tokens": ["Als", "be\u00b7sieg\u00b7te", "Ge\u00b7fahr", "o\u00b7der", "voll\u00b7en\u00b7de\u00b7te", "M\u00fch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Leben ist Lebens Lohn, Gef\u00fchl sein ewiger Kampfpreis.", "tokens": ["Le\u00b7ben", "ist", "Le\u00b7bens", "Lohn", ",", "Ge\u00b7f\u00fchl", "sein", "e\u00b7wi\u00b7ger", "Kampf\u00b7preis", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "NN", "$,", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Flie\u00dfe, wogiger Strom! nirgend ein stehender Sumpf.", "tokens": ["Flie\u00b7\u00dfe", ",", "wo\u00b7gi\u00b7ger", "Strom", "!", "nir\u00b7gend", "ein", "ste\u00b7hen\u00b7der", "Sumpf", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Flie\u00dfe, des Lebens Strom! Du gehst in Wellen vor\u00fcber,", "tokens": ["Flie\u00b7\u00dfe", ",", "des", "Le\u00b7bens", "Strom", "!", "Du", "gehst", "in", "Wel\u00b7len", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wo mit wechselnder H\u00f6h eine die andre begr\u00e4bt.", "tokens": ["Wo", "mit", "wech\u00b7seln\u00b7der", "H\u00f6h", "ei\u00b7ne", "die", "and\u00b7re", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "ART", "PIS", "VVFIN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "M\u00fche folget der M\u00fche; doch kenn'