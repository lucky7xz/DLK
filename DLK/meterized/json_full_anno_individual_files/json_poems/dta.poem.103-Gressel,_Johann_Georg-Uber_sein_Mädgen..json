{"dta.poem.103": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber sein M\u00e4dgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4dgen ist recht sch\u00f6n;", "tokens": ["Mein", "M\u00e4d\u00b7gen", "ist", "recht", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ich die Lippen k\u00fcsse/", "tokens": ["Wenn", "ich", "die", "Lip\u00b7pen", "k\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kan ich nicht gnug erh\u00f6hn", "tokens": ["Kan", "ich", "nicht", "gnug", "er\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Lust/ so ich geniesse.", "tokens": ["Die", "Lust", "/", "so", "ich", "ge\u00b7nies\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich die weisse Brust", "tokens": ["Wenn", "ich", "die", "weis\u00b7se", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "An meine Lippen dr\u00fccke/", "tokens": ["An", "mei\u00b7ne", "Lip\u00b7pen", "dr\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Alsdenn mit G\u00f6tter Lust", "tokens": ["Als\u00b7denn", "mit", "G\u00f6t\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ich meinen Geist erquicke.", "tokens": ["Ich", "mei\u00b7nen", "Geist", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ja! wenn denn seine Gunst", "tokens": ["Ja", "!", "wenn", "denn", "sei\u00b7ne", "Gunst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mir noch was bessers schencket/", "tokens": ["Mir", "noch", "was", "bes\u00b7sers", "schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PWS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So brauchts die gr\u00f6ste Kunst", "tokens": ["So", "brauchts", "die", "gr\u00f6s\u00b7te", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Da\u00df er zur\u00fccke dencket.", "tokens": ["Da\u00df", "er", "zu\u00b7r\u00fc\u00b7cke", "den\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}