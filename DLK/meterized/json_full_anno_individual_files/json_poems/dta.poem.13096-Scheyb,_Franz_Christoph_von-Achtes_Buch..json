{"dta.poem.13096": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie Wercke meiner Kunst seynd mahlen, schnizen, bauen,", "tokens": ["\u201e", "die", "Wer\u00b7cke", "mei\u00b7ner", "Kunst", "seynd", "mah\u00b7len", ",", "schni\u00b7zen", ",", "bau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "VAFIN", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edie Nachwelt kann darinn der Vorwelt Thaten schauen;", "tokens": ["\u201e", "die", "Nach\u00b7welt", "kann", "da\u00b7rinn", "der", "Vor\u00b7welt", "Tha\u00b7ten", "schau\u00b7en", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eda meine Schwester nur Red-lose Bl\u00e4tter schreibt,", "tokens": ["\u201e", "da", "mei\u00b7ne", "Schwes\u00b7ter", "nur", "Re\u00b7dlo\u00b7se", "Bl\u00e4t\u00b7ter", "schreibt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewodurch der Nachwelt nichts zu lernen \u00fcberbleibt;", "tokens": ["\u201e", "wo\u00b7durch", "der", "Nach\u00b7welt", "nichts", "zu", "ler\u00b7nen", "\u00fc\u00b7berb\u00b7leibt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "315\u201dWeil an dem Innhalt nichts als stumme Farben kleben,", "tokens": ["\"", "Weil", "an", "dem", "Inn\u00b7halt", "nichts", "als", "stum\u00b7me", "Far\u00b7ben", "kle\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "APPR", "ART", "NN", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edie durch das bunte Licht ein Schatten-Spiel erheben.", "tokens": ["\u201e", "die", "durch", "das", "bun\u00b7te", "Licht", "ein", "Schat\u00b7ten\u00b7Spiel", "er\u00b7he\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewie tr\u00fcge sie das Recht, die List, den Meineyd vor?", "tokens": ["\u201e", "wie", "tr\u00fc\u00b7ge", "sie", "das", "Recht", ",", "die", "List", ",", "den", "Mei\u00b7neyd", "vor", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewie mahlte sie, wie man dort Staat und Land verlohr?", "tokens": ["\u201e", "wie", "mahl\u00b7te", "sie", ",", "wie", "man", "dort", "Staat", "und", "Land", "ver\u00b7lohr", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$,", "PWAV", "PIS", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewie der mit Freundschaft prahlt, wann er den Ha\u00df verh\u00f6hlet?", "tokens": ["\u201e", "wie", "der", "mit", "Freund\u00b7schaft", "prahlt", ",", "wann", "er", "den", "Ha\u00df", "ver\u00b7h\u00f6h\u00b7let", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "APPR", "NN", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "320\u201dWie der zu seiner Hilff nicht Menschen, GOtt erw\u00e4hlet?", "tokens": ["\"", "Wie", "der", "zu", "sei\u00b7ner", "Hilff", "nicht", "Men\u00b7schen", ",", "Gott", "er\u00b7w\u00e4h\u00b7let", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "APPR", "PPOSAT", "NN", "PTKNEG", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewas unserm Aug entgeht, entwirfft der Pensel nicht.", "tokens": ["\u201e", "was", "un\u00b7serm", "Aug", "ent\u00b7geht", ",", "ent\u00b7wirfft", "der", "Pen\u00b7sel", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewas man nicht sehen kann, ist weder grau noch licht.", "tokens": ["\u201e", "was", "man", "nicht", "se\u00b7hen", "kann", ",", "ist", "we\u00b7der", "grau", "noch", "licht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,", "VAFIN", "KON", "ADJD", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eso wi\u00dft ihr wie die Kunst der Farben eingeschr\u00e4ncket,", "tokens": ["\u201e", "so", "wi\u00dft", "ihr", "wie", "die", "Kunst", "der", "Far\u00b7ben", "ein\u00b7ge\u00b7schr\u00e4n\u00b7cket", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ewogegen mein Gesang erz\u00e4hlt, was man gedencket.", "tokens": ["\u201e", "wo\u00b7ge\u00b7gen", "mein", "Ge\u00b7sang", "er\u00b7z\u00e4hlt", ",", "was", "man", "ge\u00b7den\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}