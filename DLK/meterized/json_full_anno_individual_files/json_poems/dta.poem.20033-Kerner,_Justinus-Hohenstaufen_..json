{"dta.poem.20033": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Hohenstaufen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es steht in stiller D\u00e4mmerung", "tokens": ["Es", "steht", "in", "stil\u00b7ler", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der alte Fels', \u00f6d' und beraubt;", "tokens": ["Der", "al\u00b7te", "Fels'", ",", "\u00f6d'", "und", "be\u00b7raubt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachtvogel kreist in tr\u00e4gem Schwung", "tokens": ["Nacht\u00b7vo\u00b7gel", "kreist", "in", "tr\u00e4\u00b7gem", "Schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wehklagend um sein moosig Haupt.", "tokens": ["Weh\u00b7kla\u00b7gend", "um", "sein", "moo\u00b7sig", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wie der Mond aus Wolken bricht,", "tokens": ["Doch", "wie", "der", "Mond", "aus", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit der Sterne klares Heer,", "tokens": ["Und", "mit", "der", "Ster\u00b7ne", "kla\u00b7res", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umstr\u00f6mt den Fels' ein seltsam Licht,", "tokens": ["Um\u00b7str\u00f6mt", "den", "Fels'", "ein", "selt\u00b7sam", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Draus bilden sich Gestalten hehr.", "tokens": ["Draus", "bil\u00b7den", "sich", "Ge\u00b7stal\u00b7ten", "hehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die alte Burg mit Thurm und Thor", "tokens": ["Die", "al\u00b7te", "Burg", "mit", "Thurm", "und", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbauet sich aus Wolken klar,", "tokens": ["Er\u00b7bau\u00b7et", "sich", "aus", "Wol\u00b7ken", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die alte Linde spro\u00dft empor,", "tokens": ["Die", "al\u00b7te", "Lin\u00b7de", "spro\u00dft", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles wird, wie's vormals war.", "tokens": ["Und", "al\u00b7les", "wird", ",", "wie's", "vor\u00b7mals", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "VVFIN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So Harfe wie Trompetensto\u00df", "tokens": ["So", "Har\u00b7fe", "wie", "Trom\u00b7pe\u00b7ten\u00b7sto\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KOKOM", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ert\u00f6nt hinab in's gr\u00fcne Thal,", "tokens": ["Er\u00b7t\u00f6nt", "hin\u00b7ab", "in's", "gr\u00fc\u00b7ne", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gezogen kommt auf schwarzem Ro\u00df", "tokens": ["Ge\u00b7zo\u00b7gen", "kommt", "auf", "schwar\u00b7zem", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rothbart' der Held, gekleid't in Stahl.", "tokens": ["Ro\u00b7th\u00b7bart'", "der", "Held", ",", "ge\u00b7kleid't", "in", "Stahl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und Philipp und Irene traut,", "tokens": ["Und", "Phi\u00b7lipp", "und", "I\u00b7re\u00b7ne", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sie wall'n zur Linde Hand in Hand;", "tokens": ["Sie", "wall'n", "zur", "Lin\u00b7de", "Hand", "in", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NE", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Vogel singt mit s\u00fc\u00dfem Laut", "tokens": ["Ein", "Vo\u00b7gel", "singt", "mit", "s\u00fc\u00b7\u00dfem", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom sch\u00f6nen griech'schen Heimat-Land.", "tokens": ["Vom", "sch\u00f6\u00b7nen", "griech'\u00b7schen", "Hei\u00b7mat\u00b7Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und Konradin, an Tugend reich,", "tokens": ["Und", "Kon\u00b7ra\u00b7din", ",", "an", "Tu\u00b7gend", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der s\u00fc\u00dfe J\u00fcngling arm, beraubt,", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "J\u00fcng\u00b7ling", "arm", ",", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Garten steht er stumm und bleich:", "tokens": ["Im", "Gar\u00b7ten", "steht", "er", "stumm", "und", "bleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lilie neigt ihr traurend Haupt.", "tokens": ["Die", "Li\u00b7lie", "neigt", "ihr", "trau\u00b7rend", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch jezt verk\u00fcnd't aus dunklem Thal", "tokens": ["Doch", "jezt", "ver\u00b7k\u00fcnd't", "aus", "dunk\u00b7lem", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den bleichen Tag der rothe Hahn,", "tokens": ["Den", "blei\u00b7chen", "Tag", "der", "ro\u00b7the", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da steht der Fels gar \u00f6d' und kahl,", "tokens": ["Da", "steht", "der", "Fels", "gar", "\u00f6d'", "und", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwunden ist die Burg fortan.", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "die", "Burg", "for\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "An ihrer St\u00e4tt' ein Dornbusch steht,", "tokens": ["An", "ih\u00b7rer", "St\u00e4tt'", "ein", "Dorn\u00b7busch", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kalt weht der Morgen auf den H\u00f6h'n, \u2014", "tokens": ["Kalt", "weht", "der", "Mor\u00b7gen", "auf", "den", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie der Fels, so kalt und \u00f6d'", "tokens": ["Und", "wie", "der", "Fels", ",", "so", "kalt", "und", "\u00f6d'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ADV", "ADJD", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint rings das deutsche Land zu steh'n.", "tokens": ["Scheint", "rings", "das", "deut\u00b7sche", "Land", "zu", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}