{"textgrid.poem.44519": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Stabat mater", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wohl, es ward euch dargebracht,", "tokens": ["Nun", "wohl", ",", "es", "ward", "euch", "dar\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr habt es nicht erkannt;", "tokens": ["Ihr", "habt", "es", "nicht", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In all der Tonkunst Zaubermacht,", "tokens": ["In", "all", "der", "Ton\u00b7kunst", "Zau\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In des Gef\u00fchles Farbenpracht,", "tokens": ["In", "des", "Ge\u00b7f\u00fch\u00b7les", "Far\u00b7ben\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr wiest es von der Hand.", "tokens": ["Ihr", "wiest", "es", "von", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihr jauchztet wenigstens nicht laut,", "tokens": ["Ihr", "jauchz\u00b7tet", "we\u00b7nigs\u00b7tens", "nicht", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df in der Zeiten Sand,", "tokens": ["Da\u00df", "in", "der", "Zei\u00b7ten", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der d\u00fcrre Kr\u00e4uter sp\u00e4rlich tr\u00e4gt,", "tokens": ["Der", "d\u00fcr\u00b7re", "Kr\u00e4u\u00b7ter", "sp\u00e4r\u00b7lich", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von Zweifelsdornen eingehegt,", "tokens": ["Von", "Zwei\u00b7fels\u00b7dor\u00b7nen", "ein\u00b7ge\u00b7hegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Rose euch entstand,", "tokens": ["Die", "Ro\u00b7se", "euch", "ent\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die dasteht mit gesenktem Haupt,", "tokens": ["Die", "das\u00b7teht", "mit", "ge\u00b7senk\u00b7tem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Euch bittend: \u00bbSeht mich an und glaubt,", "tokens": ["Euch", "bit\u00b7tend", ":", "\u00bb", "Seht", "mich", "an", "und", "glaubt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$.", "$(", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Verge\u00dft f\u00fcr einen Augenblick", "tokens": ["Ver\u00b7ge\u00dft", "f\u00fcr", "ei\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Euch selbst in des Genusses Gl\u00fcck!\u00ab", "tokens": ["Euch", "selbst", "in", "des", "Ge\u00b7nus\u00b7ses", "Gl\u00fcck", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ihr aber wieset es zur\u00fcck.", "tokens": ["Ihr", "a\u00b7ber", "wie\u00b7set", "es", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Was liegt daran! Das Werk besteht,", "tokens": ["Was", "liegt", "da\u00b7ran", "!", "Das", "Werk", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und euer sp\u00e4ter Enkelsohn", "tokens": ["Und", "eu\u00b7er", "sp\u00e4\u00b7ter", "En\u00b7kel\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zahlt einst die Schuld des Vaters schon,", "tokens": ["Zahlt", "einst", "die", "Schuld", "des", "Va\u00b7ters", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ihr f\u00fcr eure V\u00e4ter steht,", "tokens": ["Wie", "ihr", "f\u00fcr", "eu\u00b7re", "V\u00e4\u00b7ter", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Mozarts \u00bbDon Juan\u00ab verschm\u00e4ht.", "tokens": ["Die", "Mo\u00b7zarts", "\u00bb", "Don", "Juan", "\u00ab", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "$(", "NE", "NE", "$(", "VVPP", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Den Meister aber k\u00fcmmerts nicht.", "tokens": ["Den", "Meis\u00b7ter", "a\u00b7ber", "k\u00fcm\u00b7merts", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er kennt die Welt. Mir d\u00e4ucht, er spricht:", "tokens": ["Er", "kennt", "die", "Welt", ".", "Mir", "d\u00e4ucht", ",", "er", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwenn sie mit den Augen h\u00f6rt,", "tokens": ["\u00bb", "wenn", "sie", "mit", "den", "Au\u00b7gen", "h\u00f6rt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Mit den Ohren sieht,", "tokens": ["Mit", "den", "Oh\u00b7ren", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Mit dem Kopfe f\u00fchlt,", "tokens": ["Mit", "dem", "Kop\u00b7fe", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und dem Gef\u00fchle denkt,", "tokens": ["Und", "dem", "Ge\u00b7f\u00fch\u00b7le", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ist sie nicht wert, da\u00df man sich kr\u00e4nkt.\u00ab", "tokens": ["Ist", "sie", "nicht", "wert", ",", "da\u00df", "man", "sich", "kr\u00e4nkt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "PIS", "PRF", "VVFIN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Eins aber ging verloren, eins!", "tokens": ["Eins", "a\u00b7ber", "ging", "ver\u00b7lo\u00b7ren", ",", "eins", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "VVPP", "$,", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unschuld Gl\u00fcck, o \u00d6streich, deins.", "tokens": ["Der", "Un\u00b7schuld", "Gl\u00fcck", ",", "o", "\u00d6s\u00b7treich", ",", "deins", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "FM", "ADJD", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Deutschlands kalter Nebelnacht,", "tokens": ["In", "Deutschlands", "kal\u00b7ter", "Ne\u00b7bel\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo kaum ein Sonnenstrahl mehr lacht,", "tokens": ["Wo", "kaum", "ein", "Son\u00b7nen\u00b7strahl", "mehr", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Irrwische leuchten, fauler Dunst,", "tokens": ["Irr\u00b7wi\u00b7sche", "leuch\u00b7ten", ",", "fau\u00b7ler", "Dunst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit der Natur einschlief die Kunst,", "tokens": ["Mit", "der", "Na\u00b7tur", "ein\u00b7schlief", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lagst du oasen\u00e4hnlich da,", "tokens": ["Lagst", "du", "o\u00b7a\u00b7sen\u00b7\u00e4hn\u00b7lich", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr den, der be\u00dfre Zeiten sah.", "tokens": ["F\u00fcr", "den", ",", "der", "be\u00df\u00b7re", "Zei\u00b7ten", "sah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein lauer Hauch ging durch die Luft,", "tokens": ["Ein", "lau\u00b7er", "Hauch", "ging", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durchw\u00fcrzt von kleiner Veilchen Duft,", "tokens": ["Durch\u00b7w\u00fcrzt", "von", "klei\u00b7ner", "Veil\u00b7chen", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die B\u00e4ume standen hoch und frisch,", "tokens": ["Die", "B\u00e4u\u00b7me", "stan\u00b7den", "hoch", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von Licht und Schatten ein Gemisch;", "tokens": ["Von", "Licht", "und", "Schat\u00b7ten", "ein", "Ge\u00b7misch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wenn dein Wissen minder reich,", "tokens": ["Und", "wenn", "dein", "Wis\u00b7sen", "min\u00b7der", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Was wahr, teilt Gott an alle gleich;", "tokens": ["Was", "wahr", ",", "teilt", "Gott", "an", "al\u00b7le", "gleich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VVFIN", "NN", "APPR", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Drum gabs in deinen T\u00e4lern Schall,", "tokens": ["Drum", "gabs", "in", "dei\u00b7nen", "T\u00e4\u00b7lern", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Es klang das Lied der Nachtigall,", "tokens": ["Es", "klang", "das", "Lied", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Indes an deiner Grenze Saum", "tokens": ["In\u00b7des", "an", "dei\u00b7ner", "Gren\u00b7ze", "Saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der heisre Sperling zwitschert kaum,", "tokens": ["Der", "heis\u00b7re", "Sper\u00b7ling", "zwit\u00b7schert", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und Papageien sinnentfernt", "tokens": ["Und", "Pa\u00b7pa\u00b7gei\u00b7en", "sin\u00b7nent\u00b7fernt"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nachplappern, was sie eingelernt.", "tokens": ["Nach\u00b7plap\u00b7pern", ",", "was", "sie", "ein\u00b7ge\u00b7lernt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Allein die Gletscher schreiten fort,", "tokens": ["Al\u00b7lein", "die", "Glet\u00b7scher", "schrei\u00b7ten", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Es w\u00e4chst das Eis von Ort zu Ort,", "tokens": ["Es", "w\u00e4chst", "das", "Eis", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und der Pedant, ein rauher Nord,", "tokens": ["Und", "der", "Pe\u00b7dant", ",", "ein", "rau\u00b7her", "Nord", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Er bl\u00e4st dich an mit seinem Wort.", "tokens": ["Er", "bl\u00e4st", "dich", "an", "mit", "sei\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was liegt daran! Das Wort vergeht,", "tokens": ["Was", "liegt", "da\u00b7ran", "!", "Das", "Wort", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kunst, der Mensch, die Welt besteht.", "tokens": ["Die", "Kunst", ",", "der", "Mensch", ",", "die", "Welt", "be\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch wenn, nicht mehr wie sonst geneigt,", "tokens": ["Doch", "wenn", ",", "nicht", "mehr", "wie", "sonst", "ge\u00b7neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PTKNEG", "ADV", "KOKOM", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Lied dir, gleich den Nachbarn, schweigt,", "tokens": ["Das", "Lied", "dir", ",", "gleich", "den", "Nach\u00b7barn", ",", "schweigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", 