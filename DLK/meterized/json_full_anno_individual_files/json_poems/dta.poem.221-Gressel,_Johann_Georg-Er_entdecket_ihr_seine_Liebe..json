{"dta.poem.221": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er entdecket ihr seine Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Der Augen sch\u00f6ne Pracht/", "tokens": ["Der", "Au\u00b7gen", "sch\u00f6\u00b7ne", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die alle Sch\u00f6nheit \u00fcbersteigt/", "tokens": ["Die", "al\u00b7le", "Sch\u00f6n\u00b7heit", "\u00fc\u00b7bers\u00b7teigt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat mich verliebt gemacht/", "tokens": ["Hat", "mich", "ver\u00b7liebt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df sich mein Hertze neigt", "tokens": ["Da\u00df", "sich", "mein", "Hert\u00b7ze", "neigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "VVFIN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu deinem Fu\u00df", "tokens": ["Zu", "dei\u00b7nem", "Fu\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Um einen Ku\u00df/", "tokens": ["Um", "ei\u00b7nen", "Ku\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und Blick/", "tokens": ["Und", "Blick", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Kriegt den mein Hertz/", "tokens": ["Kriegt", "den", "mein", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSAT", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "So weicht der Schmertz/", "tokens": ["So", "weicht", "der", "Schmertz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und wird mein bestes Gl\u00fcck.", "tokens": ["Und", "wird", "mein", "bes\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es gleichet dein Gesicht", "tokens": ["Es", "glei\u00b7chet", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Himmels-Glantz und hellem Schein/", "tokens": ["Des", "Him\u00b7mels\u00b7Glantz", "und", "hel\u00b7lem", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein angenehmes Licht", "tokens": ["Sein", "an\u00b7ge\u00b7neh\u00b7mes", "Licht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kan nicht so sch\u00f6ne sey;", "tokens": ["Kan", "nicht", "so", "sch\u00f6\u00b7ne", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein sch\u00f6ner Hals", "tokens": ["Dein", "sch\u00f6\u00b7ner", "Hals"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ist ebenfals", "tokens": ["Ist", "e\u00b7ben\u00b7fals"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+--", "measure": "dactylic.init"}, "line.7": {"text": "Crystall:", "tokens": ["Crys\u00b7tall", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der Mund-Rubin", "tokens": ["Der", "Mun\u00b7dRu\u00b7bin"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da Rosen bl\u00fchn", "tokens": ["Da", "Ro\u00b7sen", "bl\u00fchn"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Besetzet mit Corall.", "tokens": ["Be\u00b7set\u00b7zet", "mit", "Co\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der rund-gew\u00f6lbten Brust/", "tokens": ["Der", "run\u00b7dge\u00b7w\u00f6lb\u00b7ten", "Brust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die lauter s\u00fcsse Anmuth hegt/", "tokens": ["Die", "lau\u00b7ter", "s\u00fcs\u00b7se", "An\u00b7muth", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin die holde Lust", "tokens": ["Wo\u00b7hin", "die", "hol\u00b7de", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Wohn-Haus legt/", "tokens": ["Der", "Lie\u00b7be", "Wohn\u00b7Haus", "legt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Vor ihr erbleicht", "tokens": ["Vor", "ihr", "er\u00b7bleicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Schnee/", "tokens": ["Der", "Schnee", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der Schnecken Blut", "tokens": ["Der", "Schne\u00b7cken", "Blut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Weicht vor der Gluth/", "tokens": ["Weicht", "vor", "der", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Die brennt auf dieser H\u00f6h.", "tokens": ["Die", "brennt", "auf", "die\u00b7ser", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wirf einen Gnaden-Blick", "tokens": ["Wirf", "ei\u00b7nen", "Gna\u00b7den\u00b7Blick"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein sch\u00f6nster Schatz/ mein ander ich/", "tokens": ["Mein", "sch\u00f6ns\u00b7ter", "Schatz", "/", "mein", "an\u00b7der", "ich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJD", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf deinem Knecht zur\u00fcck/", "tokens": ["Auf", "dei\u00b7nem", "Knecht", "zu\u00b7r\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Kind erquicke mich.", "tokens": ["Mein", "Kind", "er\u00b7qui\u00b7cke", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entrei\u00df mich doch", "tokens": ["Ent\u00b7rei\u00df", "mich", "doch"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den schweren Joch", "tokens": ["Den", "schwe\u00b7ren", "Joch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Pein/", "tokens": ["Der", "Pein", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Da\u00df nach dem Streit", "tokens": ["Da\u00df", "nach", "dem", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Der Traurigkeit", "tokens": ["Der", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Ich kan ein Sieger seyn.", "tokens": ["Ich", "kan", "ein", "Sie\u00b7ger", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}