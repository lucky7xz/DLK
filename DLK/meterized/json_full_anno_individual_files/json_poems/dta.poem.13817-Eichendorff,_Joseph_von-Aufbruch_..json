{"dta.poem.13817": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Aufbruch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewettern gleich bl\u00fcht Kampfes-F\u00fclle,", "tokens": ["\u201e", "wet\u00b7tern", "gleich", "bl\u00fcht", "Kamp\u00b7fes\u00b7F\u00fcl\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitze zieht das gute Schwerdt,", "tokens": ["Blit\u00b7ze", "zieht", "das", "gu\u00b7te", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher wird auf ewig stille \u2014", "tokens": ["Man\u00b7cher", "wird", "auf", "e\u00b7wig", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr Gott, es gescheh' Dein Wille!", "tokens": ["Herr", "Gott", ",", "es", "ge\u00b7scheh'", "Dein", "Wil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Blast Trompeten! Frisch mein Pferd!\u201c", "tokens": ["Blast", "Trom\u00b7pe\u00b7ten", "!", "Frisch", "mein", "Pferd", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "$.", "NE", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}