{"dta.poem.18598": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "G rabschrifft eines ungl\u00fcckse-  \n ligen Liebhabers der verbrandt  \n wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst war es meine Lust in heisser Gluth zu liegen.", "tokens": ["Sonst", "war", "es", "mei\u00b7ne", "Lust", "in", "heis\u00b7ser", "Gluth", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Flammen musten stets mein Freuden Feuer seyn.", "tokens": ["Die", "Flam\u00b7men", "mus\u00b7ten", "stets", "mein", "Freu\u00b7den", "Feu\u00b7er", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch da ich durch die Gluth auch soll mein Urtheil kriegen/", "tokens": ["Doch", "da", "ich", "durch", "die", "Gluth", "auch", "soll", "mein", "Ur\u00b7theil", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00fcnscht ich: W\u00e4r ich doch von kalten Ei\u00df und Stein.", "tokens": ["So", "w\u00fcnscht", "ich", ":", "W\u00e4r", "ich", "doch", "von", "kal\u00b7ten", "Ei\u00df", "und", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn Statt das ich in Brand von Stroh und Holtz mu\u00df lauffen", "tokens": ["Denn", "Statt", "das", "ich", "in", "Brand", "von", "Stroh", "und", "Holtz", "mu\u00df", "lauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "PPER", "APPR", "NN", "APPR", "NN", "KON", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "War sonst ein sch\u00f6ner Schoo\u00df daf\u00fcr mein Scheiter-Hauffen.", "tokens": ["War", "sonst", "ein", "sch\u00f6\u00b7ner", "Schoo\u00df", "da\u00b7f\u00fcr", "mein", "Schei\u00b7ter\u00b7Hauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}