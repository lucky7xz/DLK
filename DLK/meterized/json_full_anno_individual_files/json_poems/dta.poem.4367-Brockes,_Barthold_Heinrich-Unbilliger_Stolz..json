{"dta.poem.4367": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unbilliger Stolz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du willt ein g\u00f6ttlich Wesen leugnen, weil, wie in", "tokens": ["Du", "willt", "ein", "g\u00f6tt\u00b7lich", "We\u00b7sen", "leug\u00b7nen", ",", "weil", ",", "wie", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "NN", "VVINF", "$,", "KOUS", "$,", "PWAV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seinem Regimente", "tokens": ["Sei\u00b7nem", "Re\u00b7gi\u00b7men\u00b7te"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das B\u00f6se doch bestehen k\u00f6nnte,", "tokens": ["Das", "B\u00f6\u00b7se", "doch", "be\u00b7ste\u00b7hen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Geist nicht zu begreifen f\u00e4hig. Hei\u00dft dieses schliessen?", "tokens": ["Dein", "Geist", "nicht", "zu", "be\u00b7grei\u00b7fen", "f\u00e4\u00b7hig", ".", "Hei\u00dft", "die\u00b7ses", "schlies\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "ADJD", "$.", "VVFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+----+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Geht die\u00df an?", "tokens": ["Geht", "die\u00df", "an", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "\u201cdie Sache kann unm\u00f6glich seyn, dieweil ich sie nicht", "tokens": ["\u201c", "die", "Sa\u00b7che", "kann", "un\u00b7m\u00f6g\u00b7lich", "seyn", ",", "die\u00b7weil", "ich", "sie", "nicht"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VMFIN", "ADJD", "VAINF", "$,", "KOUS", "PPER", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "sehen kann.", "tokens": ["se\u00b7hen", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Auf welchem Schlu\u00df sich jedennoch (sprich selbst, ob sichs", "tokens": ["Auf", "wel\u00b7chem", "Schlu\u00df", "sich", "je\u00b7den\u00b7noch", "(", "sprich", "selbst", ",", "ob", "sichs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PRF", "ADV", "$(", "ADV", "ADV", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "nicht so befindet?)", "tokens": ["nicht", "so", "be\u00b7fin\u00b7det", "?", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Dein ganzer Zweifel, alles Zanken, und all dein Disputiren", "tokens": ["Dein", "gan\u00b7zer", "Zwei\u00b7fel", ",", "al\u00b7les", "Zan\u00b7ken", ",", "und", "all", "dein", "Dis\u00b7pu\u00b7ti\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PIAT", "NN", "$,", "KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "gr\u00fcndet.", "tokens": ["gr\u00fcn\u00b7det", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}