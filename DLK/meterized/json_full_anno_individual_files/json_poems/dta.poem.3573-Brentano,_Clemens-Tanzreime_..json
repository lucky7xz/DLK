{"dta.poem.3573": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein M\u00e4del hier,               ", "tokens": ["Es", "ist", "ein", "M\u00e4\u00b7del", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es hat ein Gulden vier;", "tokens": ["Es", "hat", "ein", "Gul\u00b7den", "vier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat ein spitzigs M\u00e4ule,", "tokens": ["Hat", "ein", "spit\u00b7zigs", "M\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ein N\u00e4sle als wie ein S\u00e4ule;", "tokens": ["Ein", "N\u00e4s\u00b7le", "als", "wie", "ein", "S\u00e4u\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zwey Augen als wie ein Stier,", "tokens": ["Zwey", "Au\u00b7gen", "als", "wie", "ein", "Stier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Trotz allen M\u00e4dchen hier.", "tokens": ["Trotz", "al\u00b7len", "M\u00e4d\u00b7chen", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt ist mein Liedel aus,", "tokens": ["Jetzt", "ist", "mein", "Lie\u00b7del", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es bei\u00dft mich noch eine Laus;", "tokens": ["Es", "bei\u00dft", "mich", "noch", "ei\u00b7ne", "Laus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bin so keck und nehm sie,", "tokens": ["Ich", "bin", "so", "keck", "und", "nehm", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nehm ein Messer und schind sie;", "tokens": ["Und", "nehm", "ein", "Mes\u00b7ser", "und", "schind", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und stech ihr beide Augen aus,", "tokens": ["Und", "stech", "ihr", "bei\u00b7de", "Au\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt hasts meine liebe Laus.", "tokens": ["Jetzt", "hasts", "mei\u00b7ne", "lie\u00b7be", "Laus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}