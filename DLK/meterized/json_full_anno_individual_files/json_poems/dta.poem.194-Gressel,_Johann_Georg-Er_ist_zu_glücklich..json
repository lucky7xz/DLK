{"dta.poem.194": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er ist zu gl\u00fccklich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fccke ist mir mehr/ als ich verdient/ gewogen/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "ist", "mir", "mehr", "/", "als", "ich", "ver\u00b7dient", "/", "ge\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "VVPP", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es blickt sein holdes Aug mich \u00fcberm\u00e4ssig an/", "tokens": ["Es", "blickt", "sein", "hol\u00b7des", "Aug", "mich", "\u00fc\u00b7berm\u00b7\u00e4s\u00b7sig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es leitet seine Hand mich auf die Nelcken-Bahn/", "tokens": ["Es", "lei\u00b7tet", "sei\u00b7ne", "Hand", "mich", "auf", "die", "Nel\u00b7cken\u00b7Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch hat mir seine Gunst im Lieben nichts entzogen.", "tokens": ["Noch", "hat", "mir", "sei\u00b7ne", "Gunst", "im", "Lie\u00b7ben", "nichts", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sterne sind mir hold am blauen Himmels-Bogen/", "tokens": ["Die", "Ster\u00b7ne", "sind", "mir", "hold", "am", "blau\u00b7en", "Him\u00b7mels\u00b7Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr Einflu\u00df hat mir Guts im Uberflu\u00df gethan;", "tokens": ["Ihr", "Ein\u00b7flu\u00df", "hat", "mir", "Guts", "im", "U\u00b7ber\u00b7flu\u00df", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Stellen auch so gar die Hertzen fangen kan/", "tokens": ["Mein", "Stel\u00b7len", "auch", "so", "gar", "die", "Hert\u00b7zen", "fan\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dadurch ich ", "tokens": ["Da\u00b7durch", "ich"], "token_info": ["word", "word"], "pos": ["PAV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Jm Lieben kan ich mich vor andern gl\u00fccklich nennen/", "tokens": ["Jm", "Lie\u00b7ben", "kan", "ich", "mich", "vor", "an\u00b7dern", "gl\u00fcck\u00b7lich", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "PRF", "APPR", "PIS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn wenn ein ander sich mit leerem Dunst mu\u00df heucheln/", "tokens": ["Denn", "wenn", "ein", "an\u00b7der", "sich", "mit", "lee\u00b7rem", "Dunst", "mu\u00df", "heu\u00b7cheln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "PRF", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So kan mein Hertze sich mit wahrer Liebe schmeicheln/", "tokens": ["So", "kan", "mein", "Hert\u00b7ze", "sich", "mit", "wah\u00b7rer", "Lie\u00b7be", "schmei\u00b7cheln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jocaste ist mir hold/ die Lesbia mich liebt/", "tokens": ["Jo\u00b7cas\u00b7te", "ist", "mir", "hold", "/", "die", "Les\u00b7bia", "mich", "liebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$(", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Doch wie bey heiterm Schein es finster Wolcken giebt/", "tokens": ["Doch", "wie", "bey", "hei\u00b7term", "Schein", "es", "fins\u00b7ter", "Wol\u00b7cken", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So kan mein Hertze auch bey aller Brunst nicht brennen.", "tokens": ["So", "kan", "mein", "Hert\u00b7ze", "auch", "bey", "al\u00b7ler", "Brunst", "nicht", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "VVFIN", "ADV", "APPR", "PIAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}