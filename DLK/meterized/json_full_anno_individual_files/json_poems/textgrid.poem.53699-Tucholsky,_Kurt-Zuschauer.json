"tokens": ["Das", "macht", ":", "Es", "boxt", "doch", "heu\u00b7te", "Brei\u00b7ten\u00b7str\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Na Mensch, wenn du das nicht gesehen hast!", "tokens": ["Na", "Mensch", ",", "wenn", "du", "das", "nicht", "ge\u00b7se\u00b7hen", "hast", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "KOUS", "PPER", "PDS", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Frauen schaun verz\u00fcckt ein Suspensorium.", "tokens": ["Die", "Frau\u00b7en", "schaun", "ver\u00b7z\u00fcckt", "ein", "Sus\u00b7pen\u00b7so\u00b7ri\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die M\u00e4nner boxen nicht. So sehn sie aus \u2013!", "tokens": ["Die", "M\u00e4n\u00b7ner", "bo\u00b7xen", "nicht", ".", "So", "sehn", "sie", "aus", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein paar acteurs. Ein Riesen-Auditorium.", "tokens": ["Ein", "paar", "ac\u00b7teurs", ".", "Ein", "Rie\u00b7sen\u00b7Au\u00b7di\u00b7to\u00b7ri\u00b7um", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie sehn nur zu. Tun nichts. Und gehn nach Haus.", "t