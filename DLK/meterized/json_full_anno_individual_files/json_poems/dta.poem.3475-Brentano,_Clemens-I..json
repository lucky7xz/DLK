{"dta.poem.3475": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Schatz willst du schlafen gehn,               ", "tokens": ["Ach", "Schatz", "willst", "du", "schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafe wohl, schlafe wohl,", "tokens": ["Schla\u00b7fe", "wohl", ",", "schla\u00b7fe", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schlafe wohl in guter Ruh,", "tokens": ["Schla\u00b7fe", "wohl", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und thu deine schwarzbraune Aeuglein zu,", "tokens": ["Und", "thu", "dei\u00b7ne", "schwarz\u00b7brau\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und ruh, und ruh, und ruh in sanfter Ruh.", "tokens": ["Und", "ruh", ",", "und", "ruh", ",", "und", "ruh", "in", "sanf\u00b7ter", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach Schatz, wenn ich nur bey dir w\u00e4r,", "tokens": ["Ach", "Schatz", ",", "wenn", "ich", "nur", "bey", "dir", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein, allein, allein,", "tokens": ["Al\u00b7lein", ",", "al\u00b7lein", ",", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein im Federbett,", "tokens": ["Al\u00b7lein", "im", "Fe\u00b7der\u00b7bett", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir beyde wollten vergn\u00fcget seyn,", "tokens": ["Wir", "bey\u00b7de", "woll\u00b7ten", "ver\u00b7gn\u00fc\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wollten gern allein beynander seyn,", "tokens": ["Und", "woll\u00b7ten", "gern", "al\u00b7lein", "beyn\u00b7an\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach wenn ich dich doch allein in meinen Armen h\u00e4tt.", "tokens": ["Ach", "wenn", "ich", "dich", "doch", "al\u00b7lein", "in", "mei\u00b7nen", "Ar\u00b7men", "h\u00e4tt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Die Geig, die f\u00fchrt einen sanften Ton,", "tokens": ["Die", "Geig", ",", "die", "f\u00fchrt", "ei\u00b7nen", "sanf\u00b7ten", "Ton", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zeiget an, Zeiget an,", "tokens": ["Zei\u00b7get", "an", ",", "Zei\u00b7get", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zeiget an du edle Kron,", "tokens": ["Zei\u00b7get", "an", "du", "ed\u00b7le", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie zeiget an du edle Kron,", "tokens": ["Sie", "zei\u00b7get", "an", "du", "ed\u00b7le", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie vielmal ich geklopfet schon,", "tokens": ["Wie", "viel\u00b7mal", "ich", "ge\u00b7klop\u00b7fet", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich mu\u00df, ich mu\u00df, ich mu\u00df vergeblich stohn.", "tokens": ["Ich", "mu\u00df", ",", "ich", "mu\u00df", ",", "ich", "mu\u00df", "ver\u00b7geb\u00b7lich", "stohn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach Schatz nimm dieses Liedlein an,", "tokens": ["Ach", "Schatz", "nimm", "die\u00b7ses", "Lied\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVIMP", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist, es ist, es ist,", "tokens": ["Es", "ist", ",", "es", "ist", ",", "es", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist auf dich gericht,", "tokens": ["Es", "ist", "auf", "dich", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nimm nur dies schlechte Liedlein an,", "tokens": ["Nimm", "nur", "dies", "schlech\u00b7te", "Lied\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie vielmal ich geklopfet an,", "tokens": ["Wie", "viel\u00b7mal", "ich", "ge\u00b7klop\u00b7fet", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gute Nacht, gute Nacht, gute Nacht mein Schatz.", "tokens": ["Gu\u00b7te", "Nacht", ",", "gu\u00b7te", "Nacht", ",", "gu\u00b7te", "Nacht", "mein", "Schatz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach edler Schatz verzeih es mir,", "tokens": ["Ach", "ed\u00b7ler", "Schatz", "ver\u00b7zeih", "es", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich so sp\u00e4t bin kommen,", "tokens": ["Da\u00df", "ich", "so", "sp\u00e4t", "bin", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die grosse Lieb aus Herzens Begier", "tokens": ["Die", "gros\u00b7se", "Lieb", "aus", "Her\u00b7zens", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hat mich dazu gezwungen.", "tokens": ["Hat", "mich", "da\u00b7zu", "ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jezt liegt mein Schatz, mein edler Schatz", "tokens": ["Jezt", "liegt", "mein", "Schatz", ",", "mein", "ed\u00b7ler", "Schatz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar sanft in seinem Bettchen,", "tokens": ["Gar", "sanft", "in", "sei\u00b7nem", "Bett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ey m\u00f6cht ich ihn ganz inniglich", "tokens": ["Ey", "m\u00f6cht", "ich", "ihn", "ganz", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meiner Musik wecken.", "tokens": ["Mit", "mei\u00b7ner", "Mu\u00b7sik", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Erweck ich sie, erschreck ich sie,", "tokens": ["Er\u00b7weck", "ich", "sie", ",", "er\u00b7schreck", "ich", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es m\u00fc\u00df mich herzlich reuen,", "tokens": ["Es", "m\u00fc\u00df", "mich", "herz\u00b7lich", "reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach k\u00f6nnte ich doch bey ihr seyn,", "tokens": ["Ach", "k\u00f6nn\u00b7te", "ich", "doch", "bey", "ihr", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruhn in ihren Armen.", "tokens": ["Und", "ruhn", "in", "ih\u00b7ren", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie hat zwey Cristallinisch Stein,", "tokens": ["Sie", "hat", "zwey", "Cris\u00b7tal\u00b7li\u00b7nisch", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Auf Elfenbein auch Purpur,", "tokens": ["Auf", "El\u00b7fen\u00b7bein", "auch", "Pur\u00b7pur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Solt ihr geschehn daran ein Leid,", "tokens": ["Solt", "ihr", "ge\u00b7schehn", "da\u00b7ran", "ein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So spr\u00e4ng mein Herz in St\u00fccken.", "tokens": ["So", "spr\u00e4ng", "mein", "Herz", "in", "St\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}