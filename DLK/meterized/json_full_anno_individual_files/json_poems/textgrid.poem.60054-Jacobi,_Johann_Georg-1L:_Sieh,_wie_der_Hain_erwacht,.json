{"textgrid.poem.60054": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, wie der Hain erwacht,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie der Hain erwacht,", "tokens": ["Sieh", ",", "wie", "der", "Hain", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie von umgl\u00e4nzten H\u00f6hen,", "tokens": ["Wie", "von", "um\u00b7gl\u00e4nz\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bey leisem Windes-Wehen,", "tokens": ["Bey", "lei\u00b7sem", "Win\u00b7des\u00b7We\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In frische,", "tokens": ["In", "fri\u00b7sche", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Bethaute B\u00fcsche", "tokens": ["Be\u00b7thau\u00b7te", "B\u00fc\u00b7sche"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Morgen-Wonne lacht!", "tokens": ["Die", "Mor\u00b7gen\u00b7Won\u00b7ne", "lacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wonne, wo die Bl\u00fcthen wallen;", "tokens": ["Won\u00b7ne", ",", "wo", "die", "Bl\u00fc\u00b7then", "wal\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "