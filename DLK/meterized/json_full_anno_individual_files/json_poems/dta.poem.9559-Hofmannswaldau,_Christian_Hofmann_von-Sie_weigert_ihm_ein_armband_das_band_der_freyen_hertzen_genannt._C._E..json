{"dta.poem.9559": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie weigert ihm ein armband/ das band der  \n freyen hertzen genannt.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bleib/ sch\u00f6nste Solime/ bey dem verhasten wahn/", "tokens": ["Bleib", "/", "sch\u00f6ns\u00b7te", "So\u00b7li\u00b7me", "/", "bey", "dem", "ver\u00b7has\u00b7ten", "wahn", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df dir der ordens-band alleine will geb\u00fchren/", "tokens": ["Da\u00df", "dir", "der", "or\u00b7dens\u00b7band", "al\u00b7lei\u00b7ne", "will", "ge\u00b7b\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df er/ ausser dich/ soll keinen frembden zieren/", "tokens": ["Und", "da\u00df", "er", "/", "aus\u00b7ser", "dich", "/", "soll", "kei\u00b7nen", "fremb\u00b7den", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$(", "APPR", "PPER", "$(", "VMFIN", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich nicht so/ wie du/ der freyheit r\u00fchmen kan.", "tokens": ["Der", "sich", "nicht", "so", "/", "wie", "du", "/", "der", "frey\u00b7heit", "r\u00fch\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "ADV", "$(", "PWAV", "PPER", "$(", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich will es gern gestehn/ da\u00df es um mich gethan/", "tokens": ["Ich", "will", "es", "gern", "ge\u00b7stehn", "/", "da\u00df", "es", "um", "mich", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du weist die blicke schon zu meisterlich zu f\u00fchren/", "tokens": ["Du", "weist", "die", "bli\u00b7cke", "schon", "zu", "meis\u00b7ter\u00b7lich", "zu", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ADV", "PTKA", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und kanst durch stille glut das blut in adern r\u00fchren/", "tokens": ["Und", "kanst", "durch", "stil\u00b7le", "glut", "das", "blut", "in", "a\u00b7dern", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja/ legst dem hertzen selbst die schwerste fesseln an.", "tokens": ["Ja", "/", "legst", "dem", "hert\u00b7zen", "selbst", "die", "schwers\u00b7te", "fes\u00b7seln", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was wunder/ da\u00df ich dann um meine freyheit kommen;", "tokens": ["Was", "wun\u00b7der", "/", "da\u00df", "ich", "dann", "um", "mei\u00b7ne", "frey\u00b7heit", "kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nur leugne/ sch\u00f6nste/ nicht/ da\u00df du sie mir genommen/", "tokens": ["Nur", "leug\u00b7ne", "/", "sch\u00f6ns\u00b7te", "/", "nicht", "/", "da\u00df", "du", "sie", "mir", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "$(", "PTKNEG", "$(", "KOUS", "PPER", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und da\u00df dein auge mich und meinen geist entbrandt.", "tokens": ["Und", "da\u00df", "dein", "au\u00b7ge", "mich", "und", "mei\u00b7nen", "geist", "ent\u00b7brandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Giebstu mir dieses zu/ so la\u00df ich deinen band/", "tokens": ["Giebs\u00b7tu", "mir", "die\u00b7ses", "zu", "/", "so", "la\u00df", "ich", "dei\u00b7nen", "band", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKZU", "$(", "ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und will viel lieber ihn und seinen orden missen/", "tokens": ["Und", "will", "viel", "lie\u00b7ber", "ihn", "und", "sei\u00b7nen", "or\u00b7den", "mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als meine freyheit nicht durch dich besieget wissen.", "tokens": ["Als", "mei\u00b7ne", "frey\u00b7heit", "nicht", "durch", "dich", "be\u00b7sie\u00b7get", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}