{"dta.poem.14430": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch er gr\u00fc\u00dft fr\u00f6hlich alle, wie sie fahren,", "tokens": ["Doch", "er", "gr\u00fc\u00dft", "fr\u00f6h\u00b7lich", "al\u00b7le", ",", "wie", "sie", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PIS", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und muthig sagt er zu den s\u00fc\u00dfen Schmerzen:", "tokens": ["Und", "mut\u00b7hig", "sagt", "er", "zu", "den", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201egern sterb' ich bald, wollt ihr von mir je scheiden!\u201c", "tokens": ["\u201e", "gern", "sterb'", "ich", "bald", ",", "wollt", "ihr", "von", "mir", "je", "schei\u00b7den", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$,", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}