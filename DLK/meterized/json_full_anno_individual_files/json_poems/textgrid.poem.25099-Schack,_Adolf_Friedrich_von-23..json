{"textgrid.poem.25099": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht ruht auf dem Geist mir d\u00fcster und schw\u00fcl,", "tokens": ["Nacht", "ruht", "auf", "dem", "Geist", "mir", "d\u00fcs\u00b7ter", "und", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ich f\u00fchl' ein Brausen im Hirn;", "tokens": ["Ich", "f\u00fchl'", "ein", "Brau\u00b7sen", "im", "Hirn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O neig dich herab auf meinen Pf\u00fchl", "tokens": ["O", "neig", "dich", "her\u00b7ab", "auf", "mei\u00b7nen", "Pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und leg mir die Hand auf die Stirn!", "tokens": ["Und", "leg", "mir", "die", "Hand", "auf", "die", "Stirn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Nur sie, die liebe, die wei\u00dfe Hand,", "tokens": ["Nur", "sie", ",", "die", "lie\u00b7be", ",", "die", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PR