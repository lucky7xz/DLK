{"textgrid.poem.59584": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist des Soldaten Lust?", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist des Soldaten Lust?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durchs Leben sich dr\u00e4ngen und schlagen,", "tokens": ["Durchs", "Le\u00b7ben", "sich", "dr\u00e4n\u00b7gen", "und", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit Schwestern und Piken und Lanzen", "tokens": ["Mit", "Schwes\u00b7tern", "und", "Pi\u00b7ken", "und", "Lan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die wilde Fortuna kuranzen,", "tokens": ["Die", "wil\u00b7de", "For\u00b7tu\u00b7na", "ku\u00b7ran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis Sinn ihr und Atem versagen.", "tokens": ["Bis", "Sinn", "ihr", "und", "A\u00b7tem", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das ist des Soldaten Lust.", "tokens": ["Das", "ist", "des", "Sol\u00b7da\u00b7ten", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was ist des Soldaten Gl\u00fcck?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sein lustiges Herz ist nicht wenig,", "tokens": ["Sein", "lus\u00b7ti\u00b7ges", "Herz", "ist", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Damit ist er besser beraten", "tokens": ["Da\u00b7mit", "ist", "er", "bes\u00b7ser", "be\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als andre mit Gold und Dukaten,", "tokens": ["Als", "and\u00b7re", "mit", "Gold", "und", "Du\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Damit ist er Kaiser und K\u00f6nig.", "tokens": ["Da\u00b7mit", "ist", "er", "Kai\u00b7ser", "und", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das ist des Soldaten Gl\u00fcck.", "tokens": ["Das", "ist", "des", "Sol\u00b7da\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was ist des Soldaten Schatz?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Schatz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die heute ihn k\u00fc\u00dft und nicht morgen,", "tokens": ["Die", "heu\u00b7te", "ihn", "k\u00fc\u00dft", "und", "nicht", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "KON", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sein Lieben ist Kommen und Wandern,", "tokens": ["Sein", "Lie\u00b7ben", "ist", "Kom\u00b7men", "und", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Geht immer von einer zur andern", "tokens": ["Geht", "im\u00b7mer", "von", "ei\u00b7ner", "zur", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "APPRART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und kann f\u00fcr die Zukunft nicht sorgen.", "tokens": ["Und", "kann", "f\u00fcr", "die", "Zu\u00b7kunft", "nicht", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das ist des Soldaten Schatz.", "tokens": ["Das", "ist", "des", "Sol\u00b7da\u00b7ten", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Was ist des Soldaten Braut?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eine stolzige Jungfer, hei\u00dft Ehre,", "tokens": ["Ei\u00b7ne", "stol\u00b7zi\u00b7ge", "Jung\u00b7fer", ",", "hei\u00dft", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sie streichelt mit eisernen H\u00e4nden", "tokens": ["Sie", "strei\u00b7chelt", "mit", "ei\u00b7ser\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und kleidet in Stahl ihre Lenden,", "tokens": ["Und", "klei\u00b7det", "in", "Stahl", "ih\u00b7re", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da\u00df Kindheit und Mannheit sich mehre.", "tokens": ["Da\u00df", "Kind\u00b7heit", "und", "Mann\u00b7heit", "sich", "meh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das ist des Soldaten Braut.", "tokens": ["Das", "ist", "des", "Sol\u00b7da\u00b7ten", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Was ist des Soldaten Musik?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Mu\u00b7sik", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die lustigen Trommeln und Pfeifen,", "tokens": ["Die", "lus\u00b7ti\u00b7gen", "Trom\u00b7meln", "und", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Schwerter und S\u00e4bel Geklitter,", "tokens": ["Der", "Schwer\u00b7ter", "und", "S\u00e4\u00b7bel", "Ge\u00b7klit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das laute Kanonengewitter,", "tokens": ["Das", "lau\u00b7te", "Ka\u00b7no\u00b7nen\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wann Kugeln an Kugeln sich streifen.", "tokens": ["Wann", "Ku\u00b7geln", "an", "Ku\u00b7geln", "sich", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das ist des Soldaten Musik.", "tokens": ["Das", "ist", "des", "Sol\u00b7da\u00b7ten", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Was ist des Soldaten Bett?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Bett", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Erde ist Pf\u00fchl ihm und Kissen,", "tokens": ["Die", "Er\u00b7de", "ist", "Pf\u00fchl", "ihm", "und", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sein Oberbett decket der Himmel,", "tokens": ["Sein", "O\u00b7ber\u00b7bett", "de\u00b7cket", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Nachtlicht das Sternengewimmel,", "tokens": ["Sein", "Nacht\u00b7licht", "das", "Ster\u00b7nen\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sein W\u00e4chter ein festes Gewissen.", "tokens": ["Sein", "W\u00e4ch\u00b7ter", "ein", "fes\u00b7tes", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das ist des Soldaten Bett.", "tokens": ["Das", "ist", "des", "Sol\u00b7da\u00b7ten", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Was ist des Soldaten Schutz?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Schutz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sein Mut und das blinkende Eisen", "tokens": ["Sein", "Mut", "und", "das", "blin\u00b7ken\u00b7de", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Als sicherste R\u00fcstung ihn zieret,", "tokens": ["Als", "si\u00b7chers\u00b7te", "R\u00fcs\u00b7tung", "ihn", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und Gott, der die Kugeln regieret,", "tokens": ["Und", "Gott", ",", "der", "die", "Ku\u00b7geln", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da\u00df an ihm vor\u00fcber sie reifen.", "tokens": ["Da\u00df", "an", "ihm", "vor\u00b7\u00fc\u00b7ber", "sie", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das ist des Soldaten Schutz.", "tokens": ["Das", "ist", "des", "Sol\u00b7da\u00b7ten", "Schutz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Was ist des Soldaten Lohn?", "tokens": ["Was", "ist", "des", "Sol\u00b7da\u00b7ten", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am Abend und Morgen Beschwerde", "tokens": ["Am", "A\u00b7bend", "und", "Mor\u00b7ge