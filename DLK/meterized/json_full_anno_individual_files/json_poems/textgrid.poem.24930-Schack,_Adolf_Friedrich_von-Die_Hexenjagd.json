{"textgrid.poem.24930": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hexenjagd", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnun m\u00fcssen wir reiten durch Nacht und Sturm;", "tokens": ["\u00bb", "nun", "m\u00fcs\u00b7sen", "wir", "rei\u00b7ten", "durch", "Nacht", "und", "Sturm", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schon wieder flohn drei aus dem Drudenturm,", "tokens": ["Schon", "wie\u00b7der", "flohn", "drei", "aus", "dem", "Dru\u00b7den\u00b7turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "CARD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die morgen mir brennen sollten.", "tokens": ["Die", "mor\u00b7gen", "mir", "bren\u00b7nen", "soll\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf, meine Dogge, mein H\u00f6llenzwang!", "tokens": ["Auf", ",", "mei\u00b7ne", "Dog\u00b7ge", ",", "mein", "H\u00f6l\u00b7len\u00b7zwang", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Herbei, ihr Knechte! Denn solch einem Fang,", "tokens": ["Her\u00b7bei", ",", "ihr", "Knech\u00b7te", "!", "Denn", "solch", "ei\u00b7nem", "Fang", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$.", "KON", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem hat es seit lang nicht gegolten.\u00ab", "tokens": ["Dem", "hat", "es", "seit", "lang", "nicht", "ge\u00b7gol\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sprengt aus dem Thore von Lindheim Gei\u00df,", "tokens": ["So", "sprengt", "aus", "dem", "Tho\u00b7re", "von", "Lind\u00b7heim", "Gei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Der grimmige Bauernbedr\u00e4nger;", "tokens": ["Der", "grim\u00b7mi\u00b7ge", "Bau\u00b7ern\u00b7be\u00b7dr\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Ihm folgen die B\u00fcttel auf sein Gehei\u00df", "tokens": ["Ihm", "fol\u00b7gen", "die", "B\u00fct\u00b7tel", "auf", "sein", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und die Hunde, die Hexenf\u00e4nger.", "tokens": ["Und", "die", "Hun\u00b7de", ",", "die", "He\u00b7xen\u00b7f\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Von dannen st\u00fcrmt er mit wildem Hallo.", "tokens": ["Von", "dan\u00b7nen", "st\u00fcrmt", "er", "mit", "wil\u00b7dem", "Hal\u00b7lo", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was braut auf dem Moore? Was flackert so loh?", "tokens": ["Was", "braut", "auf", "dem", "Moo\u00b7re", "?", "Was", "fla\u00b7ckert", "so", "loh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$.", "PWS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was huschelt und raunt auf der Wiese?", "tokens": ["Was", "hu\u00b7schelt", "und", "raunt", "auf", "der", "Wie\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dort kauern am Feuer von qualmendem Torf", "tokens": ["Dort", "kau\u00b7ern", "am", "Feu\u00b7er", "von", "qual\u00b7men\u00b7dem", "Torf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Hanne, der Schrecken vom ganzen Dorf,", "tokens": ["Die", "Han\u00b7ne", ",", "der", "Schre\u00b7cken", "vom", "gan\u00b7zen", "Dorf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die alte Margret und die Lise;", "tokens": ["Die", "al\u00b7te", "Marg\u00b7ret", "und", "die", "Li\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie schaffen am Kessel und r\u00fchren geschwind", "tokens": ["Sie", "schaf\u00b7fen", "am", "Kes\u00b7sel", "und", "r\u00fch\u00b7ren", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Das schwarze Gebr\u00e4u mit der Kelle,", "tokens": ["Das", "schwar\u00b7ze", "Ge\u00b7br\u00e4u", "mit", "der", "Kel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Und sch\u00fcren die Glut im Wirbelwind:", "tokens": ["Und", "sch\u00fc\u00b7ren", "die", "Glut", "im", "Wir\u00b7bel\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "\u00bbhilf, Teufel! Hilf, Buhlgeselle!\u00ab", "tokens": ["\u00bb", "hilf", ",", "Teu\u00b7fel", "!", "Hilf", ",", "Buhl\u00b7ge\u00b7sel\u00b7le", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$.", "NE", "$,", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da steigt es herauf wie Nebel und Rauch", "tokens": ["Da", "steigt", "es", "her\u00b7auf", "wie", "Ne\u00b7bel", "und", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "NN", "KON", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ballt sich und wirbelt um Busch und Strauch", "tokens": ["Und", "ballt", "sich", "und", "wir\u00b7belt", "um", "Busch", "und", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und kreist und dreht sich in Ringen;", "tokens": ["Und", "kreist", "und", "dreht", "sich", "in", "Rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hier zuckt es empor, dort huscht es im Flug;", "tokens": ["Hier", "zuckt", "es", "em\u00b7por", ",", "dort", "huscht", "es", "im", "Flug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von Hexen wimmelt der ganze Bruch;", "tokens": ["Von", "He\u00b7xen", "wim\u00b7melt", "der", "gan\u00b7ze", "Bruch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie h\u00fcpfen und lachen und springen.", "tokens": ["Sie", "h\u00fcp\u00b7fen", "und", "la\u00b7chen", "und", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Mit Besen und B\u00fcchse und Zauberkn\u00e4ul", "tokens": ["Mit", "Be\u00b7sen", "und", "B\u00fcch\u00b7se", "und", "Zau\u00b7ber\u00b7kn\u00e4ul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Umtanzen sie Ro\u00df und Reiter;", "tokens": ["Um\u00b7tan\u00b7zen", "sie", "Ro\u00df", "und", "Rei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Bald leises Zischeln, bald w\u00fcstes Geheul:", "tokens": ["Bald", "lei\u00b7ses", "Zi\u00b7scheln", ",", "bald", "w\u00fcs\u00b7tes", "Ge\u00b7heul", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "\u00bbnur weiter, Herr Gei\u00df, nur weiter!\u00ab", "tokens": ["\u00bb", "nur", "wei\u00b7ter", ",", "Herr", "Gei\u00df", ",", "nur", "wei\u00b7ter", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "NN", "NN", "$,", "ADV", "ADV", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Hell wird's auf der Wiese von r\u00f6tlichem Licht,", "tokens": ["Hell", "wird's", "auf", "der", "Wie\u00b7se", "von", "r\u00f6t\u00b7li\u00b7chem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und Holzsto\u00df dr\u00e4ngt sich an Holzsto\u00df dicht", "tokens": ["Und", "Holz\u00b7sto\u00df", "dr\u00e4ngt", "sich", "an", "Holz\u00b7sto\u00df", "dicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit leckenden Flammenzungen.", "tokens": ["Mit", "le\u00b7cken\u00b7den", "Flam\u00b7men\u00b7zun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herr Gei\u00df h\u00e4lt inne; von links und rechts,", "tokens": ["Herr", "Gei\u00df", "h\u00e4lt", "in\u00b7ne", ";", "von", "links", "und", "rechts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "$.", "APPR", "ADV", "KON", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von vorn und von hinten vernimmt er Ge\u00e4chz;", "tokens": ["Von", "vorn", "und", "von", "hin\u00b7ten", "ver\u00b7nimmt", "er", "Ge\u00b7\u00e4chz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "APPR", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "So hat es noch nie ihm geklungen.", "tokens": ["So", "hat", "es", "noch", "nie", "ihm", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Er sieht durch die Glut und den Wirbeldampf", "tokens": ["Er", "sieht", "durch", "die", "Glut", "und", "den", "Wir\u00b7bel\u00b7dampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Der aufw\u00e4rts lodernden Br\u00e4nde", "tokens": ["Der", "auf\u00b7w\u00e4rts", "lo\u00b7dern\u00b7den", "Br\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Gesichter, erbleichend im Todeskrampf,", "tokens": ["Ge\u00b7sich\u00b7ter", ",", "er\u00b7blei\u00b7chend", "im", "To\u00b7des\u00b7krampf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Und jammernd gerungene H\u00e4nde.", "tokens": ["Und", "jam\u00b7mernd", "ge\u00b7run\u00b7ge\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Nur weiter, nur weiter! Auf einmal klafft", "tokens": ["Nur", "wei\u00b7ter", ",", "nur", "wei\u00b7ter", "!", "Auf", "ein\u00b7mal", "klafft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADV", "$.", "APPR", "ADV", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Graben vor ihm; er spornt mit Kraft", "tokens": ["Ein", "Gra\u00b7ben", "vor", "ihm", ";", "er", "spornt", "mit", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Den schnaubenden Renner zum Springen.", "tokens": ["Den", "schnau\u00b7ben\u00b7den", "Ren\u00b7ner", "zum", "Sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da taucht aus der Tiefe im wei\u00dfen Gewand", "tokens": ["Da", "taucht", "aus", "der", "Tie\u00b7fe", "im", "wei\u00b7\u00dfen", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die tolle Gertraud, die er gestern verbrannt;", "tokens": ["Die", "tol\u00b7le", "Ger\u00b7traud", ",", "die", "er", "ge\u00b7stern", "ver\u00b7brannt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In die Arme will sie ihn schlingen.", "tokens": ["In", "die", "Ar\u00b7me", "will", "sie", "ihn", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Er starrt; ihn d\u00fcnkt, als ob himmelan", "tokens": ["Er", "starrt", ";", "ihn", "d\u00fcnkt", ",", "als", "ob", "him\u00b7me\u00b7lan"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zur Riesin sie w\u00fcchs' und schw\u00f6lle.", "tokens": ["Zur", "Rie\u00b7sin", "sie", "w\u00fcchs'", "und", "schw\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "\u00bbhoho! Hoho! \u2013 mein s\u00fc\u00dfer Kumpan!", "tokens": ["\u00bb", "ho\u00b7ho", "!", "Ho\u00b7ho", "!", "\u2013", "mein", "s\u00fc\u00b7\u00dfer", "Kum\u00b7pan", "!"], "token_info": ["punct", "word", "pu