{"dta.poem.13472": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Feld und Buchenhallen,", "tokens": ["Durch", "Feld", "und", "Bu\u00b7chen\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bald singend, bald fr\u00f6hlich still,", "tokens": ["Bald", "sin\u00b7gend", ",", "bald", "fr\u00f6h\u00b7lich", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Recht lustig sey vor allen", "tokens": ["Recht", "lus\u00b7tig", "sey", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VAFIN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer's Reisen w\u00e4hlen will!", "tokens": ["Wer's", "Rei\u00b7sen", "w\u00e4h\u00b7len", "will", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn's kaum in Osten gl\u00fchte,", "tokens": ["Wenn's", "kaum", "in", "Os\u00b7ten", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Welt noch still und weit:", "tokens": ["Die", "Welt", "noch", "still", "und", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da weht recht durch's Gem\u00fcthe", "tokens": ["Da", "weht", "recht", "durch's", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sch\u00f6ne Bl\u00fcthenzeit!", "tokens": ["Die", "sch\u00f6\u00b7ne", "Bl\u00fc\u00b7then\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Lerch' als Morgenbote", "tokens": ["Die", "Ler\u00b7ch'", "als", "Mor\u00b7gen\u00b7bo\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich in die L\u00fcfte schwingt,", "tokens": ["Sich", "in", "die", "L\u00fcf\u00b7te", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eine frische Reisenote", "tokens": ["Ei\u00b7ne", "fri\u00b7sche", "Rei\u00b7se\u00b7no\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch Wald und Herz erklingt.", "tokens": ["Durch", "Wald", "und", "Herz", "er\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Lust, vom Berg zu schauen,", "tokens": ["O", "Lust", ",", "vom", "Berg", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit \u00fcber Wald und Strom,", "tokens": ["Weit", "\u00fc\u00b7ber", "Wald", "und", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber sich den blauen", "tokens": ["Hoch", "\u00fc\u00b7ber", "sich", "den", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PRF", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tiefklaren Himmelsdom!", "tokens": ["Tief\u00b7kla\u00b7ren", "Him\u00b7mels\u00b7dom", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vom Berge V\u00f6glein fliegen", "tokens": ["Vom", "Ber\u00b7ge", "V\u00f6\u00b7glein", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Wolken so geschwind,", "tokens": ["Und", "Wol\u00b7ken", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gedanken \u00fcberfliegen", "tokens": ["Ge\u00b7dan\u00b7ken", "\u00fc\u00b7berf\u00b7lie\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die V\u00f6gel und den Wind.", "tokens": ["Die", "V\u00f6\u00b7gel", "und", "den", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Wolken zieh'n hernieder,", "tokens": ["Die", "Wol\u00b7ken", "zieh'n", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das V\u00f6glein senkt sich gleich,", "tokens": ["Das", "V\u00f6\u00b7glein", "senkt", "sich", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gedanken gehn und Lieder", "tokens": ["Ge\u00b7dan\u00b7ken", "gehn", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fort bis in's Himmelreich.", "tokens": ["Fort", "bis", "in's", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}