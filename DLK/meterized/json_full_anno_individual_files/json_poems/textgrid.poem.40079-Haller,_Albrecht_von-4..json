{"textgrid.poem.40079": {"metadata": {"author": {"name": "Haller, Albrecht von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ruhm, der Weise kr\u00f6nt, der um die Helden strahlt", "tokens": ["Der", "Ruhm", ",", "der", "Wei\u00b7se", "kr\u00f6nt", ",", "der", "um", "die", "Hel\u00b7den", "strahlt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den bem\u00fchten Dienst erhabner B\u00fcrger zahlt,", "tokens": ["Und", "den", "be\u00b7m\u00fch\u00b7ten", "Dienst", "er\u00b7hab\u00b7ner", "B\u00fcr\u00b7ger", "zahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist f\u00fcr sie selbst ein Rauch, den sie nicht ungern missen;", "tokens": ["Ist", "f\u00fcr", "sie", "selbst", "ein", "Rauch", ",", "den", "sie", "nicht", "un\u00b7gern", "mis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ersten Tugend Lohn hat Gott und ihr Gewissen.", "tokens": ["Der", "ers\u00b7ten", "Tu\u00b7gend", "Lohn", "hat", "Gott", "und", "ihr", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann ist der Ruhm kein Dunst, wann er den jungen Geist,", "tokens": ["Dann", "ist", "der", "Ruhm", "kein", "Dunst", ",", "wann", "er", "den", "jun\u00b7gen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der regen Flamme gleich, mit sich zur H\u00f6he rei\u00dft,", "tokens": ["Der", "re\u00b7gen", "Flam\u00b7me", "gleich", ",", "mit", "sich", "zur", "H\u00f6\u00b7he", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "APPR", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nach edler Ahnen Bild die Nachwelt reizt zu streben,", "tokens": ["Nach", "ed\u00b7ler", "Ah\u00b7nen", "Bild", "die", "Nach\u00b7welt", "reizt", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Alexandern zwingt, im C\u00e4sar aufzuleben.", "tokens": ["Und", "A\u00b7lex\u00b7an\u00b7dern", "zwingt", ",", "im", "C\u00e4\u00b7sar", "auf\u00b7zu\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "APPRART", "NE", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Ruhm, der Weise kr\u00f6nt, der um die Helden st