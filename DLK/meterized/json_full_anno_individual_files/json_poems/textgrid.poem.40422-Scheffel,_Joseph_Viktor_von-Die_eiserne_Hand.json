{"textgrid.poem.40422": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die eiserne Hand", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bburfehde schw\u00f6rt! ansonst ich", "tokens": ["\u00bb", "ur\u00b7feh\u00b7de", "schw\u00f6rt", "!", "an\u00b7sonst", "ich"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "NE", "VVFIN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch in den Brummturm setz'\u00ab,", "tokens": ["Euch", "in", "den", "Brumm\u00b7turm", "setz'", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$(", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sprach Heilbronns B\u00fcrgermeister", "tokens": ["Sprach", "Heil\u00b7bronns", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Berlichinger G\u00f6tz.", "tokens": ["Zum", "Ber\u00b7li\u00b7chin\u00b7ger", "G\u00f6tz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu besserm Nachdruck standen", "tokens": ["Zu", "bes\u00b7serm", "Nach\u00b7druck", "stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Rathausgang entlang", "tokens": ["Den", "Rat\u00b7haus\u00b7gang", "ent\u00b7lang"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPO"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weinschr\u00f6ter, Schmied' und Fl\u00f6zer", "tokens": ["Wein\u00b7schr\u00f6\u00b7ter", ",", "Schmied'", "und", "Fl\u00f6\u00b7zer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Haken, Spie\u00df und Stang'.", "tokens": ["Mit", "Ha\u00b7ken", ",", "Spie\u00df", "und", "Stang'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wie ein Ratsherrnfinger", "tokens": ["Doch", "wie", "ein", "Rats\u00b7herrn\u00b7fin\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Hinterhalt winkt vor,", "tokens": ["Den", "Hin\u00b7ter\u00b7halt", "winkt", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hub grimm der Berlichinger", "tokens": ["Hub", "grimm", "der", "Ber\u00b7li\u00b7chin\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Eisenfaust empor.", "tokens": ["Die", "Ei\u00b7sen\u00b7faust", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und still ward's auf den B\u00e4nken", "tokens": ["Und", "still", "ward's", "auf", "den", "B\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und still in Saal und Haus:", "tokens": ["Und", "still", "in", "Saal", "und", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Fingern und Gelenken", "tokens": ["In", "Fin\u00b7gern", "und", "Ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sah sie so schlagbar aus,", "tokens": ["Sah", "sie", "so", "schlag\u00b7bar", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als w\u00fcchsen n\u00e4chstens Feigen", "tokens": ["Als", "w\u00fcch\u00b7sen", "n\u00e4chs\u00b7tens", "Fei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So saftig und kandiert,", "tokens": ["So", "saf\u00b7tig", "und", "kan\u00b7diert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie noch nie auf Erden", "tokens": ["Wie", "sie", "noch", "nie", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein sterblich Ohr geziert.", "tokens": ["Ein", "sterb\u00b7lich", "Ohr", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Derweil klang's wie Trompeten", "tokens": ["Der\u00b7weil", "klang's", "wie", "Trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "KOKOM", "NN"], "meter": "--+-++-", "measure": "anapaest.init"}, "line.2": {"text": "Vom Neckartor herauf;", "tokens": ["Vom", "Ne\u00b7ckar\u00b7tor", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sickingens Reiter trabten,", "tokens": ["Si\u00b7ckin\u00b7gens", "Rei\u00b7ter", "trab\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Fu\u00dfvolk kam im Lauf.", "tokens": ["Und", "Fu\u00df\u00b7volk", "kam", "im", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbso seid doch nicht so zornig!\u00ab", "tokens": ["\u00bb", "so", "seid", "doch", "nicht", "so", "zor\u00b7nig", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach jetzt der Magistrat,", "tokens": ["Sprach", "jetzt", "der", "Ma\u00b7gist\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbes kommt in Stadt wie Land vor,", "tokens": ["\u00bb", "es", "kommt", "in", "Stadt", "wie", "Land", "vor", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df man sich mi\u00dfverst\u00e2t.", "tokens": ["Da\u00df", "man", "sich", "mi\u00df\u00b7ver\u00b7st\u00e2t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wer drohte je mit Brummturm?", "tokens": ["Wer", "droh\u00b7te", "je", "mit", "Brumm\u00b7turm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir bieten Losament", "tokens": ["Wir", "bie\u00b7ten", "Lo\u00b7sa\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und laden den Herrn Schwager", "tokens": ["Und", "la\u00b7den", "den", "Herrn", "Schwa\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Samt Euch zum Traktament.\u00ab", "tokens": ["Samt", "Euch", "zum", "Trak\u00b7ta\u00b7ment", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbah so, liebwerte Nachbarn\u00ab,", "tokens": ["\u00bb", "ah", "so", ",", "lieb\u00b7wer\u00b7te", "Nach\u00b7barn", "\u00ab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "ADJA", "NN", "$(", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sprach G\u00f6tz, \u00bbviel Dank, ich komm'...", "tokens": ["Sprach", "G\u00f6tz", ",", "\u00bb", "viel", "Dank", ",", "ich", "komm'", "..."], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "$(", "PIAT", "NN", "$,", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Teufel soll Euch holen,", "tokens": ["Der", "Teu\u00b7fel", "soll", "Euch", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie seid Ihr pl\u00f6tzlich fromm.", "tokens": ["Wie", "seid", "Ihr", "pl\u00f6tz\u00b7lich", "fromm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mir scheint, die Faust von Eisen", "tokens": ["Mir", "scheint", ",", "die", "Faust", "von", "Ei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bringt Wunderwerk zuweg:", "tokens": ["Bringt", "Wun\u00b7der\u00b7werk", "zu\u00b7weg", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r's ein Glanzleder-Handschuh,", "tokens": ["W\u00e4r's", "ein", "Glanz\u00b7le\u00b7der\u00b7Hand\u00b7schuh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wei\u00df Gott, wo ich heut l\u00e4g'!\u00ab", "tokens": ["Wei\u00df", "Gott", ",", "wo", "ich", "heut", "l\u00e4g'", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bburfehde schw\u00f6rt! ansonst ich", "tokens": ["\u00bb", "ur\u00b7feh\u00b7de", "schw\u00f6rt", "!", "an\u00b7sonst", "ich"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "NE", "VVFIN", "$.", "VVF