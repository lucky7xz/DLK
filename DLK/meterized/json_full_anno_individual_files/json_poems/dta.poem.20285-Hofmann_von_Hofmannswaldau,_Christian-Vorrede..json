{"dta.poem.20285": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn so viel zucker w\u00e4r/ als schnee/", "tokens": ["Wenn", "so", "viel", "zu\u00b7cker", "w\u00e4r", "/", "als", "schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "VAFIN", "$(", "KOUS", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und so viel bienen/ als der fliegen;", "tokens": ["Und", "so", "viel", "bie\u00b7nen", "/", "als", "der", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$(", "KOUS", "ART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn alle berge Hyblens klee/", "tokens": ["Wenn", "al\u00b7le", "ber\u00b7ge", "Hyb\u00b7lens", "klee", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und des Hymettus kr\u00e4uter tr\u00fcgen/", "tokens": ["Und", "des", "Hy\u00b7met\u00b7tus", "kr\u00e4u\u00b7ter", "tr\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus allen eichen trieff\u2019 ein honig von Athen/", "tokens": ["Aus", "al\u00b7len", "ei\u00b7chen", "trief\u00b7f'", "ein", "ho\u00b7nig", "von", "A\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und man auff d\u00f6rnern nichts als feigen s\u00e4he stehn;", "tokens": ["Und", "man", "auff", "d\u00f6r\u00b7nern", "nichts", "als", "fei\u00b7gen", "s\u00e4\u00b7he", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ADV", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn milch in allen str\u00f6hmen flie\u00df\u2019/", "tokens": ["Wenn", "milch", "in", "al\u00b7len", "str\u00f6h\u00b7men", "flie\u00df'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und reben-safft aus allen qvellen;", "tokens": ["Und", "re\u00b7ben\u00b7\u00b7safft", "aus", "al\u00b7len", "qvel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn alle schleen w\u00e4ren s\u00fc\u00df/", "tokens": ["Wenn", "al\u00b7le", "schleen", "w\u00e4\u00b7ren", "s\u00fc\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "VAFIN", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Im meere lauter nectar-wellen;", "tokens": ["Im", "mee\u00b7re", "lau\u00b7ter", "nec\u00b7ta\u00b7rwel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn nur ja\u00dfminen-oel der wolcken n\u00e4sse w\u00e4r/", "tokens": ["Wenn", "nur", "ja\u00df\u00b7mi\u00b7nen\u00b7o\u00b7el", "der", "wol\u00b7cken", "n\u00e4s\u00b7se", "w\u00e4r", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ART", "NN", "VVFIN", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Der monde nichts als thau von zimmet fl\u00f6\u00dfte her;", "tokens": ["Der", "mon\u00b7de", "nichts", "als", "thau", "von", "zim\u00b7met", "fl\u00f6\u00df\u00b7te", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "KOKOM", "VVFIN", "APPR", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn die gestirne schwitzten safft/", "tokens": ["Wenn", "die", "ge\u00b7stir\u00b7ne", "schwitz\u00b7ten", "safft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der W\u00fcrtz und balsam \u00fcberstiege/", "tokens": ["Der", "W\u00fcrtz", "und", "bal\u00b7sam", "\u00fc\u00b7bers\u00b7tie\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und dieser s\u00fc\u00dfigkeiten krafft/", "tokens": ["Und", "die\u00b7ser", "s\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", "krafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In einen geist und kern gediege/", "tokens": ["In", "ei\u00b7nen", "geist", "und", "kern", "ge\u00b7die\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So w\u00fcrde dieser doch bey liebe wermuth seyn:", "tokens": ["So", "w\u00fcr\u00b7de", "die\u00b7ser", "doch", "bey", "lie\u00b7be", "wer\u00b7muth", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn diese zuckert auch das bittre sterben ein.", "tokens": ["Denn", "die\u00b7se", "zu\u00b7ckert", "auch", "das", "bitt\u00b7re", "ster\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}}}}