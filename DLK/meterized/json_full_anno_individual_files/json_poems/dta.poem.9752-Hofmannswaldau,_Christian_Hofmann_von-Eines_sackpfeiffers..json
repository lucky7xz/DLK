{"dta.poem.9752": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines sackpfeiffers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich machte knecht und magd recht lustig durch mein pfeiffen/", "tokens": ["Ich", "mach\u00b7te", "knecht", "und", "magd", "recht", "lus\u00b7tig", "durch", "mein", "pfeif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Niemahls war ich beschwert zum dr\u00fccken/ blasen/ greiffen/", "tokens": ["Nie\u00b7mahls", "war", "ich", "be\u00b7schwert", "zum", "dr\u00fc\u00b7cken", "/", "bla\u00b7sen", "/", "greif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPRART", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Jtzt liegt nach meinem todt die pfeiffe gantz allein.", "tokens": ["Jtzt", "liegt", "nach", "mei\u00b7nem", "todt", "die", "pfeif\u00b7fe", "gantz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJD", "ART", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Thu mir den liebes-dienst und bla\u00df einmahl darein!", "tokens": ["Thu", "mir", "den", "lie\u00b7bes\u00b7dienst", "und", "bla\u00df", "ein\u00b7mahl", "da\u00b7rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "ADJD", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}