{"dta.poem.13073": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe Freuden-voll die schl\u00e4frigen Freundinnen", "tokens": ["Ich", "sa\u00b7he", "Freu\u00b7den\u00b7voll", "die", "schl\u00e4f\u00b7ri\u00b7gen", "Freun\u00b7din\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey diesem Tugend-Streit auch einen Plaz gewinnen.", "tokens": ["Bey", "die\u00b7sem", "Tu\u00b7gen\u00b7dStreit", "auch", "ei\u00b7nen", "Plaz", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "10Und bracht die Tugenden in allgemeine Freud.", "tokens": ["bracht", "die", "Tu\u00b7gen\u00b7den", "in", "all\u00b7ge\u00b7mei\u00b7ne", "Freud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Ein sch\u00f6nes junges Weib! das eine Cither hielte,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "jun\u00b7ges", "Weib", "!", "das", "ei\u00b7ne", "Cit\u00b7her", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ungef\u00e4hr den Saal mit ihrem Klang erf\u00fcllte,", "tokens": ["Und", "un\u00b7ge\u00b7f\u00e4hr", "den", "Saal", "mit", "ih\u00b7rem", "Klang", "er\u00b7f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df Ohr und Aug und Sinn sich auferweckt gesp\u00fchrt;", "tokens": ["Da\u00df", "Ohr", "und", "Aug", "und", "Sinn", "sich", "au\u00b7fer\u00b7weckt", "ge\u00b7sp\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil sie durch das Gedr\u00e4ng die Sayten anger\u00fchrt.", "tokens": ["Weil", "sie", "durch", "das", "Ge\u00b7dr\u00e4ng", "die", "Say\u00b7ten", "an\u00b7ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "15Sie fragte Demuth-voll, ob ihnen was zu schaffen?", "tokens": ["frag\u00b7te", "De\u00b7muth\u00b7voll", ",", "ob", "ih\u00b7nen", "was", "zu", "schaf\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "KOUS", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Sie seyen zwar noch m\u00fcd\u2019 und haben nichts geschlaffen,", "tokens": ["Sie", "se\u00b7yen", "zwar", "noch", "m\u00fcd'", "und", "ha\u00b7ben", "nichts", "ge\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "KON", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Seit dem sie diese Stadt so pr\u00e4chtig ausgeziert,", "tokens": ["Seit", "dem", "sie", "die\u00b7se", "Stadt", "so", "pr\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PDAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mehr als einen Wald von Wercken aufgef\u00fchrt.", "tokens": ["Und", "mehr", "als", "ei\u00b7nen", "Wald", "von", "Wer\u00b7cken", "auf\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}