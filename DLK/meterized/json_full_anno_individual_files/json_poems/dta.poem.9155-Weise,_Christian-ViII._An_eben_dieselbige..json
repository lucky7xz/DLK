{"dta.poem.9155": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n An eben dieselbige.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Gute nacht ihr edlen beyde/", "tokens": ["Gu\u00b7te", "nacht", "ihr", "ed\u00b7len", "bey\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lobt den Himmel der euch liebt/", "tokens": ["Lobt", "den", "Him\u00b7mel", "der", "euch", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die angenehmste freude/", "tokens": ["Und", "die", "an\u00b7ge\u00b7nehms\u00b7te", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser nacht zu eigen gibt/", "tokens": ["Die\u00b7ser", "nacht", "zu", "ei\u00b7gen", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKA", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt den Himmel und bedenckt/", "tokens": ["Lobt", "den", "Him\u00b7mel", "und", "be\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was er euch vor gnade schenckt.", "tokens": ["Was", "er", "euch", "vor", "gna\u00b7de", "schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "APPR", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Euer leben und gedeyen", "tokens": ["Eu\u00b7er", "le\u00b7ben", "und", "ge\u00b7de\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVINF", "KON", "VVINF"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Das bezeugt euch mehr als wol/", "tokens": ["Das", "be\u00b7zeugt", "euch", "mehr", "als", "wol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "KOKOM", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sich manches haupt erfreuen/", "tokens": ["Da\u00df", "sich", "man\u00b7ches", "haupt", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manches hertze tr\u00f6sten soll/", "tokens": ["Man\u00b7ches", "hert\u00b7ze", "tr\u00f6s\u00b7ten", "soll", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese freude kan allein", "tokens": ["Die\u00b7se", "freu\u00b7de", "kan", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht vor zwo personen seyn.", "tokens": ["Nicht", "vor", "zwo", "per\u00b7so\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "CARD", "ADJA", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Die geliebten Eltern zwingen", "tokens": ["Die", "ge\u00b7lieb\u00b7ten", "El\u00b7tern", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich zu der verlangten ruh/", "tokens": ["Sich", "zu", "der", "ver\u00b7lang\u00b7ten", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Doch die lieben hertzen bringen", "tokens": ["Doch", "die", "lie\u00b7ben", "hert\u00b7zen", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der lust kein auge zu/", "tokens": ["Vor", "der", "lust", "kein", "au\u00b7ge", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann sie dencken allzugleich", "tokens": ["Dann", "sie", "den\u00b7cken", "all\u00b7zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Edles beyde nur an euch.", "tokens": ["Ed\u00b7les", "bey\u00b7de", "nur", "an", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Sie erwarten mit verlangen", "tokens": ["Sie", "er\u00b7war\u00b7ten", "mit", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die erw\u00fcnschte tageszeit/", "tokens": ["Die", "er\u00b7w\u00fcnschte", "ta\u00b7ges\u00b7zeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Euch empfindlich zu empfangen/", "tokens": ["Euch", "emp\u00b7find\u00b7lich", "zu", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein pfand das sich verneut/", "tokens": ["Als", "ein", "pfand", "das", "sich", "ver\u00b7neut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "PRF", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die freundschafft in der that", "tokens": ["Und", "die", "freund\u00b7schafft", "in", "der", "that"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich recht verkn\u00fcpffet hat.", "tokens": ["End\u00b7lich", "recht", "ver\u00b7kn\u00fcpf\u00b7fet", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Nun des g\u00fcnstgen Himmels-segen/", "tokens": ["Nun", "des", "g\u00fcnst\u00b7gen", "Him\u00b7mels\u00b7se\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halte ferner den gebrauch/", "tokens": ["Hal\u00b7te", "fer\u00b7ner", "den", "ge\u00b7brauch", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wachset nicht nur eurentwegen/", "tokens": ["Wach\u00b7set", "nicht", "nur", "eu\u00b7rent\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern ihrentwegen auch/", "tokens": ["Son\u00b7dern", "ih\u00b7rent\u00b7we\u00b7gen", "auch", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die an allen was geschehn/", "tokens": ["Die", "an", "al\u00b7len", "was", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "PIS", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhres hertzens wollust sehn.", "tokens": ["Ih\u00b7res", "hert\u00b7zens", "wol\u00b7lust", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}