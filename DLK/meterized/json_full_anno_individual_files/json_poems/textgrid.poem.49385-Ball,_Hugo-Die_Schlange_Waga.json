{"textgrid.poem.49385": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlange Waga", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer kennt mich noch? Ich trug die Perserm\u00fctze.", "tokens": ["Wer", "kennt", "mich", "noch", "?", "Ich", "trug", "die", "Per\u00b7ser\u00b7m\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es spiegelte darin der Schlange Bild.", "tokens": ["Es", "spie\u00b7gel\u00b7te", "da\u00b7rin", "der", "Schlan\u00b7ge", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr grauer Blick, der mich gefangen hielt,", "tokens": ["Ihr", "grau\u00b7er", "Blick", ",", "der", "mich", "ge\u00b7fan\u00b7gen", "hielt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verwehrte mir des Paradieses Spitze.", "tokens": ["Ver\u00b7wehr\u00b7te", "mir", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Lanze schwang ich bis zu ihrem Sitze.", "tokens": ["Die", "Lan\u00b7ze", "schwang", "ich", "bis", "zu", "ih\u00b7rem", "Sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "wor