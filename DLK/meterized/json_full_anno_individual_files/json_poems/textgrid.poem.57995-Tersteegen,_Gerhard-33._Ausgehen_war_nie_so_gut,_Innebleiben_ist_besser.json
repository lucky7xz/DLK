{"textgrid.poem.57995": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "33. Ausgehen war nie so gut, Innebleiben ist besser", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein andrer gehet aus und sich gern divertieret;", "tokens": ["Ein", "an\u00b7drer", "ge\u00b7het", "aus", "und", "sich", "gern", "di\u00b7ver\u00b7tie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "KON", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich geh am liebsten ein, so werd' ich nicht verf\u00fchret,", "tokens": ["Ich", "geh", "am", "liebs\u00b7ten", "ein", ",", "so", "werd'", "ich", "nicht", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn ich aus soll gehn, so bleib' ich eingekehrt,", "tokens": ["Und", "wenn", "ich", "aus", "soll", "gehn", ",", "so", "bleib'", "ich", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "VMFIN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich im Ausgehn selbst nicht abgezogen werd'.", "tokens": ["Da\u00df", "ich", "im", "Aus\u00b7gehn", "selbst", "nicht", "ab\u00b7ge\u00b7zo\u00b7gen", "werd'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "p