 "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr Feinde wie getan.", "tokens": ["ihr", "Fein\u00b7de", "wie", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Her alle Fr\u00f6lichkeit", "tokens": ["Her", "al\u00b7le", "Fr\u00f6\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "und was mein Herz erfreut!", "tokens": ["und", "was", "mein", "Herz", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Di\u00df ist ein Teil der Zeit,", "tokens": ["Di\u00df", "ist", "ein", "Teil", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "der ich mich r\u00fchmen kan.", "tokens": ["der", "ich", "mich", "r\u00fch\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}