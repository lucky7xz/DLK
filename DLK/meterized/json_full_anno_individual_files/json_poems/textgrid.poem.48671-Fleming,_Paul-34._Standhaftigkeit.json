{"textgrid.poem.48671": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "34. Standhaftigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag denn kein Rat und Trost mir lindern meine Pein?", "tokens": ["Mag", "denn", "kein", "Rat", "und", "Trost", "mir", "lin\u00b7dern", "mei\u00b7ne", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "KON", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie mu\u00df erlitten sein,", "tokens": ["Sie", "mu\u00df", "er\u00b7lit\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "so will ich, wie sichs f\u00fcgt,", "tokens": ["so", "will", "ich", ",", "wie", "sichs", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mit Allem sein vergn\u00fcgt.", "tokens": ["mit", "Al\u00b7lem", "sein", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer sich entsetzt, der liegt;", "tokens": ["Wer", "sich", "ent\u00b7setzt", ",", "der", "liegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "ich wag es k\u00fchne drein.", "tokens": ["ich", "wag", "es", "k\u00fch\u00