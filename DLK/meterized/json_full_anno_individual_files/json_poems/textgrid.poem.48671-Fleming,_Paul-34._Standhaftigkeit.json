ct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Pein, Trauren, Not und Qual und wie ihr andern hei\u00dft,", "tokens": ["Pein", ",", "Trau\u00b7ren", ",", "Not", "und", "Qual", "und", "wie", "ihr", "an\u00b7dern", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die ihr so auf mich rei\u00dft,", "tokens": ["die", "ihr", "so", "auf", "mich", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "seid noch so arg gesinnt,", "tokens": ["seid", "noch", "so", "arg", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "tut Alles, was ihr k\u00fcnnt',", "tokens": ["tut", "Al\u00b7les", ",", "was", "ihr", "k\u00fcnnt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "hier ist er, der gewinnt,", "tokens": ["hier", "ist", "er", ",", "der", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "hier steht er, der euch schmei\u00dft.", "tokens": ["hier", "steht", "er", ",", "der", "euch", "schmei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "wor