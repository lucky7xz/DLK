{"textgrid.poem.24864": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Jungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halbdunkel schon \u00fcber den Th\u00e4lern;", "tokens": ["Halb\u00b7dun\u00b7kel", "schon", "\u00fc\u00b7ber", "den", "Th\u00e4\u00b7lern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wolken, in schwerem Zuge", "tokens": ["Wol\u00b7ken", ",", "in", "schwe\u00b7rem", "Zu\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Von Klippe zu Klippe sich w\u00e4lzend;", "tokens": ["Von", "Klip\u00b7pe", "zu", "Klip\u00b7pe", "sich", "w\u00e4l\u00b7zend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Um mich zerrissene Schluchten", "tokens": ["Um", "mich", "zer\u00b7ris\u00b7se\u00b7ne", "Schluch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und Meere von Stein, deren Wogen", "tokens": ["Und", "Mee\u00b7re", "von", "Stein", ",", "de\u00b7ren", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "$,", "PRELAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Seit dem letzten Weltorkan nicht mehr branden;", "tokens": ["Seit", "dem", "letz\u00b7ten", "Welt\u00b7or\u00b7kan", "nicht", "mehr", "bran\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hin schweift mein Blick", "tokens": ["Hin", "schweift", "mein", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ueber Oeden, nur von Adlern bewohnt,", "tokens": ["Ue\u00b7ber", "O\u00b7e\u00b7den", ",", "nur", "von", "Ad\u00b7lern", "be\u00b7wohnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Empor zu den Felsensteilen,", "tokens": ["Em\u00b7por", "zu", "den", "Fel\u00b7sen\u00b7stei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wo die Riesentannen,", "tokens": ["Wo", "die", "Rie\u00b7sen\u00b7tan\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Gleich Giganten der Vorzeit", "tokens": ["Gleich", "Gi\u00b7gan\u00b7ten", "der", "Vor\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Hoch und h\u00f6her im Himmelssturme klimmend,", "tokens": ["Hoch", "und", "h\u00f6\u00b7her", "im", "Him\u00b7mels\u00b7stur\u00b7me", "klim\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Sich im wallenden Dunste verlieren.", "tokens": ["Sich", "im", "wal\u00b7len\u00b7den", "Duns\u00b7te", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Doch sieh! zu wirbeln, zu wogen", "tokens": ["Doch", "sieh", "!", "zu", "wir\u00b7beln", ",", "zu", "wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Beginnt das Gew\u00f6lk;", "tokens": ["Be\u00b7ginnt", "das", "Ge\u00b7w\u00f6lk", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Nebeldecke zerrei\u00dft,", "tokens": ["Die", "Ne\u00b7bel\u00b7de\u00b7cke", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und durch die st\u00e4ubenden Flocken", "tokens": ["Und", "durch", "die", "st\u00e4u\u00b7ben\u00b7den", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Fern in der blauen Unendlichkeit \u2013", "tokens": ["Fern", "in", "der", "blau\u00b7en", "Un\u00b7end\u00b7lich\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Welcher Silberglanz,", "tokens": ["Wel\u00b7cher", "Sil\u00b7ber\u00b7glanz", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Das Auge mit Strahlenschimmer blendend!", "tokens": ["Das", "Au\u00b7ge", "mit", "Strah\u00b7len\u00b7schim\u00b7mer", "blen\u00b7dend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie ist es, sie ist's, der Berge hohe K\u00f6nigin,", "tokens": ["Sie", "ist", "es", ",", "sie", "ist's", ",", "der", "Ber\u00b7ge", "ho\u00b7he", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPER", "VAFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Auf ihrem Gletscherthrone,", "tokens": ["Auf", "ih\u00b7rem", "Glet\u00b7scher\u00b7thro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Hoch \u00fcber die Erde den m\u00e4chtigen Scheitel erhebend,", "tokens": ["Hoch", "\u00fc\u00b7ber", "die", "Er\u00b7de", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Schei\u00b7tel", "er\u00b7he\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.11": {"text": "Die riesigen Glieder", "tokens": ["Die", "rie\u00b7si\u00b7gen", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Von Schneegewanden umwallt.", "tokens": ["Von", "Schnee\u00b7ge\u00b7wan\u00b7den", "um\u00b7wallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Schon schweigend zu ihren F\u00fc\u00dfen", "tokens": ["Schon", "schwei\u00b7gend", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lagert die Nacht;", "tokens": ["La\u00b7gert", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch weithin im Strahle der sinkenden Sonne", "tokens": ["Doch", "weit\u00b7hin", "im", "Strah\u00b7le", "der", "sin\u00b7ken\u00b7den", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Blitzt auf ihrem Haupt die Demantenkrone,", "tokens": ["Blitzt", "auf", "ih\u00b7rem", "Haupt", "die", "De\u00b7man\u00b7ten\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und, in Nebel zerflatternd, enth\u00fcllt", "tokens": ["Und", ",", "in", "Ne\u00b7bel", "zer\u00b7flat\u00b7ternd", ",", "ent\u00b7h\u00fcllt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Der Schleier das majest\u00e4tische Antlitz.", "tokens": ["Der", "Schlei\u00b7er", "das", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7sche", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ueber die Stirn ihr gleitet", "tokens": ["Ue\u00b7ber", "die", "Stirn", "ihr", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Bleich und golden rot", "tokens": ["Bleich", "und", "gol\u00b7den", "rot"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ein wechselnder Schimmer.", "tokens": ["Ein", "wech\u00b7seln\u00b7der", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Pl\u00f6tzlich erblassend", "tokens": ["Pl\u00f6tz\u00b7lich", "er\u00b7blas\u00b7send"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Vor den g\u00e4hnenden Tiefen des Alls,", "tokens": ["Vor", "den", "g\u00e4h\u00b7nen\u00b7den", "Tie\u00b7fen", "des", "Alls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "In die der Blick ihr hinunterstarrt,", "tokens": ["In", "die", "der", "Blick", "ihr", "hin\u00b7un\u00b7ter\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Scheint sie zur\u00fcckzubeben;", "tokens": ["Scheint", "sie", "zu\u00b7r\u00fcck\u00b7zu\u00b7be\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Dann wieder umfliegt", "tokens": ["Dann", "wie\u00b7der", "um\u00b7fliegt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Ein rosiger Glanz ihr die Z\u00fcge,", "tokens": ["Ein", "ro\u00b7si\u00b7ger", "Glanz", "ihr", "die", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Wie Widerschein von Gedanken und Tr\u00e4umen,", "tokens": ["Wie", "Wi\u00b7der\u00b7schein", "von", "Ge\u00b7dan\u00b7ken", "und", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Die ihr durch die Seele ziehen.", "tokens": ["Die", "ihr", "durch", "die", "See\u00b7le", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Giebt sie mit Geistern anderer Welten", "tokens": ["Giebt", "sie", "mit", "Geis\u00b7tern", "an\u00b7de\u00b7rer", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sich Flammenzeichen?", "tokens": ["Sich", "Flam\u00b7men\u00b7zei\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Oder gewahrt ihr Auge", "tokens": ["O\u00b7der", "ge\u00b7wahrt", "ihr", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri