{"textgrid.poem.44592": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[so soll die Kunst euch denn belehren?]", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So soll die Kunst euch denn belehren?", "tokens": ["So", "soll", "die", "Kunst", "euch", "denn", "be\u00b7leh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kunst ist kaum im Denken frei.", "tokens": ["Die", "Kunst", "ist", "kaum", "im", "Den\u00b7ken", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kann des Angenehmen nicht entbehren", "tokens": ["Sie", "kann", "des", "An\u00b7ge\u00b7neh\u00b7men", "nicht", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mischt es ihren Lehren bei.", "tokens": ["Und", "mischt", "es", "ih\u00b7ren", "Leh\u00b7ren", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dadurch ger\u00e4t der Satz ins Schiefe,", "tokens": ["Da\u00b7durch", "ge\u00b7r\u00e4t", "der", "Satz", "ins", "Schie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Wahrheit ist denn doch gerad;", "tokens": ["Und", "Wahr\u00b7heit", "ist", "denn", "doch", "ge\u00b7rad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Ungeme\u00dfne ist nicht ihre Tiefe,", "tokens": ["Das", "Un\u00b7ge\u00b7me\u00df\u00b7ne", "ist", "nicht", "ih\u00b7re", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre H\u00f6he nicht ihr Pfad.", "tokens": ["Und", "ih\u00b7re", "H\u00f6\u00b7he", "nicht", "ihr", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Stellt dar. Teilt nur mit Gott die Ehre,", "tokens": ["Stellt", "dar", ".", "Teilt", "nur", "mit", "Gott", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "NN", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr seid dann wahr f\u00fcr jeden Fall.", "tokens": ["Ihr", "seid", "dann", "wahr", "f\u00fcr", "je\u00b7den", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist, hat in sich selbst die Lehre,", "tokens": ["Was", "ist", ",", "hat", "in", "sich", "selbst", "die", "Leh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos"