{"textgrid.poem.33826": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Am Fenster", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine M\u00fccke, wei\u00df gekleidet,", "tokens": ["Ei\u00b7ne", "M\u00fc\u00b7cke", ",", "wei\u00df", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will hinaus durchs Fensterglas,", "tokens": ["Will", "hin\u00b7aus", "durchs", "Fens\u00b7ter\u00b7glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mein Zimmer ihr entleidet?", "tokens": ["Ist", "mein", "Zim\u00b7mer", "ihr", "ent\u00b7lei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sucht sie drau\u00dfen was?", "tokens": ["Sucht", "sie", "drau\u00b7\u00dfen", "was", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Achtet sie so hoch die Ehre,", "tokens": ["Ach\u00b7tet", "sie", "so", "hoch", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie dort ein Frosch verzehre?", "tokens": ["Da\u00df", "sie", "dort", "ein", "Frosch", "ver\u00b7zeh\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df ein Sp\u00e4tzlein sie verspeise,", "tokens": ["Da\u00df", "ein", "Sp\u00e4tz\u00b7lein", "sie", "ver\u00b7spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie in der Spinne Netz", "tokens": ["Da\u00df", "sie", "in", "der", "Spin\u00b7ne", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sich verirr' auf ihrer Reise?", "tokens": ["Sich", "ver\u00b7irr'", "auf", "ih\u00b7rer", "Rei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laut Naturgesetz", "tokens": ["Laut", "Na\u00b7tur\u00b7ge\u00b7setz"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Zappelt sie, vor Lust zum Sterben,", "tokens": ["Zap\u00b7pelt", "sie", ",", "vor", "Lust", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ihr eigenes Verderben.", "tokens": ["In", "ihr", "ei\u00b7ge\u00b7nes", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber von dem gleichen Drange", "tokens": ["A\u00b7ber", "von", "dem", "glei\u00b7chen", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die ganze Welt erf\u00fcllt;", "tokens": ["Ist", "die", "gan\u00b7ze", "Welt", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem eignen Untergange,", "tokens": ["Nach", "dem", "eig\u00b7nen", "Un\u00b7ter\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor sich selbst verh\u00fcllt,", "tokens": ["Vor", "sich", "selbst", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dr\u00e4ngt sich Gro\u00df und Klein auf Erden,", "tokens": ["Dr\u00e4ngt", "sich", "Gro\u00df", "und", "Klein", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles will vernichtet werden.", "tokens": ["Al\u00b7les", "will", "ver\u00b7nich\u00b7tet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach dem R\u00e4cher, nach dem Falle", "tokens": ["Nach", "dem", "R\u00e4\u00b7cher", ",", "nach", "dem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S