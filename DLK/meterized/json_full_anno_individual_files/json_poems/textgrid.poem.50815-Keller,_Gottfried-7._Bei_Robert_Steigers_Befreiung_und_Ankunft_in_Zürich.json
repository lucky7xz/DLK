{"textgrid.poem.50815": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7. Bei Robert Steigers Befreiung und Ankunft in Z\u00fcrich", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit deinem Adelsbriefe wohl versehen,", "tokens": ["Mit", "dei\u00b7nem", "A\u00b7dels\u00b7brie\u00b7fe", "wohl", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Todesurteil mit dem argen Ri\u00df,", "tokens": ["Dem", "To\u00b7des\u00b7ur\u00b7teil", "mit", "dem", "ar\u00b7gen", "Ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sehn wir dich jugendlich und stark erstehen", "tokens": ["Sehn", "wir", "dich", "ju\u00b7gend\u00b7lich", "und", "stark", "er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus deines Grabes kalter Finsternis.", "tokens": ["Aus", "dei\u00b7nes", "Gra\u00b7bes", "kal\u00b7ter", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Ungl\u00fccks Feuertaufe auf dem Haupte,", "tokens": ["Des", "Un\u00b7gl\u00fccks", "Feu\u00b7er\u00b7tau\u00b7fe", "auf", "dem", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den letzten Kettenring noch an der Hand:", "tokens": ["Den", "letz\u00b7ten", "Ket\u00b7ten\u00b7ring", "noch", "an", "der", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So schreitest du durch dieses jungbelaubte", "tokens": ["So", "schrei\u00b7test", "du", "durch", "die\u00b7ses", "jung\u00b7be\u00b7laub\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und doch so tief gebeugte Vaterland!", "tokens": ["Und", "doch", "so", "tief", "ge\u00b7beug\u00b7te", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wo du gehst, da weckst du auf den Bergen", "tokens": ["Und", "wo", "du", "gehst", ",", "da", "weckst", "du", "auf", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die hellen Freudenfeuer ohne Zahl!", "tokens": ["Die", "hel\u00b7len", "Freu\u00b7den\u00b7feu\u00b7er", "oh\u00b7ne", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Geblendet noch vom unverhofften Strahl:", "tokens": ["Ge\u00b7blen\u00b7det", "noch", "vom", "un\u00b7ver\u00b7hoff\u00b7ten", "Strahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Apostat, des Name nun zertreten", "tokens": ["Der", "A\u00b7pos\u00b7tat", ",", "des", "Na\u00b7me", "nun", "zer\u00b7tre\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Staube an des Volkes Sohlen klebt,", "tokens": ["Im", "Stau\u00b7be", "an", "des", "Vol\u00b7kes", "Soh\u00b7len", "klebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Indes den deinen es mit lautem Beten", "tokens": ["In\u00b7des", "den", "dei\u00b7nen", "es", "mit", "lau\u00b7tem", "Be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PPOSAT", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und kindlich dankbar zu den Sternen hebt!", "tokens": ["Und", "kind\u00b7lich", "dank\u00b7bar", "zu", "den", "Ster\u00b7nen", "hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es gr\u00fc\u00dfe dich das goldne Licht der Sonne,", "tokens": ["Es", "gr\u00fc\u00b7\u00dfe", "dich", "das", "gold\u00b7ne", "Licht", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich gr\u00fc\u00dft die Freiheit und das Vaterland!", "tokens": ["Dich", "gr\u00fc\u00dft", "die", "Frei\u00b7heit", "und", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es gr\u00fc\u00dfen dich mit hei\u00dfem Schlag der Wonne", "tokens": ["Es", "gr\u00fc\u00b7\u00dfen", "dich", "mit", "hei\u00b7\u00dfem", "Schlag", "der", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Viel tausend Herzen, freudig zugewandt!", "tokens": ["Viel", "tau\u00b7send", "Her\u00b7zen", ",", "freu\u00b7dig", "zu\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nimm hin in vollem Ma\u00df des Volkes Liebe", "tokens": ["Nimm", "hin", "in", "vol\u00b7lem", "Ma\u00df", "des", "Vol\u00b7kes", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und seinen Dank, den es den Helden zollt:", "tokens": ["Und", "sei\u00b7nen", "Dank", ",", "den", "es", "den", "Hel\u00b7den", "zollt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der M\u00e4nner L\u00e4rm und jubelndes Getriebe,", "tokens": ["Der", "M\u00e4n\u00b7ner", "L\u00e4rm", "und", "ju\u00b7beln\u00b7des", "Ge\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Weibes Tr\u00e4ne, die im stillen rollt!", "tokens": ["Des", "Wei\u00b7bes", "Tr\u00e4\u00b7ne", ",", "die", "im", "stil\u00b7len", "rollt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nimm hin die Lieder und die Festges\u00e4nge!", "tokens": ["Nimm", "hin", "die", "Lie\u00b7der", "und", "die", "Fest\u00b7ge\u00b7s\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es lauscht ein heil'ger, starker Zorn darin!", "tokens": ["Es", "lauscht", "ein", "heil'\u00b7ger", ",", "star\u00b7ker", "Zorn", "da\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die bittre Klage in dem Lustgedr\u00e4nge,", "tokens": ["Die", "bitt\u00b7re", "Kla\u00b7ge", "in", "dem", "Lust\u00b7ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Dorn, den diese Rose birgt, nimm hin!", "tokens": ["Den", "Dorn", ",", "den", "die\u00b7se", "Ro\u00b7se", "birgt", ",", "nimm", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn was dem m\u00fcden Volk das Herz durchzittert,", "tokens": ["Denn", "was", "dem", "m\u00fc\u00b7den", "Volk", "das", "Herz", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Legt's heimlich in die Gr\u00fc\u00dfe mit hinein;", "tokens": ["Legt's", "heim\u00b7lich", "in", "die", "Gr\u00fc\u00b7\u00dfe", "mit", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ob's nun in Freude oder Leid gewittert:", "tokens": ["Ob's", "nun", "in", "Freu\u00b7de", "o\u00b7der", "Leid", "ge\u00b7wit\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es wird nicht minder ein Gewitter sein!", "tokens": ["Es", "wird", "nicht", "min\u00b7der", "ein", "Ge\u00b7wit\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mit deinem Adelsbriefe wohl versehen,", "tokens": ["Mit", "dei\u00b7nem", "A\u00b7dels\u00b7brie\u00b7fe", "wohl", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct