{"textgrid.poem.55279": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gro\u00dfer Brahma! nun erkenn ich,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfer Brahma! nun erkenn ich,", "tokens": ["Gro\u00b7\u00dfer", "Brah\u00b7ma", "!", "nun", "er\u00b7kenn", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du Sch\u00f6pfer bist der Welten!", "tokens": ["Da\u00df", "du", "Sch\u00f6p\u00b7fer", "bist", "der", "Wel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich als meinen Herrscher nenn ich,", "tokens": ["Dich", "als", "mei\u00b7nen", "Herr\u00b7scher", "nenn", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn du l\u00e4ssest alle gelten.", "tokens": ["Denn", "du", "l\u00e4s\u00b7sest", "al\u00b7le", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und verschlie\u00dfest auch dem", "tokens": ["Und", "ver\u00b7schlie\u00b7\u00dfest", "auch", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Letzten Keines von den tausend Ohren;", "tokens": ["Letz\u00b7ten", "Kei\u00b7nes", "von", "den", "tau\u00b7send", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "CARD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Uns, die tief Herabgesetzten,", "tokens": ["Uns", ",", "die", "tief", "Her\u00b7ab\u00b7ge\u00b7setz\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle hast du neu geboren.", "tokens": ["Al\u00b7le", "hast", "du", "neu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wendet euch zu dieser Frauen,", "tokens": ["Wen\u00b7det", "euch", "zu", "die\u00b7ser", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Schmerz zur G\u00f6ttin wandelt;", "tokens": ["Die", "der", "Schmerz", "zur", "G\u00f6t\u00b7tin", "wan\u00b7delt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun beharr ich anzuschauen", "tokens": ["Nun", "be\u00b7harr", "ich", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den, der einzig wirkt und handelt.", "tokens": ["Den", ",", "der", "ein\u00b7zig", "wirkt", "und", "han\u00b7delt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,",