{"dta.poem.18224": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Auf moos'ger Bank am Buchenhag", "tokens": ["Auf", "moos'\u00b7ger", "Bank", "am", "Bu\u00b7chen\u00b7hag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sann begl\u00fcckt und selbstvergessen", "tokens": ["Und", "sann", "be\u00b7gl\u00fcckt", "und", "selbst\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem R\u00e4thsel deines Wesens nach!", "tokens": ["Dem", "R\u00e4th\u00b7sel", "dei\u00b7nes", "We\u00b7sens", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann sang am waldverschwiegnen Orte", "tokens": ["Dann", "sang", "am", "wald\u00b7ver\u00b7schwieg\u00b7nen", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr hohes Lied die Maienfee,", "tokens": ["Ihr", "ho\u00b7hes", "Lied", "die", "Mai\u00b7en\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jedes ihrer s\u00fc\u00dfen Worte", "tokens": ["Und", "je\u00b7des", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Fiel mir ins Herz wie Bl\u00fcthenschnee;", "tokens": ["Fiel", "mir", "ins", "Herz", "wie", "Bl\u00fc\u00b7then\u00b7schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jedes ihrer s\u00fc\u00dfen Worte", "tokens": ["Und", "je\u00b7des", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Klang mir wie Deutung deines Seins", "tokens": ["Klang", "mir", "wie", "Deu\u00b7tung", "dei\u00b7nes", "Seins"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KOKOM", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Und golden that sich auf die Pforte", "tokens": ["Und", "gol\u00b7den", "that", "sich", "auf", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und ich und du, wir waren Eins!", "tokens": ["Und", "ich", "und", "du", ",", "wir", "wa\u00b7ren", "Eins", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPER", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und doch; wenn du dann kamst und l\u00e4chelnd", "tokens": ["Und", "doch", ";", "wenn", "du", "dann", "kamst", "und", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "KOUS", "PPER", "ADV", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Anmuth dir zur Seite ging,", "tokens": ["Die", "An\u00b7muth", "dir", "zur", "Sei\u00b7te", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und s\u00fc\u00dfer als der Maiwind f\u00e4chelnd", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "als", "der", "Mai\u00b7wind", "f\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein weicher Odem mich umfing:", "tokens": ["Dein", "wei\u00b7cher", "O\u00b7dem", "mich", "um\u00b7fing", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann war dahin, was kaum gewesen", "tokens": ["Dann", "war", "da\u00b7hin", ",", "was", "kaum", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PAV", "$,", "PRELS", "ADV", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was nur dunkel mir geschwant,", "tokens": ["Und", "was", "nur", "dun\u00b7kel", "mir", "ge\u00b7schwant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinen Augen konnt ich's lesen,", "tokens": ["In", "dei\u00b7nen", "Au\u00b7gen", "konnt", "ich's", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Wundern, die ich nie geahnt;", "tokens": ["Von", "Wun\u00b7dern", ",", "die", "ich", "nie", "ge\u00b7ahnt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In deinen Augen konnt ich's lesen,", "tokens": ["In", "dei\u00b7nen", "Au\u00b7gen", "konnt", "ich's", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich gewann, was ich verlor,", "tokens": ["Was", "ich", "ge\u00b7wann", ",", "was", "ich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und s\u00fc\u00dferschreckt schien mir dein Wesen", "tokens": ["Und", "s\u00fc\u00b7\u00dfer\u00b7schreckt", "schien", "mir", "dein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur r\u00e4thselhafter als zuvor!", "tokens": ["Nur", "r\u00e4th\u00b7sel\u00b7haf\u00b7ter", "als", "zu\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}