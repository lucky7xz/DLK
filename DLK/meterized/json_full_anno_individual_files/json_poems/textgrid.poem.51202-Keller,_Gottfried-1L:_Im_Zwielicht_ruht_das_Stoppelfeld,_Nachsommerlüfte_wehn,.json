{"textgrid.poem.51202": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Zwielicht ruht das Stoppelfeld, Nachsommerl\u00fcfte wehn,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Zwielicht ruht das Stoppelfeld, Nachsommerl\u00fcfte wehn,", "tokens": ["Im", "Zwie\u00b7licht", "ruht", "das", "Stop\u00b7pel\u00b7feld", ",", "Nach\u00b7som\u00b7mer\u00b7l\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und fliegend \u00fcber das falbe Land ein J\u00fcngling ist zu sehn;", "tokens": ["Und", "flie\u00b7gend", "\u00fc\u00b7ber", "das", "fal\u00b7be", "Land", "ein", "J\u00fcng\u00b7ling", "ist", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sein Kranz ist wie von Tr\u00e4nen schwer, des Jahres letztem Tau,", "tokens": ["Sein", "Kranz", "ist", "wie", "von", "Tr\u00e4\u00b7nen", "schwer", ",", "des", "Jah\u00b7res", "letz\u00b7tem", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "APPR", "NN", "ADJD", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Verfolgt und zitternd flieht er hin durch Morgend\u00e4mmergrau.", "tokens": ["Ver\u00b7folgt", "und", "zit\u00b7ternd", "flieht", "er", "hin", "durch", "Mor\u00b7gen\u00b7d\u00e4m\u00b7mer\u00b7grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "In seines Mantels Seidengr\u00fcn verbirgt und h\u00fcllt er scheu", "tokens": ["In", "sei\u00b7nes", "Man\u00b7tels", "Sei\u00b7den\u00b7gr\u00fcn", "ver\u00b7birgt", "und", "h\u00fcllt", "er", "scheu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Des Krieges grimmes Schwert, das er geh\u00fctet fromm und treu;", "tokens": ["Des", "Krie\u00b7ges", "grim\u00b7mes", "Schwert", ",", "das", "er", "ge\u00b7h\u00fc\u00b7tet", "fromm", "und", "treu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Doch dies zu holen hat sich schon die Zwietracht aufgemacht,", "tokens": ["Doch", "dies", "zu", "ho\u00b7len", "hat", "sich", "schon", "die", "Zwiet\u00b7racht", "auf\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKZU", "VVINF", "VAFIN", "PRF", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Drum \u00fcber die Stoppelheide floh das Kind die ganze Nacht.", "tokens": ["Drum", "\u00fc\u00b7ber", "die", "Stop\u00b7pel\u00b7hei\u00b7de", "floh", "das", "Kind", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Es sucht des Berges dunkle Schlucht und eilet todesbang", "tokens": ["Es", "sucht", "des", "Ber\u00b7ges", "dunk\u00b7le", "Schlucht", "und", "ei\u00b7let", "to\u00b7des\u00b7bang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Durch Wurzeln und Gestein hinan den rauschenden Bach entlang,", "tokens": ["Durch", "Wur\u00b7zeln", "und", "Ge\u00b7stein", "hi\u00b7nan", "den", "rau\u00b7schen\u00b7den", "Bach", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und im Geschiebe h\u00f6rt es schon der G\u00f6ttin wilden Tritt,", "tokens": ["Und", "im", "Ge\u00b7schie\u00b7be", "h\u00f6rt", "es", "schon", "der", "G\u00f6t\u00b7tin", "wil\u00b7den", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Als es, wie ein gehetztes Reh, schnell in das Wasser glitt,", "tokens": ["Als", "es", ",", "wie", "ein", "ge\u00b7hetz\u00b7tes", "Reh", ",", "schnell", "in", "das", "Was\u00b7ser", "glitt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Schnell in die Flut, wo ihre Wucht sich von den Felsen schwingt,", "tokens": ["Schnell", "in", "die", "Flut", ",", "wo", "ih\u00b7re", "Wucht", "sich", "von", "den", "Fel\u00b7sen", "schwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Da duckt es unter das Wurzelwerk, vom wei\u00dfen Gischt umringt;", "tokens": ["Da", "duckt", "es", "un\u00b7ter", "das", "Wur\u00b7zel\u00b7werk", ",", "vom", "wei\u00b7\u00dfen", "Gischt", "um\u00b7ringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sie aber teilt's mit straffem Arm, ergl\u00fchend vorgebeugt:", "tokens": ["Sie", "a\u00b7ber", "teilt's", "mit", "straf\u00b7fem", "Arm", ",", "er\u00b7gl\u00fc\u00b7hend", "vor\u00b7ge\u00b7beugt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "APPR", "ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "\u00bbgib mir das Schwert, du weichlich Kind, in falscher Eh gezeugt!", "tokens": ["\u00bb", "gib", "mir", "das", "Schwert", ",", "du", "weich\u00b7lich", "Kind", ",", "in", "fal\u00b7scher", "Eh", "ge\u00b7zeugt", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "$,", "PPER", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Des Jahres Frucht ist eingebracht und m\u00fc\u00dfig liegt das Feld,", "tokens": ["Des", "Jah\u00b7res", "Frucht", "ist", "ein\u00b7ge\u00b7bracht", "und", "m\u00fc\u00b7\u00dfig", "liegt", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ges\u00e4ttigt ruht der Bauer aus, der M\u00e4kler z\u00e4hlt sein Geld,", "tokens": ["Ge\u00b7s\u00e4t\u00b7tigt", "ruht", "der", "Bau\u00b7er", "aus", ",", "der", "M\u00e4k\u00b7ler", "z\u00e4hlt", "sein", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "In schweren Trauben reift der Wein und reizt zum \u00dcbermut,", "tokens": ["In", "schwe\u00b7ren", "Trau\u00b7ben", "reift", "der", "Wein", "und", "reizt", "zum", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Bald jagt er mir im Volk empor das eingeschlafne Blut!", "tokens": ["Bald", "jagt", "er", "mir", "im", "Volk", "em\u00b7por", "das", "ein\u00b7ge\u00b7schlaf\u00b7ne", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Was schaust du mich so flehend an, du s\u00fc\u00dfes Engelherz?", "tokens": ["Was", "schaust", "du", "mich", "so", "fle\u00b7hend", "an", ",", "du", "s\u00fc\u00b7\u00dfes", "En\u00b7gel\u00b7herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ich bin das Weh, das m\u00e4chtiger ist als all dein eitler Schmerz!", "tokens": ["Ich", "bin", "das", "Weh", ",", "das", "m\u00e4ch\u00b7ti\u00b7ger", "ist", "als", "all", "dein", "eit\u00b7ler", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "VAFIN", "KOKOM", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ich bin die Wut und Unvernunft, die wie die H\u00f6lle brennt,", "tokens": ["Ich", "bin", "die", "Wut", "und", "Un\u00b7ver\u00b7nunft", ",", "die", "wie", "die", "H\u00f6l\u00b7le", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der D\u00e4mon, der sich weinend selbst den b\u00f6sen Willen nennt!", "tokens": ["Der", "D\u00e4\u00b7mon", ",", "der", "sich", "wei\u00b7nend", "selbst", "den", "b\u00f6\u00b7sen", "Wil\u00b7len", "nennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Gib her das Schwert!\u00ab Und wie der Knauf aus den Gew\u00e4ndern blickt,", "tokens": ["Gib", "her", "das", "Schwert", "!", "\u00ab", "Und", "wie", "der", "Knauf", "aus", "den", "Ge\u00b7w\u00e4n\u00b7dern", "blickt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$.", "$(", "KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Hat blitzesschnell die sehnige Hand der Eris ihn umstrickt;", "tokens": ["Hat", "blit\u00b7zes\u00b7schnell", "die", "seh\u00b7ni\u00b7ge", "Hand", "der", "E\u00b7ris", "ihn", "um\u00b7strickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sie rei\u00dft durch beide H\u00e4nd dem Kind den Stahl, der l\u00fcstern blinkt,", "tokens": ["Sie", "rei\u00dft", "durch", "bei\u00b7de", "H\u00e4nd", "dem", "Kind", "den", "Stahl", ",", "der", "l\u00fcs\u00b7tern", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Da\u00df es mit den zerschnittenen lautlos zusammensinkt.", "tokens": ["Da\u00df", "es", "mit", "den", "zer\u00b7schnit\u00b7te\u00b7nen", "laut\u00b7los", "zu\u00b7sam\u00b7men\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Nun steht sie auf des Berges Grat und schl\u00e4gt den roten Schein", "tokens": ["Nun", "steht", "sie", "auf", "des", "Ber\u00b7ges", "Grat", "und", "schl\u00e4gt", "den", "ro\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Der Morgensonne mit dem Schwert tief in die Welt hinein.", "tokens": ["Der", "Mor\u00b7gen\u00b7son\u00b7ne", "mit", "dem", "Schwert", "tief", "in", "die", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "In wilder Sch\u00f6nheit atmet sie, wie Brandung wogt die Brust,", "tokens": ["In", "wil\u00b7der", "Sch\u00f6n\u00b7heit", "at\u00b7met", "sie", ",", "wie", "Bran\u00b7dung", "wogt", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und in den T\u00e4lern wacht es auf mit dumpfer Todeslust!", "tokens": ["Und", "in", "den", "T\u00e4\u00b7lern", "wacht", "es", "auf", "mit", "dum\u00b7pfer", "To\u00b7des\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Im Zwielicht ruht das Stoppelfeld, Nachsommerl\u00fcfte wehn,", "tokens": ["Im", "Zwie\u00b7licht", "ruht", "das", "Stop\u00b7pel\u00b7feld", ",", "Nach\u00b7som\u00b7mer\u00b7l\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und fliegend \u00fcber das falbe Land ein J\u00fcngling ist zu sehn;", "tokens": ["Und", "flie\u00b7gend", "\u00fc\u00b7ber", "das", "fal\u00b7be", "Land", "ein", "J\u00fcng\u00b7ling", "ist", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sein Kranz ist wie von Tr\u00e4nen schwer, des Jahres letztem Tau,", "tokens": ["Sein", "Kranz", "ist", "wie", "von", "Tr\u00e4\u00b7nen", "schwer", ",", "des", "Jah\u00b7res", "letz\u00b7tem", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "APPR", "NN", "ADJD", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Verfolgt und zitternd flieht er hin durch Morgend\u00e4mmergrau.", "tokens": ["Ver\u00b7folgt", "und", "zit\u00b7ternd", "flieht", "er", "hin", "durch", "Mor\u00b7gen\u00b7d\u00e4m\u00b7mer\u00b7grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "In seines Mantels Seidengr\u00fcn verbirgt und h