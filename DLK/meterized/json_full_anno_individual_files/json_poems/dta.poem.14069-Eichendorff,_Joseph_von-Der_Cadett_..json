{"dta.poem.14069": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Cadett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da kann ich von Gl\u00fccke sagen,", "tokens": ["Da", "kann", "ich", "von", "Gl\u00fc\u00b7cke", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn w\u00e4r' sie die Sch\u00f6nste just,", "tokens": ["Denn", "w\u00e4r'", "sie", "die", "Sch\u00f6ns\u00b7te", "just", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "M\u00fc\u00dft' ich mit Allen mich schlagen", "tokens": ["M\u00fc\u00dft'", "ich", "mit", "Al\u00b7len", "mich", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NE", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um die Eine nach Herzenslust.", "tokens": ["Um", "die", "Ei\u00b7ne", "nach", "Her\u00b7zens\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}