{"dta.poem.19294": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sobald dem Menschen wir die Freiheit zugestehn,", "tokens": ["So\u00b7bald", "dem", "Men\u00b7schen", "wir", "die", "Frei\u00b7heit", "zu\u00b7ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scheints um die g\u00f6ttliche Allwissenheit geschehn.", "tokens": ["Scheints", "um", "die", "g\u00f6tt\u00b7li\u00b7che", "All\u00b7wis\u00b7sen\u00b7heit", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Denn wenn die Gottheit wei\u00df, wohin mein Thun sich lenkt,", "tokens": ["Denn", "wenn", "die", "Got\u00b7theit", "wei\u00df", ",", "wo\u00b7hin", "mein", "Thun", "sich", "lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bin ich ja zu thun gezwungen, wie sie denkt.", "tokens": ["So", "bin", "ich", "ja", "zu", "thun", "ge\u00b7zwun\u00b7gen", ",", "wie", "sie", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der alte Meister sprach: das sei nur als ein Zeichen", "tokens": ["Der", "al\u00b7te", "Meis\u00b7ter", "sprach", ":", "das", "sei", "nur", "als", "ein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PDS", "VAFIN", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch angef\u00fchrt, wie weit des Menschen Kr\u00e4fte reichen,", "tokens": ["Euch", "an\u00b7ge\u00b7f\u00fchrt", ",", "wie", "weit", "des", "Men\u00b7schen", "Kr\u00e4f\u00b7te", "rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PWAV", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und da\u00df sein schwacher Witz sich lasse nicht verf\u00fchren,", "tokens": ["Und", "da\u00df", "sein", "schwa\u00b7cher", "Witz", "sich", "las\u00b7se", "nicht", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An unbegreifliche Geheimnisse zu r\u00fchren.", "tokens": ["An", "un\u00b7be\u00b7greif\u00b7li\u00b7che", "Ge\u00b7heim\u00b7nis\u00b7se", "zu", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}