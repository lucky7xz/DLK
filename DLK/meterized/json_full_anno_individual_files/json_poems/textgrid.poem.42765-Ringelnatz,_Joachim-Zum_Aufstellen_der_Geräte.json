{"textgrid.poem.42765": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Zum Aufstellen der Ger\u00e4te", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So unterwegs in einem sch\u00f6nen Hechtsprung", "tokens": ["So", "un\u00b7ter\u00b7wegs", "in", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Hechts\u00b7prung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Erblickte er das Licht der Welt, das Leben,", "tokens": ["Er\u00b7blick\u00b7te", "er", "das", "Licht", "der", "Welt", ",", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hat \u2013 obwohl er damals doch noch recht jung \u2013", "tokens": ["Und", "hat", "\u2013", "ob\u00b7wohl", "er", "da\u00b7mals", "doch", "noch", "recht", "jung", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sich doch sofort in Hilfsstellung begeben.", "tokens": ["Sich", "doch", "so\u00b7fort", "in", "Hilfs\u00b7stel\u00b7lung", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Kniesturz \u00fcbend und manch andre Tugend,", "tokens": ["Den", "Knies\u00b7turz", "\u00fc\u00b7bend", "und", "manch", "and\u00b7re", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Verging ihm eine turnerische Jugend", "tokens": ["Ver\u00b7ging", "ihm", "ei\u00b7ne", "tur\u00b7ne\u00b7ri\u00b7sche", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Wachen teils und teils im Traum", "tokens": ["Im", "Wa\u00b7chen", "teils", "und", "teils", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Freitags nachmittags am Schwebebaum.", "tokens": ["Und", "Frei\u00b7tags", "nach\u00b7mit\u00b7tags", "am", "Schwe\u00b7be\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vorturner wurde er und L\u00f6wenb\u00e4ndiger,", "tokens": ["Vor\u00b7tur\u00b7ner", "wur\u00b7de", "er", "und", "L\u00f6\u00b7wen\u00b7b\u00e4n\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seemann und Schornsteinfeger, Akrobat", "tokens": ["See\u00b7mann", "und", "Schorn\u00b7stein\u00b7fe\u00b7ger", ",", "Ak\u00b7ro\u00b7bat"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$,", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und schlie\u00dflich turnerischer Sachverst\u00e4ndiger", "toke