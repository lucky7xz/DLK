{"dta.poem.4370": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abbildung der Gottheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df GOtt, Den aller Ewigkeiten Unendlichkeiten", "tokens": ["Da\u00df", "Gott", ",", "Den", "al\u00b7ler", "E\u00b7wig\u00b7kei\u00b7ten", "Un\u00b7end\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "nicht verschr\u00e4nken,", "tokens": ["nicht", "ver\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Jm Bildni\u00df eines alten Mannes sey vorzustellen, zu ge-", "tokens": ["Jm", "Bild\u00b7ni\u00df", "ei\u00b7nes", "al\u00b7ten", "Man\u00b7nes", "sey", "vor\u00b7zu\u00b7stel\u00b7len", ",", "zu", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VAFIN", "VVIZU", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "denken,", "tokens": ["den\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ist ungereimt. Ein solches Bild erregt mir stets ein", "tokens": ["Ist", "un\u00b7ge\u00b7reimt", ".", "Ein", "sol\u00b7ches", "Bild", "er\u00b7regt", "mir", "stets", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$.", "ART", "PIAT", "NN", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "bitter Lachen.", "tokens": ["bit\u00b7ter", "La\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wer es recht \u00fcberlegt, erschrickt, da\u00df die Vernunft sich so", "tokens": ["Wer", "es", "recht", "\u00fc\u00b7ber\u00b7legt", ",", "er\u00b7schrickt", ",", "da\u00df", "die", "Ver\u00b7nunft", "sich", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VVPP", "$,", "VVPP", "$,", "KOUS", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "vergeht,", "tokens": ["ver\u00b7geht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Und sich vom unerme\u00dfnen Geist ein Bild zu machen un-", "tokens": ["Und", "sich", "vom", "un\u00b7er\u00b7me\u00df\u00b7nen", "Geist", "ein", "Bild", "zu", "ma\u00b7chen", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "tersteht,", "tokens": ["ter\u00b7steht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Da wir so gar von unserm Geist uns stets gesch\u00e4mt ein", "tokens": ["Da", "wir", "so", "gar", "von", "un\u00b7serm", "Geist", "uns", "stets", "ge\u00b7sch\u00e4mt", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bild zu machen.", "tokens": ["Bild", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}