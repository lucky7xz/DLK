": "Blitzfunken die N\u00fcster scho\u00df.", "tokens": ["Blitz\u00b7fun\u00b7ken", "die", "N\u00fcs\u00b7ter", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Dem Renner, dem war sie treu und hold,", "tokens": ["Dem", "Ren\u00b7ner", ",", "dem", "war", "sie", "treu", "und", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Silber war er gez\u00e4umt,", "tokens": ["Mit", "Sil\u00b7ber", "war", "er", "ge\u00b7z\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Beschlagen der Huf mit rotem Gold,", "tokens": ["Be\u00b7schla\u00b7gen", "der", "Huf", "mit", "ro\u00b7tem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Perlen der Gurt ges\u00e4umt.", "tokens": ["Mit", "Per\u00b7len", "der", "Gurt", "ge\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.17": {"line.1": {"text": "Und eh' die Sonne am Himmel schwamm,", "tokens": ["Und", "eh'", "die", "Son\u00b7ne", "am", "Him\u00b7mel", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"