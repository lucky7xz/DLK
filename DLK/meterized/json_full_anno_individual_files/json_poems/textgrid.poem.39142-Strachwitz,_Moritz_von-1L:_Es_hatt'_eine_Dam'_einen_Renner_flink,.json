 "line.4": {"text": "Der goldenen Halfter Bann.", "tokens": ["Der", "gol\u00b7de\u00b7nen", "Half\u00b7ter", "Bann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.22": {"line.1": {"text": "\u00bbmein rotes Ro\u00df, mein rasches Ro\u00df,", "tokens": ["\u00bb", "mein", "ro\u00b7tes", "Ro\u00df", ",", "mein", "ra\u00b7sches", "Ro\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut rette mich, oder nie!\u00ab", "tokens": ["Heut", "ret\u00b7te", "mich", ",", "o\u00b7der", "nie", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tief senkte das Ro\u00df, tief bog das Ro\u00df", "tokens": ["Tief", "senk\u00b7te", "das", "Ro\u00df", ",", "tief", "bog", "das", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor der Herrin das schlanke Knie.", "tokens": ["Vor", "der", "Her\u00b7rin", "das", "schlan\u00b7ke", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.23": {"line.1": {"text": "Und sah sie an gar bang und lang,", "tokens": ["Und", "sah", "sie", "an", "gar", "bang", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar traulich und flehentlich,", "tokens": ["Gar", "trau\u00b7lich", "und", "fle\u00b7hent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Dame sich auf den Renner schwang,", "tokens": ["Die", "Da\u00b7me", "sich", "auf", "den", "Ren\u00b7ner", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Renner von hinnen strich.", "tokens": ["Der", "Ren\u00b7ner", "von", "hin\u00b7nen", "strich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.24": {"line.1": {"text": "Die Schwalbe, die unten im Sturme glitt,", "tokens": ["Die", "Schwal\u00b7be", ",", "die", "un\u00b7ten", "im", "Stur\u00b7me", "glitt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie holt' ihn nimmer ein,", "tokens": ["Sie", "holt'", "ihn", "nim\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sturm, der oben auf Wolken ritt,", "tokens": ["Der", "Sturm", ",", "der", "o\u00b7ben", "auf", "Wol\u00b7ken", "ritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Keucht' \u00e4chzend hinterdrein.", "tokens": ["Keucht'", "\u00e4ch\u00b7zend", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Es steht ein Schlo\u00df im Elfenwald,", "tokens": ["Es", "steht", "ein", "Schlo\u00df", "im", "El\u00b7fen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein diamantenes Schlo\u00df,", "tokens": ["Ein", "di\u00b7a\u00b7man\u00b7te\u00b7nes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da stockt' es im Laufe, da macht' es halt,", "tokens": ["Da", "stockt'", "es", "im", "Lau\u00b7fe", ",", "da", "macht'", "es", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da stand es, das schnelle Ro\u00df.", "tokens": ["Da", "stand", "es", ",", "das", "schnel\u00b7le", "Ro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.26": {"line.1": {"text": "Und als sie ihm dankend den Hals umfing,", "tokens": ["Und", "als", "sie", "ihm", "dan\u00b7kend", "den", "Hals", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es koste mit Mund und Hand,", "tokens": ["Es", "kos\u00b7te", "mit", "Mund", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Statt des Renners der Dame im Arme hing", "tokens": ["Statt", "des", "Ren\u00b7ners", "der", "Da\u00b7me", "im", "Ar\u00b7me", "hing"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der K\u00f6nig von Elfenland:", "tokens": ["Der", "K\u00f6\u00b7nig", "von", "El\u00b7fen\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.27": {"line.1": {"text": "\u00bbdu sch\u00f6ne Frau, du minnige Frau,", "tokens": ["\u00bb", "du", "sch\u00f6\u00b7ne", "Frau", ",", "du", "min\u00b7ni\u00b7ge", "Frau", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nun sollst du mein eigen sein.", "tokens": ["Nun", "sollst", "du", "mein", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Elfenschlo\u00df und der Elfengau", "tokens": ["Das", "El\u00b7fen\u00b7schlo\u00df", "und", "der", "El\u00b7fen\u00b7gau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist alles, alles dein!", "tokens": ["Ist", "al\u00b7les", ",", "al\u00b7les", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PIS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Und wie du vordem in Hof und Stall", "tokens": ["Und", "wie", "du", "vor\u00b7dem", "in", "Hof", "und", "Stall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kredenzt mir den roten Wein,", "tokens": ["Kre\u00b7denzt", "mir", "den", "ro\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So kredenze fortan mir in Schlo\u00df und Hall'", "tokens": ["So", "kre\u00b7den\u00b7ze", "for\u00b7tan", "mir", "in", "Schlo\u00df", "und", "Hall'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die roten Lippen dein.\u00ab", "tokens": ["Die", "ro\u00b7ten", "Lip\u00b7pen", "dein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}