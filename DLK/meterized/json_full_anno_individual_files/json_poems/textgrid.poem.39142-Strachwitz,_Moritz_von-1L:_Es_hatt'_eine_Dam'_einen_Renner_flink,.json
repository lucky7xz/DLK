{"textgrid.poem.39142": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es hatt' eine Dam' einen Renner flink,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hatt' eine Dam' einen Renner flink,", "tokens": ["Es", "hatt'", "ei\u00b7ne", "Dam'", "ei\u00b7nen", "Ren\u00b7ner", "flink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein rasches, rotes Ro\u00df;", "tokens": ["Ein", "ra\u00b7sches", ",", "ro\u00b7tes", "Ro\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Boden herab die M\u00e4hne hing,", "tokens": ["Zum", "Bo\u00b7den", "her\u00b7ab", "die", "M\u00e4h\u00b7ne", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Blitzfunken die N\u00fcster scho\u00df.", "tokens": ["Blitz\u00b7fun\u00b7ken", "die", "N\u00fcs\u00b7ter", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem Renner, dem war sie treu und hold,", "tokens": ["Dem", "Ren\u00b7ner", ",", "dem", "war", "sie", "treu", "und", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Silber war er gez\u00e4umt,", "tokens": ["Mit", "Sil\u00b7ber", "war", "er", "ge\u00b7z\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Beschlagen der Huf mit rotem Gold,", "tokens": ["Be\u00b7schla\u00b7gen", "der", "Huf", "mit", "ro\u00b7tem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Perlen der Gurt ges\u00e4umt.", "tokens": ["Mit", "Per\u00b7len", "der", "Gurt", "ge\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und eh' die Sonne am Himmel schwamm,", "tokens": ["Und", "eh'", "die", "Son\u00b7ne", "am", "Him\u00b7mel", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In dem Stalle die Dame war,", "tokens": ["In", "dem", "Stal\u00b7le", "die", "Da\u00b7me", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sie k\u00e4mmte dem Tier mit goldigem Kamm", "tokens": ["Sie", "k\u00e4mm\u00b7te", "dem", "Tier", "mit", "gol\u00b7di\u00b7gem", "Kamm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein goldiges M\u00e4hnenhaar.", "tokens": ["Sein", "gol\u00b7di\u00b7ges", "M\u00e4h\u00b7nen\u00b7haar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und Seide sie flocht und Perlenband", "tokens": ["Und", "Sei\u00b7de", "sie", "flocht", "und", "Per\u00b7len\u00b7band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem Lilienfinger hinein,", "tokens": ["Mit", "dem", "Li\u00b7li\u00b7en\u00b7fin\u00b7ger", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Es trank der Renner aus ihrer Hand", "tokens": ["Es", "trank", "der", "Ren\u00b7ner", "aus", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den roten Burgunderwein.", "tokens": ["Den", "ro\u00b7ten", "Bur\u00b7gun\u00b7der\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Den vollen Arm, den wei\u00dfen Arm", "tokens": ["Den", "vol\u00b7len", "Arm", ",", "den", "wei\u00b7\u00dfen", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um des Tieres Nacken sie schlug;", "tokens": ["Um", "des", "Tie\u00b7res", "Na\u00b7cken", "sie", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es rann von der Wange die Tr\u00e4ne warm", "tokens": ["Es", "rann", "von", "der", "Wan\u00b7ge", "die", "Tr\u00e4\u00b7ne", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf des Renners gl\u00e4nzenden Bug:", "tokens": ["Auf", "des", "Ren\u00b7ners", "gl\u00e4n\u00b7zen\u00b7den", "Bug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "\u00bbmein stolzes Ro\u00df, mein treues Ro\u00df,", "tokens": ["\u00bb", "mein", "stol\u00b7zes", "Ro\u00df", ",", "mein", "treu\u00b7es", "Ro\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir klag' ich all mein Leid.\u00ab", "tokens": ["Dir", "klag'", "ich", "all", "mein", "Leid", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf ri\u00df das Ro\u00df, auf dehnte das Ro\u00df", "tokens": ["Auf", "ri\u00df", "das", "Ro\u00df", ",", "auf", "dehn\u00b7te", "das", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "APPR", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die schnaubende N\u00fcster weit.", "tokens": ["Die", "schnau\u00b7ben\u00b7de", "N\u00fcs\u00b7ter", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbsie wollen mir trauen als Bettgeno\u00df", "tokens": ["\u00bb", "sie", "wol\u00b7len", "mir", "trau\u00b7en", "als", "Bett\u00b7ge\u00b7no\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVINF", "KOKOM", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den falschen, verha\u00dften Mann.\u00ab", "tokens": ["Den", "fal\u00b7schen", ",", "ver\u00b7ha\u00df\u00b7ten", "Mann", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sprengte das Ro\u00df, da ri\u00df das Ro\u00df", "tokens": ["Da", "spreng\u00b7te", "das", "Ro\u00df", ",", "da", "ri\u00df", "das", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der goldenen Halfter Bann.", "tokens": ["Der", "gol\u00b7de\u00b7nen", "Half\u00b7ter", "Bann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbmein rotes Ro\u00df, mein rasches Ro\u00df,", "tokens": ["\u00bb", "mein", "ro\u00b7tes", "Ro\u00df", ",", "mein", "ra\u00b7sches", "Ro\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut rette mich, oder nie!\u00ab", "tokens": ["Heut", "ret\u00b7te", "mich", ",", "o\u00b7der", "nie", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tief senkte das Ro\u00df, tief bog das Ro\u00df", "tokens": ["Tief", "senk\u00b7te", "das", "Ro\u00df", ",", "tief", "bog", "das", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor der Herrin das schlanke Knie.", "tokens": ["Vor", "der", "Her\u00b7rin", "das", "schlan\u00b7ke", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Und sah sie an gar bang und lang,", "tokens": ["Und", "sah", "sie", "an", "gar", "bang", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar traulich und flehentlich,", "tokens": ["Gar", "trau\u00b7lich", "und", "fle\u00b7hent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Dame sich auf den Renner schwang,", "tokens": ["Die", "Da\u00b7me", "sich", "auf", "den", "Ren\u00b7ner", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Renner von hinnen strich.", "tokens": ["Der", "Ren\u00b7ner", "von", "hin\u00b7nen", "strich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Die Schwalbe, die unten im Sturme glitt,", "tokens": ["Die", "Schwal\u00b7be", ",", "die", "un\u00b7ten", "im", "Stur\u00b7me", "glitt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie holt' ihn nimmer ein,", "tokens": ["Sie", "holt'", "ihn", "nim\u00b7mer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sturm, der oben auf Wolken ritt,", "tokens": ["Der", "Sturm", ",", "der", "o\u00b7ben", "auf", "Wol\u00b7ken", "ritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Keucht' \u00e4chzend hinterdrein.", "tokens": ["Keucht'", "\u00e4ch\u00b7zend", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es steht ein Schlo\u00df im Elfenwald,", "tokens": ["Es", "steht", "ein", "Schlo\u00df", "im", "El\u00b7fen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein diamantenes Schlo\u00df,", "tokens": ["Ein", "di\u00b7a\u00b7man\u00b7te\u00b7nes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da stockt' es im Laufe, da macht' es halt,", "tokens": ["Da", "stockt'", "es", "im", "Lau\u00b7fe", ",", "da", "macht'", "es", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da stand es, das schnelle Ro\u00df.", "tokens": ["Da", "stand", "es", ",", "das", "schnel\u00b7le", "Ro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Und als sie ihm dankend den Hals umfing,", "tokens": ["Und", "als", "sie", "ihm", "dan\u00b7kend", "den", "Hals", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es koste mit Mund und Hand,", "tokens": ["Es", "kos\u00b7te", "mit", "Mund", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Statt des Renners der Dame im Arme hing", "tokens": ["Statt", "des", "Ren\u00b7ners", "der", "Da\u00b7me", "im", "Ar\u00b7me", "hing"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der K\u00f6nig von Elfenland:", "tokens": ["Der", "K\u00f6\u00b7nig", "von", "El\u00b7fen\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbdu sch\u00f6ne Frau, du minnige Frau,", "tokens": ["\u00bb", "du", "sch\u00f6\u00b7ne", "Frau", ",", "du", "min\u00b7ni\u00b7ge", "Frau", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nun sollst du mein eigen sein.", "tokens": ["Nun", "sollst", "du", "mein", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Elfenschlo\u00df und der Elfengau", "tokens": ["Das", "El\u00b7fen\u00b7schlo\u00df", "und", "der", "El\u00b7fen\u00b7gau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist alles, alles dein!", "tokens": ["Ist", "al\u00b7les", ",", "al\u00b7les", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PIS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und wie du vordem in Hof und Stall", "tokens": ["Und", "wie", "du", "vor\u00b7dem", "in", "Hof", "und", "Stall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "ia