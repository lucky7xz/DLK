, "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.19": {"line.1": {"text": "Den vollen Arm, den wei\u00dfen Arm", "tokens": ["Den", "vol\u00b7len", "Arm", ",", "den", "wei\u00b7\u00dfen", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um des Tieres Nacken sie schlug;", "tokens": ["Um", "des", "Tie\u00b7res", "Na\u00b7cken", "sie", "schlug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es rann von der Wange die Tr\u00e4ne warm", "tokens": ["Es", "rann", "von", "der", "Wan\u00b7ge", "die", "Tr\u00e4\u00b7ne", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf des Renners gl\u00e4nzenden Bug:", "tokens": ["Auf", "des", "Ren\u00b7ners", "gl\u00e4n\u00b7zen\u00b7den", "Bug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.20": {"line.1": {"text": "\u00bbmein stolzes Ro\u00df, mein treues Ro\u00df,", "tokens": ["\u00bb", "mein", "stol\u00b7zes", "Ro\u00df", ",", "mein", "treu\u00b7es", "Ro\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir klag' ich all mein Leid.\u00ab", "tokens": ["Dir", "klag'", "ich", "all", "mein", "Leid", ".", "\u0