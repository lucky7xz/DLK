": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPER", "VVFIN", "NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Gib Acht, da\u00df der Hausknecht die Zeit nicht verschl\u00e4ft,", "tokens": ["Gib", "Acht", ",", "da\u00df", "der", "Haus\u00b7knecht", "die", "Zeit", "nicht", "ver\u00b7schl\u00e4ft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er soll mich pr\u00e4cis vier Uhr wecken -", "tokens": ["Er", "soll", "mich", "pr\u00e4\u00b7cis", "vier", "Uhr", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuvor soll ein Fr\u00fchst\u00fcck mir schmecken!", "tokens": ["Zu\u00b7vor", "soll", "ein", "Fr\u00fch\u00b7st\u00fcck", "mir", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.14": {"line.1": {"text": "Dann sollst Du die Arche des No\u00e4h erbaun,", "tokens": ["Dann", "sollst", "Du", "die", "Ar\u00b7che", "des", "No\u00e4h", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Holz kannst Du freveln, ich will Dir zuschaun.", "tokens": ["Das", "Holz", "kannst", "Du", "fre\u00b7veln", ",", "ich", "will", "Dir", "zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie sie aussieht, das steht in der Bibel,", "tokens": ["Wie", "sie", "aus\u00b7sieht", ",", "das", "steht", "in", "der", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du bist drin bewandert nicht \u00fcbel!", "tokens": ["Du", "bist", "drin", "be\u00b7wan\u00b7dert", "nicht", "\u00fc\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Nur ja vergi\u00df mir die Spanfergel nicht,", "tokens": ["Nur", "ja", "ver\u00b7gi\u00df", "mir", "die", "Span\u00b7fer\u00b7gel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.2": {"text": "Denn dieses gibt sp\u00e4ter ein Leibgericht,", "tokens": ["Denn", "die\u00b7ses", "gibt", "sp\u00e4\u00b7ter", "ein", "Leib\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Auch nicht die Ameisenhaufen,", "tokens": ["Auch", "nicht", "die", "A\u00b7mei\u00b7sen\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Fisch' - werden schwerlich versaufen!", "tokens": ["Die", "Fisch'", "wer\u00b7den", "schwer\u00b7lich", "ver\u00b7sau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.16": {"line.1": {"text": "Auch's Ungeziefer versorgst Du mir,", "tokens": ["Auch's", "Un\u00b7ge\u00b7zie\u00b7fer", "ver\u00b7sorgst", "Du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Carl Vogt zur Strafe verzeichnets Dir;", "tokens": ["Carl", "Vogt", "zur", "Stra\u00b7fe", "ver\u00b7zeich\u00b7nets", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch kr\u00fcmme keinem ein H\u00e4rchen,", "tokens": ["Doch", "kr\u00fcm\u00b7me", "kei\u00b7nem", "ein", "H\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und nimm von jedem ein P\u00e4rchen!\"", "tokens": ["Und", "nimm", "von", "je\u00b7dem", "ein", "P\u00e4r\u00b7chen", "!", "\""], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "APPR", "PIAT", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}