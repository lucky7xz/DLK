{"textgrid.poem.52860": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3. Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hab' ich genug jetzt, man wird mir zu keck.", "tokens": ["Das", "hab'", "ich", "ge\u00b7nug", "jetzt", ",", "man", "wird", "mir", "zu", "keck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "$,", "PIS", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich tilge die Brut von dem Erdsboden weg!", "tokens": ["Ich", "til\u00b7ge", "die", "Brut", "von", "dem", "Erds\u00b7bo\u00b7den", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hab' ich ", "tokens": ["Hab'", "ich"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "++", "measure": "spondeus"}, "line.4": {"text": "Sie sollen sich h\u00f6llisch vergaffen!", "tokens": ["Sie", "sol\u00b7len", "sich", "h\u00f6l\u00b7lisch", "ver\u00b7gaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Aber Du, lieber Noah, Du habe nicht Angst,", "tokens": ["A\u00b7ber", "Du", ",", "lie\u00b7ber", "Noah", ",", "Du", "ha\u00b7be", "nicht", "Angst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "NE", "$,", "PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Weil Du und Dein Haus mir in Freuden anhangst,", "tokens": ["Weil", "Du", "und", "Dein", "Haus", "mir", "in", "Freu\u00b7den", "an\u00b7hangst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wenn Alle zu Grund m\u00fcssen gehen,", "tokens": ["Wenn", "Al\u00b7le", "zu", "Grund", "m\u00fcs\u00b7sen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Euch soll nichts B\u00f6ses geschehen.", "tokens": ["Euch", "soll", "nichts", "B\u00f6\u00b7ses", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Mich haben die Menschen auf's Tiefste gekr\u00e4nkt,", "tokens": ["Mich", "ha\u00b7ben", "die", "Men\u00b7schen", "auf's", "Tiefs\u00b7te", "ge\u00b7kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Am Besten wird's sein, wenn man Alle ", "tokens": ["Am", "Bes\u00b7ten", "wird's", "sein", ",", "wenn", "man", "Al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "VAINF", "$,", "KOUS", "PIS", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte Geschlecht ist voll S\u00fcnden,", "tokens": ["Das", "al\u00b7te", "Ge\u00b7schlecht", "ist", "voll", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Du wirst mir ein neues begr\u00fcnden.", "tokens": ["Du", "wirst", "mir", "ein", "neu\u00b7es", "be\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Vermittlung, Vers\u00f6hnung gilt heute noch Nichts;", "tokens": ["Ver\u00b7mitt\u00b7lung", ",", "Ver\u00b7s\u00f6h\u00b7nung", "gilt", "heu\u00b7te", "noch", "Nichts", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADV", "ADV", "PIS", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Drum h\u00f6re den Plan meines Rachegerichts!", "tokens": ["Drum", "h\u00f6\u00b7re", "den", "Plan", "mei\u00b7nes", "Ra\u00b7che\u00b7ge\u00b7richts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das Feuer ist mir zu kostspielig,", "tokens": ["Das", "Feu\u00b7er", "ist", "mir", "zu", "kost\u00b7spie\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich thu es mit Wasser h\u00f6chst billig.", "tokens": ["Ich", "thu", "es", "mit", "Was\u00b7ser", "h\u00f6chst", "bil\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Schlaf jetzt! Wir geh'n Morgen ganz fr\u00fch an's Gesch\u00e4ft,", "tokens": ["Schlaf", "jetzt", "!", "Wir", "geh'n", "Mor\u00b7gen", "ganz", "fr\u00fch", "an's", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPER", "VVFIN", "NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Gib Acht, da\u00df der Hausknecht die Zeit nicht verschl\u00e4ft,", "tokens": ["Gib", "Acht", ",", "da\u00df", "der", "Haus\u00b7knecht", "die", "Zeit", "nicht", "ver\u00b7schl\u00e4ft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er soll mich pr\u00e4cis vier Uhr wecken -", "tokens": ["Er", "soll", "mich", "pr\u00e4\u00b7cis", "vier", "Uhr", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuvor soll ein Fr\u00fchst\u00fcck mir schmecken!", "tokens": ["Zu\u00b7vor", "soll", "ein", "Fr\u00fch\u00b7st\u00fcck", "mir", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Dann sollst Du die Arche des No\u00e4h erbaun,", "tokens": ["Dann", "sollst", "Du", "die", "Ar\u00b7che", "des", "No\u00e4h", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Holz kannst Du freveln, ich will Dir zuschaun.", "tokens": ["Das", "Holz", "kannst", "Du", "fre\u00b7veln", ",", "ich", "will", "Dir", "zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie sie aussieht, das steht in der Bibel,", "tokens": ["Wie", "sie", "aus\u00b7sieht", ",", "das", "steht", "in", "der", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du bist drin bewandert nicht \u00fcbel!", "tokens": ["Du", "bist", "drin", "be\u00b7wan\u00b7dert", "nicht", "\u00fc\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Nur ja vergi\u00df mir die Spanfergel nicht,", "tokens": ["Nur", "ja", "ver\u00b7gi\u00df", "mir", "die", "Span\u00b7fer\u00b7gel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.2": {"text": "Denn dieses gibt sp\u00e4ter ein Leibgericht,", "tokens": ["Denn", "die\u00b7ses", "gibt", "sp\u00e4\u00b7ter", "ein", "Leib\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Auch nicht die Ameisenhaufen,", "tokens": ["Auch", "nicht", "die", "A\u00b7mei\u00b7sen\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Fisch' - werden schwerlich versaufen!", "tokens": ["Die", "Fisch'", "wer\u00b7den", "schwer\u00b7lich", "ver\u00b7sau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Auch's Ungeziefer versorgst Du mir,", "tokens": ["Auch's", "Un\u00b7ge\u00b7zie\u00b7fer", "ver\u00b7sorgst", "Du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Carl Vogt zur Strafe verzeichnets Dir;", "tokens": ["Carl", "Vogt", "zur", "Stra\u00b7fe", "ver\u00b7zeich\u00b7nets", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch kr\u00fcmme keinem ein H\u00e4rchen,", "tokens": ["Doch", "kr\u00fcm\u00b7me", "kei\u00b7nem", "ein", "H\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und nimm von jedem ein P\u00e4rchen!\"", "tokens": ["Und", "nimm", "von", "je\u00b7dem", "ein", "P\u00e4r\u00b7chen", "!", "\""], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "APPR", "PIAT", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.