{"textgrid.poem.66296": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Es ist genug", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist genug! So nimm denn meine Seele,", "tokens": ["Es", "ist", "ge\u00b7nug", "!", "So", "nimm", "denn", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ADV", "VVIMP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die m\u00fcde Seele nimm zu dir.", "tokens": ["Die", "m\u00fc\u00b7de", "See\u00b7le", "nimm", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wei\u00dft, wie ich die Augenblicke z\u00e4hle,", "tokens": ["Du", "wei\u00dft", ",", "wie", "ich", "die", "Au\u00b7gen\u00b7bli\u00b7cke", "z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du kennst dies bange Herz in mir,", "tokens": ["Du", "kennst", "dies", "ban\u00b7ge", "Herz", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das oft, get\u00e4uscht, dem Tod entgegenschlug:", "tokens": ["Das", "oft", ",", "ge\u00b7t\u00e4uscht", ",", "dem", "Tod", "ent\u00b7ge\u00b7gen\u00b7schlug", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "VVPP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es ist genug!", "tokens": ["Es", "ist", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mich lockt nicht mehr die bunte Kraft der Erde,", "tokens": ["Mich", "lockt", "nicht", "mehr", "die", "bun\u00b7te", "Kraft", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gold ist mir Staub und Ehre Tand;", "tokens": ["Gold", "ist", "mir", "Staub", "und", "Eh\u00b7re", "Tand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "KON", "NN", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der frechen Lust einladende Geberde,", "tokens": ["Der", "fre\u00b7chen", "Lust", "ein\u00b7la\u00b7den\u00b7de", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Stolz im strahlenden Gewand,", "tokens": ["Der", "Stolz", "im", "strah\u00b7len\u00b7den", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Schwelgers Tisch, vom s\u00fc\u00dfen Gifte schwer,", "tokens": ["Des", "Schwel\u00b7gers", "Tisch", ",", "vom", "s\u00fc\u00b7\u00dfen", "Gif\u00b7te", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Lockt mich nicht mehr.", "tokens": ["Lockt", "mich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Die Thorheit geht der Weisheit hier zur Seite,", "tokens": ["Die", "Thor\u00b7heit", "geht", "der", "Weis\u00b7heit", "hier", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bei der Wahrheit steht der Wahn;", "tokens": ["Und", "bei", "der", "Wahr\u00b7heit", "steht", "der", "Wahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die K\u00fcnste sind nicht mehr der reinen Freude,", "tokens": ["Die", "K\u00fcns\u00b7te", "sind", "nicht", "mehr", "der", "rei\u00b7nen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sind der Wollust unterthan:", "tokens": ["Sie", "sind", "der", "Wol\u00b7lust", "un\u00b7ter\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Tugend klagt; in schwarzen Klausen weint", "tokens": ["Die", "Tu\u00b7gend", "klagt", ";", "in", "schwar\u00b7zen", "Klau\u00b7sen", "weint"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Menschenfreund.", "tokens": ["Der", "Men\u00b7schen\u00b7freund", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Selbst der Natur unschuldigstes Vergn\u00fcgen", "tokens": ["Selbst", "der", "Na\u00b7tur", "un\u00b7schul\u00b7digs\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wird oft durch's Schmerzgef\u00fchl entweiht,", "tokens": ["Wird", "oft", "durch's", "Schmerz\u00b7ge\u00b7f\u00fchl", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df unter Blumen Menschenbeine liegen", "tokens": ["Da\u00df", "un\u00b7ter", "Blu\u00b7men", "Men\u00b7schen\u00b7bei\u00b7ne", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df der Thron der Eitelkeit", "tokens": ["Und", "da\u00df", "der", "Thron", "der", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom sanften West und Todtenduft beweht", "tokens": ["Vom", "sanf\u00b7ten", "West", "und", "Tod\u00b7ten\u00b7duft", "be\u00b7weht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf Sch\u00e4deln steht.", "tokens": ["Auf", "Sch\u00e4\u00b7deln", "steht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der junge Mai, verstrickt in Rosenfesseln,", "tokens": ["Der", "jun\u00b7ge", "Mai", ",", "ver\u00b7strickt", "in", "Ro\u00b7sen\u00b7fes\u00b7seln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stirbt, wie der K\u00e4fer, den er n\u00e4hrt;", "tokens": ["Stirbt", ",", "wie", "der", "K\u00e4\u00b7fer", ",", "den", "er", "n\u00e4hrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Lilie verwelkt mit rauhen Nesseln,", "tokens": ["Die", "Li\u00b7lie", "ver\u00b7welkt", "mit", "rau\u00b7hen", "Nes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Rose wird vom Wurm versehrt;", "tokens": ["Die", "Ro\u00b7se", "wird", "vom", "Wurm", "ver\u00b7sehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Bl\u00fcthe f\u00e4llt, des goldnen Abends Pracht", "tokens": ["Die", "Bl\u00fc\u00b7the", "f\u00e4llt", ",", "des", "gold\u00b7nen", "A\u00b7bends", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verschlingt die Nacht.", "tokens": ["Ver\u00b7schlingt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Sprich, Gott, wie lang' ich noch im Schauerthale", "tokens": ["Sprich", ",", "Gott", ",", "wie", "lang'", "ich", "noch", "im", "Schau\u00b7er\u00b7tha\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "PWAV", "ADV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ein Gebundner schmachten soll?", "tokens": ["Als", "ein", "Ge\u00b7bund\u00b7ner", "schmach\u00b7ten", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's bald genug? Und ist die Leidensschale", "tokens": ["Ist's", "bald", "ge\u00b7nug", "?", "Und", "ist", "die", "Lei\u00b7dens\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "$.", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht bald von meinen Thr\u00e4nen voll?", "tokens": ["Nicht", "bald", "von", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "voll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sind Seufzer, tief ins Herzblut eingetaucht,", "tokens": ["Sind", "Seuf\u00b7zer", ",", "tief", "ins", "Herz\u00b7blut", "ein\u00b7ge\u00b7taucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht bald verhaucht?", "tokens": ["Nicht", "bald", "ver\u00b7haucht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Es ist genug! Entr\u00fcck' mich den Gefahren,", "tokens": ["Es", "ist", "ge\u00b7nug", "!", "Ent\u00b7r\u00fcck", "mich", "den", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Aengsten meiner Lebenszeit!", "tokens": ["Den", "A\u00b7engs\u00b7ten", "mei\u00b7ner", "Le\u00b7bens\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bin ich denn nicht, wie meine V\u00e4ter waren,", "tokens": ["Bin", "ich", "denn", "nicht", ",", "wie", "mei\u00b7ne", "V\u00e4\u00b7ter", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wurm, ein Spiel der Eitelkeit?", "tokens": ["Ein", "Wurm", ",", "ein", "Spiel", "der", "Ei\u00b7tel\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Vater, dessen Ruthe mich zerschlug:", "tokens": ["O", "Va\u00b7ter", ",", "des\u00b7sen", "Ru\u00b7the", "mich", "zer\u00b7schlug", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es ist genug!", "tokens": ["Es", "ist", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Zur Ewigkeit, ich f\u00fchl's, bin ich geboren;", "tokens": ["Zur", "E\u00b7wig\u00b7keit", ",", "ich", "f\u00fchl's", ",", "bin", "ich", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier bin ich Wandrer, B\u00fcrger nicht!", "tokens": ["Hier", "bin", "ich", "Wand\u00b7rer", ",", "B\u00fcr\u00b7ger", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Erbe ist; du Gott! hast es geschworen;", "tokens": ["Mein", "Er\u00b7be", "ist", ";", "du", "Gott", "!", "hast", "es", "ge\u00b7schwo\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "PPER", "NN", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein ewig Erbe ist im Licht.", "tokens": ["Mein", "e\u00b7wig", "Er\u00b7be", "ist", "im", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist's S\u00fcnde denn, wenn meine Seele schreit", "tokens": ["Ist's", "S\u00fcn\u00b7de", "denn", ",", "wenn", "mei\u00b7ne", "See\u00b7le", "schreit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach Ewigkeit?", "tokens": ["Nach", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Genug, genug! Es ist genug gejammert;", "tokens": ["Ge\u00b7nug", ",", "ge\u00b7nug", "!", "Es", "ist", "ge\u00b7nug", "ge\u00b7jam\u00b7mert", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Genug hab' ich die bleiche Hand", "tokens": ["Ge\u00b7nug", "hab'", "ich", "die", "blei\u00b7che", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Gitter meines Kerkers eingeklammert", "tokens": ["Ins", "Git\u00b7ter", "mei\u00b7nes", "Ker\u00b7kers", "ein\u00b7ge\u00b7klam\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Seufzer himmelan gesandt.", "tokens": ["Und", "Seuf\u00b7zer", "him\u00b7me\u00b7lan", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Genug hab' ich die Fesseln rasseln h\u00f6ren,", "tokens": ["Ge\u00b7nug", "hab'", "ich", "die", "Fes\u00b7seln", "ras\u00b7seln", "h\u00f6\u00b7ren", 