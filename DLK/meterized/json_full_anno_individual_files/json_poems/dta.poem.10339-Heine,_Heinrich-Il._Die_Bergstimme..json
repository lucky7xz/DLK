{"dta.poem.10339": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Il.  \n  Die Bergstimme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Reiter durch das Bergthal zieht,", "tokens": ["Ein", "Rei\u00b7ter", "durch", "das", "Berg\u00b7thal", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im traurig stillen Trab':", "tokens": ["Im", "trau\u00b7rig", "stil\u00b7len", "Trab'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! zieh' ich jetzt wohl in Liebchens Arm,", "tokens": ["Ach", "!", "zieh'", "ich", "jetzt", "wohl", "in", "Lieb\u00b7chens", "Arm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Oder zieh' ich in's dunkle Grab?", "tokens": ["O\u00b7der", "zieh'", "ich", "in's", "dunk\u00b7le", "Grab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Bergstimm Antwort gab:", "tokens": ["Die", "Bergs\u00b7timm", "Ant\u00b7wort", "gab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In's dunkle Grab!", "tokens": ["In's", "dunk\u00b7le", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und weiter reitet der Reitersmann,", "tokens": ["Und", "wei\u00b7ter", "rei\u00b7tet", "der", "Rei\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und seufzet schwer dazu:", "tokens": ["Und", "seuf\u00b7zet", "schwer", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So zieh' ich denn hin in's Grab so fr\u00fch, \u2014", "tokens": ["So", "zieh'", "ich", "denn", "hin", "in's", "Grab", "so", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADV", "ADJD", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohlan im Grab ist Ruh.", "tokens": ["Wo\u00b7hlan", "im", "Grab", "ist", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Stimme sprach dazu:", "tokens": ["Die", "Stim\u00b7me", "sprach", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Im Grab ist Ruh!", "tokens": ["Im", "Grab", "ist", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dem Reitersmann eine Thr\u00e4ne rollt", "tokens": ["Dem", "Rei\u00b7ters\u00b7mann", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "rollt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von der Wange bleich und kummervoll:", "tokens": ["Von", "der", "Wan\u00b7ge", "bleich", "und", "kum\u00b7mer\u00b7voll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ist nur im Grabe die Ruhe f\u00fcr mich, \u2014", "tokens": ["Und", "ist", "nur", "im", "Gra\u00b7be", "die", "Ru\u00b7he", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "APPR", "PPER", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So ist mir im Grabe wohl.", "tokens": ["So", "ist", "mir", "im", "Gra\u00b7be", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Stimm' erwiedert hohl:", "tokens": ["Die", "Stimm'", "er\u00b7wie\u00b7dert", "hohl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Im Grabe wohl!", "tokens": ["Im", "Gra\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}