{"dta.poem.13591": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "no:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich der Sommermorgen still erhebt,", "tokens": ["Wenn", "sich", "der", "Som\u00b7mer\u00b7mor\u00b7gen", "still", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein W\u00f6lkchen in den blauen L\u00fcften schwebt,", "tokens": ["Kein", "W\u00f6lk\u00b7chen", "in", "den", "blau\u00b7en", "L\u00fcf\u00b7ten", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Wonneschauern naht das Licht der Welt,", "tokens": ["Mit", "Won\u00b7ne\u00b7schau\u00b7ern", "naht", "das", "Licht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sich die Aehrenfelder leise neigen,", "tokens": ["Da\u00df", "sich", "die", "A\u00b7eh\u00b7ren\u00b7fel\u00b7der", "lei\u00b7se", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Da sink' ich auf die Knie im stillen Feld,", "tokens": ["Da", "sink'", "ich", "auf", "die", "Knie", "im", "stil\u00b7len", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und bete wenn noch alle Stimmen schweigen.", "tokens": ["Und", "be\u00b7te", "wenn", "noch", "al\u00b7le", "Stim\u00b7men", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}