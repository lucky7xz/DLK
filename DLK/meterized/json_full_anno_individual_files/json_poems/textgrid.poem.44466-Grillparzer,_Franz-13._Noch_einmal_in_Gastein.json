{"textgrid.poem.44466": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "13. Noch einmal in Gastein", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, dieses Ortes Einsamkeit,", "tokens": ["Du", ",", "die\u00b7ses", "Or\u00b7tes", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du mich nicht erquickt vor zehen Jahren,", "tokens": ["Hast", "du", "mich", "nicht", "er\u00b7quickt", "vor", "ze\u00b7hen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schien die Welt, das Tal so weit,", "tokens": ["Da", "schien", "die", "Welt", ",", "das", "Tal", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie in den Schacht, der goldne Sch\u00e4tze beut,", "tokens": ["Wie", "in", "den", "Schacht", ",", "der", "gold\u00b7ne", "Sch\u00e4t\u00b7ze", "beut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kam ich durch deine Klamm gefahren.", "tokens": ["Kam", "ich", "durch", "dei\u00b7ne", "Klamm", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und war dein Umfang schmal umgrenzt,", "tokens": ["Und", "war", "dein", "Um\u00b7fang", "schmal", "um\u00b7grenzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Geist stand auf der Hoffnung Sonnenh\u00fcgeln,", "tokens": ["Mein", "Geist", "stand", "auf", "der", "Hoff\u00b7nung", "Son\u00b7nen\u00b7h\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und h\u00f6her als dein ewger Schnee ergl\u00e4nzt,", "tokens": ["Und", "h\u00f6\u00b7her", "als", "dein", "ew\u00b7ger", "Schnee", "er\u00b7gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Trugs mich empor auf Adlerfl\u00fcgeln.", "tokens": ["Trugs", "mich", "em\u00b7por", "auf", "Ad\u00b7ler\u00b7fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nun bin ich m\u00fcd, gest\u00f6rt, entzweit,", "tokens": ["Nun", "bin", "ich", "m\u00fcd", ",", "ge\u00b7st\u00f6rt", ",", "ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nur Mauern l\u00e4\u00dft die Bergwand mir gewahren,", "tokens": ["Nur", "Mau\u00b7ern", "l\u00e4\u00dft", "die", "Berg\u00b7wand", "mir", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "O, eine ganze Ewigkeit", "tokens": ["O", ",", "ei\u00b7ne", "gan\u00b7ze", "E\u00b7wig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Liegt in dem Raum von zehen Jahren!", "tokens": ["Liegt", "in", "dem", "Raum", "von", "ze\u00b7hen", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, dieses Ortes Einsamkeit,", "tokens": ["Du", ",", "die\u00b7ses", "Or\u00b7tes", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du mich nicht erquickt vor zehen Jahren,", "tokens": ["Hast", "du", "mich", "nicht", "er\u00b7quickt", "vor", "ze\u00b7hen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schien die Welt, das Tal so weit,", "tokens": ["Da", "schien", "die", "Welt", ",", "das", "Tal", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text"