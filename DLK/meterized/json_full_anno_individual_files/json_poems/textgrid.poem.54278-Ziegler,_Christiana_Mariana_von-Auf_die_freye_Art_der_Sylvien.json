{"textgrid.poem.54278": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die freye Art der Sylvien", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Blick soll, wie du denckst, gleich durch die Hertzen dringen,", "tokens": ["Dein", "Blick", "soll", ",", "wie", "du", "denckst", ",", "gleich", "durch", "die", "Hert\u00b7zen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine blancke Brust der M\u00e4nner Netze seyn,", "tokens": ["Und", "dei\u00b7ne", "blan\u00b7cke", "Brust", "der", "M\u00e4n\u00b7ner", "Net\u00b7ze", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Betrogne Sylvia! es seynd vergebne Schlingen,", "tokens": ["Be\u00b7trog\u00b7ne", "Syl\u00b7via", "!", "es", "seynd", "ver\u00b7geb\u00b7ne", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie plumpen nicht so gleich, wie tumme Gimpel, ein.", "tokens": ["Sie", "plum\u00b7pen", "nicht", "so", "gleich", ",", "wie", "tum\u00b7me", "Gim\u00b7pel", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "$,", "PWAV", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer wolte sich von dir was t\u00fcchtiges versprechen?", "tokens": ["Wer", "wol\u00b7te", "sich", "von", "dir", "was", "t\u00fcch\u00b7ti\u00b7ges", "ver\u00b7spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "PPER", "PIS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mann wei\u00df schon, was dein Sinn vor sch\u00f6ne Mucken hegt.", "tokens": ["Mann", "wei\u00df", "schon", ",", "was", "dein", "Sinn", "vor", "sch\u00f6\u00b7ne", "Mu\u00b7cken", "hegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum sehnt sich keiner nicht die Frucht bey dir zu brechen,", "tokens": ["Drum", "sehnt", "sich", "kei\u00b7ner", "nicht", "die", "Frucht", "bey", "dir", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "PIS", "PTKNEG", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wornach sich doch in dir ein gro\u00df Verlangen regt.", "tokens": ["Wor\u00b7nach", "sich", "doch", "in", "dir", "ein", "gro\u00df", "Ver\u00b7lan\u00b7gen", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PRF", "ADV", "APPR", "PPER", "ART", "ADJD", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wir lachen, wann du sie durch Minen wilst ber\u00fccken.", "tokens": ["Wir", "la\u00b7chen", ",", "wann", "du", "sie", "durch", "Mi\u00b7nen", "wilst", "be\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach! stich dich nicht darein, es beist leicht keiner an.", "tokens": ["Ach", "!", "stich", "dich", "nicht", "da\u00b7rein", ",", "es", "beist", "leicht", "kei\u00b7ner", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "PPER", "PTKNEG", "PAV", "$,", "PPER", "ADV", "ADJD", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es mu\u00df ein Hercul seyn, der sich zu dir soll schicken,", "tokens": ["Es", "mu\u00df", "ein", "Her\u00b7cul", "seyn", ",", "der", "sich", "zu", "dir", "soll", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,", "PRELS", "PRF", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil dich, Syrene, nicht ein Mann ers\u00e4ttgen kan.", "tokens": ["Weil", "dich", ",", "Sy\u00b7re\u00b7ne", ",", "nicht", "ein", "Mann", "er\u00b7s\u00e4tt\u00b7gen", "kan", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Dein Blick soll, wie du denckst, gleich durch die Hertzen dringen,", "