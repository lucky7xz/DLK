{"textgrid.poem.52224": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Am Baum der Menschheit dr\u00e4ngt sich Bl\u00fct' an Bl\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Baum der Menschheit dr\u00e4ngt sich Bl\u00fct' an Bl\u00fcte,", "tokens": ["Am", "Baum", "der", "Menschheit", "dr\u00e4ngt", "sich", "Bl\u00fct'", "an", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach ew'gen Regeln wiegen sie sich drauf;", "tokens": ["Nach", "ew'\u00b7gen", "Re\u00b7geln", "wie\u00b7gen", "sie", "sich", "drauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn hier die eine matt und welk vergl\u00fchte,", "tokens": ["Wenn", "hier", "die", "ei\u00b7ne", "matt", "und", "welk", "ver\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ART", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Springt dort die andre voll und pr\u00e4chtig auf.", "tokens": ["Springt", "dort", "die", "and\u00b7re", "voll", "und", "pr\u00e4ch\u00b7tig", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIS", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein ewig Kommen und ein ewig Gehen,", "tokens": ["Ein", "e\u00b7wig", "Kom\u00b7men", "und", "ein", "e\u00b7wig", "Ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nun und nimmer tr\u00e4ger Stillestand!", "tokens": ["Und", "nun", "und", "nim\u00b7mer", "tr\u00e4\u00b7ger", "Stil\u00b7les\u00b7tand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir sehn sie auf, wir sehn sie niederwehen,", "tokens": ["Wir", "sehn", "sie", "auf", ",", "wir", "sehn", "sie", "nie\u00b7der\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jede Bl\u00fcte ist ein Volk, ein Land!", "tokens": ["Und", "je\u00b7de", "Bl\u00fc\u00b7te", "ist", "ein", "Volk", ",", "ein", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir, die wir wandeln noch auf jungen Sohlen,", "tokens": ["Wir", ",", "die", "wir", "wan\u00b7deln", "noch", "auf", "jun\u00b7gen", "Soh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sahn doch schon manche sterbend und geknickt.", "tokens": ["Sahn", "doch", "schon", "man\u00b7che", "ster\u00b7bend", "und", "ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIS", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Steppengeier ward die Rose Polen", "tokens": ["Vom", "Step\u00b7pen\u00b7gei\u00b7er", "ward", "die", "Ro\u00b7se", "Po\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor unsern Augen wild und grimm zerpfl\u00fcckt!", "tokens": ["Vor", "un\u00b7sern", "Au\u00b7gen", "wild", "und", "grimm", "zer\u00b7pfl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durchs Laub Hispaniens ernst auf ihrem Gange", "tokens": ["Durchs", "Laub", "His\u00b7pa\u00b7ni\u00b7ens", "ernst", "auf", "ih\u00b7rem", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "St\u00fcrmt die Geschichte \u2013 ob es fallen mu\u00df?", "tokens": ["St\u00fcrmt", "die", "Ge\u00b7schich\u00b7te", "\u2013", "ob", "es", "fal\u00b7len", "mu\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Ob nicht ein andres, morsch und faul schon lange,", "tokens": ["Ob", "nicht", "ein", "and\u00b7res", ",", "morsch", "und", "faul", "schon", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "PIS", "$,", "ADJD", "KON", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zerflatternd hinsaust \u00fcbern Bosporus?", "tokens": ["Zer\u00b7flat\u00b7ternd", "hin\u00b7saust", "\u00fc\u00b7bern", "Bos\u00b7po\u00b7rus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch neben diesen, die des Weltgeists Weben", "tokens": ["Doch", "ne\u00b7ben", "die\u00b7sen", ",", "die", "des", "Welt\u00b7geists", "We\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Aste sch\u00fcttet mit gewalt'ger Kraft,", "tokens": ["Vom", "As\u00b7te", "sch\u00fct\u00b7tet", "mit", "ge\u00b7walt'\u00b7ger", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sehn wir ans Licht auch andre Triebe streben,", "tokens": ["Sehn", "wir", "ans", "Licht", "auch", "and\u00b7re", "Trie\u00b7be", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hellaugig, freudig, voll von jungem Saft.", "tokens": ["Hel\u00b7lau\u00b7gig", ",", "freu\u00b7dig", ",", "voll", "von", "jun\u00b7gem", "Saft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.5": {"text": "O, welch ein Sprossen, welch ein reich Entfalten!", "tokens": ["O", ",", "welch", "ein", "Spros\u00b7sen", ",", "welch", "ein", "reich", "Ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "$,", "PWAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O, welch ein Drang in alt und neuem Holz!", "tokens": ["O", ",", "welch", "ein", "Drang", "in", "alt", "und", "neu\u00b7em", "Holz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "APPR", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie manche Knospe sahn auch wir sich spalten,", "tokens": ["Wie", "man\u00b7che", "Knos\u00b7pe", "sahn", "auch", "wir", "sich", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie manche platzen, laut und voll und stolz!", "tokens": ["Wie", "man\u00b7che", "plat\u00b7zen", ",", "laut", "und", "voll", "und", "stolz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Regt sich's im Scho\u00df! Dem Bersten scheint sie nah \u2013", "tokens": ["Regt", "sich's", "im", "Scho\u00df", "!", "Dem", "Bers\u00b7ten", "scheint", "sie", "nah", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Frisch, wie Hermann auf den Weserwiesen,", "tokens": ["Frisch", ",", "wie", "Her\u00b7mann", "auf", "den", "We\u00b7ser\u00b7wie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Frisch, wie sie Luther auf der Wartburg sah!", "tokens": ["Frisch", ",", "wie", "sie", "Lu\u00b7ther", "auf", "der", "Wart\u00b7burg", "sah", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein alter Trieb! Doch immer mutig keimend,", "tokens": ["Ein", "al\u00b7ter", "Trieb", "!", "Doch", "im\u00b7mer", "mu\u00b7tig", "kei\u00b7mend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch immer lechzend nach der Sonne Strahl,", "tokens": ["Doch", "im\u00b7mer", "lech\u00b7zend", "nach", "der", "Son\u00b7ne", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch immer Fr\u00fchling, immer Freiheit tr\u00e4umend \u2013", "tokens": ["Doch", "im\u00b7mer", "Fr\u00fch\u00b7ling", ",", "im\u00b7mer", "Frei\u00b7heit", "tr\u00e4u\u00b7mend", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O, wird die Knospe Blume nicht einmal?", "tokens": ["O", ",", "wird", "die", "Knos\u00b7pe", "Blu\u00b7me", "nicht", "ein\u00b7mal", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ART", "NN", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ja, voller Kelch! \u2013 Dafern man nur nicht h\u00fctet,", "tokens": ["Ja", ",", "vol\u00b7ler", "Kelch", "!", "\u2013", "Da\u00b7fern", "man", "nur", "nicht", "h\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$.", "$(", "KOUS", "PIS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was frei und freudig sich entwickeln mu\u00df!", "tokens": ["Was", "frei", "und", "freu\u00b7dig", "sich", "ent\u00b7wi\u00b7ckeln", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dafern man nicht, was die Natur gebietet,", "tokens": ["Da\u00b7fern", "man", "nicht", ",", "was", "die", "Na\u00b7tur", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Ranke nimmt und eitel wilden Schu\u00df!", "tokens": ["F\u00fcr", "Ran\u00b7ke", "nimmt", "und", "ei\u00b7tel", "wil\u00b7den", "Schu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dafern man zusieht, da\u00df kein Meltau zehre", "tokens": ["Da\u00b7fern", "man", "zu\u00b7sieht", ",", "da\u00df", "kein", "Mel\u00b7tau", "zeh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Tief an der Bl\u00e4tter edlem, zartem Kern!", "tokens": ["Tief", "an", "der", "Bl\u00e4t\u00b7ter", "ed\u00b7lem", ",", "zar\u00b7tem", "Kern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Dafern den Bast man wegwirft und die Schere!", "tokens": ["Da\u00b7fern", "den", "Bast", "man", "weg\u00b7wirft", "und", "die", "Sche\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dafern \u2013 ja nun, ich meine nur: dafern!", "tokens": ["Da\u00b7fern", "\u2013", "ja", "nun", ",", "ich", "mei\u00b7ne", "nur", ":", "da\u00b7fern", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$(", "ADV", "ADV", "$,", "PPER", "VVFIN", "ADV", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der du die Blumen auseinanderfaltest,", "tokens": ["Der", "du", "die", "Blu\u00b7men", "aus\u00b7ein\u00b7an\u00b7der\u00b7fal\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Hauch des Lenzes, weh' auch uns heran!", "tokens": ["O", "Hauch", "des", "Len\u00b7zes", ",", "weh'", "auch", "uns", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der du der V\u00f6lker heil'ge Knospen spaltest,", "tokens": ["Der", "du", "der", "V\u00f6l\u00b7ker", "heil'\u00b7ge", "Knos\u00b7pen", "spal\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Hauch der Freiheit, weh' auch diese an!", "tokens": ["O", "Hauch", "der", "Frei\u00b7heit", ",", "weh'", "auch", "die\u00b7se", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In ihrem tiefsten, stillsten Heiligtume", "tokens": ["In", "ih\u00b7rem", "tiefs\u00b7ten", ",", "stills\u00b7ten", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O, k\u00fcss' sie auf zu Duft und Glanz und Schein \u2013", "tokens": ["O", ",", "k\u00fcss'", "sie", "auf", "zu", "Duft", "und", "Glanz", "und", "Schein", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Herr Gott im Himmel, welche Wunderblume", "tokens": ["Herr", "Gott", "im", "Him\u00b7mel", ",", "wel\u00b7che", "