ord", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein alter Trieb! Doch immer mutig keimend,", "tokens": ["Ein", "al\u00b7ter", "Trieb", "!", "Doch", "im\u00b7mer", "mu\u00b7tig", "kei\u00b7mend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch immer lechzend nach der Sonne Strahl,", "tokens": ["Doch", "im\u00b7mer", "lech\u00b7zend", "nach", "der", "Son\u00b7ne", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch immer Fr\u00fchling, immer Freiheit tr\u00e4umend \u2013", "tokens": ["Doch", "im\u00b7mer", "Fr\u00fch\u00b7ling", ",", "im\u00b7mer", "Frei\u00b7heit", "tr\u00e4u\u00b7mend", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O, wird die Knospe Blume nicht einmal?", "tokens": ["O", ",", "wird", "die", "Knos\u00b7pe", "Blu\u00b7me", "nicht", "ein\u00b7mal", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ART", "NN", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ja, voller Kelch! \u2013 Dafern man nur nicht h\u00fctet,", "tokens": ["Ja", ",", "vol\u00b7ler", "Kelch", "!", "\u2013", "Da\u00b7fern", "man", "nur", "nicht", "h\u00fc\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$.", "$(", "KOUS", "PIS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was frei und freudig sich entwickeln mu\u00df!", "tokens": ["Was", "frei", "und", "freu\u00b7dig", "sich", "ent\u00b7wi\u00b7ckeln", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dafern man nicht, was die Natur gebietet,", "tokens": ["Da\u00b7fern", "man", "nicht", ",", "was", "die", "Na\u00b7tur", "ge\u00b7bie\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Ranke nimmt und eitel wilden Schu\u00df!", "tokens": ["F\u00fcr", "Ran\u00b7ke", "nimmt", "und", "ei\u00b7tel", "wil\u00b7den", "Schu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dafern man zusieht, da\u00df kein Meltau zehre", "tokens": ["Da\u00b7fern", "man", "zu\u00b7sieht", ",", "da\u00df", "kein", "Mel\u00b7tau", "zeh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Tief an der Bl\u00e4tter edlem, zartem Kern!", "tokens": ["Tief", "an", "der", "Bl\u00e4t\u00b7ter", "ed\u00b7lem", ",", "zar\u00b7tem", "Kern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Dafern den Bast man wegwirft und die Schere!", "tokens": ["Da\u00b7fern", "den", "Bast", "man", "weg\u00b7wirft", "und", "die", "Sche\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dafern \u2013 ja nun, ich meine nur: dafern!", "tokens": ["Da\u00b7fern", "\u2013", "ja", "nun", ",", "ich", "mei\u00b7ne", "nur", ":", "da\u00b7fern", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$(", "ADV", "ADV", "$,", "PPER", "VVFIN", "ADV", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Der du die Blumen auseinanderfaltest,", "tokens": ["Der", "du", "die", "Blu\u00b7men", "aus\u00b7ein\u00b7an\u00b7der\u00b7fal\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Hauch des Lenzes, weh' auch uns heran!", "tokens": ["O", "Hauch", "des", "Len\u00b7zes", ",", "weh'", "auch", "uns", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der du der V\u00f6lker heil'ge Knospen spaltest,", "tokens": ["Der", "du", "der", "V\u00f6l\u00b7ker", "heil'\u00b7ge", "Knos\u00b7pen", "spal\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Hauch der Freiheit, weh' auch diese an!", "tokens": ["O", "Hauch", "der", "Frei\u00b7heit", ",", "weh'", "auch", "die\u00b7se", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In ihrem tiefsten, stillsten Heiligtume", "tokens": ["In", "ih\u00b7rem", "tiefs\u00b7ten", ",", "stills\u00b7ten", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O, k\u00fcss' sie auf zu Duft und Glanz und Schein \u2013", "tokens": ["O", ",", "k\u00fcss'", "sie", "auf", "zu", "Duft", "und", "Glanz", "und", "Schein", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Herr Gott im Himmel, welche Wunderblume", "tokens": ["Herr", "Gott", "im", "Him\u00b7mel", ",", "wel\u00b7che", "Wun\u00b7der\u00b7blu\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "APPRART", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird einst vor allen dieses Deutschland sein!", "tokens": ["Wird", "einst", "vor", "al\u00b7len", "die\u00b7ses", "Deutschland", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Am Baum der Menschheit dr\u00e4ngt sich Bl\u00fct' an Bl\u00fcte,", "tokens": ["Am", "Baum", "der", "Menschheit", "dr\u00e4ngt", "sich", "Bl\u00fct'", "an", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach ew'gen regeln wiegen sie sich drauf;", "tokens": ["Nach", "ew'\u00b7gen", "re\u00b7geln", "wie\u00b7gen", "sie", "sich", "drauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn hier die eine matt und welk vergl\u00fchte,", "tokens": ["Wenn", "hier", "die", "ei\u00b7ne", "matt", "und", "welk", "ver\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ART", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Springt dort die andre voll und pr\u00e4chtig auf.", "tokens": ["Springt", "dort", "die", "and\u00b7re", "voll", "und", "pr\u00e4ch\u00b7tig", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIS", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein ewig Kommen und ein ewig Gehen,", "tokens": ["Ein", "e\u00b7wig", "Kom\u00b7men", "und", "ein", "e\u00b7wig", "Ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nun und nimmer tr\u00e4ger Stillestand!", "tokens": ["Und", "nun", "und", "nim\u00b7mer", "tr\u00