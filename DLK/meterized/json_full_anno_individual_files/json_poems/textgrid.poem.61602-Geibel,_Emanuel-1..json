{"textgrid.poem.61602": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wird es licht, nun will der Fr\u00fchling nahn,", "tokens": ["Nun", "wird", "es", "licht", ",", "nun", "will", "der", "Fr\u00fch\u00b7ling", "nahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VMFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch blaue L\u00fcfte schifft der wilde Schwan,", "tokens": ["Durch", "blau\u00b7e", "L\u00fcf\u00b7te", "schifft", "der", "wil\u00b7de", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Berg zu Bergen webt der Sonnenstrahl,", "tokens": ["Von", "Berg", "zu", "Ber\u00b7gen", "webt", "der", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es jauchzt der Bach und springt ins Bl\u00fctental,", "tokens": ["Es", "jauchzt", "der", "Bach", "und", "springt", "ins", "Bl\u00fc\u00b7ten\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Wolke treibt im Wind, die Seglerin, \u2013", "tokens": ["Die", "Wol\u00b7ke", "treibt", "im", "Wind", ",", "die", "Seg\u00b7le\u00b7rin", ",", "\u