{"textgrid.poem.66502": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn du geliebt, wenn du gehofft,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du geliebt, wenn du gehofft,", "tokens": ["Wenn", "du", "ge\u00b7liebt", ",", "wenn", "du", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du gestrebt, gerungen,", "tokens": ["Wenn", "du", "ge\u00b7strebt", ",", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn du mit starkem Willen oft", "tokens": ["Wenn", "du", "mit", "star\u00b7kem", "Wil\u00b7len", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein lechzend Herz bezwungen,", "tokens": ["Dein", "lech\u00b7zend", "Herz", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann f\u00fchlst du, wie zu neuem Werth", "tokens": ["Dann", "f\u00fchlst", "du", ",", "wie", "zu", "neu\u00b7em", "Werth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erwacht dein ganzes Leben,", "tokens": ["Er\u00b7wacht", "dein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Denn jeder Schmerz, der dich beschwert,", "tokens": ["Denn", "je\u00b7der", "Schmerz", ",", "der", "dich", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird h\u00f6her dich erheben.", "tokens": ["Wird", "h\u00f6\u00b7her", "dich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schmerz ist das eine gro\u00dfe Band,", "tokens": ["Schmerz", "ist", "das", "ei\u00b7ne", "gro\u00b7\u00dfe", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das alle Welt umschlungen,", "tokens": ["Das", "al\u00b7le", "Welt", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es macht den Besten dich verwandt,", "tokens": ["Es", "macht", "den", "Bes\u00b7ten", "dich", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die je gedarbt, gerungen.", "tokens": ["Die", "je", "ge\u00b7darbt", ",", "ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es lehret den gesch\u00e4rften Blick,", "tokens": ["Es", "leh\u00b7ret", "den", "ge\u00b7sch\u00e4rf\u00b7ten", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was sich in Qual verborgen,", "tokens": ["Was", "sich", "in", "Qual", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir zeigt dein eigenes Geschick", "tokens": ["Dir", "zeigt", "dein", "ei\u00b7ge\u00b7nes", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Der ganzen Menschheit Sorgen.", "tokens": ["Der", "gan\u00b7zen", "Menschheit", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Schmerz ist die Sturmesmelodie,", "tokens": ["Schmerz", "ist", "die", "Stur\u00b7mes\u00b7me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mit deren Ton ersch\u00fctternd", "tokens": ["Mit", "de\u00b7ren", "Ton", "er\u00b7sch\u00fct\u00b7ternd"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jahrhundert um Jahrhundert schrie,", "tokens": ["Jahr\u00b7hun\u00b7dert", "um", "Jahr\u00b7hun\u00b7dert", "schrie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Todeskampf erzitternd.", "tokens": ["Dem", "To\u00b7des\u00b7kampf", "er\u00b7zit\u00b7ternd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Fluch, der mit Erweckungsmacht", "tokens": ["Ein", "Fluch", ",", "der", "mit", "Er\u00b7we\u00b7ckungs\u00b7macht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der V\u00f6lker Schlaf verklagte,", "tokens": ["Der", "V\u00f6l\u00b7ker", "Schlaf", "ver\u00b7klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mahnend aus der Zeiten Nacht", "tokens": ["Und", "mah\u00b7nend", "aus", "der", "Zei\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Kampf, zum Licht sie jagte.", "tokens": ["Zum", "Kampf", ",", "zum", "Licht", "sie", "jag\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und so entspro\u00df dem Fluch das Heil,", "tokens": ["Und", "so", "ent\u00b7spro\u00df", "dem", "Fluch", "das", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn aus Vernichtungstoben", "tokens": ["Denn", "aus", "Ver\u00b7nich\u00b7tungs\u00b7to\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat stets der Menschheit bessres Theil", "tokens": ["Hat", "stets", "der", "Menschheit", "bess\u00b7res", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Verj\u00fcngt sich und erhoben.", "tokens": ["Ver\u00b7j\u00fcngt", "sich", "und", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und jede gr\u00f6\u00dfte, sch\u00f6nste That,", "tokens": ["Und", "je\u00b7de", "gr\u00f6\u00df\u00b7te", ",", "sch\u00f6ns\u00b7te", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die neu die Welt gestaltet,", "tokens": ["Die", "neu", "die", "Welt", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hat sich aus schmerzgegebner Saat", "tokens": ["Hat", "sich", "aus", "schmerz\u00b7ge\u00b7geb\u00b7ner", "Saat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Sonnenlicht entfaltet.", "tokens": ["Dem", "Son\u00b7nen\u00b7licht", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was als ein weihevoller Klang", "tokens": ["Was", "als", "ein", "wei\u00b7he\u00b7vol\u00b7ler", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus heil'gem Schmerz erbl\u00fchte,", "tokens": ["Aus", "heil'\u00b7gem", "Schmerz", "er\u00b7bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird mit erl\u00f6sendem Empfang", "tokens": ["Wird", "mit", "er\u00b7l\u00f6\u00b7sen\u00b7dem", "Emp\u00b7fang"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergriffen im Gem\u00fcthe.", "tokens": ["Er\u00b7grif\u00b7fen", "im", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn Freude nur die Stunde weiht,", "tokens": ["Wenn", "Freu\u00b7de", "nur", "die", "Stun\u00b7de", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Lied, dem Schmerz entrungen,", "tokens": ["Ein", "Lied", ",", "dem", "Schmerz", "ent\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geht durch die Welt, geht durch die Zeit,", "tokens": ["Geht", "durch", "die", "Welt", ",", "geht", "durch", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Innern fortgesungen.", "tokens": ["Im", "In\u00b7nern", "fort\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn du geliebt, wenn du gehofft,", "tokens": ["Wenn", "du", "ge\u00b7liebt", ",", "wenn", "du", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du gestrebt, gerungen,", "tokens": ["Wenn", "du", "ge\u00b7strebt", ",", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn du mit starkem Willen oft", "tokens": ["Wenn", "du", "mit", "star\u00b7kem", "Wil\u00b7len", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein lechzend Herz bezwungen,", "tokens": ["Dein", "lech\u00b7zend", "Herz", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann f\u00fchlst du, wie zu neuem Werth", "tokens": ["Dann", "f\u00fchlst", "du", ",", "wie", "zu", "neu\u00b7em", "Werth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erwacht dein ganzes Leben,", "tokens": ["Er\u00b7wacht", "dein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-