{"textgrid.poem.50188": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "20. Auff Rebuffus den warmen Disputanten", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Ursach ist, mein Herr, dass ich diss Ding behaubt',", "tokens": ["Die", "Ur\u00b7sach", "ist", ",", "mein", "Herr", ",", "dass", "ich", "diss", "Ding", "be\u00b7haubt'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil die \u2013 \u2013 \u2013 ", "tokens": ["Die\u00b7weil", "die", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ART", "$(", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie? Eh' er mich geh\u00f6rt? Und darauff hebt er an,", "tokens": ["Wie", "?", "Eh'", "er", "mich", "ge\u00b7h\u00f6rt", "?", "Und", "dar\u00b7auff", "hebt", "er", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "PRF", "VVFIN", "$.", "KON", "PAV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und dass so wenig er, wie ich, verstehen kan.", "tokens": ["Und", "dass", "so", "we\u00b7nig", "er", ",", "wie", "ich", ",", "ver\u00b7ste\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "PPER", "$,", "PWAV", "PPER", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er redet, dass der Mund ihm sch\u00e4umt,", "tokens": ["Er", "re\u00b7det", ",", "dass", "der", "Mund", "ihm", "sch\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Und setzet den ", "tokens": ["Und", "set\u00b7zet", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "Ich segne mich, und mercke woll,", "tokens": ["Ich", "seg\u00b7ne", "mich", ",", "und", "mer\u00b7cke", "woll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dass ich die Thorheit sehr verd\u00e4chtig mich gemacht;", "tokens": ["Dass", "ich", "die", "Thor\u00b7heit", "sehr", "ver\u00b7d\u00e4ch\u00b7tig", "mich", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil ich ", "tokens": ["Weil", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Die Ursach ist, mein Herr, dass ich diss Ding behaubt',", "tokens": ["Die", "Ur\u00b7sach", "ist", ",", "mein", "Herr", ",", "dass", "ich", "diss", "Ding", "be\u00b7haubt'", ","], "token_in