{"dta.poem.4113": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Johannes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verschwieg'ner Liebesflammen Lust und Schmerz,", "tokens": ["Ver\u00b7schwieg'\u00b7ner", "Lie\u00b7bes\u00b7flam\u00b7men", "Lust", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und von den Kl\u00e4ngen f\u00fchl' ich nun durchdrungen", "tokens": ["Und", "von", "den", "Kl\u00e4n\u00b7gen", "f\u00fchl'", "ich", "nun", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit tiefer Regung fast mein eignes Herz.", "tokens": ["Mit", "tie\u00b7fer", "Re\u00b7gung", "fast", "mein", "eig\u00b7nes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Fr\u00fchling naht: schon tr\u00e4gt man aus dem Hause", "tokens": ["Der", "Fr\u00fch\u00b7ling", "naht", ":", "schon", "tr\u00e4gt", "man", "aus", "dem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Blumen an das freie Tageslicht;", "tokens": ["Die", "Blu\u00b7men", "an", "das", "frei\u00b7e", "Ta\u00b7ges\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und l\u00e4nger bleiben auch in ihrer Klause", "tokens": ["Und", "l\u00e4n\u00b7ger", "blei\u00b7ben", "auch", "in", "ih\u00b7rer", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Winterbl\u00fcthen meiner Muse nicht.", "tokens": ["Die", "Win\u00b7ter\u00b7bl\u00fc\u00b7then", "mei\u00b7ner", "Mu\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gedeihen mu\u00df die Lenzluft ihnen geben", "tokens": ["Ge\u00b7dei\u00b7hen", "mu\u00df", "die", "Lenz\u00b7luft", "ih\u00b7nen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und junges Gr\u00fcn und frischen Knospendrang,", "tokens": ["Und", "jun\u00b7ges", "Gr\u00fcn", "und", "fri\u00b7schen", "Knos\u00b7pen\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auf da\u00df sie sich befreunden mit dem Leben,", "tokens": ["Auf", "da\u00df", "sie", "sich", "be\u00b7freun\u00b7den", "mit", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und werben nach der Leute Lob und Dank.", "tokens": ["Und", "wer\u00b7ben", "nach", "der", "Leu\u00b7te", "Lob", "und", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So ziehn sie aus im Duft und Glanz des Maien,", "tokens": ["So", "ziehn", "sie", "aus", "im", "Duft", "und", "Glanz", "des", "Mai\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bekr\u00e4nzt mit schwarzem Leid und bunter Lust;", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "schwar\u00b7zem", "Leid", "und", "bun\u00b7ter", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und will der Winter sie mit Schnee bestreuen,", "tokens": ["Und", "will", "der", "Win\u00b7ter", "sie", "mit", "Schnee", "be\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "So fl\u00fcchten sie zur\u00fcck in deine Brust.", "tokens": ["So", "fl\u00fcch\u00b7ten", "sie", "zu\u00b7r\u00fcck", "in", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}