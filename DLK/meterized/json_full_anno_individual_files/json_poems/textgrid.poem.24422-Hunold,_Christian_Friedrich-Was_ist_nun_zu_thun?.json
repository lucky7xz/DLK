{"textgrid.poem.24422": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Was ist nun zu thun?", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brauch' alle gute Kunst/ dir einen Freund zu fangen.", "tokens": ["Brauch'", "al\u00b7le", "gu\u00b7te", "Kunst", "/", "dir", "ei\u00b7nen", "Freund", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$(", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du erlanget meinst/ such immer zu erlangen.", "tokens": ["Die", "du", "er\u00b7lan\u00b7get", "meinst", "/", "such", "im\u00b7mer", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$(", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey aller Leute Freund/ und glaube/ was nicht ist/", "tokens": ["Sey", "al\u00b7ler", "Leu\u00b7te", "Freund", "/", "und", "glau\u00b7be", "/", "was", "nicht", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$(", "KON", "VVFIN", "$(", "PWS", "PTKNEG", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df man dein Freund kan seyn/ wie du es allen bist.", "tokens": ["Da\u00df", "man", "dein", "Freund", "kan", "seyn", "/", "wie", "du", "es", "al\u00b7len", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VMFIN", "VAINF", "$(", "PWAV", "PPER", "PPER", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur traue keinem viel/ so sch\u00f6n er anzuschauen:", "tokens": ["Nur", "trau\u00b7e", "kei\u00b7nem", "viel", "/", "so", "sch\u00f6n", "er", "an\u00b7zu\u00b7schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$(", "ADV", "ADJD", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du bist dein bester Freund/ und darfst dir doch nicht trauen.", "tokens": ["Du", "bist", "dein", "bes\u00b7ter", "Freund", "/", "und", "darfst", "dir", "doch", "nicht", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Brauch' alle gute Kunst/ dir einen Freund zu fangen.", "tokens": ["Brauch'", "al\u00b7le", "gu\u00b7te", "Kunst", "/", "dir", "ei\u00b7nen", "Freund", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$(", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du erlanget meinst/ such immer zu erlangen.", "tokens": ["Die", "du", "er\u00b7lan\u00b7get", "meinst", "/", "such", "im\u00b7mer", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "$(", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-"