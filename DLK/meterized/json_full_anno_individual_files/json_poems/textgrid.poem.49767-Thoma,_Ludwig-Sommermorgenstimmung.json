{"textgrid.poem.49767": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sommermorgenstimmung", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, wie ist man froher Laune,", "tokens": ["Ja", ",", "wie", "ist", "man", "fro\u00b7her", "Lau\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "VAFIN", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man seine Zeitung liest", "tokens": ["Wenn", "man", "sei\u00b7ne", "Zei\u00b7tung", "liest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgens hinterm Gartenzaune", "tokens": ["Mor\u00b7gens", "hin\u00b7term", "Gar\u00b7ten\u00b7zau\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dazu Kaffee genie\u00dft!", "tokens": ["Und", "da\u00b7zu", "Kaf\u00b7fee", "ge\u00b7nie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Frauchen, gibt mir doch die Butter!", "tokens": ["Frau\u00b7chen", ",", "gibt", "mir", "doch", "die", "But\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Was heut' in der Zeitung steht? \u2013", "tokens": ["\u2013", "Was", "heut'", "in", "der", "Zei\u00b7tung", "steht", "?", "\u2013"], "token_info": ["punct", "word", "word", "word", "w