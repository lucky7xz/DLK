{"textgrid.poem.28104": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jener Zeit, die ietzt die alte,", "tokens": ["Je\u00b7ner", "Zeit", ",", "die", "ietzt", "die", "al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hilt man Deutschland viel zu kalte,", "tokens": ["Hilt", "man", "Deutschland", "viel", "zu", "kal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NE", "ADV", "APPR", "ADJA", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df daselbst, wie ieder wolte,", "tokens": ["Da\u00df", "da\u00b7selbst", ",", "wie", "ie\u00b7der", "wol\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "$,", "PWAV", "PIS", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Geduld erwachsen solte.", "tokens": ["Die", "Ge\u00b7duld", "er\u00b7wach\u00b7sen", "sol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun nur aber die sind kummen,", "tokens": ["Nun", "nur", "a\u00b7ber", "die", "sind", "kum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Bau recht f\u00fcrgenummen,", "tokens": ["Die", "den", "Bau", "recht", "f\u00fcr\u00b7ge\u00b7num\u00b7m