{"dta.poem.18982": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "9.  \n  Von jhren vbersch\u00f6nen augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr augen/ die jhr mich mit einem blick vnd plitz", "tokens": ["Ihr", "au\u00b7gen", "/", "die", "jhr", "mich", "mit", "ei\u00b7nem", "blick", "vnd", "plitz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "PRF", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scharpf oder s\u00fc\u00df nach lust k\u00f6nt strafen vnd be-", "tokens": ["Scharpf", "o\u00b7der", "s\u00fc\u00df", "nach", "lust", "k\u00f6nt", "stra\u00b7fen", "vnd", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "VMFIN", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "lohnen;", "tokens": ["loh\u00b7nen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "O liebliches gestirn/ Stern/ deren liecht vnd hitz", "tokens": ["O", "lieb\u00b7li\u00b7ches", "ge\u00b7stirn", "/", "Stern", "/", "de\u00b7ren", "liecht", "vnd", "hitz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "NN", "$(", "PDS", "VVFIN", "KON", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ka\u0303/ z\u00fcchtigend den stoltz/ der z\u00fcchtigen verschonen:", "tokens": ["K\u00e3", "/", "z\u00fcch\u00b7ti\u00b7gend", "den", "stoltz", "/", "der", "z\u00fcch\u00b7ti\u00b7gen", "ver\u00b7scho\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVPP", "ART", "NN", "$(", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vnd jhr/ der Lieb werckzeug/ kundschaffter vnsrer", "tokens": ["Vnd", "jhr", "/", "der", "Lieb", "wer\u00b7ck\u00b7zeug", "/", "kund\u00b7schaff\u00b7ter", "vns\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "$(", "ART", "NN", "VVFIN", "$(", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Witz/", "tokens": ["Witz", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Augbrawen/ ja vilmehr triumfbogen/ nein/ Crone\u0304/", "tokens": ["Aug\u00b7bra\u00b7wen", "/", "ja", "vil\u00b7mehr", "tri\u00b7umf\u00b7bo\u00b7gen", "/", "nein", "/", "Cron\u0113", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "ADV", "ADV", "VVINF", "$(", "PTKANT", "$(", "NE", "$("], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Darunder lieb vnd zucht in vbersch\u00f6nem sitz", "tokens": ["Da\u00b7run\u00b7der", "lieb", "vnd", "zucht", "in", "vber\u00b7sch\u00f6\u00b7nem", "sitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Mit brauner klarheit schmuck erleuchtet/ leuch-", "tokens": ["Mit", "brau\u00b7ner", "klar\u00b7heit", "schmuck", "er\u00b7leuch\u00b7tet", "/", "leuch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$(", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "tend wohnen!", "tokens": ["tend", "woh\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wer recht kan ewre form/ farb/ wesen/ w\u00fcrckung/", "tokens": ["Wer", "recht", "kan", "ew\u00b7re", "form", "/", "farb", "/", "we\u00b7sen", "/", "w\u00fcr\u00b7ckung", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPOSAT", "NN", "$(", "VVFIN", "$(", "VVINF", "$(", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "krafft/", "tokens": ["krafft", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der kan der Engeln stand/ schein/ sch\u00f6nheit/ thun", "tokens": ["Der", "kan", "der", "En\u00b7geln", "stand", "/", "schein", "/", "sch\u00f6n\u00b7heit", "/", "thun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVFIN", "$(", "ADJD", "$(", "ADJD", "$(", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vnd gehen/", "tokens": ["vnd", "ge\u00b7hen", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der kan der wahren lieb gewalt vn\u0303 aygenschafft/", "tokens": ["Der", "kan", "der", "wah\u00b7ren", "lieb", "ge\u00b7walt", "v\u00f1", "ay\u00b7gen\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "ADJD", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Sch\u00f6nheit sch\u00f6nheit selbs/ der seelen frewd vnd", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "sch\u00f6n\u00b7heit", "selbs", "/", "der", "see\u00b7len", "frewd", "vnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ART", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "flehen/", "tokens": ["fle\u00b7hen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Vnd der Glickseeligkeit vnd Tugenten freind-", "tokens": ["Vnd", "der", "Glick\u00b7see\u00b7lig\u00b7keit", "vnd", "Tu\u00b7gen\u00b7ten", "frein\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "schafft/", "tokens": ["schafft", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "In Euch (der Natur k\u0169st besehend) wol verstehen.", "tokens": ["In", "Euch", "(", "der", "Na\u00b7tur", "k\u0169st", "be\u00b7se\u00b7hend", ")", "wol", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "ART", "NN", "VVFIN", "VVPP", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}