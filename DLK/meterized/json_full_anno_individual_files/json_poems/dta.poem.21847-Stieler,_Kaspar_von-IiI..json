{"dta.poem.21847": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hielt\u2019 au\u00df Scherz ein liebes Kind umfan-", "tokens": ["Ich", "hielt'", "au\u00df", "Scherz", "ein", "lie\u00b7bes", "Kind", "um\u00b7fan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(gen/", "tokens": ["(", "gen", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "die Eyfer-sucht verf\u00e4rbte Mund und Wau-", "tokens": ["die", "Ey\u00b7fer\u00b7sucht", "ver\u00b7f\u00e4rb\u00b7te", "Mund", "und", "Wau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "als sie di\u00df Spiel von ungesehr ersach.", "tokens": ["als", "sie", "di\u00df", "Spiel", "von", "un\u00b7ge\u00b7sehr", "er\u00b7sach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "H\u00e4tt\u2019 Eyfer nicht den Eyfer lassen sehen/", "tokens": ["H\u00e4tt'", "Ey\u00b7fer", "nicht", "den", "Ey\u00b7fer", "las\u00b7sen", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ich w\u00fcrde nicht so wol mit Buschgen stehen/", "tokens": ["ich", "w\u00fcr\u00b7de", "nicht", "so", "wol", "mit", "Buschgen", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nu wird es Ernst/ was vor au\u00df Scherz", "tokens": ["Nu", "wird", "es", "Ernst", "/", "was", "vor", "au\u00df", "Scherz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$(", "PWS", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}