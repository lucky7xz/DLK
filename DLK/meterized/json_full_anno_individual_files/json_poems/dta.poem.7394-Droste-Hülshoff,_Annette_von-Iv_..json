{"dta.poem.7394": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.85", "sv:0.14"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ho! Gl\u00e4serklang und Jubelsang und \u201eHurrah hoch!\u201c f\u00e4hrt's", "tokens": ["Ho", "!", "Gl\u00e4\u00b7ser\u00b7klang", "und", "Ju\u00b7bel\u00b7sang", "und", "\u201e", "Hur\u00b7rah", "hoch", "!", "\u201c", "f\u00e4hrt's"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["NE", "$.", "NN", "KON", "NN", "KON", "$(", "NN", "ADJD", "$.", "$(", "VVFIN"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "durch die Scheiben,", "tokens": ["durch", "die", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Getroffen schwankt der goldne Leu, die Buben aus einander", "tokens": ["Ge\u00b7trof\u00b7fen", "schwankt", "der", "gold\u00b7ne", "Leu", ",", "die", "Bu\u00b7ben", "aus", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "st\u00e4uben,", "tokens": ["st\u00e4u\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und dr\u00e4ngen sich und balgen sich das fliegende Confekt zu", "tokens": ["Und", "dr\u00e4n\u00b7gen", "sich", "und", "bal\u00b7gen", "sich", "das", "flie\u00b7gen\u00b7de", "Con\u00b7fekt", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "fangen;", "tokens": ["fan\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Ein Glas, 'ne Frucht, 'ne B\u00f6rse gar, die blieb am Speer", "tokens": ["Ein", "Glas", ",", "'ne", "Frucht", ",", "'ne", "B\u00f6r\u00b7se", "gar", ",", "die", "blieb", "am", "Speer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "des Schildes hangen,", "tokens": ["des", "Schil\u00b7des", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und schreiend nach der Stange sticht", "tokens": ["Und", "schrei\u00b7end", "nach", "der", "Stan\u00b7ge", "sticht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das kleine gierige Gez\u00fccht.", "tokens": ["Das", "klei\u00b7ne", "gie\u00b7ri\u00b7ge", "Ge\u00b7z\u00fccht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da klirrt aus des Balkones Th\u00fcr ein Mann mit Gert' und", "tokens": ["Da", "klirrt", "aus", "des", "Bal\u00b7ko\u00b7nes", "Th\u00fcr", "ein", "Mann", "mit", "Gert'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "KON"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Eisensporen,", "tokens": ["Ei\u00b7sen\u00b7spo\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ihm nach ein Andrer, Flasch' im Arm, in Rausches Selig-", "tokens": ["Ihm", "nach", "ein", "A\u00b7ndrer", ",", "Fla\u00b7sch'", "im", "Arm", ",", "in", "Rau\u00b7sches", "Se\u00b7lig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "NN", "APPRART", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "keit verloren,", "tokens": ["keit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "\u201egesindel!\u201c ruft der Eine: \u201ehalt! ich will euch lehren B\u00f6rsen", "tokens": ["\u201e", "ge\u00b7sin\u00b7del", "!", "\u201c", "ruft", "der", "Ei\u00b7ne", ":", "\u201e", "halt", "!", "ich", "will", "euch", "leh\u00b7ren", "B\u00f6r\u00b7sen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "ART", "NN", "$.", "$(", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "stechen!\u201c", "tokens": ["ste\u00b7chen", "!", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["VVINF", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "\u201efrisch, Jungens, frisch!\u201c der Andre drauf: \u201edie Birn ist mein,", "tokens": ["\u201e", "frisch", ",", "Jun\u00b7gens", ",", "frisch", "!", "\u201c", "der", "And\u00b7re", "drauf", ":", "\u201e", "die", "Birn", "ist", "mein", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "ADJD", "$.", "$(", "ART", "PIS", "PTKVZ", "$.", "$(", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wer kann sie brechen?", "tokens": ["wer", "kann", "sie", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ihn schlag' ich heut', ich, Hans von Spaa,", "tokens": ["Ihn", "schlag'", "ich", "heut'", ",", "ich", ",", "Hans", "von", "Spaa", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "$,", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zum Ritter von Lumpatia.\u201c", "tokens": ["Zum", "Rit\u00b7ter", "von", "Lum\u00b7pa\u00b7tia", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201ebesinnt euch,\u201c spricht der Erste; \u201ewas, besinnen? hab' ich", "tokens": ["\u201e", "be\u00b7sinnt", "euch", ",", "\u201c", "spricht", "der", "Ers\u00b7te", ";", "\u201e", "was", ",", "be\u00b7sin\u00b7nen", "?", "hab'", "ich"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ART", "ADJA", "$.", "$(", "PWS", "$,", "VVINF", "$.", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.2": {"text": "mich besonnen", "tokens": ["mich", "be\u00b7son\u00b7nen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Als euer Falber wie'n gestochner Stier zusammenbrach am", "tokens": ["Als", "eu\u00b7er", "Fal\u00b7ber", "wie'n", "ge\u00b7stoch\u00b7ner", "Stier", "zu\u00b7sam\u00b7men\u00b7brach", "am"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "ADV", "APPRART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Bronnen?", "tokens": ["Bron\u00b7nen", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Besann ich mich zu zahlen, Herr? o euer Vieh! dreihundert", "tokens": ["Be\u00b7sann", "ich", "mich", "zu", "zah\u00b7len", ",", "Herr", "?", "o", "eu\u00b7er", "Vieh", "!", "drei\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,", "NN", "$.", "FM", "PPOSAT", "NN", "$.", "CARD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Kronen!\u201c", "tokens": ["Kro\u00b7nen", "!", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die Stimme bricht in trunknem Weh, er schluchzt: \u201emag", "tokens": ["Die", "Stim\u00b7me", "bricht", "in", "trunk\u00b7nem", "Weh", ",", "er", "schluchzt", ":", "\u201e", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "$.", "$(", "VMFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "euch der Teufel lohnen!\u201c", "tokens": ["euch", "der", "Teu\u00b7fel", "loh\u00b7nen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Und schraubt den Pfropfenzieher ein;", "tokens": ["Und", "schraubt", "den", "Pfrop\u00b7fen\u00b7zie\u00b7her", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der T\u00e4uscher murmelt finster drein,", "tokens": ["Der", "T\u00e4u\u00b7scher", "mur\u00b7melt", "fins\u00b7ter", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wendet sich. \u201eHe, holla, halt!\u201c schreit's hinter ihm,", "tokens": ["Und", "wen\u00b7det", "sich", ".", "\u201e", "He", ",", "hol\u00b7la", ",", "halt", "!", "\u201c", "schreit's", "hin\u00b7ter", "ihm", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$.", "$(", "ITJ", "$,", "ITJ", "$,", "VVFIN", "$.", "$(", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enicht von der Stelle!", "tokens": ["\u201e", "nicht", "von", "der", "Stel\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hoch euer Galgenm\u00e4nnlein, hoch der kleine rauchige Geselle!", "tokens": ["Hoch", "eu\u00b7er", "Gal\u00b7gen\u00b7m\u00e4nn\u00b7lein", ",", "hoch", "der", "klei\u00b7ne", "rau\u00b7chi\u00b7ge", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "ADJD", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und wieder hoch! und dreimal hoch! \u2014 Alr\u00e4unchen, H\u00fctchen", "tokens": ["Und", "wie\u00b7der", "hoch", "!", "und", "drei\u00b7mal", "hoch", "!", "Al\u00b7r\u00e4un\u00b7chen", ",", "H\u00fct\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "ADJD", "$.", "KON", "ADV", "ADJD", "$.", "$(", "NE", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "meinetwegen,", "tokens": ["mei\u00b7net\u00b7we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mag's ferner goldne Eier euch, und Andern todte B\u00e4lge", "tokens": ["Mag's", "fer\u00b7ner", "gold\u00b7ne", "Ei\u00b7er", "euch", ",", "und", "An\u00b7dern", "tod\u00b7te", "B\u00e4l\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "NN", "PPER", "$,", "KON", "ADJA", "ADJA", "NN"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.7": {"text": "legen!'", "tokens": ["le\u00b7gen", "!", "'"], "token_info": ["word", "punct", "word"], "pos": ["VVINF", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Der T\u00e4uscher l\u00e4chelt, aschenfahl,", "tokens": ["Der", "T\u00e4u\u00b7scher", "l\u00e4\u00b7chelt", ",", "asc\u00b7hen\u00b7fahl", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und schlendert pfeifend in den Saal.", "tokens": ["Und", "schlen\u00b7dert", "pfei\u00b7fend", "in", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Noch zwei Minuten, und du siehst den Gassenp\u00f6bel vor ihm", "tokens": ["Noch", "zwei", "Mi\u00b7nu\u00b7ten", ",", "und", "du", "siehst", "den", "Gas\u00b7sen\u00b7p\u00f6\u00b7bel", "vor", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "weichen,", "tokens": ["wei\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ihn scheu wie ein umstelltes Wild entlang die H\u00e4userreihen", "tokens": ["Ihn", "scheu", "wie", "ein", "um\u00b7stell\u00b7tes", "Wild", "ent\u00b7lang", "die", "H\u00e4u\u00b7ser\u00b7rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "streichen:", "tokens": ["strei\u00b7chen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So schleicht kein Trinker schweren Hirns und freudesatt sich", "tokens": ["So", "schleicht", "kein", "Trin\u00b7ker", "schwe\u00b7ren", "Hirns", "und", "freu\u00b7de\u00b7satt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADJA", "NN", "KON", "ADJD", "PRF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "vom Gelage,", "tokens": ["vom", "Ge\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "So gr\u00fc\u00dft kein freies Herz, nicht steht auf offner Stirn so", "tokens": ["So", "gr\u00fc\u00dft", "kein", "frei\u00b7es", "Herz", ",", "nicht", "steht", "auf", "off\u00b7ner", "Stirn", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "tr\u00fcbe Frage;", "tokens": ["tr\u00fc\u00b7be", "Fra\u00b7ge", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Man meint, das Thor gewinne jetzt", "tokens": ["Man", "meint", ",", "das", "Thor", "ge\u00b7win\u00b7ne", "jetzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Schelm, von Gl\u00e4ubigern gehetzt.", "tokens": ["Ein", "Schelm", ",", "von", "Gl\u00e4u\u00b7bi\u00b7gern", "ge\u00b7hetzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Erst als die Fichte ihn umstarrt, an seiner Sohle Nadeln", "tokens": ["Erst", "als", "die", "Fich\u00b7te", "ihn", "um\u00b7starrt", ",", "an", "sei\u00b7ner", "Soh\u00b7le", "Na\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "meas