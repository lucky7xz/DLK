{"dta.poem.846": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII.  Auf den Sontag de\u00df vor vns bitten-  \n den Vorsprechers/ oder  Vocem Jucundita-  \n tis.  Johan. 16.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich! der ich Asch vnd Kott/ mag ich mich vnterfangen ", "tokens": ["Ich", "!", "der", "ich", "Asch", "vnd", "Kott", "/", "mag", "ich", "mich", "vn\u00b7ter\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$.", "PRELS", "PPER", "NN", "KON", "NN", "$(", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den S\u00fcnd vnd fluch beschwert/ zu knien HErr f\u00fcr", "tokens": ["Den", "S\u00fcnd", "vnd", "fluch", "be\u00b7schwert", "/", "zu", "kni\u00b7en", "Herr", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$(", "PTKZU", "VVFIN", "NN", "APPR"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "dich? ", "tokens": ["dich", "?"], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Mein eigen Hertz vnd Geist vnd schuld verklaget mich/", "tokens": ["Mein", "ei\u00b7gen", "Hertz", "vnd", "Geist", "vnd", "schuld", "ver\u00b7kla\u00b7get", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "KON", "ADJD", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Teufel schrey\u2019t mich an: Vmbsonst sey mein verlangen/", "tokens": ["Der", "Teu\u00b7fel", "schrey't", "mich", "an", ":", "Vmbsonst", "sey", "mein", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "PPOSAT", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Hast du dein Antlitz HErr/ mit dicker Nacht vmb-hangen?", "tokens": ["Hast", "du", "dein", "Ant\u00b7litz", "Herr", "/", "mit", "di\u00b7cker", "Nacht", "vm\u00b7bhan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "NN", "$(", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6rst du die S\u00fcnder nicht? di\u00df ist der Schlangen stich", "tokens": ["H\u00f6rst", "du", "die", "S\u00fcn\u00b7der", "nicht", "?", "di\u00df", "ist", "der", "Schlan\u00b7gen", "stich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$.", "PDS", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der zu verzweifeln dringt. Mein Vatter schaw doch; Ich/", "tokens": ["Der", "zu", "ver\u00b7zwei\u00b7feln", "dringt", ".", "Mein", "Vat\u00b7ter", "schaw", "doch", ";", "Ich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "$.", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich dein betr\u00fcbtes Kind/ bin schier in angst vergangen.", "tokens": ["Ich", "dein", "be\u00b7tr\u00fcb\u00b7tes", "Kind", "/", "bin", "schier", "in", "angst", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$(", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Mein abge\u00e4ngster Geist aus dieser toden grufft", "tokens": ["Mein", "ab\u00b7ge\u00b7\u00e4ngs\u00b7ter", "Geist", "aus", "die\u00b7ser", "to\u00b7den", "grufft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schaw Vatter vmb sein Blutt auff dieses threnen-rinnen.", "tokens": ["Schaw", "Vat\u00b7ter", "vmb", "sein", "Blutt", "auff", "die\u00b7ses", "thre\u00b7nen\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weil mich dein liebster Sohn inst\u00e4ndig bitten heist", "tokens": ["Weil", "mich", "dein", "liebs\u00b7ter", "Sohn", "in\u00b7st\u00e4n\u00b7dig", "bit\u00b7ten", "heist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "vnd mir ohn vnterla\u00df selbst f\u00fcr dir beystand leist", "tokens": ["vnd", "mir", "ohn", "vn\u00b7ter\u00b7la\u00df", "selbst", "f\u00fcr", "dir", "beys\u00b7tand", "leist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird was ich heische/ mir/ dein Hertz nicht wegern k\u00f6nnen.", "tokens": ["Wird", "was", "ich", "hei\u00b7sche", "/", "mir", "/", "dein", "Hertz", "nicht", "we\u00b7gern", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWS", "PPER", "ADJA", "$(", "PPER", "$(", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}