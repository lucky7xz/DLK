{"dta.poem.3730": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Abz\u00e4hlen bei dem Spiel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00e4ger bind dein H\u00fcndlein an,               ", "tokens": ["J\u00e4\u00b7ger", "bind", "dein", "H\u00fcnd\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df es mich nicht beissen kann,", "tokens": ["Da\u00df", "es", "mich", "nicht", "beis\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei\u00dft es mich,", "tokens": ["Bei\u00dft", "es", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Straf ich dich,", "tokens": ["Straf", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Um sechshundert dreissig.", "tokens": ["Um", "sechs\u00b7hun\u00b7dert", "dreis\u00b7sig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}