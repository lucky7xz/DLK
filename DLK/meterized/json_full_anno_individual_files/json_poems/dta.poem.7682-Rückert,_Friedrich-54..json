{"dta.poem.7682": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nun genug die Fluren mir beschaut,", "tokens": ["Ich", "ha\u00b7be", "nun", "ge\u00b7nug", "die", "Flu\u00b7ren", "mir", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mich an Blumenschrift nach meinem Sinn erbaut.", "tokens": ["Und", "mich", "an", "Blu\u00b7men\u00b7schrift", "nach", "mei\u00b7nem", "Sinn", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich suche sch\u00f6nere nicht mehr im Erdenreich,", "tokens": ["Ich", "su\u00b7che", "sch\u00f6\u00b7ne\u00b7re", "nicht", "mehr", "im", "Er\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn alle Fluren sehn in Einem doch sich gleich.", "tokens": ["Denn", "al\u00b7le", "Flu\u00b7ren", "sehn", "in", "Ei\u00b7nem", "doch", "sich", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PIS", "ADV", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O buntes Einerlei, statt deiner m\u00f6cht' ich Auen", "tokens": ["O", "bun\u00b7tes", "Ei\u00b7ner\u00b7lei", ",", "statt", "dei\u00b7ner", "m\u00f6cht'", "ich", "Au\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KOUI", "PPOSAT", "VMFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einmal ganz andre mit ganz andern Augen schauen.", "tokens": ["Ein\u00b7mal", "ganz", "and\u00b7re", "mit", "ganz", "an\u00b7dern", "Au\u00b7gen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}