{"dta.poem.21529": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Sang der Lebendigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wer still sein Leben in altem Geleis               ", "tokens": ["Wer", "still", "sein", "Le\u00b7ben", "in", "al\u00b7tem", "Ge\u00b7leis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verschleppt und m\u00fchsam sorgendem Flei\u00df,", "tokens": ["Ver\u00b7schleppt", "und", "m\u00fch\u00b7sam", "sor\u00b7gen\u00b7dem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der falte die H\u00e4nde nur gl\u00e4ubig im Schoo\u00df", "tokens": ["Der", "fal\u00b7te", "die", "H\u00e4n\u00b7de", "nur", "gl\u00e4u\u00b7big", "im", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und lalle und stammle von seligem Loos!", "tokens": ["Und", "lal\u00b7le", "und", "stamm\u00b7le", "von", "se\u00b7li\u00b7gem", "Loos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Doch wir f\u00fchlen die Kraft und wir st\u00fcrmen hinaus", "tokens": ["Doch", "wir", "f\u00fch\u00b7len", "die", "Kraft", "und", "wir", "st\u00fcr\u00b7men", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "APZR"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Mit dem flammenden Haupte zum Kampfe, zum Strau\u00df,", "tokens": ["Mit", "dem", "flam\u00b7men\u00b7den", "Haup\u00b7te", "zum", "Kamp\u00b7fe", ",", "zum", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "APPRART", "NE", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "In den d\u00fcsteren Augen den blitzenden Strahl,", "tokens": ["In", "den", "d\u00fcs\u00b7te\u00b7ren", "Au\u00b7gen", "den", "blit\u00b7zen\u00b7den", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Den Donner im Mund, auf der Stirne das Mal.", "tokens": ["Den", "Don\u00b7ner", "im", "Mund", ",", "auf", "der", "Stir\u00b7ne", "das", "Mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "In Gluthen getaucht mag die Welt uns vergeh\u2019n,", "tokens": ["In", "Glu\u00b7then", "ge\u00b7taucht", "mag", "die", "Welt", "uns", "ver\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Erbrausen mu\u00df sie im Sturmesweh\u2019n,", "tokens": ["Er\u00b7brau\u00b7sen", "mu\u00df", "sie", "im", "Stur\u00b7mes\u00b7weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Soll\u2019n den lebendigen Odem wir trinken,", "tokens": ["Soll'n", "den", "le\u00b7ben\u00b7di\u00b7gen", "O\u00b7dem", "wir", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Nicht in dem l\u00e4hmenden Joche mehr hinken,", "tokens": ["Nicht", "in", "dem", "l\u00e4h\u00b7men\u00b7den", "Jo\u00b7che", "mehr", "hin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "In dem sie zieht und schleppt und lebt", "tokens": ["In", "dem", "sie", "zieht", "und", "schleppt", "und", "lebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und nimmer an ew\u2019gen Gebilden mehr webt. \u2014", "tokens": ["Und", "nim\u00b7mer", "an", "ew'\u00b7gen", "Ge\u00b7bil\u00b7den", "mehr", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Du heilige Liebe, du hast uns gefeit,", "tokens": ["Du", "hei\u00b7li\u00b7ge", "Lie\u00b7be", ",", "du", "hast", "uns", "ge\u00b7feit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du gabst uns allen das stahlharte Kleid,", "tokens": ["Du", "gabst", "uns", "al\u00b7len", "das", "stahl\u00b7har\u00b7te", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und drunter das Herz mit dem zuckenden Schlag,", "tokens": ["Und", "drun\u00b7ter", "das", "Herz", "mit", "dem", "zu\u00b7cken\u00b7den", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das blutet vom Dorne des Elends, der Schmach,", "tokens": ["Das", "blu\u00b7tet", "vom", "Dor\u00b7ne", "des", "E\u00b7lends", ",", "der", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Das Herz, das den Kreuzestod tausendmal litt \u2014", "tokens": ["Das", "Herz", ",", "das", "den", "Kreu\u00b7ze\u00b7stod", "tau\u00b7send\u00b7mal", "litt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Doch drau\u00dfen die Stirn, so kalt wie Granit,", "tokens": ["Doch", "drau\u00b7\u00dfen", "die", "Stirn", ",", "so", "kalt", "wie", "Gra\u00b7nit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So still, wie die Felsen zum Himmel starr\u2019n,", "tokens": ["So", "still", ",", "wie", "die", "Fel\u00b7sen", "zum", "Him\u00b7mel", "starr'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Durchfurcht von der St\u00fcrme gewalt\u2019gem Beharr\u2019n.", "tokens": ["Durch\u00b7furcht", "von", "der", "St\u00fcr\u00b7me", "ge\u00b7walt'\u00b7gem", "Be\u00b7harr'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "NE"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Kein lockendes Eiland, von Palmen ums\u00e4umt,", "tokens": ["Kein", "lo\u00b7cken\u00b7des", "Ei\u00b7land", ",", "von", "Pal\u00b7men", "um\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Wie\u2019s der Dichter in seligen Tr\u00e4umen sich tr\u00e4umt,", "tokens": ["Wie's", "der", "Dich\u00b7ter", "in", "se\u00b7li\u00b7gen", "Tr\u00e4u\u00b7men", "sich", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Kein lachendes Thal von Gl\u00fcckseligkeit,", "tokens": ["Kein", "la\u00b7chen\u00b7des", "Thal", "von", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Kein wonniges Eden der goldenen Zeit", "tokens": ["Kein", "won\u00b7ni\u00b7ges", "E\u00b7den", "der", "gol\u00b7de\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Ist\u2019s, was uns lockt in kindlichem Drang", "tokens": ["Ist's", ",", "was", "uns", "lockt", "in", "kind\u00b7li\u00b7chem", "Drang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Mit der Freude s\u00fc\u00dfem Sirenengesang.", "tokens": ["Mit", "der", "Freu\u00b7de", "s\u00fc\u00b7\u00dfem", "Si\u00b7re\u00b7nen\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+---+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Zur Fahrt auf der Schmerzen wildwogende See,", "tokens": ["Zur", "Fahrt", "auf", "der", "Schmer\u00b7zen", "wild\u00b7wo\u00b7gen\u00b7de", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Sturme des Lebens, durch Kummer und Weh,", "tokens": ["Im", "Stur\u00b7me", "des", "Le\u00b7bens", ",", "durch", "Kum\u00b7mer", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Durch die Fluthen flammender Leidenschaft", "tokens": ["Durch", "die", "Flut\u00b7hen", "flam\u00b7men\u00b7der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zieht uns die heil\u2019ge besel\u2019gende Kraft \u2014", "tokens": ["Zieht", "uns", "die", "heil'\u00b7ge", "be\u00b7sel'\u00b7gen\u00b7de", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Aus des gl\u00fchenden Weibes wei\u00dfen Arm,", "tokens": ["Aus", "des", "gl\u00fc\u00b7hen\u00b7den", "Wei\u00b7bes", "wei\u00b7\u00dfen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der so leis um den Hals sich, so weich und so warm,", "tokens": ["Der", "so", "leis", "um", "den", "Hals", "sich", ",", "so", "weich", "und", "so", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "ART", "NN", "PRF", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "So fest, wie mit ehernen Banden sich schmiegt,", "tokens": ["So", "fest", ",", "wie", "mit", "e\u00b7her\u00b7nen", "Ban\u00b7den", "sich", "schmiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PWAV", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Im d\u00e4mmernden Hauche, dem alles erliegt,", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Hau\u00b7che", ",", "dem", "al\u00b7les", "er\u00b7liegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Der m\u00fcden, duftigen Sommernacht \u2014", "tokens": ["Der", "m\u00fc\u00b7den", ",", "duf\u00b7ti\u00b7gen", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Hinweg rei\u00dft\u2019s uns \u2014 aus den S\u00e4len der Pracht,", "tokens": ["Hin\u00b7weg", "rei\u00dft's", "uns", "aus", "den", "S\u00e4\u00b7len", "der", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Aus der Welt des Taumels, der \u00fcppigsten Lust,", "tokens": ["Aus", "der", "Welt", "des", "Tau\u00b7mels", ",", "der", "\u00fcp\u00b7pigs\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Zu Noth und Elend und Todeswust \u2014", "tokens": ["Zu", "Noth", "und", "E\u00b7lend", "und", "To\u00b7des\u00b7wust"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Aus dem jubelnden Schwarm und dem l\u00e4rmenden Fest \u2014", "tokens": ["Aus", "dem", "ju\u00b7beln\u00b7den", "Schwarm", "und", "dem", "l\u00e4r\u00b7men\u00b7den", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Zu trocknen die Thr\u00e4nen, die bitter erpre\u00dft", "tokens": ["Zu", "trock\u00b7nen", "die", "Thr\u00e4\u00b7nen", ",", "die", "bit\u00b7ter", "er\u00b7pre\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.15": {"text": "Jahrtausende lang mit blutigem Zahn,", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "lang", "mit", "blu\u00b7ti\u00b7gem", "Zahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Der grimmer wie Pest \u2014 ein elender Wahn.", "tokens": ["Der", "grim\u00b7mer", "wie", "Pest", "ein", "e\u00b7len\u00b7der", "Wahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Gott, der uns nicht straucheln l\u00e4\u00dft,", "tokens": ["Der", "Gott", ",", "der", "uns", "nicht", "strau\u00b7cheln", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hinan uns f\u00fchrt, so sicher und fest", "tokens": ["Der", "hi\u00b7nan", "uns", "f\u00fchrt", ",", "so", "si\u00b7cher", "und", "fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER",