{"dta.poem.10489": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Herz, mein Herz, sey nicht beklommen,", "tokens": ["Herz", ",", "mein", "Herz", ",", "sey", "nicht", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ertrage dein Geschick,", "tokens": ["Und", "er\u00b7tra\u00b7ge", "dein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neuer Fr\u00fchling giebt zur\u00fcck,", "tokens": ["Neu\u00b7er", "Fr\u00fch\u00b7ling", "giebt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Winter dir genommen.", "tokens": ["Was", "der", "Win\u00b7ter", "dir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie viel ist dir geblieben!", "tokens": ["Und", "wie", "viel", "ist", "dir", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie sch\u00f6n ist noch die Welt;", "tokens": ["Und", "wie", "sch\u00f6n", "ist", "noch", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und, mein Herz, was dir gef\u00e4llt,", "tokens": ["Und", ",", "mein", "Herz", ",", "was", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, Alles darfst du lieben!", "tokens": ["Al\u00b7les", ",", "Al\u00b7les", "darfst", "du", "lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}