{"textgrid.poem.58133": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "171. La\u00df, was nichtig; tu, was wichtig!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kann ein Mensch mit eiteln Sachen", "tokens": ["Wie", "kann", "ein", "Mensch", "mit", "ei\u00b7teln", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich so viel M\u00fch und Unruh machen!", "tokens": ["Sich", "so", "viel", "M\u00fch", "und", "Un\u00b7ruh", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt ist keinen Seufzer wert.", "tokens": ["Die", "Welt", "ist", "kei\u00b7nen", "Seuf\u00b7zer", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wichtig Werk hab' ich zu handeln:", "tokens": ["Ein", "wich\u00b7tig", "Werk", "hab'", "ich", "zu", "han\u00b7deln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Gott und in Gott kindlich wandeln,", "tokens": ["Vor", "Gott", "und", "in", "Gott", "kind\u00b7lich", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic