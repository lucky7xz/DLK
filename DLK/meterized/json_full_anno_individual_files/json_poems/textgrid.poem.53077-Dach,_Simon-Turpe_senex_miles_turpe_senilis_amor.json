{"textgrid.poem.53077": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Turpe senex miles turpe senilis amor", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll denn mein Junges Leben,", "tokens": ["Soll", "denn", "mein", "Jun\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da alles liebt vnd freyht,", "tokens": ["Da", "al\u00b7les", "liebt", "vnd", "freyht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alleine sich ergeben", "tokens": ["Al\u00b7lei\u00b7ne", "sich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der langen Einsamkeit?", "tokens": ["Der", "lan\u00b7gen", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bleibt dann die Frewd vnd Lust", "tokens": ["Bleibt", "dann", "die", "Frewd", "vnd", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der schleyer-weissen Brust,", "tokens": ["Der", "schleyer\u00b7weis\u00b7sen", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nach der wir alle streben,", "tokens": ["Nach", "der", "wir", "al\u00b7le", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir Ewig vnbewust?", "tokens": ["Mir", "E\u00b7wig", "vn\u00b7be\u00b7wust", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die W\u00fcrme, die nur schleichen,", "tokens": ["Die", "W\u00fcr\u00b7me", ",", "die", "nur", "schlei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schnellen Fisch im Meer,", "tokens": ["Die", "schnel\u00b7len", "Fisch", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wild in den Gestr\u00e4uchen,", "tokens": ["Das", "Wild", "in", "den", "Ge\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00f6gel leichtes Heer,", "tokens": ["Der", "V\u00f6\u00b7gel", "leich\u00b7tes", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd was sich in der Welt", "tokens": ["Vnd", "was", "sich", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch Lufft vnd Flut erhelt,", "tokens": ["Durch", "Lufft", "vnd", "Flut", "er\u00b7helt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kriegt jedes seines gleichen,", "tokens": ["Kriegt", "je\u00b7des", "sei\u00b7nes", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sobald es jhm gefellt.", "tokens": ["So\u00b7bald", "es", "jhm", "ge\u00b7fellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur ich mu\u00df nicht geniessen", "tokens": ["Nur", "ich", "mu\u00df", "nicht", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Worauff die\u00df Leben geht,", "tokens": ["Wo\u00b7rauff", "die\u00df", "Le\u00b7ben", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Gl\u00fcck will mir verschlissen", "tokens": ["Das", "Gl\u00fcck", "will", "mir", "ver\u00b7schlis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was andern offen steht;", "tokens": ["Was", "an\u00b7dern", "of\u00b7fen", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Fr\u00fcling meiner Zier", "tokens": ["Der", "Fr\u00fc\u00b7ling", "mei\u00b7ner", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist ferne schon von hier,", "tokens": ["Ist", "fer\u00b7ne", "schon", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gleich wie die B\u00e4che fliessen", "tokens": ["Gleich", "wie", "die", "B\u00e4\u00b7che", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So eilt mein Herbst zu mir.", "tokens": ["So", "eilt", "mein", "Herbst", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich aber mu\u00df noch bleiben", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "noch", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wie ich vormahls war,", "tokens": ["So", "wie", "ich", "vor\u00b7mahls", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll nimmer mich beweiben,", "tokens": ["Soll", "nim\u00b7mer", "mich", "be\u00b7wei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit keiner seyn ein paar;", "tokens": ["Mit", "kei\u00b7ner", "seyn", "ein", "paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "ART", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das s\u00fcsse Wangen-roht", "tokens": ["Das", "s\u00fcs\u00b7se", "Wan\u00b7gen\u00b7roht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll nimmer mir die Noht", "tokens": ["Soll", "nim\u00b7mer", "mir", "die", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Einsamkeit vertreiben,", "tokens": ["Der", "Ein\u00b7sam\u00b7keit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Solch Leben ist ein Tod.", "tokens": ["Solch", "Le\u00b7ben", "ist", "ein", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du K\u00f6niginn Dione,", "tokens": ["Du", "K\u00f6\u00b7ni\u00b7ginn", "Dio\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von der es einig r\u00fchrt,", "tokens": ["Von", "der", "es", "ei\u00b7nig", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df meiner Zeiten Crone", "tokens": ["Da\u00df", "mei\u00b7ner", "Zei\u00b7ten", "Cro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir keine Lust geb\u00fchrt;", "tokens": ["Mir", "kei\u00b7ne", "Lust", "ge\u00b7b\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist die\u00df der Lieder Danck,", "tokens": ["Ist", "die\u00df", "der", "Lie\u00b7der", "Danck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Ich mein lebenlang", "tokens": ["Die", "Ich", "mein", "le\u00b7ben\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von dir vnd deinem Sohne", "tokens": ["Von", "dir", "vnd", "dei\u00b7nem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meine Geige sang?", "tokens": ["In", "mei\u00b7ne", "Gei\u00b7ge", "sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es hat mich nie gefangen", "tokens": ["Es", "hat", "mich", "nie", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir verbothen ist,", "tokens": ["Was", "mir", "ver\u00b7bo\u00b7then", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin nie dem nachgegangen", "tokens": ["Bin", "nie", "dem", "nach\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Leib vnd Seele b\u00fcst;", "tokens": ["Was", "Leib", "vnd", "See\u00b7le", "b\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will keiner wilden Brunst,", "tokens": ["Will", "kei\u00b7ner", "wil\u00b7den", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur eines Menschen Gunst", "tokens": ["Nur", "ei\u00b7nes", "Men\u00b7schen", "Gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In Ehren zu erlangen", "tokens": ["In", "Eh\u00b7ren", "zu", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Versuch ich alle Kunst.", "tokens": ["Ver\u00b7such", "ich", "al\u00b7le", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Soll ich mir dann erst rathen,", "tokens": ["Soll", "ich", "mir", "dann", "erst", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn schon mein Winter schneyt,", "tokens": ["Wenn", "schon", "mein", "Win\u00b7ter", "schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was thue ich dann vor Thaten", "tokens": ["Was", "thue", "ich", "dann", "vor", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im s\u00fcssen Liebes-streit?", "tokens": ["Im", "s\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7streit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer jung ist, liebt den Krieg,", "tokens": ["Wer", "jung", "ist", ",", "liebt", "den", "Krieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein alter bleibt zu r\u00fcck,", "tokens": ["Ein", "al\u00b7ter", "bleibt", "zu", "r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn solcher arth Soldaten", "tokens": ["Denn", "sol\u00b7cher", "ar\u00b7th", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Erhalten schlechten Sieg.", "tokens": ["Er\u00b7hal\u00b7ten", "schlech\u00b7ten", "Sieg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nein jetzund wil ich haben", "tokens": ["Nein", "je\u00b7tzund", "wil", "ich", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was auff mein Leiden dient,", "tokens": ["Was", "auff", "mein", "Lei\u00b7den", "dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil noch die F\u00fcsse traben", "tokens": ["Weil", "noch", "die", "F\u00fcs\u00b7se", "tra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd noch mein Alter gr\u00fcnt;", "tokens": ["Vnd", "noch", "mein", "Al\u00b7ter", "gr\u00fcnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm Venus, schleu\u00df mich ein", "tokens": ["Komm", "Ve\u00b7nus", ",", "schleu\u00df", "mich", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "PPER", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Liebsten, die ich mein'!", "tokens": ["Der", "Liebs\u00b7ten", ",", "die", "ich", "mein'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will von deinen Gaben", "tokens": ["Ich", "will", "von", "dei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Recht satt vnd truncken seyn.", "tokens": ["Recht", "satt", "vnd", "trun\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Soll denn mein Junges Leben,", "tokens": ["Soll", "denn", "mein", "Jun\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da alles liebt vnd freyht,", "tokens": ["Da", "al\u00b7les", "liebt", "vnd", "freyht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alleine sich ergeben", "tokens": ["Al\u00b7lei\u00b7ne", "sich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der langen Einsamkeit?", "tokens": ["Der", "lan\u00b7gen", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bleibt dann die Frewd vnd Lust", "tokens": ["Bleibt", "dann", "die", "Frewd", "vnd", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der schleyer-weissen Brust,", "tokens": ["Der", "schleyer\u00b7weis\u00b7sen", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nach der wir alle streben,", "tokens": ["Nach", "der", "wir", "al\u00b7le", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir Ewig vnbewust?", "tokens": ["Mir", "E\u00b7wig", "vn\u00b7be\u00b7wust", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die W\u00fcrme, die nur schleichen,", "tokens": ["Die", "W\u00fcr\u00b7me", ",", "die", "nur", "schlei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schnellen Fisch im Meer,", "tokens": ["Die", "schnel\u00b7len", "Fisch", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wild in den Gestr\u00e4uchen,", "tokens": ["Das", "Wild", "in", "den", "Ge\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00f6gel leichtes Heer,", "tokens": ["Der", "V\u00f6\u00b7gel", "leich\u00b7tes", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd was sich in der Welt", "tokens": ["Vnd", "was", "sich", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch Lufft vnd Flut erhelt,", "tokens": ["Durch", "Lufft", "vnd", "Flut", "er\u00b7helt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kriegt jedes seines gleichen,", "tokens": ["Kriegt", "je\u00b7des", "sei\u00b7nes", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sobald es jhm gefellt.", "tokens": ["So\u00b7bald", "es", "jhm", "ge\u00b7fellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nur ich mu\u00df nicht geniessen", "tokens": ["Nur", "ich", "mu\u00df", "nicht", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Worauff die\u00df Leben geht,", "tokens": ["Wo\u00b7rauff", "die\u00df", "Le\u00b7ben", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Gl\u00fcck will mir verschlissen", "tokens": ["Das", "Gl\u00fcck", "will", "mir", "ver\u00b7schlis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was andern offen steht;", "tokens": ["Was", "an\u00b7dern", "of\u00b7fen", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Fr\u00fcling meiner Zier", "tokens": ["Der", "Fr\u00fc\u00b7ling", "mei\u00b7ner", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist ferne schon von hier,", "tokens": ["Ist", "fer\u00b7ne", "schon", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gleich wie die B\u00e4che fliessen", "tokens": ["Gleich", "wie", "die", "B\u00e4\u00b7che", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So eilt mein Herbst zu mir.", "tokens": ["So", "eilt", "mein", "Herbst", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich aber mu\u00df noch bleiben", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "noch", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wie ich vormahls war,", "tokens": ["So", "wie", "ich", "vor\u00b7mahls", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll nimmer mich beweiben,", "tokens": ["Soll", "nim\u00b7mer", "mich", "be\u00b7wei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit keiner seyn ein paar;", "tokens": ["Mit", "kei\u00b7ner", "seyn", "ein", "paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "ART", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das s\u00fcsse Wangen-roht", "tokens": ["Das", "s\u00fcs\u00b7se", "Wan\u00b7gen\u00b7roht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll nimmer mir die Noht", "tokens": ["Soll", "nim\u00b7mer", "mir", "die", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Einsamkeit vertreiben,", "tokens": ["Der", "Ein\u00b7sam\u00b7keit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Solch Leben ist ein Tod.", "tokens": ["Solch", "Le\u00b7ben", "ist", "ein", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Du K\u00f6niginn Dione,", "tokens": ["Du", "K\u00f6\u00b7ni\u00b7ginn", "Dio\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von der es einig r\u00fchrt,", "tokens": ["Von", "der", "es", "ei\u00b7nig", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df meiner Zeiten Crone", "tokens": ["Da\u00df", "mei\u00b7ner", "Zei\u00b7ten", "Cro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir keine Lust geb\u00fchrt;", "tokens": ["Mir", "kei\u00b7ne", "Lust", "ge\u00b7b\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist die\u00df der Lieder Danck,", "tokens": ["Ist", "die\u00df", "der", "Lie\u00b7der", "Danck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Ich mein lebenlang", "tokens": ["Die", "Ich", "mein", "le\u00b7ben\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von dir vnd deinem Sohne", "tokens": ["Von", "dir", "vnd", "dei\u00b7nem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meine Geige sang?", "tokens": ["In", "mei\u00b7ne", "Gei\u00b7ge", "sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Es hat mich nie gefangen", "tokens": ["Es", "hat", "mich", "nie", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mir verbothen ist,", "tokens": ["Was", "mir", "ver\u00b7bo\u00b7then", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin nie dem nachgegangen", "tokens": ["Bin", "nie", "dem", "nach\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Leib vnd Seele b\u00fcst;", "tokens": ["Was", "Leib", "vnd", "See\u00b7le", "b\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will keiner wilden Brunst,", "tokens": ["Will", "kei\u00b7ner", "wil\u00b7den", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur eines Menschen Gunst", "tokens": ["Nur", "ei\u00b7nes", "Men\u00b7schen", "Gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In Ehren zu erlangen", "tokens": ["In", "Eh\u00b7ren", "zu", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Versuch ich alle Kunst.", "tokens": ["Ver\u00b7such", "ich", "al\u00b7le", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Soll ich mir dann erst rathen,", "tokens": ["Soll", "ich", "mir", "dann", "erst", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn schon mein Winter schneyt,", "tokens": ["Wenn", "schon", "mein", "Win\u00b7ter", "schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was thue ich dann vor Thaten", "tokens": ["Was", "thue", "ich", "dann", "vor", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im s\u00fcssen Liebes-streit?", "tokens": ["Im", "s\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7streit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer jung ist, liebt den Krieg,", "tokens": ["Wer", "jung", "ist", ",", "liebt", "den", "Krieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein alter bleibt zu r\u00fcck,", "tokens": ["Ein", "al\u00b7ter", "bleibt", "zu", "r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn solcher arth Soldaten", "tokens": ["Denn", "sol\u00b7cher", "ar\u00b7th", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Erhalten schlechten Sieg.", "tokens": ["Er\u00b7hal\u00b7ten", "schlech\u00b7ten", "Sieg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Nein jetzund wil ich haben", "tokens": ["Nein", "je\u00b7tzund", "wil", "ich", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VMFIN", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was auff mein Leiden dient,", "tokens": ["Was", "auff", "mein", "Lei\u00b7den", "dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil noch die F\u00fcsse traben", "tokens": ["Weil", "noch", "die", "F\u00fcs\u00b7se", "tra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd noch mein Alter gr\u00fcnt;", "tokens": ["Vnd", "noch", "mein", "Al\u00b7ter", "gr\u00fcnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm Venus, schleu\u00df mich ein", "tokens": ["Komm", "Ve\u00b7nus", ",", "schleu\u00df", "mich", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "PPER", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Liebsten, die ich mein'!", "tokens": ["Der", "Liebs\u00b7ten", ",", "die", "ich", "mein'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will von deinen Gaben", "tokens": ["Ich", "will", "von", "dei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Recht satt vnd truncken seyn.", "tokens": ["Recht", "satt", "vnd", "trun\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}