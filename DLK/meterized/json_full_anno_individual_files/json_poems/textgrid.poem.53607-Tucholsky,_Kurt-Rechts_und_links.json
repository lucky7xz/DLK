{"textgrid.poem.53607": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Rechts und links", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rechts sind B\u00e4ume, links sind B\u00e4ume,", "tokens": ["Rechts", "sind", "B\u00e4u\u00b7me", ",", "links", "sind", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und dazwischen Zwischenr\u00e4ume.", "tokens": ["und", "da\u00b7zwi\u00b7schen", "Zwi\u00b7schen\u00b7r\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Mitte flie\u00dft ein Bach!", "tokens": ["In", "der", "Mit\u00b7te", "flie\u00dft", "ein", "Bach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach!", "tokens": ["Ach", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Rechts hat man die Industriellen,", "tokens": ["Rechts", "hat", "man", "die", "In\u00b7dust\u00b7ri\u00b7el\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "welche eine Presse wellen,", "tokens": ["wel\u00b7che", "ei\u00b7ne", "Pres\u00b7se", "wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "eine, die den Abonnenten", "tokens": ["ei\u00b7ne", ",", "die", "den", "A\u00b7bon\u00b7nen\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "ART", "NN"], "meter": "+-+-----", "measure": "unknown.measure.di"}, "line.4": {"text": "nationale fette Enten", "tokens": ["na\u00b7ti\u00b7o\u00b7na\u00b7le", "fet\u00b7te", "En\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "t\u00e4glich aufzubinden hat.", "tokens": ["t\u00e4g\u00b7lich", "auf\u00b7zu\u00b7bin\u00b7den", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und so f\u00e4llt denn Blatt auf Blatt", "tokens": ["Und", "so", "f\u00e4llt", "denn", "Blatt", "auf", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "in die H\u00e4nde von Kartellen", "tokens": ["in", "die", "H\u00e4n\u00b7de", "von", "Kar\u00b7tel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "unsrer Gro\u00dfindustriellen.", "tokens": ["uns\u00b7rer", "Gro\u00df\u00b7in\u00b7dust\u00b7ri\u00b7el\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und man schiebt sich dies und jenes,", "tokens": ["Und", "man", "schiebt", "sich", "dies", "und", "je\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "PDS", "KON", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "weils bequem is und gemeen is.", "tokens": ["weils", "be\u00b7quem", "is", "und", "ge\u00b7meen", "is", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "FM", "KON", "VVPP", "FM", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Und die Aktie kommandiert \u2013", "tokens": ["Und", "die", "Ak\u00b7tie", "kom\u00b7man\u00b7diert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "die Verwaltung salutiert.", "tokens": ["die", "Ver\u00b7wal\u00b7tung", "sa\u00b7lu\u00b7tiert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Helfferich ruft Weh und Ach . . .", "tokens": ["Helf\u00b7fe\u00b7rich", "ruft", "Weh", "und", "Ach", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Mitte flie\u00dft ein Bach.", "tokens": ["In", "der", "Mit\u00b7te", "flie\u00dft", "ein", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Links hat man die neuen Helden,", "tokens": ["Links", "hat", "man", "die", "neu\u00b7en", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die sich schon seit 18 melden,", "tokens": ["die", "sich", "schon", "seit", "18", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "number", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "APPR", "CARD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wenns was zu vermitteln gibt.", "tokens": ["wenns", "was", "zu", "ver\u00b7mit\u00b7teln", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "(dies Gesch\u00e4ft ist so beliebt.)", "tokens": ["(", "dies", "Ge\u00b7sch\u00e4ft", "ist", "so", "be\u00b7liebt", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "NN", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Barmat, Parvus, Sklarz Gebr\u00fcder \u2013", "tokens": ["Bar\u00b7mat", ",", "Par\u00b7vus", ",", "Sklarz", "Ge\u00b7br\u00fc\u00b7der", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NE", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ei, man ist so brav und b\u00fcder.", "tokens": ["Ei", ",", "man", "ist", "so", "brav", "und", "b\u00fc\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Regierung ist schockiert", "tokens": ["Die", "Re\u00b7gie\u00b7rung", "ist", "scho\u00b7ckiert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und wird m\u00e4chtig angeschmiert.", "tokens": ["und", "wird", "m\u00e4ch\u00b7tig", "an\u00b7ge\u00b7schmiert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Manches Silber ist vernickelt,", "tokens": ["Man\u00b7ches", "Sil\u00b7ber", "ist", "ver\u00b7ni\u00b7ckelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "mancher Handel ist verwickelt.", "tokens": ["man\u00b7cher", "Han\u00b7del", "ist", "ver\u00b7wi\u00b7ckelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Reine Finger hab, wer kann!", "tokens": ["Rei\u00b7ne", "Fin\u00b7ger", "hab", ",", "wer", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,", "PWS", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Schlimmstenfalls zieh Handschuh an!", "tokens": ["Schlimms\u00b7ten\u00b7falls", "zieh", "Hand\u00b7schuh", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rechts sind Schieber, links sind Schieber.", "tokens": ["Rechts", "sind", "Schie\u00b7ber", ",", "links", "sind", "Schie\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Antlitz ein Kassiber.", "tokens": ["Je\u00b7des", "Ant\u00b7litz", "ein", "Kas\u00b7si\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der weiland gro\u00dfen Zeit", "tokens": ["In", "der", "wei\u00b7land", "gro\u00b7\u00dfen", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schob man Seins im grauen Kleid.", "tokens": ["schob", "man", "Seins", "im", "grau\u00b7en", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh die Rechten, sieh die Linken \u2013", "tokens": ["Sieh", "die", "Rech\u00b7ten", ",", "sieh", "die", "Lin\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und es will mich schier bed\u00fcnken,", "tokens": ["und", "es", "will", "mich", "schier", "be\u00b7d\u00fcn\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": " . . .", "tokens": [".", ".", "."], "token_info": ["punct", "punct", "punct"], "pos": ["$.", "$.", "$."]}, "line.8": {"text": "Rechts sind B\u00e4ume, links sind B\u00e4ume,", "tokens": ["Rechts", "sind", "B\u00e4u\u00b7me", ",", "links", "sind", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "und dazwischen Zwischenr\u00e4ume.", "tokens": ["und", "da\u00b7zwi\u00b7schen", "Zwi\u00b7schen\u00b7r\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "In der Mitte flie\u00dft ein Bach \u2013"