{"textgrid.poem.33948": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgen und Abend", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Morgenzeit, du frische Zeit!", "tokens": ["O", "Mor\u00b7gen\u00b7zeit", ",", "du", "fri\u00b7sche", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens reichste Quelle!", "tokens": ["Des", "Le\u00b7bens", "reichs\u00b7te", "Quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du machst die enge Brust mir weit,", "tokens": ["Du", "machst", "die", "en\u00b7ge", "Brust", "mir", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das tr\u00fcbe Aug' mir helle!", "tokens": ["Das", "tr\u00fc\u00b7be", "Aug'", "mir", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir ist, als d\u00fcrft' ich aufersteh'n", "tokens": ["Mir", "ist", ",", "als", "d\u00fcrft'", "ich", "auf\u00b7er\u00b7steh'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus einem dumpfen Grabe,", "tokens": ["Aus", "ei\u00b7nem", "dum\u00b7pfen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn ich das erste Licht geseh'n,", "tokens": ["Wenn", "ich", "das", "ers\u00b7te", "Licht", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Hauch getrunken habe.", "tokens": ["Den", "Hauch", "ge\u00b7trun\u00b7ken", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Teich Bethesda gleicht mein Herz", "tokens": ["Dem", "Teich", "Be\u00b7thes\u00b7da", "gleicht", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinen frischen S\u00e4ften,", "tokens": ["Mit", "sei\u00b7nen", "fri\u00b7schen", "S\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die schwellen es zu Lust und Schmerz", "tokens": ["Die", "schwel\u00b7len", "es", "zu", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit tausend neuen Kr\u00e4ften:", "tokens": ["Mit", "tau\u00b7send", "neu\u00b7en", "Kr\u00e4f\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr trunk'nes Durcheinanderspiel", "tokens": ["Ihr", "trunk'\u00b7nes", "Durch\u00b7ein\u00b7an\u00b7der\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erf\u00fcllt mich mit Entz\u00fccken;", "tokens": ["Er\u00b7f\u00fcllt", "mich", "mit", "Ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich wei\u00df nicht was, doch will ich viel,", "tokens": ["Ich", "wei\u00df", "nicht", "was", ",", "doch", "will", "ich", "viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PIS", "$,", "ADV", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Alles mu\u00df mir gl\u00fccken!", "tokens": ["Und", "Al\u00b7les", "mu\u00df", "mir", "gl\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allein, unendlich ist die Welt,", "tokens": ["Al\u00b7lein", ",", "un\u00b7end\u00b7lich", "ist", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, wie die Brust sich dehne,", "tokens": ["Und", ",", "wie", "die", "Brust", "sich", "deh\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "PRF", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fchlt's zuletzt, und brennend f\u00e4llt", "tokens": ["Sie", "f\u00fchlt's", "zu\u00b7letzt", ",", "und", "bren\u00b7nend", "f\u00e4llt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die reinste Menschenthr\u00e4ne.", "tokens": ["Die", "reins\u00b7te", "Men\u00b7schen\u00b7thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann sinkt des Abends heil'ge Ruh',", "tokens": ["Dann", "sinkt", "des", "A\u00b7bends", "heil'\u00b7ge", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00e4r's auf eine Wunde,", "tokens": ["Als", "w\u00e4r's", "auf", "ei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf sie herab, und schlie\u00dft sie zu,", "tokens": ["Auf", "sie", "her\u00b7ab", ",", "und", "schlie\u00dft", "sie", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Damit sie still gesunde.", "tokens": ["Da\u00b7mit", "sie", "still", "ge\u00b7sun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Menschen Kraft reicht eben aus", "tokens": ["Des", "Men\u00b7schen", "Kraft", "reicht", "e\u00b7ben", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum K\u00e4mpfen, nicht zum Siegen,", "tokens": ["Zum", "K\u00e4mp\u00b7fen", ",", "nicht", "zum", "Sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sollen in dem ew'gen Strau\u00df", "tokens": ["Wir", "sol\u00b7len", "in", "dem", "ew'\u00b7gen", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht steh'n und nicht erliegen;", "tokens": ["Nicht", "steh'n", "und", "nicht", "er\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch, wenn uns die\u00df das Herz beschwert,", "tokens": ["Doch", ",", "wenn", "uns", "die\u00df", "das", "Herz", "be\u00b7schwert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PDS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Naht der ersehnte Schlummer,", "tokens": ["Naht", "der", "er\u00b7sehn\u00b7te", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, ward der letzte Wunsch gew\u00e4hrt:", "tokens": ["Und", ",", "ward", "der", "letz\u00b7te", "Wunsch", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wem macht der erste Kummer?", "tokens": ["Wem", "macht", "der", "ers\u00b7te", "Kum\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Morgenzeit, du frische Zeit!", "tokens": ["O", "Mor\u00b7gen\u00b7zeit", ",", "du", "fri\u00b7sche", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens reichste Quelle!", "tokens": ["Des", "Le\u00b7bens", "reichs\u00b7te", "Quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du machst die enge Brust mir weit,", "tokens": ["Du", "machst", "die", "en\u00b7ge", "Brust", "mir", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das tr\u00fcbe Aug' mir helle!", "tokens": ["Das", "tr\u00fc\u00b7be", "Aug'", "mir", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir ist, als d\u00fcrft' ich aufersteh'n", "tokens": ["Mir", "ist", ",", "als", "d\u00fcrft'", "ich", "auf\u00b7er\u00b7steh'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus einem dumpfen Grabe,", "tokens": ["Aus", "ei\u00b7nem", "dum\u00b7pfen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.t