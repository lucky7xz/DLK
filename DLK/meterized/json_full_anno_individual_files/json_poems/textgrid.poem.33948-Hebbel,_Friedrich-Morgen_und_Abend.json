en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sollen in dem ew'gen Strau\u00df", "tokens": ["Wir", "sol\u00b7len", "in", "dem", "ew'\u00b7gen", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht steh'n und nicht erliegen;", "tokens": ["Nicht", "steh'n", "und", "nicht", "er\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch, wenn uns die\u00df das Herz beschwert,", "tokens": ["Doch", ",", "wenn", "uns", "die\u00df", "das", "Herz", "be\u00b7schwert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PDS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Naht der ersehnte Schlummer,", "tokens": ["Naht", "der", "er\u00b7sehn\u00b7te", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, ward der letzte Wunsch gew\u00e4hrt:", "tokens": ["Und", ",", "ward", "der", "letz\u00b7te", "Wunsch", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wem macht der erste Kummer?", "tokens": ["Wem", "macht", "der", "ers\u00b7te", "Kum\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}