{"dta.poem.21623": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Das All.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Im ewgen All ist Harmonie:               ", "tokens": ["Im", "ew\u00b7gen", "All", "ist", "Har\u00b7mo\u00b7nie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Morgenroth, das Abendroth.", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", ",", "das", "A\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da eint, als herrlicher Accord,", "tokens": ["Da", "eint", ",", "als", "herr\u00b7li\u00b7cher", "Ac\u00b7cord", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben sich dem schrillen Tod.", "tokens": ["Das", "Le\u00b7ben", "sich", "dem", "schril\u00b7len", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur in der fl\u00fccht\u2019gen Menschenbrust", "tokens": ["Nur", "in", "der", "fl\u00fccht'\u00b7gen", "Men\u00b7schen\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trennt sich die Wonne von der Pein.", "tokens": ["Trennt", "sich", "die", "Won\u00b7ne", "von", "der", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und all sein zuckend F\u00fchlen denkt", "tokens": ["Und", "all", "sein", "zu\u00b7ckend", "F\u00fch\u00b7len", "denkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "VVPP", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er in das todte All hinein.", "tokens": ["Er", "in", "das", "tod\u00b7te", "All", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}