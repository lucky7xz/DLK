{"dta.poem.20012": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die heilige Regiswind von Laufen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Ritter Ernst, der war ergrimmt zu einer b\u00f6sen Stund',", "tokens": ["Herr", "Rit\u00b7ter", "Ernst", ",", "der", "war", "er\u00b7grimmt", "zu", "ei\u00b7ner", "b\u00f6\u00b7sen", "Stund'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,", "PRELS", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Er schlug die falsche Dienerin mit seinen F\u00e4usten wund.", "tokens": ["Er", "schlug", "die", "fal\u00b7sche", "Die\u00b7ne\u00b7rin", "mit", "sei\u00b7nen", "F\u00e4us\u00b7ten", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Er schlug die falsche Dienerin, er stie\u00df sie mit dem Fu\u00df:", "tokens": ["Er", "schlug", "die", "fal\u00b7sche", "Die\u00b7ne\u00b7rin", ",", "er", "stie\u00df", "sie", "mit", "dem", "Fu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "\u201eherr Ritter Ernst! und wi\u00dft f\u00fcrwahr, da\u00df Euch die\u00df reuen", "tokens": ["\u201e", "herr", "Rit\u00b7ter", "Ernst", "!", "und", "wi\u00dft", "f\u00fcr\u00b7wahr", ",", "da\u00df", "Euch", "die\u00df", "reu\u00b7en"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NE", "NE", "NE", "$.", "KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "mu\u00df.\u201c", "tokens": ["mu\u00df", ".", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["VMFIN", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Es war die falsche Dienerin, die eilte durch den Saal,", "tokens": ["Es", "war", "die", "fal\u00b7sche", "Die\u00b7ne\u00b7rin", ",", "die", "eil\u00b7te", "durch", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Sie eilte durch den weiten Hof, hinab ins gr\u00fcne Thal.", "tokens": ["Sie", "eil\u00b7te", "durch", "den", "wei\u00b7ten", "Hof", ",", "hin\u00b7ab", "ins", "gr\u00fc\u00b7ne", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Da sa\u00df Herrn Ernsts sein T\u00f6chterlein, ein Fr\u00e4ulein fromm", "tokens": ["Da", "sa\u00df", "Herrn", "Ernsts", "sein", "T\u00f6ch\u00b7ter\u00b7lein", ",", "ein", "Fr\u00e4u\u00b7lein", "fromm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NE", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und zart.", "tokens": ["und", "zart", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Es spielt mit bunten Bl\u00fcmelein nach anderer Kinder Art.", "tokens": ["Es", "spielt", "mit", "bun\u00b7ten", "Bl\u00fc\u00b7me\u00b7lein", "nach", "an\u00b7de\u00b7rer", "Kin\u00b7der", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Da pfl\u00fcckt die falsche Dienerin drei R\u00f6slein auf dem Plan,", "tokens": ["Da", "pfl\u00fcckt", "die", "fal\u00b7sche", "Die\u00b7ne\u00b7rin", "drei", "R\u00f6s\u00b7lein", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Zu locken dieses stille Kind zum wilden Strom hinan.", "tokens": ["Zu", "lo\u00b7cken", "die\u00b7ses", "stil\u00b7le", "Kind", "zum", "wil\u00b7den", "Strom", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "\u201ekomm liebes Kind! komm, s\u00fc\u00dfes Kind! da bl\u00fchen R\u00f6s-", "tokens": ["\u201e", "komm", "lie\u00b7bes", "Kind", "!", "komm", ",", "s\u00fc\u00b7\u00dfes", "Kind", "!", "da", "bl\u00fc\u00b7hen", "R\u00f6s"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADJA", "NN", "$.", "VVFIN", "$,", "ADJA", "NN", "$.", "ADV", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "lein rund!\u201c", "tokens": ["lein", "rund", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "ADJD", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Sie fa\u00dft es an dem goldnen Haar, sie schleudert's in den", "tokens": ["Sie", "fa\u00dft", "es", "an", "dem", "gold\u00b7nen", "Haar", ",", "sie", "schleu\u00b7dert's", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Grund.", "tokens": ["Grund", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Eine Weil' das Kind die Tiefe barg, eine Weil' es oben", "tokens": ["Ei\u00b7ne", "Weil'", "das", "Kind", "die", "Tie\u00b7fe", "barg", ",", "ei\u00b7ne", "Weil'", "es", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "ADV"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "schwamm,", "tokens": ["schwamm", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Auflacht die falsche Dienerin, doch bald ihr Reue kam.", "tokens": ["Auf\u00b7lacht", "die", "fal\u00b7sche", "Die\u00b7ne\u00b7rin", ",", "doch", "bald", "ihr", "Reu\u00b7e", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Sie flieht von dem unsel'gen Strom, flieht \u00fcber Berg und", "tokens": ["Sie", "flieht", "von", "dem", "un\u00b7sel'\u00b7gen", "Strom", ",", "flieht", "\u00fc\u00b7ber", "Berg", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "KON"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Thal,", "tokens": ["Thal", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Sie irrt so viele hundert Jahr, kann ruh'n kein einzigmal.", "tokens": ["Sie", "irrt", "so", "vie\u00b7le", "hun\u00b7dert", "Jahr", ",", "kann", "ruh'n", "kein", "ein\u00b7zig\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "CARD", "NN", "$,", "VMFIN", "VVFIN", "PIAT", "PIS", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "Es sah Herr Ernst von hoher Burg, sah in den gr\u00fcnen", "tokens": ["Es", "sah", "Herr", "Ernst", "von", "ho\u00b7her", "Burg", ",", "sah", "in", "den", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NE", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Grund,", "tokens": ["Grund", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Sie brachten todt sein s\u00fc\u00dfes Kind, auf Rosen man es fund.", "tokens": ["Sie", "brach\u00b7ten", "todt", "sein", "s\u00fc\u00b7\u00dfes", "Kind", ",", "auf", "Ro\u00b7sen", "man", "es", "fund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "Es bl\u00fcht wie eine Rose roth, wie eine Lilie wei\u00df.", "tokens": ["Es", "bl\u00fcht", "wie", "ei\u00b7ne", "Ro\u00b7se", "roth", ",", "wie", "ei\u00b7ne", "Li\u00b7lie", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "Er legt's in einen gold'nen Sarg, bestattet es mit Flei\u00df.", "tokens": ["Er", "legt's", "in", "ei\u00b7nen", "gold'\u00b7nen", "Sarg", ",", "be\u00b7stat\u00b7tet", "es", "mit", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "Manch' Mutter kniet' mit ihrem Kind auf Regiswindens", "tokens": ["Man\u00b7ch'", "Mut\u00b7ter", "kniet'", "mit", "ih\u00b7rem", "Kind", "auf", "Re\u00b7gis\u00b7win\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "Gruft,", "tokens": ["Gruft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Doch wenn Herr Ernst, der Vater, kam, entstieg ihr Ro-", "tokens": ["Doch", "wenn", "Herr", "Ernst", ",", "der", "Va\u00b7ter", ",", "kam", ",", "ent\u00b7stieg", "ihr", "Ro"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "NE", "$,", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "senduft.", "tokens": ["sen\u00b7duft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.32": {"text": "Seitdem erscheint zur Todesnacht gar manchem frommen", "tokens": ["Seit\u00b7dem", "er\u00b7scheint", "zur", "To\u00b7des\u00b7nacht", "gar", "man\u00b7chem", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "ADV", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kind,", "tokens": ["Kind", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Bekr\u00e4nzt mit duft'gen R\u00f6slein roth, die heil'ge Regiswind.", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "duft'\u00b7gen", "R\u00f6s\u00b7lein", "roth", ",", "die", "heil'\u00b7ge", "Re\u00b7gis\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.35": {"text": "Auch liegt seitdem manch' frommes Kind, das Nachts erlitt", "tokens": ["Auch", "liegt", "seit\u00b7dem", "man\u00b7ch'", "from\u00b7mes", "Kind", ",", "das", "Nachts", "er\u00b7litt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "den Tod,", "tokens": ["den", "Tod", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.37": {"text": "Am Morgen in der Wieg' umkr\u00e4nzt mit jungen R\u00f6slein", "tokens": ["Am", "Mor\u00b7gen", "in", "der", "Wieg'", "um\u00b7kr\u00e4nzt", "mit", "jun\u00b7gen", "R\u00f6s\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.38": {"text": "roth.", "tokens": ["roth", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}}}}}