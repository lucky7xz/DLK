{"textgrid.poem.57044": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die weggeworfene Flinte", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palmstr\u00f6m findet eines Abends,", "tokens": ["Palm\u00b7str\u00f6m", "fin\u00b7det", "ei\u00b7nes", "A\u00b7bends", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "als er zwischen hohem Korn", "tokens": ["als", "er", "zwi\u00b7schen", "ho\u00b7hem", "Korn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "singend schweift,", "tokens": ["sin\u00b7gend", "schweift", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "eine Flinte.", "tokens": ["ei\u00b7ne", "Flin\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Trauernd bricht er seinen Hymnus", "tokens": ["Trau\u00b7ernd", "bricht", "er", "sei\u00b7nen", "Hym\u00b7nus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "ab und setzt sich in den Mohn,", "tokens": ["ab", "und", "setzt", "sich", "in", "den", "Mohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "seinen Fund", "tokens": ["sei\u00b7nen", "Fund"], "token_info": ["word", "word"], "po