{"dta.poem.20729": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Phillis, die Helferin.                  \n  Eine Idylle an Damon .                  \n                                    1763.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hellaugigte, dem Fr\u00fchling \u00e4hnliche Tage w\u00fcnsch ich                 ", "tokens": ["Hel\u00b7lau\u00b7gig\u00b7te", ",", "dem", "Fr\u00fch\u00b7ling", "\u00e4hn\u00b7li\u00b7che", "Ta\u00b7ge", "w\u00fcnsch", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "O Damon! dir, und jeglichem zartf\u00fchlenden Sch\u00e4fer,", "tokens": ["O", "Da\u00b7mon", "!", "dir", ",", "und", "jeg\u00b7li\u00b7chem", "zart\u00b7f\u00fch\u00b7len\u00b7den", "Sch\u00e4\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "$,", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--++--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem sein Schicksal verlieh eine H\u00fctte von d\u00fcnnen", "tokens": ["Dem", "sein", "Schick\u00b7sal", "ver\u00b7lieh", "ei\u00b7ne", "H\u00fct\u00b7te", "von", "d\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gewebten Widerstand gegen die herbstlichen Sturm-", "tokens": ["Ge\u00b7web\u00b7ten", "Wi\u00b7der\u00b7stand", "ge\u00b7gen", "die", "herbst\u00b7li\u00b7chen", "Stur\u00b7m"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "winde,", "tokens": ["win\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Welche gefiedert mit Schneeflocken, oder mit peit-", "tokens": ["Wel\u00b7che", "ge\u00b7fie\u00b7dert", "mit", "Schnee\u00b7flo\u00b7cken", ",", "o\u00b7der", "mit", "peit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "VVPP", "APPR", "NN", "$,", "KON", "APPR", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "schenden", "tokens": ["schen\u00b7den"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Wolkeng\u00fcssen daher kommen, und in mittern\u00e4chtlicher", "tokens": ["Wol\u00b7ken\u00b7g\u00fcs\u00b7sen", "da\u00b7her", "kom\u00b7men", ",", "und", "in", "mit\u00b7ter\u00b7n\u00e4cht\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PAV", "VVINF", "$,", "KON", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.9": {"text": "Stunde", "tokens": ["Stun\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Feindseelig von deinem Auge verscheuchen den Kraft", "tokens": ["Feind\u00b7see\u00b7lig", "von", "dei\u00b7nem", "Au\u00b7ge", "ver\u00b7scheu\u00b7chen", "den", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "einfl\u00f6\u00dfenden", "tokens": ["ein\u00b7fl\u00f6\u00b7\u00dfen\u00b7den"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+--", "measure": "dactylic.init"}, "line.12": {"text": "Schlummer. Du l\u00e4chelst meinen Bedaurungen, denn", "tokens": ["Schlum\u00b7mer", ".", "Du", "l\u00e4\u00b7chelst", "mei\u00b7nen", "Be\u00b7dau\u00b7run\u00b7gen", ",", "denn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Deine zufriedne ruhige Seele sch\u00e4tzt dich gl\u00fcckseelig", "tokens": ["Dei\u00b7ne", "zu\u00b7fried\u00b7ne", "ru\u00b7hi\u00b7ge", "See\u00b7le", "sch\u00e4tzt", "dich", "gl\u00fcck\u00b7see\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Gegen einer unz\u00e4hlbaren Menge von Menschen, de-", "tokens": ["Ge\u00b7gen", "ei\u00b7ner", "un\u00b7z\u00e4hl\u00b7ba\u00b7ren", "Men\u00b7ge", "von", "Men\u00b7schen", ",", "de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+--+--", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "nen nicht", "tokens": ["nen", "nicht"], "token_info": ["word", "word"], "pos": ["VVFIN", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Das gefr\u00e4\u00dfige Raubthier, der Krieg, gelassen hat", "tokens": ["Das", "ge\u00b7fr\u00e4\u00b7\u00dfi\u00b7ge", "Raubt\u00b7hier", ",", "der", "Krieg", ",", "ge\u00b7las\u00b7sen", "hat"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVPP", "VAFIN"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "das Fell", "tokens": ["das", "Fell"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Eines Lammes, um ihre Schultern zu sch\u00fctzen vor", "tokens": ["Ei\u00b7nes", "Lam\u00b7mes", ",", "um", "ih\u00b7re", "Schul\u00b7tern", "zu", "sch\u00fct\u00b7zen", "vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "APPR"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Dem Mauerdurchdringenden Nordwind, der", "tokens": ["Dem", "Mau\u00b7er\u00b7durch\u00b7drin\u00b7gen\u00b7den", "Nord\u00b7wind", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Die W\u00e4lder entbl\u00f6\u00dft, und abstreift sorgf\u00e4ltig gepfle-", "tokens": ["Die", "W\u00e4l\u00b7der", "ent\u00b7bl\u00f6\u00dft", ",", "und", "ab\u00b7streift", "sorg\u00b7f\u00e4l\u00b7tig", "ge\u00b7pfle"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "TRUNC"], "meter": "-+--+-+-++--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "geten B\u00e4umen", "tokens": ["ge\u00b7ten", "B\u00e4u\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.22": {"text": "Des ungez\u00e4uneten Gartens jegliches Blatt. Traurig", "tokens": ["Des", "un\u00b7ge\u00b7z\u00e4u\u00b7ne\u00b7ten", "Gar\u00b7tens", "jeg\u00b7li\u00b7ches", "Blatt", ".", "Trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Stehen sie da! gleich der nakkenden Armuth weinen", "tokens": ["Ste\u00b7hen", "sie", "da", "!", "gleich", "der", "nak\u00b7ken\u00b7den", "Ar\u00b7muth", "wei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+---+-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Sie von ihrer schmucklosen Stirn herunter die N\u00e4sse", "tokens": ["Sie", "von", "ih\u00b7rer", "schmuck\u00b7lo\u00b7sen", "Stirn", "her\u00b7un\u00b7ter", "die", "N\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "APZR", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Der dicken niederfallenden Luft, voll Schauer einhau-", "tokens": ["Der", "di\u00b7cken", "nie\u00b7der\u00b7fal\u00b7len\u00b7den", "Luft", ",", "voll", "Schau\u00b7er", "ein\u00b7hau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADJD", "NE", "TRUNC"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "chenden", "tokens": ["chen\u00b7den"], "token_info": ["word"], "pos": ["NE"], "meter": "--", "measure": "unknown.measure.zero"}, "line.27": {"text": "K\u00e4lte. Dieser Anblick, o Damon! erinnertdeine Phillis", "tokens": ["K\u00e4l\u00b7te", ".", "Die\u00b7ser", "An\u00b7blick", ",", "o", "Da\u00b7mon", "!", "e\u00b7rin\u00b7nert\u00b7dei\u00b7ne", "Phil\u00b7lis"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PDAT", "NN", "$,", "FM", "NE", "$.", "VVFIN", "NE"], "meter": "+-+-+--+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "An das Bild einer ungl\u00fccklichen Hirtin. H\u00f6re von mir", "tokens": ["An", "das", "Bild", "ei\u00b7ner", "un\u00b7gl\u00fcck\u00b7li\u00b7chen", "Hir\u00b7tin", ".", "H\u00f6\u00b7re", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$.", "NE", "APPR", "PPER"], "meter": "+-+--++--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Du leisef\u00fchlender Sch\u00e4fer! Diese das Herz angreifende", "tokens": ["Du", "lei\u00b7se\u00b7f\u00fch\u00b7len\u00b7der", "Sch\u00e4\u00b7fer", "!", "Die\u00b7se", "das", "Herz", "an\u00b7grei\u00b7fen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PDAT", "ART", "NN", "ADJA"], "meter": "-+-+--+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Geschichte, und liebe noch anbetender Deine Phillis.", "tokens": ["Ge\u00b7schich\u00b7te", ",", "und", "lie\u00b7be", "noch", "an\u00b7be\u00b7ten\u00b7der", "Dei\u00b7ne", "Phil\u00b7lis", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "ADV", "ADV", "PPOSAT", "NE", "$."], "meter": "-+--+--+---+--+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Sie bewohnte mit ihrer trauertragenden Mutter", "tokens": ["Sie", "be\u00b7wohn\u00b7te", "mit", "ih\u00b7rer", "trau\u00b7er\u00b7tra\u00b7gen\u00b7den", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eine nicht weitr\u00e4umichte H\u00fctte auf wehrloser Trift", "tokens": ["Ei\u00b7ne", "nicht", "weit\u00b7r\u00e4u\u00b7mich\u00b7te", "H\u00fct\u00b7te", "auf", "wehr\u00b7lo\u00b7ser", "Trift"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "In jenen schrecklichen Tagen, als R\u00e4uberheerden", "tokens": ["In", "je\u00b7nen", "schreck\u00b7li\u00b7chen", "Ta\u00b7gen", ",", "als", "R\u00e4u\u00b7ber\u00b7heer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KOUS", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aus fernen W\u00fcsteneyen gezogen kamen. Gleich den", "tokens": ["Aus", "fer\u00b7nen", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", "ge\u00b7zo\u00b7gen", "ka\u00b7men", ".", "Gleich", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN", "$.", "ADV", "ART"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Verderblichen Wetterwolken hagelten sie Verw\u00fcstung", "tokens": ["Ver\u00b7derb\u00b7li\u00b7chen", "Wet\u00b7ter\u00b7wol\u00b7ken", "ha\u00b7gel\u00b7ten", "sie", "Ver\u00b7w\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "NN"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Auf bl\u00fchende Fluren, nnd dichtverwachsene Weitzen-", "tokens": ["Auf", "bl\u00fc\u00b7hen\u00b7de", "Flu\u00b7ren", ",", "nnd", "dicht\u00b7ver\u00b7wach\u00b7se\u00b7ne", "Weit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJA", "TRUNC"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "furchen,", "tokens": ["fur\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und zerbissen mit sch\u00e4umendem Zahn den grasgr\u00fcnen", "tokens": ["Und", "zer\u00b7bis\u00b7sen", "mit", "sch\u00e4u\u00b7men\u00b7dem", "Zahn", "den", "gras\u00b7gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Apfel", "tokens": ["Ap\u00b7fel"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Und die unvollkommene, steinharte Birn", "tokens": ["Und", "die", "un\u00b7voll\u00b7kom\u00b7me\u00b7ne", ",", "stein\u00b7har\u00b7te", "Birn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Abgerissen von den widerstrebenden Aesten des un-", "tokens": ["Ab\u00b7ge\u00b7ris\u00b7sen", "von", "den", "wi\u00b7der\u00b7stre\u00b7ben\u00b7den", "A\u00b7es\u00b7ten", "des", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "+-+-+-+-+--+---+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "willigen", "tokens": ["wil\u00b7li\u00b7gen"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+--", "measure": "dactylic.init"}, "line.13": {"text": "Baumes. Br\u00fcllend st\u00fcrzten sie \u00fcber die Felder", "tokens": ["Bau\u00b7mes", ".", "Br\u00fcl\u00b7lend", "st\u00fcrz\u00b7ten", "sie", "\u00fc\u00b7ber", "die", "Fel\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Auf Pfeilschnellen Ro\u00df. Ihr Ansehn war das furchtbare", "tokens": ["Auf", "Pfeil\u00b7schnel\u00b7len", "Ro\u00df", ".", "Ihr", "An\u00b7sehn", "war", "das", "furcht\u00b7ba\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "ADJA"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {