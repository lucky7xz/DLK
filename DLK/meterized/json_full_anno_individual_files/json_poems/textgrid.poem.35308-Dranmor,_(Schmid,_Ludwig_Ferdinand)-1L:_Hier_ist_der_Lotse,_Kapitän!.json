{"textgrid.poem.35308": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier ist der Lotse, Kapit\u00e4n!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist der Lotse, Kapit\u00e4n!", "tokens": ["Hier", "ist", "der", "Lot\u00b7se", ",", "Ka\u00b7pi\u00b7t\u00e4n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun m\u00f6gt Ihr gern der Ruhe pflegen", "tokens": ["Nun", "m\u00f6gt", "Ihr", "gern", "der", "Ru\u00b7he", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Euch auf Lorbeern niederlegen,", "tokens": ["Und", "Euch", "auf", "Lor\u00b7beern", "nie\u00b7der\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wir Euch willig zugestehn.", "tokens": ["Die", "wir", "Euch", "wil\u00b7lig", "zu\u00b7ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Hafen schwindet Euer Zorn;", "tokens": ["Im", "Ha\u00b7fen", "schwin\u00b7det", "Eu\u00b7er", "Zorn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Ihr die Mannschaft ausgescholten,", "tokens": ["Wenn", "Ihr", "die", "Mann\u00b7schaft", "aus\u00b7ge\u00b7schol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So wart Ihr selbst, wenn es gegolten,", "tokens": ["So", "wart", "Ihr", "selbst", ",", "wenn", "es", "ge\u00b7gol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Mann von echtem Schrot und Korn.", "tokens": ["Ein", "Mann", "von", "ech\u00b7tem", "Schrot", "und", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seid Ihr nicht immer delikat", "tokens": ["Seid", "Ihr", "nicht", "im\u00b7mer", "de\u00b7li\u00b7kat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "PPER", "PTKNEG", "ADV", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und mit den Damen gar zu offen \u2013", "tokens": ["Und", "mit", "den", "Da\u00b7men", "gar", "zu", "of\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PTKA", "ADJD", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wir sind im Hafen eingetroffen,", "tokens": ["Wir", "sind", "im", "Ha\u00b7fen", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist das beste Resultat;", "tokens": ["Das", "ist", "das", "bes\u00b7te", "Re\u00b7sul\u00b7tat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und freuen wird's mich, Kapit\u00e4n,", "tokens": ["Und", "freu\u00b7en", "wird's", "mich", ",", "Ka\u00b7pi\u00b7t\u00e4n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wir uns wiederum begegnen.", "tokens": ["Wenn", "wir", "uns", "wie\u00b7de\u00b7rum", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Herr soll Eure Fahrten segnen,", "tokens": ["Der", "Herr", "soll", "Eu\u00b7re", "Fahr\u00b7ten", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein alter Freund \u2013 auf Wiedersehn!", "tokens": ["Mein", "al\u00b7ter", "Freund", "\u2013", "auf", "Wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier ist der Lotse, Kapit\u00e4n!", "tokens": ["Hier", "ist", "der", "Lot\u00b7se", ",", "Ka\u00b7pi\u00b7t\u00e4n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun m\u00f6gt Ihr gern der Ruhe pflegen", "tokens": ["Nun", "m\u00f6gt", "Ihr", "gern", "der", "Ru\u00b7he", "pfle\u00b7gen"], "token_info": ["word", "