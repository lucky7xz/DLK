, "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "O Hoffen, Schmachten, Liebesleid und Sehnen,", "tokens": ["O", "Hof\u00b7fen", ",", "Schmach\u00b7ten", ",", "Lie\u00b7be\u00b7sleid", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie d\u00fcrst' ich nach den s\u00fc\u00dfen Thr\u00e4nen!", "tokens": ["Wie", "d\u00fcr\u00b7st'", "ich", "nach", "den", "s\u00fc\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O tr\u00f6ste mich doch, eitles W\u00e4hnen,", "tokens": ["O", "tr\u00f6s\u00b7te", "mich", "doch", ",", "eit\u00b7les", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So leer du bist, so todt, so nichtig!", "tokens": ["So", "leer", "du", "bist", ",", "so", "todt", ",", "so", "nich\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verla\u00dft ihr alle mich so fl\u00fcchtig?", "tokens": ["Ver\u00b7la\u00dft", "ihr", "al\u00b7le", "mich", "so", "fl\u00fcch\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "O Gegenwart, wie bist du schnell!", "tokens": ["O", "Ge\u00b7gen\u00b7wart", ",", "wie", "bist", "du", "schnell", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergangenheit, wie bist du klein!", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", ",", "wie", "bist", "du", "klein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Zukunft, wie wirst du unendlich seyn!", "tokens": ["O", "Zu\u00b7kunft", ",", "wie", "wirst", "du", "un\u00b7end\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Unendlich wie am Himmelsbogen", "tokens": ["Un\u00b7end\u00b7lich", "wie", "am", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sterne in die ew'gen R\u00e4ume steigen,", "tokens": ["Die", "Ster\u00b7ne", "in", "die", "ew'\u00b7gen", "R\u00e4u\u00b7me", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So f\u00fchl' ich Stunden, Tage, Monden hergezogen,", "tokens": ["So", "f\u00fchl'", "ich", "Stun\u00b7den", ",", "Ta\u00b7ge", ",", "Mon\u00b7den", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und durch mein tiefstes Seyn das tr\u00fcbe Schweigen,", "tokens": ["Und", "durch", "mein", "tiefs\u00b7tes", "Seyn", "das", "tr\u00fc\u00b7be", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um mich ein unverg\u00e4nglich Meer von schwarzen Wogen,", "tokens": ["Um", "mich", "ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Meer", "von", "schwar\u00b7zen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ach! kein gr\u00fcnes Ufer will sich zeigen!", "tokens": ["Und", "ach", "!", "kein", "gr\u00fc\u00b7nes", "U\u00b7fer", "will", "sich", "zei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PIAT", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}