{"textgrid.poem.38754": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ist es denn, da\u00df tr\u00fcb und schwer", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist es denn, da\u00df tr\u00fcb und schwer", "tokens": ["Wie", "ist", "es", "denn", ",", "da\u00df", "tr\u00fcb", "und", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So alles k\u00f6mmt, vor\u00fcberzieht,", "tokens": ["So", "al\u00b7les", "k\u00f6mmt", ",", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wechselnd, qu\u00e4lend, immer leer,", "tokens": ["Und", "wech\u00b7selnd", ",", "qu\u00e4\u00b7lend", ",", "im\u00b7mer", "leer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das arme Herz in sich vergl\u00fcht?", "tokens": ["Das", "ar\u00b7me", "Herz", "in", "sich", "ver\u00b7gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum gekommen", "tokens": ["Kaum", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word"], "pos": ["ADV", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Soll ich scheiden,", "tokens": ["Soll", "ich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Kaum entglommen", "tokens": ["Kaum", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "L\u00f6schen wieder", "tokens": ["L\u00f6\u00b7schen", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Alle Freuden,", "tokens": ["Al\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und der Leiden", "tokens": ["Und", "der", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dunkle Wolke senkt sich nieder.", "tokens": ["Dunk\u00b7le", "Wol\u00b7ke", "senkt", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus den Lichtern in die Nacht,", "tokens": ["Aus", "den", "Lich\u00b7tern", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Aus den Augen, die mir tagen,", "tokens": ["Aus", "den", "Au\u00b7gen", ",", "die", "mir", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Die mein ganzes Herz durchlacht,", "tokens": ["Die", "mein", "gan\u00b7zes", "Herz", "durch\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich wieder allen Plagen,", "tokens": ["Bin", "ich", "wie\u00b7der", "al\u00b7len", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem d\u00fcrren Leben", "tokens": ["Dem", "d\u00fcr\u00b7ren", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zur\u00fcck gegeben.", "tokens": ["Zu\u00b7r\u00fcck", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wie fl\u00fccht'ge Augenblicke", "tokens": ["Wie", "fl\u00fccht'\u00b7ge", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Gl\u00fccke!", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Wie lange, lange Dauer", "tokens": ["Wie", "lan\u00b7ge", ",", "lan\u00b7ge", "Dau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der Trennung d\u00fcstre schwere Trauer! \u2013", "tokens": ["Der", "Tren\u00b7nung", "d\u00fcst\u00b7re", "schwe\u00b7re", "Trau\u00b7er", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zur\u00fcck zu kehren", "tokens": ["Zu\u00b7r\u00fcck", "zu", "keh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und dich entbehren!", "tokens": ["Und", "dich", "ent\u00b7beh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O als ich dich noch nicht gesehn,", "tokens": ["O", "als", "ich", "dich", "noch", "nicht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da durfte Sehnsucht bei mir seyn,", "tokens": ["Da", "durf\u00b7te", "Sehn\u00b7sucht", "bei", "mir", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hoffnungswind in meinen W\u00fcnschen wehn,", "tokens": ["Ein", "Hoff\u00b7nungs\u00b7wind", "in", "mei\u00b7nen", "W\u00fcn\u00b7schen", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Zukunft war ein heller Schein:", "tokens": ["Die", "Zu\u00b7kunft", "war", "ein", "hel\u00b7ler", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt mu\u00df ich vom Erinnern kaufen,", "tokens": ["Jetzt", "mu\u00df", "ich", "vom", "E\u00b7rin\u00b7nern", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich kaum zerstreut empfand;", "tokens": ["Was", "ich", "kaum", "zer\u00b7streut", "emp\u00b7fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieder durch die w\u00fcsten Haufen,", "tokens": ["Wie\u00b7der", "durch", "die", "w\u00fcs\u00b7ten", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch ein unbewohntes Land,", "tokens": ["Durch", "ein", "un\u00b7be\u00b7wohn\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll ich irre, klagend, schweifen,", "tokens": ["Soll", "ich", "ir\u00b7re", ",", "kla\u00b7gend", ",", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und des Gl\u00fcckes goldne Streifen,", "tokens": ["Und", "des", "Gl\u00fc\u00b7ckes", "gold\u00b7ne", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auch die letzten, abgewandt.", "tokens": ["Auch", "die", "letz\u00b7ten", ",", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Noch f\u00fchl' ich deine Hand,", "tokens": ["Noch", "f\u00fchl'", "ich", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Noch wie im Traume deine K\u00fcsse,", "tokens": ["Noch", "wie", "im", "Trau\u00b7me", "dei\u00b7ne", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Noch folgen mir die holden Blicke,", "tokens": ["Noch", "fol\u00b7gen", "mir", "die", "hol\u00b7den", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und die Empfindung, da\u00df ich alles misse,", "tokens": ["Und", "die", "Emp\u00b7fin\u00b7dung", ",", "da\u00df", "ich", "al\u00b7les", "mis\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Bleibt bei mir zur\u00fccke.", "tokens": ["Bleibt", "bei", "mir", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "O Hoffen, Schmachten, Liebesleid und Sehnen,", "tokens": ["O", "Hof\u00b7fen", ",", "Schmach\u00b7ten", ",", "Lie\u00b7be\u00b7sleid", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie d\u00fcrst' ich nach den s\u00fc\u00dfen Thr\u00e4nen!", "tokens": ["Wie", "d\u00fcr\u00b7st'", "ich", "nach", "den", "s\u00fc\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O tr\u00f6ste mich doch, eitles W\u00e4hnen,", "tokens": ["O", "tr\u00f6s\u00b7te", "mich", "doch", ",", "eit\u00b7les", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So leer du bist, so todt, so nichtig!", "tokens": ["So", "leer", "du", "bist", ",", "so", "todt", ",", "so", "nich\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verla\u00dft ihr alle mich so fl\u00fcchtig?", "tokens": ["Ver\u00b7la\u00dft", "ihr", "al\u00b7le", "mich", "so", "fl\u00fcch\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Gegenwart, wie bist du schnell!", "tokens": ["O", "Ge\u00b7gen\u00b7wart", ",", "wie", "bist", "du", "schnell", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergangenheit, wie bist du klein!", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", ",", "wie", "bist", "du", "klein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Zukunft, wie wirst du unendlich seyn!", "tokens": ["O", "Zu\u00b7kunft", ",", "wie", "wirst", "du", "un\u00b7end\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Unendlich wie am Himmelsbogen", "tokens": ["Un\u00b7end\u00b7lich", "wie", "am", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sterne in die ew'gen R\u00e4ume steigen,", "tokens": ["Die", "Ster\u00b7ne", "in", "die", "ew'\u00b7gen", "R\u00e4u\u00b7me", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So f\u00fchl' ich Stunden, Tage, Monden hergezogen,", "tokens": ["So", "f\u00fchl'", "ich", "Stun\u00b7den", ",", "Ta\u00b7ge", ",", "Mon\u00b7den", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und durch mein tiefstes Seyn das tr\u00fcbe Schweigen,", "tokens": ["Und", "durch", "mein", "tiefs\u00b7tes", "Seyn", "das", "tr\u00fc\u00b7be", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um mich ein unverg\u00e4nglich Meer von schwarzen Wogen,", "tokens": ["Um", "mich", "ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Meer", "von", "schwar\u00b7zen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ach! kein gr\u00fcnes Ufer will sich zeigen!", "tokens": ["Und", "ach", "!", "kein", "gr\u00fc\u00b7nes", "U\u00b7fer", "will", "sich", "zei\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PIAT", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie ist es denn, da\u00df tr\u00fcb und schwer", "tokens": ["Wie", "ist", "es", "denn", ",", "da\u00df", "tr\u00fcb", "und", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So alles k\u00f6mmt, vor\u00fcberzieht,", "tokens": ["So", "al\u00b7les", "k\u00f6mmt", ",", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wechselnd, qu\u00e4lend, immer leer,", "tokens": ["Und", "wech\u00b7selnd", ",", "qu\u00e4\u00b7lend", ",", "im\u00b7mer", "leer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das arme Herz in sich vergl\u00fcht?", "tokens": ["Das", "ar\u00b7me", "Herz", "in", "sich", "ver\u00b7gl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Kaum gekommen", "tokens": ["Kaum", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word"], "pos": ["ADV", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Soll ich scheiden,", "tokens": ["Soll", "ich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Kaum entglommen", "tokens": ["Kaum", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "L\u00f6schen wieder", "tokens": ["L\u00f6\u00b7schen", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Alle Freuden,", "tokens": ["Al\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und der Leiden", "tokens": ["Und", "der", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dunkle Wolke senkt sich nieder.", "tokens": ["Dunk\u00b7le", "Wol\u00b7ke", "senkt", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aus den Lichtern in die Nacht,", "tokens": ["Aus", "den", "Lich\u00b7tern", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "