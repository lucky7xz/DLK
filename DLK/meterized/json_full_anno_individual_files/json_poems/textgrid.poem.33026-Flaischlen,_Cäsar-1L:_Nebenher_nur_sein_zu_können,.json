{"textgrid.poem.33026": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nebenher nur sein zu k\u00f6nnen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nebenher nur sein zu k\u00f6nnen,", "tokens": ["Ne\u00b7ben\u00b7her", "nur", "sein", "zu", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAINF", "PTKZU", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was du sein willst,", "tokens": ["was", "du", "sein", "willst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAINF", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "das ist das Bittere ...", "tokens": ["das", "ist", "das", "Bit\u00b7te\u00b7re", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "in halbverlorenen, abendm\u00fcden Stunden nur ...", "tokens": ["in", "halb\u00b7ver\u00b7lo\u00b7re\u00b7nen", ",", "a\u00b7bend\u00b7m\u00fc\u00b7den", "Stun\u00b7den", "nur", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "ADV", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "statt mit der vollen Lust des vollen Tags", "tokens": ["statt", "mit", "der", "vol\u00b7len", "Lust", "des", "vol\u00b7len", "Tags"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "freihaupt, gradaus, wie andre,", "tokens": ["frei\u00b7haupt", ",", "gra\u00b7daus", ",", "wie", "and\u00b7