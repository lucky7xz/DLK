{"textgrid.poem.36834": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein! Apulien hat der Hohenstaufen", "tokens": ["Nein", "!", "A\u00b7pu\u00b7li\u00b7en", "hat", "der", "Ho\u00b7hen\u00b7stau\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "NE", "VAFIN", "ART", "NN"], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Letzten Spr\u00f6\u00dfling geraubt dem Vaterlande,", "tokens": ["Letz\u00b7ten", "Spr\u00f6\u00df\u00b7ling", "ge\u00b7raubt", "dem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nicht den Dichter, o Kaiserhaus von Schwaben.", "tokens": ["Nicht", "den", "Dich\u00b7ter", ",", "o", "Kai\u00b7ser\u00b7haus", "von", "Schwa\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "FM", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Nein, hochherziger Freund, in gold'ner Str\u00f6mung", "tokens": ["Nein", ",", "hoch\u00b7her\u00b7zi\u00b7ger", "Freund", ",", "in", "gold'\u00b7ner", "Str\u00f6\u00b7mung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Flossen Jahre dahin, seit ich am Tiber", "tokens": ["Flos\u00b7sen", "Jah\u00b7re", "da\u00b7hin", ",", "seit", "ich", "am", "Ti\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "PAV", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und am st\u00e4dtebes\u00e4ten, meerumsp\u00fclten", "tokens": ["Und", "am", "st\u00e4d\u00b7te\u00b7be\u00b7s\u00e4\u00b7ten", ",", "meer\u00b7um\u00b7sp\u00fcl\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "APPRART", "ADJA", "$,", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Aschenberge der Vorwelt Heldengr\u00f6\u00dfe", "tokens": ["A\u00b7schen\u00b7ber\u00b7ge", "der", "Vor\u00b7welt", "Hel\u00b7den\u00b7gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und der reizendsten Mitwelt Lust genie\u00dfe.", "tokens": ["Und", "der", "rei\u00b7zends\u00b7ten", "Mit\u00b7welt", "Lust", "ge\u00b7nie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Alter R\u00f6mer gedacht' ich, doch beim gro\u00dfen,", "tokens": ["Al\u00b7ter", "R\u00f6\u00b7mer", "ge\u00b7dacht'", "ich", ",", "doch", "beim", "gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "$,", "ADV", "APPRART", "ADJA", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Theuern Namen des Vaterlands und Friedrichs", "tokens": ["Theu\u00b7ern", "Na\u00b7men", "des", "Va\u00b7ter\u00b7lands", "und", "Fried\u00b7richs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "KON", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Herrschergenius, Freund, geschworen sei dir's,", "tokens": ["Herr\u00b7scher\u00b7ge\u00b7nius", ",", "Freund", ",", "ge\u00b7schwo\u00b7ren", "sei", "dir's", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVPP", "VAFIN", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Deutscher Glorie dacht' ich auch. Wohl hat ans", "tokens": ["Deut\u00b7scher", "Glo\u00b7rie", "dacht'", "ich", "auch", ".", "Wohl", "hat", "ans"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$.", "ADV", "VAFIN", "APPRART"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Junge Herz der Sirene Lied geklungen", "tokens": ["Jun\u00b7ge", "Herz", "der", "Si\u00b7re\u00b7ne", "Lied", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.14": {"text": "Und im Rausch des Moments der Zukunft Plane,", "tokens": ["Und", "im", "Rausch", "des", "Mo\u00b7ments", "der", "Zu\u00b7kunft", "Pla\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Der Vergangenheit Kraft verga\u00df der Wandrer.", "tokens": ["Der", "Ver\u00b7gan\u00b7gen\u00b7heit", "Kraft", "ver\u00b7ga\u00df", "der", "Wand\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Doch nur kurz; aus des Anio Wasserst\u00fcrzen,", "tokens": ["Doch", "nur", "kurz", ";", "aus", "des", "A\u00b7nio", "Was\u00b7ser\u00b7st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$.", "APPR", "ART", "NE", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Aus des Pantheons heil'gen D\u00e4mmerungen,", "tokens": ["Aus", "des", "Pan\u00b7the\u00b7ons", "heil'\u00b7gen", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Von der S\u00e4ule herab des Imperators", "tokens": ["Von", "der", "S\u00e4u\u00b7le", "her\u00b7ab", "des", "Im\u00b7pe\u00b7ra\u00b7tors"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "Und aus P\u00e4stums gewalt'gen Dorertempeln", "tokens": ["Und", "aus", "P\u00e4s\u00b7tums", "ge\u00b7walt'\u00b7gen", "Do\u00b7rer\u00b7tem\u00b7peln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Sprach der strengere Gott: Wach' auf zum Werke!", "tokens": ["Sprach", "der", "stren\u00b7ge\u00b7re", "Gott", ":", "Wach'", "auf", "zum", "Wer\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Feire muthig dein Volk und seine Helden!", "tokens": ["Fei\u00b7re", "mut\u00b7hig", "dein", "Volk", "und", "sei\u00b7ne", "Hel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Dir bekenn' ich besch\u00e4mt, dem gro\u00dfen Rufe", "tokens": ["Dir", "be\u00b7kenn'", "ich", "be\u00b7sch\u00e4mt", ",", "dem", "gro\u00b7\u00dfen", "Ru\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Folgt' ich nicht und des eig'nen Herzens Leiden", "tokens": ["Folgt'", "ich", "nicht", "und", "des", "eig'\u00b7nen", "Her\u00b7zens", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "KON", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und vermessene W\u00fcnsch' und Liebefreuden", "tokens": ["Und", "ver\u00b7mes\u00b7se\u00b7ne", "W\u00fcn\u00b7sch'", "und", "Lie\u00b7be\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sang ich nur; auf dem Haupt Weinlaub und Rosen,", "tokens": ["Sang", "ich", "nur", ";", "auf", "dem", "Haupt", "Wein\u00b7laub", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Oft die Asche des Grams, doch nie den Lorbeer,", "tokens": ["Oft", "die", "A\u00b7sche", "des", "Grams", ",", "doch", "nie", "den", "Lor\u00b7beer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Oeffnet' ich zum Gesang die Lipp' und str\u00f6mte", "tokens": ["Oeff\u00b7net'", "ich", "zum", "Ge\u00b7sang", "die", "Lipp'", "und", "str\u00f6m\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gluth aus eigenem Feuerquell in manches", "tokens": ["Gluth", "aus", "ei\u00b7ge\u00b7nem", "Feu\u00b7er\u00b7quell", "in", "man\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "PIS"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Gl\u00fch'nde Herz; doch vergieb, o Freund, der Jugend.", "tokens": ["Gl\u00fch'n\u00b7de", "Herz", ";", "doch", "ver\u00b7gieb", ",", "o", "Freund", ",", "der", "Ju\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "$,", "FM", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Denn voll bl\u00fchte der Fr\u00fchling meines Lebens", "tokens": ["Denn", "voll", "bl\u00fch\u00b7te", "der", "Fr\u00fch\u00b7ling", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und ergieb'ger vielleicht als dort im Norden", "tokens": ["Und", "er\u00b7gie\u00b7b'\u00b7ger", "viel\u00b7leicht", "als", "dort", "im", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "KOKOM", "ADV", "APPRART", "NN"], "meter": "--+---+-+-+-", "measure": "anapaest.init"}, "line.11": {"text": "Du zu sehen gewohnt; und feur'ge Wetter,", "tokens": ["Du", "zu", "se\u00b7hen", "ge\u00b7wohnt", ";", "und", "feur'\u00b7ge", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVPP", "$.", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Brausten st\u00fcrmend im wilden Geist des Fr\u00fchlings,", "tokens": ["Braus\u00b7ten", "st\u00fcr\u00b7mend", "im", "wil\u00b7den", "Geist", "des", "Fr\u00fch\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Kr\u00e4fte str\u00f6mend im Kampf der Leidenschaften,", "tokens": ["Kr\u00e4f\u00b7te", "str\u00f6\u00b7mend", "im", "Kampf", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Und was Wetter und Sturm dem auferweckten", "tokens": ["Und", "was", "Wet\u00b7ter", "und", "Sturm", "dem", "au\u00b7fer\u00b7weck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "NN", "KON", "NN", "ART", "ADJA"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Fr\u00fchlingsdrang der Natur, war mir die Liebe.", "tokens": ["Fr\u00fch\u00b7lings\u00b7drang", "der", "Na\u00b7tur", ",", "war", "mir", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch vom Sommer die Frucht, vom hei\u00dfen Mittag", "tokens": ["Doch", "vom", "Som\u00b7mer", "die", "Frucht", ",", "vom", "hei\u00b7\u00dfen", "Mit\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht die kr\u00e4ftige That zu fordern, d\u00e4ucht mir", "tokens": ["Nicht", "die", "kr\u00e4f\u00b7ti\u00b7ge", "That", "zu", "for\u00b7dern", ",", "d\u00e4ucht", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Billig. Komm' in den S\u00fcden, Freund, und lerne,", "tokens": ["Bil\u00b7lig", ".", "Komm'", "in", "den", "S\u00fc\u00b7den", ",", "Freund", ",", "und", "ler\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ob geschmeichelt, getr\u00e4nkt von s\u00fc\u00dfern L\u00fcften,", "tokens": ["Ob", "ge\u00b7schmei\u00b7chelt", ",", "ge\u00b7tr\u00e4nkt", "von", "s\u00fc\u00b7\u00dfern", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ob am athmenden Busen nicht Armida's,", "tokens": ["Ob", "am", "ath\u00b7men\u00b7den", "Bu\u00b7sen", "nicht", "Ar\u00b7mi\u00b7da's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "PTKNEG", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Ob dein Auge nicht bricht. Ich harre deiner", "tokens": ["Ob", "dein", "Au\u00b7ge", "nicht", "bricht", ".", "Ich", "har\u00b7re", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "In Sorrento. Mein Retter willst du werden;", "tokens": ["In", "Sor\u00b7ren\u00b7to", ".", "Mein", "Ret\u00b7ter", "willst", "du", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Komm' und bleibe bezaubert wie Rinaldo.", "tokens": ["Komm'", "und", "blei\u00b7be", "be\u00b7zau\u00b7bert", "wie", "Ri\u00b7nal\u00b7do", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "VVPP", "KOKOM", "NE", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Nein! Apulien hat der Hohenstaufen", "tokens": ["Nein", "!", "A\u00b7pu\u00b7li\u00b7en", "hat", "der", "Ho\u00b7hen\u00b7stau\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "NE", "VAFIN", "ART", "NN"], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Letzten Spr\u00f6\u00dfling geraubt dem Vaterlande,", "tokens": ["Letz\u00b7ten", "Spr\u00f6\u00df\u00b7ling", "ge\u00b7raubt", "dem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nicht den Dichter, o Kaiserhaus von Schwaben.", "tokens": ["Nicht", "den", "Dich\u00b7ter", ",", "o", "K