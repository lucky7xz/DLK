{"textgrid.poem.36599": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der reiche Mann", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein reicher Mann bin ich, ich habe", "tokens": ["Ein", "rei\u00b7cher", "Mann", "bin", "ich", ",", "ich", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das g\u00f6ttliche Geschenk, die Gabe,", "tokens": ["Das", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7schenk", ",", "die", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Wenigem vergn\u00fcgt zu sein!", "tokens": ["Mit", "We\u00b7ni\u00b7gem", "ver\u00b7gn\u00fcgt", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein M\u00e4dchen hab' ich, gut zum k\u00fcssen,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "hab'", "ich", ",", "gut", "zum", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADJD", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und einen Freund, ein gut Gewissen,", "tokens": ["Und", "ei\u00b7nen", "Freund", ",", "ein", "gut", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-",