}, "line.4": {"text": "Verdank' ich deinem Rath.", "tokens": ["Ver\u00b7dank'", "ich", "dei\u00b7nem", "Rath", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Aufrichtiger, als du bist, kann", "tokens": ["Auf\u00b7rich\u00b7ti\u00b7ger", ",", "als", "du", "bist", ",", "kann"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "$,", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Freund auf Erden sein;", "tokens": ["Kein", "Freund", "auf", "Er\u00b7den", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du zeigst mir jedes Fleckchen an,", "tokens": ["Du", "zeigst", "mir", "je\u00b7des", "Fleck\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4r es noch so klein.", "tokens": ["Und", "w\u00e4r", "es", "noch", "so", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Dabei bist du galant und sag'st", "tokens": ["Da\u00b7bei", "bist", "du", "ga\u00b7lant", "und", "sag'st"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir stets, wie sch\u00f6n ich sei,", "tokens": ["Mir", "stets", ",", "wie", "sch\u00f6n", "ich", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Complimente, die du machst,", "tokens": ["Und", "Com\u00b7pli\u00b7men\u00b7te", ",", "die", "du", "machst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind keine Schmeichelei.", "tokens": ["Sind", "kei\u00b7ne", "Schmei\u00b7che\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "O Lieber, thu nur immerhin,", "tokens": ["O", "Lie\u00b7ber", ",", "thu", "nur", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du bisher gethan,", "tokens": ["Wie", "du", "bis\u00b7her", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und werde, wenn ich \u00e4lter bin,", "tokens": ["Und", "wer\u00b7de", ",", "wenn", "ich", "\u00e4l\u00b7ter", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ja kein Grobian.", "tokens": ["Mir", "ja", "kein", "Gro\u00b7bi\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}