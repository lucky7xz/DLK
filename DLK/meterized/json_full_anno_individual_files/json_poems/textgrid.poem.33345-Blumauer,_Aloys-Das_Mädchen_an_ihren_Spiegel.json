 "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In jedem Umgang f\u00fchlt man Zwang,", "tokens": ["In", "je\u00b7dem", "Um\u00b7gang", "f\u00fchlt", "man", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur in dem deinen nicht.", "tokens": ["Nur", "in", "dem", "dei\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ist mir oft so \u00e4rgerlich,", "tokens": ["Und", "ist", "mir", "oft", "so", "\u00e4r\u00b7ger\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich's nicht sagen kann,", "tokens": ["Da\u00df", "ich's", "nicht", "sa\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So f\u00e4ngt beim ersten Blick auf dich", "tokens": ["So", "f\u00e4ngt", "beim", "ers\u00b7ten", "Blick", "auf", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Mund zu l\u00e4cheln an.", "tokens": ["Mein", "Mund", "zu", "l\u00e4\u00b7cheln", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die sch\u00f6nste Freundesharmonie", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Freun\u00b7des\u00b7har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herrscht zwischen mir und dir,", "tokens": ["Herrscht", "zwi\u00b7schen", "mir", "und", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du seufzest mit aus Sympathie,", "tokens": ["Du", "seuf\u00b7zest", "mit", "aus", "Sym\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lachst und weinst mit mir.", "tokens": ["Und", "lachst", "und", "weinst", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein Freund auf Erden stimmt so sehr", "tokens": ["Kein", "Freund", "auf", "Er\u00b7den", "stimmt", "so", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach meinen Launen sich,", "tokens": ["Nach", "mei\u00b7nen", "Lau\u00b7nen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Freund auf Erden liebt mich mehr,", "tokens": ["Kein", "Freund", "auf", "Er\u00b7den", "liebt", "mich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als du, mein zweitest Ich! \u2013", "tokens": ["Als", "du", ",", "mein", "zwei\u00b7test", "Ich", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du bist mein Lehrer jederzeit,", "tokens": ["Du", "bist", "mein", "Leh\u00b7rer", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie werd' ich deiner satt;", "tokens": ["Nie", "werd'", "ich", "dei\u00b7ner", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "All' meine Liebensw\u00fcrdigkeit", "tokens": ["All'", "mei\u00b7ne", "Lie\u00b7bens\u00b7w\u00fcr\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdank' ich deinem Rath.", "tokens": ["Ver\u00b7dank'", "ich", "dei\u00b7nem", "Rath", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Aufrichtiger, als du bist, kann", "tokens": ["Auf\u00b7rich\u00b7ti\u00b7ger", ",", "als", "du", "bist", ",", "kann"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "$,", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Freund auf Erden sein;", "tokens": ["Kein", "Freund", "auf", "Er\u00b7den", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du zeigst mir jedes Fleckchen an,", "tokens": ["Du", "zeigst", "mir", "je\u00b7des", "Fleck\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4r es noch so klein.", "tokens": ["Und", "w\u00e4r", "es", "noch", "so", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dabei bist du galant und sag'st", "tokens": ["Da\u00b7bei", "bist", "du", "ga\u00b7lant", "und", "sag'st"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir stets, wie sch\u00f6n ich sei,", "tokens": ["Mir", "stets", ",", "wie", "sch\u00f6n", "ich", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Complimente, die du machst,", "tokens": ["Und", "Com\u00b7pli\u00b7men\u00b7te", ",", "die", "du", "machst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind keine Schmeichelei.", "tokens": ["Sind", "kei\u00b7ne", "Schmei\u00b7che\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Lieber, thu nur immerhin,", "tokens": ["O", "Lie\u00b7ber", ",", "thu", "nur", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du bisher gethan,", "tokens": ["Wie", "du", "bis\u00b7her", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und werde, wenn ich \u00e4lter bin,", "tokens": ["Und", "wer\u00b7de", ",", "wenn", "ich", "\u00e4l\u00b7ter", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ja kein Grobian.", "tokens": ["Mir", "ja", "kein", "Gro\u00b7bi\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O Spiegel, wie lebendig scheint", "tokens": ["O", "Spie\u00b7gel", ",", "wie", "le\u00b7ben\u00b7dig", "scheint"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein liebes Bild aus dir!", "tokens": ["Mein", "lie\u00b7bes", "Bild", "aus", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Rath, mein Zeitvertreib, mein Freund,", "tokens": ["Mein", "Rath", ",", "mein", "Zeit\u00b7ver\u00b7treib", ",", "mein", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$