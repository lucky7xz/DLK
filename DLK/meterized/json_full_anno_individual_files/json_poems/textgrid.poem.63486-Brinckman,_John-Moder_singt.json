d", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "NN", "PTKNEG", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un weegt di in, du s\u00f6tes J\u00f6r,", "tokens": ["un", "weegt", "di", "in", ",", "du", "s\u00f6\u00b7tes", "J\u00f6r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "un sitt un wakt bi di.", "tokens": ["un", "sitt", "un", "wakt", "bi", "di."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM", "FM", "FM", "FM", "FM", "NE"], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Slap, Kinning, slap!", "tokens": ["Slap", ",", "Kin\u00b7ning", ",", "slap", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Denn fleegt de Engels all to Hus", "tokens": ["Denn", "fleegt", "de", "En\u00b7gels", "all", "to", "Hus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "NE", "PIAT", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "un kamt nich mihr mit Blomenstru\u00df,", "tokens": ["un", "kamt", "nich", "mihr", "mit", "Blo\u00b7menst\u00b7ru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "mit L\u00e4uschen un Gesang.", "tokens": ["mit", "L\u00e4u\u00b7schen", "un", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "All fleegen furt se bet up een,", "tokens": ["All", "flee\u00b7gen", "furt", "se", "bet", "up", "e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "de blifft, de lett di nich alleen", "tokens": ["de", "blifft", ",", "de", "lett", "di", "nich", "al\u00b7le\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "din ganzes Laben lang.", "tokens": ["din", "gan\u00b7zes", "La\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Slap, Kinning, slap!", "tokens": ["Slap", ",", "Kin\u00b7ning", ",", "slap", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ja, de h\u00f6lt ut un blifft di tru,", "tokens": ["Ja", ",", "de", "h\u00f6lt", "ut", "un", "blifft", "di", "tru", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "bet dat an't Starben geihst eens du.", "tokens": ["bet", "dat", "an't", "Star\u00b7ben", "geihst", "e\u00b7ens", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Man deist, wat got em d\u00fccht \u2013", "tokens": ["Man", "de\u00b7ist", ",", "wat", "got", "em", "d\u00fccht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wat he di rad't, is j\u00fcmmer recht,", "tokens": ["wat", "he", "di", "rad't", ",", "is", "j\u00fcm\u00b7mer", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "FM", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "gl\u00f6w du man, wat din M\u00f6ding seggt.", "tokens": ["gl\u00f6w", "du", "man", ",", "wat", "din", "M\u00f6\u00b7ding", "seggt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "de nicks nich v\u00f6r di l\u00fcggt.", "tokens": ["de", "nicks", "nich", "v\u00f6r", "di", "l\u00fcggt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Slap, Kinning, slap!", "tokens": ["Slap", ",", "Kin\u00b7ning", ",", "slap", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Denn h\u00f6d di jo, dat is min Irnst,", "tokens": ["Denn", "h\u00f6d", "di", "jo", ",", "dat", "is", "min", "Irnst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "NE", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dat du em nie un nie vert\u00fcrnst,", "t