{"textgrid.poem.63486": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Moder singt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Slap, Kinning, slap!", "tokens": ["Slap", ",", "Kin\u00b7ning", ",", "slap", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ITJ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dor buten Wisch un Brok un B\u00e4k", "tokens": ["Dor", "bu\u00b7ten", "Wisch", "un", "Brok", "un", "B\u00e4k"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "de liggen ok warm \u00fcnner Bedd un D\u00e4k", "tokens": ["de", "lig\u00b7gen", "ok", "warm", "\u00fcn\u00b7ner", "Bedd", "un", "D\u00e4k"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "un B\u00fcren blank un witt,", "tokens": ["un", "B\u00fc\u00b7ren", "blank", "un", "witt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "bet dat de l\u00fctte Lewark k\u00fcmmt", "tokens": ["bet", "dat", "de", "l\u00fct\u00b7te", "Le\u00b7wark", "k\u00fcmmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NE", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un Kiwits schriet un Immen s\u00fcmmt", "tokens": ["un", "Ki\u00b7wits", "schriet", "un", "Im\u00b7men", "s\u00fcmmt"], "token_info": ["word", "word", "word", "word", "wo