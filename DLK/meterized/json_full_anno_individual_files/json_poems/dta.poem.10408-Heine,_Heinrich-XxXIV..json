{"dta.poem.10408": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, wenn ich nur der Schemel w\u00e4r',", "tokens": ["Ach", ",", "wenn", "ich", "nur", "der", "Sche\u00b7mel", "w\u00e4r'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf der Liebsten F\u00fc\u00dfe ruhn!", "tokens": ["Wo\u00b7rauf", "der", "Liebs\u00b7ten", "F\u00fc\u00b7\u00dfe", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stampfte sie mich noch so sehr,", "tokens": ["Und", "stampf\u00b7te", "sie", "mich", "noch", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wollte doch nicht klagen thun.", "tokens": ["Ich", "woll\u00b7te", "doch", "nicht", "kla\u00b7gen", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}