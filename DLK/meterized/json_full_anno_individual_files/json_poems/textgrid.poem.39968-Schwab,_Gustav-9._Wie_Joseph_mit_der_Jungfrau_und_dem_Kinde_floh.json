{"textgrid.poem.39968": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "9. Wie Joseph mit der Jungfrau und dem Kinde floh", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging der K\u00f6n'ge Zug hinaus,", "tokens": ["Es", "ging", "der", "K\u00f6n'\u00b7ge", "Zug", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und manche Nacht kam ohne Stern,", "tokens": ["Und", "man\u00b7che", "Nacht", "kam", "oh\u00b7ne", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00f6de war's im dunkeln Haus:", "tokens": ["Und", "\u00f6\u00b7de", "wa\u00b7r's", "im", "dun\u00b7keln", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da trat der Engel ein des Herrn.", "tokens": ["Da", "trat", "der", "En\u00b7gel", "ein", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Auge, schauend in der Nacht,", "tokens": ["Sein", "Au\u00b7ge", ",", "schau\u00b7end", "in", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ruht auf der Jungfrau, auf dem Sohn,", "tokens": ["Ruht", "auf", "der", "Jung\u00b7frau", ",", "auf", "dem", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den selig schlummernden, und sacht", "tokens": ["Den", "se\u00b7lig", "schlum\u00b7mern\u00b7den", ",", "und", "sacht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVINF", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ber\u00fchrt des Vaters Ohr sein Ton:", "tokens": ["Be\u00b7r\u00fchrt", "des", "Va\u00b7ters", "Ohr", "sein", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbfleuch nach Egypten, Mann, geschwind;", "tokens": ["\u00bb", "fleuch", "nach", "E\u00b7gyp\u00b7ten", ",", "Mann", ",", "ge\u00b7schwind", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVIMP", "APPR", "NE", "$,", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Harr' aus, bis ich dich rufe dort.", "tokens": ["Harr'", "aus", ",", "bis", "ich", "dich", "ru\u00b7fe", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herodes Mordstahl sucht das Kind:", "tokens": ["He\u00b7ro\u00b7des", "Mord\u00b7stahl", "sucht", "das", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihm und mit der Mutter fort!\u00ab", "tokens": ["Mit", "ihm", "und", "mit", "der", "Mut\u00b7ter", "fort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Josephs Traume spiegelt sich", "tokens": ["In", "Jo\u00b7se\u00b7phs", "Trau\u00b7me", "spie\u00b7gelt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Des Boten selige Gestalt;", "tokens": ["Des", "Bo\u00b7ten", "se\u00b7li\u00b7ge", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Schlaf entfloh, der Engel wich,", "tokens": ["Der", "Schlaf", "ent\u00b7floh", ",", "der", "En\u00b7gel", "wich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aufsteht er mit Marien bald.", "tokens": ["Auf\u00b7steht", "er", "mit", "Ma\u00b7ri\u00b7en", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Es'lein aus dem Stall er f\u00fchrt,", "tokens": ["Das", "Es'\u00b7lein", "aus", "dem", "Stall", "er", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er l\u00f6st es mit dem Opfergold.", "tokens": ["Er", "l\u00f6st", "es", "mit", "dem", "Op\u00b7fer\u00b7gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sorgsam dann, wie sich's geb\u00fchrt,", "tokens": ["Und", "sorg\u00b7sam", "dann", ",", "wie", "sich's", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hebt er hinauf die Jungfrau hold;", "tokens": ["Hebt", "er", "hin\u00b7auf", "die", "Jung\u00b7frau", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Kn\u00e4blein schl\u00e4ft an ihrer Brust,", "tokens": ["Das", "Kn\u00e4\u00b7blein", "schl\u00e4ft", "an", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er wandelt, an dem Zaum die Hand,", "tokens": ["Er", "wan\u00b7delt", ",", "an", "dem", "Zaum", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit der Morgensonne Lust", "tokens": ["Und", "mit", "der", "Mor\u00b7gen\u00b7son\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sind sie schon weit im offnen Land.", "tokens": ["Sind", "sie", "schon", "weit", "im", "off\u00b7nen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Indersch\u00e4tze reiches Gut", "tokens": ["Der", "In\u00b7der\u00b7sch\u00e4t\u00b7ze", "rei\u00b7ches", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat sich wunderlich geschmiegt,", "tokens": ["Es", "hat", "sich", "wun\u00b7der\u00b7lich", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einem B\u00fcndelein es ruht,", "tokens": ["In", "ei\u00b7nem", "B\u00fcn\u00b7del\u00b7ein", "es", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das auf des Thieres R\u00fccken liegt.", "tokens": ["Das", "auf", "des", "Thie\u00b7res", "R\u00fc\u00b7cken", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und leicht und fr\u00f6hlich geht die Fahrt,", "tokens": ["Und", "leicht", "und", "fr\u00f6h\u00b7lich", "geht", "die", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcberall auf ihrer Spur", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "auf", "ih\u00b7rer", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Menschen werden be\u00dfrer Art,", "tokens": ["Die", "Men\u00b7schen", "wer\u00b7den", "be\u00df\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und freundlicher wird die Natur.", "tokens": ["Und", "freund\u00b7li\u00b7cher", "wird", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Die L\u00fcfte bleiben warm und rein,", "tokens": ["Die", "L\u00fcf\u00b7te", "blei\u00b7ben", "warm", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Berg wird eben ihrem Schritt,", "tokens": ["Der", "Berg", "wird", "e\u00b7ben", "ih\u00b7rem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in den \u00f6den W\u00fcstenei'n", "tokens": ["Und", "in", "den", "\u00f6\u00b7den", "W\u00fcs\u00b7te\u00b7nei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entsprossen Rosen ihrem Tritt.", "tokens": ["Ent\u00b7spros\u00b7sen", "Ro\u00b7sen", "ih\u00b7rem", "Tritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und stehen wo im Heidenland", "tokens": ["Und", "ste\u00b7hen", "wo", "im", "Hei\u00b7den\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die G\u00f6tzenbilder, riesig, stumm:", "tokens": ["Die", "G\u00f6t\u00b7zen\u00b7bil\u00b7der", ",", "rie\u00b7sig", ",", "stumm", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo nur ihr Pfad sich hingewandt,", "tokens": ["Wo", "nur", "ihr", "Pfad", "sich", "hin\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da wanken sie und st\u00fcrzen um.", "tokens": ["Da", "wan\u00b7ken", "sie", "und", "st\u00fcr\u00b7zen", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und nach der zw\u00f6lften Tagfahrt schon", "tokens": ["Und", "nach", "der", "zw\u00f6lf\u00b7ten", "Tag\u00b7fahrt", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Winkt aus Egyptens hei\u00dfem Sand", "tokens": ["Winkt", "aus", "E\u00b7gyp\u00b7tens", "hei\u00b7\u00dfem", "Sand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und beut den k\u00fchlen Blumenthron", "tokens": ["Und", "beut", "den", "k\u00fch\u00b7len", "Blu\u00b7men\u00b7thron"], "token_info": ["word", "word", "word", "w