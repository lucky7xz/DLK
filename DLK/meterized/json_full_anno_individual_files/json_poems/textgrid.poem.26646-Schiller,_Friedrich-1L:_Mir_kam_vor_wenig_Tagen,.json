{"textgrid.poem.26646": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir kam vor wenig Tagen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir kam vor wenig Tagen,", "tokens": ["Mir", "kam", "vor", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie? fragt mich eben nicht,", "tokens": ["Wie", "?", "fragt", "mich", "e\u00b7ben", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Reich der ewgen Plagen", "tokens": ["Vom", "Reich", "der", "ew\u00b7gen", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zeitung zu Gesicht.", "tokens": ["Die", "Zei\u00b7tung", "zu", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sonst frag ich diesem Essen,", "tokens": ["Sonst", "frag", "ich", "die\u00b7sem", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo noch kein Kopf zerbrach,", "tokens": ["Wo", "noch", "kein", "Kopf", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Freikorps unsrer Pressen,", "tokens": ["Dem", "Frei\u00b7korps", "uns\u00b7rer", "Pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie billig, wenig nach.", "tokens": ["Wie", "bil\u00b7lig", ",", "we\u00b7nig", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch eine Randgloss lockte", "tokens": ["Doch", "ei\u00b7ne", "Rand\u00b7gloss", "lock\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Itzt meinen F\u00fcrwitz an,", "tokens": ["Itzt", "mei\u00b7nen", "F\u00fcr\u00b7witz", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denkt! wie das Blut mir stockte,", "tokens": ["Denkt", "!", "wie", "das", "Blut", "mir", "stock\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ich das Blatt begann:", "tokens": ["Als", "ich", "das", "Blatt", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbseit zwanzig herben Jahren\u00ab", "tokens": ["\u00bb", "seit", "zwan\u00b7zig", "her\u00b7ben", "Jah\u00b7ren", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "(die Post, versteht sich, mu\u00df", "tokens": ["(", "die", "Post", ",", "ver\u00b7steht", "sich", ",", "mu\u00df"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "ART", "NN", "$,", "VVFIN", "PRF", "$,", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr saures St\u00fcndchen fahren", "tokens": ["Ihr", "sau\u00b7res", "St\u00fcnd\u00b7chen", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hieher vom Erebus)", "tokens": ["Hie\u00b7her", "vom", "E\u00b7re\u00b7bus", ")"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbverschmachteten wir Arme", "tokens": ["\u00bb", "ver\u00b7schmach\u00b7te\u00b7ten", "wir", "Ar\u00b7me"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In bittrer Wassernot,", "tokens": ["In", "bit\u00b7trer", "Was\u00b7ser\u00b7not", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die H\u00f6ll kam in Alarme", "tokens": ["Die", "H\u00f6ll", "kam", "in", "Al\u00b7ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und foderte den Tod.", "tokens": ["Und", "fo\u00b7der\u00b7te", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den Styx kann man durchwaten,", "tokens": ["Den", "Styx", "kann", "man", "durch\u00b7wa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Lethe krebset man,", "tokens": ["Im", "Le\u00b7the", "kreb\u00b7set", "man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Freund Charon mag sich raten,", "tokens": ["Freund", "Cha\u00b7ron", "mag", "sich", "ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Schlamme liegt sein Kahn.", "tokens": ["Im", "Schlam\u00b7me", "liegt", "sein", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Keck springen schon die Tote", "tokens": ["Keck", "sprin\u00b7gen", "schon", "die", "To\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hin\u00fcber, jung und alt,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", ",", "jung", "und", "alt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schiffer kommt vom Brote", "tokens": ["Der", "Schif\u00b7fer", "kommt", "vom", "Bro\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und flucht die H\u00f6lle kalt.", "tokens": ["Und", "flucht", "die", "H\u00f6l\u00b7le", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "F\u00fcrst Minos schickt Spionen", "tokens": ["F\u00fcrst", "Mi\u00b7nos", "schickt", "Spi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nach allen Grenzen hin,", "tokens": ["Nach", "al\u00b7len", "Gren\u00b7zen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Teufel m\u00fcssen fronen,", "tokens": ["Die", "Teu\u00b7fel", "m\u00fcs\u00b7sen", "fro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm Kundschaft einzuziehn.", "tokens": ["Ihm", "Kund\u00b7schaft", "ein\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Juhe! Nun ists am Tage!", "tokens": ["Ju\u00b7he", "!", "Nun", "ists", "am", "Ta\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Erwischt das R\u00e4ubernest!", "tokens": ["Er\u00b7wischt", "das", "R\u00e4u\u00b7ber\u00b7nest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heraus zum Freudgelage!", "tokens": ["He\u00b7raus", "zum", "Freud\u00b7ge\u00b7la\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, H\u00f6lle, komm zum Fest!", "tokens": ["Komm", ",", "H\u00f6l\u00b7le", ",", "komm", "zum", "Fest", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein Schwarm Autoren sp\u00fckte", "tokens": ["Ein", "Schwarm", "Au\u00b7to\u00b7ren", "sp\u00fck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um des Cocytus Rand,", "tokens": ["Um", "des", "Co\u00b7cy\u00b7tus", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Tintenf\u00e4\u00dfchen schm\u00fcckte", "tokens": ["Ein", "Tin\u00b7ten\u00b7f\u00e4\u00df\u00b7chen", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ritterliche Hand,", "tokens": ["Die", "rit\u00b7ter\u00b7li\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Hier sch\u00f6pften sie, zum Wunder,", "tokens": ["Hier", "sch\u00f6pf\u00b7ten", "sie", ",", "zum", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Buben s\u00fc\u00dfen Wein", "tokens": ["Wie", "Bu\u00b7ben", "s\u00fc\u00b7\u00dfen", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In R\u00f6hren von Holunder,", "tokens": ["In", "R\u00f6h\u00b7ren", "von", "Ho\u00b7lun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Strom in Tonnen ein.", "tokens": ["Den", "Strom", "in", "Ton\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Husch! Eh sie sichs versahen!", "tokens": ["Husch", "!", "Eh", "sie", "sichs", "ver\u00b7sa\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schlingen \u00fcber sie! \u2013", "tokens": ["Die", "Schlin\u00b7gen", "\u00fc\u00b7ber", "sie", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man wird euch sch\u00f6n empfahen,", "tokens": ["Man", "wird", "euch", "sch\u00f6n", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt nur nach Sanssouci.", "tokens": ["Kommt", "nur", "nach", "Sans\u00b7sou\u00b7ci", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Schon wittert' sie der K\u00f6nig,", "tokens": ["Schon", "wit\u00b7tert'", "sie", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wetzte seinen Zahn,", "tokens": ["Und", "wetz\u00b7te", "sei\u00b7nen", "Zahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schnauzte drauf nicht wenig", "tokens": ["Und", "schnauz\u00b7te", "drauf", "nicht", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Delinquenten an.", "tokens": ["Die", "De\u00b7lin\u00b7quen\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Aha! sieht man die R\u00e4uber?", "tokens": ["A\u00b7ha", "!", "sieht", "man", "die", "R\u00e4u\u00b7ber", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wes Handwerks? Welches Lands?", "tokens": ["Wes", "Hand\u00b7werks", "?", "Wel\u00b7ches", "Lands", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u203a", "tokens": ["\u203a"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Da haben wir den Tanz!", "tokens": ["Da", "ha\u00b7ben", "wir", "den", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Schon h\u00e4tt ich Lust, gleichbalden", "tokens": ["Schon", "h\u00e4tt", "ich", "Lust", ",", "gleich\u00b7bal\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch, wie ihr geht und steht,", "tokens": ["Euch", ",", "wie", "ihr", "geht", "und", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beim Essen zu behalten,", "tokens": ["Beim", "Es\u00b7sen", "zu", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eh euch mein Schwager m\u00e4ht.", "tokens": ["Eh", "euch", "mein", "Schwa\u00b7ger", "m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.16": {"line.1": {"text": "Doch schw\u00f6r ichs hier beim Styxe,", "tokens": ["Doch", "schw\u00f6r", "ichs", "hier", "beim", "Sty\u00b7xe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den eure Brut bestahl!", "tokens": ["Den", "eu\u00b7re", "Brut", "be\u00b7stahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch Marder und euch F\u00fcchse", "tokens": ["Euch", "Mar\u00b7der", "und", "euch", "F\u00fcch\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwartet Schand und Qual!", "tokens": ["Er\u00b7war\u00b7tet", "Schand", "und", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Solange, bis er splittert,", "tokens": ["So\u00b7lan\u00b7ge", ",", "bis", "er", "split\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spaziert zum Born der Krug!", "tokens": ["Spa\u00b7ziert", "zum", "Born", "der", "Krug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was nur nach Dinten wittert,", "tokens": ["Was", "nur", "nach", "Din\u00b7ten", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entgelte den Betrug!", "tokens": ["Ent\u00b7gel\u00b7te", "den", "Be\u00b7trug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Herab mit ihren Daumen!", "tokens": ["Her\u00b7ab", "mit", "ih\u00b7ren", "Dau\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00dft meinen Hund heraus!", "tokens": ["La\u00dft", "mei\u00b7nen", "Hund", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon w\u00e4ssert ihm der Gaumen", "tokens": ["Schon", "w\u00e4s\u00b7sert", "ihm", "der", "Gau\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach einem solchen Schmaus.", "tokens": ["Nach", "ei\u00b7nem", "sol\u00b7chen", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Wie zuckten ihre Waden", "tokens": ["Wie", "zuck\u00b7ten", "ih\u00b7re", "Wa\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor dieses Bullen Zahn!", "tokens": ["Vor", "die\u00b7ses", "Bul\u00b7len", "Zahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schnalzen Seine Gnaden,", "tokens": ["Es", "schnal\u00b7zen", "Sei\u00b7ne", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Joli packte an.", "tokens": ["Und", "Jo\u00b7li", "pack\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Man schw\u00f6rt, da\u00df noch der Stumpen", "tokens": ["Man", "schw\u00f6rt", ",", "da\u00df", "noch", "der", "Stum\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich krampfigt eingedruckt,", "tokens": ["Sich", "kramp\u00b7figt", "ein\u00b7ge\u00b7druckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Lethe auszupumpen", "tokens": ["Den", "Le\u00b7the", "aus\u00b7zu\u00b7pum\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch gichterisch gezuckt.\u00ab", "tokens": ["Noch", "gich\u00b7te\u00b7risch", "ge\u00b7zuckt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Und nun, ihr guten Christen,", "tokens": ["Und", "nun", ",", "ihr", "gu\u00b7ten", "Chris\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beherziget den Traum!", "tokens": ["Be\u00b7her\u00b7zi\u00b7get", "den", "Traum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fragt ihr nach Journalisten,", "tokens": ["Fragt", "ihr", "nach", "Jour\u00b7na\u00b7lis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sucht nur ihren Daum'!", "tokens": ["So", "sucht", "nur", "ih\u00b7ren", "Daum'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Sie bergen oft die L\u00fccken,", "tokens": ["Sie", "ber\u00b7gen", "oft", "die", "L\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Jauner ohne Ohr", "tokens": ["Wie", "Jau\u00b7ner", "oh\u00b7ne", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie helfen mit Per\u00fccken, \u2013", "tokens": ["Sie", "hel\u00b7fen", "mit", "Pe\u00b7r\u00fc\u00b7cken", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Probatum! Gut davor!", "tokens": ["Pro\u00b7ba\u00b7tum", "!", "Gut", "da\u00b7vor", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "PAV", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.23": {"line.1": {"text": "Mir kam vor wenig Tagen,", "tokens": ["Mir", "kam", "vor", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie? fragt mich eben nicht,", "tokens": ["Wie", "?", "fragt", "mich", "e\u00b7ben", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Reich der ewgen Plagen", "tokens": ["Vom", "Reich", "der", "ew\u00b7gen", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zeitung zu Gesicht.", "tokens": ["Die", "Zei\u00b7tung", "zu", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Sonst frag ich diesem Essen,", "tokens": ["Sonst", "frag", "ich", "die\u00b7sem", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo noch kein Kopf zerbrach,", "tokens": ["Wo", "noch", "kein", "Kopf", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Freikorps unsrer Pressen,", "tokens": ["Dem", "Frei\u00b7korps", "uns\u00b7rer", "Pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie billig, wenig nach.", "tokens": ["Wie", "bil\u00b7lig", ",", "we\u00b7nig", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Doch eine Randgloss lockte", "tokens": ["Doch", "ei\u00b7ne", "Rand\u00b7gloss", "lock\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Itzt meinen F\u00fcrwitz an,", "tokens": ["Itzt", "mei\u00b7nen", "F\u00fcr\u00b7witz", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denkt! wie das Blut mir stockte,", "tokens": ["Denkt", "!", "wie", "das", "Blut", "mir", "stock\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ich das Blatt begann:", "tokens": ["Als", "ich", "das", "Blatt", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "\u00bbseit zwanzig herben Jahren\u00ab", "tokens": ["\u00bb", "seit", "zwan\u00b7zig", "her\u00b7ben", "Jah\u00b7ren", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "(die Post, versteht sich, mu\u00df", "tokens": ["(", "die", "Post", ",", "ver\u00b7steht", "sich", ",", "mu\u00df"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "ART", "NN", "$,", "VVFIN", "PRF", "$,", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr saures St\u00fcndchen fahren", "tokens": ["Ihr", "sau\u00b7res", "St\u00fcnd\u00b7chen", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hieher vom Erebus)", "tokens": ["Hie\u00b7her", "vom", "E\u00b7re\u00b7bus", ")"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "\u00bbverschmachteten wir Arme", "tokens": ["\u00bb", "ver\u00b7schmach\u00b7te\u00b7ten", "wir", "Ar\u00b7me"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In bittrer Wassernot,", "tokens": ["In", "bit\u00b7trer", "Was\u00b7ser\u00b7not", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die H\u00f6ll kam in Alarme", "tokens": ["Die", "H\u00f6ll", "kam", "in", "Al\u00b7ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und foderte den Tod.", "tokens": ["Und", "fo\u00b7der\u00b7te", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Den Styx kann man durchwaten,", "tokens": ["Den", "Styx", "kann", "man", "durch\u00b7wa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Lethe krebset man,", "tokens": ["Im", "Le\u00b7the", "kreb\u00b7set", "man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Freund Charon mag sich raten,", "tokens": ["Freund", "Cha\u00b7ron", "mag", "sich", "ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Schlamme liegt sein Kahn.", "tokens": ["Im", "Schlam\u00b7me", "liegt", "sein", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Keck springen schon die Tote", "tokens": ["Keck", "sprin\u00b7gen", "schon", "die", "To\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hin\u00fcber, jung und alt,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", ",", "jung", "und", "alt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schiffer kommt vom Brote", "tokens": ["Der", "Schif\u00b7fer", "kommt", "vom", "Bro\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und flucht die H\u00f6lle kalt.", "tokens": ["Und", "flucht", "die", "H\u00f6l\u00b7le", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "F\u00fcrst Minos schickt Spionen", "tokens": ["F\u00fcrst", "Mi\u00b7nos", "schickt", "Spi\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nach allen Grenzen hin,", "tokens": ["Nach", "al\u00b7len", "Gren\u00b7zen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Teufel m\u00fcssen fronen,", "tokens": ["Die", "Teu\u00b7fel", "m\u00fcs\u00b7sen", "fro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm Kundschaft einzuziehn.", "tokens": ["Ihm", "Kund\u00b7schaft", "ein\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Juhe! Nun ists am Tage!", "tokens": ["Ju\u00b7he", "!", "Nun", "ists", "am", "Ta\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Erwischt das R\u00e4ubernest!", "tokens": ["Er\u00b7wischt", "das", "R\u00e4u\u00b7ber\u00b7nest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heraus zum Freudgelage!", "tokens": ["He\u00b7raus", "zum", "Freud\u00b7ge\u00b7la\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, H\u00f6lle, komm zum Fest!", "tokens": ["Komm", ",", "H\u00f6l\u00b7le", ",", "komm", "zum", "Fest", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Ein Schwarm Autoren sp\u00fckte", "tokens": ["Ein", "Schwarm", "Au\u00b7to\u00b7ren", "sp\u00fck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um des Cocytus Rand,", "tokens": ["Um", "des", "Co\u00b7cy\u00b7tus", "Rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Tintenf\u00e4\u00dfchen schm\u00fcckte", "tokens": ["Ein", "Tin\u00b7ten\u00b7f\u00e4\u00df\u00b7chen", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ritterliche Hand,", "tokens": ["Die", "rit\u00b7ter\u00b7li\u00b7che", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Hier sch\u00f6pften sie, zum Wunder,", "tokens": ["Hier", "sch\u00f6pf\u00b7ten", "sie", ",", "zum", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Buben s\u00fc\u00dfen Wein", "tokens": ["Wie", "Bu\u00b7ben", "s\u00fc\u00b7\u00dfen", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In R\u00f6hren von Holunder,", "tokens": ["In", "R\u00f6h\u00b7ren", "von", "Ho\u00b7lun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Strom in Tonnen ein.", "tokens": ["Den", "Strom", "in", "Ton\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "Husch! Eh sie sichs versahen!", "tokens": ["Husch", "!", "Eh", "sie", "sichs", "ver\u00b7sa\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schlingen \u00fcber sie! \u2013", "tokens": ["Die", "Schlin\u00b7gen", "\u00fc\u00b7ber", "sie", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man wird euch sch\u00f6n empfahen,", "tokens": ["Man", "wird", "euch", "sch\u00f6n", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt nur nach Sanssouci.", "tokens": ["Kommt", "nur", "nach", "Sans\u00b7sou\u00b7ci", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.35": {"line.1": {"text": "Schon wittert' sie der K\u00f6nig,", "tokens": ["Schon", "wit\u00b7tert'", "sie", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wetzte seinen Zahn,", "tokens": ["Und", "wetz\u00b7te", "sei\u00b7nen", "Zahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schnauzte drauf nicht wenig", "tokens": ["Und", "schnauz\u00b7te", "drauf", "nicht", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Delinquenten an.", "tokens": ["Die", "De\u00b7lin\u00b7quen\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.36": {"line.1": {"text": "Aha! sieht man die R\u00e4uber?", "tokens": ["A\u00b7ha", "!", "sieht", "man", "die", "R\u00e4u\u00b7ber", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wes Handwerks? Welches Lands?", "tokens": ["Wes", "Hand\u00b7werks", "?", "Wel\u00b7ches", "Lands", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u203a", "tokens": ["\u203a"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Da haben wir den Tanz!", "tokens": ["Da", "ha\u00b7ben", "wir", "den", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.37": {"line.1": {"text": "Schon h\u00e4tt ich Lust, gleichbalden", "tokens": ["Schon", "h\u00e4tt", "ich", "Lust", ",", "gleich\u00b7bal\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch, wie ihr geht und steht,", "tokens": ["Euch", ",", "wie", "ihr", "geht", "und", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beim Essen zu behalten,", "tokens": ["Beim", "Es\u00b7sen", "zu", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eh euch mein Schwager m\u00e4ht.", "tokens": ["Eh", "euch", "mein", "Schwa\u00b7ger", "m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.38": {"line.1": {"text": "Doch schw\u00f6r ichs hier beim Styxe,", "tokens": ["Doch", "schw\u00f6r", "ichs", "hier", "beim", "Sty\u00b7xe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den eure Brut bestahl!", "tokens": ["Den", "eu\u00b7re", "Brut", "be\u00b7stahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch Marder und euch F\u00fcchse", "tokens": ["Euch", "Mar\u00b7der", "und", "euch", "F\u00fcch\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwartet Schand und Qual!", "tokens": ["Er\u00b7war\u00b7tet", "Schand", "und", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.39": {"line.1": {"text": "Solange, bis er splittert,", "tokens": ["So\u00b7lan\u00b7ge", ",", "bis", "er", "split\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spaziert zum Born der Krug!", "tokens": ["Spa\u00b7ziert", "zum", "Born", "der", "Krug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was nur nach Dinten wittert,", "tokens": ["Was", "nur", "nach", "Din\u00b7ten", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entgelte den Betrug!", "tokens": ["Ent\u00b7gel\u00b7te", "den", "Be\u00b7trug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.40": {"line.1": {"text": "Herab mit ihren Daumen!", "tokens": ["Her\u00b7ab", "mit", "ih\u00b7ren", "Dau\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00dft meinen Hund heraus!", "tokens": ["La\u00dft", "mei\u00b7nen", "Hund", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon w\u00e4ssert ihm der Gaumen", "tokens": ["Schon", "w\u00e4s\u00b7sert", "ihm", "der", "Gau\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach einem solchen Schmaus.", "tokens": ["Nach", "ei\u00b7nem", "sol\u00b7chen", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.41": {"line.1": {"text": "Wie zuckten ihre Waden", "tokens": ["Wie", "zuck\u00b7ten", "ih\u00b7re", "Wa\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor dieses Bullen Zahn!", "tokens": ["Vor", "die\u00b7ses", "Bul\u00b7len", "Zahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schnalzen Seine Gnaden,", "tokens": ["Es", "schnal\u00b7zen", "Sei\u00b7ne", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Joli packte an.", "tokens": ["Und", "Jo\u00b7li", "pack\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.42": {"line.1": {"text": "Man schw\u00f6rt, da\u00df noch der Stumpen", "tokens": ["Man", "schw\u00f6rt", ",", "da\u00df", "noch", "der", "Stum\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich krampfigt eingedruckt,", "tokens": ["Sich", "kramp\u00b7figt", "ein\u00b7ge\u00b7druckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Lethe auszupumpen", "tokens": ["Den", "Le\u00b7the", "aus\u00b7zu\u00b7pum\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch gichterisch gezuckt.\u00ab", "tokens": ["Noch", "gich\u00b7te\u00b7risch", "ge\u00b7zuckt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.43": {"line.1": {"text": "Und nun, ihr guten Christen,", "tokens": ["Und", "nun", ",", "ihr", "gu\u00b7ten", "Chris\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beherziget den Traum!", "tokens": ["Be\u00b7her\u00b7zi\u00b7get", "den", "Traum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fragt ihr nach Journalisten,", "tokens": ["Fragt", "ihr", "nach", "Jour\u00b7na\u00b7lis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sucht nur ihren Daum'!", "tokens": ["So", "sucht", "nur", "ih\u00b7ren", "Daum'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.44": {"line.1": {"text": "Sie bergen oft die L\u00fccken,", "tokens": ["Sie", "ber\u00b7gen", "oft", "die", "L\u00fc\u00b7cken",