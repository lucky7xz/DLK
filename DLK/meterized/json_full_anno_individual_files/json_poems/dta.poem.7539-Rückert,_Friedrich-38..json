{"dta.poem.7539": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein m\u00e4cht'ger K\u00f6nig sprach: Mehr als im Ueberwinden", "tokens": ["Ein", "m\u00e4cht'\u00b7ger", "K\u00f6\u00b7nig", "sprach", ":", "Mehr", "als", "im", "Ue\u00b7ber\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PIS", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Konnt' ich erst meine Macht ganz im Verzeihn empfinden.", "tokens": ["Konnt'", "ich", "erst", "mei\u00b7ne", "Macht", "ganz", "im", "Ver\u00b7zeihn", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der weise K\u00f6nig sprach: Scheu dich, den zu beleidigen,", "tokens": ["Der", "wei\u00b7se", "K\u00f6\u00b7nig", "sprach", ":", "Scheu", "dich", ",", "den", "zu", "be\u00b7lei\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "NN", "PPER", "$,", "ART", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Den Niemand gegen dich, als Gott nur, kann vertheidigen.", "tokens": ["Den", "Nie\u00b7mand", "ge\u00b7gen", "dich", ",", "als", "Gott", "nur", ",", "kann", "ver\u00b7thei\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "$,", "KOUS", "NN", "ADV", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Die Kr\u00e4nkung fremder Ehr' ist deiner W\u00fcrd' Entweihung,", "tokens": ["Die", "Kr\u00e4n\u00b7kung", "frem\u00b7der", "Ehr'", "ist", "dei\u00b7ner", "W\u00fcrd'", "Ent\u00b7wei\u00b7hung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn den Gekr\u00e4nkten mu\u00dft du bitten um Verzeihung.", "tokens": ["Denn", "den", "Ge\u00b7kr\u00e4nk\u00b7ten", "mu\u00dft", "du", "bit\u00b7ten", "um", "Ver\u00b7zei\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn dich der Zorn bef\u00e4llt im Stehn, so setz dich nieder,", "tokens": ["Wenn", "dich", "der", "Zorn", "be\u00b7f\u00e4llt", "im", "Stehn", ",", "so", "setz", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn im Sitzen, streck' aufs Lager aus die Glieder.", "tokens": ["Und", "wenn", "im", "Sit\u00b7zen", ",", "streck'", "aufs", "La\u00b7ger", "aus", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "La\u00df seyn, was du nicht h\u00f6rst! weil sichs von selbst versteht,", "tokens": ["La\u00df", "seyn", ",", "was", "du", "nicht", "h\u00f6rst", "!", "weil", "sichs", "von", "selbst", "ver\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VAINF", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$.", "KOUS", "PIS", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df, wer ins Antlitz lobt, auch hinterm R\u00fccken schm\u00e4ht.", "tokens": ["Da\u00df", ",", "wer", "ins", "Ant\u00b7litz", "lobt", ",", "auch", "hin\u00b7term", "R\u00fc\u00b7cken", "schm\u00e4ht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "APPRART", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein unbesonnen Wort, wo du es h\u00f6rest sprechen,", "tokens": ["Ein", "un\u00b7be\u00b7son\u00b7nen", "Wort", ",", "wo", "du", "es", "h\u00f6\u00b7rest", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thu alsob du nicht h\u00f6rst, so brauchst du's nicht zu r\u00e4chen.", "tokens": ["Thu", "al\u00b7sob", "du", "nicht", "h\u00f6rst", ",", "so", "brauchst", "du's", "nicht", "zu", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Derselbe sprach: Mein Reich liegt in der Sinne Schranken;", "tokens": ["Der\u00b7sel\u00b7be", "sprach", ":", "Mein", "Reich", "liegt", "in", "der", "Sin\u00b7ne", "Schran\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich richte nach der That, und nicht nach den Gedanken.", "tokens": ["Ich", "rich\u00b7te", "nach", "der", "That", ",", "und", "nicht", "nach", "den", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}