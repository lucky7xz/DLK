{"dta.poem.15690": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich, Unw\u00fcrdigen zu geben, nicht verdrie\u00dfen!", "tokens": ["La\u00df", "dich", ",", "Un\u00b7w\u00fcr\u00b7di\u00b7gen", "zu", "ge\u00b7ben", ",", "nicht", "ver\u00b7drie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "PTKZU", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Das ist ein Vorwand nur, um karg die Hand zu schlie\u00dfen.", "tokens": ["Das", "ist", "ein", "Vor\u00b7wand", "nur", ",", "um", "karg", "die", "Hand", "zu", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,", "KOUI", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Unw\u00fcrdig deiner Gab' ist keiner, ders bedarf;", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", "dei\u00b7ner", "Gab'", "ist", "kei\u00b7ner", ",", "ders", "be\u00b7darf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VAFIN", "PIS", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer ist, der, au\u00dfer Gott, ihn schuldig sprechen darf?", "tokens": ["Wer", "ist", ",", "der", ",", "au\u00b7\u00dfer", "Gott", ",", "ihn", "schul\u00b7dig", "spre\u00b7chen", "darf", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "$,", "APPR", "NN", "$,", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sprich lieber: Hat er sich verstrickt durch seine Schuld,", "tokens": ["Sprich", "lie\u00b7ber", ":", "Hat", "er", "sich", "ver\u00b7strickt", "durch", "sei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "VAFIN", "PPER", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will ihn nun durch mich entbinden Gottes Huld.", "tokens": ["So", "will", "ihn", "nun", "durch", "mich", "ent\u00b7bin\u00b7den", "Got\u00b7tes", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Auch sage nicht: was hilfts da\u00df ich ihm helf' empor?", "tokens": ["Auch", "sa\u00b7ge", "nicht", ":", "was", "hilfts", "da\u00df", "ich", "ihm", "hel\u00b7f'", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$.", "PWS", "VVFIN", "KOUS", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Er liegt im Augenblick so elend wie zuvor.", "tokens": ["Er", "liegt", "im", "Au\u00b7gen\u00b7blick", "so", "e\u00b7lend", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Erlieg' im Augenblick er wieder dem Geschick,", "tokens": ["Er\u00b7lieg'", "im", "Au\u00b7gen\u00b7blick", "er", "wie\u00b7der", "dem", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufhalfest du ihm doch f\u00fcr einen Augenblick!", "tokens": ["Auf\u00b7hal\u00b7fest", "du", "ihm", "doch", "f\u00fcr", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}