{"dta.poem.8875": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Mit s\u00fc\u00dfem Nichts die Tage zu vertr\u00e4umen,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Nichts", "die", "Ta\u00b7ge", "zu", "ver\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey jedem fl\u00fcchtigen Genu\u00df zu s\u00e4umen,", "tokens": ["Bey", "je\u00b7dem", "fl\u00fcch\u00b7ti\u00b7gen", "Ge\u00b7nu\u00df", "zu", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Gro\u00dfen sich erg\u00f6tzend und Geringen:", "tokens": ["Am", "Gro\u00b7\u00dfen", "sich", "er\u00b7g\u00f6t\u00b7zend", "und", "Ge\u00b7rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus edlen Dichtern einen Vers zu singen,", "tokens": ["Aus", "ed\u00b7len", "Dich\u00b7tern", "ei\u00b7nen", "Vers", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gestreckt in's Gras, wo laute Quellen sch\u00e4umen,", "tokens": ["Ge\u00b7streckt", "in's", "Gras", ",", "wo", "lau\u00b7te", "Quel\u00b7len", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "PWAV", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Rosenhecken, unter Lindenb\u00e4umen", "tokens": ["An", "Ro\u00b7sen\u00b7he\u00b7cken", ",", "un\u00b7ter", "Lin\u00b7den\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben unbesorgt dahin zu bringen.", "tokens": ["Das", "Le\u00b7ben", "un\u00b7be\u00b7sorgt", "da\u00b7hin", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Mai die Stirn mit jungem Laub zu kr\u00f6nen,", "tokens": ["Im", "Mai", "die", "Stirn", "mit", "jun\u00b7gem", "Laub", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die lauen N\u00e4chte, bis es wieder taget,", "tokens": ["Die", "lau\u00b7en", "N\u00e4ch\u00b7te", ",", "bis", "es", "wie\u00b7der", "ta\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Weingenu\u00df und Liebe zu versch\u00f6nen:", "tokens": ["Durch", "Wein\u00b7ge\u00b7nu\u00df", "und", "Lie\u00b7be", "zu", "ver\u00b7sch\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dies ist, und wenn mich auch darob verklaget", "tokens": ["Dies", "ist", ",", "und", "wenn", "mich", "auch", "da\u00b7rob", "ver\u00b7kla\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "KON", "KOUS", "PPER", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sittenrichter, der es will verp\u00f6nen,", "tokens": ["Ein", "Sit\u00b7ten\u00b7rich\u00b7ter", ",", "der", "es", "will", "ver\u00b7p\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Einzige, was meinem Sinn behaget.", "tokens": ["Das", "Ein\u00b7zi\u00b7ge", ",", "was", "mei\u00b7nem", "Sinn", "be\u00b7ha\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}