{"textgrid.poem.36634": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Fr\u00f6sche hatten einen K\u00f6nig!", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00f6sche hatten einen K\u00f6nig!", "tokens": ["Die", "Fr\u00f6\u00b7sche", "hat\u00b7ten", "ei\u00b7nen", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind ein gro\u00dfes Volk! Er hat ", "tokens": ["Wir", "sind", "ein", "gro\u00b7\u00dfes", "Volk", "!", "Er", "hat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verstandesf\u00e4higkeit, und seine sch\u00f6ne Frau", "tokens": ["Ver\u00b7stan\u00b7des\u00b7f\u00e4\u00b7hig\u00b7keit", ",", "und", "sei\u00b7ne", "sch\u00f6\u00b7ne", "Frau"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist keine K\u00f6nigin, wie wir sie haben wollen", "tokens": ["Ist", "kei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ",", "wie", "wir", "sie", "ha\u00b7ben", "wol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "PPER", "VAINF", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seht! Sie beherscht den Mann, und ist ihm viel zu schlau,", "tokens": ["Seht", "!", "Sie", "be\u00b7herscht", "den", "Mann", ",", "und", "ist", "ihm", "viel", "zu", "schlau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie taugen beyde nichts, sie sollen", "tokens": ["Sie", "tau\u00b7gen", "bey\u00b7de", "nichts", ",", "sie", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PIS", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Herunter von dem Thron, sie sitzen zu bequem!", "tokens": ["Her\u00b7un\u00b7ter", "von", "dem", "Thron", ",", "sie", "sit\u00b7zen", "zu", "be\u00b7quem", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und kurz: Uns Fr\u00f6schen ist kein K\u00f6nig angenehm,", "tokens": ["Und", "kurz", ":", "Uns", "Fr\u00f6\u00b7schen", "ist", "kein", "K\u00f6\u00b7nig", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir k\u00f6nnen wohl uns selbst regieren", "tokens": ["Wir", "k\u00f6n\u00b7nen", "wohl", "uns", "selbst", "re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wir brauchen keine Majest\u00e4t!", "tokens": ["Wir", "brau\u00b7chen", "kei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wir alle sind uns selbst die Majest\u00e4t! Wir haben", "tokens": ["Wir", "al\u00b7le", "sind", "uns", "selbst", "die", "Ma\u00b7jes\u00b7t\u00e4t", "!", "Wir", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PIS", "VAFIN", "PPER", "ADV", "ART", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Frosches edles Recht begraben,", "tokens": ["Des", "Fro\u00b7sches", "ed\u00b7les", "Recht", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "La\u00dfts, sprach ein weiser Frosch, la\u00dfts wieder auferstehn!", "tokens": ["La\u00dfts", ",", "sprach", "ein", "wei\u00b7ser", "Frosch", ",", "la\u00dfts", "wie\u00b7der", "auf\u00b7er\u00b7stehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ja! quakten Tausende, Ja! ja! das mu\u00df geschehn!", "tokens": ["Ja", "!", "quak\u00b7ten", "Tau\u00b7sen\u00b7de", ",", "Ja", "!", "ja", "!", "das", "mu\u00df", "ge\u00b7schehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJA", "NN", "$,", "PTKANT", "$.", "PTKANT", "$.", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gequakt, gethan! die Quaker fiengen", "tokens": ["Ge\u00b7quakt", ",", "ge\u00b7than", "!", "die", "Qua\u00b7ker", "fi\u00b7en\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Den K\u00f6nig, sperrten ihn in einen Kefig ein!", "tokens": ["Den", "K\u00f6\u00b7nig", ",", "sperr\u00b7ten", "ihn", "in", "ei\u00b7nen", "Ke\u00b7fig", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Koaxte man, ermordete den besten", "tokens": ["Ko\u00b7ax\u00b7te", "man", ",", "er\u00b7mor\u00b7de\u00b7te", "den", "bes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PIS", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der K\u00f6nige, beging die gr\u00f6\u00dften", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7ge", ",", "be\u00b7ging", "die", "gr\u00f6\u00df\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Schandthaten! Tausend K\u00f6nige", "tokens": ["Schandt\u00b7ha\u00b7ten", "!", "Tau\u00b7send", "K\u00f6\u00b7ni\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Regierten, tausend! Ach! und Weh!", "tokens": ["Re\u00b7gier\u00b7ten", ",", "tau\u00b7send", "!", "Ach", "!", "und", "Weh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "$.", "ITJ", "$.", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Erscholl umher im K\u00f6nigreiche!", "tokens": ["Er\u00b7scholl", "um\u00b7her", "im", "K\u00f6\u00b7nig\u00b7rei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Man sah zehntausend d\u00fcnne B\u00e4uche,", "tokens": ["Man", "sah", "zehn\u00b7tau\u00b7send", "d\u00fcn\u00b7ne", "B\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Hei\u00dfhunger \u00fcbte die Gewalt", "tokens": ["Hei\u00df\u00b7hun\u00b7ger", "\u00fcb\u00b7te", "die", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die Gott der Herr ihm gab! das K\u00f6nigreich verarmte", "tokens": ["Die", "Gott", "der", "Herr", "ihm", "gab", "!", "das", "K\u00f6\u00b7nig\u00b7reich", "ver\u00b7arm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Vier Jahre noch nicht alt", "tokens": ["Vier", "Jah\u00b7re", "noch", "nicht", "alt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "War's aufgerieben! Gott erbarmte", "tokens": ["Wa\u00b7r's", "auf\u00b7ge\u00b7rie\u00b7ben", "!", "Gott", "er\u00b7barm\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Sich gn\u00e4digst seiner, gab den ", "tokens": ["Sich", "gn\u00e4\u00b7digst", "sei\u00b7ner", ",", "gab", "den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "VVFIN", "PPOSAT", "$,", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Dem irrgef\u00fchrten Volke wieder!", "tokens": ["Dem", "irr\u00b7ge\u00b7f\u00fchr\u00b7ten", "Vol\u00b7ke", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Man sang: Es wird schon gehn", "tokens": ["Man", "sang", ":", "Es", "wird", "schon", "gehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Wie w\u00fctend nun nicht mehr, sang neue Freudenlieder,", "tokens": ["Wie", "w\u00fc\u00b7tend", "nun", "nicht", "mehr", ",", "sang", "neu\u00b7e", "Freu\u00b7den\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PTKNEG", "ADV", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "(die Fr\u00f6sche schreiben auch, wie wir, Gedanken nieder,)", "tokens": ["(", "die", "Fr\u00f6\u00b7sche", "schrei\u00b7ben", "auch", ",", "wie", "wir", ",", "Ge\u00b7dan\u00b7ken", "nie\u00b7der", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$,", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was nun nicht mehr verborgen blieb!", "tokens": ["Was", "nun", "nicht", "mehr", "ver\u00b7bor\u00b7gen", "blieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun ist euch wohl, ihr Herrn! ihr Fr\u00f6sche! nun ins Freie!", "tokens": ["Nun", "ist", "euch", "wohl", ",", "ihr", "Herrn", "!", "ihr", "Fr\u00f6\u00b7sche", "!", "nun", "ins", "Frei\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von tausend K\u00f6nigen nicht mehr ins Netz gek\u00f6rnt!", "tokens": ["Von", "tau\u00b7send", "K\u00f6\u00b7ni\u00b7gen", "nicht", "mehr", "ins", "Netz", "ge\u00b7k\u00f6rnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Koaxt so viel ihr wollt, nun wieder, ach! und lernt;", "tokens": ["Ko\u00b7axt", "so", "viel", "ihr", "wollt", ",", "nun", "wie\u00b7der", ",", "ach", "!", "und", "lernt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPER", "VMFIN", "$,", "ADV", "ADV", "$,", "ITJ", "$.", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Fr\u00f6sche hatten einen K\u00f6nig!", "tokens": ["Die", "Fr\u00f6\u00b7sche", "hat\u00b7ten", "ei\u00b7nen", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["AR