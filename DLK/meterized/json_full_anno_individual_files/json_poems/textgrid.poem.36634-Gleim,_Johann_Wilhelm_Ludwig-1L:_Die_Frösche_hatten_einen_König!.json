$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Hei\u00dfhunger \u00fcbte die Gewalt", "tokens": ["Hei\u00df\u00b7hun\u00b7ger", "\u00fcb\u00b7te", "die", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die Gott der Herr ihm gab! das K\u00f6nigreich verarmte", "tokens": ["Die", "Gott", "der", "Herr", "ihm", "gab", "!", "das", "K\u00f6\u00b7nig\u00b7reich", "ver\u00b7arm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Vier Jahre noch nicht alt", "tokens": ["Vier", "Jah\u00b7re", "noch", "nicht", "alt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "War's aufgerieben! Gott erbarmte", "tokens": ["Wa\u00b7r's", "auf\u00b7ge\u00b7rie\u00b7ben", "!", "Gott", "er\u00b7barm\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Sich gn\u00e4digst seiner, gab den ", "tokens": ["Sich", "gn\u00e4\u00b7digst", "sei\u00b7ner", ",", "gab", "den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "VVFIN", "PPOSAT", "$,", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Dem irrgef\u00fchrten Volke wieder!", "tokens": ["Dem", "irr\u00b7ge\u00b7f\u00fchr\u00b7ten", "Vol\u00b7ke", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Man sang: Es wird schon g