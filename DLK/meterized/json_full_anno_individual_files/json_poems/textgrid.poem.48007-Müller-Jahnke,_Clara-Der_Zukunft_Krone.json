{"textgrid.poem.48007": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Der Zukunft Krone", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Mann der Arbeit \u2013 und ob er schwingt", "tokens": ["Dem", "Mann", "der", "Ar\u00b7beit", "\u2013", "und", "ob", "er", "schwingt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "KON", "KOUS", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die Axt in der nervigen Rechten,", "tokens": ["die", "Axt", "in", "der", "ner\u00b7vi\u00b7gen", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und ob er das Gold aus der Erde ringt", "tokens": ["und", "ob", "er", "das", "Gold", "aus", "der", "Er\u00b7de", "ringt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "aus des Bergwerks d\u00e4mmernden Sch\u00e4chten,", "tokens": ["aus", "des", "Berg\u00b7werks", "d\u00e4m\u00b7mern\u00b7den", "Sch\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "ob er lehrt und schafft und die Feder h\u00e4lt", "tokens": ["ob", "er", "lehrt", "und", "schafft", "und", "die", "Fe\u00b7der", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "K