{"textgrid.poem.49671": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das freudige Ereignis", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den vierten Juli um neun Uhr f\u00fcn'zehn", "tokens": ["Den", "vier\u00b7ten", "Ju\u00b7li", "um", "neun", "Uhr", "f\u00fcn'\u00b7zehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erhielten wir einen neuen Hohenzollernprinzen.", "tokens": ["Er\u00b7hiel\u00b7ten", "wir", "ei\u00b7nen", "neu\u00b7en", "Ho\u00b7hen\u00b7zol\u00b7lern\u00b7prin\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er besitzt bereits alle herrlichen Gaben,", "tokens": ["Er", "be\u00b7sitzt", "be\u00b7reits", "al\u00b7le", "herr\u00b7li\u00b7chen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Welche sie in dieser Familie immer haben.", "tokens": ["Wel\u00b7che", "sie", "in", "die\u00b7ser", "Fa\u00b7mi\u00b7lie", "im\u00b7mer", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Seine Laufbahn ist ihm genau vorgezeichnet,", "tokens": ["Sei\u00b7ne", "Lauf\u00b7bahn", "ist", "ihm", "ge\u00b7nau", "vor\u00b7ge\u00b7zeich\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Es geht nach der Uhr, was sich mit ihm ereignet.", "tokens": ["Es", "geht", "nach", "der", "Uhr", ",", "was", "sich", "mit", "ihm", "er\u00b7eig\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er kriegt infolge seiner gro\u00dfen Vorz\u00fcge", "tokens": ["Er", "kriegt", "in\u00b7fol\u00b7ge", "sei\u00b7ner", "gro\u00b7\u00dfen", "Vor\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ein Patent als Leutenant in die Wiege.", "tokens": ["Ein", "Pa\u00b7tent", "als", "Leu\u00b7ten\u00b7ant", "in", "die", "Wie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.3": {"line.1": {"text": "Er wird dergestalten auch mit den Jahren", "tokens": ["Er", "wird", "der\u00b7ge\u00b7stal\u00b7ten", "auch", "mit", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ganz fr\u00f6hlich im Avanxemang fortfahren,", "tokens": ["Ganz", "fr\u00f6h\u00b7lich", "im", "A\u00b7van\u00b7xe\u00b7mang", "fort\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er braucht die Karriere \u00fcberhaupt blo\u00df zu erleben", "tokens": ["Er", "braucht", "die", "Kar\u00b7rie\u00b7re", "\u00fc\u00b7ber\u00b7haupt", "blo\u00df", "zu", "er\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sich keine M\u00fche nicht dabei zu geben.", "tokens": ["Und", "sich", "kei\u00b7ne", "M\u00fc\u00b7he", "nicht", "da\u00b7bei", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PIAT", "NN", "PTKNEG", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Ingleichen wird er einstens zu Bonn studieren", "tokens": ["In\u00b7glei\u00b7chen", "wird", "er", "eins\u00b7tens", "zu", "Bonn", "stu\u00b7die\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "NE", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sich als Borusse dortselbsten habilitieren,", "tokens": ["Und", "sich", "als", "Bo\u00b7rus\u00b7se", "dort\u00b7selbs\u00b7ten", "ha\u00b7bi\u00b7li\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "KOUS", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn im ewigen Kreislauf der Hohenzollerntaten", "tokens": ["Denn", "im", "e\u00b7wi\u00b7gen", "Kreis\u00b7lauf", "der", "Ho\u00b7hen\u00b7zol\u00b7lern\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN"], "me