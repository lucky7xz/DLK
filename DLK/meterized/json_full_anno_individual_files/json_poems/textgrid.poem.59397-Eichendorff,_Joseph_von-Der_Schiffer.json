{"textgrid.poem.59397": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schiffer", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00fcfte linde f\u00e4cheln,", "tokens": ["Die", "L\u00fcf\u00b7te", "lin\u00b7de", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus stillen Meeres Schaum", "tokens": ["Aus", "stil\u00b7len", "Mee\u00b7res", "Schaum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sirenen tauchend l\u00e4cheln,", "tokens": ["Si\u00b7re\u00b7nen", "tau\u00b7chend", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Schiffer liegt im Traum.", "tokens": ["Der", "Schif\u00b7fer", "liegt", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da fa\u00dft der Sturm die Wellen,", "tokens": ["Da", "fa\u00dft", "der", "Sturm", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchw\u00fchlt die Einsamkeit:", "tokens": ["Durch\u00b7w\u00fchlt", "die", "Ein\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wacht auf, ihr Traumgesellen,", "tokens": ["Wacht", "auf", ",", "ihr", "Traum\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun ist's nicht Schlafenszeit! \u2013", "tokens": ["Nun", "ist's", "nicht", "Schla\u00b7fens\u00b7zeit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In jenen stillen Tagen", "tokens": ["In", "je\u00b7nen", "stil\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie war ich stolz und klug,", "tokens": ["Wie", "war", "ich", "stolz", "und", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In sichern Gl\u00fccks Behagen", "tokens": ["In", "si\u00b7chern", "Gl\u00fccks", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mir selber gut genug.", "tokens": ["Mir", "sel\u00b7ber", "gut", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du hast das Gl\u00fcck zerschlagen;", "tokens": ["Du", "hast", "das", "Gl\u00fcck", "zer\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimm wieder, was du gabst,", "tokens": ["Nimm", "wie\u00b7der", ",", "was", "du", "gabst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schweig und will nicht klagen,", "tokens": ["Ich", "schweig", "und", "will", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt wei\u00df ich, wie du labst.", "tokens": ["Jetzt", "wei\u00df", "ich", ",", "wie", "du", "labst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das sind die m\u00e4cht'gen St\u00fcrme,", "tokens": ["Das", "sind", "die", "m\u00e4cht'\u00b7gen", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wecken, was da ruht,", "tokens": ["Die", "we\u00b7cken", ",", "was", "da", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sinken Land und T\u00fcrme", "tokens": ["Es", "sin\u00b7ken", "Land", "und", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allm\u00e4hlich in die Flut.", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "in", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Kein Meerweib will sich zeigen,", "tokens": ["Kein", "Meer\u00b7weib", "will", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Laut mehr langt zu mir,", "tokens": ["Kein", "Laut", "mehr", "langt", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem weiten Schweigen", "tokens": ["Und", "in", "dem", "wei\u00b7ten", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steh ich allein mit dir.", "tokens": ["Steh", "ich", "al\u00b7lein", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O f\u00fchre an den Riffen", "tokens": ["O", "f\u00fch\u00b7re", "an", "den", "Rif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allm\u00e4chtig deine Hand,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin wir alle schiffen,", "tokens": ["Wo\u00b7hin", "wir", "al\u00b7le", "schif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns zu dem Heimatstr