{"textgrid.poem.63324": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich liebt kein Mensch. Ich sitze hier beim Tee.", "tokens": ["Mich", "liebt", "kein", "Mensch", ".", "Ich", "sit\u00b7ze", "hier", "beim", "Tee", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es schmerzt das Herz, die Niere tut mir weh.", "tokens": ["Es", "schmerzt", "das", "Herz", ",", "die", "Nie\u00b7re", "tut", "mir", "weh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00e4dchen, ", "tokens": ["Die", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sie sind mit gro\u00dfer Vorsicht zu genie\u00dfen.", "tokens": ["Sie", "sind", "mit", "gro\u00b7\u00dfer", "Vor\u00b7sicht", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie stellen mit des Abenteurers Buntheit", "tokens": ["Sie", "stel\u00b7len", "mit", "des", "A\u00b7bent\u00b7eu\u00b7rers", "Bunt\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anforderung an unsre Gesundheit.", "tokens": ["An\u00b7for\u00b7de\u00b7rung", "an", "uns\u00b7re", "Ge\u00b7sund\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die ist mir heilig. Etwas andres nicht.", "tokens": ["Die", "ist", "mir", "hei\u00b7lig", ".", "Et\u00b7was", "and\u00b7res", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$.", "ADV", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Mensch, kein Tier, kein Stern und kein Gedicht.", "tokens": ["Kein", "Mensch", ",", "kein", "Tier", ",", "kein", "Stern", "und", "kein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ich hier Verse reimend niederschreibe,", "tokens": ["Wenn", "ich", "hier", "Ver\u00b7se", "rei\u00b7mend", "nie\u00b7der\u00b7schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschieht es nur zu meinem Zeitvertreibe.", "tokens": ["Ge\u00b7schieht", "es", "nur", "zu", "mei\u00b7nem", "Zeit\u00b7ver\u00b7trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Man glaube nicht an Absicht oder Zweck.", "tokens": ["Man", "glau\u00b7be", "nicht", "an", "Ab\u00b7sicht", "o\u00b7der", "Zweck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin ein hirnlich infizierter Dreck.", "tokens": ["Ich", "bin", "ein", "hirn\u00b7lich", "in\u00b7fi\u00b7zier\u00b7ter", "Dreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der fiel von einem Pferd, das fern enttrabt.", "tokens": ["Der", "fiel", "von", "ei\u00b7nem", "Pferd", ",", "das", "fern", "ent\u00b7trabt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich werde weder gern noch sonst gehabt.", "tokens": ["Ich", "wer\u00b7de", "we\u00b7der", "gern", "noch", "sonst", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "ADV", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man sieht durch mich hindurch. Man geht an mir vorbei.", "tokens": ["Man", "sieht", "durch", "mich", "hin\u00b7durch", ".", "Man", "geht", "an", "mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "PAV", "$.", "PIS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und niemand h\u00f6rt des Stummen Klageschrei.", "tokens": ["Und", "nie\u00b7mand", "h\u00f6rt", "des", "Stum\u00b7men"