{"dta.poem.9685": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Rilpum.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rjlpus wolt ein frauenzimmer gleich hin k\u00fcssen auf den", "tokens": ["Rjlpus", "wolt", "ein", "frau\u00b7en\u00b7zim\u00b7mer", "gleich", "hin", "k\u00fcs\u00b7sen", "auf", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mund/", "tokens": ["mund", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Selbe gab ihm h\u00f6nisch antwort/ sprach: di\u00df ist ihm nicht gesund/", "tokens": ["Sel\u00b7be", "gab", "ihm", "h\u00f6\u00b7nisch", "ant\u00b7wort", "/", "sprach", ":", "di\u00df", "ist", "ihm", "nicht", "ge\u00b7sund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "NN", "$(", "VVFIN", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und f\u00fcr andre schon bestellt. Sein gerichte das ist hinden/", "tokens": ["Und", "f\u00fcr", "and\u00b7re", "schon", "be\u00b7stellt", ".", "Sein", "ge\u00b7rich\u00b7te", "das", "ist", "hin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADV", "VVPP", "$.", "PPOSAT", "ADJA", "PDS", "VAFIN", "ADV", "$("], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Er wird eben dieses fleisch/ doch im andern sode finden.", "tokens": ["Er", "wird", "e\u00b7ben", "die\u00b7ses", "fleisch", "/", "doch", "im", "an\u00b7dern", "so\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PDAT", "NN", "$(", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}