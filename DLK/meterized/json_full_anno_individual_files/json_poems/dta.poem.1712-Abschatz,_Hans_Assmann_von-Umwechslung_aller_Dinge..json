{"dta.poem.1712": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Am Bober ist der Ruhm der deutschen Reim' entsprungen/", "tokens": ["Am", "Bo\u00b7ber", "ist", "der", "Ruhm", "der", "deut\u00b7schen", "Reim'", "ent\u00b7sprun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald wurden Sachse/ Franck und andre Nachbarn wach/", "tokens": ["Bald", "wur\u00b7den", "Sach\u00b7se", "/", "Franck", "und", "and\u00b7re", "Nach\u00b7barn", "wach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$(", "NN", "KON", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um unsern Oder-Strom hat mancher Schwan gesungen/", "tokens": ["Um", "un\u00b7sern", "O\u00b7der\u00b7Strom", "hat", "man\u00b7cher", "Schwan", "ge\u00b7sun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hin und her entstand ein Castalinnen-Bach.", "tokens": ["Und", "hin", "und", "her", "ent\u00b7stand", "ein", "Ca\u00b7sta\u00b7lin\u00b7nen\u00b7Bach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun k\u00fchne Helden-Faust halb Grichen-Land bezwungen/", "tokens": ["Nun", "k\u00fch\u00b7ne", "Hel\u00b7den\u00b7Faust", "halb", "Gri\u00b7chen\u00b7Land", "be\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und selbst besichtigt hat der Musen Schlaffgemach/", "tokens": ["Und", "selbst", "be\u00b7sich\u00b7tigt", "hat", "der", "Mu\u00b7sen", "Schlaff\u00b7ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist auch die Dichter-Kunst in Norden durchgedrungen/", "tokens": ["Ist", "auch", "die", "Dich\u00b7ter\u00b7Kunst", "in", "Nor\u00b7den", "durch\u00b7ge\u00b7drun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihr-verwandtes Blutt gibt keinem Opiz nach.", "tokens": ["Und", "ih\u00b7rver\u00b7wand\u00b7tes", "Blutt", "gibt", "kei\u00b7nem", "O\u00b7piz", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man sucht in Morgenland vergebens Amazonen/", "tokens": ["Man", "sucht", "in", "Mor\u00b7gen\u00b7land", "ver\u00b7ge\u00b7bens", "A\u00b7maz\u00b7o\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und mi\u00dft der Mitternacht nur Ei\u00df und K\u00e4lte bey/", "tokens": ["Und", "mi\u00dft", "der", "Mit\u00b7ter\u00b7nacht", "nur", "Ei\u00df", "und", "K\u00e4l\u00b7te", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "NN", "KON", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df Feuer/ Geist und Krafft allda mit Hauffen wohnen/", "tokens": ["Da\u00df", "Feu\u00b7er", "/", "Geist", "und", "Krafft", "all\u00b7da", "mit", "Hauf\u00b7fen", "woh\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zeigt Auge/ Schrifft und Mund: Ich sage Zweiffels-frey:", "tokens": ["Zeigt", "Au\u00b7ge", "/", "Schrifft", "und", "Mund", ":", "Ich", "sa\u00b7ge", "Zweif\u00b7fels\u00b7frey", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Norden zeugt Magnet/ und dieser zeucht die Hertzen:", "tokens": ["Der", "Nor\u00b7den", "zeugt", "Mag\u00b7net", "/", "und", "die\u00b7ser", "zeucht", "die", "Hert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$(", "KON", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Di\u00df zeugen neben mir Amenens Hand und Kertzen.", "tokens": ["Di\u00df", "zeu\u00b7gen", "ne\u00b7ben", "mir", "A\u00b7me\u00b7nens", "Hand", "und", "Kert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}