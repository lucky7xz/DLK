{"textgrid.poem.61124": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1. Das M\u00e4dchen im Hades", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie gl\u00fccklich sind die gr\u00fcnen Felder,", "tokens": ["O", "wie", "gl\u00fcck\u00b7lich", "sind", "die", "gr\u00fc\u00b7nen", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O wie gl\u00fccklich sind die hohen Berge,", "tokens": ["O", "wie", "gl\u00fcck\u00b7lich", "sind", "die", "ho\u00b7hen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welche nimmermehr den Hades schauen!", "tokens": ["Wel\u00b7che", "nim\u00b7mer\u00b7mehr", "den", "Ha\u00b7des", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kommt der Winter, deckt er sie mit Reif zu", "tokens": ["Kommt", "der", "Win\u00b7ter", ",", "deckt", "er", "sie", "mit", "Reif", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKZU"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mit dichtem flockigen Gest\u00f6ber;", "tokens": ["Und", "mit", "dich\u00b7tem", "flo\u00b7cki\u00b7gen", "Ge\u00b7st\u00f6\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Kommt der Fr\u00fchling, gr\u00fcnen sie aufs neue,", "tokens": ["Kommt", "der", "Fr\u00fch\u00b7ling", ",", "gr\u00fc\u00b7nen", "sie", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Tragen Blumen, tragen w\u00fcrz'ge Kr\u00e4uter,", "tokens": ["Tra\u00b7gen", "Blu\u00b7men", ",", "tra\u00b7gen", "w\u00fcrz'\u00b7ge", "Kr\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und der Sonnenschimmer schl\u00e4ft auf ihnen;", "tokens": ["Und", "der", "Son\u00b7nen\u00b7schim\u00b7mer", "schl\u00e4ft", "auf", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Aber nimmer brauchen sie dort unten", "tokens": ["A\u00b7ber", "nim\u00b7mer", "brau\u00b7chen", "sie", "dort", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Jene tr\u00fcbe Dunkelheit zu f\u00fcrchten.", "tokens": ["Je\u00b7ne", "tr\u00fc\u00b7be", "Dun\u00b7kel\u00b7heit", "zu", "f\u00fcrch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Hatten sich drei Riesen einst verschworen,", "tokens": ["Hat\u00b7ten", "sich", "drei", "Rie\u00b7sen", "einst", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "CARD", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In das Reich der Schatten einzubrechen.", "tokens": ["In", "das", "Reich", "der", "Schat\u00b7ten", "ein\u00b7zu\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stiegen sie hinab die dunklen Pfade,", "tokens": ["Stie\u00b7gen", "sie", "hin\u00b7ab", "die", "dunk\u00b7len", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wanderten drei Tage und drei N\u00e4chte,", "tokens": ["Wan\u00b7der\u00b7ten", "drei", "Ta\u00b7ge", "und", "drei", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "KON", "CARD", "NN", "$,"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Kamen endlich in das Reich der Toten.", "tokens": ["Ka\u00b7men", "end\u00b7lich", "in", "das", "Reich", "der", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie sie alles dort erforschet hatten,", "tokens": ["Wie", "sie", "al\u00b7les", "dort", "er\u00b7for\u00b7schet", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wollten sie zur\u00fcck zum Lichte kehren.", "tokens": ["Woll\u00b7ten", "sie", "zu\u00b7r\u00fcck", "zum", "Lich\u00b7te", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Trat zu ihnen da ein sch\u00f6nes M\u00e4dchen,", "tokens": ["Trat", "zu", "ih\u00b7nen", "da", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Blond von Haaren, aber bla\u00df von Wangen,", "tokens": ["Blond", "von", "Haa\u00b7ren", ",", "a\u00b7ber", "bla\u00df", "von", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Sprach die Riesen an mit sanfter Stimme:", "tokens": ["Sprach", "die", "Rie\u00b7sen", "an", "mit", "sanf\u00b7ter", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "\u00bbnehmt mich mit hinauf, ihr lieben Riesen!", "tokens": ["\u00bb", "nehmt", "mich", "mit", "hin\u00b7auf", ",", "ihr", "lie\u00b7ben", "Rie\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "APPR", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "M\u00f6chte gern einmal die Sonne schauen", "tokens": ["M\u00f6ch\u00b7te", "gern", "ein\u00b7mal", "die", "Son\u00b7ne", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und die roten Bl\u00fcmlein auf dem Felde.\u00ab", "tokens": ["Und", "die", "ro\u00b7ten", "Bl\u00fcm\u00b7lein", "auf", "dem", "Fel\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Drauf versetzten die gewalt'gen Riesen:", "tokens": ["Drauf", "ver\u00b7setz\u00b7ten", "die", "ge\u00b7walt'\u00b7gen", "Rie\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "\u00bbdeine seidenen Gew\u00e4nder rauschen,", "tokens": ["\u00bb", "dei\u00b7ne", "sei\u00b7de\u00b7nen", "Ge\u00b7w\u00e4n\u00b7der", "rau\u00b7schen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Deine langen blonden Locken fl\u00fcstern,", "tokens": ["Dei\u00b7ne", "lan\u00b7gen", "blon\u00b7den", "Lo\u00b7cken", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "An den F\u00fc\u00dfen klappern die Pantoffeln;", "tokens": ["An", "den", "F\u00fc\u00b7\u00dfen", "klap\u00b7pern", "die", "Pan\u00b7tof\u00b7feln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "K\u00f6nnen dich nicht mit uns nehmen, M\u00e4dchen,", "tokens": ["K\u00f6n\u00b7nen", "dich", "nicht", "mit", "uns", "neh\u00b7men", ",", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Charon, unser F\u00e4hrmann, w\u00fcrd' es merken.\u00ab", "tokens": ["Cha\u00b7ron", ",", "un\u00b7ser", "F\u00e4hr\u00b7mann", ",", "w\u00fcrd'", "es", "mer\u00b7ken", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Sprach das M\u00e4dchen drauf mit sanfter Stimme:", "tokens": ["Sprach", "das", "M\u00e4d\u00b7chen", "drauf", "mit", "sanf\u00b7ter", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "\u00bbmeine Kleider will ich von mir legen,", "tokens": ["\u00bb", "mei\u00b7ne", "Klei\u00b7der", "will", "ich", "von", "mir", "le\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Will vom Haupt die langen Locken schneiden,", "tokens": ["Will", "vom", "Haupt", "die", "lan\u00b7gen", "Lo\u00b7cken", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Die Pantoffeln la\u00df ich an der Treppe;", "tokens": ["Die", "Pan\u00b7tof\u00b7feln", "la\u00df", "ich", "an", "der", "Trep\u00b7pe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Nehmt mich mit hinauf, ihr lieben Riesen!", "tokens": ["Nehmt", "mich", "mit", "hin\u00b7auf", ",", "ihr", "lie\u00b7ben", "Rie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Sehen m\u00f6cht' ich meine beiden Br\u00fcder,", "tokens": ["Se\u00b7hen", "m\u00f6cht'", "ich", "mei\u00b7ne", "bei\u00b7den", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PPER", "PPOSAT", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Wie am Herd sie sitzen, mich beweinend!", "tokens": ["Wie", "am", "Herd", "sie", "sit\u00b7zen", ",", "mich", "be\u00b7wei\u00b7nend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPER", "VVINF", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Meine Mutter m\u00f6cht' ich klagen h\u00f6ren,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "m\u00f6cht'", "ich", "kla\u00b7gen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Klagen in der rauchbeschw\u00e4rzten H\u00fctte,", "tokens": ["Kla\u00b7gen", "in", "der", "rauch\u00b7be\u00b7schw\u00e4rz\u00b7ten", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Da\u00df ihr liebstes T\u00f6chterlein gestorben.\u00ab", "tokens": ["Da\u00df", "ihr", "liebs\u00b7tes", "T\u00f6ch\u00b7ter\u00b7lein", "ge\u00b7stor\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Sprachen drauf die Riesen: \u00bbLiebes M\u00e4dchen,", "tokens": ["Spra\u00b7chen", "drauf", "die", "Rie\u00b7sen", ":", "\u00bb", "Lie\u00b7bes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Bleib nur unten bei den bleichen Schatten!", "tokens": ["Bleib", "nur", "un\u00b7ten", "bei", "den", "blei\u00b7chen", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Deine Br\u00fcder singen in den Schenken,", "tokens": ["Dei\u00b7ne", "Br\u00fc\u00b7der", "sin\u00b7gen", "in", "den", "Schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Und dein M\u00fctterlein schwatzt auf der Gasse.\u00ab", "tokens": ["Und", "dein", "M\u00fct\u00b7ter\u00b7lein", "schwatzt", "auf", "der", "Gas\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "O wie gl\u00fccklich sind die gr\u00fcnen Felder,", "tokens": ["O", "wie", "gl\u00fcck\u00b7lich", "sind", "die", "gr\u00fc\u00b7nen", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O wie gl\u00fccklich sind die hohen Berge,", "tokens": ["O", "wie", "gl\u00fcck\u00b7lich", "sind", "die", "ho\u00b7hen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welche nimmermehr den Hades schauen!", "tokens": ["Wel\u00b7che", "nim\u00b7mer\u00b7mehr", "den", "Ha\u00b7des", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kommt der Winter, deckt er sie mit Reif zu", "tokens": ["Kommt", "der", "Win\u00b7ter", ",", "deckt", "er", "sie", "mit", "Reif", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKZU"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mit dichtem flockigen Gest\u00f6ber;", "tokens": ["Und", "mit", "dich\u00b7tem", "flo\u00b7cki\u00b7gen", "Ge\u00b7st\u00f6\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text":