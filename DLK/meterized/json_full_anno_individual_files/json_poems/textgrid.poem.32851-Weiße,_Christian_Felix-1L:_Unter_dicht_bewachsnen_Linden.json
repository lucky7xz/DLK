", "tokens": ["M\u00e4d\u00b7chen", ",", "wollt", "ihr", "et\u00b7was", "wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wagt nur ein Gel\u00fcbde nicht!", "tokens": ["Wagt", "nur", "ein", "Ge\u00b7l\u00fcb\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihr brechts in ersten Tagen,", "tokens": ["Denn", "ihr", "brechts", "in", "ers\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn euch nur ein Sch\u00e4fer spricht:", "tokens": ["Wenn", "euch", "nur", "ein", "Sch\u00e4\u00b7fer", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Freyheit frey zu walten,", "tokens": ["Mit", "der", "Frey\u00b7heit", "frey", "zu", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welch gef\u00e4hrlich Eigenthum!", "tokens": ["Welch", "ge\u00b7f\u00e4hr\u00b7lich", "Ei\u00b7gen\u00b7thum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "G\u00f6tter! die\u00df Gel\u00fcbd zu halten,", "tokens": ["G\u00f6t\u00b7ter", "!", "die\u00df", "Ge\u00b7l\u00fcbd", "zu", "hal\u00b7ten", ","], "token_info": ["