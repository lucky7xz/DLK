", "mich", "reisst", "au\u00df", "die\u00b7sem", "Orth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der nur st\u00fcrmt vnd netzet,", "tokens": ["Der", "nur", "st\u00fcrmt", "vnd", "net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vnd mich an den Himmels Port", "tokens": ["Vnd", "mich", "an", "den", "Him\u00b7mels", "Port"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seeliglich au\u00dfsetzet.", "tokens": ["See\u00b7lig\u00b7lich", "au\u00df\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}