e.8": {"text": "Gern euch wolte scheiden.", "tokens": ["Gern", "euch", "wol\u00b7te", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Nein, ich lasse nimmermehr", "tokens": ["Nein", ",", "ich", "las\u00b7se", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich von jhr betriegen;", "tokens": ["Mich", "von", "jhr", "be\u00b7trie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weg mit jhrer eiteln Ehr,", "tokens": ["Weg", "mit", "jhrer", "ei\u00b7teln", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vbermuth vnd L\u00fcgen!", "tokens": ["Vber\u00b7muth", "vnd", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie der Wind den Wolcken thut,", "tokens": ["Wie", "der", "Wind", "den", "Wol\u00b7cken", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Thut die Zeit den Sch\u00e4tzen;", "tokens": ["Thut", "die", "Zeit", "den", "Sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "An dem wahren Himmel-Gut", "tokens": ["An", "dem", "wah\u00b7ren", "Him\u00b7mel\u00b7Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hab' ich mein Ergetzen.", "tokens": ["Hab'", "ich", "mein", "Er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "T\u00f6richt ist, der hie sich seumbt.", "tokens": ["T\u00f6\u00b7richt", "ist", ",", "der", "hie", "sich", "seumbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vber allen Sternen", "tokens": ["Vber", "al\u00b7len", "Ster\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Steht mein Hau\u00df mir auffgereumt,", "tokens": ["Steht", "mein", "Hau\u00df", "mir", "auff\u00b7ge\u00b7reumt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Christus winckt von fernen;", "tokens": ["Chris\u00b7tus", "winckt", "von", "fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ach, ich werde frey vnd lo\u00df", "tokens": ["Ach", ",", "ich", "wer\u00b7de", "frey", "vnd", "lo\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "KON", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Last der Erden", "tokens": ["Von", "der", "Last", "der", "Er\u00b7den"], "toke