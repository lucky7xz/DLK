{"textgrid.poem.53272": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Affecto c\u0153li sidera, sordet humus", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es vergeht mir alle Lust", "tokens": ["Es", "ver\u00b7geht", "mir", "al\u00b7le", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4nger hie zu leben,", "tokens": ["L\u00e4n\u00b7ger", "hie", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "An der Erden Koht vnd Wust", "tokens": ["An", "der", "Er\u00b7den", "Koht", "vnd", "Wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag ich nicht mehr kleben,", "tokens": ["Mag", "ich", "nicht", "mehr", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df ich, Christe, f\u00fcr vnd f\u00fcr", "tokens": ["Da\u00df", "ich", ",", "Chris\u00b7te", ",", "f\u00fcr", "vnd", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJA", "$,", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lasse so viel Thr\u00e4nen,", "tokens": ["Las\u00b7se", "so", "viel", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Macht, da\u00df ich hinauff nach Dir", "tokens": ["Macht", ",", "da\u00df", "ich", "hin\u00b7auff", "nach", "Dir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hertzlich mich mu\u00df sehnen.", "tokens": ["Hertz\u00b7lich", "mich", "mu\u00df", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sagt mir, die jhr an der Welt", "tokens": ["Sagt", "mir", ",", "die", "jhr", "an", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch so sehr verliebet,", "tokens": ["Euch", "so", "sehr", "ver\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was hat sie euch vorgestellt,", "tokens": ["Was", "hat", "sie", "euch", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Welches nicht betr\u00fcbet?", "tokens": ["Wel\u00b7ches", "nicht", "be\u00b7tr\u00fc\u00b7bet", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auch das beste, so sie euch", "tokens": ["Auch", "das", "bes\u00b7te", ",", "so", "sie", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADV", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Giebt in jhren Frewden,", "tokens": ["Giebt", "in", "jhren", "Frew\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ist da\u00df sie vom Himmelreich", "tokens": ["Ist", "da\u00df", "sie", "vom", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gern euch wolte scheiden.", "tokens": ["Gern", "euch", "wol\u00b7te", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nein, ich lasse nimmermehr", "tokens": ["Nein", ",", "ich", "las\u00b7se", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich von jhr betriegen;", "tokens": ["Mich", "von", "jhr", "be\u00b7trie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weg mit jhrer eiteln Ehr,", "tokens": ["Weg", "mit", "jhrer", "ei\u00b7teln", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vbermuth vnd L\u00fcgen!", "tokens": ["Vber\u00b7muth", "vnd", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie der Wind den Wolcken thut,", "tokens": ["Wie", "der", "Wind", "den", "Wol\u00b7cken", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Thut die Zeit den Sch\u00e4tzen;", "tokens": ["Thut", "die", "Zeit", "den", "Sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "An dem wahren Himmel-Gut", "tokens": ["An", "dem", "wah\u00b7ren", "Him\u00b7mel\u00b7Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hab' ich mein Ergetzen.", "tokens": ["Hab'", "ich", "mein", "Er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "T\u00f6richt ist, der hie sich seumbt.", "tokens": ["T\u00f6\u00b7richt", "ist", ",", "der", "hie", "sich", "seumbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vber allen Sternen", "tokens": ["Vber", "al\u00b7len", "Ster\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Steht mein Hau\u00df mir auffgereumt,", "tokens": ["Steht", "mein", "Hau\u00df", "mir", "auff\u00b7ge\u00b7reumt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Christus winckt von fernen;", "tokens": ["Chris\u00b7tus", "winckt", "von", "fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ach, ich werde frey vnd lo\u00df", "tokens": ["Ach", ",", "ich", "wer\u00b7de", "frey", "vnd", "lo\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "KON", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Last der Erden", "tokens": ["Von", "der", "Last", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "In den s\u00fcssen Frewden-scho\u00df", "tokens": ["In", "den", "s\u00fcs\u00b7sen", "Fre\u00b7wden\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bald versetzet werden!", "tokens": ["Bald", "ver\u00b7set\u00b7zet", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wo der frommen Engel Schaar", "tokens": ["Wo", "der", "from\u00b7men", "En\u00b7gel", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes Lob erklingen,", "tokens": ["Got\u00b7tes", "Lob", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Werd' ich fr\u00f6lich immerdar", "tokens": ["Werd'", "ich", "fr\u00f6\u00b7lich", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Seiten singen;", "tokens": ["In", "die", "Sei\u00b7ten", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mich auch zu erquicken gehn", "tokens": ["Mich", "auch", "zu", "er\u00b7qui\u00b7cken", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bey dem Lebens-Brunnen,", "tokens": ["Bey", "dem", "Le\u00b7bens\u00b7Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vmb vnd an bekleidet stehn", "tokens": ["Vmb", "vnd", "an", "be\u00b7klei\u00b7det", "stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "APPR", "VVFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem Licht der Sonnen.", "tokens": ["Mit", "dem", "Licht", "der", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sonne, was verzeugstu viel?", "tokens": ["Son\u00b7ne", ",", "was", "ver\u00b7zeugs\u00b7tu", "viel", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fleuch mit deinem Wagen!", "tokens": ["Fleuch", "mit", "dei\u00b7nem", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eilt jhr Stunden! bringt mein Ziel", "tokens": ["Eilt", "jhr", "Stun\u00b7den", "!", "bringt", "mein", "Ziel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit euch her getragen,", "tokens": ["Mit", "euch", "her", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APZR", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df mich reisst au\u00df diesem Orth,", "tokens": ["Da\u00df", "mich", "reisst", "au\u00df", "die\u00b7sem", "Orth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der nur st\u00fcrmt vnd netzet,", "tokens": ["Der", "nur", "st\u00fcrmt", "vnd", "net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vnd mich an den Himmels Port", "tokens": ["Vnd", "mich", "an", "den", "Him\u00b7mels", "Port"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seeliglich au\u00dfsetzet.", "tokens": ["See\u00b7lig\u00b7lich", "au\u00df\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Es vergeht mir alle Lust", "tokens": ["Es", "ver\u00b7geht", "mir", "al\u00b7le", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4nger hie zu leben,