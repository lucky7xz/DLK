{"dta.poem.3771": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Wisst nicht was unter dem fuss euch ruht \u2014", "tokens": ["Wisst", "nicht", "was", "un\u00b7ter", "dem", "fuss", "euch", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWS", "APPR", "ART", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Meister lockt nicht die landschaft am strande", "tokens": ["Den", "Meis\u00b7ter", "lockt", "nicht", "die", "land\u00b7schaft", "am", "stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie jene blendend im schosse der flut", "tokens": ["Wie", "je\u00b7ne", "blen\u00b7dend", "im", "schos\u00b7se", "der", "flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ADJD", "APPRART", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die h\u00e4user und h\u00f6fe wie er sie ersonnen", "tokens": ["Die", "h\u00e4u\u00b7ser", "und", "h\u00f6\u00b7fe", "wie", "er", "sie", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "VVFIN", "KOKOM", "PPER", "PPER", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und unter den tritten der wesen beschworen", "tokens": ["Und", "un\u00b7ter", "den", "trit\u00b7ten", "der", "we\u00b7sen", "be\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ohne beispiel die strassen die bronnen", "tokens": ["Oh\u00b7ne", "bei\u00b7spiel", "die", "stras\u00b7sen", "die", "bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und grotten in strahlendem rausche geboren", "tokens": ["Und", "grot\u00b7ten", "in", "strah\u00b7len\u00b7dem", "rau\u00b7sche", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die einen blinken in ewigen wintern", "tokens": ["Die", "ei\u00b7nen", "blin\u00b7ken", "in", "e\u00b7wi\u00b7gen", "win\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Iene von hundertfarbigen erzen", "tokens": ["Ie\u00b7ne", "von", "hun\u00b7dert\u00b7far\u00b7bi\u00b7gen", "er\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aus denen juwelen als tropfen sintern", "tokens": ["Aus", "de\u00b7nen", "ju\u00b7we\u00b7len", "als", "trop\u00b7fen", "sin\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PIS", "KOKOM", "VVINF", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und flimmern und glimmen vor w\u00e4hrenden kerzen", "tokens": ["Und", "flim\u00b7mern", "und", "glim\u00b7men", "vor", "w\u00e4h\u00b7ren\u00b7den", "ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Die str\u00f6me die in den h\u00f6heren stollen", "tokens": ["Die", "str\u00f6\u00b7me", "die", "in", "den", "h\u00f6\u00b7he\u00b7ren", "stol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie scharlach granat und rubinen spr\u00fchten", "tokens": ["Wie", "schar\u00b7lach", "gra\u00b7nat", "und", "ru\u00b7bi\u00b7nen", "spr\u00fch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "KON", "VVINF", "VVFIN"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verf\u00e4rben sich bl\u00e4sser im niederrollen", "tokens": ["Ver\u00b7f\u00e4r\u00b7ben", "sich", "bl\u00e4s\u00b7ser", "im", "nie\u00b7der\u00b7rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPRART", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und fliessen von nun ab wie rosenbl\u00fcten", "tokens": ["Und", "flies\u00b7sen", "von", "nun", "ab", "wie", "ro\u00b7sen\u00b7bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "PTKVZ", "KOKOM", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auf seen tiefgr\u00fcn in h\u00e4fen verloren", "tokens": ["Auf", "seen", "tief\u00b7gr\u00fcn", "in", "h\u00e4\u00b7fen", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADJD", "APPR", "NN", "VVPP"], "meter": "+-++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schaukeln die ruderentbehrenden nachen", "tokens": ["Schau\u00b7keln", "die", "ru\u00b7de\u00b7rent\u00b7beh\u00b7ren\u00b7den", "na\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie wissen auch in die wellen zu bohren", "tokens": ["Sie", "wis\u00b7sen", "auch", "in", "die", "wel\u00b7len", "zu", "boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei armige riffe und g\u00e4hnende drachen", "tokens": ["Bei", "ar\u00b7mi\u00b7ge", "rif\u00b7fe", "und", "g\u00e4h\u00b7nen\u00b7de", "dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Der sch\u00f6pfung wo er nur geweckt und verwaltet", "tokens": ["Der", "sch\u00f6p\u00b7fung", "wo", "er", "nur", "ge\u00b7weckt", "und", "ver\u00b7wal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PWAV", "PPER", "ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erhabene neuheit ihn manchmal erfreut", "tokens": ["Er\u00b7ha\u00b7be\u00b7ne", "neu\u00b7heit", "ihn", "manch\u00b7mal", "er\u00b7freut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wo ausser dem seinen kein wille schaltet", "tokens": ["Wo", "aus\u00b7ser", "dem", "sei\u00b7nen", "kein", "wil\u00b7le", "schal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "PPOSAT", "PIAT", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und wo er dem licht und dem wetter gebeut", "tokens": ["Und", "wo", "er", "dem", "licht", "und", "dem", "wet\u00b7ter", "ge\u00b7beut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}