{"dta.poem.12845": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als ihn Albanie mit einem schnee-  \n ball geworffen.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, wie ich mich n\u00e4chsthin verleiten lie\u00df,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "ich", "mich", "n\u00e4chst\u00b7hin", "ver\u00b7lei\u00b7ten", "lie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es mit Albanien auf schnee-b\u00e4ll anzunehmen.", "tokens": ["Es", "mit", "Al\u00b7ba\u00b7ni\u00b7en", "auf", "schnee\u00b7b\u00e4ll", "an\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "APPR", "ADJD", "VVIZU", "$."], "meter": "--+----+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ich muste mich mit ihr an einen ort beqvemen,", "tokens": ["Ich", "mus\u00b7te", "mich", "mit", "ihr", "an", "ei\u00b7nen", "ort", "be\u00b7qve\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPOSAT", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da uns die einsamkeit aus aller augen ri\u00df.", "tokens": ["Da", "uns", "die", "ein\u00b7sam\u00b7keit", "aus", "al\u00b7ler", "au\u00b7gen", "ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es muste sich der schnee, der mehr als perlen glie\u00df,", "tokens": ["Es", "mus\u00b7te", "sich", "der", "schnee", ",", "der", "mehr", "als", "per\u00b7len", "glie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "$,", "PRELS", "PIS", "KOKOM", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor der entbl\u00f6sten haut der zarten h\u00e4nde sch\u00e4men.", "tokens": ["Vor", "der", "ent\u00b7bl\u00f6s\u00b7ten", "haut", "der", "zar\u00b7ten", "h\u00e4n\u00b7de", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Allein ich hob kaum an, die waffen vorzunehmen,", "tokens": ["Al\u00b7lein", "ich", "hob", "kaum", "an", ",", "die", "waf\u00b7fen", "vor\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als mich Albanie schon auf das hertze schmie\u00df.", "tokens": ["Als", "mich", "Al\u00b7ba\u00b7nie", "schon", "auf", "das", "hert\u00b7ze", "schmie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Sie lacht\u2019, und ich erschrack; sie lief, und ich blieb stehen.", "tokens": ["Sie", "lacht'", ",", "und", "ich", "er\u00b7schrack", ";", "sie", "lief", ",", "und", "ich", "blieb", "ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es wolte mir der schnee in meiner faust zergehen.", "tokens": ["Es", "wol\u00b7te", "mir", "der", "schnee", "in", "mei\u00b7ner", "faust", "zer\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich f\u00fchlt\u2019, ich wei\u00df nicht, was vor feuer in der brust.", "tokens": ["Ich", "f\u00fchlt'", ",", "ich", "wei\u00df", "nicht", ",", "was", "vor", "feu\u00b7er", "in", "der", "brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn da\u00df Albanie den Cypripor agiret,", "tokens": ["Denn", "da\u00df", "Al\u00b7ba\u00b7nie", "den", "Cyp\u00b7ri\u00b7por", "a\u00b7gi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Und in den kalten ball, den sie so wohl gef\u00fchret,", "tokens": ["Und", "in", "den", "kal\u00b7ten", "ball", ",", "den", "sie", "so", "wohl", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der liebe glnt versteckt, das hatt\u2019 ich nicht gewust.", "tokens": ["Der", "lie\u00b7be", "glnt", "ver\u00b7steckt", ",", "das", "hatt'", "ich", "nicht", "ge\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}