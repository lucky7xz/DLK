{"textgrid.poem.52183": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Geisterro\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch den dreigeteilten Bogen,", "tokens": ["Durch", "den", "drei\u00b7ge\u00b7teil\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Triumphes prangend Tor,", "tokens": ["Des", "Tri\u00b7um\u00b7phes", "pran\u00b7gend", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Durch die lauten Menschenwogen", "tokens": ["Durch", "die", "lau\u00b7ten", "Men\u00b7schen\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort zum Kapitol empor", "tokens": ["Dort", "zum", "Ka\u00b7pi\u00b7tol", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lenkt den Tanz der wei\u00dfen Pferde", "tokens": ["Lenkt", "den", "Tanz", "der", "wei\u00b7\u00dfen", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "C\u00e4sars l\u00e4ssige Geb\u00e4rde.", "tokens": ["C\u00e4\u00b7sars", "l\u00e4s\u00b7si\u00b7ge", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinter des Triumphes Wagen", "tokens": ["Hin\u00b7ter", "des", "Tri\u00b7um\u00b7phes", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Duldend oder grollend gehn", "tokens": ["Dul\u00b7dend", "o\u00b7der", "grol\u00b7lend", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberwundne. Ketten tragen", "tokens": ["\u00dc\u00b7berw\u00b7und\u00b7ne", ".", "Ket\u00b7ten", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "C\u00e4sars lebende Troph\u00e4n.", "tokens": ["C\u00e4\u00b7sars", "le\u00b7ben\u00b7de", "Tro\u00b7ph\u00e4n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdieser!\u00ab h\u00f6hnt es im Gedr\u00e4nge,", "tokens": ["\u00bb", "die\u00b7ser", "!", "\u00ab", "h\u00f6hnt", "es", "im", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "$.", "$(", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbdieser Trotz'ge!\u00ab zischt die Menge.", "tokens": ["\u00bb", "die\u00b7ser", "Trotz'\u00b7ge", "!", "\u00ab", "zischt", "die", "Men\u00b7ge", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unber\u00fchrt vom Hohn der Stunde,", "tokens": ["Un\u00b7be\u00b7r\u00fchrt", "vom", "Hohn", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starren, traumgef\u00fcllten Blicks,", "tokens": ["Star\u00b7ren", ",", "traum\u00b7ge\u00b7f\u00fcll\u00b7ten", "Blicks", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht, ein Singen auf dem Munde,", "tokens": ["Geht", ",", "ein", "Sin\u00b7gen", "auf", "dem", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhig Vercingetorix \u2013", "tokens": ["Ru\u00b7hig", "Ver\u00b7cin\u00b7ge\u00b7to\u00b7rix", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Fremde Weise, fremde Worte,", "tokens": ["Frem\u00b7de", "Wei\u00b7se", ",", "frem\u00b7de", "Wor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Geist an fremdem Orte:", "tokens": ["Mit", "dem", "Geist", "an", "frem\u00b7dem", "Or\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbc\u00e4sar, blendend wei\u00dfe Rosse", "tokens": ["\u00bb", "c\u00e4\u00b7sar", ",", "blen\u00b7dend", "wei\u00b7\u00dfe", "Ros\u00b7se"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM.la", "$,", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat Hispanien dir gebracht!", "tokens": ["Hat", "His\u00b7pa\u00b7ni\u00b7en", "dir", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ellid, edler Ahnen Sprosse,", "tokens": ["El\u00b7lid", ",", "ed\u00b7ler", "Ah\u00b7nen", "Spros\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkel ist er wie die Nacht \u2013", "tokens": ["Dun\u00b7kel", "ist", "er", "wie", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Schimmel, deine viere,", "tokens": ["Dei\u00b7ne", "Schim\u00b7mel", ",", "dei\u00b7ne", "vie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tauscht ich nicht mit meinem Tiere...", "tokens": ["Tauscht", "ich", "nicht", "mit", "mei\u00b7nem", "Tie\u00b7re", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ellid hei\u00dft der wackre Jager", "tokens": ["El\u00b7lid", "hei\u00dft", "der", "wack\u00b7re", "Ja\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark von Wuchs und fest im Bug", "tokens": ["Stark", "von", "Wuchs", "und", "fest", "im", "Bug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher mich ins R\u00f6merlager", "tokens": ["Wel\u00b7cher", "mich", "ins", "R\u00f6\u00b7mer\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gewalt'gen Spr\u00fcngen trug...", "tokens": ["Mit", "ge\u00b7walt'\u00b7gen", "Spr\u00fcn\u00b7gen", "trug", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der zum Opfer ich gegeben", "tokens": ["Der", "zum", "Op\u00b7fer", "ich", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich f\u00fcr meines Volkes Leben!", "tokens": ["Mich", "f\u00fcr", "mei\u00b7nes", "Vol\u00b7kes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dreimal flog ich um im Kreise,", "tokens": ["Drei\u00b7mal", "flog", "ich", "um", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Faust des Schwertes Blitz,", "tokens": ["In", "der", "Faust", "des", "Schwer\u00b7tes", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch im Lauf, nach Gallier Weise,", "tokens": ["Noch", "im", "Lauf", ",", "nach", "Gal\u00b7lier", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprang ich ab vor C\u00e4sars Sitz...", "tokens": ["Sprang", "ich", "ab", "vor", "C\u00e4\u00b7sars", "Sitz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "NE", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwarzer Ellid, zu den Toten", "tokens": ["Schwar\u00b7zer", "El\u00b7lid", ",", "zu", "den", "To\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Send ich dich als meinen Boten!", "tokens": ["Send", "ich", "dich", "als", "mei\u00b7nen", "Bo\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie er mir ins Antlitz schnaubte,", "tokens": ["Wie", "er", "mir", "ins", "Ant\u00b7litz", "schnaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stie\u00df ich, Blick versenkt in Blick,", "tokens": ["Stie\u00df", "ich", ",", "Blick", "ver\u00b7senkt", "in", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter seinem m\u00e4cht'gen Haupte", "tokens": ["Hin\u00b7ter", "sei\u00b7nem", "m\u00e4cht'\u00b7gen", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stracks das Schwert ihm durchs Genick...", "tokens": ["Stracks", "das", "Schwert", "ihm", "durchs", "Ge\u00b7nick", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mir eines Rosses Ehre", "tokens": ["Da\u00df", "mir", "ei\u00b7nes", "Ros\u00b7ses", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mangle nicht im Geisterheere.", "tokens": ["Mang\u00b7le", "nicht", "im", "Geis\u00b7ter\u00b7hee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ellid sprengt seit langen Jahren", "tokens": ["El\u00b7lid", "sprengt", "seit", "lan\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten in der bleichen Jagd,", "tokens": ["Mit\u00b7ten", "in", "der", "blei\u00b7chen", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann daheim die Toten fahren", "tokens": ["Wann", "da\u00b7heim", "die", "To\u00b7ten", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die W\u00e4lder, bis es tagt...", "tokens": ["Durch", "die", "W\u00e4l\u00b7der", ",", "bis", "es", "tagt", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sehn sie meinen led'gen Renner,", "tokens": ["Sehn", "sie", "mei\u00b7nen", "le\u00b7d'\u00b7gen", "Ren\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wundern sich die stillen M\u00e4nner...", "tokens": ["Wun\u00b7dern", "sich", "die", "stil\u00b7len", "M\u00e4n\u00b7ner", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lange Jahre lag gebunden", "tokens": ["Lan\u00b7ge", "Jah\u00b7re", "lag", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich in feuchter Kerkergruft \u2013", "tokens": ["Ich", "in", "feuch\u00b7ter", "Ker\u00b7ker\u00b7gruft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kettenschwere, dumpfe Stunden \u2013", "tokens": ["Ket\u00b7ten\u00b7schwe\u00b7re", ",", "dump\u00b7fe", "Stun\u00b7den", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich wieder Tag und Luft \u2013", "tokens": ["End\u00b7lich", "wie\u00b7der", "Tag", "und", "Luft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ellid, schwarzer Ellid, spute", "tokens": ["El\u00b7lid", ",", "schwar\u00b7zer", "El\u00b7lid", ",", "spu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich! Du witterst, wo ich blute!", "tokens": ["Dich", "!", "Du", "wit\u00b7terst", ",", "wo", "ich", "blu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Heute endlich! Endlich heute!", "tokens": ["Heu\u00b7te", "end\u00b7lich", "!", "End\u00b7lich", "heu\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann der Kahle schwelgt am Mahl,", "tokens": ["Wann", "der", "Kah\u00b7le", "schwelgt", "am", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrgt er seine Siegesbeute.", "tokens": ["W\u00fcrgt", "er", "sei\u00b7ne", "Sie\u00b7ges\u00b7beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem letzten m\u00fcden Strahl,", "tokens": ["Mit", "dem", "letz\u00b7ten", "m\u00fc\u00b7den", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra