"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ellids kurzes Hufgestampfe", "tokens": ["El\u00b7lids", "kur\u00b7zes", "Huf\u00b7ge\u00b7stamp\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00f6hnt in meinem Todeskampfe!", "tokens": ["Dr\u00f6hnt", "in", "mei\u00b7nem", "To\u00b7des\u00b7kamp\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sterbend pack ich Ellids Haare,", "tokens": ["Ster\u00b7bend", "pack", "ich", "El\u00b7lids", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Befreiter spring ich auf,", "tokens": ["Ein", "Be\u00b7frei\u00b7ter", "spring", "ich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fahre, schwarzer Ellid, fahre!", "tokens": ["Fah\u00b7re", ",", "schwar\u00b7zer", "El\u00b7lid", ",", "fah\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der Heimat nimm den Lauf!", "tokens": ["Nach", "der", "Hei\u00b7mat", "nimm", "den", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wogen tosen! Rhodans Stimme!", "tokens": ["Wo\u00b7gen", "to\u00b7sen", "!", "Rho\u00b7dans", "Stim\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Strom, mein Tier, und schwimme!\u00ab", "tokens": ["In", "den", "Strom", ",", "mein", "Tier", ",", "und", "schwim\u00b7me", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "C\u00e4sars Schimmel bl\u00e4hn die N\u00fcstern.", "tokens": ["C\u00e4\u00b7sars", "Schim\u00b7mel", "bl\u00e4hn", "die", "N\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbave Triumphator!\u00ab schallt.", "tokens": ["\u00bb", "a\u00b7ve", "Tri\u00b7um\u00b7pha\u00b7tor", "!", "\u00ab", "schallt", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$(", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Gebundnen Lippen fl\u00fcstern:", "tokens": ["Des", "Ge\u00b7bund\u00b7nen", "Lip\u00b7pen", "fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbin der Heimat bin ich bald!", "tokens": ["\u00bb", "in", "der", "Hei\u00b7mat", "bin", "ich", "bald", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ellid mit gestrecktem Jagen", "tokens": ["El\u00b7lid", "mit", "ge\u00b7streck\u00b7tem", "Ja\u00b7