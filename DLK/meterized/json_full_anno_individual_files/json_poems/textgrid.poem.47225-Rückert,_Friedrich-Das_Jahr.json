{"textgrid.poem.47225": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Lande m\u00f6cht' ich wohnen,", "tokens": ["In", "ei\u00b7nem", "Lan\u00b7de", "m\u00f6cht'", "ich", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo der Natur gesetzter Zwang", "tokens": ["Wo", "der", "Na\u00b7tur", "ge\u00b7setz\u00b7ter", "Zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinwandeln l\u00e4\u00dft durch gl\u00fch'nde Zonen", "tokens": ["Hin\u00b7wan\u00b7deln", "l\u00e4\u00dft", "durch", "gl\u00fch'n\u00b7de", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Jahres unverr\u00fcckten Gang;", "tokens": ["Des", "Jah\u00b7res", "un\u00b7ver\u00b7r\u00fcck\u00b7ten", "Gang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nach des Winters Regeng\u00fcssen", "tokens": ["Wo", "nach", "des", "Win\u00b7ters", "Re\u00b7gen\u00b7g\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein langer fester Sommer kommt", "tokens": ["Ein", "lan\u00b7ger", "fes\u00b7ter", "Som\u00b7mer", "kommt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auch die Menschen f\u00fchlen m\u00fcssen,", "tokens": ["Und", "auch", "die", "Men\u00b7schen", "f\u00fch\u00b7len", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df nicht ein wirrer Wechsel frommt.", "tokens": ["Da\u00df", "nicht", "ein", "wir\u00b7rer", "Wech\u00b7sel", "frommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und w\u00e4re das mir nicht beschieden,", "tokens": ["Und", "w\u00e4\u00b7re", "das", "mir", "nicht", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So m\u00f6cht' ich wohnen an dem Pol,", "tokens": ["So", "m\u00f6cht'", "ich", "woh\u00b7nen", "an", "dem", "Pol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo eines tiefen Winters Frieden", "tokens": ["Wo", "ei\u00b7nes", "tie\u00b7fen", "Win\u00b7ters", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mir lie\u00df auch gefallen wohl;", "tokens": ["Ich", "mir", "lie\u00df", "auch", "ge\u00b7fal\u00b7len", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["