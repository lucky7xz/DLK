{"dta.poem.21757": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Vjer Jahre brauchten wir fast einen", "tokens": ["Vjer", "Jah\u00b7re", "brauch\u00b7ten", "wir", "fast", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tisch und Stube:", "tokens": ["Tisch", "und", "Stu\u00b7be", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "mir ist noch nie bewust/ da\u00df du", "tokens": ["mir", "ist", "noch", "nie", "be\u00b7wust", "/", "da\u00df", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$(", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Lied er dacht", "tokens": ["ein", "Lied", "er", "dacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "von dehm/ was Zyprie vor fremde Rei-", "tokens": ["von", "dehm", "/", "was", "Zyp\u00b7rie", "vor", "frem\u00b7de", "Rei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "PWS", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "zung macht/", "tokens": ["zung", "macht", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "nun iezo sport dich an der kleine Liebes Bu-", "tokens": ["nun", "ie\u00b7zo", "sport", "dich", "an", "der", "klei\u00b7ne", "Lie\u00b7bes", "Bu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "be.", "tokens": ["be", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Bald klagst/ bald dreuestu/ bald weistu dich zu", "tokens": ["Bald", "klagst", "/", "bald", "dreu\u00b7es\u00b7tu", "/", "bald", "weis\u00b7tu", "dich", "zu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "ADV", "CARD", "$(", "ADV", "VVFIN", "PPER", "PTKZU"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "laben/", "tokens": ["la\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "bald r\u00e4htstu andern ab/ da\u00df sie das s\u00fcsse", "tokens": ["bald", "r\u00e4hts\u00b7tu", "an\u00b7dern", "ab", "/", "da\u00df", "sie", "das", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$(", "KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gifft/", "tokens": ["Gifft", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.13": {"text": "die Liebe/ sollen fliehn/ die dich wol selber", "tokens": ["die", "Lie\u00b7be", "/", "sol\u00b7len", "fliehn", "/", "die", "dich", "wol", "sel\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "VMFIN", "VVINF", "$(", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "trifft.", "tokens": ["trifft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Du scherzest mit der Faust/ und prangest mit", "tokens": ["Du", "scher\u00b7zest", "mit", "der", "Faust", "/", "und", "pran\u00b7gest", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "den Gaben", "tokens": ["den", "Ga\u00b7ben"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "so dir Apollo schenkt. Ist so ein lindes", "tokens": ["so", "dir", "A\u00b7pol\u00b7lo", "schenkt", ".", "Ist", "so", "ein", "lin\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "NE", "VVFIN", "$.", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Schreiben", "tokens": ["Schrei\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "im Sturm auch ie erh\u00f6rt? da Schwerdt", "tokens": ["im", "Sturm", "auch", "ie", "er\u00b7h\u00f6rt", "?", "da", "Schwerdt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV", "VVFIN", "$.", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "und Pulver knallt", "tokens": ["und", "Pul\u00b7ver", "knallt"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "und der verwundten Lerm in Felsen wie-", "tokens": ["und", "der", "ver\u00b7wund\u00b7ten", "Lerm", "in", "Fel\u00b7sen", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "derschallt/", "tokens": ["der\u00b7schallt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "kanstu der Liebe Spiel in sanfften Reimen", "tokens": ["kans\u00b7tu", "der", "Lie\u00b7be", "Spiel", "in", "sanff\u00b7ten", "Rei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "treiben.", "tokens": ["trei\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Die Liebe hat offt Streit und Blut und Tod", "tokens": ["Die", "Lie\u00b7be", "hat", "offt", "Streit", "und", "Blut", "und", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "gebohren", "tokens": ["ge\u00b7boh\u00b7ren"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "hie wird im Streit\u2019 und Blut und Tode", "tokens": ["hie", "wird", "im", "Streit'", "und", "Blut", "und", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Venus wach/", "tokens": ["Ve\u00b7nus", "wach", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "(nach", "tokens": ["(", "nach"], "token_info": ["punct", "word"], "pos": ["$(", "APPR"], "meter": "+", "measure": "single.up"}, "line.30": {"text": "ich denke deinem Geist\u2019 offt bey mir selber", "tokens": ["ich", "den\u00b7ke", "dei\u00b7nem", "Geist'", "offt", "bey", "mir", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "und merke/ da\u00df sich dir der Himmel hat ver-", "tokens": ["und", "mer\u00b7ke", "/", "da\u00df", "sich", "dir", "der", "Him\u00b7mel", "hat", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PRF", "PPER", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "schwooren.", "tokens": ["schwoo\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Da du in St\u00e4ten lebtst/ da schriebestu von", "tokens": ["Da", "du", "in", "St\u00e4\u00b7ten", "lebtst", "/", "da", "schrie\u00b7be\u00b7stu", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "ADV", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Feldern/", "tokens": ["Fel\u00b7dern", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "(liebt/", "tokens": ["(", "liebt", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.36": {"text": "nu du im Felde wachst/ stellstu dich wie ver-", "tokens": ["nu", "du", "im", "Fel\u00b7de", "wachst", "/", "stell\u00b7stu", "dich", "wie", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$(", "VVFIN", "PPER", "KOKOM", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "ich wei\u00df nicht was dein Kiel nicht alles von", "tokens": ["ich", "wei\u00df", "nicht", "was", "dein", "Kiel", "nicht", "al\u00b7les", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPOSAT", "NN", "PTKNEG", "PIS", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "sich gibt.", "tokens": ["sich", "gibt", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.39": {"text": "Brich ab/ es ist verdient der Zweig au\u00df F\u00f6bus", "tokens": ["Brich", "ab", "/", "es", "ist", "ver\u00b7dient", "der", "Zweig", "au\u00df", "F\u00f6\u00b7bus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$(", "PPER", "VAFIN", "VVPP", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "W\u00e4ldern.", "tokens": ["W\u00e4l\u00b7dern", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}