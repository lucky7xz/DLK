{"dta.poem.18798": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich dein gedenke,", "tokens": ["Wenn", "ich", "dein", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fragt mich gleich der Schenke:", "tokens": ["Fragt", "mich", "gleich", "der", "Schen\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Herr! Warum so still?", "tokens": ["Herr", "!", "Wa\u00b7rum", "so", "still", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da von deinen Lehren", "tokens": ["Da", "von", "dei\u00b7nen", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Immer weiter h\u00f6ren", "tokens": ["Im\u00b7mer", "wei\u00b7ter", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Saki gerne will.", "tokens": ["Sa\u00b7ki", "ger\u00b7ne", "will", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich mich vergesse", "tokens": ["Wenn", "ich", "mich", "ver\u00b7ges\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Unter der Cypresse", "tokens": ["Un\u00b7ter", "der", "Cyp\u00b7res\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "H\u00e4lt er nichts davon,", "tokens": ["H\u00e4lt", "er", "nichts", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PAV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und im stillen Kreise", "tokens": ["Und", "im", "stil\u00b7len", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bin ich doch so weise,", "tokens": ["Bin", "ich", "doch", "so", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Klug wie Salomon.", "tokens": ["Klug", "wie", "Sa\u00b7lo\u00b7mon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}