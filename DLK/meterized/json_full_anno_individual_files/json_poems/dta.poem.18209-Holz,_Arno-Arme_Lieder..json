{"dta.poem.18209": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Arme Lieder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df er k\u00e4me, jener F\u00fcrst der Liebe,", "tokens": ["O", "da\u00df", "er", "k\u00e4\u00b7me", ",", "je\u00b7ner", "F\u00fcrst", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "$,", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der von dem Haupt die goldne Krone legt", "tokens": ["Der", "von", "dem", "Haupt", "die", "gold\u00b7ne", "Kro\u00b7ne", "legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, da\u00df kein Herz verarmt und d\u00fcrftig bliebe,", "tokens": ["Und", ",", "da\u00df", "kein", "Herz", "ver\u00b7armt", "und", "d\u00fcrf\u00b7tig", "blie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIAT", "NN", "VVPP", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den goldnen Reif zu frommen M\u00fcnzen pr\u00e4gt,", "tokens": ["Den", "gold\u00b7nen", "Reif", "zu", "from\u00b7men", "M\u00fcn\u00b7zen", "pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der seinen Purpurmantel voll Erbarmen", "tokens": ["Der", "sei\u00b7nen", "Pur\u00b7pur\u00b7man\u00b7tel", "voll", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Windeln theilte f\u00fcr die Brut der Armen!", "tokens": ["Zu", "Win\u00b7deln", "theil\u00b7te", "f\u00fcr", "die", "Brut", "der", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.8": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.2": {"line.1": {"text": "Ein sch\u00f6ner Traum! Er wird sich nicht erf\u00fcllen,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Traum", "!", "Er", "wird", "sich", "nicht", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch blickt er sch\u00f6n aus rothem D\u00e4mmerlicht.", "tokens": ["Doch", "blickt", "er", "sch\u00f6n", "aus", "ro\u00b7them", "D\u00e4m\u00b7mer\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es taugt, die Noth der Erde zu verh\u00fcllen,", "tokens": ["Es", "taugt", ",", "die", "Noth", "der", "Er\u00b7de", "zu", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumenpracht von hundert Lenzen nicht,", "tokens": ["Die", "Blu\u00b7men\u00b7pracht", "von", "hun\u00b7dert", "Len\u00b7zen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Allein so lang noch ird'sche Lenze dauern,", "tokens": ["Al\u00b7lein", "so", "lang", "noch", "ird'\u00b7sche", "Len\u00b7ze", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wird der Poet mit dem Enterbten trauern.", "tokens": ["Wird", "der", "Po\u00b7et", "mit", "dem", "En\u00b7terb\u00b7ten", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}}}}