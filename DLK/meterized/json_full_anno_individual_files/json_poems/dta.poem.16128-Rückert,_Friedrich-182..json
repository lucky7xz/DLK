{"dta.poem.16128": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "182.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der preise sein Geschick, wer irgend hat zu klagen;", "tokens": ["Der", "prei\u00b7se", "sein", "Ge\u00b7schick", ",", "wer", "ir\u00b7gend", "hat", "zu", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PWS", "ADV", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erleichtert f\u00fchle sich, wer Schweres hat zu tragen.", "tokens": ["Er\u00b7leich\u00b7tert", "f\u00fch\u00b7le", "sich", ",", "wer", "Schwe\u00b7res", "hat", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "$,", "PWS", "NE", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn alle sind wir hier zu Zins und Zoll verpflichtet", "tokens": ["Denn", "al\u00b7le", "sind", "wir", "hier", "zu", "Zins", "und", "Zoll", "ver\u00b7pflich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "APPR", "NE", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Ungl\u00fcck; gl\u00fccklich ist, wer ihn schon hat entrichtet.", "tokens": ["Dem", "Un\u00b7gl\u00fcck", ";", "gl\u00fcck\u00b7lich", "ist", ",", "wer", "ihn", "schon", "hat", "ent\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "p