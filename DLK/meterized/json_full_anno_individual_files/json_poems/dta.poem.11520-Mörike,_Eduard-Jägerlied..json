{"dta.poem.11520": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4gerlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn er wandelt auf des Berges H\u00f6h':", "tokens": ["Wenn", "er", "wan\u00b7delt", "auf", "des", "Ber\u00b7ges", "H\u00f6h'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zierlicher schreibt Liebchens liebe Hand,", "tokens": ["Zier\u00b7li\u00b7cher", "schreibt", "Lieb\u00b7chens", "lie\u00b7be", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schreibt ein Brieflein mir in ferne Land'.", "tokens": ["Schreibt", "ein", "Brie\u00b7flein", "mir", "in", "fer\u00b7ne", "Land'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "In die L\u00fcfte hoch ein Reiher steigt,", "tokens": ["In", "die", "L\u00fcf\u00b7te", "hoch", "ein", "Rei\u00b7her", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dahin weder Pfeil noch Kugel fleugt:", "tokens": ["Da\u00b7hin", "we\u00b7der", "Pfeil", "noch", "Ku\u00b7gel", "fleugt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tausendmal so hoch und so geschwind", "tokens": ["Tau\u00b7send\u00b7mal", "so", "hoch", "und", "so", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die Gedanken treuer Liebe sind.", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "treu\u00b7er", "Lie\u00b7be", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}