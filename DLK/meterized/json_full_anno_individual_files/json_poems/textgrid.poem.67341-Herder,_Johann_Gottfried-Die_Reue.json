"trochaic.tetra"}, "line.7": {"text": "So verdiene Freuden!", "tokens": ["So", "ver\u00b7die\u00b7ne", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Irrthum zwar und Thorheit sind", "tokens": ["Irr\u00b7thum", "zwar", "und", "Thor\u00b7heit", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Loos hienieden,", "tokens": ["Un\u00b7ser", "Loos", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mi\u00dfgestaltet, schwach und blind;", "tokens": ["Mi\u00df\u00b7ge\u00b7stal\u00b7tet", ",", "schwach", "und", "blind", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Fehler ist ihr Kind", "tokens": ["Je\u00b7der", "Feh\u00b7ler", "ist", "ihr", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und verscheucht den Frieden.", "tokens": ["Und", "ver\u00b7scheucht", "den", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ach, der s\u00fc\u00dfen Feinde sind", "tokens": ["Ach", ",", "der", "s\u00fc\u00b7\u00dfen", "Fein\u00b7de", "sind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns so viel beschieden.", "tokens": ["Uns", "so", "viel", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber jedem Fehl verband", "tokens": ["A\u00b7ber", "je\u00b7dem", "Fehl", "ver\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene ew'ge Treue,", "tokens": ["Je\u00b7ne", "ew'\u00b7ge", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jener g\u00f6ttliche Verstand", "tokens": ["Je\u00b7ner", "g\u00f6tt\u00b7li\u00b7che", "Ver\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Liebe bestes Pfand,", "tokens": ["Sei\u00b7ner", "Lie\u00b7be", "bes\u00b7tes", "Pfand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie uns erneue;", "tokens": ["Da\u00df", "sie", "uns", "er\u00b7neu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Besserung wird sie genannt,", "tokens": ["Bes\u00b7se\u00b7rung", "wird", "sie", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Menschen nennen s' Reue.", "tokens": ["Men\u00b7schen", "nen\u00b7nen", "s'", "Reu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Sanft zieht sie hinweg den Flor", "tokens": ["Sanft", "zieht", "sie", "hin\u00b7weg", "den", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von des Fehlers Blicke;", "tokens": ["Von", "des", "Feh\u00b7lers", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Warnend kommt sie ihm zuvor,", "tokens": ["War\u00b7nend", "kommt", "sie", "ihm", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oeffnet sanft sein taubes Ohr,", "tokens": ["Oeff\u00b7net", "sanft", "sein", "tau\u00b7bes", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchrt ihn zart zur\u00fccke;", "tokens": ["F\u00fchrt", "ihn", "zart", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Durch der Reue niedres Thor", "tokens": ["Durch", "der", "Reu\u00b7e", "nie\u00b7dres", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wandern wir zum Gl\u00fccke.", "tokens": ["Wan\u00b7dern", "wir", "zum", "Gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tr