{"dta.poem.3382": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Aussicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Umschatte mich mit deinem Engelfl&#252;gel,", "tokens": ["Um\u00b7schat\u00b7te", "mich", "mit", "dei\u00b7nem", "En\u00b7gel\u00b7fl", "&#252;", "gel", ","], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedank der Ewigkeit!", "tokens": ["Ge\u00b7dank", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh in dir, wie im krystallnen Spiegel,", "tokens": ["Ich", "seh", "in", "dir", ",", "wie", "im", "krys\u00b7tall\u00b7nen", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergolten alles Leid.", "tokens": ["Ver\u00b7gol\u00b7ten", "al\u00b7les", "Leid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wonach ich rang mit tausend Inbrunstthr\u00e4nen,", "tokens": ["Wo\u00b7nach", "ich", "rang", "mit", "tau\u00b7send", "In\u00b7brunst\u00b7thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird dort von mir erweint.", "tokens": ["Wird", "dort", "von", "mir", "er\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wonach ich schmachtete mit leisem Sehnen,", "tokens": ["Wo\u00b7nach", "ich", "schmach\u00b7te\u00b7te", "mit", "lei\u00b7sem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umarmt mich dort, wie Freund.", "tokens": ["Um\u00b7armt", "mich", "dort", ",", "wie", "Freund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was mir verborgen blieb im Reich des Wahren,", "tokens": ["Was", "mir", "ver\u00b7bor\u00b7gen", "blieb", "im", "Reich", "des", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird dort mir offenbart.", "tokens": ["Wird", "dort", "mir", "of\u00b7fen\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich verlor in hingeschiednen Jahren,", "tokens": ["Was", "ich", "ver\u00b7lor", "in", "hin\u00b7ge\u00b7schied\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird dort mir aufgespart.", "tokens": ["Wird", "dort", "mir", "auf\u00b7ge\u00b7spart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort werd' ich euch, ihr Guten, wieder schauen,", "tokens": ["Dort", "werd'", "ich", "euch", ",", "ihr", "Gu\u00b7ten", ",", "wie\u00b7der", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ich mir ausgekiest,", "tokens": ["Die", "ich", "mir", "aus\u00b7ge\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die ihr mich in dieser Wildniss Grauen", "tokens": ["Und", "die", "ihr", "mich", "in", "die\u00b7ser", "Wild\u00b7niss", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "PRF", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein zur\u00fccke liesst.", "tokens": ["Al\u00b7lein", "zu\u00b7r\u00fc\u00b7cke", "liesst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da werd' ich dich, Geliebte, wieder k\u00fcssen,", "tokens": ["Da", "werd'", "ich", "dich", ",", "Ge\u00b7lieb\u00b7te", ",", "wie\u00b7der", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "$,", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mir das Schicksal nahm.", "tokens": ["Die", "mir", "das", "Schick\u00b7sal", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich werde vor den Engeln \u201eBraut!\u201c dich gr\u00fcssen,", "tokens": ["Ich", "wer\u00b7de", "vor", "den", "En\u00b7geln", "\u201e", "Braut", "!", "\u201c", "dich", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$(", "NN", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und du mich \u201eBr\u00e4utigam!\u201c", "tokens": ["Und", "du", "mich", "\u201e", "Br\u00e4u\u00b7ti\u00b7gam", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "PRF", "$(", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich werde dich, der Welten Urgebilde,", "tokens": ["Ich", "wer\u00b7de", "dich", ",", "der", "Wel\u00b7ten", "Ur\u00b7ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich, ursprungloses Sch\u00f6n,", "tokens": ["Dich", ",", "ur\u00b7sprung\u00b7lo\u00b7ses", "Sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In aller deiner Lieb' und Treu' und Milde", "tokens": ["In", "al\u00b7ler", "dei\u00b7ner", "Lieb'", "und", "Treu'", "und", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ganz und gewandlos schn.", "tokens": ["Ganz", "und", "ge\u00b7wand\u00b7los", "schn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein Saitenspiel, das hier von Erdendingen", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", ",", "das", "hier", "von", "Er\u00b7den\u00b7din\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur matt und irdisch klang,", "tokens": ["Nur", "matt", "und", "ir\u00b7disch", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird psalmenstr\u00f6mend durch die Himmel klingen,", "tokens": ["Wird", "psal\u00b7men\u00b7str\u00f6\u00b7mend", "durch", "die", "Him\u00b7mel", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Sph\u00e4ren-Hochgesang.", "tokens": ["Wie", "Sph\u00e4\u00b7ren\u00b7Hoch\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Homer und David werden mein sich freuen,", "tokens": ["Ho\u00b7mer", "und", "Da\u00b7vid", "wer\u00b7den", "mein", "sich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VAFIN", "PPOSAT", "PRF", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihr goldnes Harfenspiel", "tokens": ["Ihr", "gold\u00b7nes", "Har\u00b7fen\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir reichen, mich zum Himmelsdichter weihen", "tokens": ["Mir", "rei\u00b7chen", ",", "mich", "zum", "Him\u00b7mels\u00b7dich\u00b7ter", "wei\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRF", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am palmbekr\u00e4nzten Ziel \u2014", "tokens": ["Am", "palm\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Ziel"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Verlass mich nicht, mein Theurer, S\u00fcsser, Lieber!", "tokens": ["Ver\u00b7lass", "mich", "nicht", ",", "mein", "Theu\u00b7rer", ",", "S\u00fcs\u00b7ser", ",", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedank der Ewigkeit!", "tokens": ["Ge\u00b7dank", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verwehe du, wird meine Seele tr\u00fcber,", "tokens": ["Ver\u00b7we\u00b7he", "du", ",", "wird", "mei\u00b7ne", "See\u00b7le", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den tr\u00fcben Dunst der Zeit.", "tokens": ["Den", "tr\u00fc\u00b7ben", "Dunst", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn mich in finstern, allzufinstern Stunden", "tokens": ["Wenn", "mich", "in", "fins\u00b7tern", ",", "all\u00b7zu\u00b7fins\u00b7tern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein alter Kummer fasst,", "tokens": ["Mein", "al\u00b7ter", "Kum\u00b7mer", "fasst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So tr\u00e4ufle heilend \u00d6l in meine Wunden,", "tokens": ["So", "tr\u00e4uf\u00b7le", "hei\u00b7lend", "\u00d6l", "in", "mei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schaff mir wieder Rast.", "tokens": ["Und", "schaff", "mir", "wie\u00b7der", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und lieg' ich einst \u2014 wer weiss, wie bald! \u2014", "tokens": ["Und", "lieg'", "ich", "einst", "wer", "weiss", ",", "wie", "bald", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "PWS", "VVFIN", "$,", "PWAV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zu sterben,", "tokens": ["zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So s\u00e4usl' auf mich herab.", "tokens": ["So", "s\u00e4usl'", "auf", "mich", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ruhig steig' ich, jenes Heil zu erben,", "tokens": ["Und", "ru\u00b7hig", "steig'", "ich", ",", "je\u00b7nes", "Heil", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ins jammerlose Grab!", "tokens": ["Ins", "jam\u00b7mer\u00b7lo\u00b7se", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}