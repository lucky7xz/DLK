r": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Liebe wollt' entgegengehn,", "tokens": ["In", "Lie\u00b7be", "wollt'", "ent\u00b7ge\u00b7gen\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sah ich in der Dornenkrone", "tokens": ["Da", "sah", "ich", "in", "der", "Dor\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Liebsten traurig seitw\u00e4rts stehn.", "tokens": ["Den", "Liebs\u00b7ten", "trau\u00b7rig", "seit\u00b7w\u00e4rts", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Und wollt' ich dennoch Ihn verlassen", "tokens": ["Und", "wollt'", "ich", "den\u00b7noch", "Ihn", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollte mich dem Andern weihn,", "tokens": ["Und", "woll\u00b7te", "mich", "dem", "An\u00b7dern", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sah ich Ihn am Kreuz erblassen,", "tokens": ["So", "sah", "ich", "Ihn", "am", "Kreuz", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So treu, so liebend, so allein.", "tokens": ["So", "treu", ",", "so", "lie\u00b7bend", ",", "so", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Da hab' ich viel geweint, gerungen,", "tokens": ["Da", "hab'", "ich", "viel", "ge\u00b7weint", ",", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Ihm gewandt den tr\u00fcben Blick,", "tokens": ["Zu", "Ihm", "ge\u00b7wandt", "den", "tr\u00fc\u00b7ben", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis siegend ich hindurchgedrungen;", "tokens": ["Bis", "sie\u00b7gend", "ich", "hin\u00b7durch\u00b7ge\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun will ich ewig nicht zur\u00fcck.", "tokens": ["Nun", "will", "ich", "e\u00b7wig", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Ich g\u00f6nn' euch ja all' eure Freunden,", "tokens": ["Ich", "g\u00f6nn'", "euch", "ja", "all'", "eu\u00b7re", "Freun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den bunten Tand, ich mag ihn nicht.", "tokens": ["Den", "bun\u00b7ten", "Tand", ",", "ich", "mag", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "All euer Freu'n, all euer Leiden", "tokens": ["All", "eu\u00b7er", "Freu'n", ",", "all", "eu\u00b7er", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht mein Schatten, nicht mein Licht.", "tokens": ["Ist", "nicht", "mein", "Schat\u00b7ten", ",", "nicht", "mein", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "O g\u00f6nnt auch ihr mir meine Freuden", "tokens": ["O", "g\u00f6nnt", "auch", "ihr", "mir", "mei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PPER", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00f6ret meinen Frieden nicht.", "tokens": ["Und", "st\u00f6\u00b7ret", "mei\u00b7nen", "Frie\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Ihm, mit Ihm ist jedes Leiden", "tokens": ["In", "Ihm", ",", "mit", "Ihm", "ist", "je\u00b7des", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "PIA