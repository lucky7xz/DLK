{"dta.poem.18269": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Bibelbiereifrig!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Jeder Mensch hat sein Pl\u00e4sirchen \u2014", "tokens": ["Je\u00b7der", "Mensch", "hat", "sein", "Pl\u00e4\u00b7sir\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So ein armer Recensent", "tokens": ["So", "ein", "ar\u00b7mer", "Re\u00b7cen\u00b7sent"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist das \u00e4rmste aller Thierchen.", "tokens": ["Ist", "das", "\u00e4rms\u00b7te", "al\u00b7ler", "Thier\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn es pfaucht und wenn es zischt,", "tokens": ["Wenn", "es", "pfaucht", "und", "wenn", "es", "zischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df es, la\u00df es sich nur schinden,", "tokens": ["La\u00df", "es", ",", "la\u00df", "es", "sich", "nur", "schin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVIMP", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn dem Ochsen, der da drischt,", "tokens": ["Denn", "dem", "Och\u00b7sen", ",", "der", "da", "drischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst du nicht das Maul verbinden!", "tokens": ["Sollst", "du", "nicht", "das", "Maul", "ver\u00b7bin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}