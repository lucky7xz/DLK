{"textgrid.poem.67436": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Gesang des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie die Tage der Menschen, so ist der Menschen Gesinnung,", "tokens": ["\u00bb", "wie", "die", "Ta\u00b7ge", "der", "Men\u00b7schen", ",", "so", "ist", "der", "Men\u00b7schen", "Ge\u00b7sin\u00b7nung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ART", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wie sie, b\u00f6s oder gut, Jupiter ihnen verh\u00e4ngt.\u00ab", "tokens": ["Wie", "sie", ",", "b\u00f6s", "o\u00b7der", "gut", ",", "Ju\u00b7pi\u00b7ter", "ih\u00b7nen", "ver\u00b7h\u00e4ngt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "KON", "ADJD", "$,", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nein, er verh\u00e4ngt nichts B\u00f6ses; doch l\u00e4\u00dft er wechseln die Tage,", "tokens": ["Nein", ",", "er", "ver\u00b7h\u00e4ngt", "nichts", "B\u00f6\u00b7ses", ";", "doch", "l\u00e4\u00dft", "er", "wech\u00b7seln", "die", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PIS", "NN", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Da\u00df Du im Wechsel lernst immer derselbige sein.", "tokens": ["Da\u00df", "Du", "im", "Wech\u00b7sel", "lernst", "im\u00b7mer", "der\u00b7sel\u00b7bi\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ADV", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Also schweift der Gesang in hoch- und niedrige Stimmen;", "tokens": ["Al\u00b7so", "schweift", "der", "Ge\u00b7sang", "in", "hoch", "und", "nied\u00b7ri\u00b7ge", "Stim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber Kalliope winkt, nie zu verlieren den Ton.", "tokens": ["A\u00b7ber", "Kal\u00b7li\u00b7o\u00b7pe", "winkt", ",", "nie", "zu", "ver\u00b7lie\u00b7ren", "den", "Ton", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbwie die Tage der Menschen, so ist der Menschen Gesinnung,", "tokens": ["\u00bb", "wie", "die", "Ta\u00b7ge", "der", "Men\u00b7schen", ",", "so", "ist", "der", "Men\u00b7schen", "Ge\u00b7sin\u00b7nung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ART", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wie sie, b\u00f6s oder gut, Jupiter ihnen verh\u00e4ngt.\u00ab", "tokens": ["Wie", "sie", ",", "b\u00f6s", "o\u00b7der", "gut", ",", "Ju\u00b7pi\u00b7ter", "ih\u00b7nen", "ver\u00b7h\u00e4ngt", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word"