{"dta.poem.12471": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Einkehr in die Schweiz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alles ringt sich von der Scholle,", "tokens": ["Al\u00b7les", "ringt", "sich", "von", "der", "Schol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles ist emporgestiegen;", "tokens": ["Al\u00b7les", "ist", "em\u00b7por\u00b7ge\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will am End' die bl\u00fctenvolle", "tokens": ["Will", "am", "End'", "die", "bl\u00fc\u00b7ten\u00b7vol\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erde in den Himmel fliegen?", "tokens": ["Er\u00b7de", "in", "den", "Him\u00b7mel", "flie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meiner Heimat Strand befeuchtend,", "tokens": ["Mei\u00b7ner", "Hei\u00b7mat", "Strand", "be\u00b7feuch\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4nzt vor mir des Seees Spiegel,", "tokens": ["Gl\u00e4nzt", "vor", "mir", "des", "See\u00b7es", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drauf die Sonne, purpurleuchtend,", "tokens": ["Drauf", "die", "Son\u00b7ne", ",", "pur\u00b7pur\u00b7leuch\u00b7tend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Liebe rothes Siegel.", "tokens": ["Wie", "der", "Lie\u00b7be", "ro\u00b7thes", "Sie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Und ich seh' in allen Fernen", "tokens": ["Und", "ich", "seh'", "in", "al\u00b7len", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Opfer von den Bergen rauchen,", "tokens": ["Op\u00b7fer", "von", "den", "Ber\u00b7gen", "rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich m\u00f6chte mit den Sternen", "tokens": ["Und", "ich", "m\u00f6ch\u00b7te", "mit", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Seele untertauchen.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "un\u00b7ter\u00b7tau\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und es dampfet durch die L\u00fcfte", "tokens": ["Und", "es", "damp\u00b7fet", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf zu mir des Thales Brodem,", "tokens": ["Auf", "zu", "mir", "des", "Tha\u00b7les", "Bro\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es duften alle D\u00fcfte,", "tokens": ["Und", "es", "duf\u00b7ten", "al\u00b7le", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie einst meiner Liebsten Odem.", "tokens": ["Wie", "einst", "mei\u00b7ner", "Liebs\u00b7ten", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fr\u00fchling unten, Fr\u00fchling oben", "tokens": ["Fr\u00fch\u00b7ling", "un\u00b7ten", ",", "Fr\u00fch\u00b7ling", "o\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "$,", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so Flur wie H\u00fcgel reicher;", "tokens": ["Und", "so", "Flur", "wie", "H\u00fc\u00b7gel", "rei\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KOKOM", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Noth hinweggehoben", "tokens": ["Al\u00b7le", "Noth", "hin\u00b7weg\u00b7ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Himmel selber weicher.", "tokens": ["Und", "der", "Him\u00b7mel", "sel\u00b7ber", "wei\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch kein Herz, das er, zu theilen,", "tokens": ["Doch", "kein", "Herz", ",", "das", "er", ",", "zu", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Seligkeit, mir g\u00f6nnte \u2014", "tokens": ["Die\u00b7se", "Se\u00b7lig\u00b7keit", ",", "mir", "g\u00f6nn\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so w\u00fcnsch' ich mir zuweilen", "tokens": ["Und", "so", "w\u00fcn\u00b7sch'", "ich", "mir", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Etwas, das ich ", "tokens": ["Et\u00b7was", ",", "das", "ich"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "PRELS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}}}}