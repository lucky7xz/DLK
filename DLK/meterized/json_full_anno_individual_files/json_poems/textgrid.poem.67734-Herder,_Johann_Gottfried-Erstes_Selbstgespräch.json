{"textgrid.poem.67734": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Selbstgespr\u00e4ch", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas ich gewesen, ward und k\u00f6nnte sein", "tokens": ["\u00bb", "was", "ich", "ge\u00b7we\u00b7sen", ",", "ward", "und", "k\u00f6nn\u00b7te", "sein"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "PPER", "VAPP", "$,", "VAFIN", "KON", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht bin?\u00ab O, ein Kn\u00e4ul von tausend Fragen,", "tokens": ["Und", "nicht", "bin", "?", "\u00ab", "O", ",", "ein", "Kn\u00e4ul", "von", "tau\u00b7send", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VAFIN", "$.", "$(", "NE", "$,", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vorw\u00fcrfen, Zweifeln, Selbstverdammung, Pein", "tokens": ["Vor\u00b7w\u00fcr\u00b7fen", ",", "Zwei\u00b7feln", ",", "Selbst\u00b7ver\u00b7dam\u00b7mung", ",", "Pein"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der innern Folter webt mich ein!", "tokens": ["Der", "in\u00b7nern", "Fol\u00b7ter", "webt", "mich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich erwach'! Gedankenloser Schlaf \u2013 und Du,", "tokens": ["Ich", "er\u00b7wach'", "!", "Ge\u00b7dan\u00b7ken\u00b7lo\u00b7ser", "Schlaf", "\u2013", "und", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "NN", "$(", "KON", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Traumvoller Schlaf \u2013 wo seid Ihr? Falsche Ruh,", "tokens": ["Traum\u00b7vol\u00b7ler", "Schlaf", "\u2013", "wo", "seid", "Ihr", "?", "Fal\u00b7sche", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PWAV", "VAFIN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "In die mich H\u00f6llenzephyrs wehten! Pestische Ruh!", "tokens": ["In", "die", "mich", "H\u00f6l\u00b7len\u00b7ze\u00b7phyrs", "weh\u00b7ten", "!", "Pes\u00b7ti\u00b7sche", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Der Lustschwei\u00df meines Traums wird Angstschwei\u00df! ach,", "tokens": ["Der", "Lust\u00b7schwei\u00df", "mei\u00b7nes", "Traums", "wird", "A\u00b7ngstschwei\u00df", "!", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "NN", "$.", "XY", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf meiner Stirn zu Eis! \u2013 Eis wie ein Fr\u00fchlingsbach", "tokens": ["Auf", "mei\u00b7ner", "Stirn", "zu", "Eis", "!", "\u2013", "Eis", "wie", "ein", "Fr\u00fch\u00b7lings\u00b7bach"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$.", "$(", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Vom nordlichen Hauch der kalten Mitternacht! \u2013", "tokens": ["Vom", "nord\u00b7li\u00b7chen", "Hauch", "der", "kal\u00b7ten", "Mit\u00b7ter\u00b7nacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Eis wie die Todesthr\u00e4n' im Aug' der Menschlichkeit! \u2013", "tokens": ["Eis", "wie", "die", "To\u00b7des\u00b7thr\u00e4n'", "im", "Aug'", "der", "Menschlich\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "So stirbt die kalte Thr\u00e4n' zu Stein! \u2013 \u2013 \u2013", "tokens": ["So", "stirbt", "die", "kal\u00b7te", "Thr\u00e4n'", "zu", "Stein", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich tr\u00e4ges Schilf an der Vergessenheit", "tokens": ["Ich", "tr\u00e4\u00b7ges", "Schilf", "an", "der", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ufer, gewiegt von stygischen Zephyrs, ach,", "tokens": ["U\u00b7fer", ",", "ge\u00b7wiegt", "von", "sty\u00b7gi\u00b7schen", "Ze\u00b7phyrs", ",", "ach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,", "ITJ", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Verwuchs im Staube \u2013 saust' der tr\u00fcben Nacht", "tokens": ["Ver\u00b7wuchs", "im", "Stau\u00b7be", "\u2013", "saust'", "der", "tr\u00fc\u00b7ben", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "M\u00fcssige Lieder, und jetzt, da Morgen beginnt,", "tokens": ["M\u00fcs\u00b7si\u00b7ge", "Lie\u00b7der", ",", "und", "jetzt", ",", "da", "Mor\u00b7gen", "be\u00b7ginnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADV", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Ich, von Hoffnungsthau entperlt, vom Strahl", "tokens": ["Ich", ",", "von", "Hoff\u00b7nungst\u00b7hau", "ent\u00b7perlt", ",", "vom", "Strahl"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "APPR", "NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Der Sonne gewelkt, sink' in der Sichel Arm", "tokens": ["Der", "Son\u00b7ne", "ge\u00b7welkt", ",", "sink'", "in", "der", "Si\u00b7chel", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und seufze: \u00bbMensch! Gott erbarm'!\u00ab", "tokens": ["Und", "seuf\u00b7ze", ":", "\u00bb", "Mensch", "!", "Gott", "er\u00b7barm'", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$.", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ein doppelt Ich! \u2013 Was bin ich denn? Ich? Nichts", "tokens": ["Ein", "dop\u00b7pelt", "Ich", "!", "\u2013", "Was", "bin", "ich", "denn", "?", "Ich", "?", "Nichts"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJD", "PPER", "$.", "$(", "PWS", "VAFIN", "PPER", "ADV", "$.", "PPER", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Halb Thier, das schl\u00e4ft und i\u00dft,", "tokens": ["Halb", "Thier", ",", "das", "schl\u00e4ft", "und", "i\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Halb Herz, das stets befiehlt, und nie geschieht's,", "tokens": ["Halb", "Herz", ",", "das", "stets", "be\u00b7fiehlt", ",", "und", "nie", "ge\u00b7schieht's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "KON", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Frech spornt und, eh es b\u00fc\u00dft,", "tokens": ["Frech", "spornt", "und", ",", "eh", "es", "b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schon bebt! denn, Herz, von Menschenfleisch gewebt,", "tokens": ["Schon", "bebt", "!", "denn", ",", "Herz", ",", "von", "Men\u00b7schen\u00b7fleisch", "ge\u00b7webt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ADV", "$,", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gekn\u00fcpft ins Erdenthum,", "tokens": ["Ge\u00b7kn\u00fcpft", "ins", "Er\u00b7den\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur schwach schl\u00e4gst Du; oft irrt, oft bebt,", "tokens": ["Nur", "schwach", "schl\u00e4gst", "Du", ";", "oft", "irrt", ",", "oft", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oft steht Dein Puls nach Ruhm.", "tokens": ["Oft", "steht", "Dein", "Puls", "nach", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn in Dir sonst ein Gott nie ruhig thront,", "tokens": ["Wenn", "in", "Dir", "sonst", "ein", "Gott", "nie", "ru\u00b7hig", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist Du mir leere Nu\u00df,", "tokens": ["Bist", "Du", "mir", "lee\u00b7re", "Nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo, schlafend jetzt, einst nagend, wohnt", "tokens": ["Wo", ",", "schla\u00b7fend", "jetzt", ",", "einst", "na\u00b7gend", ",", "wohnt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "$,", "ADJD", "ADV", "$,", "ADV", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wurm, der (o Verdru\u00df!)", "tokens": ["Ein", "Wurm", ",", "der", "(", "o", "Ver\u00b7dru\u00df", "!", ")"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$(", "FM", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Federn erste, Deinen st\u00e4rksten Ast,", "tokens": ["Der", "Fe\u00b7dern", "ers\u00b7te", ",", "Dei\u00b7nen", "st\u00e4rks\u00b7ten", "Ast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Luftbaus St\u00fctz', zernagt", "tokens": ["Des", "Luft\u00b7baus", "St\u00fctz'", ",", "zer\u00b7nagt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Moder, Schmach, Ruin! \u2013 O, fa\u00dft", "tokens": ["Zu", "Mo\u00b7der", ",", "Schmach", ",", "Ru\u00b7in", "!", "\u2013", "O", ",", "fa\u00dft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$.", "$(", "NE", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich Feuer! Ich f\u00fchl's, es tagt!", "tokens": ["Mich", "Feu\u00b7er", "!", "Ich", "f\u00fchl's", ",", "es", "tagt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ja! leben will ich und modern nicht! \u2013 Ich will! \u2013", "tokens": ["Ja", "!", "le\u00b7ben", "will", "ich", "und", "mo\u00b7dern", "nicht", "!", "\u2013", "Ich", "will", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$.", "VVINF", "VMFIN", "PPER", "KON", "ADJD", "PTKNEG", "$.", "$(", "PPER", "VMFIN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was Du und Gott, Dein G\u00f6ttlicher, spricht, steht da! \u2013", "tokens": ["Was", "Du", "und", "Gott", ",", "Dein", "G\u00f6tt\u00b7li\u00b7cher", ",", "spricht", ",", "steht", "da", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "KON", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Staub bin ich; denn Staub wollt' ich sein! \u2013 doch nah", "tokens": ["Staub", "bin", "ich", ";", "denn", "Staub", "wollt'", "ich", "sein", "!", "\u2013", "doch", "nah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "KON", "NN", "VMFIN", "PPER", "VAINF", "$.", "$(", "ADV", "ADJD"], "meter": "+---++-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Am dunkeln Feuermeer oben gebar sich still", "tokens": ["Am", "dun\u00b7keln", "Feu\u00b7er\u00b7meer", "o\u00b7ben", "ge\u00b7bar", "sich", "still"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Funke zum Gott mir, der mir gl\u00fcht", "tokens": ["Ein", "Fun\u00b7ke", "zum", "Gott", "mir", ",", "der", "mir", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In jeder Nerv'! (ich f\u00fchl's!) im Auge des Adlers sieht", "tokens": ["In", "je\u00b7der", "Ner\u00b7v'", "!", "(", "ich", "f\u00fchl's", "!", ")", "im", "Au\u00b7ge", "des", "Ad\u00b7lers", "sieht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "$(", "PPER", "VVFIN", "$.", "$(", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Nach H\u00f6hn, wovor sich kreuzten, schauderten, staunten Alle!", "tokens": ["Nach", "H\u00f6hn", ",", "wo\u00b7vor", "sich", "kreuz\u00b7ten", ",", "schau\u00b7der\u00b7ten", ",", "staun\u00b7ten", "Al\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PRF", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und ich! mein jedes Staubtheil ruft mit Schalle", "tokens": ["Und", "ich", "!", "mein", "je\u00b7des", "Staubt\u00b7heil", "ruft", "mit", "Schal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$.", "PPOSAT", "PIAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Herauf: \u00bbEin Mensch, ein Gott!\u00ab herauf!", "tokens": ["Her\u00b7auf", ":", "\u00bb", "Ein", "Mensch", ",", "ein", "Gott", "!", "\u00ab", "her\u00b7auf", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PTKVZ", "$.", "$(", "ART", "NN", "$,", "ART", "NN", "$.", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbwas ich gewesen, ward und k\u00f6nnte sein", "tokens": ["\u00bb", "was", "ich", "ge\u00b7we\u00b7sen", ",", "ward", "und", "k\u00f6nn\u00b7te", "sein"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "PPER", "VAPP", "$,", "VAFIN", "KON", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht bin?\u00ab O, ein Kn\u00e4ul von tausend Fragen,", "tokens": ["Und", "nicht", "bin", "?", "\u00ab", "O", ",", "ein", "Kn\u00e4ul", "von", "tau\u00b7send", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VAFIN", "$.", "$(", "NE", "$,", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vorw\u00fcrfen, Zweifeln, Selbstverdammung, Pein", "tokens": ["Vor\u00b7w\u00fcr\u00b7fen", ",", "Zwei\u00b7feln", ",", "Selbst\u00b7ver\u00b7dam\u00b7mung", ",", "Pein"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der innern Folter webt mich ein!", "tokens": ["Der", "in\u00b7nern", "Fol\u00b7ter", "webt", "mich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich erwach'! Gedankenloser Schlaf \u2013 und Du,", "tokens": ["Ich", "er\u00b7wach'", "!", "Ge\u00b7dan\u00b7ken\u00b7lo\u00b7ser", "Schlaf", "\u2013", "und", "Du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "NN", "$(", "KON", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Traumvoller Schlaf \u2013 wo seid Ihr? Falsche Ruh,", "tokens": ["Traum\u00b7vol\u00b7ler", "Schlaf", "\u2013", "wo", "seid", "Ihr", "?", "Fal\u00b7sche", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PWAV", "VAFIN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "In die mich H\u00f6llenzephyrs wehten! Pestische Ruh!", "tokens": ["In", "die", "mich", "H\u00f6l\u00b7len\u00b7ze\u00b7phyrs", "weh\u00b7ten", "!", "Pes\u00b7ti\u00b7sche", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Der Lustschwei\u00df meines Traums wird Angstschwei\u00df! ach,", "tokens": ["Der", "Lust\u00b7schwei\u00df", "mei\u00b7nes", "Traums", "wird", "A\u00b7ngstschwei\u00df", "!", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "NN", "$.", "XY", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf meiner Stirn zu Eis! \u2013 Eis wie ein Fr\u00fchlingsbach", "tokens": ["Auf", "mei\u00b7ner", "Stirn", "zu", "Eis", "!", "\u2013", "Eis", "wie", "ein", "Fr\u00fch\u00b7lings\u00b7bach"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$.", "$(", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Vom nordlichen Hauch der kalten Mitternacht! \u2013", "tokens": ["Vom", "nord\u00b7li\u00b7chen", "Hauch", "der", "kal\u00b7ten", "Mit\u00b7ter\u00b7nacht", "!", "\u2013"], "token_info": ["word", "word", "wo