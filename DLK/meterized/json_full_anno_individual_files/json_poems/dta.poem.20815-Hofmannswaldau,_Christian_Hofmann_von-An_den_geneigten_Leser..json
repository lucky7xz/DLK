{"dta.poem.20815": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den geneigten  \n Leser.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ziret n\u00fc der eren Sal?", "tokens": ["Wer", "zi\u00b7ret", "n\u00fc", "der", "e\u00b7ren", "Sal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jungen Ritter Z\u00fcht ist smal/", "tokens": ["Der", "jun\u00b7gen", "Rit\u00b7ter", "Z\u00fcht", "ist", "smal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So pfligent die Knehte gar unh\u00f6vishe Dinge/", "tokens": ["So", "pfli\u00b7gent", "die", "Kneh\u00b7te", "gar", "un\u00b7h\u00f6\u00b7vis\u00b7he", "Din\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit Worten und mit Werken \u00f6ch.", "tokens": ["Mit", "Wor\u00b7ten", "und", "mit", "Wer\u00b7ken", "\u00f6ch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Swer z\u00fcht hat der ist jo ir g\u00f6ch.", "tokens": ["Swer", "z\u00fcht", "hat", "der", "ist", "jo", "ir", "g\u00f6ch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VAFIN", "ART", "VAFIN", "NE", "NE", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Nemet war/ wie gar unf\u00fcge f\u00fcr sich dringe:", "tokens": ["Ne\u00b7met", "war", "/", "wie", "gar", "un\u00b7f\u00fc\u00b7ge", "f\u00fcr", "sich", "drin\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "$(", "KOKOM", "ADV", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Hie vor do bertet man die Jungen/", "tokens": ["Hie", "vor", "do", "ber\u00b7tet", "man", "die", "Jun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "VVFIN", "PIS", "ART", "NN", "$("], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Die da pflagen vrecher Zungen:", "tokens": ["Die", "da", "pfla\u00b7gen", "vre\u00b7cher", "Zun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nu ist es ire Werdekeit.", "tokens": ["Nu", "ist", "es", "i\u00b7re", "Wer\u00b7de\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie zieret man den Ehren-Saal?", "tokens": ["Wie", "zie\u00b7ret", "man", "den", "Eh\u00b7ren\u00b7Saal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jungen Ritter-Zucht ist schmal/", "tokens": ["Der", "jun\u00b7gen", "Rit\u00b7ter\u00b7Zucht", "ist", "schmal", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel ungeh\u00f6ftes Werck ist bey uns eingedrungen", "tokens": ["Viel", "un\u00b7ge\u00b7h\u00f6f\u00b7tes", "Werck", "ist", "bey", "uns", "ein\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wol mit Worten/ als mit That/", "tokens": ["So", "wol", "mit", "Wor\u00b7ten", "/", "als", "mit", "That", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$(", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der ist ein Jeck/ wer Tugend hat/", "tokens": ["Der", "ist", "ein", "Jeck", "/", "wer", "Tu\u00b7gend", "hat", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "$(", "PWS", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schaut doch den Unfug dieser Zeit/", "tokens": ["Schaut", "doch", "den", "Un\u00b7fug", "die\u00b7ser", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor strafte man die frechen Zungen/", "tokens": ["Vor", "straf\u00b7te", "man", "die", "fre\u00b7chen", "Zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jtzt ist es eine Zierligkeit.", "tokens": ["Jtzt", "ist", "es", "ei\u00b7ne", "Zier\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}