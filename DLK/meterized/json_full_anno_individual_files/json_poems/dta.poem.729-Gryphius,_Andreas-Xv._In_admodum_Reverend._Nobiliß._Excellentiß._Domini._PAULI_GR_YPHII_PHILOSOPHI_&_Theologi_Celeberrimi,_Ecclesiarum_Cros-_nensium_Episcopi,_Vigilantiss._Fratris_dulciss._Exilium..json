{"dta.poem.729": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  In admodum Reverend. Nobili\u00df. Excellenti\u00df.  \n Domini.  \n PAULI GR YPHII PHILOSOPHI &  \n Theologi Celeberrimi, Ecclesiarum Cros-  \n nensium Episcopi, Vigilantiss. Fratris  \n dulciss. Exilium.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der eyfers voll von Gott hat Tag vnd Nacht gelehret/ ", "tokens": ["Der", "ey\u00b7fers", "voll", "von", "Gott", "hat", "Tag", "vnd", "Nacht", "ge\u00b7leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Christus Lieb entz\u00fcnd't; den Gottes geist regirt/ ", "tokens": ["Den", "Chris\u00b7tus", "Lieb", "ent\u00b7z\u00fcnd't", ";", "den", "Got\u00b7tes", "geist", "re\u00b7girt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$.", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Christus S", "tokens": ["Der", "Chris\u00b7tus", "S"], "token_info": ["word", "word", "word"], "pos": ["ART", "NE", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dem offt die angst das Hertz/ vnd glutt das gut versehret/", "tokens": ["Dem", "offt", "die", "angst", "das", "Hertz", "/", "vnd", "glutt", "das", "gut", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ART", "NN", "$(", "KON", "ADJD", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der keiner Feinde glimpf/ noch schnauben je geh\u00f6ret/", "tokens": ["Der", "kei\u00b7ner", "Fein\u00b7de", "glimpf", "/", "noch", "schnau\u00b7ben", "je", "ge\u00b7h\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "ADV", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Tugend hat wie Gold durch so viel pein probirt/ ", "tokens": ["Den", "Tu\u00b7gend", "hat", "wie", "Gold", "durch", "so", "viel", "pein", "pro\u00b7birt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "APPR", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der einig nur gelebt als seiner lehr geb\u00fchrt/", "tokens": ["Der", "ei\u00b7nig", "nur", "ge\u00b7lebt", "als", "sei\u00b7ner", "lehr", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "KOKOM", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Wer\u00dfheit jhr erkiest/ den ", "tokens": ["Den", "Wer\u00df\u00b7heit", "jhr", "er\u00b7kiest", "/", "den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den hat der ", "tokens": ["Den", "hat", "der"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ins elendt? ey nicht so. Wann dieser na", "tokens": ["Ins", "e\u00b7lendt", "?", "ey", "nicht", "so", ".", "Wann", "die\u00b7ser", "na"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJD", "$.", "NE", "PTKNEG", "ADV", "$.", "PWAV", "PDAT", "ITJ"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.3": {"text": "Der das gew\u00f6lbte rundt der Erden aufgebawet; ", "tokens": ["Der", "das", "ge\u00b7w\u00f6lb\u00b7te", "rundt", "der", "Er\u00b7den", "auf\u00b7ge\u00b7ba\u00b7wet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "So mangelt nirgendt platz/ der dem di\u00df ", "tokens": ["So", "man\u00b7gelt", "nir\u00b7gendt", "platz", "/", "der", "dem", "di\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$(", "ART", "ART", "PDS"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das Vih vnd Menschen tr\u00e4gt/ zeucht in den Himmel ein", "tokens": ["Das", "Vih", "vnd", "Men\u00b7schen", "tr\u00e4gt", "/", "zeucht", "in", "den", "Him\u00b7mel", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$(", "VVFIN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der vns zum Vaterlandt vnd wohnung an vertrawet. ", "tokens": ["Der", "vns", "zum", "Va\u00b7ter\u00b7landt", "vnd", "woh\u00b7nung", "an", "ver\u00b7tra\u00b7wet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}