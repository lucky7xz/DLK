{"textgrid.poem.66725": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Finster der Bergr\u00fccken;", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Finster der Bergr\u00fccken;", "tokens": ["Fins\u00b7ter", "der", "Berg\u00b7r\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Schwarzem Ungeheuer gleich,", "tokens": ["Schwar\u00b7zem", "Un\u00b7ge\u00b7heu\u00b7er", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Langgelagert,", "tokens": ["Lang\u00b7ge\u00b7la\u00b7gert", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Droht er her\u00fcber.", "tokens": ["Droht", "er", "her\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Also lastet und schiebt das Schicksal", "tokens": ["Al\u00b7so", "las\u00b7tet", "und", "schiebt", "das", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sich dem ungl\u00fcckseligen Menschen", "tokens": ["Sich", "dem", "un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7gen", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Grausam n\u00e4her, furchtbar nah ...", "tokens": ["Grau\u00b7sam", "n\u00e4\u00b7her", ",", "furcht\u