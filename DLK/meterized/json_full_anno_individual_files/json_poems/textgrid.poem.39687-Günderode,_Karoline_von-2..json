{"textgrid.poem.39687": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Liljenleib des Purpurs dunkler Schleier", "tokens": ["Den", "Lil\u00b7jen\u00b7leib", "des", "Pur\u00b7purs", "dunk\u00b7ler", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem irren Blick der G\u00f6ttin halb entzieht;", "tokens": ["Dem", "ir\u00b7ren", "Blick", "der", "G\u00f6t\u00b7tin", "halb", "ent\u00b7zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Trauer Bild, die Anemone, bl\u00fcht", "tokens": ["Der", "Trau\u00b7er", "Bild", ",", "die", "A\u00b7ne\u00b7mo\u00b7ne", ",", "bl\u00fcht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wei\u00df als roth zur stillen Todtenfeyer.", "tokens": ["So", "wei\u00df", "als", "roth", "zur", "stil\u00b7len", "Tod\u00b7ten\u00b7fe\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Erloschen ist in Ihm des Lebens Feuer,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "in", "Ihm", "des", "Le\u00b7bens", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], 