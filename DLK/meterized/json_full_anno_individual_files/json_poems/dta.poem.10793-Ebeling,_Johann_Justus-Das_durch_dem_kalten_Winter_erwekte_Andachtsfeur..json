{"dta.poem.10793": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das durch dem kalten Winter  \n erwekte Andachtsfeur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Jahres Nacht, die Winters Zeit,", "tokens": ["Des", "Jah\u00b7res", "Nacht", ",", "die", "Win\u00b7ters", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erd und Wasser \u00fcberstreut,", "tokens": ["Die", "Erd", "und", "Was\u00b7ser", "\u00fc\u00b7bers\u00b7treut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit den gefrornen Reif und D\u00fcf-", "tokens": ["Mit", "den", "ge\u00b7fror\u00b7nen", "Reif", "und", "D\u00fcf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus den verdikten schwarzen L\u00fcf-", "tokens": ["Aus", "den", "ver\u00b7dik\u00b7ten", "schwar\u00b7zen", "L\u00fcf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schliest mich bei einem warmen Schein", "tokens": ["Schliest", "mich", "bei", "ei\u00b7nem", "war\u00b7men", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Ofens, in mein Zimmer ein,", "tokens": ["Des", "O\u00b7fens", ",", "in", "mein", "Zim\u00b7mer", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ich kein Toben kalter Winde,", "tokens": ["Wo", "ich", "kein", "To\u00b7ben", "kal\u00b7ter", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein frostig Ungemach empfinde.", "tokens": ["Kein", "fros\u00b7tig", "Un\u00b7ge\u00b7mach", "emp\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sehe durch der Fenster Glas,", "tokens": ["Ich", "se\u00b7he", "durch", "der", "Fens\u00b7ter", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne scheinet tr\u00fcb und blas,", "tokens": ["Die", "Son\u00b7ne", "schei\u00b7net", "tr\u00fcb", "und", "blas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Durch die beflokten L\u00fcfte sausen;", "tokens": ["Durch", "die", "be\u00b7flok\u00b7ten", "L\u00fcf\u00b7te", "sau\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die schwangern Wolken brechen los,", "tokens": ["Die", "schwan\u00b7gern", "Wol\u00b7ken", "bre\u00b7chen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Na\u00df das durch die Luft abflo\u00df,", "tokens": ["Das", "Na\u00df", "das", "durch", "die", "Luft", "ab\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird in dem Tr\u00f6pfeln dicht gefroren;", "tokens": ["Wird", "in", "dem", "Tr\u00f6p\u00b7feln", "dicht", "ge\u00b7fro\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es wird draus Schnee und Eis gebohren.", "tokens": ["Es", "wird", "draus", "Schnee", "und", "Eis", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich denk in warmer Ruhe nach,", "tokens": ["Ich", "denk", "in", "war\u00b7mer", "Ru\u00b7he", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schaudrigt sey das Ungemach,", "tokens": ["Wie", "schaud\u00b7rigt", "sey", "das", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das da die Flokken scherzend spielen,", "tokens": ["Das", "da", "die", "Flok\u00b7ken", "scher\u00b7zend", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die starren Glieder draussen f\u00fchlen;", "tokens": ["Die", "star\u00b7ren", "Glie\u00b7der", "draus\u00b7sen", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die K\u00e4lte und das zakkigt Eis,", "tokens": ["Die", "K\u00e4l\u00b7te", "und", "das", "zak\u00b7kigt", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht mich in reger Andacht hei\u00df,", "tokens": ["Macht", "mich", "in", "re\u00b7ger", "An\u00b7dacht", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Da ich Gottlob von Fern nur sehe", "tokens": ["Da", "ich", "Gott\u00b7lob", "von", "Fern", "nur", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des kalten Winters frostig Wehe.", "tokens": ["Des", "kal\u00b7ten", "Win\u00b7ters", "fros\u00b7tig", "We\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchle in der muntren Brust,", "tokens": ["Ich", "f\u00fch\u00b7le", "in", "der", "mun\u00b7tren", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Dichten eine rege Lust,", "tokens": ["Zum", "Dich\u00b7ten", "ei\u00b7ne", "re\u00b7ge", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sanftes Wohl durchkreucht die Glieder:", "tokens": ["Ein", "sanf\u00b7tes", "Wohl", "durch\u00b7kreucht", "die", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum sez ich mich beim Ofen nieder,", "tokens": ["Drum", "sez", "ich", "mich", "beim", "O\u00b7fen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Worin die Flamme lodernd schl\u00e4gt:", "tokens": ["Wo\u00b7rin", "die", "Flam\u00b7me", "lo\u00b7dernd", "schl\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herz wird durch das Feur bewegt", "tokens": ["Mein", "Herz", "wird", "durch", "das", "Feur", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und durch des Holzes reges Brennen,", "tokens": ["Und", "durch", "des", "Hol\u00b7zes", "re\u00b7ges", "Bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dran ", "tokens": ["Dran"], "token_info": ["word"], "pos": ["PAV"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Mein GOtt! bei der erstarrten Welt,", "tokens": ["Mein", "Gott", "!", "bei", "der", "er\u00b7starr\u00b7ten", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da mit den schroffen Eis das Feld", "tokens": ["Da", "mit", "den", "schrof\u00b7fen", "Eis", "das", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wie mit Schollen \u00fcberbr\u00fckket,", "tokens": ["Als", "wie", "mit", "Schol\u00b7len", "\u00fc\u00b7berb\u00b7r\u00fck\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd ich durch warme Lust erquikket.", "tokens": ["Werd", "ich", "durch", "war\u00b7me", "Lust", "er\u00b7quik\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich preise deine G\u00fctigkeit,", "tokens": ["Ich", "prei\u00b7se", "dei\u00b7ne", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ruhiger Zufriedenheit,", "tokens": ["In", "ru\u00b7hi\u00b7ger", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Mir deucht da\u00df alle diese Flammen", "tokens": ["Mir", "deucht", "da\u00df", "al\u00b7le", "die\u00b7se", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Feurs, aus deiner Liebe stammen.", "tokens": ["Des", "Feurs", ",", "aus", "dei\u00b7ner", "Lie\u00b7be", "stam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja! Ja, dies heisse Element,", "tokens": ["Ja", "!", "Ja", ",", "dies", "heis\u00b7se", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PTKANT", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das w\u00e4rmet, leuchtet, strahlt und brennt,", "tokens": ["Das", "w\u00e4r\u00b7met", ",", "leuch\u00b7tet", ",", "strahlt", "und", "brennt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fchrt uns HErr! deine Wunder-", "tokens": ["F\u00fchrt", "uns", "Herr", "!", "dei\u00b7ne", "Wun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$.", "PPOSAT", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Winterszeit recht ins Gem\u00fcte.", "tokens": ["Zur", "Win\u00b7ter\u00b7szeit", "recht", "ins", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir m\u00fcsten in dem Frost vergehn,", "tokens": ["Wir", "m\u00fcs\u00b7ten", "in", "dem", "Frost", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00f6nten nicht darin bestehn;", "tokens": ["Und", "k\u00f6n\u00b7ten", "nicht", "da\u00b7rin", "be\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn nicht das Feur uns k\u00f6nte w\u00e4rmen,", "tokens": ["Wenn", "nicht", "das", "Feur", "uns", "k\u00f6n\u00b7te", "w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bei der verdikten L\u00fcfte Schw\u00e4rmen.", "tokens": ["Bei", "der", "ver\u00b7dik\u00b7ten", "L\u00fcf\u00b7te", "Schw\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch mehr erkenn ich deine Macht,", "tokens": ["Noch", "mehr", "er\u00b7kenn", "ich", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Holz zum Feur hervor gebracht,", "tokens": ["Die", "Holz", "zum", "Feur", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das was verbrennlichs an sich heget,", "tokens": ["Das", "was", "ver\u00b7brenn\u00b7lichs", "an", "sich", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil etwas schweflichts drin geleget.", "tokens": ["Weil", "et\u00b7was", "schwef\u00b7lichts", "drin", "ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das r\u00fchrt ja alles HErr! von dir,", "tokens": ["Das", "r\u00fchrt", "ja", "al\u00b7les", "Herr", "!", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "$.", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies stelt uns deine ", "tokens": ["Dies", "stelt", "uns", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wenn ich auf deiner Huld Geschenke,", "tokens": ["Wenn", "ich", "auf", "dei\u00b7ner", "Huld", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Winterszeit die Augen lenke.", "tokens": ["Zur", "Win\u00b7ter\u00b7szeit", "die", "Au\u00b7gen", "len\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", 