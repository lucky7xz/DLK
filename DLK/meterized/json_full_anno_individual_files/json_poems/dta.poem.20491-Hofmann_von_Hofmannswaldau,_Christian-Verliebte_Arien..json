{"dta.poem.20491": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Flavia/ mich brennen deine thr\u00e4nen/", "tokens": ["Ge\u00b7lieb\u00b7te", "Fla\u00b7via", "/", "mich", "bren\u00b7nen", "dei\u00b7ne", "thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das s\u00fcsse na\u00df beflammet meinen Geist/", "tokens": ["Das", "s\u00fcs\u00b7se", "na\u00df", "be\u00b7flam\u00b7met", "mei\u00b7nen", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein heisser seuffzer macht mir jammer-reiches sehnen/", "tokens": ["Dein", "heis\u00b7ser", "seuff\u00b7zer", "macht", "mir", "jam\u00b7mer\u00b7rei\u00b7ches", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mich zu dir mit steiffen banden reist;", "tokens": ["So", "mich", "zu", "dir", "mit", "steif\u00b7fen", "ban\u00b7den", "reist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPER", "APPR", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wilstu/ o Flavia/ da\u00df Citron soll verderben/", "tokens": ["Wils\u00b7tu", "/", "o", "Fla\u00b7via", "/", "da\u00df", "Cit\u00b7ron", "soll", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "FM", "NE", "$(", "KOUS", "NE", "VMFIN", "VVFIN", "$("], "meter": "++-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "So sage nichts von deinem todt und sterben.", "tokens": ["So", "sa\u00b7ge", "nichts", "von", "dei\u00b7nem", "todt", "und", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist noch keine zeit im grabe zu verwesen/", "tokens": ["Es", "ist", "noch", "kei\u00b7ne", "zeit", "im", "gra\u00b7be", "zu", "ver\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jugend steht dir allzuzierlich an/", "tokens": ["Die", "ju\u00b7gend", "steht", "dir", "all\u00b7zu\u00b7zier\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du solt noch in der welt die zucker-rosen lesen/", "tokens": ["Du", "solt", "noch", "in", "der", "welt", "die", "zu\u00b7cke\u00b7rro\u00b7sen", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo deine hand kein dorn verletzen kan/", "tokens": ["Wo", "dei\u00b7ne", "hand", "kein", "dorn", "ver\u00b7let\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die wollust wird dir tranck aus ihrem becher schicken/", "tokens": ["Die", "wol\u00b7lust", "wird", "dir", "tranck", "aus", "ih\u00b7rem", "be\u00b7cher", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und selber dich an ihre br\u00fcste dr\u00fccken.", "tokens": ["Und", "sel\u00b7ber", "dich", "an", "ih\u00b7re", "br\u00fcs\u00b7te", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es soll dich noch zur zeit kein schwartzer boy umschliessen/", "tokens": ["Es", "soll", "dich", "noch", "zur", "zeit", "kein", "schwart\u00b7zer", "boy", "um\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein tugend-glantz ist etwas bessers werth/", "tokens": ["Dein", "tu\u00b7gen\u00b7dglantz", "ist", "et\u00b7was", "bes\u00b7sers", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es wird der himmel dich als seine tochter k\u00fcssen/", "tokens": ["Es", "wird", "der", "him\u00b7mel", "dich", "als", "sei\u00b7ne", "toch\u00b7ter", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der deinen mund zu seiner lust begehrt/", "tokens": ["Der", "dei\u00b7nen", "mund", "zu", "sei\u00b7ner", "lust", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df einen kleinen sturm dich nicht zu sehr bewegen/", "tokens": ["La\u00df", "ei\u00b7nen", "klei\u00b7nen", "sturm", "dich", "nicht", "zu", "sehr", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PPER", "PTKNEG", "PTKA", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein sonnen-strahl kan alles wieder legen.", "tokens": ["Ein", "son\u00b7nen\u00b7strahl", "kan", "al\u00b7les", "wie\u00b7der", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Erhebe doch nunmehr die fl\u00fcgel deiner sinnen/", "tokens": ["Er\u00b7he\u00b7be", "doch", "nun\u00b7mehr", "die", "fl\u00fc\u00b7gel", "dei\u00b7ner", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und st\u00f6hre selbst den trieb der ungedult/", "tokens": ["Und", "st\u00f6h\u00b7re", "selbst", "den", "trieb", "der", "un\u00b7ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df nicht die thr\u00e4nen-bach um deine wangen rinnen/", "tokens": ["La\u00df", "nicht", "die", "thr\u00e4\u00b7nen\u00b7bach", "um", "dei\u00b7ne", "wan\u00b7gen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sch\u00f6ne feld ist frey von aller schuld/", "tokens": ["Das", "sch\u00f6\u00b7ne", "feld", "ist", "frey", "von", "al\u00b7ler", "schuld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "PIAT", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was wiltu unverdient es unter wasser setzen/", "tokens": ["Was", "wil\u00b7tu", "un\u00b7ver\u00b7di\u00b7ent", "es", "un\u00b7ter", "was\u00b7ser", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "PPER", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die tyranney ist straffens werth zu sch\u00e4tzen.", "tokens": ["Die", "ty\u00b7ran\u00b7ney", "ist", "straf\u00b7fens", "werth", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Soll die verzweiffelung dir deine rosen bleichen/", "tokens": ["Soll", "die", "ver\u00b7zweif\u00b7fe\u00b7lung", "dir", "dei\u00b7ne", "ro\u00b7sen", "blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So um den thau der s\u00fcssen lippen stehn?", "tokens": ["So", "um", "den", "thau", "der", "s\u00fcs\u00b7sen", "lip\u00b7pen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts als Jesmin-geruch soll diesen ort bestreichen/", "tokens": ["Nichts", "als", "Jes\u00b7min\u00b7ge\u00b7ruch", "soll", "die\u00b7sen", "ort", "be\u00b7strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "VMFIN", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein purpur mu\u00df von dessen grentzen gehn/", "tokens": ["Kein", "pur\u00b7pur", "mu\u00df", "von", "des\u00b7sen", "grent\u00b7zen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VMFIN", "APPR", "PRELAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Um diese gegend mu\u00df sich lust und anmuth k\u00fcssen/", "tokens": ["Um", "die\u00b7se", "ge\u00b7gend", "mu\u00df", "sich", "lust", "und", "an\u00b7muth", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "VMFIN", "PRF", "VVFIN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und lieblichkeit dir deine zeit vers\u00fcssen.", "tokens": ["Und", "lieb\u00b7lich\u00b7keit", "dir", "dei\u00b7ne", "zeit", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ach edle Flavia! leg\u2019 angst und kummer nieder/", "tokens": ["Ach", "ed\u00b7le", "Fla\u00b7via", "!", "leg'", "angst", "und", "kum\u00b7mer", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "VVFIN", "ADJD", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es speise dich des gl\u00fcckes \u00fcberflu\u00df/", "tokens": ["Es", "spei\u00b7se", "dich", "des", "gl\u00fc\u00b7ckes", "\u00fc\u00b7berf\u00b7lu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zerrei\u00df in freudigkeit die kalten trauer-lieder/", "tokens": ["Zer\u00b7rei\u00df", "in", "freu\u00b7dig\u00b7keit", "die", "kal\u00b7ten", "trau\u00b7e\u00b7rlie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch die dein knecht als ei\u00df erstarren mu\u00df/", "tokens": ["Durch", "die", "dein", "knecht", "als", "ei\u00df", "er\u00b7star\u00b7ren", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "KOKOM", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer ihm das hertze frist/ und selbst sein fleisch verzehret/", "tokens": ["Wer", "ihm", "das", "hert\u00b7ze", "frist", "/", "und", "selbst", "sein", "fleisch", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "$(", "KON", "ADV", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der hat f\u00fcr sich zu theure kost begehret.", "tokens": ["Der", "hat", "f\u00fcr", "sich", "zu", "theu\u00b7re", "kost", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Komm braune nacht/ umh\u00fclle mich mit schatten/", "tokens": ["Komm", "brau\u00b7ne", "nacht", "/", "um\u00b7h\u00fcl\u00b7le", "mich", "mit", "schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "VVFIN", "PRF", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und decke den mit deiner schw\u00e4rtze zu/", "tokens": ["Und", "de\u00b7cke", "den", "mit", "dei\u00b7ner", "schw\u00e4rt\u00b7ze", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPR", "PPOSAT", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ungest\u00f6hrt sich will mit sonnen gatten/", "tokens": ["Der", "un\u00b7ge\u00b7st\u00f6hrt", "sich", "will", "mit", "son\u00b7nen", "gat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und im bezirck der engel suchet ruh/", "tokens": ["Und", "im", "be\u00b7zirck", "der", "en\u00b7gel", "su\u00b7chet", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja hilff mein ach/ eh du noch wirst verschwinden/", "tokens": ["Ja", "hilff", "mein", "ach", "/", "eh", "du", "noch", "wirst", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "PPOSAT", "XY", "$(", "KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit linder hand von meiner seele binden.", "tokens": ["Mit", "lin\u00b7der", "hand", "von", "mei\u00b7ner", "see\u00b7le", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wie/ h\u00f6r\u2019 ich nicht/ willkommen mein verlangen!", "tokens": ["Wie", "/", "h\u00f6r'", "ich", "nicht", "/", "will\u00b7kom\u00b7men", "mein", "ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VVFIN", "PPER", "PTKNEG", "$(", "ADJD", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon im gemach mit leiser stimme gehn?", "tokens": ["Schon", "im", "ge\u00b7mach", "mit", "lei\u00b7ser", "stim\u00b7me", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADV", "APPR", "ADJA", "NN", "