{"textgrid.poem.64094": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mann mu\u00df bald zur\u00fcck vom Walde kehren: \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann mu\u00df bald zur\u00fcck vom Walde kehren: \u2013", "tokens": ["Der", "Mann", "mu\u00df", "bald", "zu\u00b7r\u00fcck", "vom", "Wal\u00b7de", "keh\u00b7ren", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKVZ", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sammelt Reisholz: \u2013 lieblich neigt der Wind,", "tokens": ["Er", "sam\u00b7melt", "Reis\u00b7holz", ":", "\u2013", "lieb\u00b7lich", "neigt", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "$(", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Abendwind, des hohen Grases \u00c4hren", "tokens": ["Der", "Ab\u00b7end\u00b7wind", ",", "des", "ho\u00b7hen", "Gra\u00b7ses", "\u00c4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und spielt im lichten Haare meinem Kind.", "tokens": ["Und", "spielt", "im", "lich\u00b7ten", "Haa\u00b7re", "mei\u00b7nem", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie schl\u00e4fst du s\u00fc\u00df, mein Sohn, und schlingst noch fest", "tokens": ["Wie", "schl\u00e4fst", "du", "s\u00fc\u00df", ",", "mein", "Sohn", ",", "und", "schlingst", "noch", "fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Schlaf um meinen Hals den weichen Arm:", "tokens": ["Im", "Schlaf", "um", "mei\u00b7nen", "Hals", "den", "wei\u00b7chen", "Arm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht f\u00fcrchte, da\u00df die Mutter dich verl\u00e4\u00dft \u2013:", "tokens": ["Nicht", "f\u00fcrch\u00b7te", ",", "da\u00df", "die", "Mut\u00b7ter", "dich", "ver\u00b7l\u00e4\u00dft", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin bei dir: an meiner Brust ist's warm. \u2013", "tokens": ["Ich", "bin", "bei", "dir", ":", "an", "mei\u00b7ner", "Brust", "ist's", "warm", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$.", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von Osten her, da leuchtet ferner Schimmer \u2013", "tokens": ["Von", "Os\u00b7ten", "her", ",", "da", "leuch\u00b7tet", "fer\u00b7ner", "Schim\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Eden sind's die hohen goldnen Tore:", "tokens": ["Von", "E\u00b7den", "sin\u00b7d's", "die", "ho\u00b7hen", "gold\u00b7nen", "To\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die schlanken Edelpalmen seh' ich nimmer,", "tokens": ["Die", "schlan\u00b7ken", "E\u00b7del\u00b7pal\u00b7men", "seh'", "ich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dort umbl\u00fchet stehn von buntem Flore.", "tokens": ["Die", "dort", "um\u00b7bl\u00fc\u00b7het", "stehn", "von", "bun\u00b7tem", "Flo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n war es dort! Viel heller schien die Sonne \u2013!", "tokens": ["Sch\u00f6n", "war", "es", "dort", "!", "Viel", "hel\u00b7ler", "schien", "die", "Son\u00b7ne", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$.", "ADV", "ADJD", "VVFIN", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, anfangs wollte mir das Herz vergehn", "tokens": ["Ach", ",", "an\u00b7fangs", "woll\u00b7te", "mir", "das", "Herz", "ver\u00b7gehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um jenes Gartens wunderhafte Wonne,", "tokens": ["Um", "je\u00b7nes", "Gar\u00b7tens", "wun\u00b7der\u00b7haf\u00b7te", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchlt' ich von dorther s\u00fc\u00dfe D\u00fcfte wehn.", "tokens": ["F\u00fchlt'", "ich", "von", "dor\u00b7ther", "s\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun aber schweigt mir l\u00e4ngst dies eitle Sehnen:", "tokens": ["Nun", "a\u00b7ber", "schweigt", "mir", "l\u00e4ngst", "dies", "eit\u00b7le", "Seh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, du, mein Kind, hast mich davon befreit:", "tokens": ["Du", ",", "du", ",", "mein", "Kind", ",", "hast", "mich", "da\u00b7von", "be\u00b7freit", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht geb' ich meiner Mutterliebe Tr\u00e4nen", "tokens": ["Nicht", "geb'", "ich", "mei\u00b7ner", "Mut\u00b7ter\u00b7lie\u00b7be", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um jenes Paradieses Seligkeit.", "tokens": ["Um", "je\u00b7nes", "Pa\u00b7ra\u00b7die\u00b7ses", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn du mich eng umschlingst mit zarten Armen,", "tokens": ["Wenn", "du", "mich", "eng", "um\u00b7schlingst", "mit", "zar\u00b7ten", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dr\u00fcckt unsre Schuld und Gottes Fluch mich minder:", "tokens": ["Dr\u00fcckt", "uns\u00b7re", "Schuld", "und", "Got\u00b7tes", "Fluch", "mich", "min\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchl's: Gott ist ein ewiges Erbarmen:", "tokens": ["Ich", "f\u00fchl's", ":", "Gott", "ist", "ein", "e\u00b7wi\u00b7ges", "Er\u00b7bar\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er liebt uns auch, denn wir sind seine Kinder!", "tokens": ["Er", "liebt", "uns", "auch", ",", "denn", "wir", "sind", "sei\u00b7ne", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schon flutet D\u00e4mmrung \u00fcber Edens Toren:", "tokens": ["Schon", "flu\u00b7tet", "D\u00e4mm\u00b7rung", "\u00fc\u00b7ber", "E\u00b7dens", "To\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da k\u00f6mmt mein Gatte: still, Freund, schreite sacht:", "tokens": ["Da", "k\u00f6mmt", "mein", "Gat\u00b7te", ":", "still", ",", "Freund", ",", "schrei\u00b7te", "sacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "PTKVZ", "$,", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schl\u00e4ft das Kindlein, das ich dir geboren,", "tokens": ["Es", "schl\u00e4ft", "das", "Kin\u00b7dlein", ",", "das", "ich", "dir", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O k\u00fcsse leise, da\u00df es nicht erwacht!", "tokens": ["O", "k\u00fcs\u00b7se", "lei\u00b7se", ",", "da\u00df", "es", "nicht", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Mann mu\u00df bald zur\u00fcck vom Walde kehren: \u2013", "tokens": ["Der", "Mann", "mu\u00df", "bald", "zu\u00b7r\u00fcck", "vom", "Wal\u00b7de", "keh\u00b7ren", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKVZ", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "