{"dta.poem.7285": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Feuer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Dunkel! All Dunkel schwer!", "tokens": ["Dun\u00b7kel", "!", "All", "Dun\u00b7kel", "schwer", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PIAT", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie Riesen schreiten Wolken her \u2014", "tokens": ["Wie", "Rie\u00b7sen", "schrei\u00b7ten", "Wol\u00b7ken", "her"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ueber Gras und Laub", "tokens": ["Ue\u00b7ber", "Gras", "und", "Laub"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wirbelt's wie schwarzer Staub;", "tokens": ["Wir\u00b7belt's", "wie", "schwar\u00b7zer", "Staub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier und dort ein grauer Stamm;", "tokens": ["Hier", "und", "dort", "ein", "grau\u00b7er", "Stamm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Am Horizont des Berges Kamm", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "des", "Ber\u00b7ges", "Kamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00e4lt die gespenstige Wacht,", "tokens": ["H\u00e4lt", "die", "ge\u00b7spens\u00b7ti\u00b7ge", "Wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Sonst Alles Nacht \u2014 Nacht \u2014 nur Nacht.", "tokens": ["Sonst", "Al\u00b7les", "Nacht", "Nacht", "nur", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "NN", "$(", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was blitzt dort auf? \u2014 ein rother Stern \u2014", "tokens": ["Was", "blitzt", "dort", "auf", "?", "ein", "ro\u00b7ther", "Stern"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PTKVZ", "$.", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nun scheint es nah, nun wieder fern;", "tokens": ["Nun", "scheint", "es", "nah", ",", "nun", "wie\u00b7der", "fern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schau! wie es zuckt und zuckt und schweift,", "tokens": ["Schau", "!", "wie", "es", "zuckt", "und", "zuckt", "und", "schweift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wie's ringelnd gleich der Schlange pfeift.", "tokens": ["Wie's", "rin\u00b7gelnd", "gleich", "der", "Schlan\u00b7ge", "pfeift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun am Gem\u00e4uer glimmt es auf,", "tokens": ["Nun", "am", "Ge\u00b7m\u00e4u\u00b7er", "glimmt", "es", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Unwillig wirft's die Asch hinauf,", "tokens": ["Un\u00b7wil\u00b7lig", "wirft's", "die", "Asch", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wirbelnd \u00fcber'm Dach hervor", "tokens": ["Und", "wir\u00b7belnd", "\u00fc\u00b7ber'm", "Dach", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Funkens\u00e4ule steigt empor.", "tokens": ["Die", "Fun\u00b7ken\u00b7s\u00e4u\u00b7le", "steigt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dort der Mann im ru\u00df'gen Kleid,", "tokens": ["Und", "dort", "der", "Mann", "im", "ru\u00df'\u00b7gen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2014 Sein Angesicht ist bleich und kalt,", "tokens": ["Sein", "An\u00b7ge\u00b7sicht", "ist", "bleich", "und", "kalt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Bild der listigen Gewalt \u2014", "tokens": ["Ein", "Bild", "der", "lis\u00b7ti\u00b7gen", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er die Flamme d\u00e4mpft und facht,", "tokens": ["Wie", "er", "die", "Flam\u00b7me", "d\u00e4mpft", "und", "facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00e4lt den Eisenblock bereit!", "tokens": ["Und", "h\u00e4lt", "den", "Ei\u00b7sen\u00b7block", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den soll ihm die gefang'ne Macht,", "tokens": ["Den", "soll", "ihm", "die", "ge\u00b7fang'\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die wilde hartbez\u00e4hmte Glut", "tokens": ["Die", "wil\u00b7de", "hart\u00b7be\u00b7z\u00e4hm\u00b7te", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zermalmen gleich in ihrer Wuth.", "tokens": ["Zer\u00b7mal\u00b7men", "gleich", "in", "ih\u00b7rer", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau, wie das Feuer sich zersplittert!", "tokens": ["Schau", ",", "wie", "das", "Feu\u00b7er", "sich", "zer\u00b7split\u00b7tert", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie's t\u00fcckisch an der Kohle knittert!", "tokens": ["Wie's", "t\u00fc\u00b7ckisch", "an", "der", "Koh\u00b7le", "knit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lang aus die rothe Kralle streckt", "tokens": ["Lang", "aus", "die", "ro\u00b7the", "Kral\u00b7le", "streckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nach dem Kerkermeister reckt!", "tokens": ["Und", "nach", "dem", "Ker\u00b7ker\u00b7meis\u00b7ter", "reckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie's vor verhaltnem Grimme zittert:", "tokens": ["Wie's", "vor", "ver\u00b7halt\u00b7nem", "Grim\u00b7me", "zit\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eo, h\u00e4tt' ich dich, o k\u00f6nnte ich", "tokens": ["\u201e", "o", ",", "h\u00e4tt'", "ich", "dich", ",", "o", "k\u00f6nn\u00b7te", "ich"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "$,", "VAFIN", "PPER", "PRF", "$,", "FM", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit meinen Klauen fassen dich!", "tokens": ["Mit", "mei\u00b7nen", "Klau\u00b7en", "fas\u00b7sen", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich lehrte dich den Unterschied", "tokens": ["Ich", "lehr\u00b7te", "dich", "den", "Un\u00b7ter\u00b7schied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von dir zu Elementes Zier,", "tokens": ["Von", "dir", "zu", "E\u00b7le\u00b7men\u00b7tes", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "An deinem morschen, staub'gen Glied,", "tokens": ["An", "dei\u00b7nem", "mor\u00b7schen", ",", "staub'\u00b7gen", "Glied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du ruchlos Menschenthier!", "tokens": ["Du", "ruch\u00b7los", "Men\u00b7schen\u00b7thier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}