{"dta.poem.17422": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "43.  Wie GOtt so sehr gerecht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Schau GOtt ist so gerecht: W\u00e4r\u2019 etwas \u00fcber jhn/", "tokens": ["Schau", "Gott", "ist", "so", "ge\u00b7recht", ":", "W\u00e4r'", "et\u00b7was", "\u00fc\u00b7ber", "jhn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "ADJD", "$.", "VAFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ehrt, es mehr als sich/ und kniete f\u00fcr dem hin.", "tokens": ["Er", "ehrt", ",", "es", "mehr", "als", "sich", "/", "und", "knie\u00b7te", "f\u00fcr", "dem", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "KOUS", "PRF", "$(", "KON", "VVFIN", "APPR", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}