{"textgrid.poem.24086": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Reichtum", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Perlen gleiten durch meine Hand \u2013:", "tokens": ["Per\u00b7len", "glei\u00b7ten", "durch", "mei\u00b7ne", "Hand", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Das war Wasser, das verschwand;", "tokens": ["Das", "war", "Was\u00b7ser", ",", "das", "ver\u00b7schwand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gold kam \u00fcber mich hergelaufen \u2013:", "tokens": ["Gold", "kam", "\u00fc\u00b7ber", "mich", "her\u00b7ge\u00b7lau\u00b7fen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "VVPP", "$(", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wolkenberge, Wolkenhaufen;", "tokens": ["Wol\u00b7ken\u00b7ber\u00b7ge", ",", "Wol\u00b7ken\u00b7hau\u00b7fen", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts ist mehr in meiner Hand,", "tokens": ["Nichts", "ist", "mehr", "in", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich kann mir garnichts kaufen,", "tokens": ["Und", "ich", "kann", "mir", "gar\u00b7nichts",