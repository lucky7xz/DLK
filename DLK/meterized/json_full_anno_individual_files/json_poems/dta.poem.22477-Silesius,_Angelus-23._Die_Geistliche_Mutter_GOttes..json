{"dta.poem.22477": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "23. Die Geistliche Mutter GOttes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Marien Demut wird von GOtt so werth gesch\u00e4tzt/", "tokens": ["Ma\u00b7ri\u00b7en", "De\u00b7mut", "wird", "von", "Gott", "so", "werth", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "APPR", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Da\u00df Er auch selbst jhr Kind zu seyn sich hoch erg\u00f6tzt:", "tokens": ["Da\u00df", "Er", "auch", "selbst", "jhr", "Kind", "zu", "seyn", "sich", "hoch", "er\u00b7g\u00f6tzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "PTKZU", "VAINF", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bistu dem\u00fcttiglich wie eine Jungfrau rein:", "tokens": ["Bis\u00b7tu", "de\u00b7m\u00fct\u00b7tig\u00b7lich", "wie", "ei\u00b7ne", "Jung\u00b7frau", "rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird GOtt bald dein Kind/ du seine Mutter sein.", "tokens": ["So", "wird", "Gott", "bald", "dein", "Kind", "/", "du", "sei\u00b7ne", "Mut\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "A