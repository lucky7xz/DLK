{"dta.poem.11295": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vorzug des herrn B. N. vor dem  \n Homer.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es soll was wunderlichs in Neukirchs augen seyn,", "tokens": ["Es", "soll", "was", "wun\u00b7der\u00b7lichs", "in", "Neu\u00b7kirchs", "au\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADJA", "APPR", "NE", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df der Homerus sich an seine zeit gebunden.", "tokens": ["Da\u00df", "der", "Ho\u00b7me\u00b7rus", "sich", "an", "sei\u00b7ne", "zeit", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Allein mich nimmt hiebey gantz kein verwundern ein;", "tokens": ["Al\u00b7lein", "mich", "nimmt", "hie\u00b7bey", "gantz", "kein", "ver\u00b7wun\u00b7dern", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich habe grund genung f\u00fcr meine meinung funden:", "tokens": ["Ich", "ha\u00b7be", "grund", "ge\u00b7nung", "f\u00fcr", "mei\u00b7ne", "mei\u00b7nung", "fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn solte dort ein held in dem getichte stehn,", "tokens": ["Denn", "sol\u00b7te", "dort", "ein", "held", "in", "dem", "ge\u00b7tich\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So m\u00fcsten fratz\u2019 und l\u00fcg\u2019 in einem paare gehn;", "tokens": ["So", "m\u00fcs\u00b7ten", "fratz'", "und", "l\u00fcg'", "in", "ei\u00b7nem", "paa\u00b7re", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer aber Friedrichs ruhm will versen einverleiben,", "tokens": ["Wer", "a\u00b7ber", "Fried\u00b7richs", "ruhm", "will", "ver\u00b7sen", "ein\u00b7ver\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NE", "NN", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der mu\u00df so wahr als klug, wie Neukirch selber, schreiben.", "tokens": ["Der", "mu\u00df", "so", "wahr", "als", "klug", ",", "wie", "Neu\u00b7kirch", "sel\u00b7ber", ",", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$,", "PWAV", "NN", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}