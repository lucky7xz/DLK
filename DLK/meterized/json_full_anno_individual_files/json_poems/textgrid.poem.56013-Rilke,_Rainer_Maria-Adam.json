{"textgrid.poem.56013": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Adam", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Staunend steht er an der Kathedrale", "tokens": ["Stau\u00b7nend", "steht", "er", "an", "der", "Ka\u00b7thed\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "steilem Aufstieg, nah der Fensterrose,", "tokens": ["stei\u00b7lem", "Auf\u00b7stieg", ",", "nah", "der", "Fens\u00b7ter\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wie erschreckt von der Apotheose,", "tokens": ["wie", "er\u00b7schreckt", "von", "der", "A\u00b7pot\u00b7he\u00b7o\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "welche wuchs und ihn mit einem Male", "tokens": ["wel\u00b7che", "wuchs", "und", "ihn", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "niederstellte \u00fcber die und die.", "tokens": ["nie\u00b7der\u00b7stell\u00b7te", "\u00fc\u00b7ber", "die", "und", "die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "KON", "ART", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und er ragt und freut sich seiner Dauer", "tokens": ["Und", "er", "ragt", "und", "freut", "sich", "sei\u00b7ner", "Dau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "schlicht entschlossen; als der Ackerbauer", "tokens": ["schlicht", "ent\u00b7schlos\u00b7sen", ";", "als", "der", "A\u00b7cker\u00b7bau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "der begann, und der nicht wu\u00dfte, wie", "tokens": ["der", "be\u00b7gann", ",", "und", "der", "nicht", "wu\u00df\u00b7te", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "VVFIN", "$,", "KON", "ART", "PTKNEG", "VVFIN", "$,", "PWAV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "aus dem fertig-vollen Garten Eden", "tokens": ["aus", "dem", "fer\u00b7tig\u00b7vol\u00b7len", "Gar\u00b7ten", "E\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "einen Ausweg in die neue Erde", "tokens": ["ei\u00b7nen", "Aus\u00b7weg", "in", "die", "neu\u00b7e", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "finden. Gott war schwer zu \u00fcberreden;", "tokens": ["fin\u00b7den", ".", "Gott", "war", "schwer", "zu", "\u00fc\u00b7berr\u00b7e\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$.", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "und er drohte ihm, statt zu gew\u00e4hren,", "tokens": ["und", "er", "droh\u00b7te", "ihm", ",", "statt", "zu", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "immer wieder, da\u00df er sterben werde.", "tokens": ["im\u00b7mer", "wie\u00b7der", ",", "da\u00df", "er", "ster\u00b7ben", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch der Mensch bestand: sie wird geb\u00e4ren.", "tokens": ["Doch", "der", "Mensch", "be\u00b7stand", ":", "sie", "wird", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Staunend steht er an der Kathedrale", "tokens": ["Stau\u00b7nend", "steht", "er", "an", "der", "Ka\u00b7thed\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "steilem Aufstieg, nah der Fensterrose,", "tokens": ["stei\u00b7lem", "Auf\u00b7stieg", ",", "nah", "der", "Fens\u00b7ter\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wie erschreckt von der Apotheose,", "tokens": ["wie", "er\u00b7schreckt", "von", "der", "A\u00b7pot\u00b7h