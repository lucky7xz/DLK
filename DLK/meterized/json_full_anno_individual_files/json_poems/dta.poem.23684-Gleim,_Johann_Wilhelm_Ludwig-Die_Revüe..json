{"dta.poem.23684": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die  Rev\u00fce.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was lieb ich doch f&#252;r Sch\u00f6nen?", "tokens": ["Was", "lieb", "ich", "doch", "f", "&#252;", "r", "Sch\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "FM", "FM", "FM", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich liebe die Helenen,", "tokens": ["Ich", "lie\u00b7be", "die", "He\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Hanchen und die Fiekchen,", "tokens": ["Die", "Han\u00b7chen", "und", "die", "Fiek\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lischen und die Miekchen,", "tokens": ["Die", "Li\u00b7schen", "und", "die", "Miek\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Willigen, die Spr\u00f6den,", "tokens": ["Die", "Wil\u00b7li\u00b7gen", ",", "die", "Spr\u00f6\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Freundlichen, die Bl\u00f6den,", "tokens": ["Die", "Freund\u00b7li\u00b7chen", ",", "die", "Bl\u00f6\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Z\u00e4rtlichen, die Netten,", "tokens": ["Die", "Z\u00e4rt\u00b7li\u00b7chen", ",", "die", "Net\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Schlanken, die Brunetten.", "tokens": ["Die", "Schlan\u00b7ken", ",", "die", "Bru\u00b7net\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich liebe die Blondinen,", "tokens": ["Ich", "lie\u00b7be", "die", "Blon\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mit zarten Venusminen,", "tokens": ["Mit", "zar\u00b7ten", "Ve\u00b7nus\u00b7mi\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und die mit treuen Herzen,", "tokens": ["Und", "die", "mit", "treu\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und die so witzig scherzen,", "tokens": ["Und", "die", "so", "wit\u00b7zig", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und die mit edlen Seelen,", "tokens": ["Und", "die", "mit", "ed\u00b7len", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Die mich zum Schatz erw\u00e4len.", "tokens": ["Die", "mich", "zum", "Schatz", "er\u00b7w\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ich hasse nur die Sch\u00f6nen,", "tokens": ["Ich", "has\u00b7se", "nur", "die", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die dich, o Liebe, h\u00f6nen,", "tokens": ["Die", "dich", ",", "o", "Lie\u00b7be", ",", "h\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Die mit nicht edlen Trieben,", "tokens": ["Die", "mit", "nicht", "ed\u00b7len", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und die, so mich nicht lieben.", "tokens": ["Und", "die", ",", "so", "mich", "nicht", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}