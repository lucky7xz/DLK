{"textgrid.poem.55631": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[im Atemholen sind zweierlei Gnaden]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Atemholen sind zweierlei Gnaden:", "tokens": ["Im", "A\u00b7tem\u00b7ho\u00b7len", "sind", "zwei\u00b7er\u00b7lei", "Gna\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Luft einziehen, sich ihrer entladen;", "tokens": ["Die", "Luft", "ein\u00b7zie\u00b7hen", ",", "sich", "ih\u00b7rer", "ent\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRF", "PPOSAT", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jenes bedr\u00e4ngt, dieses erfrischt;", "tokens": ["Je\u00b7nes", "be\u00b7dr\u00e4ngt", ",", "die\u00b7ses", "er\u00b7frischt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So wunderbar ist das Leben gemischt.", "tokens": ["So", "wun\u00b7der\u00b7bar", "ist", "das", "Le\u00b7ben", "ge\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du danke Gott, wenn er dich pre\u00dft,", "tokens": ["Du", "dan\u00b7ke", "Gott", ",", "wenn", "er", "dich", "pre\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOU