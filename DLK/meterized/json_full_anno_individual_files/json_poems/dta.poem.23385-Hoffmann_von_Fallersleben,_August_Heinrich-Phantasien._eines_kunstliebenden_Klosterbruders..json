{"dta.poem.23385": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Phantasien.  \n  eines kunstliebenden Klosterbruders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die freie Kunst, sie ist nicht frei:", "tokens": ["Die", "frei\u00b7e", "Kunst", ",", "sie", "ist", "nicht", "frei", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Freiheit nicht vorhanden,", "tokens": ["Wo", "Frei\u00b7heit", "nicht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist es mit der Kunst vorbei", "tokens": ["Da", "ist", "es", "mit", "der", "Kunst", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen, allen Landen.", "tokens": ["In", "al\u00b7len", ",", "al\u00b7len", "Lan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und buhlt sie auch um F\u00fcrstengunst,", "tokens": ["Und", "buhlt", "sie", "auch", "um", "F\u00fcrs\u00b7ten\u00b7gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das kann ihr wenig frommen!", "tokens": ["Das", "kann", "ihr", "we\u00b7nig", "from\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch F\u00fcrsten ist herab die Kunst,", "tokens": ["Durch", "F\u00fcrs\u00b7ten", "ist", "her\u00b7ab", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nie emporgekommen.", "tokens": ["Doch", "nie", "em\u00b7por\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer nicht in Freiheit wirkt und schafft,", "tokens": ["Wer", "nicht", "in", "Frei\u00b7heit", "wirkt", "und", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann F\u00fcrsten wohl gen\u00fcgen,", "tokens": ["Kann", "F\u00fcrs\u00b7ten", "wohl", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wird er stets um Geist und Kraft", "tokens": ["Doch", "wird", "er", "stets", "um", "Geist", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich und die Welt betr\u00fcgen.", "tokens": ["Sich", "und", "die", "Welt", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur aus dem Volk, ins Volk zur\u00fcck", "tokens": ["Nur", "aus", "dem", "Volk", ",", "ins", "Volk", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df stets der K\u00fcnstler steigen,", "tokens": ["Mu\u00df", "stets", "der", "K\u00fcnst\u00b7ler", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird im Volke Ruhm und Gl\u00fcck", "tokens": ["Dann", "wird", "im", "Vol\u00b7ke", "Ruhm", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem K\u00fcnstler auch zu eigen.", "tokens": ["Dem", "K\u00fcnst\u00b7ler", "auch", "zu", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}