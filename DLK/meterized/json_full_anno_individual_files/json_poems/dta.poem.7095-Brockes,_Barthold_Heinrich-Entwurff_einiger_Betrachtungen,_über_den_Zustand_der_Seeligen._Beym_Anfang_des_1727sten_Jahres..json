{"dta.poem.7095": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Seelen edelste und beste Krafft, ", "tokens": ["Der", "See\u00b7len", "e\u00b7dels\u00b7te", "und", "bes\u00b7te", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird eine F\u00e4higkeit und eine seel\u2019ge F\u00fclle,", "tokens": ["Wird", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit", "und", "ei\u00b7ne", "seel'\u00b7ge", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den edelsten Besitz der Ruh empfangen.", "tokens": ["Den", "e\u00b7dels\u00b7ten", "Be\u00b7sitz", "der", "Ruh", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Es ist die Wissenschafft, Erfahrung und Erkenntni\u00df,", "tokens": ["Es", "ist", "die", "Wis\u00b7sen\u00b7schafft", ",", "Er\u00b7fah\u00b7rung", "und", "Er\u00b7kennt\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Begriff, Betrachtung und Verst\u00e4ndni\u00df", "tokens": ["Be\u00b7griff", ",", "Be\u00b7trach\u00b7tung", "und", "Ver\u00b7st\u00e4nd\u00b7ni\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Mittel nur; und von der Seeligkeit", "tokens": ["Ein", "Mit\u00b7tel", "nur", ";", "und", "von", "der", "See\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die w\u00fcrckliche Vollkommenheit", "tokens": ["Die", "w\u00fcrck\u00b7li\u00b7che", "Voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch lange nicht, die blo\u00df in der Verwunderung,", "tokens": ["Noch", "lan\u00b7ge", "nicht", ",", "die", "blo\u00df", "in", "der", "Ver\u00b7wun\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In der entz\u00fcckenden Belustigung,", "tokens": ["In", "der", "ent\u00b7z\u00fc\u00b7cken\u00b7den", "Be\u00b7lus\u00b7ti\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So aus so herrlicher Erkenntni\u00df stammt und gehet,", "tokens": ["So", "aus", "so", "herr\u00b7li\u00b7cher", "Er\u00b7kennt\u00b7ni\u00df", "stammt", "und", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und dem dadurch erzeugten Triebe,", "tokens": ["Und", "dem", "da\u00b7durch", "er\u00b7zeug\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der ewig seel\u2019gen Sch\u00f6pffers Liebe,", "tokens": ["Der", "e\u00b7wig", "seel'\u00b7gen", "Sch\u00f6pf\u00b7fers", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als einem Mittel-Punct der Seeligkeit, bestehet.", "tokens": ["Als", "ei\u00b7nem", "Mit\u00b7tel\u00b7Punct", "der", "See\u00b7lig\u00b7keit", ",", "be\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}