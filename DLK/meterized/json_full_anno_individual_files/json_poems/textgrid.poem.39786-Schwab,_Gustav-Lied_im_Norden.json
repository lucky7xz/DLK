}, "line.4": {"text": "Nach den stillen Wangen sehen,", "tokens": ["Nach", "den", "stil\u00b7len", "Wan\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dem braunen Augenpaar.", "tokens": ["Und", "dem", "brau\u00b7nen", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Waldesb\u00e4ume, rauscht ihr wieder?", "tokens": ["Wal\u00b7des\u00b7b\u00e4u\u00b7me", ",", "rauscht", "ihr", "wie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so sprecht: gedenkt sie mein?", "tokens": ["Nun", "so", "sprecht", ":", "ge\u00b7denkt", "sie", "mein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$.", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr lispelt tr\u00fcbe Lieder,", "tokens": ["Doch", "ihr", "lis\u00b7pelt", "tr\u00fc\u00b7be", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beugt die H\u00e4upter sch\u00fcttelnd nieder,", "tokens": ["Beugt", "die", "H\u00e4up\u00b7ter", "sch\u00fct\u00b7telnd", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie zu einem langen ", "tokens": ["Wie", "zu", "ei\u00b7nem", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Ferne durch die Luft getragen", "tokens": ["Fer\u00b7ne", "durch", "die", "Luft", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht der Ostsee Klang daher;", "tokens": ["Rauscht", "der", "Ost\u00b7see", "Klang", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Gang noch will ich wagen,", "tokens": ["Die\u00b7sen", "Gang", "noch", "will", "ich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Wellen nach mir schlagen,", "tokens": ["Wenn", "die", "Wel\u00b7len", "nach", "mir", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werf' ich Lieb' und Schmerz ins Meer.", "tokens": ["Wer\u00b7f'", "ich", "Lieb'", "und", "Schmerz", "ins", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}