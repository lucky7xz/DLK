{"textgrid.poem.39786": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Lied im Norden", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stehen denn die nord'schen Winde", "tokens": ["Ste\u00b7hen", "denn", "die", "nord'\u00b7schen", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das fremde Baumgesaus", "tokens": ["Und", "das", "frem\u00b7de", "Baum\u00b7ge\u00b7saus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch im Bund mit meinem Kinde,", "tokens": ["Auch", "im", "Bund", "mit", "mei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie sprechen leise, linde,", "tokens": ["Da\u00df", "sie", "spre\u00b7chen", "lei\u00b7se", ",", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fcdlich ihren Namen aus?", "tokens": ["S\u00fcd\u00b7lich", "ih\u00b7ren", "Na\u00b7men", "aus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weil ich einmal sie verloren,", "tokens": ["Weil", "ich", "ein\u00b7mal", "sie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollt' ich hin, wo's von ihr schweigt,", "tokens": ["Wollt'", "ich", "hin", ",", "wo's", "von", "ihr", "schweigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "$,", "PWAV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Aber unheraufbeschworen", "tokens": ["A\u00b7ber", "un\u00b7her\u00b7auf\u00b7be\u00b7schwo\u00b7ren"], "token_info": ["word", "word"], "pos": ["KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klingt der Nam' in meinen Ohren,", "tokens": ["Klingt", "der", "Nam'", "in", "mei\u00b7nen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und allw\u00e4rts ihr Bild sich zeigt.", "tokens": ["Und", "all\u00b7w\u00e4rts", "ihr", "Bild", "sich", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn ich durch ein D\u00f6rflein ziehe,", "tokens": ["Wenn", "ich", "durch", "ein", "D\u00f6r\u00b7flein", "zie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abends in dem letzten Schein,", "tokens": ["A\u00b7bends", "in", "dem", "letz\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft's wohl hier und da: ", "tokens": ["Ruft's", "wohl", "hier", "und", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "KON", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie ich zitt're, wie ich gl\u00fche,", "tokens": ["Wie", "ich", "zitt'\u00b7re", ",", "wie", "ich", "gl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Glaube gar, Sie wird es sein!", "tokens": ["Glau\u00b7be", "gar", ",", "Sie", "wird", "es", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber die dann k\u00f6mmt gegangen,", "tokens": ["A\u00b7ber", "die", "dann", "k\u00f6mmt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt ein fremdes Angesicht,", "tokens": ["Tr\u00e4gt", "ein", "frem\u00b7des", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blaues Auge, rote Wangen,", "tokens": ["Blau\u00b7es", "Au\u00b7ge", ",", "ro\u00b7te", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blonder Haare seidnes Prangen, \u2013", "tokens": ["Blon\u00b7der", "Haa\u00b7re", "seid\u00b7nes", "Pran\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach du bist die Meine nicht!", "tokens": ["Ach", "du", "bist", "die", "Mei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weiter mu\u00df ich einsam gehen,", "tokens": ["Wei\u00b7ter", "mu\u00df", "ich", "ein\u00b7sam", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem dunkeln Lockenhaar,", "tokens": ["Nach", "dem", "dun\u00b7keln", "Lo\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig mit verlornem Sp\u00e4hen", "tokens": ["E\u00b7wig", "mit", "ver\u00b7lor\u00b7nem", "Sp\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den stillen Wangen sehen,", "tokens": ["Nach", "den", "stil\u00b7len", "Wan\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dem braunen Augenpaar.", "tokens": ["Und", "dem", "brau\u00b7nen", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Waldesb\u00e4ume, rauscht ihr wieder?", "tokens": ["Wal\u00b7des\u00b7b\u00e4u\u00b7me", ",", "rauscht", "ihr", "wie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so sprecht: gedenkt sie mein?", "tokens": ["Nun", "so", "sprecht", ":", "ge\u00b7denkt", "sie", "mein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$.", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihr lispelt tr\u00fcbe Lieder,", "tokens": ["Doch", "ihr", "lis\u00b7pelt", "tr\u00fc\u00b7be", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beugt die H\u00e4upter sch\u00fcttelnd nieder,", "tokens": ["Beugt", "die", "H\u00e4up\u00b7ter", "sch\u00fct\u00b7telnd", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie zu einem langen ", "tokens": ["Wie", "zu", "ei\u00b7nem", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Ferne durch die Luft getragen", "tokens": ["Fer\u00b7ne", "durch", "die", "Luft", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht der Ostsee Klang daher;", "tokens": ["Rauscht", "der", "Ost\u00b7see", "Klang", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"