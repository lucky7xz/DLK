{"dta.poem.18964": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Filodor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ozartes Liebelein/", "tokens": ["Oz\u00b7ar\u00b7tes", "Lie\u00b7be\u00b7lein", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O s\u00fcsses Diebelein/", "tokens": ["O", "s\u00fcs\u00b7ses", "Die\u00b7be\u00b7lein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein hertz thut sich erwaichen/", "tokens": ["Dein", "hertz", "thut", "sich", "er\u00b7wai\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich mag nicht mehr verblaichen", "tokens": ["Ich", "mag", "nicht", "mehr", "ver\u00b7blai\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch hofnung-lose pein.", "tokens": ["Durch", "hof\u00b7nung\u00b7lo\u00b7se", "pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Darumb so la\u00df vns h\u00f6ren", "tokens": ["Da\u00b7rumb", "so", "la\u00df", "vns", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie meiner Liebe lehr/", "tokens": ["Wie", "mei\u00b7ner", "Lie\u00b7be", "lehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie deiner Sch\u00f6nheit ehr", "tokens": ["Wie", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "ehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die W\u00e4ld vnd Feld vermehren.", "tokens": ["Die", "W\u00e4ld", "vnd", "Feld", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}