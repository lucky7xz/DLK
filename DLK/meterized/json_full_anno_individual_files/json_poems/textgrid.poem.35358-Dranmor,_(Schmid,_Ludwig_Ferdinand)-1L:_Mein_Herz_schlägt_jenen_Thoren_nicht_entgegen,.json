{"textgrid.poem.35358": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz schl\u00e4gt jenen Thoren nicht entgegen,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz schl\u00e4gt jenen Thoren nicht entgegen,", "tokens": ["Mein", "Herz", "schl\u00e4gt", "je\u00b7nen", "Tho\u00b7ren", "nicht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Alltagsmenschen, die mit weiten Taschen", "tokens": ["Den", "All\u00b7tags\u00b7men\u00b7schen", ",", "die", "mit", "wei\u00b7ten", "Ta\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nach Mammons gn\u00e4digen Geschenken haschen", "tokens": ["Nach", "Mam\u00b7mons", "gn\u00e4\u00b7di\u00b7gen", "Ge\u00b7schen\u00b7ken", "ha\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf breitgetretnen, wohlbekannten Wegen.", "tokens": ["Auf", "breit\u00b7ge\u00b7tret\u00b7nen", ",", "wohl\u00b7be\u00b7kann\u00b7ten", "We\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer sich erg\u00f6tzt an unhaltbarem Flitter,", "tokens": ["Wer", "sich", "er\u00b7g\u00f6tzt", "an", "un\u00b7halt\u00b7ba\u00b7rem", "Flit\u00b7ter", ","], "token_info": ["word", "word", "word",