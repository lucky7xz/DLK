{"dta.poem.3694": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Beim Spaziergang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steig auf das Bergle,               ", "tokens": ["Steig", "auf", "das", "Berg\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Fall aber nit herab,", "tokens": ["Fall", "a\u00b7ber", "nit", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O herzig liebs Sch\u00e4tzle,", "tokens": ["O", "her\u00b7zig", "liebs", "Sch\u00e4tz\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Brichs F\u00fc\u00dfle nit ab!", "tokens": ["Brichs", "F\u00fc\u00df\u00b7le", "nit", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}