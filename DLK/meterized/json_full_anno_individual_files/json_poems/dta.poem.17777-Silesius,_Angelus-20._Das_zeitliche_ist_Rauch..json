{"dta.poem.17777": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "20.  Das zeitliche ist Rauch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Alls zeitlich\u2019 ist ein Rauch. L\u00e4stu es in dein Hau\u00df/", "tokens": ["Alls", "zeit\u00b7lich'", "ist", "ein", "Rauch", ".", "L\u00e4s\u00b7tu", "es", "in", "dein", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$.", "NE", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So beist es dir f\u00fcrwahr des Geistes Augen au\u00df.", "tokens": ["So", "beist", "es", "dir", "f\u00fcr\u00b7wahr", "des", "Geis\u00b7tes", "Au\u00b7gen", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}