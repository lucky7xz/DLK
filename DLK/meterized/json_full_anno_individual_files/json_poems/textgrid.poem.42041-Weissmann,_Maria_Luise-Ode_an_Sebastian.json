{"textgrid.poem.42041": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ode an Sebastian", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh, Du warst Baum! Darinnen V\u00f6gel schliefen.", "tokens": ["Oh", ",", "Du", "warst", "Baum", "!", "Da\u00b7rin\u00b7nen", "V\u00f6\u00b7gel", "schlie\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "NE", "$.", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Winde sich hold verm\u00e4hlten. Leoparden bogen k\u00fchl.", "tokens": ["Win\u00b7de", "sich", "hold", "ver\u00b7m\u00e4hl\u00b7ten", ".", "Le\u00b7o\u00b7par\u00b7den", "bo\u00b7gen", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "VVINF", "$.", "NE", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Ein Lamm, Gew\u00f6lk, lag leicht an Dich gebettet,", "tokens": ["Ein", "Lamm", ",", "Ge\u00b7w\u00f6lk", ",", "lag", "leicht", "an", "Dich", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "VVFIN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch warst Du weit, da\u00df fernster St\u00e4dte Dach", "tokens": ["Auch", "warst", "Du", "weit", ",", "da\u00df", "ferns\u00b7ter", "St\u00e4d\u00b7te", "Dach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch Deiner Zweige Schatten \u00fcberw\u00f6lbte.", "tokens": ["Noch", "Dei\u00b7ner", "Zwei\u00b7ge", "Schat\u00b7ten", "\u00fc\u00b7berw\u00b7\u00f6lb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Oh, Du warst weit! Ich konnte Deinen Wurzeln, die", "tokens": ["Oh", ",", "Du", "warst", "weit", "!", "Ich", "konn\u00b7te", "Dei\u00b7nen", "Wur\u00b7zeln", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Ball, verspielte H\u00e4nde, eng umschlangen, nicht", "tokens": ["Den", "Ball", ",", "ver\u00b7spiel\u00b7te", "H\u00e4n\u00b7de", ",", "eng", "um\u00b7schlan\u00b7gen", ",", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,", "ADJD", "VVINF", "$,", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Entgehen; Kn\u00f6chel sank und blutete betr\u00e4nt.", "tokens": ["Ent\u00b7ge\u00b7hen", ";", "Kn\u00f6\u00b7chel", "sank", "und", "blu\u00b7te\u00b7te", "be\u00b7tr\u00e4nt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "VVFIN", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und Du warst gro\u00df! Es hing der Abendstern an Dir", "tokens": ["Und", "Du", "warst", "gro\u00df", "!", "Es", "hing", "der", "A\u00b7bends\u00b7tern", "an", "Dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"