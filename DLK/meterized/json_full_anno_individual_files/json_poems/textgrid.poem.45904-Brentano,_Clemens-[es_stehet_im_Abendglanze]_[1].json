rd", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da seh' ich sie sitzen im Garten", "tokens": ["Da", "seh'", "ich", "sie", "sit\u00b7zen", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wohl bei einem reichen Mann.", "tokens": ["Wohl", "bei", "ei\u00b7nem", "rei\u00b7chen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "So kauf' ich mir Harke und Spaten,", "tokens": ["So", "kauf'", "ich", "mir", "Har\u00b7ke", "und", "Spa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bind' mir ein gr\u00fcn Sch\u00fcrzelein vor.", "tokens": ["Bind'", "mir", "ein", "gr\u00fcn", "Sch\u00fcr\u00b7zel\u00b7ein", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich stell' mich als w\u00e4r ich der G\u00e4rtner", "tokens": ["Ich", "stell'", "mich", "als", "w\u00e4r", "ich", "der", "G\u00e4rt\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und klopf' bei dem Reichen ans Tor.", "tokens": ["Und", "klopf'", "bei", "dem", "Rei\u00b7chen", "ans", "Tor", "."], "