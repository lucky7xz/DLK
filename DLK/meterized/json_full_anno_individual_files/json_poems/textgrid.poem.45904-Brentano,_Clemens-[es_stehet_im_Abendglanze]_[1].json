, "measure": "amphibrach.tri"}, "line.2": {"text": "Sind heilig und wissen es nicht,", "tokens": ["Sind", "hei\u00b7lig", "und", "wis\u00b7sen", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie spielen mit Kr\u00e4nzen und Kronen", "tokens": ["Sie", "spie\u00b7len", "mit", "Kr\u00e4n\u00b7zen", "und", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Allt\u00e4glich ein neues Gedicht.", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "ein", "neu\u00b7es", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.24": {"line.1": {"text": "Sie sind gleich den G\u00f6ttern und handlen", "tokens": ["Sie", "sind", "gleich", "den", "G\u00f6t\u00b7tern", "und", "hand\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Allt\u00e4glich in andrer Gestalt,", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "in", "an\u00b7drer", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Liebchen wird auch sich verwandlen", "tokens": ["Mein", "Lieb\u00b7chen", "wird", "auch", "sich", "ver\u00b7wand\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das tut meinem Herzen Gewalt.", "tokens": ["