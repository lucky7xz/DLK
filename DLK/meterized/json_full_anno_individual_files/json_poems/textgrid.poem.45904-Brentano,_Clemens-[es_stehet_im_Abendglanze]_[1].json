word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verflecht sie zu Netzen und Schlingen,", "tokens": ["Ver\u00b7flecht", "sie", "zu", "Net\u00b7zen", "und", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich habe ein V\u00f6gelchen hier.", "tokens": ["Ich", "ha\u00b7be", "ein", "V\u00f6\u00b7gel\u00b7chen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.30": {"line.1": {"text": "Zieh h\u00f6her und dicht mir die Laube,", "tokens": ["Zieh", "h\u00f6\u00b7her", "und", "dicht", "mir", "die", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zieh mir ein gitternes Haus,", "tokens": ["Zieh", "mir", "ein", "git\u00b7ter\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df keiner das V\u00f6gelchen raube,", "tokens": ["Da\u00df", "kei\u00b7ner", "das", "V\u00f6\u00b7gel\u00b7chen", "rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da\u00df es nicht fliege heraus.", "tokens": ["Da\u00df", "es", "nicht", "flie\u00b7ge", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PT