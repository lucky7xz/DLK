{"textgrid.poem.45904": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[es stehet im Abendglanze] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stehet im Abendglanze", "tokens": ["Es", "ste\u00b7het", "im", "A\u00b7bend\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein hochgeweihtes Haus,", "tokens": ["Ein", "hoch\u00b7ge\u00b7weih\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sehen mit schimmernden Augen", "tokens": ["Da", "se\u00b7hen", "mit", "schim\u00b7mern\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Viel Knaben und Jungfraun heraus.", "tokens": ["Viel", "Kna\u00b7ben", "und", "Jung\u00b7fraun", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie wechslen mit Weinen und Lachen,", "tokens": ["Sie", "wechs\u00b7len", "mit", "Wei\u00b7nen", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie wechslen mit Dunkel und Hell,", "tokens": ["Sie", "wechs\u00b7len", "mit", "Dun\u00b7kel", "und", "Hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit schimmernden Augen und Wangen", "tokens": ["Mit", "schim\u00b7mern\u00b7den", "Au\u00b7gen", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie wechslen ihre R\u00f6cklein gar schnell! \u2013", "tokens": ["Sie", "wechs\u00b7len", "ih\u00b7re", "R\u00f6ck\u00b7lein", "gar", "schnell", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Dort hab' ich mein Liebchen gesehen", "tokens": ["Dort", "hab'", "ich", "mein", "Lieb\u00b7chen", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein freundliches zierliches Kind;", "tokens": ["Ein", "freund\u00b7li\u00b7ches", "zier\u00b7li\u00b7ches", "Kind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie konnte wohl schweben und drehen", "tokens": ["Sie", "konn\u00b7te", "wohl", "schwe\u00b7ben", "und", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie fallende Bl\u00fcten im Wind.", "tokens": ["Wie", "fal\u00b7len\u00b7de", "Bl\u00fc\u00b7ten", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und die in dem Hause dort wohnen", "tokens": ["Und", "die", "in", "dem", "Hau\u00b7se", "dort", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ART", "NN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sind heilig und wissen es nicht,", "tokens": ["Sind", "hei\u00b7lig", "und", "wis\u00b7sen", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie spielen mit Kr\u00e4nzen und Kronen", "tokens": ["Sie", "spie\u00b7len", "mit", "Kr\u00e4n\u00b7zen", "und", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Allt\u00e4glich ein neues Gedicht.", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "ein", "neu\u00b7es", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Sie sind gleich den G\u00f6ttern und handlen", "tokens": ["Sie", "sind", "gleich", "den", "G\u00f6t\u00b7tern", "und", "hand\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Allt\u00e4glich in andrer Gestalt,", "tokens": ["All\u00b7t\u00e4g\u00b7lich", "in", "an\u00b7drer", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Liebchen wird auch sich verwandlen", "tokens": ["Mein", "Lieb\u00b7chen", "wird", "auch", "sich", "ver\u00b7wand\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das tut meinem Herzen Gewalt.", "tokens": ["Das", "tut", "mei\u00b7nem", "Her\u00b7zen", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "O Liebchen wo bist du geblieben?", "tokens": ["O", "Lieb\u00b7chen", "wo", "bist", "du", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich steh' vor dem schimmernden Haus,", "tokens": ["Ich", "steh'", "vor", "dem", "schim\u00b7mern\u00b7den", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und will dich bescheiden nur lieben", "tokens": ["Und", "will", "dich", "be\u00b7schei\u00b7den", "nur", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O Liebchen, o sehe heraus!", "tokens": ["O", "Lieb\u00b7chen", ",", "o", "se\u00b7he", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich will dein pflegen und warten", "tokens": ["Ich", "will", "dein", "pfle\u00b7gen", "und", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Herzen so treu als ich kann,", "tokens": ["Im", "Her\u00b7zen", "so", "treu", "als", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da seh' ich sie sitzen im Garten", "tokens": ["Da", "seh'", "ich", "sie", "sit\u00b7zen", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wohl bei einem reichen Mann.", "tokens": ["Wohl", "bei", "ei\u00b7nem", "rei\u00b7chen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So kauf' ich mir Harke und Spaten,", "tokens": ["So", "kauf'", "ich", "mir", "Har\u00b7ke", "und", "Spa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bind' mir ein gr\u00fcn Sch\u00fcrzelein vor.", "tokens": ["Bind'", "mir", "ein", "gr\u00fcn", "Sch\u00fcr\u00b7zel\u00b7ein", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich stell' mich als w\u00e4r ich der G\u00e4rtner", "tokens": ["Ich", "stell'", "mich", "als", "w\u00e4r", "ich", "der", "G\u00e4rt\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und klopf' bei dem Reichen ans Tor.", "tokens": ["Und", "klopf'", "bei", "dem", "Rei\u00b7chen", "ans", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Tu auf, o Reicher den Garten,", "tokens": ["Tu", "auf", ",", "o", "Rei\u00b7cher", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich will dir so gern ohne Sold", "tokens": ["Ich", "will", "dir", "so", "gern", "oh\u00b7ne", "Sold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Blumen all pflegen und warten", "tokens": ["Die", "Blu\u00b7men", "all", "pfle\u00b7gen", "und", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie sind ja mein Silber und Gold.", "tokens": ["Sie", "sind", "ja", "mein", "Sil\u00b7ber", "und", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "So sei mir o G\u00e4rtner willkommen,", "tokens": ["So", "sei", "mir", "o", "G\u00e4rt\u00b7ner", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "FM", "NN", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zieh h\u00f6her die Rosenwand mir.", "tokens": ["Zieh", "h\u00f6\u00b7her", "die", "Ro\u00b7sen\u00b7wand", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Verflecht sie zu Netzen und Schlingen,", "tokens": ["Ver\u00b7flecht", "sie", "zu", "Net\u00b7zen", "und", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich habe ein V\u00f6gelchen hier.", "tokens": ["Ich", "ha\u00b7be", "ein", "V\u00f6\u00b7gel\u00b7chen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Zieh h\u00f6her und dicht mir die Laube,", "tokens": ["Zieh", "h\u00f6\u00b7her", "und", "dicht", "mir", "die", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zieh mir ein gitternes Haus,", "tokens": ["Zieh", "mir", "ein", "git\u00b7ter\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da\u00df keiner das V\u00f6gelchen raube,", "tokens": ["Da\u00df", "kei\u00b7ner", "das", "V\u00f6\u00b7gel\u00b7chen", "rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da\u00df es nicht fliege heraus.", "tokens": ["Da\u00df", "es", "nicht", "flie\u00b7ge", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Da klinget so herzlich und s\u00fc\u00dfe", "tokens": ["Da", "klin\u00b7get", "so", "herz\u00b7lich", "und", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im Garten ein inniges Lied,", "tokens": ["Im", "Gar\u00b7ten", "ein", "in\u00b7ni\u00b7ges", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die B\u00e4ume sie senden ihr Gr\u00fc\u00dfe,", "tokens": ["Die", "B\u00e4u\u00b7me", "sie", "sen\u00b7den", "ihr", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blume lauschend ihr bl\u00fcht.", "tokens": ["Die", "Blu\u00b7me", "lau\u00b7schend", "ihr", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Da seh' ich mein Liebchen so weinen,", "tokens": ["Da", "seh'", "ich", "mein", "Lieb\u00b7chen", "so", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie sieht zu mir heimlich herauf.", "tokens": ["Sie", "sieht", "zu", "mir", "heim\u00b7lich", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Sonne will nicht mehr scheinen,", "tokens": ["Die", "Son\u00b7ne", "will", "nicht", "mehr", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Blumen sie gehen nicht auf.", "tokens": ["Die", "Blu\u00b7men", "sie", "ge\u00b7hen", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.14": {"line.1": {"text": "So hast du dann es verlassen", "tokens": ["So", "hast", "du", "dann", "es", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das schimmernde G\u00f6tterhaus,", "tokens": ["Das", "schim\u00b7mern\u00b7de", "G\u00f6t\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Deiner Locken Gold wird blassen,", "tokens": ["Dei\u00b7ner", "Lo\u00b7cken", "Gold", "wird", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Augen Licht gehet aus.", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "Licht", "ge\u00b7het", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.15": {"line.1": {"text": "O Liebchen, o sei nicht so munter", "tokens": ["O", "Lieb\u00b7chen", ",", "o", "sei", "nicht", "so", "mun\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "FM", "VAFIN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast vergeudet dein Los;", "tokens": ["Du", "hast", "ver\u00b7geu\u00b7det", "dein", "Los", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein Sternlein, es gehet ja unter", "tokens": ["Dein", "Stern\u00b7lein", ",", "es", "ge\u00b7het", "ja", "un\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tief in des Meeres Scho\u00df.", "tokens": ["Tief", "in", "des", "Mee\u00b7res", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ans Meer will ich und stehen", "tokens": ["Ans", "Meer", "will", "ich", "und", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still in dem Abendschein,", "tokens": ["Still", "in", "dem", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da mu\u00df in den Wellen ich sehen", "tokens": ["Da", "mu\u00df", "in", "den", "Wel\u00b7len", "ich", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Versinken dein Sternelein.", "tokens": ["Ver\u00b7sin\u00b7ken", "dein", "Ster\u00b7ne\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.17": {"line.1": {"text": "Im Niedersehen da rollen", "tokens": ["Im", "Nie\u00b7der\u00b7se\u00b7hen", "da", "rol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Tr\u00e4nen still hinab,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "still", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sich vereinen wollen", "tokens": ["Die", "sich", "ver\u00b7ei\u00b7nen", "wol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit deines Sternes Grab.", "tokens": ["Mit", "dei\u00b7nes", "Ster\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Dies Lied hab' ich ersonnen", "tokens": ["Dies", "Lied", "hab'", "ich", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl vor jenem 