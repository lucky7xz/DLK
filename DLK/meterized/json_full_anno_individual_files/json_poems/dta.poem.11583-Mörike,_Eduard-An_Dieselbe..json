{"dta.poem.11583": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Dieselbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wie liebreich warst du der Welt und dienetest Allen!", "tokens": ["Ach", "wie", "lieb\u00b7reich", "warst", "du", "der", "Welt", "und", "die\u00b7ne\u00b7test", "Al\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "PPER", "ART", "NN", "KON", "VVFIN", "NE", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wie klein doch, wie plump hat sie dich endlich", "tokens": ["Und", "wie", "klein", "doch", ",", "wie", "plump", "hat", "sie", "dich", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "ADV", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "PRF", "ADV"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da entsagtest du ihr; doch l\u00e4chelnd wehren die Deinen", "tokens": ["Da", "ent\u00b7sag\u00b7test", "du", "ihr", ";", "doch", "l\u00e4\u00b7chelnd", "weh\u00b7ren", "die", "Dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "ADV", "ADJD", "VVFIN", "ART", "PPOSAT"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Heute wie gestern der Hand, die sich in Liebe vergi\u00dft.", "tokens": ["Heu\u00b7te", "wie", "ge\u00b7stern", "der", "Hand", ",", "die", "sich", "in", "Lie\u00b7be", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}}}}}