{"textgrid.poem.38362": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "U u Uhu", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Uhu sieht gar ernsthaft aus, als h\u00e4tt er hoch studiert,", "tokens": ["Der", "U\u00b7hu", "sieht", "gar", "ernst\u00b7haft", "aus", ",", "als", "h\u00e4tt", "er", "hoch", "stu\u00b7diert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Geht nicht aus seiner H\u00f6l heraus, bis Nacht und finster wird,", "tokens": ["Geht", "nicht", "aus", "sei\u00b7ner", "H\u00f6l", "he\u00b7raus", ",", "bis", "Nacht", "und", "fins\u00b7ter", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KOUS", "NN", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "All Dunkelheit ist ihm ganz hell, doch sieht er nichts bei Tag,", "tokens": ["All", "Dun\u00b7kel\u00b7heit", "ist", "ihm", "ganz", "hell", ",", "doch", "sieht", "er", "nichts", "bei", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Drum ist er auch ein solch Gesell, den nie kein Vogel mag.", "tokens": ["Drum", "ist", "er", "auch", "ein", "solch", "Ge\u00b7sell", ",", "den", "nie