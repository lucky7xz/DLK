{"dta.poem.7584": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist, und bist auch nicht. Du bist, weil durch dich ist", "tokens": ["Du", "bist", ",", "und", "bist", "auch", "nicht", ".", "Du", "bist", ",", "weil", "durch", "dich", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KON", "VAFIN", "ADV", "PTKNEG", "$.", "PPER", "VAFIN", "$,", "KOUS", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist; und bist nicht, weil du das, was ist, nicht bist.", "tokens": ["Was", "ist", ";", "und", "bist", "nicht", ",", "weil", "du", "das", ",", "was", "ist", ",", "nicht", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "KON", "VAFIN", "PTKNEG", "$,", "KOUS", "PPER", "PDS", "$,", "PWS", "VAFIN", "$,", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bist das Seiende, und das Nichtseiende,", "tokens": ["Du", "bist", "das", "Sei\u00b7en\u00b7de", ",", "und", "das", "Nicht\u00b7sei\u00b7en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Seyngebende und von dem Seyn befreiende.", "tokens": ["Seyn\u00b7ge\u00b7ben\u00b7de", "und", "von", "dem", "Seyn", "be\u00b7frei\u00b7en\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ART", "PPOSAT", "ADJA", "$."], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du bist einfaches Licht, und siebenfache Farben", "tokens": ["Du", "bist", "ein\u00b7fa\u00b7ches", "Licht", ",", "und", "sie\u00b7ben\u00b7fa\u00b7che", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind Welten, die durch dich den Schein des Seyns erwarben.", "tokens": ["Sind", "Wel\u00b7ten", ",", "die", "durch", "dich", "den", "Schein", "des", "Seyns", "er\u00b7war\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Durchs Licht erscheinen sie, das Licht nicht sind die Farben,", "tokens": ["Durchs", "Licht", "er\u00b7schei\u00b7nen", "sie", ",", "das", "Licht", "nicht", "sind", "die", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "PTKNEG", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Lichte sind sie dann, wann sie im Scheine starben.", "tokens": ["Im", "Lich\u00b7te", "sind", "sie", "dann", ",", "wann", "sie", "im", "Schei\u00b7ne", "star\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du bist einfacher Ton, die siebenfachen Saiten", "tokens": ["Du", "bist", "ein\u00b7fa\u00b7cher", "Ton", ",", "die", "sie\u00b7ben\u00b7fa\u00b7chen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weltenleier sinds die dich mit dir entzweiten.", "tokens": ["Der", "Wel\u00b7ten\u00b7lei\u00b7er", "sinds", "die", "dich", "mit", "dir", "ent\u00b7zwei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du bist der Grundton, der in sieben Stralen tr\u00e4uft", "tokens": ["Du", "bist", "der", "Grund\u00b7ton", ",", "der", "in", "sie\u00b7ben", "Stra\u00b7len", "tr\u00e4uft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Leiter nieder, und zur\u00fcck zum Anfang l\u00e4uft.", "tokens": ["Die", "Lei\u00b7ter", "nie\u00b7der", ",", "und", "zu\u00b7r\u00fcck", "zum", "An\u00b7fang", "l\u00e4uft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du selber bist der Laut, und bist der Lautenschl\u00e4ger,", "tokens": ["Du", "sel\u00b7ber", "bist", "der", "Laut", ",", "und", "bist", "der", "Lau\u00b7ten\u00b7schl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Schwingungen der Seele deine Tr\u00e4ger.", "tokens": ["Und", "al\u00b7le", "Schwin\u00b7gun\u00b7gen", "der", "See\u00b7le", "dei\u00b7ne", "Tr\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du bist des Morgens Hauch, du bist des Abends Luft,", "tokens": ["Du", "bist", "des", "Mor\u00b7gens", "Hauch", ",", "du", "bist", "des", "A\u00b7bends", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist des Fr\u00fchlings Strauch, du bist des Herbstes Duft.", "tokens": ["Du", "bist", "des", "Fr\u00fch\u00b7lings", "Strauch", ",", "du", "bist", "des", "Herbs\u00b7tes", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Du bists und bist es nicht, du bist wie Tag und Jahr,", "tokens": ["Du", "bists", "und", "bist", "es", "nicht", ",", "du", "bist", "wie", "Tag", "und", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "VAFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kreis, der in sich kreist, unwandel-wandelbar.", "tokens": ["Der", "Kreis", ",", "der", "in", "sich", "kreist", ",", "un\u00b7wan\u00b7del\u00b7wan\u00b7del\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das R\u00e4thsel staun' ich an, und will es l\u00f6sen nicht,", "tokens": ["Das", "R\u00e4th\u00b7sel", "staun'", "ich", "an", ",", "und", "will", "es", "l\u00f6\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sich die L\u00f6sung in mein eignes Seyn verflicht.", "tokens": ["Weil", "sich", "die", "L\u00f6\u00b7sung", "in", "mein", "eig\u00b7nes", "Seyn", "ver\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Du, Wunderbarer, gabst mir Lust am Wunderbaren;", "tokens": ["Du", ",", "Wun\u00b7der\u00b7ba\u00b7rer", ",", "gabst", "mir", "Lust", "am", "Wun\u00b7der\u00b7ba\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich, Ewigklarer, labst du mit dem D\u00e4mmerklaren.", "tokens": ["Mich", ",", "E\u00b7wigk\u00b7la\u00b7rer", ",", "labst", "du", "mit", "dem", "D\u00e4m\u00b7mer\u00b7kla\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}