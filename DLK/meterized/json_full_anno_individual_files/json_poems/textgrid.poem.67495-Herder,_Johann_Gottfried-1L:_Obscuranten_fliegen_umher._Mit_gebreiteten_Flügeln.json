{"textgrid.poem.67495": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Obscuranten fliegen umher. Mit gebreiteten Fl\u00fcgeln", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Obscuranten fliegen umher. Mit gebreiteten Fl\u00fcgeln", "tokens": ["Ob\u00b7scu\u00b7ran\u00b7ten", "flie\u00b7gen", "um\u00b7her", ".", "Mit", "ge\u00b7brei\u00b7te\u00b7ten", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schweben bei Nacht sie hin, wo nur ein Lichtchen erscheint;", "tokens": ["Schwe\u00b7ben", "bei", "Nacht", "sie", "hin", ",", "wo", "nur", "ein", "Licht\u00b7chen", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "PTKVZ", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Gr\u00e4\u00dflich ist ihr Schatten; die Trichternasen, sie saugen", "tokens": ["Gr\u00e4\u00df\u00b7lich", "ist", "ihr", "Schat\u00b7ten", ";", "die", "Trich\u00b7ter\u00b7na\u00b7sen", ",", "sie", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Schlafenden Menschen das Blut, Blut und die Seele mit aus.", "tokens": ["Schla\u00b7fen\u00b7den", "Men\u00b7schen", "das", "Blut", ",", "Blut", "und", "die", "See\u00b7le", "mit", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "NN", "KON", "ART", "NN", "APPR", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Gar fein f\u00fchlend sind diese Gespenster; beraubet der Augen,", "tokens": ["Gar", "fein", "f\u00fch\u00b7lend", "sind", "die\u00b7se", "Ge\u00b7spens\u00b7ter", ";", "be\u00b7rau\u00b7bet", "der", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "PDAT", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Siehet das Nachtgesch\u00f6pf wie mit dem siebenten Sinn.", "tokens": ["Sie\u00b7het", "das", "Nacht\u00b7ge\u00b7sch\u00f6pf", "wie", "mit", "dem", "sie\u00b7ben\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Jaget mit Stecken sie fort, la\u00dft auf sie Katzen \u2013 o nein doch!", "tokens": ["Ja\u00b7get", "mit", "Ste\u00b7cken", "sie", "fort", ",", "la\u00dft", "auf", "sie", "Kat\u00b7zen", "\u2013", "o", "nein", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "PPER", "NN", "$(", "FM", "PTKANT", "ADV", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Lasset die Sonn' aufgehn, und sie sind alle verscheucht.", "tokens": ["Las\u00b7set", "die", "Sonn'", "auf\u00b7gehn", ",", "und", "sie", "sind", "al\u00b7le", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos"