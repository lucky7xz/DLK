{"dta.poem.3265": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Chloen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.71", "sv:0.28"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "O Chloe! h\u00f6re du", "tokens": ["O", "Chloe", "!", "h\u00f6\u00b7re", "du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Der neuen Laute zu,", "tokens": ["Der", "neu\u00b7en", "Lau\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die j\u00fcngst, bey stiller Nacht,", "tokens": ["Die", "j\u00fcngst", ",", "bey", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir Cypripor gebracht.", "tokens": ["Mir", "Cyp\u00b7ri\u00b7por", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm diese, war sein Wort,", "tokens": ["Nimm", "die\u00b7se", ",", "war", "sein", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Statt jener Stolzen dort!", "tokens": ["Statt", "je\u00b7ner", "Stol\u00b7zen", "dort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die buhlt so lange schon", "tokens": ["Die", "buhlt", "so", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Um Pindars hohen Ton:", "tokens": ["Um", "Pin\u00b7dars", "ho\u00b7hen", "Ton", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Doch da sie Siegern fr\u00f6hnt,", "tokens": ["Doch", "da", "sie", "Sie\u00b7gern", "fr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wird sie und du verh\u00f6hnt.", "tokens": ["Wird", "sie", "und", "du", "ver\u00b7h\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Thu, wie der tejer Greis,", "tokens": ["Thu", ",", "wie", "der", "te\u00b7jer", "Greis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der keines Helden Preis", "tokens": ["Der", "kei\u00b7nes", "Hel\u00b7den", "Preis"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In seine Leyer sang,", "tokens": ["In", "sei\u00b7ne", "Le\u00b7yer", "sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die nur von Liebe klang.", "tokens": ["Die", "nur", "von", "Lie\u00b7be", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sang voll Weins und Lust", "tokens": ["Er", "sang", "voll", "Weins", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und an der M\u00e4dchen Brust.", "tokens": ["Und", "an", "der", "M\u00e4d\u00b7chen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sann er auf ein Lied,", "tokens": ["Da", "sann", "er", "auf", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das noch die Herzen zieht:", "tokens": ["Das", "noch", "die", "Her\u00b7zen", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Das machten ihm alsdenn", "tokens": ["Das", "mach\u00b7ten", "ihm", "als\u00b7denn"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ich und die Grazien.", "tokens": ["Ich", "und", "die", "Gra\u00b7zi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verfolge seine Spur;", "tokens": ["Ver\u00b7fol\u00b7ge", "sei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er folgte der Natur.", "tokens": ["Er", "folg\u00b7te", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst bey Lieb und Wein,", "tokens": ["Du", "sollst", "bey", "Lieb", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie er, mein Dichter seyn.", "tokens": ["Wie", "er", ",", "mein", "Dich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ly\u00e4en kennst du schon;", "tokens": ["Ly\u00b7\u00e4\u00b7en", "kennst", "du", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch nicht Cytherens Sohn.", "tokens": ["Doch", "nicht", "Cy\u00b7the\u00b7rens", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NE", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Dir mache, wer ich bin,", "tokens": ["Dir", "ma\u00b7che", ",", "wer", "ich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die sch\u00f6ne Nachbarinn", "tokens": ["Die", "sch\u00f6\u00b7ne", "Nach\u00b7ba\u00b7rinn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und meine schnelle Hand", "tokens": ["Und", "mei\u00b7ne", "schnel\u00b7le", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Durch diesen Pfeil bekannt.", "tokens": ["Durch", "die\u00b7sen", "Pfeil", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kaum sprach der Bube so,", "tokens": ["Kaum", "sprach", "der", "Bu\u00b7be", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So scho\u00df er und entfloh;", "tokens": ["So", "scho\u00df", "er", "und", "ent\u00b7floh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So f\u00fchlte schon mein Herz", "tokens": ["So", "f\u00fchl\u00b7te", "schon", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch ungef\u00fchlten Schmerz;", "tokens": ["Noch", "un\u00b7ge\u00b7f\u00fchl\u00b7ten", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So sah ich voll Begier,", "tokens": ["So", "sah", "ich", "voll", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "O Chloe! nur nach dir.", "tokens": ["O", "Chloe", "!", "nur", "nach", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nun siege wer da will!", "tokens": ["Nun", "sie\u00b7ge", "wer", "da", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mein neues Saitenspiel", "tokens": ["Mein", "neu\u00b7es", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Soll nur dem frohen Wein", "tokens": ["Soll", "nur", "dem", "fro\u00b7hen", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und Chloen heilig seyn.", "tokens": ["Und", "Chloen", "hei\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}