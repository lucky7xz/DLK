{"textgrid.poem.62660": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf vorhergehende Disputation", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verstand, Geschicklichkeit, Flei\u00df, Tugend, Weisheit, Kunst,", "tokens": ["Ver\u00b7stand", ",", "Ge\u00b7schick\u00b7lich\u00b7keit", ",", "Flei\u00df", ",", "Tu\u00b7gend", ",", "Weis\u00b7heit", ",", "Kunst", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erwirbt bey jedermann Gewogenheit und Gunst;", "tokens": ["Er\u00b7wirbt", "bey", "je\u00b7der\u00b7mann", "Ge\u00b7wo\u00b7gen\u00b7heit", "und", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht dieses nur allein: Es kan auch Ehre bringen;", "tokens": ["Nicht", "die\u00b7ses", "nur", "al\u00b7lein", ":", "Es", "kan", "auch", "Eh\u00b7re", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "ADV", "ADV", "$.", "PPER", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer diesen Schatz besitzt, weis sich empor zu schwingen.", "tokens": ["Wer", "die\u00b7sen", "Schatz", "be\u00b7sitzt", ",", "weis", "sich", "em\u00b7por", "zu", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVPP", "$,", "KOUS", "PRF", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die\u00df ist ein solcher Satz den niemand l\u00e4ugnen kan,", "tokens": ["Die\u00df", "ist", "ein", "sol\u00b7cher", "Satz", "den", "nie\u00b7mand", "l\u00e4ug\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "PIAT", "NN", "ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man seh ", "tokens": ["Man", "seh"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "So f\u00e4llt mir jeder bey. Du hast galante Sitten,", "tokens": ["So", "f\u00e4llt", "mir", "je\u00b7der", "bey", ".", "Du", "hast", "ga\u00b7lan\u00b7te", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du hast der Weisheit Bahn bisher mit Ruhm beschritten,", "tokens": ["Du", "hast", "der", "Weis\u00b7heit", "Bahn", "bis\u00b7her", "mit", "Ruhm", "be\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nach Wissenschaft gestrebt, und sie nach Wunsch erlangt,", "tokens": ["Nach", "Wis\u00b7sen\u00b7schaft", "ge\u00b7strebt", ",", "und", "sie", "nach", "Wunsch", "er\u00b7langt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df jetzt dein muntrer Fu\u00df auf dem Catheder prangt.", "tokens": ["Da\u00df", "jetzt", "dein", "mun\u00b7trer", "Fu\u00df", "auf", "dem", "Cat\u00b7he\u00b7der", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Gerana h\u00f6rt dich heut mit Ruhme disputiren,", "tokens": ["Ge\u00b7ra\u00b7na", "h\u00f6rt", "dich", "heut", "mit", "Ruh\u00b7me", "dis\u00b7pu\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum will sie dich davor mit ", "tokens": ["Drum", "will", "sie", "dich", "da\u00b7vor", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "PAV", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "So trage denn den Schmuck, den dir Astr\u00e4a schenkt,", "tokens": ["So", "tra\u00b7ge", "denn", "den", "Schmuck", ",", "den", "dir", "A\u00b7str\u00e4a", "schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und den sie dir zum Prei\u00df um deine Schultern hengt.", "tokens": ["Und", "den", "sie", "dir", "zum", "Prei\u00df", "um", "dei\u00b7ne", "Schul\u00b7tern", "hengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON"