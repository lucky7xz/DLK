{"dta.poem.14990": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Bundeslied vor der Schlacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Bricht der gro\u00dfe Morgen an,", "tokens": ["Bricht", "der", "gro\u00b7\u00dfe", "Mor\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sonne kalt und blutig,", "tokens": ["Und", "die", "Son\u00b7ne", "kalt", "und", "blu\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtet unsrer blut'gen Bahn.", "tokens": ["Leuch\u00b7tet", "uns\u00b7rer", "blut'\u00b7gen", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der n\u00e4chsten Stunden Schoo\u00dfe,", "tokens": ["In", "der", "n\u00e4chs\u00b7ten", "Stun\u00b7den", "Schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liegt das Schicksal einer Welt,", "tokens": ["Liegt", "das", "Schick\u00b7sal", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es zittern schon die Loose,", "tokens": ["Und", "es", "zit\u00b7tern", "schon", "die", "Loo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der ehr'ne W\u00fcrfel f\u00e4llt.", "tokens": ["Und", "der", "ehr'\u00b7ne", "W\u00fcr\u00b7fel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Br\u00fcder! euch mahne die d\u00e4mmernde Stunde,", "tokens": ["Br\u00fc\u00b7der", "!", "euch", "mah\u00b7ne", "die", "d\u00e4m\u00b7mern\u00b7de", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Mahne euch ernst zu dem heiligsten Bunde,", "tokens": ["Mah\u00b7ne", "euch", "ernst", "zu", "dem", "hei\u00b7ligs\u00b7ten", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Treu, so zum Tod, als zum Leben, gesellt.", "tokens": ["Treu", ",", "so", "zum", "Tod", ",", "als", "zum", "Le\u00b7ben", ",", "ge\u00b7sellt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "$,", "KOUS", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinter uns, im Graun der N\u00e4chte,", "tokens": ["Hin\u00b7ter", "uns", ",", "im", "Graun", "der", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt die Schande, liegt die Schmach,", "tokens": ["Liegt", "die", "Schan\u00b7de", ",", "liegt", "die", "Schmach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt der Frevel fremder Knechte,", "tokens": ["Liegt", "der", "Fre\u00b7vel", "frem\u00b7der", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die deutsche Eiche brach.", "tokens": ["Der", "die", "deut\u00b7sche", "Ei\u00b7che", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsre Sprache ward gesch\u00e4ndet,", "tokens": ["Uns\u00b7re", "Spra\u00b7che", "ward", "ge\u00b7sch\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Tempel st\u00fcrzten ein,", "tokens": ["Uns\u00b7re", "Tem\u00b7pel", "st\u00fcrz\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Ehre ist verpf\u00e4ndet,", "tokens": ["Uns\u00b7re", "Eh\u00b7re", "ist", "ver\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deutsche Br\u00fcder l\u00f6\u00dft sie ein!", "tokens": ["Deut\u00b7sche", "Br\u00fc\u00b7der", "l\u00f6\u00dft", "sie", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Br\u00fcder, die Rache flammt! reicht euch die H\u00e4nde,", "tokens": ["Br\u00fc\u00b7der", ",", "die", "Ra\u00b7che", "flammt", "!", "reicht", "euch", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Da\u00df sich der Fluch der Himmlischen wende!", "tokens": ["Da\u00df", "sich", "der", "Fluch", "der", "Himm\u00b7li\u00b7schen", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "L\u00f6\u00dft das verlohrne Palladium ein!", "tokens": ["L\u00f6\u00dft", "das", "ver\u00b7lohr\u00b7ne", "Pal\u00b7la\u00b7di\u00b7um", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vor uns liegt ein gl\u00fccklich Hoffen,", "tokens": ["Vor", "uns", "liegt", "ein", "gl\u00fcck\u00b7lich", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt der Zukunft goldne Zeit,", "tokens": ["Liegt", "der", "Zu\u00b7kunft", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht ein ganzer Himmel offen,", "tokens": ["Steht", "ein", "gan\u00b7zer", "Him\u00b7mel", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht der Freiheit Seligkeit.", "tokens": ["Bl\u00fcht", "der", "Frei\u00b7heit", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deutsche Kunst und deutsche Lieder,", "tokens": ["Deut\u00b7sche", "Kunst", "und", "deut\u00b7sche", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frauenhuld und Liebesgl\u00fcck,", "tokens": ["Frau\u00b7en\u00b7huld", "und", "Lie\u00b7bes\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles Gro\u00dfe kommt uns wieder,", "tokens": ["Al\u00b7les", "Gro\u00b7\u00dfe", "kommt", "uns", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles Sch\u00f6ne kehrt zur\u00fcck.", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7ne", "kehrt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber noch gilt es ein gr\u00e4\u00dfliches Wagen,", "tokens": ["A\u00b7ber", "noch", "gilt", "es", "ein", "gr\u00e4\u00df\u00b7li\u00b7ches", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Leben und Blut in die Schanze zu schlagen;", "tokens": ["Le\u00b7ben", "und", "Blut", "in", "die", "Schan\u00b7ze", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Nur in dem Opfertod reift uns das Gl\u00fcck.", "tokens": ["Nur", "in", "dem", "Op\u00b7fer\u00b7tod", "reift", "uns", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun, mit Gott! wir wollens wagen,", "tokens": ["Nun", ",", "mit", "Gott", "!", "wir", "wol\u00b7lens", "wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$.", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest vereint dem Schicksal stehn,", "tokens": ["Fest", "ver\u00b7eint", "dem", "Schick\u00b7sal", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Herz zum Altar tragen,", "tokens": ["Un\u00b7ser", "Herz", "zum", "Al\u00b7tar", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem Tod entgegen gehn.", "tokens": ["Und", "dem", "Tod", "ent\u00b7ge\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vaterland! dir woll'n wir sterben,", "tokens": ["Va\u00b7ter\u00b7land", "!", "dir", "woll'n", "wir", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie dein gro\u00dfes Wort gebeut!", "tokens": ["Wie", "dein", "gro\u00b7\u00dfes", "Wort", "ge\u00b7beut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Lieben m\u00f6gen's erben,", "tokens": ["Uns\u00b7re", "Lie\u00b7ben", "m\u00f6\u00b7gen's", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was wir mit dem Blut befreit.", "tokens": ["Was", "wir", "mit", "dem", "Blut", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.9": {"text": "Wachse, du Freiheit der deutschen Eichen,", "tokens": ["Wach\u00b7se", ",", "du", "Frei\u00b7heit", "der", "deut\u00b7schen", "Ei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Wachse empor \u00fcber unsere Leichen! \u2014", "tokens": ["Wach\u00b7se", "em\u00b7por", "\u00fc\u00b7ber", "un\u00b7se\u00b7re", "Lei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Vaterland h\u00f6re den heiligen Eid. \u2014", "tokens": ["Va\u00b7ter\u00b7land", "h\u00f6\u00b7re", "den", "hei\u00b7li\u00b7gen", "Eid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Und nun wendet eure Blicke,", "tokens": ["Und", "nun", "wen\u00b7det", "eu\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch einmahl der Liebe nach,", "tokens": ["Noch", "ein\u00b7mahl", "der", "Lie\u00b7be", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheidet von dem Bl\u00fcthengl\u00fccke,", "tokens": ["Schei\u00b7det", "von", "dem", "Bl\u00fc\u00b7then\u00b7gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das der gift'ge S\u00fcden brach.", "tokens": ["Das", "der", "gift'\u00b7ge", "S\u00fc\u00b7den", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird euch auch das Auge tr\u00fcber \u2014", "tokens": ["Wird", "euch", "auch", "das", "Au\u00b7ge", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keine Thr\u00e4ne bringt euch Spott.", "tokens": ["Kei\u00b7ne", "Thr\u00e4\u00b7ne", "bringt", "euch", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werft den letzten Ku\u00df hin\u00fcber,", "tokens": ["Werft", "den", "letz\u00b7ten", "Ku\u00df", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann befehlt sie eurem Gott!", "tokens": ["Dann", "be\u00b7fehlt", "sie", "eu\u00b7rem", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Alle die Lippen, die f\u00fcr uns beten,", "tokens": ["Al\u00b7le", "die", "Lip\u00b7pen", ",", "die", "f\u00fcr", "uns", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Alle die Herzen, die wir zertreten,", "tokens": ["Al\u00b7le", "die", "Her\u00b7zen", ",", "die", "wir", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Tr\u00f6ste und sch\u00fctze sie, ewiger Gott! \u2014", "tokens": ["Tr\u00f6s\u00b7te", "und", "sch\u00fct\u00b7ze", "sie", ",", "e\u00b7wi\u00b7ger", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Und nun frisch zur Schlacht gewendet,", "tokens": ["Und", "nun", "frisch", "zur", "Schlacht", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aug' und Herz zum Licht hinauf!", "tokens": ["Aug'", "und", "Herz", "zum", "Licht", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Ird'sche ist vollendet,", "tokens": ["Al\u00b7les", "Ird'\u00b7sche", "ist", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Himmlische geht auf.", "tokens": ["Und", "das", "Himm\u00b7li\u00b7sche", "geht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fa\u00dft euch an, ihr deutschen Br\u00fcder!", "tokens": ["Fa\u00dft", "euch", "an", ",", "ihr", "deut\u00b7schen", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jede Nerve sey ein Held!", "tokens": ["Je\u00b7de", "Ner\u00b7ve", "sey", "ein", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Treue Herzen sehn sich wieder,", "tokens": ["Treu\u00b7e", "Her\u00b7zen", "sehn", "sich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lebewohl f\u00fcr diese Welt!", "tokens": ["Le\u00b7be\u00b7wohl", "f\u00fcr", "die\u00b7se", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6rt ihrs! schon jauchzt es uns donnernd entgegen!", "tokens": ["H\u00f6rt", "ihrs", "!", "schon", "jauchzt", "es", "uns", "don\u00b7nernd", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Br\u00fcder! hinein in den blitzenden Regen!", "tokens": ["Br\u00fc\u00b7der", "!", "hin\u00b7ein", "in", "den", "blit\u00b7zen\u00b7den", "Re\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Wiedersehn in der bessern Welt!", "tokens": ["Wie\u00b7der\u00b7sehn", "in", "der", "bes\u00b7sern", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}