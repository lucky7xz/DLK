{"dta.poem.18399": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du aber stehst und l\u00e4chelst nu/", "tokens": ["Du", "a\u00b7ber", "stehst", "und", "l\u00e4\u00b7chelst", "nu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weist den leib gewaltig wohl zu biegen/", "tokens": ["Und", "weist", "den", "leib", "ge\u00b7wal\u00b7tig", "wohl", "zu", "bie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich nur auch einmal ein liebes liebchin kriegen/", "tokens": ["La\u00df", "mich", "nur", "auch", "ein\u00b7mal", "ein", "lie\u00b7bes", "lieb\u00b7chin", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ich wil wohl sch\u00f6ner thun/ als du/", "tokens": ["Ich", "wil", "wohl", "sch\u00f6\u00b7ner", "thun", "/", "als", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$(", "KOUS", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will den einen fu\u00df ausstreichen bi\u00df zur wand/", "tokens": ["Ich", "will", "den", "ei\u00b7nen", "fu\u00df", "aus\u00b7strei\u00b7chen", "bi\u00df", "zur", "wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ART", "NN", "VVINF", "KON", "APPRART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mit der rechten hand", "tokens": ["Und", "mit", "der", "rech\u00b7ten", "hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der wehrten Rosil schuh anr\u00fchren;", "tokens": ["Der", "wehr\u00b7ten", "Ro\u00b7sil", "schuh", "an\u00b7r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du werthster schuch/ so will ich sprechen:", "tokens": ["Du", "werths\u00b7ter", "schuch", "/", "so", "will", "ich", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es pflegt dich Rosilis mit kreyden einzuschmieren/", "tokens": ["Es", "pflegt", "dich", "Ro\u00b7si\u00b7lis", "mit", "krey\u00b7den", "ein\u00b7zu\u00b7schmie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NE", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit einem band zu zieren/", "tokens": ["Mit", "ei\u00b7nem", "band", "zu", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mir aber will es stets an artzeney gebrechen/", "tokens": ["Mir", "a\u00b7ber", "will", "es", "stets", "an", "art\u00b7ze\u00b7ney", "ge\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du wehrtster schuch/ es ruht auf dir", "tokens": ["Du", "wehrts\u00b7ter", "schuch", "/", "es", "ruht", "auf", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die halbe wohlfarth des Dameten/", "tokens": ["Die", "hal\u00b7be", "wohl\u00b7farth", "des", "Da\u00b7me\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Die Rosil braucht dich f\u00fcr und f\u00fcr/", "tokens": ["Die", "Ro\u00b7sil", "braucht", "dich", "f\u00fcr", "und", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mein aber/ sagt sie/ hab sie nicht von n\u00f6then.", "tokens": ["Mein", "a\u00b7ber", "/", "sagt", "sie", "/", "hab", "sie", "nicht", "von", "n\u00f6\u00b7then", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "$(", "VVFIN", "PPER", "$(", "VAFIN", "PPER", "PTKNEG", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Du tausend zucker-s\u00fcsse gute nacht;", "tokens": ["Du", "tau\u00b7send", "zu\u00b7cke\u00b7rs\u00fcs\u00b7se", "gu\u00b7te", "nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dein liebreitz hat mich ja so gantz verliebt gemacht/", "tokens": ["Dein", "lieb\u00b7reitz", "hat", "mich", "ja", "so", "gantz", "ver\u00b7liebt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mich/ der sich deiner pflicht ergiebt/", "tokens": ["Mich", "/", "der", "sich", "dei\u00b7ner", "pflicht", "er\u00b7giebt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der dich/ eh du noch warst gebohren/", "tokens": ["Der", "dich", "/", "eh", "du", "noch", "warst", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Zu seiner liebsten au\u00dferkohren/", "tokens": ["Zu", "sei\u00b7ner", "liebs\u00b7ten", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und schon im mutterleibe dich geliebt.", "tokens": ["Und", "schon", "im", "mut\u00b7ter\u00b7lei\u00b7be", "dich", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}