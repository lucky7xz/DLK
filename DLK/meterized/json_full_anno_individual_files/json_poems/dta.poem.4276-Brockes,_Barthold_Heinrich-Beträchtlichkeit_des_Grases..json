{"dta.poem.4276": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betr\u00e4chtlichkeit des Grases.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sahe, zwischen den gepfl\u00fcgten, auf den noch nicht", "tokens": ["Man", "sa\u00b7he", ",", "zwi\u00b7schen", "den", "ge\u00b7pfl\u00fcg\u00b7ten", ",", "auf", "den", "noch", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "ART", "ADJA", "$,", "APPR", "ART", "ADV", "PTKNEG"], "meter": "-+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "gepfl\u00fcgten St\u00fccken,", "tokens": ["ge\u00b7pfl\u00fcg\u00b7ten", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die, nebst den annoch gelben Stoppeln, viel Gras und Klee,", "tokens": ["Die", ",", "nebst", "den", "an\u00b7noch", "gel\u00b7ben", "Stop\u00b7peln", ",", "viel", "Gras", "und", "Klee", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "$,", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und Kr\u00e4uter schm\u00fccken,", "tokens": ["und", "Kr\u00e4u\u00b7ter", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Ochsen, K\u00fch\u2019 und Schafe grasen, wozwischen denn der", "tokens": ["Die", "Och\u00b7sen", ",", "K\u00fch'", "und", "Scha\u00b7fe", "gra\u00b7sen", ",", "woz\u00b7wi\u00b7schen", "denn", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,", "PWAV", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "G\u00e4nse Schaar,", "tokens": ["G\u00e4n\u00b7se", "Schaar", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Mit ihren Silber-weissen Federn, nicht weniger betr\u00e4chtlich", "tokens": ["Mit", "ih\u00b7ren", "Sil\u00b7ber\u00b7weis\u00b7sen", "Fe\u00b7dern", ",", "nicht", "we\u00b7ni\u00b7ger", "be\u00b7tr\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "war,", "tokens": ["war", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Die mit sanft schnatterndem Get\u00f6n, zu unserm Nutz, den", "tokens": ["Die", "mit", "sanft", "schnat\u00b7tern\u00b7dem", "Ge\u00b7t\u00f6n", ",", "zu", "un\u00b7serm", "Nutz", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "ADJD", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.10": {"text": "Hunger stillen,", "tokens": ["Hun\u00b7ger", "stil\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Und, unsern Blick zugleich vergn\u00fcgend, best\u00e4ndig Kropf", "tokens": ["Und", ",", "un\u00b7sern", "Blick", "zu\u00b7gleich", "ver\u00b7gn\u00fc\u00b7gend", ",", "be\u00b7st\u00e4n\u00b7dig", "Kropf"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "NN", "ADV", "VVPP", "$,", "ADJD", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "und Magen f\u00fcllen.", "tokens": ["und", "Ma\u00b7gen", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Bey diesem angenehmen Anblick erwog ich, mit vergn\u00fcgtem", "tokens": ["Bey", "die\u00b7sem", "an\u00b7ge\u00b7neh\u00b7men", "An\u00b7blick", "er\u00b7wog", "ich", ",", "mit", "ver\u00b7gn\u00fcg\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "PPER", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Geist,", "tokens": ["Geist", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Wie gro\u00df die Huld, die uns der Sch\u00f6pfer, auf dieser Welt,", "tokens": ["Wie", "gro\u00df", "die", "Huld", ",", "die", "uns", "der", "Sch\u00f6p\u00b7fer", ",", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "im Gras', erweist.", "tokens": ["im", "Gras'", ",", "er\u00b7weist", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie viel an Gras und Kraut gelegen,", "tokens": ["Wie", "viel", "an", "Gras", "und", "Kraut", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist n\u00f6thig, da\u00df wirs oft erwegen,", "tokens": ["Ist", "n\u00f6\u00b7thig", ",", "da\u00df", "wirs", "oft", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, da wir seinen Nutz betrachten,", "tokens": ["Und", ",", "da", "wir", "sei\u00b7nen", "Nutz", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seines Gebers G\u00fcte achten!", "tokens": ["Auf", "sei\u00b7nes", "Ge\u00b7bers", "G\u00fc\u00b7te", "ach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es reicht, auf wunderbare Weise,", "tokens": ["Es", "reicht", ",", "auf", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gras so Thier- als Menschen Speise.", "tokens": ["Das", "Gras", "so", "Thier", "als", "Men\u00b7schen", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "TRUNC", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es n\u00e4hrt die Thiere roh, uns gahr,", "tokens": ["Es", "n\u00e4hrt", "die", "Thie\u00b7re", "roh", ",", "uns", "gahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Unmittel- sie, uns mittelbar.", "tokens": ["Un\u00b7mit\u00b7tel", "sie", ",", "uns", "mit\u00b7tel\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["TRUNC", "PPER", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht nur im Fleisch, das uns ern\u00e4hret,", "tokens": ["Nicht", "nur", "im", "Fleisch", ",", "das", "uns", "er\u00b7n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wird es, in Milch auch, die uns tr\u00e4nkt,", "tokens": ["Wird", "es", ",", "in", "Milch", "auch", ",", "die", "uns", "tr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ja gar in unser Kleid verkehret,", "tokens": ["Ja", "gar", "in", "un\u00b7ser", "Kleid", "ver\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da es uns Woll\u2019 und Leder schenkt,", "tokens": ["Da", "es", "uns", "Woll'", "und", "Le\u00b7der", "schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und noch viel ander Guts beschehret.", "tokens": ["Und", "noch", "viel", "an\u00b7der", "Guts", "be\u00b7scheh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da es auch wilder Thiere Felle,", "tokens": ["Da", "es", "auch", "wil\u00b7der", "Thie\u00b7re", "Fel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich mirs recht vor Augen stelle,", "tokens": ["Wenn", "ich", "mirs", "recht", "vor", "Au\u00b7gen", "stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Uns, zur Bequemlichkeit, gew\u00e4hret,", "tokens": ["Uns", ",", "zur", "Be\u00b7quem\u00b7lich\u00b7keit", ",", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil auch die wildesten sich n\u00e4hren", "tokens": ["Weil", "auch", "die", "wil\u00b7des\u00b7ten", "sich", "n\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "PRF", "VVINF"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Mit Thieren, welche Gras verzehren,", "tokens": ["Mit", "Thie\u00b7ren", ",", "wel\u00b7che", "Gras", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und, wenn sich diese nicht durchs Gras zu n\u00e4hren w\u00fc\u00dften,", "tokens": ["Und", ",", "wenn", "sich", "die\u00b7se", "nicht", "durchs", "Gras", "zu", "n\u00e4h\u00b7ren", "w\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PRF", "PDS", "PTKNEG", "APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch jene mit verkommen m\u00fc\u00dften.", "tokens": ["Auch", "je\u00b7ne", "mit", "ver\u00b7kom\u00b7men", "m\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "APPR", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Juden achten, wie man schaut,", "tokens": ["Die", "Ju\u00b7den", "ach\u00b7ten", ",", "wie", "man", "schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vielmehr, als wir, das Gras und Kraut.", "tokens": ["Viel\u00b7mehr", ",", "als", "wir", ",", "das", "Gras", "und", "Kraut", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie, von GOttes Eigenschaft,", "tokens": ["Wenn", "sie", ",", "von", "Got\u00b7tes", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Seiner Weisheit, Lieb\u2019 und Kraft,", "tokens": ["Von", "Sei\u00b7ner", "Weis\u00b7heit", ",", "Lieb'", "und", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was recht betr\u00e4chtlichs zu erzehlen, mit wahrem Ernst be-", "tokens": ["Was", "recht", "be\u00b7tr\u00e4cht\u00b7lichs", "zu", "er\u00b7zeh\u00b7len", ",", "mit", "wah\u00b7rem", "Ernst", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PIS", "PTKZU", "VVINF", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "m\u00fchet seyn,", "tokens": ["m\u00fc\u00b7het", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "So ist der Ausdruck bey denselben kein anderer, als blo\u00df", "tokens": ["So", "ist", "der", "Aus\u00b7druck", "bey", "den\u00b7sel\u00b7ben", "kein", "an\u00b7de\u00b7rer", ",", "als", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PDAT", "PIAT", "ADJA", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "allein:", "tokens": ["al\u00b7lein", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "\u201cder GOtt, der alle Dinge macht,", "tokens": ["\u201c", "der", "Gott", ",", "der", "al\u00b7le", "Din\u00b7ge", "macht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u201eder Laub und Gras hervorgebracht.", "tokens": ["\u201e", "der", "Laub", "und", "Gras", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wollt ihr im Christenthum denn minder, als die verworf-", "tokens": ["Wollt", "ihr", "im", "Chris\u00b7ten\u00b7thum", "denn", "min\u00b7der", ",", "als", "die", "ver\u00b7wor\u00b7f"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "KON", "ADV", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "nen Juden, seyn?", "tokens": ["nen", "Ju\u00b7den", ",", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Verehret GOtt, in Kraut und Gras, indem der Nutz so all-", "tokens": ["Ver\u00b7eh\u00b7ret", "Gott", ",", "in", "Kraut", "und", "Gras", ",", "in\u00b7dem", "der", "Nutz", "so", "all"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "KOUS", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "gemein.", "tokens": ["ge\u00b7mein", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}