{"textgrid.poem.59798": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochauf fliegt mein Herz, seit es sein Gl\u00fcck aus deines", "tokens": ["Hoch\u00b7auf", "fliegt", "mein", "Herz", ",", "seit", "es", "sein", "Gl\u00fcck", "aus", "dei\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gl\u00fccks Offenbarung zieht \u2013", "tokens": ["Gl\u00fccks", "Of\u00b7fen\u00b7ba\u00b7rung", "zieht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer kehrt's wieder, wohin es der Liebe", "tokens": ["Und", "im\u00b7mer", "kehrt's", "wie\u00b7der", ",", "wo\u00b7hin", "es", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "S\u00fc\u00dfe Erfahrung zieht \u2013", "tokens": ["S\u00fc\u00b7\u00dfe", "Er\u00b7fah\u00b7rung", "zieht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dem Springquell \u00e4hnlich, der himmelauf in", "tokens": ["Dem", "Spring\u00b7quell", "\u00e4hn\u00b7lich", ",", "der", "him\u00b7me\u00b7lauf", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "ADV", "APPR"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "toller Gebarung zieht,", "tokens": ["tol\u00b7ler", "Ge\u00b7ba\u00b7rung", "zieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und doch immer zur\u00fcckkehrt, von wo er gekommen ist", "tokens": ["Und", "doch", "im\u00b7mer", "zu\u00b7r\u00fcck\u00b7kehrt", ",", "von", "wo", "er", "ge\u00b7kom\u00b7men", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVPP", "$,", "APPR", "PWAV", "PPER", "VVPP", "VAFIN"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und seine Nahrung zieht.", "tokens": ["Und", "sei\u00b7ne", "Nah\u00b7rung", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hochauf fliegt mein Herz, seit es sein Gl\u00fcck aus deines", "tokens": ["Hoch\u00b7auf", "fliegt", "mein", "Herz", ",", "seit", "es", "sein", "Gl\u00fcck", "aus", "dei\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gl\u00fccks Offenbarung zieht \u2013", "tokens": ["Gl\u00fccks", "Of\u00b7fen\u00b7ba\u00b7rung", "zieht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer kehrt's wieder, wohin es der Liebe", "tokens": ["Und", "im\u00b7mer", "kehrt's", "wie\u00b7der", ",", "wo\u00b7hin", "es", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct"