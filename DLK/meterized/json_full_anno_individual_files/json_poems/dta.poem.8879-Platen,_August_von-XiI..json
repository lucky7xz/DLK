{"dta.poem.8879": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Sobald ein Blick, der lange tr\u00fcb' umnachtet,", "tokens": ["So\u00b7bald", "ein", "Blick", ",", "der", "lan\u00b7ge", "tr\u00fcb'", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ver\u00e4chtlich uns und blinzend nur betrachtet,", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "uns", "und", "blin\u00b7zend", "nur", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "KON", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zulezt voll Milde ruht auf unsern Z\u00fcgen!", "tokens": ["Zu\u00b7lezt", "voll", "Mil\u00b7de", "ruht", "auf", "un\u00b7sern", "Z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "W\u00e4r's Zufall, oder willst du mich betr\u00fcgen?", "tokens": ["W\u00e4r's", "Zu\u00b7fall", ",", "o\u00b7der", "willst", "du", "mich", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du vielleicht mich deiner werth erachtet?", "tokens": ["Hast", "du", "viel\u00b7leicht", "mich", "dei\u00b7ner", "werth", "e\u00b7rach\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn, Augen, ihr mir nicktet oder lachtet,", "tokens": ["Wenn", ",", "Au\u00b7gen", ",", "ihr", "mir", "nick\u00b7tet", "o\u00b7der", "lach\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "$,", "PPER", "PPER", "PTKNEG", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann wollt' ich stets mich euch als Sclave f\u00fcgen!", "tokens": ["Dann", "wollt'", "ich", "stets", "mich", "euch", "als", "Scla\u00b7ve", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O gieb Gewi\u00dfheit, wo nur Zweifel waltet,", "tokens": ["O", "gieb", "Ge\u00b7wi\u00df\u00b7heit", ",", "wo", "nur", "Zwei\u00b7fel", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "NN", "$,", "PWAV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df l\u00e4nger nicht mich hin und wieder schwanken,", "tokens": ["La\u00df", "l\u00e4n\u00b7ger", "nicht", "mich", "hin", "und", "wie\u00b7der", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PTKNEG", "PPER", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil oft im Zweifel das Gem\u00fcth erkaltet!", "tokens": ["Weil", "oft", "im", "Zwei\u00b7fel", "das", "Ge\u00b7m\u00fcth", "er\u00b7kal\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht schwer zu helfen ist gewissen Kranken:", "tokens": ["Nicht", "schwer", "zu", "hel\u00b7fen", "ist", "ge\u00b7wis\u00b7sen", "Kran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein einz'ger Wink, ein H\u00e4ndedruck entfaltet", "tokens": ["Ein", "einz'\u00b7ger", "Wink", ",", "ein", "H\u00e4n\u00b7de\u00b7druck", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns Millionen liebende Gedanken.", "tokens": ["Uns", "Mil\u00b7lion\u00b7en", "lie\u00b7ben\u00b7de", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "--+-+---+-", "measure": "anapaest.init"}}}}}