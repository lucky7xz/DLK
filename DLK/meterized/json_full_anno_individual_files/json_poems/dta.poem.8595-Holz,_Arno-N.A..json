{"dta.poem.8595": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um Euern Garten,", "tokens": ["Um", "Eu\u00b7ern", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "damit Ihr unter bl\u00fchenden B\u00e4umen lachen, jubeln und singen k\u00f6nnt,", "tokens": ["da\u00b7mit", "Ihr", "un\u00b7ter", "bl\u00fc\u00b7hen\u00b7den", "B\u00e4u\u00b7men", "la\u00b7chen", ",", "ju\u00b7beln", "und", "sin\u00b7gen", "k\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,", "VVFIN", "KON", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "runde, rolle, ringle ich meinen Drachenleib.", "tokens": ["run\u00b7de", ",", "rol\u00b7le", ",", "ring\u00b7le", "ich", "mei\u00b7nen", "Dra\u00b7chen\u00b7leib", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "In respectvoller Distance,", "tokens": ["In", "res\u00b7pect\u00b7vol\u00b7ler", "Dis\u00b7tan\u00b7ce", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "mit Steinen, Brechstangen und Kotheimern,", "tokens": ["mit", "Stei\u00b7nen", ",", "Brech\u00b7stan\u00b7gen", "und", "Ko\u00b7thei\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "steht das Gesindel.", "tokens": ["steht", "das", "Ge\u00b7sin\u00b7del", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Seine Wut sch\u00e4umt auf, seine Ohnmacht br\u00fcllt,", "tokens": ["Sei\u00b7ne", "Wut", "sch\u00e4umt", "auf", ",", "sei\u00b7ne", "Ohn\u00b7macht", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "wenn hinter den hohen Spiegelmauern, \u00fcber die Rosen ranken,", "tokens": ["wenn", "hin\u00b7ter", "den", "ho\u00b7hen", "Spie\u00b7gel\u00b7mau\u00b7ern", ",", "\u00fc\u00b7ber", "die", "Ro\u00b7sen", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "pl\u00f6tzlich Eure Cymbeln t\u00f6nen,", "tokens": ["pl\u00f6tz\u00b7lich", "Eu\u00b7re", "Cym\u00b7beln", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "oder auf weissen, springenden Wassern, \u00fcber die h\u00f6chsten, steilsten Cypressen", "tokens": ["o\u00b7der", "auf", "weis\u00b7sen", ",", "sprin\u00b7gen\u00b7den", "Was\u00b7sern", ",", "\u00fc\u00b7ber", "die", "h\u00f6chs\u00b7ten", ",", "steils\u00b7ten", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "VVINF", "$,", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+-+--+-+--+-+-+--", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "Eure goldnen B\u00e4lle tanzen.", "tokens": ["Eu\u00b7re", "gold\u00b7nen", "B\u00e4l\u00b7le", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus ihren Augen, aus ihren F\u00e4usten,", "tokens": ["Aus", "ih\u00b7ren", "Au\u00b7gen", ",", "aus", "ih\u00b7ren", "F\u00e4us\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "aus ihren lautlos geduckten Schultern", "tokens": ["aus", "ih\u00b7ren", "laut\u00b7los", "ge\u00b7duck\u00b7ten", "Schul\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "zittert die Gier:", "tokens": ["zit\u00b7tert", "die", "Gier", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "wie Bestien \u00fcber Eure Leiber st\u00fcrzen,", "tokens": ["wie", "Be\u00b7sti\u00b7en", "\u00fc\u00b7ber", "Eu\u00b7re", "Lei\u00b7ber", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "johlend nach Euern Herzen graben,", "tokens": ["joh\u00b7lend", "nach", "Eu\u00b7ern", "Her\u00b7zen", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "durch schwarze, rauchende Tempeltr\u00fcmmer Eure gest\u00fcrzten G\u00f6tter schleifen!", "tokens": ["durch", "schwar\u00b7ze", ",", "rau\u00b7chen\u00b7de", "Tem\u00b7pel\u00b7tr\u00fcm\u00b7mer", "Eu\u00b7re", "ge\u00b7st\u00fcrz\u00b7ten", "G\u00f6t\u00b7ter", "schlei\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Meine Krallen glimmen, meine Augen gl\u00fchn . . .", "tokens": ["Mei\u00b7ne", "Kral\u00b7len", "glim\u00b7men", ",", "mei\u00b7ne", "Au\u00b7gen", "gl\u00fchn", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}