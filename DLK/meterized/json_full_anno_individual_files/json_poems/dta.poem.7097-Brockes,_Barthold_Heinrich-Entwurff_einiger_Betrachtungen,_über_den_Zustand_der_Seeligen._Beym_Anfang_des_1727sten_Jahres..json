{"dta.poem.7097": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Sch\u00f6pffers Ehre selbst bestehet in der Liebe:", "tokens": ["Des", "Sch\u00f6pf\u00b7fers", "Eh\u00b7re", "selbst", "be\u00b7ste\u00b7het", "in", "der", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wenn wir noch so viel von GOTTES Macht er-", "tokens": ["Denn", "wenn", "wir", "noch", "so", "viel", "von", "GoT\u00b7TES", "Macht", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "APPR", "NE", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kennten;", "tokens": ["kenn\u00b7ten", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und solch\u2019 Erkenntni\u00df uns dahin nicht triebe,", "tokens": ["Und", "solch'", "Er\u00b7kennt\u00b7ni\u00df", "uns", "da\u00b7hin", "nicht", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "PAV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df wir in Seiner Liebe brennten;", "tokens": ["Da\u00df", "wir", "in", "Sei\u00b7ner", "Lie\u00b7be", "brenn\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00e4re GOTTES Ehr\u2019 und Ruhm dadurch nicht gr\u00f6sser,", "tokens": ["So", "w\u00e4\u00b7re", "GoT\u00b7TES", "Ehr'", "und", "Ruhm", "da\u00b7durch", "nicht", "gr\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "KON", "NN", "PAV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhm nicht gef\u00e4lliger, nichts besser;", "tokens": ["Jhm", "nicht", "ge\u00b7f\u00e4l\u00b7li\u00b7ger", ",", "nichts", "bes\u00b7ser", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "$,", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als wenn man etwann auf der Welt", "tokens": ["Als", "wenn", "man", "et\u00b7wann", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wei\u00df, da\u00df ein Sch\u00f6pffer sey: Doch Jhn im Hertzen nicht,", "tokens": ["Wei\u00df", ",", "da\u00df", "ein", "Sch\u00f6pf\u00b7fer", "sey", ":", "Doch", "Jhn", "im", "Hert\u00b7zen", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VAFIN", "$.", "KON", "PPER", "APPRART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als wie das h\u00f6chste Gut, f\u00fcr Liebensw\u00fcrdig h\u00e4lt.", "tokens": ["Als", "wie", "das", "h\u00f6chs\u00b7te", "Gut", ",", "f\u00fcr", "Lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wie der wahre Glaub\u2019 ein Strahl in unsrer Seelen", "tokens": ["So", "wie", "der", "wah\u00b7re", "Glaub'", "ein", "Strahl", "in", "uns\u00b7rer", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von GOTTES Liebe ist; so ist der Menschen Liebe,", "tokens": ["Von", "GoT\u00b7TES", "Lie\u00b7be", "ist", ";", "so", "ist", "der", "Men\u00b7schen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "$.", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die aus dem Glauben stammet,", "tokens": ["Die", "aus", "dem", "Glau\u00b7ben", "stam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ein Gegenstrahl, wodurch die Seel\u2019 entflammet,", "tokens": ["Ein", "Ge\u00b7gen\u00b7strahl", ",", "wo\u00b7durch", "die", "Seel'", "ent\u00b7flam\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Den Sch\u00f6pfer gleichsam fasst, mit Jhm sich zu verm\u00e4hlen", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "gleich\u00b7sam", "fasst", ",", "mit", "Jhm", "sich", "zu", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "APPR", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und zu verbinden w\u00fcnscht. Hieraus erhellt nun klar,", "tokens": ["Und", "zu", "ver\u00b7bin\u00b7den", "w\u00fcnscht", ".", "Hier\u00b7aus", "er\u00b7hellt", "nun", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "$.", "PAV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df unsrer Seelen Stand, Krafft, Wesen und Natur,", "tokens": ["Da\u00df", "uns\u00b7rer", "See\u00b7len", "Stand", ",", "Krafft", ",", "We\u00b7sen", "und", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In lauter Liebe nur", "tokens": ["In", "lau\u00b7ter", "Lie\u00b7be", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Dann wird verwandelt werden m\u00fcssen.", "tokens": ["Dann", "wird", "ver\u00b7wan\u00b7delt", "wer\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sie wird nichts f\u00fchlen, nichts begehren,", "tokens": ["Sie", "wird", "nichts", "f\u00fch\u00b7len", ",", "nichts", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Nichts w\u00fcnschen, als zu GOTTES Ehren:", "tokens": ["Nichts", "w\u00fcn\u00b7schen", ",", "als", "zu", "GoT\u00b7TES", "Eh\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "KOUS", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Sie wird nichts anders wollen wissen", "tokens": ["Sie", "wird", "nichts", "an\u00b7ders", "wol\u00b7len", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Als den geliebten GOTT. Sein\u2019 Allmacht, Herrlichkeit,", "tokens": ["Als", "den", "ge\u00b7lieb\u00b7ten", "GoTT", ".", "Sein'", "All\u00b7macht", ",", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Pracht, Sch\u00f6nheit, Lieblichkeit und Vollenkommenheit", "tokens": ["Pracht", ",", "Sch\u00f6n\u00b7heit", ",", "Lieb\u00b7lich\u00b7keit", "und", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wird sie ohn Hinderni\u00df in ew\u2019ge Wonne setzen.", "tokens": ["Wird", "sie", "ohn", "Hin\u00b7der\u00b7ni\u00df", "in", "ew'\u00b7ge", "Won\u00b7ne", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie nun die Lieb aus der Erkenntni\u00df spriesset;", "tokens": ["Wie", "nun", "die", "Lieb", "aus", "der", "Er\u00b7kennt\u00b7ni\u00df", "spries\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "So stammet, sprie\u00dft und fliesset,", "tokens": ["So", "stam\u00b7met", ",", "sprie\u00dft", "und", "flies\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Aus Liebe, nichts als Lust, Vergn\u00fcgen und Ergetzen:", "tokens": ["Aus", "Lie\u00b7be", ",", "nichts", "als", "Lust", ",", "Ver\u00b7gn\u00fc\u00b7gen", "und", "Er\u00b7get\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PIS", "KOKOM", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "In der Besch\u00e4fftigung der Seele nur allein", "tokens": ["In", "der", "Be\u00b7sch\u00e4ff\u00b7ti\u00b7gung", "der", "See\u00b7le", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Entsteht das G\u00f6ttliche Geniessen.", "tokens": ["Ent\u00b7steht", "das", "G\u00f6tt\u00b7li\u00b7che", "Ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}