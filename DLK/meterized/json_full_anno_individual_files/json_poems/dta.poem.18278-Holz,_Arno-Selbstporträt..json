{"dta.poem.18278": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Selbstportr\u00e4t.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Denn ach, mein Blut rollt demokratisch", "tokens": ["Denn", "ach", ",", "mein", "Blut", "rollt", "de\u00b7mo\u00b7kra\u00b7tisch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und meine Flagge wallt und weht:", "tokens": ["Und", "mei\u00b7ne", "Flag\u00b7ge", "wallt", "und", "weht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin nur ein Tendenzpoet!", "tokens": ["Ich", "bin", "nur", "ein", "Ten\u00b7denz\u00b7poet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf Reime bin ich wie versessen", "tokens": ["Auf", "Rei\u00b7me", "bin", "ich", "wie", "ver\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "KOKOM", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u2014 Drum lob ich pl\u00f6tzlich die Tscherkessen \u2014", "tokens": ["Drum", "lob", "ich", "pl\u00f6tz\u00b7lich", "die", "Tscher\u00b7kes\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Und w\u00fcst durch mein Gehirn scherwenzen", "tokens": ["Und", "w\u00fcst", "durch", "mein", "Ge\u00b7hirn", "scher\u00b7wen\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00fcckt gewordene Sentenzen.", "tokens": ["Ver\u00b7r\u00fcckt", "ge\u00b7wor\u00b7de\u00b7ne", "Sen\u00b7ten\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Blut rollt schwarz, mein Herz schl\u00e4gt matt,", "tokens": ["Mein", "Blut", "rollt", "schwarz", ",", "mein", "Herz", "schl\u00e4gt", "matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Hirn hat noch nicht ausgegoren,", "tokens": ["Mein", "Hirn", "hat", "noch", "nicht", "aus\u00b7ge\u00b7go\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn meine gute Mutter hat", "tokens": ["Denn", "mei\u00b7ne", "gu\u00b7te", "Mut\u00b7ter", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich hundert Jahr zu fr\u00fch geboren!", "tokens": ["Mich", "hun\u00b7dert", "Jahr", "zu", "fr\u00fch", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}