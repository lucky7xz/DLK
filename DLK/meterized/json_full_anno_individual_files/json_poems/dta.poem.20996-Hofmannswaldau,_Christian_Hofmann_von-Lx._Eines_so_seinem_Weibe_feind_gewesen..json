{"dta.poem.20996": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Lx.  \n Eines so seinem Weibe feind gewesen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Teufel war das Weib ihr Bette meine Helle/", "tokens": ["Mein", "Teu\u00b7fel", "war", "das", "Weib", "ihr", "Bet\u00b7te", "mei\u00b7ne", "Hel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Marter frey zu seyn begehrt ich diese Stelle.", "tokens": ["Der", "Mar\u00b7ter", "frey", "zu", "seyn", "be\u00b7gehrt", "ich", "die\u00b7se", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VAINF", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wo mein b\u00f6ses Weib hier ihre Ruh erkiest/", "tokens": ["Doch", "wo", "mein", "b\u00f6\u00b7ses", "Weib", "hier", "ih\u00b7re", "Ruh", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So glaub ich/ da\u00df mein Leib stets in der H\u00f6llen ist.", "tokens": ["So", "glaub", "ich", "/", "da\u00df", "mein", "Leib", "stets", "in", "der", "H\u00f6l\u00b7len", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}