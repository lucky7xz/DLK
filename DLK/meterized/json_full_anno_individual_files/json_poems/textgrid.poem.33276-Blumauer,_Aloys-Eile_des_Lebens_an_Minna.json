{"textgrid.poem.33276": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Eile des Lebens an Minna", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe, unser lang gepries'nes Leben", "tokens": ["Lie\u00b7be", ",", "un\u00b7ser", "lang", "ge\u00b7pries'\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist ein einz'ger Augenblick,", "tokens": ["Ist", "ein", "einz'\u00b7ger", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O genie\u00df ihn, G\u00f6tterkr\u00e4fte geben", "tokens": ["O", "ge\u00b7nie\u00df", "ihn", ",", "G\u00f6t\u00b7ter\u00b7kr\u00e4f\u00b7te", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dir ihn nimmermehr zur\u00fcck.", "tokens": ["Dir", "ihn", "nim\u00b7mer\u00b7mehr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unaufhaltsam rollt die Zeit, und f\u00fchret", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "rollt", "die", "Zeit", ",", "und", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor und nach sich keine Spur,", "tokens": ["Vor", "und", "nach", "sic