{"dta.poem.12341": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Wirthin T\u00f6chterlein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey meines Buhlen Kopfen,               ", "tokens": ["Bey", "mei\u00b7nes", "Buh\u00b7len", "Kop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht ein g\u00fcldner Schrein,", "tokens": ["Da", "steht", "ein", "g\u00fcld\u00b7ner", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin da liegt verschlossen,", "tokens": ["Da\u00b7rin", "da", "liegt", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das junge Herze mein,", "tokens": ["Das", "jun\u00b7ge", "Her\u00b7ze", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wollt Gott, ich h\u00e4tt den Schl\u00fcssel,", "tokens": ["Wollt", "Gott", ",", "ich", "h\u00e4tt", "den", "Schl\u00fcs\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich w\u00fcrf ihn in den Rhein.", "tokens": ["Ich", "w\u00fcrf", "ihn", "in", "den", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "W\u00e4r ich bey meinem Buhlen,", "tokens": ["W\u00e4r", "ich", "bey", "mei\u00b7nem", "Buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie m\u00f6cht mir ba\u00df gefein.", "tokens": ["Wie", "m\u00f6cht", "mir", "ba\u00df", "ge\u00b7fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bey meines Buhlen F\u00fc\u00dfen,", "tokens": ["Bey", "mei\u00b7nes", "Buh\u00b7len", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da fleu\u00dft ein Br\u00fcnnlein kalt,", "tokens": ["Da", "fleu\u00dft", "ein", "Br\u00fcnn\u00b7lein", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer des Br\u00fcnnlein thut trinken,", "tokens": ["Wer", "des", "Br\u00fcnn\u00b7lein", "thut", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der j\u00fcngt und wird nicht alt;", "tokens": ["Der", "j\u00fcngt", "und", "wird", "nicht", "alt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab des Br\u00fcnnleins trunken,", "tokens": ["Ich", "hab", "des", "Br\u00fcnn\u00b7leins", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Viel manchen stolzen Trunk,", "tokens": ["Viel", "man\u00b7chen", "stol\u00b7zen", "Trunk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht lieber wollt ich w\u00fcnschen", "tokens": ["Nicht", "lie\u00b7ber", "wollt", "ich", "w\u00fcn\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Meines Buhlen rothen Mund.", "tokens": ["Mei\u00b7nes", "Buh\u00b7len", "ro\u00b7then", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In meines Buhlen Garten,", "tokens": ["In", "mei\u00b7nes", "Buh\u00b7len", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht viel edle Bl\u00fcth,", "tokens": ["Da", "steht", "viel", "ed\u00b7le", "Bl\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollt Gott, sollt ich ihr warten,", "tokens": ["Wollt", "Gott", ",", "sollt", "ich", "ihr", "war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das w\u00e4r meins Herzens Freud", "tokens": ["Das", "w\u00e4r", "meins", "Her\u00b7zens", "Freud"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die edlen R\u00f6\u00dflein brechen,", "tokens": ["Die", "ed\u00b7len", "R\u00f6\u00df\u00b7lein", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn es ist an der Zeit.", "tokens": ["Denn", "es", "ist", "an", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ich trau sie wohl zu erwerben,", "tokens": ["Ich", "trau", "sie", "wohl", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die mir am Herzen leit.", "tokens": ["Die", "mir", "am", "Her\u00b7zen", "leit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In meines Buhlen Garten,", "tokens": ["In", "mei\u00b7nes", "Buh\u00b7len", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da stehn zwey B\u00e4umelein,", "tokens": ["Da", "stehn", "zwey", "B\u00e4u\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ein das tr\u00e4gt Muskaten,", "tokens": ["Das", "ein", "das", "tr\u00e4gt", "Mus\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das andre N\u00e4gelein;", "tokens": ["Das", "and\u00b7re", "N\u00e4\u00b7ge\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Muskaten die sind s\u00fc\u00dfe,", "tokens": ["Mus\u00b7ka\u00b7ten", "die", "sind", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VAFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die N\u00e4glein riechen wohl,", "tokens": ["Die", "N\u00e4g\u00b7lein", "rie\u00b7chen", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die geb ich meinem Buhlen,", "tokens": ["Die", "geb", "ich", "mei\u00b7nem", "Buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df er mein nicht verge\u00df.", "tokens": ["Da\u00df", "er", "mein", "nicht", "ver\u00b7ge\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Und der uns diesen Reihen sang,", "tokens": ["Und", "der", "uns", "die\u00b7sen", "Rei\u00b7hen", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wohl gesungen hat,", "tokens": ["So", "wohl", "ge\u00b7sun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das haben gethan zween Hauer,", "tokens": ["Das", "ha\u00b7ben", "ge\u00b7than", "zween", "Hau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "VVFIN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu Freiberg in der Stadt;", "tokens": ["Zu", "Frei\u00b7berg", "in", "der", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben so wohl gesungen", "tokens": ["Sie", "ha\u00b7ben", "so", "wohl", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bey Meth und k\u00fchlem Wein,", "tokens": ["Bey", "Me\u00b7th", "und", "k\u00fch\u00b7lem", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dabey da ist gesessen,", "tokens": ["Da\u00b7bey", "da", "ist", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Wirthin T\u00f6chterlein.", "tokens": ["Der", "Wirt\u00b7hin", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}