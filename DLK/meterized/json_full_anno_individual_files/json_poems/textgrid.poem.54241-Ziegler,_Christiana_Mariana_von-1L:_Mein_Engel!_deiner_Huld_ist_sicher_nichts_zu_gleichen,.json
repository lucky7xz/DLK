{"textgrid.poem.54241": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Engel! deiner Huld ist sicher nichts zu gleichen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Engel! deiner Huld ist sicher nichts zu gleichen,", "tokens": ["Mein", "En\u00b7gel", "!", "dei\u00b7ner", "Huld", "ist", "si\u00b7cher", "nichts", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich tauschte, glaube mir, davor kein K\u00e4yserthum,", "tokens": ["Ich", "tauschte", ",", "glau\u00b7be", "mir", ",", "da\u00b7vor", "kein", "K\u00e4y\u00b7ser\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PAV", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie k\u00f6nt ich selbge wohl in h\u00f6hern Grad erreichen?", "tokens": ["Wie", "k\u00f6nt", "ich", "selb\u00b7ge", "wohl", "in", "h\u00f6\u00b7hern", "Grad", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein gantzes Hertz ergibt sich mir zum Eigenthum.", "tokens": ["Dein", "gant\u00b7zes", "Hertz", "er\u00b7gibt", "sich", "mir", "zum", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du schmeichelst meinen Blat, als ob es dich erg\u00f6tzte,", "tokens": ["Du", "schmei\u00b7chelst", "mei\u00b7nen", "Blat", ",", "als", "ob", "es", "dich", "er\u00b7g\u00f6tz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Galante Bre\u00dflerin, wie schertzt dein netter Kiel?", "tokens": ["Ga\u00b7lan\u00b7te", "Bre\u00df\u00b7le\u00b7rin", ",", "wie", "schertzt", "dein", "net\u00b7ter", "Kiel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja wann selbst deine Hand die Sylben vor mich setzte", "tokens": ["Ja", "wann", "selbst", "dei\u00b7ne", "Hand", "die", "Syl\u00b7ben", "vor", "mich", "setz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PWAV", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So glaub ich, da\u00df so dann dir meine Schrift gefiel.", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "so", "dann", "dir", "mei\u00b7ne", "Schrift", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Unverm\u00f6gen will mich mehr als deutlich lehren,", "tokens": ["Mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "will", "mich", "mehr", "als", "deut\u00b7lich", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIAT", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df meine St\u00fcmperey vor nichts zu sch\u00e4tzen ist;", "tokens": ["Da\u00df", "mei\u00b7ne", "St\u00fcm\u00b7pe\u00b7rey", "vor", "nichts", "zu", "sch\u00e4t\u00b7zen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Ruhm, der dir geb\u00fchrt, kan mir nicht zugeh\u00f6ren,", "tokens": ["Der", "Ruhm", ",", "der", "dir", "ge\u00b7b\u00fchrt", ",", "kan", "mir", "nicht", "zu\u00b7ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ich wei\u00df mehr als zu wohl, da\u00df du die Sonne bist.", "tokens": ["Ich", "wei\u00df", "mehr", "als", "zu", "wohl", ",", "da\u00df", "du", "die", "Son\u00b7ne", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "APPR", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum m\u00fcssen wir vor dir uns wie die Sterne neigen;", "tokens": ["Drum", "m\u00fcs\u00b7sen", "wir", "vor", "dir", "uns", "wie", "die", "Ster\u00b7ne", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PPER", "PRF", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gesetzt, da\u00df auch dein Strahl uns Licht und Glantz verleiht,", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "auch", "dein", "Strahl", "uns", "Licht", "und", "Glantz", "ver\u00b7leiht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kan doch der Schimmer nicht so hoch, als deiner, steigen,", "tokens": ["Kan", "doch", "der", "Schim\u00b7mer", "nicht", "so", "hoch", ",", "als", "dei\u00b7ner", ",", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn alle Welt erkennt an dir die Treflichkeit.", "tokens": ["Denn", "al\u00b7le", "Welt", "er\u00b7kennt", "an", "dir", "die", "Tref\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der gantze Musen-Chor mu\u00df mir hier Beyfal geben,", "tokens": ["Der", "gant\u00b7ze", "Mu\u00b7sen\u00b7Chor", "mu\u00df", "mir", "hier", "Bey\u00b7fal", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und also bleibt di\u00df Blat von Lacq und Schmincke frey.", "tokens": ["Und", "al\u00b7so", "bleibt", "di\u00df", "Blat", "von", "Lacq", "und", "Schmin\u00b7cke", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PDS", "NN", "APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "An meinen Hertzen sieht man keinen F\u00fcrni\u00df kleben,", "tokens": ["An", "mei\u00b7nen", "Hert\u00b7zen", "sieht", "man", "kei\u00b7nen", "F\u00fcr\u00b7ni\u00df", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Drum ha\u00dft so Mund und Kiel auch alle Heucheley.", "tokens": ["Drum", "ha\u00dft", "so", "Mund", "und", "Kiel", "auch", "al\u00b7le", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "KON", "NE", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O! Soll ich nicht einmahl den frohen Tag erblicken?", "tokens": ["O", "!", "Soll", "ich", "nicht", "ein\u00b7mahl", "den", "fro\u00b7hen", "Tag", "er\u00b7bli\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "An den ich, Edle, dich pers\u00f6nlich k\u00fcssen kan,", "tokens": ["An", "den", "ich", ",", "Ed\u00b7le", ",", "dich", "per\u00b7s\u00f6n\u00b7lich", "k\u00fcs\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "ADJA", "$,", "PRF", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Freude w\u00fcrde mich aus Schranck und Zirckul r\u00fccken,", "tokens": ["Die", "Freu\u00b7de", "w\u00fcr\u00b7de", "mich", "aus", "Schranck", "und", "Zir\u00b7ckul", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich tr\u00e4ffe gantz gewi\u00df schon hier den Himmel an;", "tokens": ["Ich", "tr\u00e4f\u00b7fe", "gantz", "ge\u00b7wi\u00df", "schon", "hier", "den", "Him\u00b7mel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein Umgang w\u00fcrde mir die Stunden s\u00fcsse machen,", "tokens": ["Dein", "Um\u00b7gang", "w\u00fcr\u00b7de", "mir", "die", "Stun\u00b7den", "s\u00fcs\u00b7se", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich spei\u00dft' und tr\u00e4ncke nicht, so lang ich dich vor mir,", "tokens": ["Ich", "spei\u00dft'", "und", "tr\u00e4n\u00b7cke", "nicht", ",", "so", "lang", "ich", "dich", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "ADV", "ADJD", "PPER", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du Pallas unsrer Zeit, leibhafftig s\u00e4he lachen,", "tokens": ["Du", "Pal\u00b7las", "uns\u00b7rer", "Zeit", ",", "leib\u00b7haff\u00b7tig", "s\u00e4\u00b7he", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn meine Sehnsucht ist gar ungemein nach dir.", "tokens": ["Denn", "mei\u00b7ne", "Sehn\u00b7sucht", "ist", "gar", "un\u00b7ge\u00b7mein", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jedoch was gie\u00df ich Oel in meiner Sehnsucht Flammen?", "tokens": ["Je\u00b7doch", "was", "gie\u00df", "ich", "O\u00b7el", "in", "mei\u00b7ner", "Sehn\u00b7sucht", "Flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.30": {"text": "Was greiffet meine Hand nach Schatten, Wind und Lufft?", "tokens": ["Was", "greif\u00b7fet", "mei\u00b7ne", "Hand", "nach", "Schat\u00b7ten", ",", "Wind", "und", "Lufft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df, wir kommen doch, mein Engel, nicht zusammen,", "tokens": ["Ich", "wei\u00df", ",", "wir", "kom\u00b7men", "doch", ",", "mein", "En\u00b7gel", ",", "nicht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn zwischen mir und dir ist eine grosse Glufft.", "tokens": ["Denn", "zwi\u00b7schen", "mir", "und", "dir", "ist", "ei\u00b7ne", "gros\u00b7se", "Glufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Di\u00df mu\u00df mir allen Schmertz, Annehmlichste, vers\u00fcssen,", "tokens": ["Di\u00df", "mu\u00df", "mir", "al\u00b7len", "Schmertz", ",", "An\u00b7nehm\u00b7lichs\u00b7te", ",", "ver\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIAT", "NN", "$,", "NE", "$,", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Da\u00df ich, obgleich dein Mund von mir entfernet hei\u00dft,", "tokens": ["Da\u00df", "ich", ",", "ob\u00b7gleich", "dein", "Mund", "von", "mir", "ent\u00b7fer\u00b7net", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Noch deine Feder kan an jenes Stelle k\u00fcssen,", "tokens": ["Noch", "dei\u00b7ne", "Fe\u00b7der", "kan", "an", "je\u00b7nes", "Stel\u00b7le", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die mich mit Ambra-Kost und Lieblichkeiten spei\u00dft.", "tokens": ["Die", "mich", "mit", "Am\u00b7bra\u00b7Kost", "und", "Lieb\u00b7lich\u00b7kei\u00b7ten", "spei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "La\u00df deine Muse mich fein \u00f6ffters nur besuchen,", "tokens": ["La\u00df", "dei\u00b7ne", "Mu\u00b7se", "mich", "fein", "\u00f6ff\u00b7ters", "nur", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADJD", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ihr Zuspruch fl\u00f6sset mir so Geist als Leben ein.", "tokens": ["Ihr", "Zu\u00b7spruch", "fl\u00f6s\u00b7set", "mir", "so", "Geist", "als", "Le\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Will das Verh\u00e4ngni\u00df gleich uns durch Entfernung fluchen,", "tokens": ["Will", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "gleich", "uns", "durch", "Ent\u00b7fer\u00b7nung", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So soll doch unser Geist beysammen st\u00fcndlich seyn.", "tokens": ["So", "soll", "doch", "un\u00b7ser", "Geist", "bey\u00b7sam\u00b7men", "st\u00fcnd\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "VVIZU", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "H\u00f6r ich von deinen Wohl, so will ichs mit dir theilen,", "tokens": ["H\u00f6r", "ich", "von", "dei\u00b7nen", "Wohl", ",", "so", "will", "ichs", "mit", "dir", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und schreckte dich ein Fall, so f\u00fchlt ich auch den Ri\u00df;", "tokens": ["Und", "schreck\u00b7te", "dich", "ein", "Fall", ",", "so", "f\u00fchlt", "ich", "auch", "den", "Ri\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denn wohntest du von mir noch mehr als hundert Meilen,", "tokens": ["Denn", "wohn\u00b7test", "du", "von", "mir", "noch", "mehr", "als", "hun\u00b7dert", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PIAT", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So w\u00fcrckt die Sympathie doch zwischen uns gewi\u00df.", "tokens": ["So", "w\u00fcrckt", "die", "Sym\u00b7pa\u00b7thie", "doch", "zwi\u00b7schen", "uns", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Engel! deiner Huld ist sicher nichts zu gleichen,", "tokens": ["Mein", "En\u00b7gel", "!", "dei\u00b7ner", "Huld", "ist", "si\u00b7cher", "nichts", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich tauschte, glaube mir, davor kein K\u00e4yserthum,", "tokens": ["Ich", "tauschte", ",", "glau\u00b7be", "mir", ",", "da\u00b7vor", "kein", "K\u00e4y\u00b7ser\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PAV", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie k\u00f6nt ich selbge wohl in h\u00f6hern Grad erreichen?", "tokens": ["Wie", "k\u00f6nt", "ich", "selb\u00b7ge", "wohl", "in", "h\u00f6\u00b7hern", "Grad", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein gantzes Hertz ergibt sich mir zum Eigenthum.", "tokens": ["Dein", "gant\u00b7zes", "Hertz", "er\u00b7gibt", "sich", "mir", "zum", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du schmeichelst meinen Blat, als ob es dich erg\u00f6tzte,", "tokens": ["Du", "schmei\u00b7chelst", "mei\u00b7nen", "Blat", ",", "als", "ob", "es", "dich", "er\u00b7g\u00f6tz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOKOM", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Galante Bre\u00dflerin, wie schertzt dein netter Kiel?", "tokens": ["Ga\u00b7lan\u00b7te", "Bre\u00df\u00b7le\u00b7rin", ",", "wie", "schertzt", "dein", "net\u00b7ter", "Kiel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja wann selbst deine Hand die Sylben vor mich setzte", "tokens": ["Ja", "wann", "selbst", "dei\u00b7ne", "Hand", "die", "Syl\u00b7ben", "vor", "mich", "setz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PWAV", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So glaub ich, da\u00df so dann dir meine Schrift gefiel.", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "so", "dann", "dir", "mei\u00b7ne", "Schrift", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Unverm\u00f6gen will mich mehr als deutlich lehren,", "tokens": ["Mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "will", "mich", "mehr", "als", "deut\u00b7lich", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PIAT", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df meine St\u00fcmperey vor nichts zu sch\u00e4tzen ist;", "tokens": ["Da\u00df", "mei\u00b7ne", "St\u00fcm\u00b7pe\u00b7rey", "vor", "nichts", "zu", "sch\u00e4t\u00b7zen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Ruhm, der dir geb\u00fchrt, kan mir nicht zugeh\u00f6ren,", "tokens": ["Der", "Ruhm", ",", "der", "dir", "ge\u00b7b\u00fchrt", ",", "kan", "mir", "nicht", "zu\u00b7ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ich wei\u00df mehr als zu wohl, da\u00df du die Sonne bist.", "tokens": ["Ich", "wei\u00df", "mehr", "als", "zu", "wohl", ",", "da\u00df", "du", "die", "Son\u00b7ne", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "APPR", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum m\u00fcssen wir vor dir uns wie die Sterne neigen;", "tokens": ["Drum", "m\u00fcs\u00b7sen", "wir", "vor", "dir", "uns", "wie", "die", "Ster\u00b7ne", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PPER", "PRF", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gesetzt, da\u00df auch dein Strahl uns Licht und Glantz verleiht,", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "auch", "dein", "Strahl", "uns", "Licht", "und", "Glantz", "ver\u00b7leiht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kan doch der Schimmer nicht so hoch, als deiner, steigen,", "tokens": ["Kan", "doch", "der", "Schim\u00b7mer", "nicht", "so", "hoch", ",", "als", "dei\u00b7ner", ",", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn alle Welt erkennt an dir die Treflichkeit.", "tokens": ["Denn", "al\u00b7le", "Welt", "er\u00b7kennt", "an", "dir", "die", "Tref\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der gantze Musen-Chor mu\u00df mir hier Beyfal geben,", "tokens": ["Der", "gant\u00b7ze", "Mu\u00b7sen\u00b7Chor", "mu\u00df", "mir", "hier", "Bey\u00b7fal", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und also bleibt di\u00df Blat von Lacq und Schmincke frey.", "tokens": ["Und", "al\u00b7so", "bleibt", "di\u00df", "Blat", "von", "Lacq", "und", "Schmin\u00b7cke", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PDS", "NN", "APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "An meinen Hertzen sieht man keinen F\u00fcrni\u00df kleben,", "tokens": ["An", "mei\u00b7nen", "Hert\u00b7zen", "sieht", "man", "kei\u00b7nen", "F\u00fcr\u00b7ni\u00df", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Drum ha\u00dft so Mund und Kiel auch alle Heucheley.", "tokens": ["Drum", "ha\u00dft", "so", "Mund", "und", "Kiel", "auch", "al\u00b7le", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "KON", "NE", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O! Soll ich nicht einmahl den frohen Tag erblicken?", "tokens": ["O", "!", "Soll", "ich", "nicht", "ein\u00b7mahl", "den", "fro\u00b7hen", "Tag", "er\u00b7bli\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "An den ich, Edle, dich pers\u00f6nlich k\u00fcssen kan,", "tokens": ["An", "den", "ich", ",", "Ed\u00b7le", ",", "dich", "per\u00b7s\u00f6n\u00b7lich", "k\u00fcs\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "ADJA", "$,", "PRF", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Freude w\u00fcrde mich aus Schranck und Zirckul r\u00fccken,", "tokens": ["Die", "Freu\u00b7de", "w\u00fcr\u00b7de", "mich", "aus", "Schranck", "und", "Zir\u00b7ckul", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich tr\u00e4ffe gantz gewi\u00df schon hier den Himmel an;", "tokens": ["Ich", "tr\u00e4f\u00b7fe", "gantz", "ge\u00b7wi\u00df", "schon", "hier", "den", "Him\u00b7mel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein Umgang w\u00fcrde mir die Stunden s\u00fcsse machen,", "tokens": ["Dein", "Um\u00b7gang", "w\u00fcr\u00b7de", "mir", "die", "Stun\u00b7den", "s\u00fcs\u00b7se", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich spei\u00dft' und tr\u00e4ncke nicht, so lang ich dich vor mir,", "tokens": ["Ich", "spei\u00dft'", "und", "tr\u00e4n\u00b7cke", "nicht", ",", "so", "lang", "ich", "dich", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "ADV", "ADJD", "PPER", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du Pallas unsrer Zeit, leibhafftig s\u00e4he lachen,", "tokens": ["Du", "Pal\u00b7las", "uns\u00b7rer", "Zeit", ",", "leib\u00b7haff\u00b7tig", "s\u00e4\u00b7he", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn meine Sehnsucht ist gar ungemein nach dir.", "tokens": ["Denn", "mei\u00b7ne", "Sehn\u00b7sucht", "ist", "gar", "un\u00b7ge\u00b7mein", "nach", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jedoch was gie\u00df ich Oel in meiner Sehnsucht Flammen?", "tokens": ["Je\u00b7doch", "was", "gie\u00df", "ich", "O\u00b7el", "in", "mei\u00b7ner", "Sehn\u00b7sucht", "Flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.30": {"text": "Was greiffet meine Hand nach Schatten, Wind und Lufft?", "tokens": ["Was", "greif\u00b7fet", "mei\u00b7ne", "Hand", "nach", "Schat\u00b7ten", ",", "Wind", "und", "Lufft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df, wir kommen doch, mein Engel, nicht zusammen,", "tokens": ["Ich", "wei\u00df", ",", "wir", "kom\u00b7men", "doch", ",", "mein", "En\u00b7gel", ",", "nicht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn zwischen mir und dir ist eine grosse Glufft.", "tokens": ["Denn", "zwi\u00b7schen", "mir", "und", "dir", "ist", "ei\u00b7ne", "gros\u00b7se", "Glufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Di\u00df mu\u00df mir allen Schmertz, Annehmlichste, vers\u00fcssen,", "tokens": ["Di\u00df", "mu\u00df", "mir", "al\u00b7len", "Schmertz", ",", "An\u00b7nehm\u00b7lichs\u00b7te", ",", "ver\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIAT", "NN", "$,", "NE", "$,", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Da\u00df ich, obgleich dein Mund von mir entfernet hei\u00dft,", "tokens": ["Da\u00df", "ich", ",", "ob\u00b7gleich", "dein", "Mund", "von", "mir", "ent\u00b7fer\u00b7net", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Noch deine Feder kan an jenes Stelle k\u00fcssen,", "tokens": ["Noch", "dei\u00b7ne", "Fe\u00b7der", "kan", "an", "je\u00b7nes", "Stel\u00b7le", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die mich mit Ambra-Kost und Lieblichkeiten spei\u00dft.", "tokens": ["Die", "mich", "mit", "Am\u00b7bra\u00b7Kost", "und", "Lieb\u00b7lich\u00b7kei\u00b7ten", "spei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "La\u00df deine Muse mich fein \u00f6ffters nur besuchen,", "tokens": ["La\u00df", "dei\u00b7ne", "Mu\u00b7se", "mich", "fein", "\u00f6ff\u00b7ters", "nur", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADJD", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ihr Zuspruch fl\u00f6sset mir so Geist als Leben ein.", "tokens": ["Ihr", "Zu\u00b7spruch", "fl\u00f6s\u00b7set", "mir", "so", "Geist", "als", "Le\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Will das Verh\u00e4ngni\u00df gleich uns durch Entfernung fluchen,", "tokens": ["Will", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "gleich", "uns", "durch", "Ent\u00b7fer\u00b7nung", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So soll doch unser Geist beysammen st\u00fcndlich seyn.", "tokens": ["So", "soll", "doch", "un\u00b7ser", "Geist", "bey\u00b7sam\u00b7men", "st\u00fcnd\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "VVIZU", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "H\u00f6r ich von deinen Wohl, so will ichs mit dir theilen,", "tokens": ["H\u00f6r", "ich", "von", "dei\u00b7nen", "Wohl", ",", "so", "will", "ichs", "mit", "dir", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und schreckte dich ein Fall, so f\u00fchlt ich auch den Ri\u00df;", "tokens": ["Und", "schreck\u00b7te", "dich", "ein", "Fall", ",", "so", "f\u00fchlt", "ich", "auch", "den", "Ri\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Denn wohntest du von mir noch mehr als hundert Meilen,", "tokens": ["Denn", "wohn\u00b7test", "du", "von", "mir", "noch", "mehr", "als", "hun\u00b7dert", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PIAT", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So w\u00fcrckt die Sympathie doch zwischen uns gewi\u00df.", "tokens": ["So", "w\u00fcrckt", "die", "Sym\u00b7pa\u00b7thie", "doch", "zwi\u00b7schen", "uns", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}