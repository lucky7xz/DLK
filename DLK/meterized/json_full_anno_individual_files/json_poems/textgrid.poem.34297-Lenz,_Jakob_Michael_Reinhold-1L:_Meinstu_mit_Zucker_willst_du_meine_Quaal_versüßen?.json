", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ITJ", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df ich im Wege bin \u2013 so thu es beste Hand!", "tokens": ["Da\u00df", "ich", "im", "We\u00b7ge", "bin", "\u2013", "so", "thu", "es", "bes\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich mu\u00df mir t\u00e4glich nur das Leben mehr verbittern,", "tokens": ["Ich", "mu\u00df", "mir", "t\u00e4g\u00b7lich", "nur", "das", "Le\u00b7ben", "mehr", "ver\u00b7bit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und thust du's nicht \u2013 dann Gott! erhalt mir den Verstand! \u2013", "tokens": ["Und", "thust", "du's", "nicht", "\u2013", "dann", "Gott", "!", "er\u00b7halt", "mir", "den", "Ver\u00b7stand", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "$(", "ADV", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}