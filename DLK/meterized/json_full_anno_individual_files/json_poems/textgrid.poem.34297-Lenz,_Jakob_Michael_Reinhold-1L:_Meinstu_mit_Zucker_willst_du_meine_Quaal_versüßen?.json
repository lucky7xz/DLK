{"textgrid.poem.34297": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meinstu mit Zucker willst du meine Quaal vers\u00fc\u00dfen?", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meinstu mit Zucker willst du meine Quaal vers\u00fc\u00dfen?", "tokens": ["Meins\u00b7tu", "mit", "Zu\u00b7cker", "willst", "du", "mei\u00b7ne", "Qua\u00b7al", "ver\u00b7s\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mitleidig g\u00f6ttlich Herz! wie wenig kennstu sie?", "tokens": ["Mit\u00b7lei\u00b7dig", "g\u00f6tt\u00b7lich", "Herz", "!", "wie", "we\u00b7nig", "kenns\u00b7tu", "sie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$.", "PWAV", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sich nach Mitternacht die nassen Augen schlie\u00dfen", "tokens": ["Wenn", "sich", "nach", "Mit\u00b7ter\u00b7nacht", "die", "nas\u00b7sen", "Au\u00b7gen", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schl\u00e4ft doch mein Herz nicht ein, es w\u00fctet spat und fr\u00fch.", "tokens": ["Schl\u00e4ft", "doch", "mein", "Herz", "nicht", "ein", ",", "es", "w\u00fc\u00b7tet", "spat", "und", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor Tage lieg ich schon und sinn auf mein Verderben", "tokens": ["Vor", "Ta\u00b7ge", "lieg", "ich", "schon", "und", "sinn", "auf", "mein", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und straffe mich oft selbst und nehm' mir Tugend vor", "tokens": ["Und", "straf\u00b7fe", "mich", "oft", "selbst", "und", "nehm'", "mir", "Tu\u00b7gend", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "PPER", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und k\u00e4mpf und ring mit mir und sterb und kann nicht sterben,", "tokens": ["Und", "k\u00e4mpf", "und", "ring", "mit", "mir", "und", "sterb", "und", "kann", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "APPR", "PPER", "KON", "ADJD", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil mich mein Unstern nur zum Leiden auserkohr.", "tokens": ["Weil", "mich", "mein", "Uns\u00b7tern", "nur", "zum", "Lei\u00b7den", "au\u00b7ser\u00b7kohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich soll dich sehn und fliehn? Dein L\u00e4cheln sehn und meiden?", "tokens": ["Ich", "soll", "dich", "sehn", "und", "fliehn", "?", "Dein", "L\u00e4\u00b7cheln", "sehn", "und", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$.", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und du verstehst es wohl, wo mirs am wehsten thut.", "tokens": ["Und", "du", "ver\u00b7stehst", "es", "wohl", ",", "wo", "mirs", "am", "wehs\u00b7ten", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du ha\u00dfest meine Ruh, es scheint dich freut mein Leiden,", "tokens": ["Du", "ha\u00b7\u00dfest", "mei\u00b7ne", "Ruh", ",", "es", "scheint", "dich", "freut", "mein", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du w\u00fcnschst es gr\u00f6\u00dfer noch, es scheint du willst mein Blut.", "tokens": ["Du", "w\u00fcnschst", "es", "gr\u00f6\u00b7\u00dfer", "noch", ",", "es", "scheint", "du", "willst", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "$,", "PPER", "VVFIN", "PPER", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So nimm es g\u00f6ttliche! ein kleines Federmesser", "tokens": ["So", "nimm", "es", "g\u00f6tt\u00b7li\u00b7che", "!", "ein", "klei\u00b7nes", "Fe\u00b7der\u00b7mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJA", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er\u00f6fnet mir die Brust, wie sanft w\u00fcrd es mir thun?", "tokens": ["Er\u00b7\u00f6f\u00b7net", "mir", "die", "Brust", ",", "wie", "sanft", "w\u00fcrd", "es", "mir", "thun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach thus, durchbor mein Herz, gewi\u00df dann wird mir besser,", "tokens": ["Ach", "thus", ",", "durch\u00b7bor", "mein", "Herz", ",", "ge\u00b7wi\u00df", "dann", "wird", "mir", "bes\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In deinen Armen will ich dann vom Leben ruhn.", "tokens": ["In", "dei\u00b7nen", "Ar\u00b7men", "will", "ich", "dann", "vom", "Le\u00b7ben", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach welche S\u00fcssigkeit! von Lieb und Wollust trunken", "tokens": ["Ach", "wel\u00b7che", "S\u00fcs\u00b7sig\u00b7keit", "!", "von", "Lieb", "und", "Wol\u00b7lust", "trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAT", "NN", "$.", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Schl\u00e4ft dann mein mattes Haupt von seiner Unruh ein,", "tokens": ["Schl\u00e4ft", "dann", "mein", "mat\u00b7tes", "Haupt", "von", "sei\u00b7ner", "Un\u00b7ruh", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auf deinen s\u00fc\u00dfen Schoo\u00df verliebt herabgesunken,", "tokens": ["Auf", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Schoo\u00df", "ver\u00b7liebt", "her\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und k\u00fc\u00dfet sterbend noch die Ursach seiner Pein.", "tokens": ["Und", "k\u00fc\u00b7\u00dfet", "ster\u00b7bend", "noch", "die", "Ur\u00b7sach", "sei\u00b7ner", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ja thus! von deiner Hand wie kann der Tod mich schr\u00f6cken?", "tokens": ["Ja", "thus", "!", "von", "dei\u00b7ner", "Hand", "wie", "kann", "der", "Tod", "mich", "schr\u00f6\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "$.", "APPR", "PPOSAT", "NN", "KOKOM", "VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es ist das gr\u00f6ste Gl\u00fcck, das ich erhalten kann.", "tokens": ["Es", "ist", "das", "gr\u00f6s\u00b7te", "Gl\u00fcck", ",", "das", "ich", "er\u00b7hal\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein Sto\u00df, so ists geschehn: wie s\u00fc\u00df wird er mir schmecken,", "tokens": ["Ein", "Sto\u00df", ",", "so", "ists", "ge\u00b7schehn", ":", "wie", "s\u00fc\u00df", "wird", "er", "mir", "schme\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "VVPP", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein kleiner Sto\u00df und dann geht erst mein Leben an.", "tokens": ["Ein", "klei\u00b7ner", "Sto\u00df", "und", "dann", "geht", "erst", "mein", "Le\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Dann will ich z\u00e4rtlich dir als Geist zur Seite schweben,", "tokens": ["Dann", "will", "ich", "z\u00e4rt\u00b7lich", "dir", "als", "Geist", "zur", "Sei\u00b7te", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "KOUS", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dann wehrt es niemand mir, du selber wehrst es nicht;", "tokens": ["Dann", "wehrt", "es", "nie\u00b7mand", "mir", ",", "du", "sel\u00b7ber", "wehrst", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PPER", "$,", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn darf ich ungescheut dem Munde K\u00fcsse geben,", "tokens": ["Denn", "darf", "ich", "un\u00b7ge\u00b7scheut", "dem", "Mun\u00b7de", "K\u00fcs\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der so verf\u00fchrisch lacht und so bezaubernd spricht.", "tokens": ["Der", "so", "ver\u00b7f\u00fch\u00b7risch", "lacht", "und", "so", "be\u00b7zau\u00b7bernd", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dann darf so lang ich will mein Auge nach dir sehnen,", "tokens": ["Dann", "darf", "so", "lang", "ich", "will", "mein", "Au\u00b7ge", "nach", "dir", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADJD", "PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn hasch ich deinen Blick und schlie\u00df ihn in mein Herz.", "tokens": ["Denn", "hasch", "ich", "dei\u00b7nen", "Blick", "und", "schlie\u00df", "ihn", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn wein ich, wenn ich will, und niemand schilt die Tr\u00e4nen,", "tokens": ["Denn", "wein", "ich", ",", "wenn", "ich", "will", ",", "und", "nie\u00b7mand", "schilt", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dann seufz ich, wenn ich will, und niemand schilt den Schmerz.", "tokens": ["Dann", "seufz", "ich", ",", "wenn", "ich", "will", ",", "und", "nie\u00b7mand", "schilt", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dann will ich dir im Traum zu deinen F\u00fcssen liegen", "tokens": ["Dann", "will", "ich", "dir", "im", "Traum", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und wachend horch ich auf, wie dirs im Busen schl\u00e4gt.", "tokens": ["Und", "wa\u00b7chend", "horch", "ich", "auf", ",", "wie", "dirs", "im", "Bu\u00b7sen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bistu vergn\u00fcgt, o Gl\u00fcck! so theil ich dein Vergn\u00fcgen,", "tokens": ["Bis\u00b7tu", "ver\u00b7gn\u00fcgt", ",", "o", "Gl\u00fcck", "!", "so", "theil", "ich", "dein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "FM", "NN", "$.", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wo nicht, so theil ich auch, was dir Verdru\u00df erregt.", "tokens": ["Wo", "nicht", ",", "so", "theil", "ich", "auch", ",", "was", "dir", "Ver\u00b7dru\u00df", "er\u00b7regt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "KOUS", "PPER", "ADV", "$,", "PWS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dann mein unsch\u00e4tzbar Gut! dann straft mich das Gewissen", "tokens": ["Dann", "mein", "un\u00b7sch\u00e4tz\u00b7bar", "Gut", "!", "dann", "straft", "mich", "das", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJD", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "F\u00fcr meine Liebe nicht, nur dann, dann steht mirs frey;", "tokens": ["F\u00fcr", "mei\u00b7ne", "Lie\u00b7be", "nicht", ",", "nur", "dann", ",", "dann", "steht", "mirs", "frey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$,", "ADV", "ADV", "$,", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dann f\u00fchl ich keinen mehr von den verha\u00dften Bissen", "tokens": ["Dann", "f\u00fchl", "ich", "kei\u00b7nen", "mehr", "von", "den", "ver\u00b7ha\u00df\u00b7ten", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Als ob ich Frevler Schuld an deiner Unruh sey.", "tokens": ["Als", "ob", "ich", "Frev\u00b7ler", "Schuld", "an", "dei\u00b7ner", "Un\u00b7ruh", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dann bistu meiner lo\u00df, nicht wahr du bist es m\u00fcde", "tokens": ["Dann", "bis\u00b7tu", "mei\u00b7ner", "lo\u00df", ",", "nicht", "wahr", "du", "bist", "es", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "PTKVZ", "$,", "PTKNEG", "ADJD", "PPER", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Von mir gekr\u00e4nkt zu seyn? dann wei\u00dftu es nicht mehr", "tokens": ["Von", "mir", "ge\u00b7kr\u00e4nkt", "zu", "seyn", "?", "dann", "wei\u00df\u00b7tu", "es", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Was mich schmerzt oder nicht, denn hast du ewig Friede,", "tokens": ["Was", "mich", "schmerzt", "o\u00b7der", "nicht", ",", "denn", "hast", "du", "e\u00b7wig", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PTKNEG", "$,", "KON", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Denn nach dem Tode r\u00fchrt mein Schmerz dich nicht so sehr.", "tokens": ["Denn", "nach", "dem", "To\u00b7de", "r\u00fchrt", "mein", "Schmerz", "dich", "nicht", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Selbst ach! dein Gl\u00fcck verlangts, ich f\u00fchl es, ach! mit Zittern,", "tokens": ["Selbst", "ach", "!", "dein", "Gl\u00fcck", "ver\u00b7langts", ",", "ich", "f\u00fchl", "es", ",", "ach", "!", "mit", "Zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ITJ", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df ich im Wege bin \u2013 so thu es beste Hand!", "tokens": ["Da\u00df", "ich", "im", "We\u00b7ge", "bin", "\u2013", "so", "thu", "es", "bes\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich mu\u00df mir t\u00e4glich nur das Leben mehr verbittern,", "tokens": ["Ich", "mu\u00df", "mir", "t\u00e4g\u00b7lich", "nur", "das", "Le\u00b7ben", "mehr", "ver\u00b7bit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und thust du's nicht \u2013 dann Gott! erhalt mir den Verstand! \u2013", "tokens": ["Und", "thust", "du's", "nicht", "\u2013", "dann", "Gott", "!", "er\u00b7halt", "mir", "den", "Ver\u00b7stand", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "$(", "ADV", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Meinstu mit Zucker willst du meine Quaal vers\u00fc\u00dfen?", "tokens": ["Meins\u00b7tu", "mit", "Zu\u00b7cker", "willst", "du", "mei\u00b7ne", "Qua\u00b7al", "ver\u00b7s\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mitleidig g\u00f6ttlich Herz! wie wenig kennstu sie?", "tokens": ["Mit\u00b7lei\u00b7dig", "g\u00f6tt\u00b7lich", "Herz", "!", "wie", "we\u00b7nig", "kenns\u00b7tu", "sie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$.", "PWAV", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sich nach Mitternacht die nassen Augen schlie\u00dfen", "tokens": ["Wenn", "sich", "nach", "Mit\u00b7ter\u00b7nacht", "die", "nas\u00b7sen", "Au\u00b7gen", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schl\u00e4ft doch mein Herz nicht ein, es w\u00fctet spat und fr\u00fch.", "tokens": ["Schl\u00e4ft", "doch", "mein", "Herz", "nicht", "ein", ",", "es", "w\u00fc\u00b7tet", "spat", "und", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor Tage lieg ich schon und sinn auf mein Verderben", "tokens": ["Vor", "Ta\u00b7ge", "lieg", "ich", "schon", "und", "sinn", "auf", "mein", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und straffe mich oft selbst und nehm' mir Tugend vor", "tokens": ["Und", "straf\u00b7fe", "mich", "oft", "selbst", "und", "nehm'", "mir", "Tu\u00b7gend", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "PPER", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und k\u00e4mpf und ring mit mir und sterb und kann nicht sterben,", "tokens": ["Und", "k\u00e4mpf", "und", "ring", "mit", "mir", "und", "sterb", "und", "kann", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "APPR", "PPER", "KON", "ADJD", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil mich mein Unstern nur zum Leiden auserkohr.", "tokens": ["Weil", "mich", "mein", "Uns\u00b7tern", "nur", "zum", "Lei\u00b7den", "au\u00b7ser\u00b7kohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich soll dich sehn und fliehn? Dein L\u00e4cheln sehn und meiden?", "tokens": ["Ich", "soll", "dich", "sehn", "und", "fliehn", "?", "Dein", "L\u00e4\u00b7cheln", "sehn", "und", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$.", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und du verstehst es wohl, wo mirs am wehsten thut.", "tokens": ["Und", "du", "ver\u00b7stehst", "es", "wohl", ",", "wo", "mirs", "am", "wehs\u00b7ten", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du ha\u00dfest meine Ruh, es scheint dich freut mein Leiden,", "tokens": ["Du", "ha\u00b7\u00dfest", "mei\u00b7ne", "Ruh", ",", "es", "scheint", "dich", "freut", "mein", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du w\u00fcnschst es gr\u00f6\u00dfer noch, es scheint du willst mein Blut.", "tokens": ["Du", "w\u00fcnschst", "es", "gr\u00f6\u00b7\u00dfer", "noch", ",", "es", "scheint", "du", "willst", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "$,", "PPER", "VVFIN", "PPER", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So nimm es g\u00f6ttliche! ein kleines Federmesser", "tokens": ["So", "nimm", "es", "g\u00f6tt\u00b7li\u00b7che", "!", "ein", "klei\u00b7nes", "Fe\u00b7der\u00b7mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJA", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er\u00f6fnet mir die Brust, wie sanft w\u00fcrd es mir thun?", "tokens": ["Er\u00b7\u00f6f\u00b7net", "mir", "die", "Brust", ",", "wie", "sanft", "w\u00fcrd", "es", "mir", "thun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach thus, durchbor mein Herz, gewi\u00df dann wird mir besser,", "tokens": ["Ach", "thus", ",", "durch\u00b7bor", "mein", "Herz", ",", "ge\u00b7wi\u00df", "dann", "wird", "mir", "bes\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In deinen Armen will ich dann vom Leben ruhn.", "tokens": ["In", "dei\u00b7nen", "Ar\u00b7men", "will", "ich", "dann", "vom", "Le\u00b7ben", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach welche S\u00fcssigkeit! von Lieb und Wollust trunken", "tokens": ["Ach", "wel\u00b7che", "S\u00fcs\u00b7sig\u00b7keit", "!", "von", "Lieb", "und", "Wol\u00b7lust", "trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAT", "NN", "$.", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Schl\u00e4ft dann mein mattes Haupt von seiner Unruh ein,", "tokens": ["Schl\u00e4ft", "dann", "mein", "mat\u00b7tes", "Haupt", "von", "sei\u00b7ner", "Un\u00b7ruh", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auf deinen s\u00fc\u00dfen Schoo\u00df verliebt herabgesunken,", "tokens": ["Auf", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Schoo\u00df", "ver\u00b7liebt", "her\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und k\u00fc\u00dfet sterbend noch die Ursach seiner Pein.", "tokens": ["Und", "k\u00fc\u00b7\u00dfet", "ster\u00b7bend", "noch", "die", "Ur\u00b7sach", "sei\u00b7ner", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ja thus! von deiner Hand wie kann der Tod mich schr\u00f6cken?", "tokens": ["Ja", "thus", "!", "von", "dei\u00b7ner", "Hand", "wie", "kann", "der", "Tod", "mich", "schr\u00f6\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NE", "$.", "APPR", "PPOSAT", "NN", "KOKOM", "VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es ist das gr\u00f6ste Gl\u00fcck, das ich erhalten kann.", "tokens": ["Es", "ist", "das", "gr\u00f6s\u00b7te", "Gl\u00fcck", ",", "das", "ich", "er\u00b7hal\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein Sto\u00df, so ists geschehn: wie s\u00fc\u00df wird er mir schmecken,", "tokens": ["Ein", "Sto\u00df", ",", "so", "ists", "ge\u00b7schehn", ":", "wie", "s\u00fc\u00df", "wird", "er", "mir", "schme\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "VVPP", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein kleiner Sto\u00df und dann geht erst mein Leben an.", "tokens": ["Ein", "klei\u00b7ner", "Sto\u00df", "und", "dann", "geht", "erst", "mein", "Le\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Dann will ich z\u00e4rtlich dir als Geist zur Seite schweben,", "tokens": ["Dann", "will", "ich", "z\u00e4rt\u00b7lich", "dir", "als", "Geist", "zur", "Sei\u00b7te", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "KOUS", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dann wehrt es niemand mir, du selber wehrst es nicht;", "tokens": ["Dann", "wehrt", "es", "nie\u00b7mand", "mir", ",", "du", "sel\u00b7ber", "wehrst", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PPER", "$,", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn darf ich ungescheut dem Munde K\u00fcsse geben,", "tokens": ["Denn", "darf", "ich", "un\u00b7ge\u00b7scheut", "dem", "Mun\u00b7de", "K\u00fcs\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der so verf\u00fchrisch lacht und so bezaubernd spricht.", "tokens": ["Der", "so", "ver\u00b7f\u00fch\u00b7risch", "lacht", "und", "so", "be\u00b7zau\u00b7bernd", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dann darf so lang ich will mein Auge nach dir sehnen,", "tokens": ["Dann", "darf", "so", "lang", "ich", "will", "mein", "Au\u00b7ge", "nach", "dir", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADJD", "PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn hasch ich deinen Blick und schlie\u00df ihn in mein Herz.", "tokens": ["Denn", "hasch", "ich", "dei\u00b7nen", "Blick", "und", "schlie\u00df", "ihn", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn wein ich, wenn ich will, und niemand schilt die Tr\u00e4nen,", "tokens": ["Denn", "wein", "ich", ",", "wenn", "ich", "will", ",", "und", "nie\u00b7mand", "schilt", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dann seufz ich, wenn ich will, und niemand schilt den Schmerz.", "tokens": ["Dann", "seufz", "ich", ",", "wenn", "ich", "will", ",", "und", "nie\u00b7mand", "schilt", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dann will ich dir im Traum zu deinen F\u00fcssen liegen", "tokens": ["Dann", "will", "ich", "dir", "im", "Traum", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und wachend horch ich auf, wie dirs im Busen schl\u00e4gt.", "tokens": ["Und", "wa\u00b7chend", "horch", "ich", "auf", ",", "wie", "dirs", "im", "Bu\u00b7sen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bistu vergn\u00fcgt, o Gl\u00fcck! so theil ich dein Vergn\u00fcgen,", "tokens": ["Bis\u00b7tu", "ver\u00b7gn\u00fcgt", ",", "o", "Gl\u00fcck", "!", "so", "theil", "ich", "dein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "FM", "NN", "$.", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wo nicht, so theil ich auch, was dir Verdru\u00df erregt.", "tokens": ["Wo", "nicht", ",", "so", "theil", "ich", "auch", ",", "was", "dir", "Ver\u00b7dru\u00df", "er\u00b7regt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "KOUS", "PPER", "ADV", "$,", "PWS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Dann mein unsch\u00e4tzbar Gut! dann straft mich das Gewissen", "tokens": ["Dann", "mein", "un\u00b7sch\u00e4tz\u00b7bar", "Gut", "!", "dann", "straft", "mich", "das", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJD", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "F\u00fcr meine Liebe nicht, nur dann, dann steht mirs frey;", "tokens": ["F\u00fcr", "mei\u00b7ne", "Lie\u00b7be", "nicht", ",", "nur", "dann", ",", "dann", "steht", "mirs", "frey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$,", "ADV", "ADV", "$,", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dann f\u00fchl ich keinen mehr von den verha\u00dften Bissen", "tokens": ["Dann", "f\u00fchl", "ich", "kei\u00b7nen", "mehr", "von", "den", "ver\u00b7ha\u00df\u00b7ten", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Als ob ich Frevler Schuld an deiner Unruh sey.", "tokens": ["Als", "ob", "ich", "Frev\u00b7ler", "Schuld", "an", "dei\u00b7ner", "Un\u00b7ruh", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dann bistu meiner lo\u00df, nicht wahr du bist es m\u00fcde", "tokens": ["Dann", "bis\u00b7tu", "mei\u00b7ner", "lo\u00df", ",", "nicht", "wahr", "du", "bist", "es", "m\