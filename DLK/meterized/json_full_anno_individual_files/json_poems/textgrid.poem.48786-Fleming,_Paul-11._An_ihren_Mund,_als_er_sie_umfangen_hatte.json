{"textgrid.poem.48786": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11. An ihren Mund, als er sie umfangen hatte", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzt hab' ich, was ich will und was ich werde wollen.", "tokens": ["Itzt", "hab'", "ich", ",", "was", "ich", "will", "und", "was", "ich", "wer\u00b7de", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "KON", "PWS", "PPER", "VAFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Wohnhaus meines Geists, der als zu einer T\u00fcr'", "tokens": ["Du", "Wohn\u00b7haus", "mei\u00b7nes", "Geists", ",", "der", "als", "zu", "ei\u00b7ner", "T\u00fcr'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "PRELS", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "itzt ein, itzt aus hier geht; ihr g\u00fcldnen Pforten ihr,", "tokens": ["itzt", "ein", ",", "itzt", "aus", "hier", "geht", ";", "ihr", "g\u00fcld\u00b7nen", "Pfor\u00b7ten", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "APPR", "ADV", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die auch die G\u00f6tter selbst um Sch\u00f6ne neiden sollen,", "tokens": ["die", "auch", "die", "G\u00f6t\u00b7ter", "selbst", "um", "Sch\u00f6\u00b7ne", "nei\u00b7den", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "ihr hohen Lippen ihr, die ihr so hoch geschwollen", "tokens": ["ihr", "ho\u00b7hen", "Lip\u00b7pen", "ihr", ",", "die", "ihr", "so", "hoch", "ge\u00b7schwol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von feuchter S\u00fc\u00dfe seid, itzt hab' ich eure Zier,", "tokens": ["von", "feuch\u00b7ter", "S\u00fc\u00b7\u00dfe", "seid", ",", "itzt", "hab'", "ich", "eu\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das Wesen, das man selbst dem Leben setzet f\u00fcr,", "tokens": ["das", "We\u00b7sen", ",", "das", "man", "selbst", "dem", "Le\u00b7ben", "set\u00b7zet", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "ART", "NN", "VVFIN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dem t\u00e4glich wir ein Teil von unserm Leben zollen.", "tokens": ["dem", "t\u00e4g\u00b7lich", "wir", "ein", "Teil", "von", "un\u00b7serm", "Le\u00b7ben", "zol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr Bienen, die ihr liegt an Hyblens s\u00fc\u00dfen Br\u00fcsten", "tokens": ["Ihr", "Bie\u00b7nen", ",", "die", "ihr", "liegt", "an", "Hyb\u00b7lens", "s\u00fc\u00b7\u00dfen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und saugt die edle Milch, den Honigreif mit L\u00fcsten,", "tokens": ["und", "saugt", "die", "ed\u00b7le", "Milch", ",", "den", "Ho\u00b7ni\u00b7greif", "mit", "L\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hier, hier ist mein Hymet. Komt, fliegt zu mir herein.", "tokens": ["hier", ",", "hier", "ist", "mein", "Hy\u00b7met", ".", "Komt", ",", "fliegt", "zu", "mir", "her\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$.", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}}, "stanza.4": {"line.1": {"text": "Seht, wie das hohe Tun, das trefliche, das starke,", "tokens": ["Seht", ",", "wie", "das", "ho\u00b7he", "Tun", ",", "das", "tref\u00b7li\u00b7che", ",", "das", "star\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das der Mund meinem gibt, sich regt in Seel' und Marke!", "tokens": ["das", "der", "Mund", "mei\u00b7nem", "gibt", ",", "sich", "regt", "in", "Seel'", "und", "Mar\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPOSAT", "VVFIN", "$,", "PRF", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach! da\u00df mein ganzer Leib doch Nichts als Mund solt' sein!", "tokens": ["Ach", "!", "da\u00df", "mein", "gan\u00b7zer", "Leib", "doch", "Nichts", "als", "Mund", "solt'", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PIS", "KOKOM", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Itzt hab' ich, was ich will und was ich werde wollen.", "tokens": ["Itzt", "hab'", "ich", ",", "was", "ich", "will", "und", "was", "ich", "wer\u00b7de", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "KON", "PWS", "PPER", "VAFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Wohnhaus meines Geists, der als zu einer T\u00fcr'", "tokens": ["Du", "Wohn\u00b7haus", "mei\u00b7nes", "Geists", ",", "der", "als", "zu", "ei\u00b7ner", "T\u00fcr'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "PRELS", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "itzt ein, itzt aus hier geht; ihr g\u00fcldnen Pforten ihr,", "tokens": ["i