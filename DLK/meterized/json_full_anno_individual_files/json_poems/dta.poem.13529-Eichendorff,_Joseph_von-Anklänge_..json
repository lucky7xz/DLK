{"dta.poem.13529": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Kl\u00e4nge nah'n und fliehen,", "tokens": ["Wenn", "die", "Kl\u00e4n\u00b7ge", "nah'n", "und", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Wogen s\u00fc\u00dfer Lust,", "tokens": ["In", "den", "Wo\u00b7gen", "s\u00fc\u00b7\u00dfer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! nach tiefern Melodieen", "tokens": ["Ach", "!", "nach", "tie\u00b7fern", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sehnt sich einsam oft die Brust.", "tokens": ["Sehnt", "sich", "ein\u00b7sam", "oft", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}