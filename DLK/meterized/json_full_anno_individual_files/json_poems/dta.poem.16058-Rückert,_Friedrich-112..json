{"dta.poem.16058": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heil, wenn das Gute du aus freiem Triebe thust,", "tokens": ["Heil", ",", "wenn", "das", "Gu\u00b7te", "du", "aus", "frei\u00b7em", "Trie\u00b7be", "thust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das Gesetz erf\u00fcllst, weil es ist deine Lust.", "tokens": ["Und", "das", "Ge\u00b7setz", "er\u00b7f\u00fcllst", ",", "weil", "es", "ist", "dei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann f\u00fchlest du allein nicht des Gesetzes Zwang,", "tokens": ["Dann", "f\u00fch\u00b7lest", "du", "al\u00b7lein", "nicht", "des", "Ge\u00b7set\u00b7zes", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du's verwandelt hast in deines Herzens Drang.", "tokens": ["Wenn", "du's", "ver\u00b7wan\u00b7delt", "hast", "in", "dei\u00b7nes", "Her\u00b7zens", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-