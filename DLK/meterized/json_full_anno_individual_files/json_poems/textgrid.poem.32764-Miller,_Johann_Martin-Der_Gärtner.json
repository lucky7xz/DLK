{"textgrid.poem.32764": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00e4rtner", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein G\u00e4rtner,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der sang ein traurigs Lied.", "tokens": ["Der", "sang", "ein", "trau\u00b7rigs", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er th\u00e4t in seinem Garten", "tokens": ["Er", "th\u00e4t", "in", "sei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Blumen flei\u00dfig warten,", "tokens": ["Der", "Blu\u00b7men", "flei\u00b7\u00dfig", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und all sein Flei\u00df geriet;", "tokens": ["Und", "all", "sein", "Flei\u00df", "ge\u00b7riet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und all sein Flei\u00df geriet.", "tokens": ["Und", "all", "sein", "Flei\u00df", "ge\u00b7riet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sang in tr\u00fcbem Mute", "tokens": ["Er", "sang", "in", "tr\u00fc\u00b7bem", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel liebe Tage lang.", "tokens": ["Viel", "lie\u00b7be", "Ta\u00b7ge", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Thr\u00e4nen, die ihm flossen,", "tokens": ["Von", "Thr\u00e4\u00b7nen", ",", "die", "ihm", "flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward manche Pflanz' begossen.", "tokens": ["Ward", "man\u00b7che", "Pflanz'", "be\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Also der G\u00e4rtner sang!", "tokens": ["Al\u00b7so", "der", "G\u00e4rt\u00b7ner", "sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Also der G\u00e4rtner sang!", "tokens": ["Al\u00b7so", "der", "G\u00e4rt\u00b7ner", "sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "\u00bbdas Leben ist mir traurig,", "tokens": ["\u00bb", "das", "Le\u00b7ben", "ist", "mir", "trau\u00b7rig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und giebt mir keine Freud'!", "tokens": ["Und", "giebt", "mir", "kei\u00b7ne", "Freud'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier schmacht' ich, wie die Nelken,", "tokens": ["Hier", "schmacht'", "ich", ",", "wie", "die", "Nel\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die in der Sonne welken,", "tokens": ["Die", "in", "der", "Son\u00b7ne", "wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In bangem Herzeleid,", "tokens": ["In", "ban\u00b7gem", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In bangem Herzeleid.\u00ab", "tokens": ["In", "ban\u00b7gem", "Her\u00b7ze\u00b7leid", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbei du, mein G\u00e4rtnerm\u00e4dchen,", "tokens": ["\u00bb", "ei", "du", ",", "mein", "G\u00e4rt\u00b7ner\u00b7m\u00e4d\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll ich dich nimmer sehn?", "tokens": ["Soll", "ich", "dich", "nim\u00b7mer", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du mu\u00dft in dunkeln Mauren", "tokens": ["Du", "mu\u00dft", "in", "dun\u00b7keln", "Mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den sch\u00f6nen Mai vertrauren?", "tokens": ["Den", "sch\u00f6\u00b7nen", "Mai", "ver\u00b7trau\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00dft ohne mich vergehn,", "tokens": ["Mu\u00dft", "oh\u00b7ne", "mich", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach, ohne mich vergehn?\u00ab", "tokens": ["Ach", ",", "oh\u00b7ne", "mich", "ver\u00b7gehn", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KOUI", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbes freut mich keine Blume,", "tokens": ["\u00bb", "es", "freut", "mich", "kei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil du die sch\u00f6nste bist.", "tokens": ["Weil", "du", "die", "sch\u00f6ns\u00b7te", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, d\u00fcrft' ich deiner warten,", "tokens": ["Ach", ",", "d\u00fcrft'", "ich", "dei\u00b7ner", "war\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich lie\u00dfe meinen Garten", "tokens": ["Ich", "lie\u00b7\u00dfe", "mei\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sogleich zu dieser Frist,", "tokens": ["Sog\u00b7leich", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sogleich zu dieser Frist!\u00ab", "tokens": ["Sog\u00b7leich", "zu", "die\u00b7ser", "Frist", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbseh' ich die Blumen sterben,", "tokens": ["\u00bb", "seh'", "ich", "die", "Blu\u00b7men", "ster\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "W\u00fcnsch' ich den Tod auch mir", "tokens": ["W\u00fcn\u00b7sch'", "ich", "den", "Tod", "auch", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie sterben ohne Regen,", "tokens": ["Sie", "ster\u00b7ben", "oh\u00b7ne", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sterb' ich deinetwegen.", "tokens": ["So", "sterb'", "ich", "dei\u00b7net\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["A