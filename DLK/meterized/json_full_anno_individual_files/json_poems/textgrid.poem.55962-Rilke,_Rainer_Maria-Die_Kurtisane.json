{"textgrid.poem.55962": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Kurtisane", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venedigs Sonne wird in meinem Haar", "tokens": ["Ve\u00b7ne\u00b7digs", "Son\u00b7ne", "wird", "in", "mei\u00b7nem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "ein Gold bereiten: aller Alchemie", "tokens": ["ein", "Gold", "be\u00b7rei\u00b7ten", ":", "al\u00b7ler", "Al\u00b7che\u00b7mie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erlauchten Ausgang. Meine Brauen, die", "tokens": ["er\u00b7lauch\u00b7ten", "Aus\u00b7gang", ".", "Mei\u00b7ne", "Brau\u00b7en", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "den Br\u00fccken gleichen, siehst du sie", "tokens": ["den", "Br\u00fc\u00b7cken", "glei\u00b7chen", ",", "siehst", "du", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "hinf\u00fchren ob der lautlosen Gefahr", "tokens": ["hin\u00b7f\u00fch\u00b7ren", "ob", "der", "laut\u00b7lo\u00b7sen", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "der Augen, die ein heimlicher Verkehr", "tokens": ["der", "Au\u00b7gen", ",", "die", "ein", "heim\u00b7li\u00b7cher", "Ver\u00b7kehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "an die Kan\u00e4le schlie\u00dft, so da\u00df das Meer", "tokens": ["an", "die", "Ka\u00b7n\u00e4\u00b7le", "schlie\u00dft", ",", "so", "da\u00df", "das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in ihnen steigt und f\u00e4llt und wechselt. Wer", "tokens": ["in", "ih\u00b7nen", "steigt", "und", "f\u00e4llt", "und", "wech\u00b7selt", ".", "Wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$.", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "mich einmal sah, beneidet meinen Hund,", "tokens": ["mich", "ein\u00b7mal", "sah", ",", "be\u00b7nei\u00b7det", "mei\u00b7nen", "Hund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "weil sich auf ihm oft in zerstreuter Pause", "tokens": ["weil", "sich", "auf", "ihm", "oft", "in", "zer\u00b7streu\u00b7ter", "Pau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "die Hand, die nie an keiner Glut verkohlt,", "tokens": ["die", "Hand", ",", "die", "nie", "an", "kei\u00b7ner", "Glut", "ver\u00b7kohlt", ","], "token_info": ["word"