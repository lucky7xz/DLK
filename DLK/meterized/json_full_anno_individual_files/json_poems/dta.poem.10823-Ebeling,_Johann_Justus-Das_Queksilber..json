{"dta.poem.10823": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das Queksilber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein gl\u00e4nzend Silberflus, der fl\u00fcchtig sich\nzertrennt,", "tokens": ["Ein", "gl\u00e4n\u00b7zend", "Sil\u00b7ber\u00b7flus", ",", "der", "fl\u00fcch\u00b7tig", "sich", "zer\u00b7trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "$,", "PRELS", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den man erhaschen wil, doch Augen-", "tokens": ["Den", "man", "er\u00b7ha\u00b7schen", "wil", ",", "doch", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und der Queksilber heist, giebt uns in seinen We-", "tokens": ["Und", "der", "Quek\u00b7sil\u00b7ber", "heist", ",", "giebt", "uns", "in", "sei\u00b7nen", "We"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was Reichthum, Geld und Gut, aufs deutlichste zu", "tokens": ["Was", "Reicht\u00b7hum", ",", "Geld", "und", "Gut", ",", "aufs", "deut\u00b7lichs\u00b7te", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "NN", "KON", "ADJD", "$,", "APPRART", "ADJA", "PTKZU"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Es scheint ins Auge sch\u00f6n, weil es wie Silber", "tokens": ["Es", "scheint", "ins", "Au\u00b7ge", "sch\u00f6n", ",", "weil", "es", "wie", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "$,", "KOUS", "PPER", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wird zu vielerlei gebrauchet und gen\u00fczt;", "tokens": ["Und", "wird", "zu", "vie\u00b7ler\u00b7lei", "ge\u00b7brau\u00b7chet", "und", "ge\u00b7n\u00fczt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn mans nicht wohl verwahrt; so fliegt es schnell", "tokens": ["Wenn", "mans", "nicht", "wohl", "ver\u00b7wahrt", ";", "so", "fliegt", "es", "schnell"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VVPP", "$.", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es kan dem Wasser gleich, im Tr\u00f6pfeln auch zerrin-", "tokens": ["Es", "kan", "dem", "Was\u00b7ser", "gleich", ",", "im", "Tr\u00f6p\u00b7feln", "auch", "zer\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "$,", "APPRART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn mans nicht recht gebraucht; so schadet es", "tokens": ["Wenn", "mans", "nicht", "recht", "ge\u00b7braucht", ";", "so", "scha\u00b7det", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "ADJD", "VVPP", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nach seiner Eigenschaft ist es in Klumpen schwer:", "tokens": ["Nach", "sei\u00b7ner", "Ei\u00b7gen\u00b7schaft", "ist", "es", "in", "Klum\u00b7pen", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Die G\u00fcter dieser Welt sind scheinbar und vergn\u00fc-", "tokens": ["Die", "G\u00fc\u00b7ter", "die\u00b7ser", "Welt", "sind", "schein\u00b7bar", "und", "ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch k\u00f6nnen sie auch leicht wie der Mercur verflie-", "tokens": ["Doch", "k\u00f6n\u00b7nen", "sie", "auch", "leicht", "wie", "der", "Mer\u00b7cur", "ver\u00b7flie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn man mit Klugheit nicht dieselbigen verwahrt,", "tokens": ["Wenn", "man", "mit", "Klug\u00b7heit", "nicht", "die\u00b7sel\u00b7bi\u00b7gen", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PTKNEG", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sie mit Sorgfalt nicht, zum k\u00fcnftgen Nuzzen", "tokens": ["Und", "sie", "mit", "Sorg\u00b7falt", "nicht", ",", "zum", "k\u00fcnft\u00b7gen", "Nuz\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "PTKNEG", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Man braucht dieselbigen zu vielen in der Zeit,", "tokens": ["Man", "braucht", "die\u00b7sel\u00b7bi\u00b7gen", "zu", "vie\u00b7len", "in", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDS", "APPR", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.16": {"text": "Sie sind das Element das in der Eitelkeit,", "tokens": ["Sie", "sind", "das", "E\u00b7le\u00b7ment", "das", "in", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Leben unterh\u00e4lt. Der Misbrauch wirkt Be-", "tokens": ["Das", "Le\u00b7ben", "un\u00b7ter\u00b7h\u00e4lt", ".", "Der", "Mis\u00b7brauch", "wirkt", "Be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Es kan das schn\u00f6de Geld ein Gift der Seelen wer-", "tokens": ["Es", "kan", "das", "schn\u00f6\u00b7de", "Geld", "ein", "Gift", "der", "See\u00b7len", "wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und eine schwere Last die unser Herze dr\u00fckt,", "tokens": ["Und", "ei\u00b7ne", "schwe\u00b7re", "Last", "die", "un\u00b7ser", "Her\u00b7ze", "dr\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn man dasselbe h\u00e4uft, und es dadurch zerst\u00fckt.", "tokens": ["Wenn", "man", "das\u00b7sel\u00b7be", "h\u00e4uft", ",", "und", "es", "da\u00b7durch", "zer\u00b7st\u00fckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDAT", "NN", "$,", "KON", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O! wol dem der da lernt beizeiten zu erkennen,", "tokens": ["O", "!", "wol", "dem", "der", "da", "lernt", "bei\u00b7zei\u00b7ten", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ART", "ART", "ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df aller Reichthum sey Queksilber zu benennen.", "tokens": ["Da\u00df", "al\u00b7ler", "Reicht\u00b7hum", "sey", "Quek\u00b7sil\u00b7ber", "zu", "be\u00b7nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie bald zerschmelzen sie in des Verschwenders H\u00e4n-", "tokens": ["Wie", "bald", "zer\u00b7schmel\u00b7zen", "sie", "in", "des", "Ver\u00b7schwen\u00b7ders", "H\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die sie bald hie bald da ich weis nicht wie anwenden.", "tokens": ["Die", "sie", "bald", "hie", "bald", "da", "ich", "weis", "nicht", "wie", "an\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "KOUS", "PPER", "PTKVZ", "PTKNEG", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}