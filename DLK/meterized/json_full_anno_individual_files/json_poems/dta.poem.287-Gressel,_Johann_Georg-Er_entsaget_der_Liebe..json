{"dta.poem.287": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er entsaget der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du schwacher Liebes-Gott wo sind nun deine Banden?", "tokens": ["Du", "schwa\u00b7cher", "Lie\u00b7bes\u00b7Gott", "wo", "sind", "nun", "dei\u00b7ne", "Ban\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Hertz und Sinn", "tokens": ["Mein", "Hertz", "und", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hat seine Freyheit funden.", "tokens": ["Hat", "sei\u00b7ne", "Frey\u00b7heit", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du solt annoch an mir erstreiten Schimpff und Schanden/", "tokens": ["Du", "solt", "an\u00b7noch", "an", "mir", "er\u00b7strei\u00b7ten", "Schimpff", "und", "Schan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Weil ich nun bin", "tokens": ["Weil", "ich", "nun", "bin"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Von Ketten lo\u00df gebunden/", "tokens": ["Von", "Ket\u00b7ten", "lo\u00df", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Verlach\u2019 ich den Zunder und Plunder der Liebe/", "tokens": ["Ver\u00b7lach'", "ich", "den", "Zun\u00b7der", "und", "Plun\u00b7der", "der", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Vermeide die sch\u00f6nen ", "tokens": ["Ver\u00b7mei\u00b7de", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sieh hier mein Hertze ist gantz lo\u00df von deinen Ketten/", "tokens": ["Sieh", "hier", "mein", "Hert\u00b7ze", "ist", "gantz", "lo\u00df", "von", "dei\u00b7nen", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "VVFIN", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das willig hat", "tokens": ["Das", "wil\u00b7lig", "hat"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJD", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von dir sich fangen lassen.", "tokens": ["Von", "dir", "sich", "fan\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man kan sich gleichwol noch aus deinen Banden retten/", "tokens": ["Man", "kan", "sich", "gleich\u00b7wol", "noch", "aus", "dei\u00b7nen", "Ban\u00b7den", "ret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn in der That/", "tokens": ["Wenn", "in", "der", "That", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Man will die Fesseln hassen/", "tokens": ["Man", "will", "die", "Fes\u00b7seln", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So deine verschmitzete Schw\u00e4ncke uns geben/", "tokens": ["So", "dei\u00b7ne", "ver\u00b7schmit\u00b7ze\u00b7te", "Schw\u00e4n\u00b7cke", "uns", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Verk\u00fcrtzen und w\u00fcrtzen das edelste Leben.", "tokens": ["Ver\u00b7k\u00fcrt\u00b7zen", "und", "w\u00fcrt\u00b7zen", "das", "e\u00b7dels\u00b7te", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und also lieffert dir die Freyheit jetzt ", "tokens": ["Und", "al\u00b7so", "lief\u00b7fert", "dir", "die", "Frey\u00b7heit", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit vollem Gl\u00fcck", "tokens": ["Mit", "vol\u00b7lem", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sucht sie dich zu besiegen/", "tokens": ["Sucht", "sie", "dich", "zu", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du solt wol tausendmahl mit ihr dich wieder balgen/", "tokens": ["Du", "solt", "wol", "tau\u00b7send\u00b7mahl", "mit", "ihr", "dich", "wie\u00b7der", "bal\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eh deine T\u00fcck", "tokens": ["Eh", "dei\u00b7ne", "T\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Sie werden unter kriegen.", "tokens": ["Sie", "wer\u00b7den", "un\u00b7ter", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich trotze/ und schelte nun billig die Ketten/", "tokens": ["Ich", "trot\u00b7ze", "/", "und", "schel\u00b7te", "nun", "bil\u00b7lig", "die", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "ADV", "ADJD", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wovon sich sonst trauet leicht keiner zu retten.", "tokens": ["Wo\u00b7von", "sich", "sonst", "trau\u00b7et", "leicht", "kei\u00b7ner", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "VVFIN", "ADJD", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}