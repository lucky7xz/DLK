{"dta.poem.19195": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein V\u00f6glein hatte sich in meinen Schutz begeben,", "tokens": ["Ein", "V\u00f6\u00b7glein", "hat\u00b7te", "sich", "in", "mei\u00b7nen", "Schutz", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wollt' in Sicherheit, wenngleich gefangen, leben,", "tokens": ["Es", "wollt'", "in", "Si\u00b7cher\u00b7heit", ",", "wenn\u00b7gleich", "ge\u00b7fan\u00b7gen", ",", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "KOUS", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In Sicherheit vorm Schreck des Reichs der Luft, dem Geier,", "tokens": ["In", "Si\u00b7cher\u00b7heit", "vorm", "Schreck", "des", "Reichs", "der", "Luft", ",", "dem", "Gei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor welchem sicher sich kein Vogel f\u00fchlt, kein freier.", "tokens": ["Vor", "wel\u00b7chem", "si\u00b7cher", "sich", "kein", "Vo\u00b7gel", "f\u00fchlt", ",", "kein", "frei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "PRF", "PIAT", "NN", "VVFIN", "$,", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So gern gefangen nun vor meinem Fenster hieng's,", "tokens": ["So", "gern", "ge\u00b7fan\u00b7gen", "nun", "vor", "mei\u00b7nem", "Fens\u00b7ter", "hieng's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch im Gef\u00e4ngnisse dem Schicksal nicht entgieng's.", "tokens": ["Doch", "im", "Ge\u00b7f\u00e4ng\u00b7nis\u00b7se", "dem", "Schick\u00b7sal", "nicht", "ent\u00b7gieng'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "PTKNEG", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Geier nahte k\u00fchn zum Kerker seiner Lust,", "tokens": ["Ein", "Gei\u00b7er", "nah\u00b7te", "k\u00fchn", "zum", "Ker\u00b7ker", "sei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schlug durchs Gitter ihm die Krallen in die Brust.", "tokens": ["Und", "schlug", "durchs", "Git\u00b7ter", "ihm", "die", "Kral\u00b7len", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch konnt' er seinen Raub nicht in die L\u00fcfte tragen,", "tokens": ["Doch", "konnt'", "er", "sei\u00b7nen", "Raub", "nicht", "in", "die", "L\u00fcf\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sterbend lie\u00df ers uns zur\u00fcck, es zu beklagen.", "tokens": ["Und", "ster\u00b7bend", "lie\u00df", "ers", "uns", "zu\u00b7r\u00fcck", ",", "es", "zu", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Durch seine Dienstbarkeit hat es nur dis erworben,", "tokens": ["Durch", "sei\u00b7ne", "Dienst\u00b7bar\u00b7keit", "hat", "es", "nur", "dis", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PDS", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df es nicht unbemerkt noch unbeklagt gestorben.", "tokens": ["Da\u00df", "es", "nicht", "un\u00b7be\u00b7merkt", "noch", "un\u00b7be\u00b7klagt", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}