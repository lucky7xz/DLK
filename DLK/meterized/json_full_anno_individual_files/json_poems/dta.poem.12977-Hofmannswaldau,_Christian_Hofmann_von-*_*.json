{"dta.poem.12977": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die armuth bringt zwar angst; doch hat sie auch vergn\u00fc-", "tokens": ["Die", "ar\u00b7muth", "bringt", "zwar", "angst", ";", "doch", "hat", "sie", "auch", "ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$.", "ADV", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "get.", "tokens": ["get."], "token_info": ["abbreviation"], "pos": ["XY"]}, "line.3": {"text": "Ich gebe gerne zu: da, wo sie eingekehrt,", "tokens": ["Ich", "ge\u00b7be", "ger\u00b7ne", "zu", ":", "da", ",", "wo", "sie", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verbleibt kein schmeichler mehr: die stube wird geleert:", "tokens": ["Ver\u00b7bleibt", "kein", "schmeich\u00b7ler", "mehr", ":", "die", "stu\u00b7be", "wird", "ge\u00b7leert", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lust, pracht und ansehn flieht: was sich zuvor geschmieget,", "tokens": ["Lust", ",", "pracht", "und", "an\u00b7sehn", "flieht", ":", "was", "sich", "zu\u00b7vor", "ge\u00b7schmie\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVINF", "VVFIN", "$.", "PWS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Emp\u00f6rt sich gegen den, der ferner nichts vermag:", "tokens": ["Em\u00b7p\u00f6rt", "sich", "ge\u00b7gen", "den", ",", "der", "fer\u00b7ner", "nichts", "ver\u00b7mag", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die perlen kehren sich alsdenn in heisse thr\u00e4nen;", "tokens": ["Die", "per\u00b7len", "keh\u00b7ren", "sich", "als\u00b7denn", "in", "heis\u00b7se", "thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch macht die armuth gleich noch so viel noth und sehnen,", "tokens": ["Doch", "macht", "die", "ar\u00b7muth", "gleich", "noch", "so", "viel", "noth", "und", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "PIAT", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So legt sie doch allein die wahren freund an tag.", "tokens": ["So", "legt", "sie", "doch", "al\u00b7lein", "die", "wah\u00b7ren", "freund", "an", "tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}