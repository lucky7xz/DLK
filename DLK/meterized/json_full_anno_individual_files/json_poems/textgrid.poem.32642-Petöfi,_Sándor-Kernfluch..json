{"textgrid.poem.32642": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Kernfluch.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da fahre des Blitzes Flamme drein,", "tokens": ["Da", "fah\u00b7re", "des", "Blit\u00b7zes", "Flam\u00b7me", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir brennt der Zorn durch Mark und Bein,", "tokens": ["Mir", "brennt", "der", "Zorn", "durch", "Mark", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es tobt in mir so wild und j\u00e4h,", "tokens": ["Es", "tobt", "in", "mir", "so", "wild", "und", "j\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4re ich der Plattensee!", "tokens": ["Als", "w\u00e4\u00b7re", "ich", "der", "Plat\u00b7ten\u00b7see", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Leben ist Verdru\u00df und Schmerz,", "tokens": ["Mein", "Le\u00b7ben", "ist", "Ver\u00b7dru\u00df", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"te