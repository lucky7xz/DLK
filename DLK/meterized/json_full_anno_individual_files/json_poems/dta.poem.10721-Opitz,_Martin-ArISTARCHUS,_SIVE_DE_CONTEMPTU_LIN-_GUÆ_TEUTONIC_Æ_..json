{"dta.poem.10721": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "ArISTARCHUS,  \n SIVE DE CONTEMPTU LIN-  \n GU\u00c6 TEUTONIC \u00c6 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Der Erenspegel ist du scham/", "tokens": ["Der", "E\u00b7rens\u00b7pe\u00b7gel", "ist", "du", "scham", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Swer sich darinne ersiht", "tokens": ["Swer", "sich", "da\u00b7rin\u00b7ne", "er\u00b7siht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PRF", "PAV", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Der wirt vnzehmen bliken gram:", "tokens": ["Der", "wirt", "vn\u00b7zeh\u00b7men", "bli\u00b7ken", "gram", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du schame ist argen worten vigent/ vntrewem ha\u00df/ vnsteten fluch:", "tokens": ["Du", "scha\u00b7me", "ist", "ar\u00b7gen", "wor\u00b7ten", "vi\u00b7gent", "/", "vntre\u00b7wem", "ha\u00df", "/", "vns\u00b7te\u00b7ten", "fluch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VAFIN", "ADJA", "NN", "NE", "$(", "ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Scham ist ein tugent du mannes nahmen gegen frawen prjses giht;", "tokens": ["Scham", "ist", "ein", "tu\u00b7gent", "du", "man\u00b7nes", "nah\u00b7men", "ge\u00b7gen", "fra\u00b7wen", "prj\u00b7ses", "giht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPER", "PIS", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Du reinen Wib funt man alsam jhr beider lieb mit schame geschicht;", "tokens": ["Du", "rei\u00b7nen", "Wib", "funt", "man", "al\u00b7sam", "jhr", "bei\u00b7der", "lieb", "mit", "scha\u00b7me", "ge\u00b7schicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "PPER", "ADV", "ADJD", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+--+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Schame get edelen gesteine vor/ vnd turet bas danne siden tuh;", "tokens": ["Scha\u00b7me", "get", "e\u00b7de\u00b7len", "ge\u00b7stei\u00b7ne", "vor", "/", "vnd", "tu\u00b7ret", "bas", "dan\u00b7ne", "si\u00b7den", "tuh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADJA", "NN", "PTKVZ", "$(", "KON", "VVFIN", "NE", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.invert"}, "line.8": {"text": "Schame ist mit bescheidenheit der werden Minne bi/", "tokens": ["Scha\u00b7me", "ist", "mit", "be\u00b7schei\u00b7den\u00b7heit", "der", "wer\u00b7den", "Min\u00b7ne", "bi", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN", "NE", "$("], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.9": {"text": "Du scham in eren garten ist ein bluen des zwi;", "tokens": ["Du", "scham", "in", "e\u00b7ren", "gar\u00b7ten", "ist", "ein", "blu\u00b7en", "des", "zwi", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVINF", "VVFIN", "VAFIN", "ART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "Du scham ist eren schilt;", "tokens": ["Du", "scham", "ist", "e\u00b7ren", "schilt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Du scham alsam ein reines Kint in schoner frawen schozen spilt;", "tokens": ["Du", "scham", "al\u00b7sam", "ein", "rei\u00b7nes", "Kint", "in", "scho\u00b7ner", "fra\u00b7wen", "scho\u00b7zen", "spilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Schame zieret reine wib vnd wirdet edelen man/", "tokens": ["Scha\u00b7me", "zie\u00b7ret", "rei\u00b7ne", "wib", "vnd", "wir\u00b7det", "e\u00b7de\u00b7len", "man", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "KON", "VVFIN", "ADJA", "PIS", "$("], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.13": {"text": "Schame kan leiden vf den ban", "tokens": ["Scha\u00b7me", "kan", "lei\u00b7den", "vf", "den", "ban"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF", "APPR", "ART", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Da nie schand entrit kam an/", "tokens": ["Da", "nie", "schand", "ent\u00b7rit", "kam", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "VVFIN", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Swer schame minnet den bkret in schanden dienest selten han/", "tokens": ["Swer", "scha\u00b7me", "min\u00b7net", "den", "bkret", "in", "schan\u00b7den", "die\u00b7nest", "sel\u00b7ten", "han", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "VVFIN", "ART", "VVFIN", "APPR", "ADJA", "VVFIN", "ADJD", "VAFIN", "$("], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Scham ist ein du hasten tugent/ sagent vns die meister vnd du buch.", "tokens": ["Scham", "ist", "ein", "du", "has\u00b7ten", "tu\u00b7gent", "/", "sa\u00b7gent", "vns", "die", "meis\u00b7ter", "vnd", "du", "buch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "PPER", "ADJA", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "KON", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}