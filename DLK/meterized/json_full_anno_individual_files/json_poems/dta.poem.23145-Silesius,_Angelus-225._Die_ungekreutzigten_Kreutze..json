{"dta.poem.23145": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "225. Die ungekreutzigten Kreutze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Viel sind der Welt ein Kreutz/ die Welt ist aber jhne\u0303", "tokens": ["Viel", "sind", "der", "Welt", "ein", "Kreutz", "/", "die", "Welt", "ist", "a\u00b7ber", "jhn\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht dieses widerumb: weil sie sie noch bedienen.", "tokens": ["Nicht", "die\u00b7ses", "wi\u00b7de\u00b7rumb", ":", "weil", "sie", "sie", "noch", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "ADV", "$.", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}