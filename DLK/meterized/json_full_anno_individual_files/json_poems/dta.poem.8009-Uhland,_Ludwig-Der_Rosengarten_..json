{"dta.poem.8009": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Rosengarten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Vom sch\u00f6nen Rosengarten", "tokens": ["Vom", "sch\u00f6\u00b7nen", "Ro\u00b7sen\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich mit Sang euch melden.", "tokens": ["Will", "ich", "mit", "Sang", "euch", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Morgen lustwandelten Fraun,", "tokens": ["Am", "Mor\u00b7gen", "lust\u00b7wan\u00b7del\u00b7ten", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Am Abend fochten die Helden.", "tokens": ["Am", "A\u00b7bend", "foch\u00b7ten", "die", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201emein Herr ist K\u00f6nig im Land,", "tokens": ["\u201e", "mein", "Herr", "ist", "K\u00f6\u00b7nig", "im", "Land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ich herrsch\u2019 im Garten der Rosen,", "tokens": ["Ich", "herr\u00b7sch'", "im", "Gar\u00b7ten", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er hat sich die g\u00fcldene Kron\u2019,", "tokens": ["Er", "hat", "sich", "die", "g\u00fcl\u00b7de\u00b7ne", "Kron'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ich den Blumenkranz mir erkosen.", "tokens": ["Ich", "den", "Blu\u00b7men\u00b7kranz", "mir", "er\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So h\u00f6rt, ihr junge Recken,", "tokens": ["So", "h\u00f6rt", ",", "ihr", "jun\u00b7ge", "Re\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr lieben drei W\u00e4chter mein!", "tokens": ["Ihr", "lie\u00b7ben", "drei", "W\u00e4ch\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "CARD", "NN", "PPOSAT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00dft alle zarten Jungfr\u00e4ulein,", "tokens": ["La\u00dft", "al\u00b7le", "zar\u00b7ten", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft keinen Ritter herein!", "tokens": ["La\u00dft", "kei\u00b7nen", "Rit\u00b7ter", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Sie m\u00f6chten die Rosen verderben,", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "die", "Ro\u00b7sen", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das br\u00e4chte mir gro\u00dfe Sorgen.\u201c", "tokens": ["Das", "br\u00e4ch\u00b7te", "mir", "gro\u00b7\u00dfe", "Sor\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So sprach die sch\u00f6ne K\u00f6nigin", "tokens": ["So", "sprach", "die", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als sie dannen ging am Morgen.", "tokens": ["Als", "sie", "dan\u00b7nen", "ging", "am", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da wandelten die drei W\u00e4chter", "tokens": ["Da", "wan\u00b7del\u00b7ten", "die", "drei", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar treulich vor der Th\u00fcr.", "tokens": ["Gar", "treu\u00b7lich", "vor", "der", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die R\u00f6slein dufteten stille", "tokens": ["Die", "R\u00f6s\u00b7lein", "duf\u00b7te\u00b7ten", "stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und blickten lieblich herf\u00fcr.", "tokens": ["Und", "blick\u00b7ten", "lieb\u00b7lich", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}