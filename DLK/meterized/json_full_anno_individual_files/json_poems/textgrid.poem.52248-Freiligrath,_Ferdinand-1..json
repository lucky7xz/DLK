{"textgrid.poem.52248": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Nassaus Burg der edle Herr vom Steine", "tokens": ["Von", "Nas\u00b7saus", "Burg", "der", "ed\u00b7le", "Herr", "vom", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und noch ein Wackrer, derb und turnerfahren,", "tokens": ["Und", "noch", "ein", "Wack\u00b7rer", ",", "derb", "und", "tur\u00b7ner\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein B\u00fcrgerkind mit langen Buschenhaaren \u2013", "tokens": ["Ein", "B\u00fcr\u00b7ger\u00b7kind", "mit", "lan\u00b7gen", "Bu\u00b7schen\u00b7haa\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die fuhren einst zusammen auf dem Rheine.", "tokens": ["Die", "fuh\u00b7ren", "einst", "zu\u00b7sam\u00b7men", "auf", "dem", "Rhei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie war er gr\u00fcn von Walnu\u00dflaub und Weine!", "tokens": ["Wie", "war", "er", "gr\u00fcn", "von", "Wal\u00b7nu\u00df\u00b7laub", "und", "Wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "wo