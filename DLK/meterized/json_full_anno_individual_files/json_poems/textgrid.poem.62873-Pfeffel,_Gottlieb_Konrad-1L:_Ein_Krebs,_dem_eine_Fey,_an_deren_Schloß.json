{"textgrid.poem.62873": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Krebs, dem eine Fey, an deren Schlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Krebs, dem eine Fey, an deren Schlo\u00df", "tokens": ["Ein", "Krebs", ",", "dem", "ei\u00b7ne", "Fey", ",", "an", "de\u00b7ren", "Schlo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein klarer Bach vor\u00fcberflo\u00df,", "tokens": ["Ein", "kla\u00b7rer", "Bach", "vor\u00b7\u00fc\u00b7ber\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tischgesellschaft zu erg\u00f6tzen,", "tokens": ["Die", "Tischge\u00b7sell\u00b7schaft", "zu", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In seinem rothen Wamms das Leben wiedergab,", "tokens": ["In", "sei\u00b7nem", "ro\u00b7then", "Wamms", "das", "Le\u00b7ben", "wie\u00b7der\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Flog aus der Elfe Hand ins nasse Reich hinab.", "tokens": ["Flog", "aus", "der", "El\u00b7fe", "Hand", "ins", "nas\u00b7se", "Reich", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word",