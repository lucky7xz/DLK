{"dta.poem.6393": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Bewegung hart und strenge,", "tokens": ["Wenn", "die", "Be\u00b7we\u00b7gung", "hart", "und", "stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df der kleinen C\u00f6rper Menge,", "tokens": ["Und", "da\u00df", "der", "klei\u00b7nen", "C\u00f6r\u00b7per", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch die Nerven starck gedr\u00fccket seyn,", "tokens": ["Wo\u00b7durch", "die", "Ner\u00b7ven", "starck", "ge\u00b7dr\u00fc\u00b7cket", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sehr spitzig, sich zu tieff in sie versencken;", "tokens": ["Sehr", "spit\u00b7zig", ",", "sich", "zu", "tieff", "in", "sie", "ver\u00b7sen\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRF", "PTKA", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mi\u00dff\u00e4llet uns der Druck, nimmt nus mit Eckel ein.", "tokens": ["Mi\u00df\u00b7f\u00e4l\u00b7let", "uns", "der", "Druck", ",", "nimmt", "nus", "mit", "E\u00b7ckel", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}