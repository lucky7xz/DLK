{"dta.poem.19566": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4mmergang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Du lebst meer\u00fcber", "tokens": ["Du", "lebst", "meer\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In blauer Ferne", "tokens": ["In", "blau\u00b7er", "Fer\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und du besuchst mich", "tokens": ["Und", "du", "be\u00b7suchst", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER"], "meter": "-+---", "measure": "dactylic.init"}, "line.4": {"text": "Beim ersten Sterne.", "tokens": ["Beim", "ers\u00b7ten", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich mach' im Felde", "tokens": ["Ich", "mach'", "im", "Fel\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die D\u00e4mmerrunde,", "tokens": ["Die", "D\u00e4m\u00b7mer\u00b7run\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Umkreist, umbollen", "tokens": ["Um\u00b7kreist", ",", "um\u00b7bol\u00b7len"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Von meinem Hunde.", "tokens": ["Von", "mei\u00b7nem", "Hun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Es rauscht im Dickicht,", "tokens": ["Es", "rauscht", "im", "Di\u00b7ckicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Es webt im D\u00fcster,", "tokens": ["Es", "webt", "im", "D\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auf meine Wange", "tokens": ["Auf", "mei\u00b7ne", "Wan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Haucht warm Gefl\u00fcster.", "tokens": ["Haucht", "warm", "Ge\u00b7fl\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Das Weggeleite", "tokens": ["Das", "Weg\u00b7ge\u00b7lei\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wird trauter, trauter,", "tokens": ["Wird", "trau\u00b7ter", ",", "trau\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du schmiegst dich n\u00e4her,", "tokens": ["Du", "schmiegst", "dich", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du plauderst lauter.", "tokens": ["Du", "plau\u00b7derst", "lau\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Da giebt's zu schelten,", "tokens": ["Da", "giebt's", "zu", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da giebt's zu fragen,", "tokens": ["Da", "giebt's", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und hell zu lachen", "tokens": ["Und", "hell", "zu", "la\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und leis zu klagen.", "tokens": ["Und", "leis", "zu", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Was wedelt Barry", "tokens": ["Was", "we\u00b7delt", "Bar\u00b7ry"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So gl\u00fcckverloren?", "tokens": ["So", "gl\u00fcck\u00b7ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du kraust dem Liebling", "tokens": ["Du", "kraust", "dem", "Lieb\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die weichen Ohren ...", "tokens": ["Die", "wei\u00b7chen", "Oh\u00b7ren", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}