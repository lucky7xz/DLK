{"textgrid.poem.43242": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Er denkt:", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich werde sie nicht los die alte Weise,", "tokens": ["Ich", "wer\u00b7de", "sie", "nicht", "los", "die", "al\u00b7te", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich mu\u00df sie summen schon seit langen Stunden,", "tokens": ["Ich", "mu\u00df", "sie", "sum\u00b7men", "schon", "seit", "lan\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als h\u00e4tt' ich ein verge\u00df'nes Lieb gefunden,", "tokens": ["Als", "h\u00e4tt'", "ich", "ein", "ver\u00b7ge\u00df'\u00b7nes", "Lieb", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So schmeichelt, bittet, lockt es immer leise.", "tokens": ["So", "schmei\u00b7chelt", ",", "bit\u00b7tet", ",", "lockt", "es", "im\u00b7mer", "lei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Welch reiches Fest das war! wie sch\u00f6n die Frauen!", "tokens": ["Welch", "rei\u00b7ches", "Fest", "das", "war", "!", "wie", "sch\u00f6n", "die", "Frau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PDS", "VAFIN", "$.", "PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch nur Puppen gleichend, seelenlosen,", "tokens": ["Und", "doch", "nur", "Pup\u00b7pen", "glei\u00b7chend", ",", "see\u00b7len\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die man geschm\u00fcckt mit duftig-frischen Rosen ...", "tokens": ["Die", "man", "ge\u00b7schm\u00fcckt", "mit", "duf\u00b7tig\u00b7fri\u00b7schen", "Ro\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zuweilen packte mich ein fr\u00f6stelnd Grauen.", "tokens": ["Zu\u00b7wei\u00b7len", "pack\u00b7te", "mich", "ein", "fr\u00f6s\u00b7telnd", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stets diese Nacken, diese k\u00fcnstlich-wei\u00dfen,", "tokens": ["Stets", "die\u00b7se", "Na\u00b7cken", ",", "die\u00b7se", "k\u00fcnst\u00b7lich\u00b7wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stets dieselben gutgeschulten Augen!", "tokens": ["Und", "stets", "die\u00b7sel\u00b7ben", "gut\u00b7ge\u00b7schul\u00b7ten", "Au\u0