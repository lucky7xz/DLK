{"dta.poem.21390": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich habe Fisch und G\u00e4nsefleisch", "tokens": ["\u201e", "ich", "ha\u00b7be", "Fisch", "und", "G\u00e4n\u00b7se\u00b7fleisch"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00f6ne Apfelsinen.\u201c", "tokens": ["Und", "sch\u00f6\u00b7ne", "Ap\u00b7fel\u00b7si\u00b7nen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gieb mir Fisch und G\u00e4nsefleisch", "tokens": ["So", "gieb", "mir", "Fisch", "und", "G\u00e4n\u00b7se\u00b7fleisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00f6ne Apfelsinen.", "tokens": ["Und", "sch\u00f6\u00b7ne", "Ap\u00b7fel\u00b7si\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als ich a\u00df mit gro\u00dfem Ap\u2019tit,", "tokens": ["Und", "als", "ich", "a\u00df", "mit", "gro\u00b7\u00dfem", "Ap'\u00b7tit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Mutter ward gl\u00fccklich und munter,", "tokens": ["Die", "Mut\u00b7ter", "ward", "gl\u00fcck\u00b7lich", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie frug wohl dies, sie frug wohl das,", "tokens": ["Sie", "frug", "wohl", "dies", ",", "sie", "frug", "wohl", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "$,", "PPER", "VVFIN", "ADV", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verf\u00e4ngliche Fragen mitunter.", "tokens": ["Ver\u00b7f\u00e4ng\u00b7li\u00b7che", "Fra\u00b7gen", "mi\u00b7tun\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u201emein liebes Kind! und wirst du auch", "tokens": ["\u201e", "mein", "lie\u00b7bes", "Kind", "!", "und", "wirst", "du", "auch"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$.", "KON", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Recht sorgsam gepflegt in der Fremde?", "tokens": ["Recht", "sorg\u00b7sam", "ge\u00b7pflegt", "in", "der", "Frem\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Versteht deine Frau die Haushaltung,", "tokens": ["Ver\u00b7steht", "dei\u00b7ne", "Frau", "die", "Haus\u00b7hal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und flickt sie dir Str\u00fcmpfe und Hemde?\u201c", "tokens": ["Und", "flickt", "sie", "dir", "Str\u00fcmp\u00b7fe", "und", "Hem\u00b7de", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der Fisch ist gut, lieb M\u00fctterlein,", "tokens": ["Der", "Fisch", "ist", "gut", ",", "lieb", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch mu\u00df man ihn schweigend verzehren;", "tokens": ["Doch", "mu\u00df", "man", "ihn", "schwei\u00b7gend", "ver\u00b7zeh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Man kriegt so leicht eine Gr\u00e4t\u2019 in den Hals,", "tokens": ["Man", "kriegt", "so", "leicht", "ei\u00b7ne", "Gr\u00e4t'", "in", "den", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du darfst mich jetzt nicht st\u00f6ren.", "tokens": ["Du", "darfst", "mich", "jetzt", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}