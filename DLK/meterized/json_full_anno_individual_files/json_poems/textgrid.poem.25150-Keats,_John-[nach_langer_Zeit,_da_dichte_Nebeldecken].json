{"textgrid.poem.25150": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[nach langer Zeit, da dichte Nebeldecken]", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach langer Zeit, da dichte Nebeldecken", "tokens": ["Nach", "lan\u00b7ger", "Zeit", ",", "da", "dich\u00b7te", "Ne\u00b7bel\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Land bedr\u00fcckten, wacht mit sanfter Schw\u00fcle", "tokens": ["Das", "Land", "be\u00b7dr\u00fcck\u00b7ten", ",", "wacht", "mit", "sanf\u00b7ter", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Tag auf von des S\u00fcdens sonnigem Pf\u00fchle", "tokens": ["Ein", "Tag", "auf", "von", "des", "S\u00fc\u00b7dens", "son\u00b7ni\u00b7gem", "Pf\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und fegt vom kranken Himmel alle Flecken.", "tokens": ["Und", "fegt", "vom", "kran\u00b7ken", "Him\u00b7mel", "al\u00b7le", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Fr\u00f6hlich erl\u00f6st aus tr\u00fcbem Winterschrecken", "tokens": ["Fr\u00f6h\u00b7lich", "er\u00b7l\u00f6st", "aus", "tr\u00fc\u00b7bem"