{"dta.poem.10277": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Was bl\u00fcht ihr wieder, heitere Syringen,", "tokens": ["Was", "bl\u00fcht", "ihr", "wie\u00b7der", ",", "hei\u00b7te\u00b7re", "Sy\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wollt ihr den Gru\u00df mir eines Toten bringen?", "tokens": ["Wollt", "ihr", "den", "Gru\u00df", "mir", "ei\u00b7nes", "To\u00b7ten", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er war mein Freund, er war\u2019s in Lust und Leiden,", "tokens": ["Er", "war", "mein", "Freund", ",", "er", "wa\u00b7r's", "in", "Lust", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Um dessen Stirn die Fr\u00fchlingslocken hingen.", "tokens": ["Um", "des\u00b7sen", "Stirn", "die", "Fr\u00fch\u00b7lings\u00b7lo\u00b7cken", "hin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRELAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns schwanden manche Stunden, jugendtolle,", "tokens": ["Uns", "schwan\u00b7den", "man\u00b7che", "Stun\u00b7den", ",", "ju\u00b7gend\u00b7tol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Morgenrot noch gr\u00fc\u00dfte Becherklingen.", "tokens": ["Das", "Mor\u00b7gen\u00b7rot", "noch", "gr\u00fc\u00df\u00b7te", "Be\u00b7cher\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das nahm ein Ende, als die Schlachtenadler", "tokens": ["Das", "nahm", "ein", "En\u00b7de", ",", "als", "die", "Schlach\u00b7ten\u00b7ad\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Fl\u00fcgel breiteten auf Sturmesschwingen,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "brei\u00b7te\u00b7ten", "auf", "Stur\u00b7mes\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und der Granaten unheilvolle Wolken", "tokens": ["Und", "der", "Gra\u00b7na\u00b7ten", "un\u00b7heil\u00b7vol\u00b7le", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In L\u00fcften spielten gleich den Schmetterlingen,", "tokens": ["In", "L\u00fcf\u00b7ten", "spiel\u00b7ten", "gleich", "den", "Schmet\u00b7ter\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als unsre Fahnen, rot in Abendgluten,", "tokens": ["Als", "uns\u00b7re", "Fah\u00b7nen", ",", "rot", "in", "A\u00b7bend\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Siegk\u00fcndend flatterten nach hei\u00dfem Ringen.", "tokens": ["Sieg\u00b7k\u00fcn\u00b7dend", "flat\u00b7ter\u00b7ten", "nach", "hei\u00b7\u00dfem", "Rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Auf allen H\u00f6hen, in den Thalen schliefen,", "tokens": ["Auf", "al\u00b7len", "H\u00f6\u00b7hen", ",", "in", "den", "Tha\u00b7len", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die gar zu br\u00fcderlich den Tod umfingen,", "tokens": ["Die", "gar", "zu", "br\u00fc\u00b7der\u00b7lich", "den", "Tod", "um\u00b7fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und unter ihnen fand in einem Garten,", "tokens": ["Und", "un\u00b7ter", "ih\u00b7nen", "fand", "in", "ei\u00b7nem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Von fern her\u00fcber t\u00f6nte Siegessingen,", "tokens": ["Von", "fern", "her\u00b7\u00fc\u00b7ber", "t\u00f6n\u00b7te", "Sie\u00b7ges\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Den Freund ich, abendk\u00fchl, wie traumbezwungen,", "tokens": ["Den", "Freund", "ich", ",", "a\u00b7bend\u00b7k\u00fchl", ",", "wie", "traum\u00b7be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ADV", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Beschattet still von bl\u00fchenden Syringen.", "tokens": ["Be\u00b7schat\u00b7tet", "still", "von", "bl\u00fc\u00b7hen\u00b7den", "Sy\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}