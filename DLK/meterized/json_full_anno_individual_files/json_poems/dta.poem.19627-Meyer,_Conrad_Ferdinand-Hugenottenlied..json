{"dta.poem.19627": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Hugenottenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In die Schule bin ich gangen", "tokens": ["In", "die", "Schu\u00b7le", "bin", "ich", "gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem Meister Hans Calvin,", "tokens": ["Bei", "dem", "Meis\u00b7ter", "Hans", "Cal\u00b7vin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehre hab ich dort empfangen:", "tokens": ["Leh\u00b7re", "hab", "ich", "dort", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vorbestimmt ist Alles ewighin!", "tokens": ["Vor\u00b7be\u00b7stimmt", "ist", "Al\u00b7les", "e\u00b7wig\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jeder volle Wurf im W\u00fcrfelspiele,", "tokens": ["Je\u00b7der", "vol\u00b7le", "Wurf", "im", "W\u00fcr\u00b7fel\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jeder Diebestritt auf Liebchens Diele,", "tokens": ["Je\u00b7der", "Die\u00b7be\u00b7stritt", "auf", "Lieb\u00b7chens", "Die\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Jeder Ku\u00df \u2014", "tokens": ["Je\u00b7der", "Ku\u00df"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Schicksalsschlu\u00df!", "tokens": ["Schick\u00b7sals\u00b7schlu\u00df", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Dann bin ich zu Ro\u00df gestiegen", "tokens": ["Dann", "bin", "ich", "zu", "Ro\u00df", "ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit dem Hauptmann Des Adrets,", "tokens": ["Mit", "dem", "Haupt\u00b7mann", "Des", "Ad\u00b7rets", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der das Kindlein in der Wiegen", "tokens": ["Der", "das", "Kin\u00b7dlein", "in", "der", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrgt und sich erg\u00f6tzt an Qual und Weh!", "tokens": ["W\u00fcrgt", "und", "sich", "er\u00b7g\u00f6tzt", "an", "Qual", "und", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PRF", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jeder First, der raucht und dampft und lodert,", "tokens": ["Je\u00b7der", "First", ",", "der", "raucht", "und", "dampft", "und", "lo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jeder Todte, der im Graben modert,", "tokens": ["Je\u00b7der", "Tod\u00b7te", ",", "der", "im", "Gra\u00b7ben", "mo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Jeder Schu\u00df \u2014", "tokens": ["Je\u00b7der", "Schu\u00df"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Schicksalsschlu\u00df!", "tokens": ["Schick\u00b7sals\u00b7schlu\u00df", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}