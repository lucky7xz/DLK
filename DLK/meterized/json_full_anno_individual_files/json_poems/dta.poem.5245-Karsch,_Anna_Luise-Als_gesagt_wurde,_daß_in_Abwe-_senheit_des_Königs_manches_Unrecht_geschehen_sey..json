{"dta.poem.5245": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als gesagt wurde, da\u00df in Abwe-  \n senheit des K\u00f6nigs manches Unrecht  \n geschehen sey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "O! wenn du wiederkommen wirst;", "tokens": ["O", "!", "wenn", "du", "wie\u00b7der\u00b7kom\u00b7men", "wirst", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann, Friedrich! nimmt das Recht die Wage hurtig", "tokens": ["Dann", ",", "Fried\u00b7rich", "!", "nimmt", "das", "Recht", "die", "Wa\u00b7ge", "hur\u00b7tig"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NE", "$.", "VVFIN", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wieder.", "tokens": ["wie\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Kein b\u00f6ser Richter beugt die gute Sache nieder;", "tokens": ["Kein", "b\u00f6\u00b7ser", "Rich\u00b7ter", "beugt", "die", "gu\u00b7te", "Sa\u00b7che", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und kein Hebr\u00e4er wird ein F\u00fcrst.", "tokens": ["Und", "kein", "Heb\u00b7r\u00e4\u00b7er", "wird", "ein", "F\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}