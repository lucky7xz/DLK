{"textgrid.poem.60593": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Laura", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, Laura, seh ich? Dich, Petrarchs Geliebte?", "tokens": ["Dich", ",", "Lau\u00b7ra", ",", "seh", "ich", "?", "Dich", ",", "Pe\u00b7trarchs", "Ge\u00b7lieb\u00b7te", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VVFIN", "PPER", "$.", "PPER", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die\u00df ist das himmlische Gesicht,", "tokens": ["Die\u00df", "ist", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch dem ein Himmel, den sie oft betr\u00fcbte,", "tokens": ["Auch", "dem", "ein", "Him\u00b7mel", ",", "den", "sie", "oft", "be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Grausamkeiten strenger Pflicht?", "tokens": ["Durch", "Grau\u00b7sam\u00b7kei\u00b7ten", "stren\u00b7ger", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du blaues Auge, schmachtend vom Verlangen,", "tokens": ["Du", "blau\u00b7es", "Au\u00b7ge", ",", "schmach\u00b7tend", "vom", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Laurens Mund verschweigen mu\u00df!", "tokens": ["Das", "Lau\u00b7rens", "Mund", "ver\u00b7schwei\u00b7gen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O welche Rosen bl\u00fchen aus den Wangen!", "tokens": ["O", "wel\u00b7che", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "aus", "den", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die vollen Lippen! welch ein Ku\u00df!", "tokens": ["Die", "vol\u00b7len", "Lip\u00b7pen", "!", "welch", "ein", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O s\u00e4ht ihr Lauren unter Bluhmen wallen,", "tokens": ["O", "s\u00e4ht", "ihr", "Lau\u00b7ren", "un\u00b7ter", "Bluh\u00b7men", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Gang w\u00e4r einer G\u00f6ttinn Gang!", "tokens": ["Ihr", "Gang", "w\u00e4r", "ei\u00b7ner", "G\u00f6t\u00b7tinn", "Gang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre Stimme m\u00fc\u00dfte s\u00fcsser schallen,", "tokens": ["Und", "ih\u00b7re", "Stim\u00b7me", "m\u00fc\u00df\u00b7te", "s\u00fcs\u00b7ser", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als einer Nachtigall Gesang!", "tokens": ["Als", "ei\u00b7ner", "Nach\u00b7ti\u00b7gall", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Herz kennt Lauren und klopft ihr entgegen!", "tokens": ["Mein", "Herz", "kennt", "Lau\u00b7ren", "und", "klopft", "ihr", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun f\u00fchl ich, da\u00df es m\u00f6glich sey,", "tokens": ["Nun", "f\u00fchl", "ich", ",", "da\u00df", "es", "m\u00f6g\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll s\u00fcsser Schwermuth einer Laura wegen,", "tokens": ["Voll", "s\u00fcs\u00b7ser", "Schwer\u00b7muth", "ei\u00b7ner", "Lau\u00b7ra", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur ihr zu leben, ewig treu;", "tokens": ["Nur", "ihr", "zu", "le\u00b7ben", ",", "e\u00b7wig", "treu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Fern von der Sch\u00f6nen, wachend sie zu tr\u00e4umen,", "tokens": ["Fern", "von", "der", "Sch\u00f6\u00b7nen", ",", "wa\u00b7chend", "sie", "zu", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Abwesend auch ihr nachzugehn,", "tokens": ["Ab\u00b7we\u00b7send", "auch", "ihr", "nach\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An allen B\u00e4chen, unter allen B\u00e4umen", "tokens": ["An", "al\u00b7len", "B\u00e4\u00b7chen", ",", "un\u00b7ter", "al\u00b7len", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur sie zu h\u00f6ren, sie zu sehn.", "tokens": ["Nur", "sie", "zu", "h\u00f6\u00b7ren", ",", "sie", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! alle Sch\u00f6nheit dieser schlanken Glieder", "tokens": ["Ach", "!", "al\u00b7le", "Sch\u00f6n\u00b7heit", "die\u00b7ser", "schlan\u00b7ken", "Glie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PIAT", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschlo\u00df ein unbarmherzig Grab!", "tokens": ["Ver\u00b7schlo\u00df", "ein", "un\u00b7barm\u00b7her\u00b7zig", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Laura lebet durch des Dichters Lieder,", "tokens": ["Doch", "Lau\u00b7ra", "le\u00b7bet", "durch", "des", "Dich\u00b7ters", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ihr der Gott der Liebe gab.", "tokens": ["Den", "ihr", "der", "Gott", "der", "Lie\u00b7be", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In seinen Liedern rieselt noch die Quelle,", "tokens": ["In", "sei\u00b7nen", "Lie\u00b7dern", "rie\u00b7selt", "noch", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit klarer, k\u00fchler, sanfter Fluth,", "tokens": ["Mit", "kla\u00b7rer", ",", "k\u00fch\u00b7ler", ",", "sanf\u00b7ter", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lacht noch mit Bluhmen jene werthe Stelle,", "tokens": ["Lacht", "noch", "mit", "Bluh\u00b7men", "je\u00b7ne", "wert\u00b7he", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo sie an gr\u00fcnen Baum geruht.", "tokens": ["Wo", "sie", "an", "gr\u00fc\u00b7nen", "Baum", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Ort ist heilig! Junge Weste schweigen,", "tokens": ["Der", "Ort", "ist", "hei\u00b7lig", "!", "Jun\u00b7ge", "Wes\u00b7te", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fchlen sie mit stiller Lust:", "tokens": ["Und", "k\u00fch\u00b7len", "sie", "mit", "stil\u00b7ler", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wei\u00dfen Bl\u00fcthen taumeln von den Zweigen", "tokens": ["Die", "wei\u00b7\u00dfen", "Bl\u00fc\u00b7then", "tau\u00b7meln", "von", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verliebt herab auf Laurens Brust.", "tokens": ["Ver\u00b7liebt", "her\u00b7ab", "auf", "Lau\u00b7rens", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "In s\u00fcsser Irre sanft herumgetragen,", "tokens": ["In", "s\u00fcs\u00b7ser", "Ir\u00b7re", "sanft", "her\u00b7um\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verziehn und sinken sie gemach:", "tokens": ["Ver\u00b7ziehn", "und", "sin\u00b7ken", "sie", "ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier herrscht die Liebe! scheinen sie zu sagen;", "tokens": ["Hier", "herrscht", "die", "Lie\u00b7be", "!", "schei\u00b7nen", "sie", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier herrscht die Liebe! seufz ich nach.", "tokens": ["Hier", "herrscht", "die", "Lie\u00b7be", "!", "seufz", "ich", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dich, Laura, seh ich? Dich, Petrarchs Geliebte?", "tokens": ["Dich", ",", "Lau\u00b7ra", ",", "seh", "ich", "?", "Dich", ",", "Pe\u00b7trarchs", "Ge\u00b7lieb\u00b7te", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VVFIN", "PPER", "$.", "PPER", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die\u00df ist das himmlische Gesicht,", "tokens": ["Die\u00df", "ist", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch dem ein Himmel, den sie oft betr\u