{"textgrid.poem.35652": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "15. An Klopstock", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt hin, mein Lied! Tritt mutig vors Angesicht", "tokens": ["Tritt", "hin", ",", "mein", "Lied", "!", "Tritt", "mu\u00b7tig", "vors", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Sioniten! Zittre, wer Frevler ist!", "tokens": ["Des", "Si\u00b7o\u00b7ni\u00b7ten", "!", "Zitt\u00b7re", ",", "wer", "Frev\u00b7ler", "ist", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NE", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du, keines Knecht, selbst sein nicht! weihtest", "tokens": ["Du", ",", "kei\u00b7nes", "Knecht", ",", "selbst", "sein", "nicht", "!", "weih\u00b7test"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "PIAT", "NN", "$,", "ADV", "VAINF", "PTKNEG", "$.", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00fche dich Gott und dem Vaterlande!", "tokens": ["Fr\u00fc\u00b7he", "dich", "Gott", "und", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "In hoher Wolke feiert den Ewigen", "tokens": ["In", "ho\u00b7her", "Wol\u00b7ke", "fei\u00b7ert", "den", "E\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Ruf des Donners; aber ihn feiert auch", "tokens": ["Der", "Ruf", "des", "Don\u00b7ners", ";", "a\u00b7ber", "ihn", "fei\u00b7ert", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Halmes Grille, die dem Schnitter", "tokens": ["Des", "Hal\u00b7mes", "Gril\u00b7le", ",", "die", "dem", "Schnit\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00f6hlichkeit singt und der jungen Hirtin.", "tokens": ["Fr\u00f6h\u00b7lich\u00b7keit", "singt", "und", "der", "jun\u00b7gen", "Hir\u00b7tin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Hat nicht Eloa seinen Gesang geehrt?", "tokens": ["Hat", "nicht", "E\u00b7loa", "sei\u00b7nen", "Ge\u00b7sang", "ge\u00b7ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NE", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sprecht, Edens Wonnen, welch' ihm der Seraph schuf!", "tokens": ["Sprecht", ",", "E\u00b7dens", "Won\u00b7nen", ",", "welch'", "ihm", "der", "Se\u00b7raph", "schuf", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und er, von seinem Tabor, sollte", "tokens": ["Und", "er", ",", "von", "sei\u00b7nem", "Ta\u00b7bor", ",", "soll\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stolz auf den J\u00fcnglingsgesang herabschaun?", "tokens": ["Stolz", "auf", "den", "J\u00fcng\u00b7lings\u00b7ge\u00b7sang", "her\u00b7ab\u00b7schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Mann Gottes, wahrlich! kannst es nicht! kannst mein Herz", "tokens": ["Mann", "Got\u00b7tes", ",", "wahr\u00b7lich", "!", "kannst", "es", "nicht", "!", "kannst", "mein", "Herz"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "ADV", "$.", "VMFIN", "PPER", "PTKNEG", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht so betr\u00fcben! Schmeichler umarmte mich", "tokens": ["Nicht", "so", "be\u00b7tr\u00fc\u00b7ben", "!", "Schmeich\u00b7ler", "um\u00b7arm\u00b7te", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVINF", "$.", "NN", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Bruder? rief mir falschen Beifall?", "tokens": ["Mein", "Bru\u00b7der", "?", "rief", "mir", "fal\u00b7schen", "Bei\u00b7fall", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00e4dchen, du weintest mir Heuchelthr\u00e4nen?", "tokens": ["M\u00e4d\u00b7chen", ",", "du", "wein\u00b7test", "mir", "Heu\u00b7chelt\u00b7hr\u00e4\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Still, meine Seele! Wahrlich! er kann es nicht!...", "tokens": ["Still", ",", "mei\u00b7ne", "See\u00b7le", "!", "Wahr\u00b7lich", "!", "er", "kann", "es", "nicht", "!", "..."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$.", "ADV", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wenn sein Antlitz Segen mir l\u00e4chelte?", "tokens": ["Und", "wenn", "sein", "Ant\u00b7litz", "Se\u00b7gen", "mir", "l\u00e4\u00b7chel\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbgetrost, mein Sohn! du singst der Eiche", "tokens": ["\u00bb", "ge\u00b7trost", ",", "mein", "Sohn", "!", "du", "singst", "der", "Ei\u00b7che"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcrdiger einst, und der Palme Sions!\u00ab...", "tokens": ["W\u00fcr\u00b7di\u00b7ger", "einst", ",", "und", "der", "Pal\u00b7me", "Si\u00b7ons", "!", "\u00ab", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ART", "NN", "NE", "$.", "$(", "$("], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "O dann, ihr Br\u00fcder, schwur ich mit euch dem Herrn!", "tokens": ["O", "dann", ",", "ihr", "Br\u00fc\u00b7der", ",", "schwur", "ich", "mit", "euch", "dem", "Herrn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dann kniet' ich einsamweinend, als Knabe schon,", "tokens": ["Dann", "kniet'", "ich", "ein\u00b7sam\u00b7wei\u00b7nend", ",", "als", "Kna\u00b7be", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor meinem Gott, und fleht' um Weisheit;", "tokens": ["Vor", "mei\u00b7nem", "Gott", ",", "und", "fleht'", "um", "Weis\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich umschwoll's, wie Gesang des Himmels!", "tokens": ["Und", "mich", "um\u00b7schwoll's", ",", "wie", "Ge\u00b7sang", "des", "Him\u00b7mels", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann r\u00e4ch' ich, Unschuld, dich mit Jehovahs Kraft", "tokens": ["Dann", "r\u00e4ch'", "ich", ",", "Un\u00b7schuld", ",", "dich", "mit", "Je\u00b7ho\u00b7vahs", "Kraft"], "token_info": ["word", "