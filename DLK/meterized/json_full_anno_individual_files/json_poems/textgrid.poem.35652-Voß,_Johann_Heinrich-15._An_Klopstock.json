 "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor meinem Gott, und fleht' um Weisheit;", "tokens": ["Vor", "mei\u00b7nem", "Gott", ",", "und", "fleht'", "um", "Weis\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich umschwoll's, wie Gesang des Himmels!", "tokens": ["Und", "mich", "um\u00b7schwoll's", ",", "wie", "Ge\u00b7sang", "des", "Him\u00b7mels", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Dann r\u00e4ch' ich, Unschuld, dich mit Jehovahs Kraft", "tokens": ["Dann", "r\u00e4ch'", "ich", ",", "Un\u00b7schuld", ",", "dich", "mit", "Je\u00b7ho\u00b7vahs", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PRF", "APPR", "NE", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An Satans Priestern! an den Verr\u00e4tern dich", "tokens": ["An", "Sa\u00b7tans", "Pries\u00b7tern", "!", "an", "den", "Ver\u00b7r\u00e4\u00b7tern", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$.", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Vaterland! des P\u00f6bels Hohnruf", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "!", "des", "P\u00f6\u00b7bels", "Hohn\u00b7ruf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trotzend, und trotzend dem Wink des W\u00fctrichs!", "tokens": ["Trot\u00b7zend", ",", "und", "trot\u00b7zend", "dem", "Wink", "des", "W\u00fct\u00b7richs", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}