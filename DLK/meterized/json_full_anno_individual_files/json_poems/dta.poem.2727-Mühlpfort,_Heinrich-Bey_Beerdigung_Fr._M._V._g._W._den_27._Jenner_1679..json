{"dta.poem.2727": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung Fr. M. V. g. W. den 27.  \n Jenner 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll/ Ehrw\u00fcrdiger, die Poesi hier n\u00fctzen/", "tokens": ["Was", "soll", "/", "Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "die", "Poe\u00b7si", "hier", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So ein ergrim\u0303ter Schmertz schl\u00e4gt Kunst und Wei\u00df-", "tokens": ["So", "ein", "er\u00b7grim\u0303ter", "Schmertz", "schl\u00e4gt", "Kunst", "und", "Wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "NN", "KON", "TRUNC"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn diese Wetter dr\u00e4un und die Cometen blitzen/", "tokens": ["Wenn", "die\u00b7se", "Wet\u00b7ter", "dr\u00e4un", "und", "die", "Co\u00b7me\u00b7ten", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist von Angst und Noth erf\u00fcllt das gantze Hau\u00df.", "tokens": ["So", "ist", "von", "Angst", "und", "Noth", "er\u00b7f\u00fcllt", "das", "gant\u00b7ze", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sind die Thr\u00e4nen Blut der tieff-verwundten Seelen;", "tokens": ["Und", "sind", "die", "Thr\u00e4\u00b7nen", "Blut", "der", "tief\u00b7fver\u00b7wund\u00b7ten", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wei\u00df ich da\u00df sein Hertz in nichts als Blute schwimmt.", "tokens": ["So", "wei\u00df", "ich", "da\u00df", "sein", "Hertz", "in", "nichts", "als", "Blu\u00b7te", "schwimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "APPR", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es kan der Augen-Brunn die Quellen nicht verh\u00f6len/", "tokens": ["Es", "kan", "der", "Au\u00b7gen\u00b7Brunn", "die", "Quel\u00b7len", "nicht", "ver\u00b7h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Fluth vertrocknet nicht/ so da den Ursprung nimmt.", "tokens": ["Die", "Fluth", "ver\u00b7trock\u00b7net", "nicht", "/", "so", "da", "den", "Ur\u00b7sprung", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie viel er sonst geheilt/ wie viel er aufgerichtet/", "tokens": ["Wie", "viel", "er", "sonst", "ge\u00b7heilt", "/", "wie", "viel", "er", "auf\u00b7ge\u00b7rich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VVPP", "$(", "PWAV", "PIS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wann sie dergleichen Rie\u00df zur Erden hat gebeugt/", "tokens": ["Wann", "sie", "derg\u00b7lei\u00b7chen", "Rie\u00df", "zur", "Er\u00b7den", "hat", "ge\u00b7beugt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wird doch durch den Schmertz itzt aller Trost zernichtet", "tokens": ["So", "wird", "doch", "durch", "den", "Schmertz", "itzt", "al\u00b7ler", "Trost", "zer\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein Hertz ist Traurens voll und seine Lippe schweigt.", "tokens": ["Sein", "Hertz", "ist", "Trau\u00b7rens", "voll", "und", "sei\u00b7ne", "Lip\u00b7pe", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADJD", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hat alles seine Zeit/ so ist itzt Zeit zu weinen.", "tokens": ["Hat", "al\u00b7les", "sei\u00b7ne", "Zeit", "/", "so", "ist", "itzt", "Zeit", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie man sich in dem Gl\u00fcck mit Freunden sonst ergetzt/", "tokens": ["Wie", "man", "sich", "in", "dem", "Gl\u00fcck", "mit", "Freun\u00b7den", "sonst", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So sollen wir im Fall mitleidig auch erscheinen/", "tokens": ["So", "sol\u00b7len", "wir", "im", "Fall", "mit\u00b7lei\u00b7dig", "auch", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn sie des H\u00f6chsten Hand in Staub und Asche setzt.", "tokens": ["Wenn", "sie", "des", "H\u00f6chs\u00b7ten", "Hand", "in", "Staub", "und", "A\u00b7sche", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er mu\u00df/ ", "tokens": ["Er", "mu\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VMFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Sein Leit-Stern der versinckt itzt in des Grabes Nacht:", "tokens": ["Sein", "Leit\u00b7S\u00b7tern", "der", "ver\u00b7sinckt", "itzt", "in", "des", "Gra\u00b7bes", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Des Hauses Sonn ist hin/ er sitzt in Finstern\u00fcssen/", "tokens": ["Des", "Hau\u00b7ses", "Sonn", "ist", "hin", "/", "er", "sitzt", "in", "Fins\u00b7ter\u00b7n\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$(", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Leuchte scheint nicht mehr/ so alles hell gemacht.", "tokens": ["Die", "Leuch\u00b7te", "scheint", "nicht", "mehr", "/", "so", "al\u00b7les", "hell", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$(", "ADV", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die treue Pflegerin wird ihm zu fr\u00fch\u2019 entzogen/", "tokens": ["Die", "treu\u00b7e", "Pfle\u00b7ge\u00b7rin", "wird", "ihm", "zu", "fr\u00fch'", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die beste Freundin geht zu fl\u00fcchtig aus der Welt:", "tokens": ["Die", "bes\u00b7te", "Freun\u00b7din", "geht", "zu", "fl\u00fcch\u00b7tig", "aus", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Taube seiner Eh ist gar zu bald entflogen/", "tokens": ["Die", "Tau\u00b7be", "sei\u00b7ner", "Eh", "ist", "gar", "zu", "bald", "ent\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Oelbaum seiner Ruh\u2019 ist durch den Tod zerschellt.", "tokens": ["Der", "O\u00b7el\u00b7baum", "sei\u00b7ner", "Ruh'", "ist", "durch", "den", "Tod", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Der Ertz-Schrein ist entzwey/ darein sein Hertz verschlossen:", "tokens": ["Der", "Ertz\u00b7Schrein", "ist", "ent\u00b7zwey", "/", "da\u00b7rein", "sein", "Hertz", "ver\u00b7schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$(", "PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Schlaff-Gemach zerst\u00f6rt/ wo seine Seele schlieff:", "tokens": ["Das", "Schlaff\u00b7Ge\u00b7mach", "zer\u00b7st\u00f6rt", "/", "wo", "sei\u00b7ne", "See\u00b7le", "schlieff", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sein Lebens-Balsam ist nur/ leider/ gantz vergossen!", "tokens": ["Sein", "Le\u00b7bens\u00b7Bal\u00b7sam", "ist", "nur", "/", "lei\u00b7der", "/", "gantz", "ver\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "ADV", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Er sieht im Todten-Meer vergehn sein Wohlfarts-Schiff.", "tokens": ["Er", "sieht", "im", "Tod\u00b7ten\u00b7Meer", "ver\u00b7gehn", "sein", "Wohlf\u00b7arts\u00b7Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Traube die ihn hat mit Nectar stets getr\u00e4ncket/", "tokens": ["Die", "Trau\u00b7be", "die", "ihn", "hat", "mit", "Nec\u00b7tar", "stets", "ge\u00b7tr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Gewehret nunmehr nichts als einen Myrrhen-Safft.", "tokens": ["Ge\u00b7weh\u00b7ret", "nun\u00b7mehr", "nichts", "als", "ei\u00b7nen", "Myr\u00b7rhen\u00b7\u00b7S\u00b7afft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Das Kleinod/ das er hat stets an die Brust gehencket/", "tokens": ["Das", "Klei\u00b7nod", "/", "das", "er", "hat", "stets", "an", "die", "Brust", "ge\u00b7hen\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wird durch die Grausamkeit de\u00df Todes hingerafft.", "tokens": ["Wird", "durch", "die", "Grau\u00b7sam\u00b7keit", "de\u00df", "To\u00b7des", "hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hochwichtig ist sein Leid. So einen Schatz verliehren", "tokens": ["Hoch\u00b7wich\u00b7tig", "ist", "sein", "Leid", ".", "So", "ei\u00b7nen", "Schatz", "ver\u00b7lieh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Kan ohne Seelen-Weh ja nimmermehr geschehn/", "tokens": ["Kan", "oh\u00b7ne", "See\u00b7len\u00b7Weh", "ja", "nim\u00b7mer\u00b7mehr", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es mu\u00df das innerste in Marck und Adern r\u00fchren/", "tokens": ["Es", "mu\u00df", "das", "in\u00b7ners\u00b7te", "in", "Marck", "und", "A\u00b7dern", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Wenn so ein Tugend-Bild man soll erblassen sehn.", "tokens": ["Wenn", "so", "ein", "Tu\u00b7gen\u00b7dBild", "man", "soll", "er\u00b7blas\u00b7sen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PIS", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und wenn der Ph\u00f6bus mir die Wort in Perlen kehrte/", "tokens": ["Und", "wenn", "der", "Ph\u00f6\u00b7bus", "mir", "die", "Wort", "in", "Per\u00b7len", "kehr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und seine g\u00f6ldne Harff und Laute reichte dar/", "tokens": ["Und", "sei\u00b7ne", "g\u00f6ld\u00b7ne", "Harff", "und", "Lau\u00b7te", "reich\u00b7te", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn mich der Bienen-Volck mit ihrem Honig n\u00e4hrte", "tokens": ["Wenn", "mich", "der", "Bie\u00b7nen\u00b7Volck", "mit", "ih\u00b7rem", "Ho\u00b7nig", "n\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und mir den Lorber-Baum Thalia flicht ins Haar/", "tokens": ["Und", "mir", "den", "Lor\u00b7ber\u00b7Baum", "Tha\u00b7lia", "flicht", "ins", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NE", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.41": {"text": "So w\u00fcrd ich doch nicht recht die Einigkeit erheben/", "tokens": ["So", "w\u00fcrd", "ich", "doch", "nicht", "recht", "die", "Ei\u00b7nig\u00b7keit", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD",