{"dta.poem.12679": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Rasinden.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist es, da\u00df bey uns die jungfern he\u00dflich macht?", "tokens": ["Was", "ist", "es", ",", "da\u00df", "bey", "uns", "die", "jung\u00b7fern", "he\u00df\u00b7lich", "macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die kleider-ordnung nimmt den meisten ihre pracht;", "tokens": ["Die", "klei\u00b7der\u00b7ord\u00b7nung", "nimmt", "den", "meis\u00b7ten", "ih\u00b7re", "pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ordnung ist gewi\u00df auch zur Rasinde kommen,", "tokens": ["Die", "ord\u00b7nung", "ist", "ge\u00b7wi\u00df", "auch", "zur", "Ra\u00b7sin\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum hat auf einmal sie so schrecklich abgenommen.", "tokens": ["Drum", "hat", "auf", "ein\u00b7mal", "sie", "so", "schreck\u00b7lich", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}