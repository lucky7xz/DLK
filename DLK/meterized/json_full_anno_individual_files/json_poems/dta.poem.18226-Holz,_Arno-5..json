{"dta.poem.18226": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Der Abendstrahl,", "tokens": ["Der", "A\u00b7bend\u00b7strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und droben im Flieder fl\u00f6tet", "tokens": ["Und", "dro\u00b7ben", "im", "Flie\u00b7der", "fl\u00f6\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Nachtigall.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O, wie die Kl\u00e4nge mir schlagen", "tokens": ["O", ",", "wie", "die", "Kl\u00e4n\u00b7ge", "mir", "schla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PPER", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "So s\u00fc\u00df ans Ohr,", "tokens": ["So", "s\u00fc\u00df", "ans", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Als wollten auch ", "tokens": ["Als", "woll\u00b7ten", "auch"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VMFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und mit silbernem Klanggefieder", "tokens": ["Und", "mit", "sil\u00b7ber\u00b7nem", "Klang\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ins Herz mir dringt:", "tokens": ["Ins", "Herz", "mir", "dringt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u201enun hast auch Du empfunden", "tokens": ["\u201e", "nun", "hast", "auch", "Du", "emp\u00b7fun\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Liebe Leid", "tokens": ["Der", "Lie\u00b7be", "Leid"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und kannst nun nie gesunden", "tokens": ["Und", "kannst", "nun", "nie", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu keiner Zeit!", "tokens": ["Zu", "kei\u00b7ner", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Vom Himmelszelt,", "tokens": ["Vom", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das Weh der Welt.", "tokens": ["Das", "Weh", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O da\u00df am Himmel die Sonne", "tokens": ["O", "da\u00df", "am", "Him\u00b7mel", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Nicht ewig steht,", "tokens": ["Nicht", "e\u00b7wig", "steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und da\u00df der Liebe ", "tokens": ["Und", "da\u00df", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "So bald verweht!\u201c .....", "tokens": ["So", "bald", "ver\u00b7weht", "!", "\u201c", "....."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$.", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Durch d\u00e4mmernde Abendh\u00fclle", "tokens": ["Durch", "d\u00e4m\u00b7mern\u00b7de", "A\u00b7bend\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "T\u00f6nt so der Sang,", "tokens": ["T\u00f6nt", "so", "der", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und eine Lieder", "tokens": ["Und", "ei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ist jeder Klang.", "tokens": ["Ist", "je\u00b7der", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schon senkte sich zitternden Fluges", "tokens": ["Schon", "senk\u00b7te", "sich", "zit\u00b7tern\u00b7den", "Flu\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Aufs Thal die Nacht,", "tokens": ["Aufs", "Thal", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da ist dem Tage des Truges", "tokens": ["Da", "ist", "dem", "Ta\u00b7ge", "des", "Tru\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Ein End gemacht.", "tokens": ["Ein", "End", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Doch ob auch die Sterne nun blinken", "tokens": ["Doch", "ob", "auch", "die", "Ster\u00b7ne", "nun", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Am Himmelsdom,", "tokens": ["Am", "Him\u00b7mels\u00b7dom", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und ihre Lichter mir winken", "tokens": ["Und", "ih\u00b7re", "Lich\u00b7ter", "mir", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus jedem Strom,", "tokens": ["Aus", "je\u00b7dem", "Strom", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und ob auch der Mond mir ins Zimmer", "tokens": ["Und", "ob", "auch", "der", "Mond", "mir", "ins", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Gie\u00dft all den Schein:", "tokens": ["Gie\u00dft", "all", "den", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Das wiegt doch nie und nimmer", "tokens": ["Das", "wiegt", "doch", "nie", "und", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Herz mir ein! \u2014", "tokens": ["Das", "Herz", "mir", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Du aber, o Traum, o umf\u00e4chle", "tokens": ["Du", "a\u00b7ber", ",", "o", "Traum", ",", "o", "um\u00b7f\u00e4ch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "FM", "FM"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das s\u00fc\u00dfe Kind,", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df es im Schlafe noch l\u00e4chle", "tokens": ["Da\u00df", "es", "im", "Schla\u00b7fe", "noch", "l\u00e4ch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So wunderlind!", "tokens": ["So", "wun\u00b7der\u00b7lind", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O schwebe im Glorienschimmer", "tokens": ["O", "schwe\u00b7be", "im", "Glo\u00b7ri\u00b7en\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Von Gottes Thron", "tokens": ["Von", "Got\u00b7tes", "Thron"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und la\u00df es vergessen auf immer", "tokens": ["Und", "la\u00df", "es", "ver\u00b7ges\u00b7sen", "auf", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVPP", "APPR", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Liedersohn! \u2014 \u2014", "tokens": ["Den", "Lie\u00b7der\u00b7sohn", "!"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}