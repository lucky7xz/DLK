{"dta.poem.13029": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas n\u00fczet dieser Streit? zu was das eitle Zancken? \u201e", "tokens": ["\u201e", "was", "n\u00fc\u00b7zet", "die\u00b7ser", "Streit", "?", "zu", "was", "das", "eit\u00b7le", "Zan\u00b7cken", "?", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PDAT", "NN", "$.", "APPR", "PRELS", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach hier die ", "tokens": ["Sprach", "hier", "die"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "\u201ees ist mein Meisterst\u00fcck! das Buch ist eine Frucht,", "tokens": ["\u201e", "es", "ist", "mein", "Meis\u00b7ter\u00b7st\u00fcck", "!", "das", "Buch", "ist", "ei\u00b7ne", "Frucht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie meiner Sorge Macht zu pflanzen hat gesucht;", "tokens": ["\u201e", "die", "mei\u00b7ner", "Sor\u00b7ge", "Macht", "zu", "pflan\u00b7zen", "hat", "ge\u00b7sucht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "175\u201dSie wuchse so behend, und triebe solche Sprossen,", "tokens": ["\"", "Sie", "wuch\u00b7se", "so", "be\u00b7hend", ",", "und", "trie\u00b7be", "sol\u00b7che", "Spros\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eaus derer Kern und Saft dieselben B\u00e4ume schossen,", "tokens": ["\u201e", "aus", "de\u00b7rer", "Kern", "und", "Saft", "die\u00b7sel\u00b7ben", "B\u00e4u\u00b7me", "schos\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDS", "NN", "KON", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "\u201evon welchen meine Kunst den S\u00e4ulen-Bau geschnizt,", "tokens": ["\u201e", "von", "wel\u00b7chen", "mei\u00b7ne", "Kunst", "den", "S\u00e4u\u00b7len\u00b7Bau", "ge\u00b7schnizt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201emit welchem man den Thron der K\u00f6niginn gest\u00fczt,", "tokens": ["\u201e", "mit", "wel\u00b7chem", "man", "den", "Thron", "der", "K\u00f6\u00b7ni\u00b7ginn", "ge\u00b7st\u00fczt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PIS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eals ihn der Sturm des Kriegs von weiten nur gedrohet,", "tokens": ["\u201e", "als", "ihn", "der", "Sturm", "des", "Kriegs", "von", "wei\u00b7ten", "nur", "ge\u00b7dro\u00b7het", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "ART", "NN", "APPR", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "180\u201dUnd ihr zum Theil verzagt aus der Gefahr entflohet.", "tokens": ["\"", "Und", "ihr", "zum", "Theil", "ver\u00b7zagt", "aus", "der", "Ge\u00b7fahr", "ent\u00b7flo\u00b7het", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}