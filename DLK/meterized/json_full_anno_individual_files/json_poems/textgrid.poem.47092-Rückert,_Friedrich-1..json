{"textgrid.poem.47092": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unvergleichlich bl\u00fcht um mich der Fr\u00fchling,", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7lich", "bl\u00fcht", "um", "mich", "der", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die Fenster schlagen Nachtigallen,", "tokens": ["In", "die", "Fens\u00b7ter", "schla\u00b7gen", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heiter blickt der Himmel her, die Sonne", "tokens": ["Hei\u00b7ter", "blickt", "der", "Him\u00b7mel", "her", ",", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In das St\u00fcbchen, wo ich sitz' und dichte.", "tokens": ["In", "das", "St\u00fcb\u00b7chen", ",", "wo", "ich", "sitz'", "und", "dich\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mehr als Blumen im Gefilde, sprossen", "tokens": ["Mehr", "als", "Blu\u00b7men", "im", "Ge\u00b7fil\u00b7de", ",", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "KOKOM", "NN", "APPRART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lieder t\u00e4glich unter meiner Feder.", "tokens": ["Lie\u00b7der", "t\u00e4g\u00b7lich", "un\u00b7ter", "mei\u00b7ner", "Fe\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und vom Flore meiner Bl\u00e4tter blick' ich", "tokens": ["Und", "vom", "Flo\u00b7re", "mei\u00b7ner", "Bl\u00e4t\u00b7ter", "blick'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zwischenhin auf den des Fr\u00fchlings drau\u00dfen,", "tokens": ["Zwi\u00b7schen\u00b7hin", "auf", "den", "des", "Fr\u00fch\u00b7lings", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "L\u00e4chl' ihm zu und seh' ihn wieder l\u00e4chlen.", "tokens": ["L\u00e4chl'", "ihm", "zu", "und", "seh'", "ihn", "wie\u00b7der", "l\u00e4ch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Jeder von uns beiden scheint zufrieden", "tokens": ["Je\u00b7der", "von", "uns", "bei\u00b7den", "scheint", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PPER", "PIS", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Mit sich selbst und mit dem andern, jeder", "tokens": ["Mit", "sich", "selbst", "und", "mit", "dem", "an\u00b7dern", ",", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PRF", "ADV", "KON", "APPR", "ART", "ADJA", "$,", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Thut und l\u00e4\u00dft den andern thun das Seine.", "tokens": ["Thut", "und", "l\u00e4\u00dft", "den", "an\u00b7dern", "thun", "das", "Sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "ADJA", "VVFIN", "ART", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und, den Tag lang dichtend, denk' ich immer", "tokens": ["Und", ",", "den", "Tag", "lang", "dich\u00b7tend", ",", "denk'", "ich", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ART", "NN", "ADJD", "ADJD", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "An den Abend, wo, zu s\u00fc\u00dfen Tagwerks", "tokens": ["An", "den", "A\u00b7bend", ",", "wo", ",", "zu", "s\u00fc\u00b7\u00dfen", "Tag\u00b7werks"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "S\u00fc\u00dfem Lohn, ich gehe zu der Guten,", "tokens": ["S\u00fc\u00b7\u00dfem", "Lohn", ",", "ich", "ge\u00b7he", "zu", "der", "Gu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Die mit treuer anspruchloser Neigung", "tokens": ["Die", "mit", "treu\u00b7er", "an\u00b7spruch\u00b7lo\u00b7ser", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Mich begl\u00fcckt, wie ich es nie mir tr\u00e4umte.", "tokens": ["Mich", "be\u00b7gl\u00fcckt", ",", "wie", "ich", "es", "nie", "mir", "tr\u00e4um\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PWAV", "PPER", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Hab' ich doch allein f\u00fcr sie gedichtet,", "tokens": ["Hab'", "ich", "doch", "al\u00b7lein", "f\u00fcr", "sie", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Wie der Fr\u00fchling sich f\u00fcr sie nur schm\u00fcckte.", "tokens": ["Wie", "der", "Fr\u00fch\u00b7ling", "sich", "f\u00fcr", "sie", "nur", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Und sie freut sich meiner Liedesbl\u00fcten,", "tokens": ["Und", "sie", "freut", "sich", "mei\u00b7ner", "Lie\u00b7des\u00b7bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Wie der Kr\u00e4nze, die der Lenz ihr bietet,", "tokens": ["Wie", "der", "Kr\u00e4n\u00b7ze", ",", "die", "der", "Lenz", "ihr", "bie\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Teilt ihr L\u00e4cheln zwischen beiden Freunden,", "tokens": ["Teilt", "ihr", "L\u00e4\u00b7cheln", "zwi\u00b7schen", "bei\u00b7den", "Freun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Die einander nicht den Anteil neiden.", "tokens": ["Die", "ein\u00b7an\u00b7der", "nicht", "den", "An\u00b7teil", "nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Lieben, dichten und den Fr\u00fchling schauen,", "tokens": ["Lie\u00b7ben", ",", "dich\u00b7ten", "und", "den", "Fr\u00fch\u00b7ling", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Dichten und den Fr\u00fchling schaun und lieben \u2013", "tokens": ["Dich\u00b7ten", "und", "den", "Fr\u00fch\u00b7ling", "schaun", "und", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVINF", "KON", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Gibt es einen angenehmern Kreislauf,", "tokens": ["Gibt", "es", "ei\u00b7nen", "an\u00b7ge\u00b7neh\u00b7mern", "Kreis\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Als in dem ich spielend mich bewege?", "tokens": ["Als", "in", "dem", "ich", "spie\u00b7lend", "mich", "be\u00b7we\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und, den s\u00fc\u00dfen Kelch mir scharf zu w\u00fcrzen,", "tokens": ["Und", ",", "den", "s\u00fc\u00b7\u00dfen", "Kelch", "mir", "scharf", "zu", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Rascher zum Genu\u00df mich aufzufordern,", "tokens": ["Ra\u00b7scher", "zum", "Ge\u00b7nu\u00df", "mich", "auf\u00b7zu\u00b7for\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Steht der Abschied winkend in der Ferne.", "tokens": ["Steht", "der", "Ab\u00b7schied", "win\u00b7kend", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "N\u00e4her treten seh' ich ihn bedeutsam,", "tokens": ["N\u00e4\u00b7her", "tre\u00b7ten", "seh'", "ich", "ihn", "be\u00b7deut\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Sprechend: Alles dieses mu\u00dft du lassen.", "tokens": ["Spre\u00b7chend", ":", "Al\u00b7les", "die\u00b7ses", "mu\u00dft", "du", "las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PIS", "PDAT", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Wie das Leben sch\u00f6n ist, weil es endet,", "tokens": ["Wie", "das", "Le\u00b7ben", "sch\u00f6n", "ist", ",", "weil", "es", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Wie die Jugend lieblich, weil sie fliehet,", "tokens": ["Wie", "die", "Ju\u00b7gend", "lieb\u00b7lich", ",", "weil", "sie", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Wie die Rose reizend, weil sie welket;", "tokens": ["Wie", "die", "Ro\u00b7se", "rei\u00b7zend", ",", "weil", "sie", "wel\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "So empfind' ich heut ein Gl\u00fcck gedoppelt,", "tokens": ["So", "emp\u00b7find'", "ich", "heut", "ein", "Gl\u00fcck", "ge\u00b7dop\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Das mir morgen schon der Tod will rauben.", "tokens": ["Das", "mir", "mor\u00b7gen", "schon", "der", "Tod", "will", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Angefangne Lieder m\u00f6cht' ich enden,", "tokens": ["An\u00b7ge\u00b7fang\u00b7ne", "Lie\u00b7der", "m\u00f6cht'", "ich", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Doch unendlich quellen sie im Herzen.", "tokens": ["Doch", "un\u00b7end\u00b7lich", "quel\u00b7len", "sie", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Rosenknospen m\u00f6cht' ich noch im Garten", "tokens": ["Ro\u00b7sen\u00b7knos\u00b7pen", "m\u00f6cht'", "ich", "noch", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Sich zur Bl\u00fct' erschlie\u00dfen sehn und brechen.", "tokens": ["Sich", "zur", "Bl\u00fct'", "er\u00b7schlie\u00b7\u00dfen", "sehn", "und", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Und die Sonne dieser tiefen Augen,", "tokens": ["Und", "die", "Son\u00b7ne", "die\u00b7ser", "tie\u00b7fen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.43": {"text": "Die mit jedem Blick von Seelentreue,", "tokens": ["Die", "mit", "je\u00b7dem", "Blick", "von", "See\u00b7len\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Ew'ger F\u00fclle der Empfindung sprechen,", "tokens": ["Ew'\u00b7ger", "F\u00fcl\u00b7le", "der", "Emp\u00b7fin\u00b7dung", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "M\u00f6cht' ich ganz noch in die Seele trinken.", "tokens": ["M\u00f6cht'", "ich", "ganz", "noch", "in", "die", "See\u00b7le", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "La\u00df, o Herz, dich nicht vom Drang verwirren,", "tokens": ["La\u00df", ",", "o", "Herz", ",", "dich", "nicht", "vom", "Drang", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "Sondern nimm, was du noch darfst, besonnen:", "tokens": ["Son\u00b7dern", "nimm", ",", "was", "du", "noch", "darfst", ",", "be\u00b7son\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PWS", "PPER", "ADV", "PAV", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Diese ungebornen Lieder alle,", "tokens": ["Die\u00b7se", "un\u00b7ge\u00b7bor\u00b7nen", "Lie\u00b7der", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.49": {"text": "All die Hoffnung dieser Rosenknospen,", "tokens": ["All", "die", "Hoff\u00b7nung", "die\u00b7ser", "Ro\u00b7sen\u00b7knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Diesen Fr\u00fchling, diesen Liebeshimmel,", "tokens": ["Die\u00b7sen", "Fr\u00fch\u00b7ling", ",", "die\u00b7sen", "Lie\u00b7bes\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "All dies Gl\u00fcck, o fass' es, wenn du scheidest,", "tokens": ["All", "dies", "Gl\u00fcck", ",", "o", "fass'", "es", ",", "wenn", "du", "schei\u00b7dest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "NN", "$,", "FM", "FM", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.52": {"text": "In ein liebendes Gef\u00fchl zusammen,", "tokens": ["In", "ein", "lie\u00b7ben\u00b7des", "Ge\u00b7f\u00fchl", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.53": {"text": "Nimm es mit! Wer kann's der Seele rauben?", "tokens": ["Nimm", "es", "mit", "!", "Wer", "kann's", "der", "See\u00b7le", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "Die Erinn'rung wird davon sich n\u00e4hren,", "tokens": ["Die", "Er\u00b7inn'\u00b7rung", "wird", "da\u00b7von", 