en?", "tokens": ["Nimm", "es", "mit", "!", "Wer", "kann's", "der", "See\u00b7le", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "Die Erinn'rung wird davon sich n\u00e4hren,", "tokens": ["Die", "Er\u00b7inn'\u00b7rung", "wird", "da\u00b7von", "sich", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.55": {"text": "Wenn die Gegenwart die s\u00fc\u00dfe Nahrung", "tokens": ["Wenn", "die", "Ge\u00b7gen\u00b7wart", "die", "s\u00fc\u00b7\u00dfe", "Nah\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.56": {"text": "Dir versagt, woran dein Herz gew\u00f6hnt ist.", "tokens": ["Dir", "ver\u00b7sagt", ",", "wo\u00b7ran", "dein", "Herz", "ge\u00b7w\u00f6hnt", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.57": {"text": "Phantasie und Liebe, deren Fl\u00fcgel", "tokens": ["Phan\u00b7ta\u00b7sie", "und", "Lie\u00b7be", ",", "de\u00b7ren", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PRELAT", "NN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.58": {"text": "Nicht der Zeit, der R\u00e4ume Trennung achtet,", "tokens": ["Nicht", "der", "Zeit", ",", "der", "R\u00e4u\u00b7me", "Tren\u00b7nung", "ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.59": {"text": "Wird, wo du auf \u00f6den Steppen weilest,", "tokens": ["Wird", ",", "wo", "du", "auf", "\u00f6\u00b7den", "Step\u00b7pen", "wei\u00b7lest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.60": {"text": "Jeden Augenblick zur\u00fcck dich tragen", "tokens": ["Je\u00b7den", "Au\u00b7gen\u00b7blick", "zu\u00b7r\u00fcck", "dich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKVZ", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.61": {"text": "In das Paradies, das du verlassen.", "tokens": ["In", "das", "Pa\u00b7ra\u00b7dies", ",", "das", "du", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}