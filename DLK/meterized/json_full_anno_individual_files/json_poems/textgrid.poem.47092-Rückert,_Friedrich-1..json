"meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Gibt es einen angenehmern Kreislauf,", "tokens": ["Gibt", "es", "ei\u00b7nen", "an\u00b7ge\u00b7neh\u00b7mern", "Kreis\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Als in dem ich spielend mich bewege?", "tokens": ["Als", "in", "dem", "ich", "spie\u00b7lend", "mich", "be\u00b7we\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und, den s\u00fc\u00dfen Kelch mir scharf zu w\u00fcrzen,", "tokens": ["Und", ",", "den", "s\u00fc\u00b7\u00dfen", "Kelch", "mir", "scharf", "zu", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Rascher zum Genu\u00df mich aufzufordern,", "tokens": ["Ra\u00b7scher", "zum", "Ge\u00b7nu\u00df", "mich", "auf\u00b7zu\u00b7for\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Steht der Abschied winkend in der Ferne.", "tokens": ["Steht", "der", "Ab\u00b7schied", "win\u00b7kend", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "N\u00e4her treten seh' ich ihn bedeutsam,", "tokens": ["N\u00e4\u00b7her", "tre\u00b7ten", "seh'", "ich", "ihn", "be\u00b7deut\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Sprechend: Alles dieses mu\u00dft du lassen.", "tokens": ["Spre\u00b7chend", ":", "Al\u00b7les", "die\u00b7ses", "mu\u00dft", "du", "las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PIS", "PDAT", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Wie das Leben sch\u00f6n ist, weil es endet,", "tokens": ["Wie", "das", "Le\u00b7ben", "sch\u00f6n", "ist", ",", "weil", "es", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Wie die Jugend lieblich, weil sie fliehet,", "tokens": ["Wie", "die", "Ju\u00b7gend", "lieb\u00b7lich", ",", "weil", "sie", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Wie die Rose reizend, weil sie welket;", "tokens": ["Wie", "die", "Ro\u00b7se", "rei\u00b7zend", ",", "weil", "sie", "wel\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "So empfind' ich heut ein Gl\u00fcck gedoppelt,", "tokens": ["So", "emp\u00b7find'", "ich", "heut", "ein", "Gl\u00fcck", "ge\u00b7dop\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Das mir morgen schon der Tod will rauben.", "tokens": ["Das", "mir", "mor\u00b7gen", "schon", "der", "Tod", "will", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Angefangne Lieder m\u00f6cht' ich enden,", "tokens": ["An\u00b7ge\u00b7fang\u00b7ne", "Lie\u00b7der", "m\u00f6cht'", "ich", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Doch unendlich quellen sie im Herzen.", "tokens": ["Doch", "un\u00b7end\u00b7lich", "quel\u00b7len", "sie", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Rosenknospen m\u00f6cht' ich noch im Garten", "tokens": ["Ro\u00b7sen\u00b7knos\u00b7pen", "m\u00f6cht'", "ich", "noch", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Sich zur Bl\u00fct' erschlie\u00dfen sehn und brechen.", "tokens": ["Sich", "zur", "Bl\u00fct'", "er\u00b7schlie\u00b7\u00dfen", "sehn", "und", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Und die Sonne dieser tiefen Augen,", "tokens": ["Und", "die", "Son\u00b7ne", "die\u00b7ser", "tie\u00b7fen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.43": {"text": "Die mit jedem Blick von Seelentreue,", "tokens": ["Die", "mit", "je\u00b7dem", "Blick", "von", "See\u00b7len\u00b7treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Ew'ger F\u00fclle der Empfindung sprechen,", "tokens": ["Ew'\u00b7ger", "F\u00fcl\u00b7le", "der", "Emp\u00b7fin\u00b7dung", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "M\u00f6cht' ich ganz noch in die Seele trinken.", "tokens": ["M\u00f6cht'", "ich", "ganz", "noch", "in", "die", "See\u00b7le", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "La\u00df, o Herz, dich nicht vom Drang verwirren,", "tokens": ["La\u00df", ",", "o", "Herz", ",", "dich", "nicht", "vom", "Drang", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "Sondern nimm, was du noch darfst, besonnen:", "tokens": ["Son\u00b7dern", "nimm", ",", "was", "du", "noch", "darfst", ",", "be\u00b7son\u00b7nen", ":"], "token_info": ["word", "word", "punct", "w