{"dta.poem.837": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.  Auf den Sontag de\u00df Ewigen Messi \u00e6 /  \n oder  Judica,  Johan. 8.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Nvn kan ich/ wenn ich soll/ vol trost die Augen schliessen/ ", "tokens": ["Nvn", "kan", "ich", "/", "wenn", "ich", "soll", "/", "vol", "trost", "die", "Au\u00b7gen", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$(", "KOUS", "PPER", "VMFIN", "$(", "ADJD", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd sagen Welt Ade/ wer Christi worten traw\u2019t", "tokens": ["Vnd", "sa\u00b7gen", "Welt", "A\u00b7de", "/", "wer", "Chris\u00b7ti", "wor\u00b7ten", "tra\u00b7w't"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN", "$(", "PWS", "NE", "NN", "VVFIN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Schl\u00e4fft/ wenn er stirb\u2019t/ nur eyn! wer jrrdisch ist d\u1ebd graw\u2019t/", "tokens": ["Schl\u00e4fft", "/", "wenn", "er", "stir\u00b7b't", "/", "nur", "eyn", "!", "wer", "jrr\u00b7disch", "ist", "d\u1ebd", "gra\u00b7w't", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "VVFIN", "$(", "ADV", "VAINF", "$.", "PWS", "PPER", "VAFIN", "ADV", "VVFIN", "$("], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dafern er soll zuletzt di\u00df todte Leben gr\u00fcssen/", "tokens": ["Da\u00b7fern", "er", "soll", "zu\u00b7letzt", "di\u00df", "tod\u00b7te", "Le\u00b7ben", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "PDS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Di\u00df Leben da wir schmach vnd Steine leiden m\u00fcssen/", "tokens": ["Di\u00df", "Le\u00b7ben", "da", "wir", "schmach", "vnd", "Stei\u00b7ne", "lei\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "KOUS", "PPER", "VVFIN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd nichts denn Teuffel seyn/ O seelig/ wer bald schaw\u2019t", "tokens": ["Vnd", "nichts", "denn", "Teuf\u00b7fel", "seyn", "/", "O", "see\u00b7lig", "/", "wer", "bald", "scha\u00b7w't"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "KON", "NN", "VAINF", "$(", "NE", "ADJD", "$(", "PWS", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was Abraham ergetz\u2019t vnd auf den grundsteinbaw\u2019t", "tokens": ["Was", "Ab\u00b7ra\u00b7ham", "er\u00b7getz't", "vnd", "auf", "den", "grund\u00b7stein\u00b7ba\u00b7w't"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "VVFIN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Gott die warheit selbst/ die alles kan durchs\u00fcssen", "tokens": ["Der", "Gott", "die", "war\u00b7heit", "selbst", "/", "die", "al\u00b7les", "kan", "durch\u00b7s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$(", "ART", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was Menschen saw\u2019r eingeht/ Mich treugt di\u00df hoffen", "tokens": ["Was", "Men\u00b7schen", "sa\u00b7w'r", "ein\u00b7geht", "/", "Mich", "treugt", "di\u00df", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "ADJD", "VVFIN", "$(", "PPER", "VVFIN", "PDS", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "nicht/ ", "tokens": ["nicht", "/"], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ja! Mu\u00df ich in den thal der Finstern\u00fc\u00df/ Mein Licht/", "tokens": ["Ja", "!", "Mu\u00df", "ich", "in", "den", "thal", "der", "Fins\u00b7ter\u00b7n\u00fc\u00df", "/", "Mein", "Licht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Er ist das Leben selbst! mein Leben ist nur Noth.", "tokens": ["Er", "ist", "das", "Le\u00b7ben", "selbst", "!", "mein", "Le\u00b7ben", "ist", "nur", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Schatten/ Rauch v\u00f1 Wind/ ein tansendfacher Todt:", "tokens": ["Ein", "Schat\u00b7ten", "/", "Rauch", "v\u00f1", "Wind", "/", "ein", "tan\u00b7send\u00b7fa\u00b7cher", "Todt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein sterben aber nichts: al\u00df; in den Himmel schreitten", "tokens": ["Mein", "ster\u00b7ben", "a\u00b7ber", "nichts", ":", "al\u00df", ";", "in", "den", "Him\u00b7mel", "schreit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "ADV", "PIS", "$.", "KOUS", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}