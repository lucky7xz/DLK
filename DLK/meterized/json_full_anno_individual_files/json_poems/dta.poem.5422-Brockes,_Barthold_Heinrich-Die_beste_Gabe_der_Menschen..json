{"dta.poem.5422": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die beste Gabe der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt! was soll ich dir doch geben", "tokens": ["Mein", "Gott", "!", "was", "soll", "ich", "dir", "doch", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alles, was, in meinem Leben,", "tokens": ["F\u00fcr", "al\u00b7les", ",", "was", ",", "in", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr als man sinnet, wei\u00df und denckt,", "tokens": ["Mehr", "als", "man", "sin\u00b7net", ",", "wei\u00df", "und", "denckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir deine Vater Huld geschenckt?", "tokens": ["Mir", "dei\u00b7ne", "Va\u00b7ter", "Huld", "ge\u00b7schenckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ich mich selbst und alles meine", "tokens": ["Wenn", "ich", "mich", "selbst", "und", "al\u00b7les", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KON", "PIS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir, HErr, zur Gabe reichen wollte,", "tokens": ["Dir", ",", "Herr", ",", "zur", "Ga\u00b7be", "rei\u00b7chen", "woll\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und alles dir zum Opffer zollte;", "tokens": ["Und", "al\u00b7les", "dir", "zum", "Opf\u00b7fer", "zoll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So ist es doch schon alles deine.", "tokens": ["So", "ist", "es", "doch", "schon", "al\u00b7les", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PIS", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es leidet deine Gr\u00f6sse nicht,", "tokens": ["Es", "lei\u00b7det", "dei\u00b7ne", "Gr\u00f6s\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die unerm\u00e4\u00dflich, zu gedencken,", "tokens": ["Die", "un\u00b7er\u00b7m\u00e4\u00df\u00b7lich", ",", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ob k\u00f6nne man dir etwas schencken;", "tokens": ["Ob", "k\u00f6n\u00b7ne", "man", "dir", "et\u00b7was", "schen\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PIS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da dir von allem nichts gebricht.", "tokens": ["Da", "dir", "von", "al\u00b7lem", "nichts", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Was aller Himmel Himmel fassen,", "tokens": ["Was", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und alle Welt, geh\u00f6rt ja dir;", "tokens": ["Und", "al\u00b7le", "Welt", ",", "ge\u00b7h\u00f6rt", "ja", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und dennoch scheints als w\u00e4re mir", "tokens": ["Und", "den\u00b7noch", "scheints", "als", "w\u00e4\u00b7re", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "KOKOM", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Noch etwas zum Geschenck gelassen.", "tokens": ["Noch", "et\u00b7was", "zum", "Ge\u00b7schenck", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ich opfre dir, f\u00fcr deine G\u00fcte,", "tokens": ["Ich", "opf\u00b7re", "dir", ",", "f\u00fcr", "dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein, von der Creaturen Zier", "tokens": ["Ein", ",", "von", "der", "Crea\u00b7tu\u00b7ren", "Zier"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Erf\u00fclltes, fr\u00f6liches Gem\u00fchte,", "tokens": ["Er\u00b7f\u00fcll\u00b7tes", ",", "fr\u00f6\u00b7li\u00b7ches", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Woraus die heisse Danck-Begier,", "tokens": ["Wo\u00b7raus", "die", "heis\u00b7se", "Dan\u00b7ck\u00b7Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "F\u00fcr alle Gnade, die uns hier", "tokens": ["F\u00fcr", "al\u00b7le", "Gna\u00b7de", ",", "die", "uns", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dein Gnaden-Will empfinden l\u00e4\u00dft,", "tokens": ["Dein", "Gna\u00b7den\u00b7Will", "emp\u00b7fin\u00b7den", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Oft einen frohen Seufzer pre\u00dft.", "tokens": ["Oft", "ei\u00b7nen", "fro\u00b7hen", "Seuf\u00b7zer", "pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}