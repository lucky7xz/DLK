{"textgrid.poem.43989": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebe, mindre doch die Plagen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe, mindre doch die Plagen,", "tokens": ["Lie\u00b7be", ",", "mind\u00b7re", "doch", "die", "Pla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich kan sie kaum mehr tragen,", "tokens": ["Denn", "ich", "kan", "sie", "kaum", "mehr", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kr\u00e4fte treuer Brust", "tokens": ["Und", "die", "Kr\u00e4f\u00b7te", "treu\u00b7er", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwinden unter Schmerz und Lust;", "tokens": ["Schwin\u00b7den", "un\u00b7ter", "Schmerz", "und", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder binde mir so lange", "tokens": ["O\u00b7der", "bin\u00b7de", "mir", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den Schlummer Geist und Sinn,", "tokens": ["Durch", "den", "Schlum\u00b7mer", "Geist", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ich meinen Schaz umfange,", "tokens": ["Bis", "ich", "mei\u00b7nen", "Schaz", "um\u00b7fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem ich l\u00e4ngst versehen bin.", "tokens": ["Dem", "ich", "l\u00e4ngst", "ver\u00b7se\u00b7hen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jezo lern ich erst empfinden,", "tokens": ["Je\u00b7zo", "lern", "ich", "erst", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was dein heimliches Entz\u00fcnden", "tokens": ["Was", "dein", "heim\u00b7li\u00b7ches", "Ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey so schwerer Sclaverey", "tokens": ["Bey", "so", "schwe\u00b7rer", "Scla\u00b7ve\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor ein grausam Leiden sey.", "tokens": ["Vor", "ein", "grau\u00b7sam", "Lei\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vormahls dacht ich auch im Herzen,", "tokens": ["Vor\u00b7mahls", "dacht", "ich", "auch", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich erkennte deine Macht,", "tokens": ["Ich", "er\u00b7kenn\u00b7te", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber dies' und jene Schmerzen", "tokens": ["A\u00b7ber", "dies'", "und", "je\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "KON", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind vorwahr wie Tag und Nacht.", "tokens": ["Sind", "vor\u00b7wahr", "wie", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Filindrene war mir g\u00fcnstig,", "tokens": ["Fi\u00b7lind\u00b7re\u00b7ne", "war", "mir", "g\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Leonore gut und br\u00fcnstig,", "tokens": ["Le\u00b7o\u00b7no\u00b7re", "gut", "und", "br\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von beiden lidt ich viel,", "tokens": ["Und", "von", "bei\u00b7den", "lidt", "ich", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jezo nenn ich's Kinderspiel.", "tokens": ["Je\u00b7zo", "nenn", "ich's", "Kin\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Filindrenens fr\u00fche Leiche", "tokens": ["Fi\u00b7lind\u00b7re\u00b7nens", "fr\u00fc\u00b7he", "Lei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Lockte mir bey Sarg und Grab", "tokens": ["Lock\u00b7te", "mir", "bey", "Sarg", "und", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der andern falsche Streiche", "tokens": ["Wie", "der", "an\u00b7dern", "fal\u00b7sche", "Strei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Manchen Fluch und Thr\u00e4nen ab.", "tokens": ["Man\u00b7chen", "Fluch", "und", "Thr\u00e4\u00b7nen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Phillis l\u00e4st mich kaum drey Morgen", "tokens": ["Phil\u00b7lis", "l\u00e4st", "mich", "kaum", "drey", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Hofnung, Furcht und Sorgen,", "tokens": ["Zwi\u00b7schen", "Hof\u00b7nung", ",", "Furcht", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich schleiche durch den Thau,", "tokens": ["Und", "ich", "schlei\u00b7che", "durch", "den", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon vor Unmuth bleich und grau,", "tokens": ["Schon", "vor", "Un\u00b7muth", "bleich", "und", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Garthen, Wald, Camin und Linde,", "tokens": ["Gar\u00b7then", ",", "Wald", ",", "Ca\u00b7min", "und", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles macht mich noch betr\u00fcbt,", "tokens": ["Al\u00b7les", "macht", "mich", "noch", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was mir von dem lieben Kinde", "tokens": ["Was", "mir", "von", "dem", "lie\u00b7ben", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein Erinnrungszeichen giebt.", "tokens": ["Ein", "E\u00b7rinn\u00b7rungs\u00b7zei\u00b7chen", "giebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ist mir doch die Welt zu enge,", "tokens": ["Ist", "mir", "doch", "die", "Welt", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht mir doch das Feld gedr\u00e4nge,", "tokens": ["Macht", "mir", "doch", "das", "Feld", "ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein m\u00fcrrisch Angesicht", "tokens": ["Und", "mein", "m\u00fcr\u00b7risch", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lacht dem besten Freunde nicht.", "tokens": ["Lacht", "dem", "bes\u00b7ten", "Freun\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser S\u00fcdwind hat die St\u00e4rcke", "tokens": ["Un\u00b7ser", "S\u00fcd\u00b7wind", "hat", "die", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den Seufzern meiner Angst,", "tokens": ["Von", "den", "Seuf\u00b7zern", "mei\u00b7ner", "Angst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die du, Phillis, wie ich mercke,", "tokens": ["Die", "du", ",", "Phil\u00b7lis", ",", "wie", "ich", "mer\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch mit Flei\u00df von mir verlangst.", "tokens": ["Noch", "mit", "Flei\u00df", "von", "mir", "ver\u00b7langst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Phillis, Phillis, komm doch wieder,", "tokens": ["Phil\u00b7lis", ",", "Phil\u00b7lis", ",", "komm", "doch", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst verlieren Geist und Lieder", "tokens": ["Sonst", "ver\u00b7lie\u00b7ren", "Geist", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Verm\u00f6gen und die Kraft,", "tokens": ["Das", "Ver\u00b7m\u00f6\u00b7gen", "und", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dir viel Erg\u00f6zung schaft.", "tokens": ["Die", "dir", "viel", "Er\u00b7g\u00f6\u00b7zung", "schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Licht und Schatten macht die Farben", "tokens": ["Licht", "und", "Schat\u00b7ten", "macht", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dein Blick mein Wohlergehn;", "tokens": ["Und", "dein", "Blick", "mein", "Woh\u00b7ler\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df ich de\u00dfen Einflu\u00df darben,", "tokens": ["Mu\u00df", "ich", "de\u00b7\u00dfen", "Ein\u00b7flu\u00df", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kan ich nimmermehr bestehn.", "tokens": ["Kan", "ich", "nim\u00b7mer\u00b7mehr", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Meine Kunst ist hier nichts n\u00fcze;", "tokens": ["Mei\u00b7ne", "Kunst", "ist", "hier", "nichts", "n\u00fc\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ich bey dem Fieber schwize", "tokens": ["Ob", "ich", "bey", "dem", "Fie\u00b7ber", "schwi\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder mich des Raths verzeih,", "tokens": ["O\u00b7der", "mich", "des", "Raths", "ver\u00b7zeih", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Beides ist mir einerley.", "tokens": ["Bei\u00b7des", "ist", "mir", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollt ich dich nur sehn und r\u00fchren,", "tokens": ["Sollt", "ich", "dich", "nur", "sehn", "und", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erw\u00e4rmte mich dein Mund,", "tokens": ["Und", "er\u00b7w\u00e4rm\u00b7te", "mich", "dein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrd ich ohne Zeitverlieren", "tokens": ["W\u00fcrd", "ich", "oh\u00b7ne", "Zeit\u00b7ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den ersten Ku\u00df gesund.", "tokens": ["Auf", "den", "ers\u00b7ten", "Ku\u00df", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebe, mindre doch die Plagen,", "tokens": ["Lie\u00b7be", ",", "mind\u00b7re", "doch", "die", "Pla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich kan sie kaum mehr tragen,", "tokens": ["Denn", "ich", "kan", "sie", "kaum", "mehr", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kr\u00e4fte treuer Brust", "tokens": ["Und", "die", "Kr\u00e4f\u00b7te", "treu\u00b7er", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwinden unter Schmerz und Lust;", "tokens": ["Schwin\u00b7den", "un\u00b7ter", "Schmerz", "und", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder binde mir so lange", "tokens": ["O\u00b7der", "bin\u00b7de", "mir", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den Schlummer Geist und Sinn,", "tokens": ["Durch", "den", "Schlum\u00b7mer", "Geist", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ich meinen Schaz umfange,", "tokens": ["Bis", "ich", "mei\u00b7nen", "Schaz", "um\u00b7fan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem ich l\u00e4ngst versehen bin.", "tokens": ["Dem", "ich", "l\u00e4ngst", "ver\u00b7se\u00b7hen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jezo lern ich erst empfinden,", "tokens": ["Je\u00b7zo", "lern", "ich", "erst", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was dein heimliches Entz\u00fcnden", "tokens": ["Was", "dein", "heim\u00b7li\u00b7ches", "Ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey so schwerer Sclaverey", "tokens": ["Bey", "so", "schwe\u00b7rer", "Scla\u00b7ve\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor ein grausam Leiden sey.", "tokens": ["Vor", "ein", "grau\u00b7sam", "Lei\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vormahls dacht ich auch im Herzen,", "tokens": ["Vor\u00b7mahls", "dacht", "ich", "auch", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich erkennte deine Macht,", "tokens": ["Ich", "er\u00b7kenn\u00b7te", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber dies' und jene Schmerzen", "tokens": ["A\u00b7ber", "dies'", "und", "je\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "KON", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind vorwahr wie Tag und Nacht.", "tokens": ["Sind", "vor\u00b7wahr", "wie", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Filindrene war mir g\u00fcnstig,", "tokens": ["Fi\u00b7lind\u00b7re\u00b7ne", "war", "mir", "g\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Leonore gut und br\u00fcnstig,", "tokens": ["Le\u00b7o\u00b7no\u00b7re", "gut", "und", "br\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von beiden lidt ich viel,", "tokens": ["Und", "von", "bei\u00b7den", "lidt", "ich", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jezo nenn ich's Kinderspiel.", "tokens": ["Je\u00b7zo", "nenn", "ich's", "Kin\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Filindrenens fr\u00fche Leiche", "tokens": ["Fi\u00b7lind\u00b7re\u00b7nens", "fr\u00fc\u00b7he", "Lei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Lockte mir bey Sarg und Grab", "tokens": ["Lock\u00b7te", "mir", "bey", "Sarg", "und", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der andern falsche Streiche", "tokens": ["Wie", "der", "an\u00b7dern", "fal\u00b7sche", "Strei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Manchen Fluch und Thr\u00e4nen ab.", "tokens": ["Man\u00b7chen", "Fluch", "und", "Thr\u00e4\u00b7nen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Phillis l\u00e4st mich kaum drey Morgen", "tokens": ["Phil\u00b7lis", "l\u00e4st", "mich", "kaum", "drey", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Hofnung, Furcht und Sorgen,", "tokens": ["Zwi\u00b7schen", "Hof\u00b7nung", ",", "Furcht", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich schleiche durch den Thau,", "tokens": ["Und", "ich", "schlei\u00b7che", "durch", "den", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon vor Unmuth bleich und grau,", "tokens": ["Schon", "vor", "Un\u00b7muth", "bleich", "und", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Garthen, Wald, Camin und Linde,", "tokens": ["Gar\u00b7then", ",", "Wald", ",", "Ca\u00b7min", "und", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles macht mich noch betr\u00fcbt,", "tokens": ["Al\u00b7les", "macht", "mich", "noch", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was mir von dem lieben Kinde", "tokens": ["Was", "mir", "von", "dem", "lie\u00b7ben", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein Erinnrungszeichen giebt.", "tokens": ["Ein", "E\u00b7rinn\u00b7rungs\u00b7zei\u00b7chen", "giebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ist mir doch die Welt zu enge,", "tokens": ["Ist", "mir", "doch", "die", "Welt", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht mir doch das Feld gedr\u00e4nge,", "tokens": ["Macht", "mir", "doch", "das", "Feld", "ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein m\u00fcrrisch Angesicht", "tokens": ["Und", "mein", "m\u00fcr\u00b7risch", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lacht dem besten Freunde nicht.", "tokens": ["Lacht", "dem", "bes\u00b7ten", "Freun\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser S\u00fcdwind hat die St\u00e4rcke", "tokens": ["Un\u00b7ser", "S\u00fcd\u00b7wind", "hat", "die", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den Seufzern meiner Angst,", "tokens": ["Von", "den", "Seuf\u00b7zern", "mei\u00b7ner", "Angst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die du, Phillis, wie ich mercke,", "tokens": ["Die", "du", ",", "Phil\u00b7lis", ",", "wie", "ich", "mer\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch mit Flei\u00df von mir verlangst.", "tokens": ["Noch", "mit", "Flei\u00df", "von", "mir", "ver\u00b7langst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Phillis, Phillis, komm doch wieder,", "tokens": ["Phil\u00b7lis", ",", "Phil\u00b7lis", ",", "komm", "doch", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonst verlieren Geist und Lieder", "tokens": ["Sonst", "ver\u00b7lie\u00b7ren", "Geist", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Verm\u00f6gen und die Kraft,", "tokens": ["Das", "Ver\u00b7m\u00f6\u00b7gen", "und", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dir viel Erg\u00f6zung schaft.", "tokens": ["Die", "dir", "viel", "Er\u00b7g\u00f6\u00b7zung", "schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Licht und Schatten macht die Farben", "tokens": ["Licht", "und", "Schat\u00b7ten", "macht", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dein Blick mein Wohlergehn;", "tokens": ["Und", "dein", "Blick", "mein", "Woh\u00b7ler\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df ich de\u00dfen Einflu\u00df darben,", "tokens": ["Mu\u00df", "ich", "de\u00b7\u00dfen", "Ein\u00b7flu\u00df", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kan ich nimmermehr bestehn.", "tokens": ["Kan", "ich", "nim\u00b7mer\u00b7mehr", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Meine Kunst ist hier nichts n\u00fcze;", "tokens": ["Mei\u00b7ne", "Kunst", "ist", "hier", "nichts", "n\u00fc\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ich bey dem Fieber schwize", "tokens": ["Ob", "ich", "bey", "dem", "Fie\u00b7ber", "schwi\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder mich des Raths verzeih,", "tokens": ["O\u00b7der", "mich", "des", "Raths", "ver\u00b7zeih", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Beides ist mir einerley.", "tokens": ["Bei\u00b7des", "ist", "mir", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollt ich dich nur sehn und r\u00fchren,", "tokens": ["Sollt", "ich", "dich", "nur", "sehn", "und", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erw\u00e4rmte mich dein Mund,", "tokens": ["Und", "er\u00b7w\u00e4rm\u00b7te", "mich", "dein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrd ich ohne Zeitverlieren", "tokens": ["W\u00fcrd", "ich", "oh\u00b7ne", "Zeit\u00b7ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den ersten Ku\u00df gesund.", "tokens": ["Auf", "den", "ers\u00b7ten", "Ku\u00df", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}