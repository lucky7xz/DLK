{"textgrid.poem.24624": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Tugend", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe liebt darum nicht wieder die Natur/", "tokens": ["Die", "Lie\u00b7be", "liebt", "da\u00b7rum", "nicht", "wie\u00b7der", "die", "Na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKNEG", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie der Tugend hold; die bl\u00f6den Sinnen nur.", "tokens": ["Wenn", "sie", "der", "Tu\u00b7gend", "hold", ";", "die", "bl\u00f6\u00b7den", "Sin\u00b7nen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$.", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat leichter Sch\u00f6nheit Glantz auf eine Zeit gewonnen.", "tokens": ["Hat", "leich\u00b7ter", "Sch\u00f6n\u00b7heit", "Glantz", "auf", "ei\u00b7ne", "Zeit", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Geist hat Adlers Krafft/ und sieht in be\u00dfre Sonnen.", "tokens": ["Der", "Geist", "hat", "Ad\u00b7lers", "Krafft", "/", "und", "sieht", "in", "be\u00df\u00b7re", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN",