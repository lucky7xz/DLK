{"dta.poem.14143": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Klage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott, wem soll ich trauen?", "tokens": ["Ach", "Gott", ",", "wem", "soll", "ich", "trau\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will Sie mich nicht versteh'n,", "tokens": ["Will", "Sie", "mich", "nicht", "ver\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Thun all' so fremde schauen,", "tokens": ["Thun", "all'", "so", "frem\u00b7de", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles mu\u00df vergeh'n.", "tokens": ["Und", "al\u00b7les", "mu\u00df", "ver\u00b7geh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PIS", "VMFIN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}