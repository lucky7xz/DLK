{"textgrid.poem.65280": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Abrede", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meiner Liebsten Fenster", "tokens": ["Vor", "mei\u00b7ner", "Liebs\u00b7ten", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da klingen meine Sporn:", "tokens": ["Da", "klin\u00b7gen", "mei\u00b7ne", "Sporn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Thu' auf, Herzallerliebste,", "tokens": ["Thu'", "auf", ",", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df schwinden deinen Zorn!", "tokens": ["La\u00df", "schwin\u00b7den", "dei\u00b7nen", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fiedel ruft zum Tanze,", "tokens": ["Die", "Fie\u00b7del", "ruft", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Meine T\u00e4nz'rin sollst du sein:", "tokens": ["Mei\u00b7ne", "T\u00e4nz'\u00b7rin", "sollst", "du", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich kann nicht von dir lassen,", "tokens": ["Ich", "kann", "nicht", "von", "dir", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es f\u00e4llt mir gar nicht ein.", "tokens": ["Es", "f\u00e4llt", "mir", "gar", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbmein Zorn der ist verschwunden,", "tokens": ["\u00bb", "mein", "Zorn", "der", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Tanzkleid ist bereit,", "tokens": ["Mein", "Tanz\u00b7kleid", "ist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenn's ein Nachbar s\u00e4he,", "tokens": ["Doch", "wenn's", "ein", "Nach\u00b7bar", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es br\u00e4cht' mir Schmach und Leid.\u00ab", "tokens": ["Es", "br\u00e4cht'", "mir", "Schmach", "und", "Leid", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So geh' voraus zur Schenke", "tokens": ["So", "geh'", "vo\u00b7raus", "zur", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und steh' nicht vorn am Thor,", "tokens": ["Und", "steh'", "nicht", "vorn", "am", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tritt in den tiefsten Winkel,", "tokens": ["Tritt", "in", "den", "tiefs\u00b7ten", "Win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewi\u00df, ich hol' dich vor.", "tokens": ["Ge\u00b7wi\u00df", ",", "ich", "hol'", "dich", "vor", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und schwenk' ich dich im Tanze,", "tokens": ["Und", "schwenk'", "ich", "dich", "im", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So zieh' mir ein Gesicht.", "tokens": ["So", "zieh'", "mir", "ein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann denken alle Leute:", "tokens": ["Dann", "den\u00b7ken", "al\u00b7le", "Leu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die tanzte lieber nicht!", "tokens": ["Die", "tanz\u00b7te", "lie\u00b7ber", "nicht", "!"], "token_info": ["word", "