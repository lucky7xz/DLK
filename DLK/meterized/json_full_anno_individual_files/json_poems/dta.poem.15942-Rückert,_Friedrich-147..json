{"dta.poem.15942": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "147.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du in Gem\u00e4chlichkeit ges\u00e4ttigt und bekleidet,", "tokens": ["Du", "in", "Ge\u00b7m\u00e4ch\u00b7lich\u00b7keit", "ge\u00b7s\u00e4t\u00b7tigt", "und", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denkst du des Bruders auch, der friert und Hunger leidet?", "tokens": ["Denkst", "du", "des", "Bru\u00b7ders", "auch", ",", "der", "friert", "und", "Hun\u00b7ger", "lei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An ihn zu denken nur, verst\u00f6rt dich im Genu\u00df,", "tokens": ["An", "ihn", "zu", "den\u00b7ken", "nur", ",", "ver\u00b7st\u00f6rt", "dich", "im", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "ADV", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis du dem D\u00fcrft'gen gibst von deinem Ueberflu\u00df.", "tokens": ["Bis", "du", "dem", "D\u00fcrft'\u00b7gen", "gibst", "von", "dei\u00b7nem", "Ue\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "V