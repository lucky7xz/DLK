{"textgrid.poem.49370": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Exhibitionist stellt sich gespreizt am Vorhang auf", "tokens": ["Der", "Ex\u00b7hi\u00b7bi\u00b7ti\u00b7o\u00b7nist", "stellt", "sich", "ge\u00b7spreizt", "am", "Vor\u00b7hang", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVPP", "APPRART", "NN", "APPR"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "und Pimpronella reizt ihn mit den roten Unterr\u00f6cken.", "tokens": ["und", "Pim\u00b7pro\u00b7nel\u00b7la", "reizt", "ihn", "mit", "den", "ro\u00b7ten", "Un\u00b7ter\u00b7r\u00f6\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Koko der gr\u00fcne Gott klatscht laut im Publikum.", "tokens": ["Ko\u00b7ko", "der", "gr\u00fc\u00b7ne", "Gott", "klatscht", "laut", "im", "Pub\u00b7li\u00b7kum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da werden geil die \u00e4ltesten S\u00fcndenb\u00f6cke.", "tokens": ["Da", "wer\u00b7den", "geil", "die", "\u00e4l\u00b7tes\u00b7ten", "S\u00fcn\u00b7den\u00b7b\u00f6\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Tsingtara! Da 