{"textgrid.poem.67170": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Reif sind, in Feuer getaucht, gekochet", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reif sind, in Feuer getaucht, gekochet", "tokens": ["Reif", "sind", ",", "in", "Feu\u00b7er", "ge\u00b7taucht", ",", "ge\u00b7ko\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "$,", "APPR", "NN", "VVPP", "$,", "VVPP"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Fr\u00fccht und auf der Erde gepr\u00fcfet und ein Gesetz ist,", "tokens": ["Die", "Fr\u00fccht", "und", "auf", "der", "Er\u00b7de", "ge\u00b7pr\u00fc\u00b7fet", "und", "ein", "Ge\u00b7setz", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "APPR", "ART", "NN", "VVPP", "KON", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df alles hineingeht, Schlangen gleich,", "tokens": ["Da\u00df", "al\u00b7les", "hin\u00b7ein\u00b7geht", ",", "Schlan\u00b7gen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Prophetisch, tr\u00e4umend auf", "tokens": ["Pro\u00b7phe\u00b7tisch", ",", "tr\u00e4u\u00b7mend", "auf"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den H\u00fcgeln des Himmels. Und vieles", "tokens": ["Den", "H\u00fc\u00b7geln", "des", "Him\u00b7mels", ".", "Und", "vie\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "KON", "PIS"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wie auf den Schultern eine", "tokens": ["Wie", "auf", "den", "Schul\u00b7tern", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Last von Scheitern ist", "tokens": ["Last", "von", "Schei\u00b7tern", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Zu behalten. Aber b\u00f6s sind", "tokens": ["Zu", "be\u00b7hal\u00b7ten", ".", "A\u00b7ber", "b\u00f6s", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "KON", "ADJD", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die Pfade. N\u00e4mlich unrecht,", "tokens": ["Die", "Pfa\u00b7de", ".", "N\u00e4m\u00b7lich", "un\u00b7recht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie Rosse, gehn die gefangenen", "tokens": ["Wie", "Ros\u00b7se", ",", "gehn", "die", "ge\u00b7fan\u00b7ge\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Element und alten", "tokens": ["E\u00b7le\u00b7ment", "und", "al\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Gesetze der Erd. Und immer", "tokens": ["Ge\u00b7set\u00b7ze", "der", "Erd", ".", "Und", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "KON", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Ins Ungebundene gehet eine Sehnsucht. Vieles aber ist", "tokens": ["Ins", "Un\u00b7ge\u00b7bun\u00b7de\u00b7ne", "ge\u00b7het", "ei\u00b7ne", "Sehn\u00b7sucht", ".", "Vie\u00b7les", "a\u00b7ber", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "PIS", "ADV", "VAFIN"], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.14": {"text": "Zu behalten. Und not die Treue.", "tokens": ["Zu", "be\u00b7hal\u00b7ten", ".", "Und", "not", "die", "Treu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "KON", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Vorw\u00e4rts aber und r\u00fcckw\u00e4rts wollen wir", "tokens": ["Vor\u00b7w\u00e4rts", "a\u00b7ber", "und", "r\u00fcck\u00b7w\u00e4rts", "wol\u00b7len", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV", "VMFIN", "PPER"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Nicht sehn. Uns wiegen lassen, wie", "tokens": ["Nicht", "sehn", ".", "Uns", "wie\u00b7gen", "las\u00b7sen", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "VVINF", "$.", "PPER", "VVINF", "VVINF", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Auf schwankem Kahne der See.", "tokens": ["Auf", "schwan\u00b7kem", "Kah\u00b7ne", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wie aber Liebes? Sonnenschein", "tokens": ["Wie", "a\u00b7ber", "Lie\u00b7bes", "?", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Boden sehen wir und trockenen Staub", "tokens": ["Am", "Bo\u00b7den", "se\u00b7hen", "wir", "und", "tro\u00b7cke\u00b7nen", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und heimatlich die Schatten der W\u00e4lder und es bl\u00fchet", "tokens": ["Und", "hei\u00b7mat\u00b7lich", "die", "Schat\u00b7ten", "der", "W\u00e4l\u00b7der", "und", "es", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "An D\u00e4chern der Rauch, bei alter Krone", "tokens": ["An", "D\u00e4\u00b7chern", "der", "Rauch", ",", "bei", "al\u00b7ter", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der T\u00fcrme, friedsam; gut sind n\u00e4mlich", "tokens": ["Der", "T\u00fcr\u00b7me", ",", "fried\u00b7sam", ";", "gut", "sind", "n\u00e4m\u00b7lich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$.", "ADJD", "VAFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat gegenredend die Seele", "tokens": ["Hat", "ge\u00b7gen\u00b7re\u00b7dend", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ein Himmlisches verwundet, die Tageszeichen.", "tokens": ["Ein", "Himm\u00b7li\u00b7sches", "ver\u00b7wun\u00b7det", ",", "die", "Ta\u00b7ges\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Denn Schnee, wie Maienblumen", "tokens": ["Denn", "Schnee", ",", "wie", "Mai\u00b7en\u00b7blu\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das Edelm\u00fctige, wo", "tokens": ["Das", "E\u00b7del\u00b7m\u00fc\u00b7ti\u00b7ge", ",", "wo"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PWAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Es seie, bedeutend, gl\u00e4nzet auf", "tokens": ["Es", "sei\u00b7e", ",", "be\u00b7deu\u00b7tend", ",", "gl\u00e4n\u00b7zet", "auf"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "$,", "VVFIN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Der gr\u00fcnen Wiese", "tokens": ["Der", "gr\u00fc\u00b7nen", "Wie\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Der Alpen, h\u00e4lftig, da, vom Kreuze redend, das", "tokens": ["Der", "Al\u00b7pen", ",", "h\u00e4lf\u00b7tig", ",", "da", ",", "vom", "Kreu\u00b7ze", "re\u00b7dend", ",", "das"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "KOUS", "$,", "APPRART", "NN", "VVPP", "$,", "PRELS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gesetzt ist unterwegs einmal", "tokens": ["Ge\u00b7setzt", "ist", "un\u00b7ter\u00b7wegs", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gestorbenen, auf hoher Stra\u00df", "tokens": ["Ge\u00b7stor\u00b7be\u00b7nen", ",", "auf", "ho\u00b7her", "Stra\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Ein Wandersmann geht zornig,", "tokens": ["Ein", "Wan\u00b7ders\u00b7mann", "geht", "zor\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Fern ahnend mit", "tokens": ["Fern", "ah\u00b7nend", "mit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Dem andern, aber was ist dies?", "tokens": ["Dem", "an\u00b7dern", ",", "a\u00b7ber", "was", "ist", "dies", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PWS", "VAFIN", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Feigenbaum ist mein", "tokens": ["Am", "Fei\u00b7gen\u00b7baum", "ist", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Achilles mir gestorben,", "tokens": ["A\u00b7chil\u00b7les", "mir", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Ajax liegt", "tokens": ["Und", "A\u00b7jax", "liegt"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "An den Grotten der See,", "tokens": ["An", "den", "Grot\u00b7ten", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "An B\u00e4chen, benachbart dem Skamandros.", "tokens": ["An", "B\u00e4\u00b7chen", ",", "be\u00b7nach\u00b7bart", "dem", "Ska\u00b7man\u00b7dros", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An Schl\u00e4fen Sausen einst, nach", "tokens": ["An", "Schl\u00e4\u00b7fen", "Sau\u00b7sen", "einst", ",", "nach"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "NN", "ADV", "$,", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Der unbewegten Salamis steter", "tokens": ["Der", "un\u00b7be\u00b7weg\u00b7ten", "Sa\u00b7la\u00b7mis", "ste\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gewohnheit, in der Fremd, ist gro\u00df", "tokens": ["Ge\u00b7wohn\u00b7heit", ",", "in", "der", "Fremd", ",", "ist", "gro\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ajax gestorben,", "tokens": ["A\u00b7jax", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.10": {"text": "Patroklos aber in des K\u00f6niges Harnisch. Und es starben", "tokens": ["Pat\u00b7ro\u00b7klos", "a\u00b7ber", "in", "des", "K\u00f6\u00b7ni\u00b7ges", "Har\u00b7nisch", ".", "Und", "es", "star\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "NE", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Noch andere viel. Am Kith\u00e4ron aber lag", "tokens": ["Noch", "an\u00b7de\u00b7re", "viel", ".", "Am", "Kit\u00b7h\u00e4\u00b7ron", "a\u00b7ber", "lag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADV", "$.", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Elevther\u00e4, der Mnemosyne Stadt. Der auch, als", "tokens": ["E\u00b7levt\u00b7he\u00b7r\u00e4", ",", "der", "Mne\u00b7mo\u00b7sy\u00b7ne", "Stadt", ".", "Der", "auch", ",", "als"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$.", "ART", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.13": {"text": "Ablegte den Mantel Gott, das Abendliche nachher l\u00f6ste", "tokens": ["Ab\u00b7leg\u00b7te", "den", "Man\u00b7tel", "Gott", ",", "das", "A\u00b7bend\u00b7li\u00b7che", "nach\u00b7her", "l\u00f6s\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "ADV", "VVFIN"], "meter": "++--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.14": {"text": "Die Locken. Himmlische n\u00e4mlich sind", "tokens": ["Die", "Lo\u00b7cken", ".", "Himm\u00b7li\u00b7sche", "n\u00e4m\u00b7lich", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "ADV", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Unwillig, wenn einer nicht die Seele schonend sich", "tokens": ["Un\u00b7wil\u00b7lig", ",", "wenn", "ei\u00b7ner", "nicht", "die", "See\u00b7le", "scho\u00b7nend", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PIS", "PTKNEG", "ART", "NN", "ADJD", "PRF"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "Zusammengenommen, aber er mu\u00df doch; dem", "tokens": ["Zu\u00b7sam\u00b7men\u00b7ge\u00b7nom\u00b7men", ",", "a\u00b7ber", "er", "mu\u00df", "doch", ";", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "KON", "PPER", "VMFIN", "ADV", "$.", "ART"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Gleich fehlet die Trauer.", "tokens": ["Gleich", "feh\u00b7let", "die", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Reif sind, in Feuer getaucht, gekochet", "tokens": ["Reif", "sind", ",", "in", "Feu\u00b7er", "ge\u00b7taucht", ",", "ge\u00b7ko\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "$,", "APPR", "NN", "VVPP", "$,", "VVPP"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Fr\u00fccht und auf der Erde gepr\u00fcfet und ein Gesetz ist,", "tokens": ["Die", "Fr\u00fccht", "und", "auf", "der", "Er\u00b7de", "ge\u00b7pr\u00fc\u00b7fet", "und", "ein", "Ge\u00b7setz", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "APPR", "ART", "NN", "VVPP", "KON", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df alles hineingeht, Schlangen gleich,", "tokens": ["Da\u00df", "al\u00b7les", "hin\u00b7ein\u00b7geht", ",", "Schlan\u00b7gen", "gleich", ","], "token_info": 