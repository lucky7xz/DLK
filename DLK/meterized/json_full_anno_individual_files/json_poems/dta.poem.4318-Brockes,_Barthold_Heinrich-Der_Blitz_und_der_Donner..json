{"dta.poem.4318": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Blitz und der Donner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn aber der entflammte Stoff sich in so grosser", "tokens": ["Wenn", "a\u00b7ber", "der", "ent\u00b7flamm\u00b7te", "Stoff", "sich", "in", "so", "gros\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PRF", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Menge findet,", "tokens": ["Men\u00b7ge", "fin\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df er sich, wie ein Strohm von Feuer, in einem Au-", "tokens": ["Da\u00df", "er", "sich", ",", "wie", "ein", "Strohm", "von", "Feu\u00b7er", ",", "in", "ei\u00b7nem", "Au"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "genblick entz\u00fcndet,", "tokens": ["gen\u00b7blick", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und gleichsam sich herunter st\u00fcrzt; ist es der Blitz, de\u00df", "tokens": ["Und", "gleich\u00b7sam", "sich", "her\u00b7un\u00b7ter", "st\u00fcrzt", ";", "ist", "es", "der", "Blitz", ",", "de\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "PRF", "APZR", "VVFIN", "$.", "VAFIN", "PPER", "ART", "NN", "$,", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wilde Kraft", "tokens": ["wil\u00b7de", "Kraft"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Entsetzliche Gewalt ver\u00fcbt, nachdem in ihm ein strenger", "tokens": ["Ent\u00b7setz\u00b7li\u00b7che", "Ge\u00b7walt", "ver\u00b7\u00fcbt", ",", "nach\u00b7dem", "in", "ihm", "ein", "stren\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA"], "meter": "-+---+-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Wind,", "tokens": ["Wind", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Die Heft- und Giftigkeit des Stoffs, draus er besteht, ver-", "tokens": ["Die", "Heft", "und", "Gif\u00b7tig\u00b7keit", "des", "Stoffs", ",", "draus", "er", "be\u00b7steht", ",", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "bunden sind.", "tokens": ["bun\u00b7den", "sind", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Die Luft aus den zerri\u00dfnen Bl\u00e4sgen, und die, so zwischen", "tokens": ["Die", "Luft", "aus", "den", "zer\u00b7ri\u00df\u00b7nen", "Bl\u00e4s\u00b7gen", ",", "und", "die", ",", "so", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ART", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wolken steckt,", "tokens": ["Wol\u00b7ken", "steckt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Mu\u00df, durch die greuliche Gewalt, die der entflammte", "tokens": ["Mu\u00df", ",", "durch", "die", "greu\u00b7li\u00b7che", "Ge\u00b7walt", ",", "die", "der", "ent\u00b7flamm\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Stoff erweckt,", "tokens": ["Stoff", "er\u00b7weckt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Entsetzlich ausgespannet werden. Wenn diese nun ver-", "tokens": ["Ent\u00b7setz\u00b7lich", "aus\u00b7ge\u00b7span\u00b7net", "wer\u00b7den", ".", "Wenn", "die\u00b7se", "nun", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VAINF", "$.", "KOUS", "PDS", "ADV", "TRUNC"], "meter": "-+-+-+-+----+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "schr\u00e4nket sind", "tokens": ["schr\u00e4n\u00b7ket", "sind"], "token_info": ["word", "word"], "pos": ["VVFIN", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Durch dick-gepre\u00dfte Wolken-Klumpen, die oft ein wider-", "tokens": ["Durch", "dick\u00b7ge\u00b7pre\u00df\u00b7te", "Wol\u00b7ken\u00b7Klum\u00b7pen", ",", "die", "oft", "ein", "wi\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "w\u00e4rt'ger Wind", "tokens": ["w\u00e4rt'\u00b7ger", "Wind"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Sehr heftig vor sich st\u00f6\u00dft und pre\u00dft, auch \u00f6fters auf ein-", "tokens": ["Sehr", "hef\u00b7tig", "vor", "sich", "st\u00f6\u00dft", "und", "pre\u00dft", ",", "auch", "\u00f6f\u00b7ters", "auf", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PRF", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.20": {"text": "ander fallen;", "tokens": ["an\u00b7der", "fal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "So kanns unm\u00f6glich anders seyn, als da\u00df mit solchem", "tokens": ["So", "kanns", "un\u00b7m\u00f6g\u00b7lich", "an\u00b7ders", "seyn", ",", "als", "da\u00df", "mit", "sol\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "ADV", "VAINF", "$,", "KOKOM", "KOUS", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "starken Knallen,", "tokens": ["star\u00b7ken", "Knal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Und einem krachenden Get\u00fcmmel, die Theilchen aus ein-", "tokens": ["Und", "ei\u00b7nem", "kra\u00b7chen\u00b7den", "Ge\u00b7t\u00fcm\u00b7mel", ",", "die", "Theil\u00b7chen", "aus", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "ander geh'n,", "tokens": ["an\u00b7der", "geh'n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Die sich so stark gepre\u00dft befinden, woraus die Donner-", "tokens": ["Die", "sich", "so", "stark", "ge\u00b7pre\u00dft", "be\u00b7fin\u00b7den", ",", "wo\u00b7raus", "die", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVPP", "VVFIN", "$,", "PWAV", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Schl\u00e4g' entsteh'n.", "tokens": ["Schl\u00e4g'", "ent\u00b7steh'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["XY", "XY", "XY"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Nun scheint der Schlag, der einfach ist, sich zu verdoppeln,", "tokens": ["Nun", "scheint", "der", "Schlag", ",", "der", "ein\u00b7fach", "ist", ",", "sich", "zu", "ver\u00b7dop\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "zu vermehren,", "tokens": ["zu", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Und, ob er gleich so gleich vorbey, doch eine Zeitlang noch", "tokens": ["Und", ",", "ob", "er", "gleich", "so", "gleich", "vor\u00b7bey", ",", "doch", "ei\u00b7ne", "Zeit\u00b7lang", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$,", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.30": {"text": "zu w\u00e4hren.", "tokens": ["zu", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die Ursach\u2019 ist, dieweil der Schlag von dem Gew\u00f6lk", "tokens": ["Die", "Ur\u00b7sach'", "ist", ",", "die\u00b7weil", "der", "Schlag", "von", "dem", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zur\u00fccke schl\u00e4gt,", "tokens": ["zu\u00b7r\u00fc\u00b7cke", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und dadurch ein erschrecklichs Echo, durch wiederholten", "tokens": ["Und", "da\u00b7durch", "ein", "er\u00b7schreck\u00b7lichs", "E\u00b7cho", ",", "durch", "wie\u00b7der\u00b7hol\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Knall, erregt,", "tokens": ["Knall", ",", "er\u00b7regt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Den Augenblick, so bald das Feuer aus den zerri\u00dfnen", "tokens": ["Den", "Au\u00b7gen\u00b7blick", ",", "so", "bald", "das", "Feu\u00b7er", "aus", "den", "zer\u00b7ri\u00df\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Blasen kracht,", "tokens": ["Bla\u00b7sen", "kracht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Zusammt dem angeflammten Stoff, und solch ein heftiges", "tokens": ["Zu\u00b7sammt", "dem", "an\u00b7ge\u00b7flamm\u00b7ten", "Stoff", ",", "und", "solch", "ein", "hef\u00b7ti\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PIAT", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Ersch\u00fcttern", "tokens": ["Er\u00b7sch\u00fct\u00b7tern"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "In der schnell ausgespannten Luft, mit g\u00e4hlinger Zertren-", "tokens": ["In", "der", "schnell", "aus\u00b7ge\u00b7spann\u00b7ten", "Luft", ",", "mit", "g\u00e4h\u00b7lin\u00b7ger", "Zer\u00b7tren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "--++-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "nung, macht;", "tokens": ["nung", ",", "macht", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "So m\u00fcssen, durch so starken Druck, die nahen Wolken-", "tokens": ["So", "m\u00fcs\u00b7sen", ",", "durch", "so", "star\u00b7ken", "Druck", ",", "die", "na\u00b7hen", "Wol\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "C\u00f6rper zittern,", "tokens": ["C\u00f6r\u00b7per", "zit\u00b7tern", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Dem ungeheuren Stosse weichen, und heftig in einander", "tokens": ["Dem", "un\u00b7ge\u00b7heu\u00b7ren", "Stos\u00b7se", "wei\u00b7chen", ",", "und", "hef\u00b7tig", "in", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "ADJD", "APPR", "PRF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "dringen.", "tokens": ["drin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Wann dann die Bl\u00e4sgen ebenfalls, aus welchen sie be-", "tokens": ["Wann", "dann", "die", "Bl\u00e4s\u00b7gen", "e\u00b7ben\u00b7falls", ",", "aus", "wel\u00b7chen", "sie", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "$,", "APPR", "PWAT", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "steh'n, zerspringen,", "tokens": ["steh'n", ",", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Und Feur aus allen Orten bricht; so mu\u00df ein stetigs", "tokens": ["Und", "Feur", "aus", "al\u00b7len", "Or\u00b7ten", "bricht", ";", "so", "mu\u00df", "ein", "ste\u00b7tigs"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$.", "ADV", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Blitzen, Wittern,", "tokens": ["Blit\u00b7zen", ",", "Wit\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Und auch zugleich ein starker Regen herunter st\u00fcrzen,", "tokens": ["Und", "auch", "zu\u00b7gleich", "ein", "star\u00b7ker", "Re\u00b7gen", "her\u00b7un\u00b7ter", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "welches man", "tokens": ["wel\u00b7ches", "man"], "token_info": ["word", "word"], "pos": ["PWS", "PIS"], "meter": "+--", "measure": "dactylic.init"}, "line.21": {"text": "In allen heftigen Gewittern best\u00e4ndig sp\u00fchren wird und", "tokens": ["In", "al\u00b7len", "hef\u00b7ti\u00b7gen", "Ge\u00b7wit\u00b7tern", "be\u00b7st\u00e4n\u00b7dig", "sp\u00fch\u00b7ren", "wird", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADJD", "VVINF", "VAFIN", "KON"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "kann.", "tokens": ["kann", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}}}}}