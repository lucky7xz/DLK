{"dta.poem.11518": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Was doch heut Nacht ein Sturm gewesen,", "tokens": ["Was", "doch", "heut", "Nacht", "ein", "Sturm", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "NN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis erst der Morgen sich geregt!", "tokens": ["Bis", "erst", "der", "Mor\u00b7gen", "sich", "ge\u00b7regt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat der ungebetne Besen", "tokens": ["Wie", "hat", "der", "un\u00b7ge\u00b7bet\u00b7ne", "Be\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kamin und Gassen ausgefegt!", "tokens": ["Ka\u00b7min", "und", "Gas\u00b7sen", "aus\u00b7ge\u00b7fegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kommt ein M\u00e4dchen schon die Stra\u00dfen,", "tokens": ["Da", "kommt", "ein", "M\u00e4d\u00b7chen", "schon", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das halb versch\u00fcchtert um sich sieht;", "tokens": ["Das", "halb", "ver\u00b7sch\u00fcch\u00b7tert", "um", "sich", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Rosen, die der Wind zerblasen,", "tokens": ["Wie", "Ro\u00b7sen", ",", "die", "der", "Wind", "zer\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So unstet ihr Gesichtchen gl\u00fcht.", "tokens": ["So", "un\u00b7stet", "ihr", "Ge\u00b7sicht\u00b7chen", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein sch\u00f6ner Bursch tritt ihr entgegen,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Bursch", "tritt", "ihr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er will ihr voll Entz\u00fccken nahn:", "tokens": ["Er", "will", "ihr", "voll", "Ent\u00b7z\u00fc\u00b7cken", "nahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sehn sich freudig und verlegen", "tokens": ["Wie", "sehn", "sich", "freu\u00b7dig", "und", "ver\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ungewohnten Schelme an!", "tokens": ["Die", "un\u00b7ge\u00b7wohn\u00b7ten", "Schel\u00b7me", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er scheint zu fragen, ob das Liebchen", "tokens": ["Er", "scheint", "zu", "fra\u00b7gen", ",", "ob", "das", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Z\u00f6pfe schon zurecht gemacht,", "tokens": ["Die", "Z\u00f6p\u00b7fe", "schon", "zu\u00b7recht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die heute Nacht im offnen St\u00fcbchen", "tokens": ["Die", "heu\u00b7te", "Nacht", "im", "off\u00b7nen", "St\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sturm in Unordnung gebracht.", "tokens": ["Ein", "Sturm", "in", "Un\u00b7ord\u00b7nung", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Bursche tr\u00e4umt noch von den K\u00fcssen,", "tokens": ["Der", "Bur\u00b7sche", "tr\u00e4umt", "noch", "von", "den", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihm das s\u00fc\u00dfe Kind getauscht,", "tokens": ["Die", "ihm", "das", "s\u00fc\u00b7\u00dfe", "Kind", "ge\u00b7tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er steht, von Anmuth hingerissen,", "tokens": ["Er", "steht", ",", "von", "An\u00b7muth", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Derweil sie um die Ecke rauscht.", "tokens": ["Der\u00b7weil", "sie", "um", "die", "E\u00b7cke", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}