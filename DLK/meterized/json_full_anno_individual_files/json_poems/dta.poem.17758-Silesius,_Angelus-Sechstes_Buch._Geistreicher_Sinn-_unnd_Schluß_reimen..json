{"dta.poem.17758": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Sechstes Buch.  \n Geistreicher Sinn- unnd  \n Schlu\u00df reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stank f\u00fcr Eitelkeit die h\u00e4uffig war verhanden:", "tokens": ["Ich", "stank", "f\u00fcr", "Ei\u00b7tel\u00b7keit", "die", "h\u00e4uf\u00b7fig", "war", "ver\u00b7han\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich war dem Abgrund nah/ und fieng schon anzu-", "tokens": ["Ich", "war", "dem", "Ab\u00b7grund", "nah", "/", "und", "fi\u00b7eng", "schon", "an\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$(", "KON", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "stranden.", "tokens": ["stran\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Ich lebte wie ein Vieh/ und fragte nicht nach GOtt/", "tokens": ["Ich", "leb\u00b7te", "wie", "ein", "Vieh", "/", "und", "frag\u00b7te", "nicht", "nach", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$(", "KON", "VVFIN", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich war ein Schatten Mensch/ und noch lebendig Todt.", "tokens": ["Ich", "war", "ein", "Schat\u00b7ten", "Mensch", "/", "und", "noch", "le\u00b7ben\u00b7dig", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$(", "KON", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nu bin ich widernmb in Christo auferstanden/", "tokens": ["Nu", "bin", "ich", "wi\u00b7dernmb", "in", "Chris\u00b7to", "auf\u00b7er\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "NE", "VVPP", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Und lebendig gemacht: die Ketten sind entzwey/", "tokens": ["Und", "le\u00b7ben\u00b7dig", "ge\u00b7macht", ":", "die", "Ket\u00b7ten", "sind", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Teuffel ist verjagt/ und ich bin lo\u00df und frey.", "tokens": ["Der", "Teuf\u00b7fel", "ist", "ver\u00b7jagt", "/", "und", "ich", "bin", "lo\u00df", "und", "frey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KON", "PPER", "VAFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich suche GOtt allein mit eifrigem Gem\u00fctte/", "tokens": ["Ich", "su\u00b7che", "Gott", "al\u00b7lein", "mit", "eif\u00b7ri\u00b7gem", "Ge\u00b7m\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und gebe mich Jhm auf. Was Er mir jmmer thut/", "tokens": ["Und", "ge\u00b7be", "mich", "Jhm", "auf", ".", "Was", "Er", "mir", "jm\u00b7mer", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "PWS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In Zeit und Ewigkeit/ das sprech\u2019 ich alles gut.", "tokens": ["In", "Zeit", "und", "E\u00b7wig\u00b7keit", "/", "das", "sprech'", "ich", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PDS", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach da\u00df Er mich doch nur f\u00fcr mehrerm fall beh\u00fctte!", "tokens": ["Ach", "da\u00df", "Er", "mich", "doch", "nur", "f\u00fcr", "meh\u00b7rerm", "fall", "be\u00b7h\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}