{"textgrid.poem.43739": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hab' Dank, du lieber Wind!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin in den Garten gegangen", "tokens": ["Ich", "bin", "in", "den", "Gar\u00b7ten", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und mag nicht wieder hinaus.", "tokens": ["Und", "mag", "nicht", "wie\u00b7der", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die goldigen Aepfel prangen", "tokens": ["Die", "gol\u00b7di\u00b7gen", "A\u00b7e\u00b7pfel", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren rothen Wangen", "tokens": ["Mit", "ih\u00b7ren", "ro\u00b7then", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und laden ein zum Schmaus.", "tokens": ["Und", "la\u00b7den", "ein", "zum", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ist es anzufangen?", "tokens": ["Wie", "ist", "es", "an\u00b7zu\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}