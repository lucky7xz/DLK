{"dta.poem.9570": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An seine augen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr augen habts gethan! durch euch bin ich betrogen/", "tokens": ["Ihr", "au\u00b7gen", "habts", "ge\u00b7than", "!", "durch", "euch", "bin", "ich", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr habt/ als m\u00f6rder/ mich um meine freyheit bracht.", "tokens": ["Ihr", "habt", "/", "als", "m\u00f6r\u00b7der", "/", "mich", "um", "mei\u00b7ne", "frey\u00b7heit", "bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "NN", "$(", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verr\u00e4ther! sagt einmahl/ was habt ihr doch gemacht/", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", "!", "sagt", "ein\u00b7mahl", "/", "was", "habt", "ihr", "doch", "ge\u00b7macht", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "$(", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ihr von Solimen die flammen eingesogen?", "tokens": ["Da\u00df", "ihr", "von", "So\u00b7li\u00b7men", "die", "flam\u00b7men", "ein\u00b7ge\u00b7so\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "VVINF", "VVINF", "$."], "meter": "+---+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sagt mir: was hat euch doch zu dieser that bewogen?", "tokens": ["Sagt", "mir", ":", "was", "hat", "euch", "doch", "zu", "die\u00b7ser", "that", "be\u00b7wo\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PWS", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schweigt still von Solimens verf\u00fchrerischen pracht!", "tokens": ["Schweigt", "still", "von", "So\u00b7li\u00b7mens", "ver\u00b7f\u00fch\u00b7re\u00b7ri\u00b7schen", "pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie ist nicht schnee und ei\u00df. Ich hab es wohl gedacht.", "tokens": ["Sie", "ist", "nicht", "schnee", "und", "ei\u00df", ".", "Ich", "hab", "es", "wohl", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVFIN", "KON", "ADJD", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr augen habts gethan! ihr habt mir vorgelogen.", "tokens": ["Ihr", "au\u00b7gen", "habts", "ge\u00b7than", "!", "ihr", "habt", "mir", "vor\u00b7ge\u00b7lo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun seht auch/ wie ihr sie zur gegen-liebe zwingt.", "tokens": ["Nun", "seht", "auch", "/", "wie", "ihr", "sie", "zur", "ge\u00b7gen\u00b7lie\u00b7be", "zwingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "PWAV", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sagt: da\u00df mein leben selbst schon mit dem tode ringt.", "tokens": ["Sagt", ":", "da\u00df", "mein", "le\u00b7ben", "selbst", "schon", "mit", "dem", "to\u00b7de", "ringt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOUS", "PPOSAT", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es ist um mich gethan! jetzt d\u00fcrfft ihr nichts verschweigen/", "tokens": ["Es", "ist", "um", "mich", "ge\u00b7than", "!", "jetzt", "d\u00fcrfft", "ihr", "nichts", "ver\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$.", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch euch/ als fenster/ sieht man in das hertz hinein.", "tokens": ["Durch", "euch", "/", "als", "fens\u00b7ter", "/", "sieht", "man", "in", "das", "hertz", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "KOUS", "NN", "$(", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sagt: da\u00df in kurtzen es wird staub und asche seyn/", "tokens": ["Sagt", ":", "da\u00df", "in", "kurt\u00b7zen", "es", "wird", "staub", "und", "asc\u00b7he", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOUS", "APPR", "ADJA", "PPER", "VAFIN", "ADJD", "KON", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn eure farbe kan davon zur gn\u00fcge zeugen.", "tokens": ["Denn", "eu\u00b7re", "far\u00b7be", "kan", "da\u00b7von", "zur", "gn\u00fc\u00b7ge", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PAV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}