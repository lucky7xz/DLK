{"dta.poem.19314": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch seyn ohne Gott, was ist das f\u00fcr ein Seyn!", "tokens": ["Ein", "Mensch", "seyn", "oh\u00b7ne", "Gott", ",", "was", "ist", "das", "f\u00fcr", "ein", "Seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "APPR", "NN", "$,", "PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein be\u00dfres hat das Thier, die Pflanze, ja der Stein.", "tokens": ["Ein", "be\u00df\u00b7res", "hat", "das", "Thier", ",", "die", "Pflan\u00b7ze", ",", "ja", "der", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Stein und Pflanz' und Thier, die zwar um Gott nicht wissen,", "tokens": ["Denn", "Stein", "und", "Pflanz'", "und", "Thier", ",", "die", "zwar", "um", "Gott", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er aber wei\u00df um sie, sie sind ihm nicht entrissen.", "tokens": ["Er", "a\u00b7ber", "wei\u00df", "um", "sie", ",", "sie", "sind", "ihm", "nicht", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPER", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie sind nicht los von Gott, gottlos bist du allein,", "tokens": ["Sie", "sind", "nicht", "los", "von", "Gott", ",", "gott\u00b7los", "bist", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mensch, der du f\u00fchlst mit ihm, und leugnest, den Verein.", "tokens": ["Mensch", ",", "der", "du", "f\u00fchlst", "mit", "ihm", ",", "und", "leug\u00b7nest", ",", "den", "Ver\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}}}}}