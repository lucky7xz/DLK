{"textgrid.poem.61095": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Teppichzelten, die zum Schlummer taugen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Teppichzelten, die zum Schlummer taugen,", "tokens": ["In", "Tep\u00b7pich\u00b7zel\u00b7ten", ",", "die", "zum", "Schlum\u00b7mer", "tau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Spiele der Gedanken sich vergn\u00fcgen,", "tokens": ["Am", "Spie\u00b7le", "der", "Ge\u00b7dan\u00b7ken", "sich", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dazwischen dann und wann in langen Z\u00fcgen", "tokens": ["Da\u00b7zwi\u00b7schen", "dann", "und", "wann", "in", "lan\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den k\u00fchlen Rauch der Wasserpfeife saugen,", "tokens": ["Den", "k\u00fch\u00b7len", "Rauch", "der", "Was\u00b7ser\u00b7pfei\u00b7fe", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald einsam tr\u00e4umen von geliebten Augen", "tokens": ["Bald", "ein\u00b7sam", "tr\u00e4u\u00b7men", "von", "ge\u00b7lieb\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit dem Traum die Gegenwart betr\u00fcgen,", "tokens": ["Und", "mit", "dem", "Traum", "die", "Ge\u00b7gen\u00b7wart", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald mit den Freunden bei gef\u00fcllten Kr\u00fcgen", "tokens": ["Bald", "mit", "den", "Freun\u00b7den", "bei", "ge\u00b7f\u00fcll\u00b7ten", "Kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In leichtem Witz der Toren Werk durchlaugen:", "tokens": ["In", "leich\u00b7tem", "Witz", "der", "To\u00b7ren", "Werk", "durch\u00b7lau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist das einz'ge, was in diesen Tagen,", "tokens": ["Das", "ist", "das", "einz'\u00b7ge", ",", "was", "in", "die\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "PRELS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo alle Blumen vor der Sonne fl\u00fcchten,", "tokens": ["Wo", "al\u00b7le", "Blu\u00b7men", "vor", "der", "Son\u00b7ne", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir tunlich noch erscheint und zu ertragen.", "tokens": ["Mir", "tun\u00b7lich", "noch", "er\u00b7scheint", "und", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVFIN", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wollt mich drum des Leichtsinns nicht bez\u00fcchten;", "tokens": ["Doch", "wollt", "mich", "drum", "des", "Leicht\u00b7sinns", "nicht", "be\u00b7z\u00fcch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PAV", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Dichter darf schon auszuruhen wagen,", "tokens": ["Ein", "Dich\u00b7ter", "darf", "schon", "aus\u00b7zu\u00b7ru\u00b7hen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn auch sein M\u00fc\u00dfiggang ist reich an Fr\u00fcchten.", "tokens": ["Denn", "auch", "sein", "M\u00fc\u00b7\u00dfig\u00b7gang", "ist", "reich", "an", "Fr\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In Teppichzelten, die zum Schlummer taugen,", "tokens": ["In", "Tep\u00b7pich\u00b7zel\u00b7ten", ",", "die", "zum", "Schlum\u00b7mer", "tau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure":