{"dta.poem.18131": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ueberschriften.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die keusche Lieb ist dieses Lebens Sonne,", "tokens": ["Die", "keu\u00b7sche", "Lieb", "ist", "die\u00b7ses", "Le\u00b7bens", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So unser Hertz erquickt mit Freud und Wonne;", "tokens": ["So", "un\u00b7ser", "Hertz", "er\u00b7quickt", "mit", "Freud", "und", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der rothe Mund ist ihr Altar; der Ku\u00df,", "tokens": ["Der", "ro\u00b7the", "Mund", "ist", "ihr", "Al\u00b7tar", ";", "der", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Opfer, so man ihr verehren mu\u00df.", "tokens": ["Das", "Op\u00b7fer", ",", "so", "man", "ihr", "ver\u00b7eh\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "PPER", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}