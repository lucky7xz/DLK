{"dta.poem.10295": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Vier Augen sind im Wege.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Panzer, den Graf Albrecht trug,             ", "tokens": ["Der", "Pan\u00b7zer", ",", "den", "Graf", "Al\u00b7brecht", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NE", "NE", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "War schwer von Gold und Eisen.", "tokens": ["War", "schwer", "von", "Gold", "und", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Feind, den er zu Boden schlug,", "tokens": ["Der", "Feind", ",", "den", "er", "zu", "Bo\u00b7den", "schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Teufel mu\u00dft\u2019 er reisen.", "tokens": ["Zum", "Teu\u00b7fel", "mu\u00dft'", "er", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sah sie vorbei den Ritter ziehn,", "tokens": ["Sah", "sie", "vor\u00b7bei", "den", "Rit\u00b7ter", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War jede Frau vernarrt in ihn.", "tokens": ["War", "je\u00b7de", "Frau", "ver\u00b7narrt", "in", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jedes Auge taute,", "tokens": ["Und", "je\u00b7des", "Au\u00b7ge", "tau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Griff seine Hand die Laute.", "tokens": ["Griff", "sei\u00b7ne", "Hand", "die", "Lau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Einst liebt\u2019 ihn eine Edeldam\u2019,", "tokens": ["Einst", "liebt'", "ihn", "ei\u00b7ne", "E\u00b7deldam'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im Schlo\u00df war Tanz und Prassen,", "tokens": ["Im", "Schlo\u00df", "war", "Tanz", "und", "Pras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wollte, als er Abschied nahm,", "tokens": ["Und", "woll\u00b7te", ",", "als", "er", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn nimmer ziehen lassen.", "tokens": ["Ihn", "nim\u00b7mer", "zie\u00b7hen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch er empfiehlt sich ehrfurchtsvoll,", "tokens": ["Doch", "er", "emp\u00b7fiehlt", "sich", "ehr\u00b7furchts\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotzt auch und grollt sie liebestoll.", "tokens": ["Trotzt", "auch", "und", "grollt", "sie", "lie\u00b7be\u00b7stoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jagt auf ihrer Stute", "tokens": ["Und", "jagt", "auf", "ih\u00b7rer", "Stu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm nach mit hei\u00dfem Blute.", "tokens": ["Ihm", "nach", "mit", "hei\u00b7\u00dfem", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ehalt an, halt an! Graf Albrecht mein,", "tokens": ["\u201e", "halt", "an", ",", "halt", "an", "!", "Graf", "Al\u00b7brecht", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "NE", "NE", "PPOSAT", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Du hast mein Herz genommen,", "tokens": ["Du", "hast", "mein", "Herz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann, ich will bei dir nur sein,", "tokens": ["Ich", "kann", ",", "ich", "will", "bei", "dir", "nur", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPR", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df Schmach und Schande kommen.", "tokens": ["La\u00df", "Schmach", "und", "Schan\u00b7de", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O, nimm mich auf dein Grauro\u00df vorn,", "tokens": ["O", ",", "nimm", "mich", "auf", "dein", "Grau\u00b7ro\u00df", "vorn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dir, mit dir durch Sturm und Dorn", "tokens": ["Mit", "dir", ",", "mit", "dir", "durch", "Sturm", "und", "Dorn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Helmbusch, sie mich flehen,", "tokens": ["Dein", "Helm\u00b7busch", ",", "sie", "mich", "fle\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Soll um mein Blondhaar wehen.\u201c", "tokens": ["Soll", "um", "mein", "Blond\u00b7haar", "we\u00b7hen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Graf Albrecht zog den Hengst steil an,", "tokens": ["Graf", "Al\u00b7brecht", "zog", "den", "Hengst", "steil", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und schaut das Weib von oben.", "tokens": ["Und", "schaut", "das", "Weib", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch hat er sie vom Sattel dann,", "tokens": ["Doch", "hat", "er", "sie", "vom", "Sat\u00b7tel", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Sattel nicht gehoben", "tokens": ["Vom", "Sat\u00b7tel", "nicht", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Winde weht sein langer Bart,", "tokens": ["Im", "Win\u00b7de", "weht", "sein", "lan\u00b7ger", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und finster spricht er, streng und hart:", "tokens": ["Und", "fins\u00b7ter", "spricht", "er", ",", "streng", "und", "hart", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Reit heim in dein Gehege,", "tokens": ["Reit", "heim", "in", "dein", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Vier Augen sind im Wege.", "tokens": ["Vier", "Au\u00b7gen", "sind", "im", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sch\u00f6ne Burgherrin erbla\u00dft,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Burg\u00b7her\u00b7rin", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Finger spielt am Z\u00fcgel.", "tokens": ["Ihr", "Fin\u00b7ger", "spielt", "am", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Goldfuchs wendet sie mit Hast,", "tokens": ["Den", "Gold\u00b7fuchs", "wen\u00b7det", "sie", "mit", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon ist sie hinterm H\u00fcgel.", "tokens": ["Schon", "ist", "sie", "hin\u00b7term", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es sieht der Graf ihr sp\u00f6ttisch nach", "tokens": ["Es", "sieht", "der", "Graf", "ihr", "sp\u00f6t\u00b7tisch", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und murmelt unterm Augendach:", "tokens": ["Und", "mur\u00b7melt", "un\u00b7term", "Au\u00b7gen\u00b7dach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das traf das Herz ihr mitten,", "tokens": ["Das", "traf", "das", "Herz", "ihr", "mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die kommt nicht mehr geritten.", "tokens": ["Die", "kommt", "nicht", "mehr", "ge\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Sommernacht liegt schwer und schw\u00fcl,", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", "liegt", "schwer", "und", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein regungslos Erwarten.", "tokens": ["Ein", "re\u00b7gungs\u00b7los", "Er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Wittib ist zu hei\u00df der Pf\u00fchl,", "tokens": ["Der", "Wit\u00b7tib", "ist", "zu", "hei\u00df", "der", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ruhlos irrt sie zum Garten.", "tokens": ["Ruh\u00b7los", "irrt", "sie", "zum", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und immer wilder wird ihr Sinn,", "tokens": ["Und", "im\u00b7mer", "wil\u00b7der", "wird", "ihr", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihm, zu ihm nur will sie hin.", "tokens": ["Zu", "ihm", ",", "zu", "ihm", "nur", "will", "sie", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vier Augen sind im Wege,", "tokens": ["Vier", "Au\u00b7gen", "sind", "im", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So fl\u00fcsterts aller Stege.", "tokens": ["So", "fl\u00fcs\u00b7terts", "al\u00b7ler", "Ste\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im Erker oben liegen weich", "tokens": ["Im", "Er\u00b7ker", "o\u00b7ben", "lie\u00b7gen", "weich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei blondgelockte Knaben,", "tokens": ["Zwei", "blond\u00b7ge\u00b7lock\u00b7te", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sich im Kinderhimmelreich", "tokens": ["Die", "sich", "im", "Kin\u00b7der\u00b7him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00e4rtlich umschlungen haben.", "tokens": ["Z\u00e4rt\u00b7lich", "um\u00b7schlun\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "O, Mutter, sieh dein Knabenpaar,", "tokens": ["O", ",", "Mut\u00b7ter", ",", "sieh", "dein", "Kna\u00b7ben\u00b7paar", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O, sieh das gelbe Ringelhaar,", "tokens": ["O", ",", "sieh", "das", "gel\u00b7be", "Rin\u00b7gel\u00b7haar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Schlafe, wie sie gl\u00fchen,", "tokens": ["Im", "Schla\u00b7fe", ",", "wie", "sie", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gesund und frisch erbl\u00fchen.", "tokens": ["Ge\u00b7sund", "und", "frisch", "er\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Zur\u00fcck, was soll der Dolch, zur\u00fcck \u2014", "tokens": ["Zu\u00b7r\u00fcck", ",", "was", "soll