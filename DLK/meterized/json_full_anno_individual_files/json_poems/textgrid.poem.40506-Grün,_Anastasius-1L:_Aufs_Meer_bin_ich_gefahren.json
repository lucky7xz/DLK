{"textgrid.poem.40506": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aufs Meer bin ich gefahren", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufs Meer bin ich gefahren", "tokens": ["Aufs", "Meer", "bin", "ich", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Kahne ganz allein,", "tokens": ["Im", "Kah\u00b7ne", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Begeisterung im Herzen,", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Korb die Flasche Wein.", "tokens": ["Im", "Korb", "die", "Fla\u00b7sche", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aufs Meer bin ich gefahren,", "tokens": ["Aufs", "Meer", "bin", "ich", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu leeren die Flasche rein!", "tokens": ["Zu", "lee\u00b7ren", "die", "Fla\u00b7sche", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sieht man so vieles Wasser,", "tokens": ["Sieht", "man", "so", "vie\u00b7les", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schmeckt doppelt s\u00fc\u00df der Wein.", "tokens": ["Schmeckt", "dop\u00b7pelt", "s\u00fc\u00df", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den vollen blinkenden Becher", "tokens": ["Den", "vol\u00b7len", "blin\u00b7ken\u00b7den", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Empor hebt meine Hand:", "tokens": ["Em\u00b7por", "hebt", "mei\u00b7ne", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch, all' ihr fernen Lieben!", "tokens": ["Hoch", ",", "all'", "ihr", "fer\u00b7nen", "Lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Hoch, deutsches Vaterland!", "tokens": ["Hoch", ",", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hinaus bin ich gefahren,", "tokens": ["Hin\u00b7aus", "bin", "ich", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu sehn, was bewegter wallt:", "tokens": ["Zu", "sehn", ",", "was", "be\u00b7weg\u00b7ter", "wallt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Herz, wenn's denkt der Lieben,", "tokens": ["Mein", "Herz", ",", "wenn's", "denkt", "der", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIS", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Meer, wenn's in Wogen sich ballt?", "tokens": ["Das", "Meer", ",", "wenn's", "in", "Wo\u00b7gen", "sich", "ballt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Ein Zug von holden Gestalten", "tokens": ["Ein", "Zug", "von", "hol\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der schreitet \u00fcber den Plan,", "tokens": ["Der", "schrei\u00b7tet", "\u00fc\u00b7ber", "den", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als Heiland mit dem Oelzweig", "tokens": ["Als", "Hei\u00b7land", "mit", "dem", "O\u00b7el\u00b7zweig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wallt jede von ihnen heran.", "tokens": ["Wallt", "je\u00b7de", "von", "ih\u00b7nen", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Es sind viel Bilder der Lieben,", "tokens": ["Es", "sind", "viel", "Bil\u00b7der", "der", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie sitzen zu mir herein;", "tokens": ["Sie", "sit\u00b7zen", "zu", "mir", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gottlob, da\u00df es nicht die Leiber,", "tokens": ["Gott\u00b7lob", ",", "da\u00df", "es", "nicht", "die", "Lei\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonst s\u00e4nke der Nachen ein!", "tokens": ["Sonst", "s\u00e4n\u00b7ke", "der", "Na\u00b7chen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Aufs Meer bin ich gefahren,", "tokens": ["Aufs", "Meer", "bin", "ich", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu schw\u00f6ren festen Eid,", "tokens": ["Zu", "schw\u00f6\u00b7ren", "fes\u00b7ten", "Eid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Best\u00e4ndig hier inmitten", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "hier", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Unbest\u00e4ndigkeit!", "tokens": ["Der", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dem Wahren, Rechten, Sch\u00f6nen", "tokens": ["Dem", "Wah\u00b7ren", ",", "Rech\u00b7ten", ",", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Banner treu zu stehn!", "tokens": ["Zum", "Ban\u00b7ner", "treu", "zu", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann ich zu den Besten nicht klimmen,", "tokens": ["Kann", "ich", "zu", "den", "Bes\u00b7ten", "nicht", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Doch nie mit den Schlechten zu gehn!", "tokens": ["Doch", "nie", "mit", "den", "Schlech\u00b7ten", "zu", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Wo edel der Kampf, zu k\u00e4mpfen,", "tokens": ["Wo", "e\u00b7del", "der", "Kampf", ",", "zu", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch fern, wo Wahnwitz ficht!", "tokens": ["Doch", "fern", ",", "wo", "Wahn\u00b7witz", "ficht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Herz und Mund und Leben", "tokens": ["Und", "Herz", "und", "Mund", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Freiheit, Recht und Licht!", "tokens": ["F\u00fcr", "Frei\u00b7heit", ",", "Recht", "und", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Liegt einer krank am Lager,", "tokens": ["Liegt", "ei\u00b7ner", "krank", "am", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hat zum Scherzen nicht Zeit;", "tokens": ["Der", "hat", "zum", "Scher\u00b7zen", "nicht", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Trennt wen ein Brett nur vom Tode,", "tokens": ["Trennt", "wen", "ein", "Brett", "nur", "vom", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Der schw\u00f6rt nicht falschen Eid.", "tokens": ["Der", "schw\u00f6rt", "nicht", "fal\u00b7schen", "Eid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Aufs Meer bin ich gefahren,", "tokens": ["Aufs", "Meer", "bin", "ich", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu singen nebenbei", "tokens": ["Zu", "sin\u00b7gen", "ne\u00b7ben\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lied in den freien Aether,", "tokens": ["Ein", "Lied", "in", "den", "frei\u00b7en", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+----", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Gleich ihm so frisch und frei!", "tokens": ["Gleich", "ihm", "so", "frisch", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Hat guten Klang das Liedlein,", "tokens": ["Hat", "gu\u00b7ten", "Klang", "das", "Lied\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann klingt es doppelt gut,", "tokens": ["Dann", "klingt", "es", "dop\u00b7pelt", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn's auf den Fl\u00fcgeln d