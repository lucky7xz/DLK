{"textgrid.poem.41608": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft du, mein Herz, noch, was im lichten Morgenscheine", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du, mein Herz, noch, was im lichten Morgenscheine", "tokens": ["Wei\u00dft", "du", ",", "mein", "Herz", ",", "noch", ",", "was", "im", "lich\u00b7ten", "Mor\u00b7gen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir jenen Sommertag entdeckt:", "tokens": ["Wir", "je\u00b7nen", "Som\u00b7mer\u00b7tag", "ent\u00b7deckt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sch\u00e4ndlich Aas, nicht weit vom schmalen Wegesraine,", "tokens": ["Ein", "sch\u00e4nd\u00b7lich", "Aas", ",", "nicht", "weit", "vom", "schma\u00b7len", "We\u00b7ge\u00b7srai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PTKNEG", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf Kieselsteinen hingestreckt.", "tokens": ["Auf", "Kie\u00b7sel\u00b7stei\u00b7nen", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Beine in der Luft, wie liederliche Frauen,", "tokens": ["Die", "Bei\u00b7ne", "in", "der", "Luft", ",", "wie", "lie\u00b7der\u00b7li\u00b7che", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Strome gl\u00fchnder Gifte voll,", "tokens": ["Vom", "Stro\u00b7me", "gl\u00fchn\u00b7der", "Gif\u00b7te", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lie\u00df es voll L\u00e4ssigkeit und ohne Scham uns schauen", "tokens": ["Lie\u00df", "es", "voll", "L\u00e4s\u00b7sig\u00b7keit", "und", "oh\u00b7ne", "Scham", "uns", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Leib, dem grauser Stank entquoll.", "tokens": ["Den", "Leib", ",", "dem", "grau\u00b7ser", "Stank", "ent\u00b7quoll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Sonne strahlte auf die ekle F\u00e4ulnis nieder,", "tokens": ["Die", "Son\u00b7ne", "strahl\u00b7te", "auf", "die", "ek\u00b7le", "F\u00e4ul\u00b7nis", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihre Glut zu kochen schien,", "tokens": ["Die", "ih\u00b7re", "Glut", "zu", "ko\u00b7chen", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als g\u00e4be hundertfach sie der Natur das wieder,", "tokens": ["Als", "g\u00e4\u00b7be", "hun\u00b7dert\u00b7fach", "sie", "der", "Na\u00b7tur", "das", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "VVFIN", "PPER", "ART", "NN", "ART", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem einst sie eine Form verliehn.", "tokens": ["Dem", "einst", "sie", "ei\u00b7ne", "Form", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Himmel schaute nach dem wundersamen Aase,", "tokens": ["Der", "Him\u00b7mel", "schau\u00b7te", "nach", "dem", "wun\u00b7der\u00b7sa\u00b7men", "Aa\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie es sich bl\u00fctengleich erschlo\u00df,", "tokens": ["Wie", "es", "sich", "bl\u00fc\u00b7ten\u00b7gleich", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fcrchterlich war der Geruch, da\u00df auf dem Grase", "tokens": ["So", "f\u00fcrch\u00b7ter\u00b7lich", "war", "der", "Ge\u00b7ruch", ",", "da\u00df", "auf", "dem", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Fast eine Ohnmacht dich umflo\u00df.", "tokens": ["Fast", "ei\u00b7ne", "Ohn\u00b7macht", "dich", "um\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Die Fliegen summten um die modernden Atome,", "tokens": ["Die", "Flie\u00b7gen", "summ\u00b7ten", "um", "die", "mo\u00b7dern\u00b7den", "A\u00b7to\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes gedr\u00e4ngt und schauerlich", "tokens": ["In\u00b7des", "ge\u00b7dr\u00e4ngt", "und", "schau\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Larven ekle Schar, in schwerem, schwarzem Strome", "tokens": ["Der", "Lar\u00b7ven", "ek\u00b7le", "Schar", ",", "in", "schwe\u00b7rem", ",", "schwar\u00b7zem", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch die lebendgen Fetzen schlich.", "tokens": ["Durch", "die", "le\u00b7bend\u00b7gen", "Fet\u00b7zen", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das alles senkte sich und knisterte verquellend", "tokens": ["Das", "al\u00b7les", "senk\u00b7te", "sich", "und", "knis\u00b7ter\u00b7te", "ver\u00b7quel\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "PRF", "KON", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stieg, wie sich die Woge hebt,", "tokens": ["Und", "stieg", ",", "wie", "sich", "die", "Wo\u00b7ge", "hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man meinte beinah, da\u00df von fremdem Hauche schwellend", "tokens": ["Man", "mein\u00b7te", "bei\u00b7nah", ",", "da\u00df", "von", "frem\u00b7dem", "Hau\u00b7che", "schwel\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Leib vervielfacht aufgelebt.", "tokens": ["Der", "Leib", "ver\u00b7viel\u00b7facht", "auf\u00b7ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und dieser Welt entrann ein T\u00f6nen, seltsam klingend,", "tokens": ["Und", "die\u00b7ser", "Welt", "ent\u00b7rann", "ein", "T\u00f6\u00b7nen", ",", "selt\u00b7sam", "klin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Wind und Wasser es erregt,", "tokens": ["Wie", "Wind", "und", "Was\u00b7ser", "es", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleichwie von K\u00f6rnern, die der Landmann rhythmisch schwingend", "tokens": ["Gleich\u00b7wie", "von", "K\u00f6r\u00b7nern", ",", "die", "der", "Land\u00b7mann", "rhyth\u00b7misch", "schwin\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Siebe sch\u00fcttelt und bewegt.", "tokens": ["Im", "Sie\u00b7be", "sch\u00fct\u00b7telt", "und", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Form verwischte sich zu einem Traum, der fahler", "tokens": ["Die", "Form", "ver\u00b7wischte", "sich", "zu", "ei\u00b7nem", "Traum", ",", "der", "fah\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als eine fl\u00fcchtge Skizze war,", "tokens": ["Als", "ei\u00b7ne", "fl\u00fccht\u00b7ge", "Skiz\u00b7ze", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die auf verge\u00dfnem Blatt erg\u00e4nzt wird, die dem Maler", "tokens": ["Die", "auf", "ver\u00b7ge\u00df\u00b7nem", "Blatt", "er\u00b7g\u00e4nzt", "wird", ",", "die", "dem", "Ma\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus der Erinnrung sich gebar.", "tokens": ["Aus", "der", "E\u00b7rinn\u00b7rung", "sich", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und eine H\u00fcndin sah aus felsigem Geklippe", "tokens": ["Und", "ei\u00b7ne", "H\u00fcn\u00b7din", "sah", "aus", "fel\u00b7si\u00b7gem", "Ge\u00b7klip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unruhig, mit erz\u00fcrntem Blick,", "tokens": ["Un\u00b7ru\u00b7hig", ",", "mit", "er\u00b7z\u00fcrn\u00b7tem", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nur die Gelegenheit ersp\u00e4hend, vom Gerippe", "tokens": ["Nur", "die", "Ge\u00b7le\u00b7gen\u00b7heit", "er\u00b7sp\u00e4\u00b7hend", ",", "vom", "Ge\u00b7rip\u00b7pe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu rei\u00dfen sich ein neues St\u00fcck.", "tokens": ["Zu", "rei\u00b7\u00dfen", "sich", "ein", "neu\u00b7es", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und dennoch wirst du gleich der eklen F\u00e4ulnis werden,", "tokens": ["Und", "den\u00b7noch", "wirst", "du", "gleich", "der", "ek\u00b7len", "F\u00e4ul\u00b7nis", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ganz so zerst\u00f6rt und grauenhaft,", "tokens": ["Ganz", "so", "zer\u00b7st\u00f6rt", "und", "grau\u00b7en\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du meiner Augen Stern, du Sonne mir auf Erden,", "tokens": ["Du", "mei\u00b7ner", "Au\u00b7gen", "Stern", ",", "du", "Son\u00b7ne", "mir", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,", "PPER", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Engel, meine Leidenschaft!", "tokens": ["Mein", "En\u00b7gel", ",", "mei\u00b7ne", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So wirst du aussehn, wann, o K\u00f6n'gin holder G\u00fcte,", "tokens": ["So", "wirst", "du", "aus\u00b7sehn", ",", "wann", ",", "o", "K\u00f6n'\u00b7gin", "hol\u00b7der", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$,", "PWAV", "$,", "FM", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du nach der letzten \u00d6lung gehst", "tokens": ["Du", "nach", "der", "letz\u00b7ten", "\u00d6\u00b7lung", "gehst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dorthin, wo unter \u00fcppgem Kraut und reicher Bl\u00fcte", "tokens": ["Dor\u00b7thin", ",", "wo", "un\u00b7ter", "\u00fcpp\u00b7gem", "Kraut", "und", "rei\u00b7cher", "Bl\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei den Gerippen du verwest.", "tokens": ["Bei", "den", "Ge\u00b7rip\u00b7pen", "du", "ver\u00b7west", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Dann, meine Sch\u00f6ne, sprich zum Wurm, der dich erlesen", "tokens": ["Dann", ",", "mei\u00b7ne", "Sch\u00f6\u00b7ne", ",", "sprich", "zum", "Wurm", ",", "der", "dich", "er\u00b7le\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "APPRART", "NN", "$,", "PRELS", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dem dein Leib zum K\u00fcssen lieb,", "tokens": ["Und", "dem", "dein", "Leib", "zum", "K\u00fcs\u00b7sen", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df prangende Gestalt und unverg\u00e4nglich Wesen", "tokens": ["Da\u00df", "pran\u00b7gen\u00b7de", "Ge\u00b7stalt", "und", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "We\u00b7sen"], "token_info": ["word", "wor