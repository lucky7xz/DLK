{"dta.poem.4047": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Hiller .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Du lebtest, Freund! &#8212; Wer nicht die", "tokens": ["Du", "leb\u00b7test", ",", "Freund", "!", "&#8212;", "Wer", "nicht", "die"], "token_info": ["word", "word", "punct", "word", "punct", "XML_entity", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "$(", "PWS", "PTKNEG", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "k\u00f6stliche", "tokens": ["k\u00f6st\u00b7li\u00b7che"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Reliquie des Paradieses, nicht", "tokens": ["Re\u00b7li\u00b7qui\u00b7e", "des", "Pa\u00b7ra\u00b7die\u00b7ses", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "PTKNEG"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Liebe goldne k\u00f6nigliche Frucht,", "tokens": ["Der", "Lie\u00b7be", "gold\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie Du, auf seinem Lebenswege brach,", "tokens": ["Wie", "Du", ",", "auf", "sei\u00b7nem", "Le\u00b7bens\u00b7we\u00b7ge", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wem nie im Kreise freier J\u00fcnglinge", "tokens": ["Wem", "nie", "im", "Krei\u00b7se", "frei\u00b7er", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.7": {"text": "In s\u00fc\u00dfem Ernst der Freundschaft trunkne Z\u00e4hre", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "Ernst", "der", "Freund\u00b7schaft", "trunk\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hinab ins Blut der heil'gen Rebe rann,", "tokens": ["Hin\u00b7ab", "ins", "Blut", "der", "heil'\u00b7gen", "Re\u00b7be", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wer nicht, wie Du, aus dem begeisternden", "tokens": ["Wer", "nicht", ",", "wie", "Du", ",", "aus", "dem", "be\u00b7geis\u00b7tern\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "$,", "PWAV", "PPER", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Dem ewigvollen Becher der Natur", "tokens": ["Dem", "e\u00b7wig\u00b7vol\u00b7len", "Be\u00b7cher", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sich Muth und Kraft, und Lieb' und Freude trank,", "tokens": ["Sich", "Muth", "und", "Kraft", ",", "und", "Lieb'", "und", "Freu\u00b7de", "trank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der lebte nie, und wenn sich ein Jahrhundert,", "tokens": ["Der", "leb\u00b7te", "nie", ",", "und", "wenn", "sich", "ein", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KON", "KOUS", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wie eine Last, auf seiner Schulter h\u00e4uft. \u2014", "tokens": ["Wie", "ei\u00b7ne", "Last", ",", "auf", "sei\u00b7ner", "Schul\u00b7ter", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Du lebtest, Freund! es bl\u00fcht nur wenigen", "tokens": ["Du", "leb\u00b7test", ",", "Freund", "!", "es", "bl\u00fcht", "nur", "we\u00b7ni\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "PPER", "VVFIN", "ADV", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Des Lebens Morgen, wie er Dir gebl\u00fcht;", "tokens": ["Des", "Le\u00b7bens", "Mor\u00b7gen", ",", "wie", "er", "Dir", "ge\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Du fandest Herzen, Dir an Einfalt, Dir", "tokens": ["Du", "fan\u00b7dest", "Her\u00b7zen", ",", "Dir", "an", "Ein\u00b7falt", ",", "Dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "APPR", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "An edelm Stolze gleich; es spro\u00dften Dir", "tokens": ["An", "e\u00b7delm", "Stol\u00b7ze", "gleich", ";", "es", "spro\u00df\u00b7ten", "Dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Viel sch\u00f6ne Bl\u00fcthen der Geselligkeit;", "tokens": ["Viel", "sch\u00f6\u00b7ne", "Bl\u00fc\u00b7then", "der", "Ge\u00b7sel\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Auch adelte die innigere Lust,", "tokens": ["Auch", "a\u00b7del\u00b7te", "die", "in\u00b7ni\u00b7ge\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Die Tochter weiser Einsamkeit, Dein Herz;", "tokens": ["Die", "Toch\u00b7ter", "wei\u00b7ser", "Ein\u00b7sam\u00b7keit", ",", "Dein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "F\u00fcr jeden Reitz der H\u00fcgel und der Thale,", "tokens": ["F\u00fcr", "je\u00b7den", "Reitz", "der", "H\u00fc\u00b7gel", "und", "der", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "F\u00fcr jede Grazien des Fr\u00fchlings ward", "tokens": ["F\u00fcr", "je\u00b7de", "Gra\u00b7zi\u00b7en", "des", "Fr\u00fch\u00b7lings", "ward"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ein offnes unumw\u00f6lktes Auge Dir.", "tokens": ["Ein", "off\u00b7nes", "un\u00b7um\u00b7w\u00f6lk\u00b7tes", "Au\u00b7ge", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dich, Gl\u00fccklicher, umfieng die Riesentochter", "tokens": ["Dich", ",", "Gl\u00fcck\u00b7li\u00b7cher", ",", "um\u00b7fi\u00b7eng", "die", "Rie\u00b7sen\u00b7toch\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der schaffenden Natur, Helvetia;", "tokens": ["Der", "schaf\u00b7fen\u00b7den", "Na\u00b7tur", ",", "Hel\u00b7ve\u00b7tia", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "$."], "meter": "-+---++-+", "measure": "dactylic.init"}, "line.3": {"text": "Wo frei und stark, der alte, stolze Rhein", "tokens": ["Wo", "frei", "und", "stark", ",", "der", "al\u00b7te", ",", "stol\u00b7ze", "Rhein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "$,", "ADJA", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Fels hinunter donnert, standest Du,", "tokens": ["Vom", "Fels", "hin\u00b7un\u00b7ter", "don\u00b7nert", ",", "stan\u00b7dest", "Du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPO", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jubeltest ins herrliche Get\u00fcmmel.", "tokens": ["Und", "ju\u00b7bel\u00b7test", "ins", "herr\u00b7li\u00b7che", "Ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo Fels und Wald ein holdes zauberisches", "tokens": ["Wo", "Fels", "und", "Wald", "ein", "hol\u00b7des", "zau\u00b7be\u00b7ri\u00b7sches"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "KON", "NN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Arkadien umschlie\u00dft, wo himmelhoch Gebirg,", "tokens": ["Ar\u00b7ka\u00b7di\u00b7en", "um\u00b7schlie\u00dft", ",", "wo", "him\u00b7mel\u00b7hoch", "Ge\u00b7birg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "De\u00df tausendj\u00e4hr'gen Scheitel ew'ger Schnee,", "tokens": ["De\u00df", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gen", "Schei\u00b7tel", "ew'\u00b7ger", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie Silberhaar des Greisen Stirne, kr\u00e4nzt,", "tokens": ["Wie", "Sil\u00b7ber\u00b7haar", "des", "Grei\u00b7sen", "Stir\u00b7ne", ",", "kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Umschwebt von Wetterwolken und von Adlern,", "tokens": ["Um\u00b7schwebt", "von", "Wet\u00b7ter\u00b7wol\u00b7ken", "und", "von", "Ad\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sich unabsehbar in die Ferne dehnt,", "tokens": ["Sich", "un\u00b7ab\u00b7seh\u00b7bar", "in", "die", "Fer\u00b7ne", "dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Der unentweihten freundlichen Natur", "tokens": ["Der", "un\u00b7ent\u00b7weih\u00b7ten", "freund\u00b7li\u00b7chen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Im Schoose schl\u00e4ft, und manches Helden Staub", "tokens": ["Im", "Schoo\u00b7se", "schl\u00e4ft", ",", "und", "man\u00b7ches", "Hel\u00b7den", "Staub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Vom leisen Abendwind emporgeweht,", "tokens": ["Vom", "lei\u00b7sen", "Ab\u00b7end\u00b7wind", "em\u00b7por\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Des Sennen sorgenfreies Dach umwallt,", "tokens": ["Des", "Sen\u00b7nen", "sor\u00b7gen\u00b7frei\u00b7es", "Dach", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dort f\u00fchltest Du, was gro\u00df und g\u00f6ttlich is