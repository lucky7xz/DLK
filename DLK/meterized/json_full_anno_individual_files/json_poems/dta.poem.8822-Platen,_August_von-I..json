{"dta.poem.8822": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wird endlich Allen aufgetischt:", "tokens": ["Wird", "end\u00b7lich", "Al\u00b7len", "auf\u00b7ge\u00b7tischt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesegnet werde, wer da lobt,", "tokens": ["Ge\u00b7seg\u00b7net", "wer\u00b7de", ",", "wer", "da", "lobt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesegnet werde, wer da zischt!", "tokens": ["Ge\u00b7seg\u00b7net", "wer\u00b7de", ",", "wer", "da", "zischt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PWS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo find' ich den Verschwiegenen,", "tokens": ["Wo", "find'", "ich", "den", "Ver\u00b7schwie\u00b7ge\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Dem nie ein rasches Wort entwischt?", "tokens": ["Dem", "nie", "ein", "ra\u00b7sches", "Wort", "ent\u00b7wischt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wort sey Jedem gern verg\u00f6nnt,", "tokens": ["Das", "Wort", "sey", "Je\u00b7dem", "gern", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch wenn er leere Halme drischt.", "tokens": ["Auch", "wenn", "er", "lee\u00b7re", "Hal\u00b7me", "drischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er\u00f6ffnet er die Muschel nie,", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "er", "die", "Mu\u00b7schel", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Was frommt's, ob Einer Perlen fischt?", "tokens": ["Was", "frommt's", ",", "ob", "Ei\u00b7ner", "Per\u00b7len", "fischt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wer schilt die Rose, wenn ihr Duft", "tokens": ["Wer", "schilt", "die", "Ro\u00b7se", ",", "wenn", "ihr", "Duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sich mit des Aethers Wolke mischt?", "tokens": ["Sich", "mit", "des", "A\u00b7e\u00b7thers", "Wol\u00b7ke", "mischt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Was staunst du, da du ziehst den Kork,", "tokens": ["Was", "staunst", "du", ",", "da", "du", "ziehst", "den", "Kork", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df an die Decke springt der Gischt?", "tokens": ["Da\u00df", "an", "die", "De\u00b7cke", "springt", "der", "Gischt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das Herz ist eine Flamme, Freund,", "tokens": ["Das", "Herz", "ist", "ei\u00b7ne", "Flam\u00b7me", ",", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sie lodert bis sie ganz erlischt.", "tokens": ["Sie", "lo\u00b7dert", "bis", "sie", "ganz", "er\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}