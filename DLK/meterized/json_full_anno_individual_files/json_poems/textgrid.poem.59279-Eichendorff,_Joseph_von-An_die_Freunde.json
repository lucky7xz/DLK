{"textgrid.poem.59279": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Freunde", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Jugend Glanz, der Sehnsucht irre Weisen,", "tokens": ["Der", "Ju\u00b7gend", "Glanz", ",", "der", "Sehn\u00b7sucht", "ir\u00b7re", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die tausend Str\u00f6me durch das duft'ge Land,", "tokens": ["Die", "tau\u00b7send", "Str\u00f6\u00b7me", "durch", "das", "duft'\u00b7ge", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es zieht uns all zu seinen Zauberkreisen. \u2013", "tokens": ["Es", "zieht", "uns", "all", "zu", "sei\u00b7nen", "Zau\u00b7ber\u00b7krei\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wem Gottesdienst in tiefster Brust entbrannt,", "tokens": ["Wem", "Got\u00b7tes\u00b7dienst", "in", "tiefs\u00b7ter", "Brust", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der sieht mit Wehmut ein unendlich Reisen", "tokens": ["Der", "sieht", "mit", "Weh\u00b7mut", "ein", "un\u00b7end\u00b7lich", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu ferner Heimat, die er fromm erkannt;", "tokens": ["Zu", "fer\u00b7ner", "Hei\u00b7mat", ",", "die", "er", "fromm", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und was sich spielend wob als ird'sche Blume,", "tokens": ["Und", "was", "sich", "spie\u00b7lend", "wob", "als", "ird'\u00b7sche", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADJD", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "W\u00f6lbt still den Kelch zum ", "tokens": ["W\u00f6lbt", "still", "den", "Kelch", "zum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "So schauet denn das buntbewegte Leben", "tokens": ["So", "schau\u00b7et", "denn", "das", "bunt\u00b7be\u00b7weg\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ringsum von meines Gartens heitrer Zinn,", "tokens": ["Ring\u00b7sum", "von", "mei\u00b7nes", "Gar\u00b7tens", "hei\u00b7trer", "Zinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df hoch die Bilder, die noch d\u00e4mmernd schweben \u2013", "tokens": ["Da\u00df", "hoch", "die", 