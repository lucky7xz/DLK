{"dta.poem.12683": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einem kauffmann.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die hochzeit-freuden sind auf dieses jahr vollbracht,", "tokens": ["Die", "hoch\u00b7zeit\u00b7freu\u00b7den", "sind", "auf", "die\u00b7ses", "jahr", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und er, Herr Br\u00e4utigam! ist, halt ich wohl, der letzte.", "tokens": ["Und", "er", ",", "Herr", "Br\u00e4u\u00b7ti\u00b7gam", "!", "ist", ",", "halt", "ich", "wohl", ",", "der", "letz\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "NE", "$.", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was w\u00fcnsch ich, da\u00df ihn noch zu guter letzt ergetzte?", "tokens": ["Was", "w\u00fcnsch", "ich", ",", "da\u00df", "ihn", "noch", "zu", "gu\u00b7ter", "letzt", "er\u00b7getz\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was man zu letzte w\u00fcnscht: Ein\u2019 angenehme nacht.", "tokens": ["Was", "man", "zu", "letz\u00b7te", "w\u00fcnscht", ":", "Ein'", "an\u00b7ge\u00b7neh\u00b7me", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "ADJA", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}