{"dta.poem.7636": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt, wer alles nicht mu\u00df durch sich selber werden,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wer", "al\u00b7les", "nicht", "mu\u00df", "durch", "sich", "sel\u00b7ber", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PIS", "PTKNEG", "VMFIN", "APPR", "PRF", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich nur anbilden darf vorbildliche Geberden;", "tokens": ["Sich", "nur", "an\u00b7bil\u00b7den", "darf", "vor\u00b7bild\u00b7li\u00b7che", "Ge\u00b7ber\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer einen Vater hat, wer einen Lehrer findet,", "tokens": ["Wer", "ei\u00b7nen", "Va\u00b7ter", "hat", ",", "wer", "ei\u00b7nen", "Leh\u00b7rer", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Muster, daran ihn Lieb' und Nachahmung bindet.", "tokens": ["Ein", "Mus\u00b7ter", ",", "da\u00b7ran", "ihn", "Lieb'", "und", "Nac\u00b7hah\u00b7mung", "bin\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Er rankt daran empor mit unbewu\u00dftem Flei\u00df,", "tokens": ["Er", "rankt", "da\u00b7ran", "em\u00b7por", "mit", "un\u00b7be\u00b7wu\u00df\u00b7tem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist geworden gut und edel, eh ers wei\u00df.", "tokens": ["Und", "ist", "ge\u00b7wor\u00b7den", "gut", "und", "e\u00b7del", ",", "eh", "ers", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VAPP", "ADJD", "KON", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und f\u00fchlt er dann, wozu Beruf und Pflicht ihn treiben,", "tokens": ["Und", "f\u00fchlt", "er", "dann", ",", "wo\u00b7zu", "Be\u00b7ruf", "und", "Pflicht", "ihn", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darf er bewu\u00dft, was unbewu\u00dft er ward, nur bleiben.", "tokens": ["Darf", "er", "be\u00b7wu\u00dft", ",", "was", "un\u00b7be\u00b7wu\u00dft", "er", "ward", ",", "nur", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "$,", "PRELS", "ADJD", "PPER", "VAFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}