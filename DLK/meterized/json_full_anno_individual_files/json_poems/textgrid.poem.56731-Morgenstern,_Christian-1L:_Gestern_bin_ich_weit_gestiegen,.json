"pos": ["PDS", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus des Waldgrunds braunem Tuch.", "tokens": ["aus", "des", "Wald\u00b7grunds", "brau\u00b7nem", "Tuch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der frische Erdgeruch", "tokens": ["Und", "der", "fri\u00b7sche", "Erd\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "tat mir wohl, und heiter sprach ich:", "tokens": ["tat", "mir", "wohl", ",", "und", "hei\u00b7ter", "sprach", "ich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahrlich, ich vergleich euch Riesen", "tokens": ["Wahr\u00b7lich", ",", "ich", "ver\u00b7gleich", "euch", "Rie\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "unerbittlichen Gedanken,", "tokens": ["un\u00b7er\u00b7bitt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "die sich ohne weichlich Wanken", "tokens": ["die", "sich", "oh\u00b7ne", "weich\u00b7lich", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "APPR", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6henluft der Wahrheit kiesen.", "tokens": ["H\u00f6\u00b7hen\u00b7luft", "der", "Wahr\u00b7heit", "kie\u00b7