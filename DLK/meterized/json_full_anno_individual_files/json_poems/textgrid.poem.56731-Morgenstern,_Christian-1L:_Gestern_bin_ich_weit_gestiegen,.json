{"textgrid.poem.56731": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gestern bin ich weit gestiegen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern bin ich weit gestiegen,", "tokens": ["Ge\u00b7stern", "bin", "ich", "weit", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "abw\u00e4rts, aufw\u00e4rts, kreuz und quer;", "tokens": ["ab\u00b7w\u00e4rts", ",", "auf\u00b7w\u00e4rts", ",", "kreuz", "und", "quer", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und am Ende, gliederschwer,", "tokens": ["und", "am", "En\u00b7de", ",", "glie\u00b7der\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "blieb im Tannenforst ich liegen.", "tokens": ["blieb", "im", "Tan\u00b7nen\u00b7forst", "ich", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil' ich gern in heitrer Buchen", "tokens": ["Weil'", "ich", "gern", "in", "hei\u00b7trer", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sonnengr\u00fcnem Feierlichte,", "tokens": ["son\u00b7nen\u00b7gr\u00fc\u00b7nem", "Fei\u00b7er\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "lieber noch, wo Tann und Fichte", "tokens": ["lie\u00b7ber", "noch", ",", "wo", "Tann", "und", "Fich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "kerzenstarr den Himmel suchen.", "tokens": ["ker\u00b7zen\u00b7starr", "den", "Him\u00b7mel", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aufrecht wird mir selbst die Seele,", "tokens": ["Auf\u00b7recht", "wird", "mir", "selbst", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "l\u00e4uft mein Aug empor den Stamm:", "tokens": ["l\u00e4uft", "mein", "Aug", "em\u00b7por", "den", "Stamm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Kriegsvolk, straff und stramm,", "tokens": ["Wie", "ein", "Kriegs\u00b7volk", ",", "straff", "und", "stramm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "stehn sie da, ohn Furcht und Fehle;", "tokens": ["stehn", "sie", "da", ",", "ohn", "Furcht", "und", "Feh\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ernst, in selbstgewollter Bu\u00dfe,", "tokens": ["ernst", ",", "in", "selbst\u00b7ge\u00b7woll\u00b7ter", "Bu\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nicht zur Rechten nicht zur Linken:", "tokens": ["nicht", "zur", "Rech\u00b7ten", "nicht", "zur", "Lin\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "wer der Sonne Ku\u00df will trinken,", "tokens": ["wer", "der", "Son\u00b7ne", "Ku\u00df", "will", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "hat im D\u00e4mmer keine Mu\u00dfe.", "tokens": ["hat", "im", "D\u00e4m\u00b7mer", "kei\u00b7ne", "Mu\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denksam sa\u00df ich. Moose stach ich", "tokens": ["Denk\u00b7sam", "sa\u00df", "ich", ".", "Moo\u00b7se", "stach", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus des Waldgrunds braunem Tuch.", "tokens": ["aus", "des", "Wald\u00b7grunds", "brau\u00b7nem", "Tuch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der frische Erdgeruch", "tokens": ["Und", "der", "fri\u00b7sche", "Erd\u00b7ge\u00b7ruch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "tat mir wohl, und heiter sprach ich:", "tokens": ["tat", "mir", "wohl", ",", "und", "hei\u00b7ter", "sprach", "ich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahrlich, ich vergleich euch Riesen", "tokens": ["Wahr\u00b7lich", ",", "ich", "ver\u00b7gleich", "euch", "Rie\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "unerbittlichen Gedanken,", "tokens": ["un\u00b7er\u00b7bitt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "die sich ohne weichlich Wanken", "tokens": ["die", "sich", "oh\u00b7ne", "weich\u00b7lich", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "APPR", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6henluft der Wahrheit kiesen.", "tokens": ["H\u00f6\u00b7hen\u00b7luft", "der", "Wahr\u00b7heit", "kie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Philosophin Mutter Erde", "tokens": ["Phi\u00b7lo\u00b7so\u00b7phin", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hat euch klar und schlicht gedacht,", "tokens": ["hat", "euch", "klar", "und", "schlicht", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "jeglichem zu Lehr und Acht,", "tokens": ["jeg\u00b7li\u00b7chem", "zu", "Lehr", "und", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "CARD", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "wie man teil des Lichtes werde.", "tokens": ["wie", "man", "teil", "des", "Lich\u00b7tes", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stolz aus lauem D\u00e4mmer fl\u00fcchten,", "tokens": ["Stolz", "aus", "lau\u00b7em", "D\u00e4m\u00b7mer", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rast und Abweg herb verachten,", "tokens": ["Rast", "und", "Ab\u00b7weg", "herb", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "nur das eine Ziel ertrachten \u2013", "tokens": ["nur", "das", "ei\u00b7ne", "Ziel", "ert\u00b7rach\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "also mu\u00df der Geist sich z\u00fcchten.", "tokens": ["al\u00b7so", "mu\u00df", "der", "Geist", "sich", "z\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lang noch an den schlanken Fichten", "tokens": ["Lang", "noch", "an", "den", "schlan\u00b7ken", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sah ich auf mit ernstem Sinn.", "tokens": ["sah", "ich", "auf", "mit", "erns\u00b7tem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erde! Gro\u00dfe Meisterin", "tokens": ["Er\u00b7de", "!", "Gro\u00b7\u00dfe", "Meis\u00b7te\u00b7rin"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bist du mir im Unterrichten!", "tokens": ["bist", "du", "mir", "im", "Un\u00b7ter\u00b7rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Besser als Folianten lehren,", "tokens": ["Bes\u00b7ser", "als", "Fo\u00b7li\u00b7an\u00b7ten", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "lehrst mich du, solang mein Leben.", "tokens": ["lehrst", "mich", "du", ",", "so\u00b7lang", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unersch\u00f6pflich ist dein Geben,", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "ist", "dein", "Ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "doch noch tiefer mein Verehren.", "tokens": ["doch", "noch", "tie\u00b7fer", "mein", "Ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gestern bin ich weit gestiegen,", "tokens": ["Ge\u00b7stern", "bin", "ich", "weit", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "abw\u00e4rts, aufw\u00e4rts, kreuz und quer;", "tokens": ["ab\u00b7w\u00e4rts", ",", "auf\u00b7w\u00e4rts", ",", "kreuz", "und", "quer", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und am Ende, gliederschwer,", "tokens": ["und", "am", "En\u00b7de", ",", "glie\u00b7der\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "blieb im Tannenforst ich liegen.", "tokens": ["blieb", "im", "Tan\u00b7nen\u00b7forst", "ich", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil' ich gern in h