rtrachten \u2013", "tokens": ["nur", "das", "ei\u00b7ne", "Ziel", "ert\u00b7rach\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "also mu\u00df der Geist sich z\u00fcchten.", "tokens": ["al\u00b7so", "mu\u00df", "der", "Geist", "sich", "z\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lang noch an den schlanken Fichten", "tokens": ["Lang", "noch", "an", "den", "schlan\u00b7ken", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sah ich auf mit ernstem Sinn.", "tokens": ["sah", "ich", "auf", "mit", "erns\u00b7tem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erde! Gro\u00dfe Meisterin", "tokens": ["Er\u00b7de", "!", "Gro\u00b7\u00dfe", "Meis\u00b7te\u00b7rin"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bist du mir im Unterrichten!", "tokens": ["bist", "du", "mir", "im", "Un\u00b7ter\u00b7rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Besser als Folianten lehren,", "tokens": ["Bes\u00b7ser", "als", "Fo\u00b7li\u00b7an\u00b7ten", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD"