{"textgrid.poem.56185": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hast du mir den ersten Schmerz getan,", "tokens": ["Nun", "hast", "du", "mir", "den", "ers\u00b7ten", "Schmerz", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der aber traf.", "tokens": ["Der", "a\u00b7ber", "traf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du schl\u00e4fst, du harter, unbarmherz'ger Mann,", "tokens": ["Du", "schl\u00e4fst", ",", "du", "har\u00b7ter", ",", "un\u00b7barm\u00b7her\u00b7z'\u00b7ger", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Todesschlaf.", "tokens": ["Den", "To\u00b7des\u00b7schlaf", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es blicket die Verla\u00dfne vor sich hin,", "tokens": ["Es", "bli\u00b7cket", "die", "Ver\u00b7la\u00df\u00b7ne", "vor", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt ist leer.", "tokens": ["Die", "Welt", "ist", "leer", "."], "token_info": ["word", "word", "word", "word