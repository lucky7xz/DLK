{"textgrid.poem.49371": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem gespitzten Ohr des Esels f\u00e4ngt die Fliegen", "tokens": ["Von", "dem", "ge\u00b7spitz\u00b7ten", "Ohr", "des", "E\u00b7sels", "f\u00e4ngt", "die", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ein Clown, der eine andre Heimat hat.", "tokens": ["ein", "Clown", ",", "der", "ei\u00b7ne", "and\u00b7re", "Hei\u00b7mat", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch kleine R\u00f6hrchen, die sich gr\u00fcnlich biegen,", "tokens": ["Durch", "klei\u00b7ne", "R\u00f6hr\u00b7chen", ",", "die", "sich", "gr\u00fcn\u00b7lich", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "hat er Verbindung mit Baronen in der Stadt.", "tokens": ["hat", "er", "Ver\u00b7bin\u00b7dung", "mit", "Ba\u00b7ro\u00b7nen", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In hohen Luftgeleisen, wo sich enharmonisch", "tokens": ["In", "ho\u00b7hen"