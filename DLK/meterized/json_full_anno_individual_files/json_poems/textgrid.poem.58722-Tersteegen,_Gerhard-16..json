{"textgrid.poem.58722": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid getrost, ich habe die Welt \u00fcberwunden. Joh. 16, 33", "tokens": ["Seid", "ge\u00b7trost", ",", "ich", "ha\u00b7be", "die", "Welt", "\u00fc\u00b7berw\u00b7un\u00b7den", ".", "Joh", ".", "16", ",", "33"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "So liegt denn nun der Feind durch deine G\u00fcte tot", "tokens": ["So", "liegt", "denn", "nun", "der", "Feind", "durch", "dei\u00b7ne", "G\u00fc\u00b7te", "tot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und macht mir ferner keine Not,", "tokens": ["Und", "macht", "mir", "fer\u00b7ner", "kei\u00b7ne", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die reine Liebe hat's erk\u00e4mpfet", "tokens": ["Die", "rei\u00b7ne", "Lie\u00b7be", "hat's", "er\u00b7k\u00e4mp\u00b7fet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit gewissem Sieg die falsche Lieb' ged\u00e4mpfet.", "tokens": ["Und", "mit", "ge\u00b7wis\u00b7sem", "Sieg", "die", "fal\u00b7sche", "Lieb'", "ge\u00b7d\u00e4mp\u00b7fet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die heil'ge Gotteslieb' treibt andre Liebe weit,", "tokens": ["Die", "heil'\u00b7ge", "Got\u00b7tes\u00b7lieb'", "treibt", "and\u00b7re", "Lie\u00b7be", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man f\u00fchlt den gro\u00dfen Unterscheid;", "tokens": ["Man", "f\u00fchlt", "den", "gro\u00b7\u00dfen", "Un\u00b7ter\u00b7scheid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Unbestand, man mu\u00df erblassen,", "tokens": ["O", "Un\u00b7be\u00b7stand", ",", "man", "mu\u00df", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kann ein undankbar' Herz die Liebe je verlassen!", "tokens": ["Kann", "ein", "un\u00b7dank\u00b7ba\u00b7r'", "Herz", "die", "Lie\u00b7be", "je", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fcrcht' nicht, da\u00df die Lust ein neues Leben kriegt,", "tokens": ["Ich", "f\u00fcrcht'", "nicht", ",", "da\u00df", "die", "Lust", "ein", "neu\u00b7es", "Le\u