{"textgrid.poem.42060": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihn trug das Schiff. Und seine Lippen sangen", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihn trug das Schiff. Und seine Lippen sangen", "tokens": ["Ihn", "trug", "das", "Schiff", ".", "Und", "sei\u00b7ne", "Lip\u00b7pen", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hin \u00fcber Weite, Wasser und den Wind:", "tokens": ["Hin", "\u00fc\u00b7ber", "Wei\u00b7te", ",", "Was\u00b7ser", "und", "den", "Wind", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Ferne! Flucht! Entgleitendes Verlangen!", "tokens": ["O", "Fer\u00b7ne", "!", "Flucht", "!", "Ent\u00b7glei\u00b7ten\u00b7des", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Augen, o Augen, immer noch zu blind,", "tokens": ["Au\u00b7gen", ",", "o", "Au\u00b7gen", ",", "im\u00b7mer", "noch", "zu", "blind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "O Atem, niemals tief genug getrunken,", "tokens": ["O", "A\u00b7tem", ",", "nie\u00b7mals", "tief", "ge\u00b7nug", "ge\u00