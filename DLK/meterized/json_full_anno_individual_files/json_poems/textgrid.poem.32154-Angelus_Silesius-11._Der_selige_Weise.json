{"textgrid.poem.32154": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "11. Der selige Weise", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie selig ist der Mensch, der alle seine Zeit", "tokens": ["Wie", "se\u00b7lig", "ist", "der", "Mensch", ",", "der", "al\u00b7le", "sei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit anders nichts verbringt als mit der Ewigkeit!", "tokens": ["Mit", "an\u00b7ders", "nichts", "ver\u00b7bringt", "als", "mit", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "VVFIN", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der, jung und alt, allein betrachtet und beschaut", "tokens": ["Der", ",", "jung", "und", "alt", ",", "al\u00b7lein", "be\u00b7trach\u00b7tet", "und", "be\u00b7schaut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "KON", "ADJD", "$,", "ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Weisheit Schlo\u00df, das Gott, sein Vater, hat gebaut.", "tokens": ["Der", "Weis\u00b7heit", "Schlo\u00df", ",", "das", "Gott", ",", "sein", "Va\u00b7ter", ",", "hat", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sich auf seinen Stab, das ewge Wort, aufst\u00fctzt", "tokens": ["Der", "sich", "auf", "sei\u00b7nen", "Stab", ",", "das", "ew\u00b7ge", "Wort", ",", "auf\u00b7st\u00fctzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nicht wie mancher Tor im fremden Sande sitzt;", "tokens": ["Und", "nicht", "wie", "man\u00b7cher", "Tor", "im", "frem\u00b7den", "San\u00b7de", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PIAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der nicht nach Haus und Hof, nach Gold und Silber sieht,", "tokens": ["Der", "nicht", "nach", "Haus", "und", "Hof", ",", "nach", "Gold", "und", "Sil\u00b7ber", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch seines Lebens Zeit zu z\u00e4hlen sich bem\u00fcht.", "tokens": ["Noch", "sei\u00b7nes", "Le\u00b7bens", "Zeit", "zu", "z\u00e4h\u00b7len", "sich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PTKZU", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihn wird das blinde Gl\u00fcck nicht hin und her vexiern,", "tokens": ["Ihn", "wird", "das", "blin\u00b7de", "Gl\u00fcck", "nicht", "hin", "und", "her", "ve\u00b7xiern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Noch etwa eitler Durst zu fremden Wassern f\u00fchrn.", "tokens": ["Noch", "et\u00b7wa", "eit\u00b7ler", "Durst", "zu", "frem\u00b7den", "Was\u00b7sern", "f\u00fchrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er wei\u00df von keinem Zwang, er liebt nicht Kr\u00e4merei,", "tokens": ["Er", "wei\u00df", "von", "kei\u00b7nem", "Zwang", ",", "er", "liebt", "nicht", "Kr\u00e4\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er trachtet nicht darnach, da\u00df er gesehen sei.", "tokens": ["Er", "trach\u00b7tet", "nicht", "dar\u00b7nach", ",", "da\u00df", "er", "ge\u00b7se\u00b7hen", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er ist der Welt ein Kind, die allern\u00e4chste Stadt", "tokens": ["Er", "ist", "der", "Welt", "ein", "Kind", ",", "die", "al\u00b7ler\u00b7n\u00e4chs\u00b7te", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist ihm so viel bekannt, als die der Tagus hat.", "tokens": ["Ist", "ihm", "so", "viel", "be\u00b7kannt", ",", "als", "die", "der", "Ta\u00b7gus", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "ART", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er schaut nur \u00fcber sich, so frei er immer kann,", "tokens": ["Er", "schaut", "nur", "\u00fc\u00b7ber", "sich", ",", "so", "frei", "er", "im\u00b7mer", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "$,", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sein rechtes Vaterland, den lieben Himmel, an.", "tokens": ["Sein", "rech\u00b7tes", "Va\u00b7ter\u00b7land", ",", "den", "lie\u00b7ben", "Him\u00b7mel", ",", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sein Alter rechnet er nicht nach der Jahre Zahl,", "tokens": ["Sein", "Al\u00b7ter", "rech\u00b7net", "er", "nicht", "nach", "der", "Jah\u00b7re", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In Gott vollkommen sein, das hei\u00dft er alt zumal.", "tokens": ["In", "Gott", "voll\u00b7kom\u00b7men", "sein", ",", "das", "hei\u00dft", "er", "alt", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAINF", "$,", "PDS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Sonne leuchtet ihm in seinen Acker ein", "tokens": ["Die", "Son\u00b7ne", "leuch\u00b7tet", "ihm", "in", "sei\u00b7nen", "A\u00b7cker", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und, wenns gleich Abend wird, so bleibt ihm doch ihr Schein.", "tokens": ["Und", ",", "wenns", "gleich", "A\u00b7bend", "wird", ",", "so", "bleibt", "ihm", "doch", "ihr", "Schein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er sieht des Lebens Baum im Geist begierlich an", "tokens": ["Er", "sieht", "des", "Le\u00b7bens", "Baum", "im", "Geist", "be\u00b7gier\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "APPRART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und geht mit allem Flei\u00df zu ihm die n\u00e4chste Bahn.", "tokens": ["Und", "geht", "mit", "al\u00b7lem", "Flei\u00df", "zu", "ihm", "die", "n\u00e4chs\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er k\u00fcmmert sich um nichts; was neben ihm geschieht,", "tokens": ["Er", "k\u00fcm\u00b7mert", "sich", "um", "nichts", ";", "was", "ne\u00b7ben", "ihm", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "$.", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ist ihm so fremd und klar, als was ein Blinder sieht.", "tokens": ["Ist", "ihm", "so", "fremd", "und", "klar", ",", "als", "was", "ein", "Blin\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch ist er stark und frisch, er scheuet keinen Feind,", "tokens": ["Doch", "ist", "er", "stark", "und", "frisch", ",", "er", "scheu\u00b7et", "kei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn gleich Welt, Teufel, Fleisch und mehr beisammen seind.", "tokens": ["Wenn", "gleich", "Welt", ",", "Teu\u00b7fel", ",", "Fleisch", "und", "mehr", "bei\u00b7sam\u00b7men", "seind", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "$,", "NN", "$,", "NN", "KON", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein andrer laufe hin, zerstreu sich mit der Welt,", "tokens": ["Ein", "an\u00b7drer", "lau\u00b7fe", "hin", ",", "zer\u00b7streu", "sich", "mit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dies ist das Leben und die Bahn, so mir gef\u00e4llt. ", "tokens": ["Dies", "ist", "das", "Le\u00b7ben", "und", "die", "Bahn", ",", "so", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie selig ist der Mensch, der alle seine Zeit", "tokens": ["Wie", "se\u00b7lig", "ist", "der", "Mensch", ",", "der", "al\u00b7le", "sei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit anders nichts verbringt als mit der Ewigkeit!", "tokens": ["Mit", "an\u00b7ders", "nichts", "ver\u00b7bringt", "als", "mit", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "VVFIN", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der, jung und alt, allein betrachtet und beschaut", "tokens": ["Der", ",", "jung", "und", "alt", ",", "al\u00b7lein", "be\u00b7trach\u00b7tet", "und", "be\u00b7schaut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "KON", "ADJD", "$,", "ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Weisheit Schlo\u00df, das Gott, sein Vater, hat gebaut.", "tokens": ["Der", "Weis\u00b7heit", "Schlo\u00df", ",", "das", "Gott", ",", "sein", "Va\u00b7ter", ",", "hat", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sich auf seinen Stab, das ewge Wort, aufst\u00fctzt", "tokens": ["Der", "sich", "auf", "sei\u00b7nen", "Stab", ",", "das", "ew\u00b7ge", "Wort", ",", "auf\u00b7st\u00fctzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nicht wie mancher Tor im fremden Sande sitzt;", "tokens": ["Und", "nicht", "wie", "man\u00b7cher", "Tor", "im", "frem\u00b7den", "San\u00b7de", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "PIAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der nicht nach Haus und Hof, nach Gold und Silber sieht,", "tokens": ["Der", "nicht", "nach", "Haus", "und", "Hof", ",", "nach", "Gold", "und", "Sil\u00b7ber", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch seines Lebens Zeit zu z\u00e4hlen sich bem\u00fcht.", "tokens": ["Noch", "sei\u00b7nes", "Le\u00b7bens", "Zeit", "zu", "z\u00e4h\u00b7len", "sich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PTKZU", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihn wird das blinde Gl\u00fcck nicht hin und her vexiern,", "tokens": ["Ihn", "wird", "das", "blin\u00b7de", "Gl\u00fcck", "nicht", "hin", "und", "her", "ve\u00b7xiern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Noch etwa eitler Durst zu fremden Wassern f\u00fchrn.", "tokens": ["Noch", "et\u00b7wa", "eit\u00b7ler", "Durst", "zu", "frem\u00b7den", "Was\u00b7sern", "f\u00fchrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er wei\u00df von keinem Zwang, er liebt nicht Kr\u00e4merei,", "tokens": ["Er", "wei\u00df", "von", "kei\u00b7nem", "Zwang", ",", "er", "liebt", "nicht", "Kr\u00e4\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er trachtet nicht darnach, da\u00df er gesehen sei.", "tokens": ["Er", "trach\u00b7tet", "nicht", "dar\u00b7nach", ",", "da\u00df", "er", "ge\u00b7se\u00b7hen", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er ist der Welt ein Kind, die allern\u00e4chste Stadt", "tokens": ["Er", "ist", "der", "Welt", "ein", "Kind", ",", "die", "al\u00b7ler\u00b7n\u00e4chs\u00b7te", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist ihm so viel bekannt, als die der Tagus hat.", "tokens": ["Ist", "ihm", "so", "viel", "be\u00b7kannt", ",", "als", "die", "der", "Ta\u00b7gus", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "ART", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er schaut nur \u00fcber sich, so frei er immer kann,", "tokens": ["Er", "schaut", "nur", "\u00fc\u00b7ber", "sich", ",", "so", "frei", "er", "im\u00b7mer", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "$,", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sein rechtes Vaterland, den lieben Himmel, an.", "tokens": ["Sein", "rech\u00b7tes", "Va\u00b7ter\u00b7land", ",", "den", "lie\u00b7ben", "Him\u00b7mel", ",", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sein Alter rechnet er nicht nach der Jahre Zahl,", "tokens": ["Sein", "Al\u00b7ter", "rech\u00b7net", "er", "nicht", "nach", "der", "Jah\u00b7re", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In Gott vollkommen sein, das hei\u00dft er alt zumal.", "tokens": ["In", "Gott", "voll\u00b7kom\u00b7men", "sein", ",", "das", "hei\u00dft", "er", "alt", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAINF", "$,", "PDS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Sonne leuchtet ihm in seinen Acker ein", "tokens": ["Die", "Son\u00b7ne", "leuch\u00b7tet", "ihm", "in", "sei\u00b7nen", "A\u00b7cker", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und, wenns gleich Abend wird, so bleibt ihm doch ihr Schein.", "tokens": ["Und", ",", "wenns", "gleich", "A\u00b7bend", "wird", ",", "so", "bleibt", "ihm", "doch", "ihr", "Schein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er sieht des Lebens Baum im Geist begierlich an", "tokens": ["Er", "sieht", "des", "Le\u00b7bens", "Baum", "im", "Geist", "be\u00b7gier\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "APPRART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und geht mit allem Flei\u00df zu ihm die n\u00e4chste Bahn.", "tokens": ["Und", "geht", "mit", "al\u00b7lem", "Flei\u00df", "zu", "ihm", "die", "n\u00e4chs\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er k\u00fcmmert sich um nichts; was neben ihm geschieht,", "tokens": ["Er", "k\u00fcm\u00b7mert", "sich", "um", "nichts", ";", "was", "ne\u00b7ben", "ihm", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "$.", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ist ihm so fremd und klar, als was ein Blinder sieht.", "tokens": ["Ist", "ihm", "so", "fremd", "und", "klar", ",", "als", "was", "ein", "Blin\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch ist er stark und frisch, er scheuet keinen Feind,", "tokens": ["Doch", "ist", "er", "stark", "und", "frisch", ",", "er", "scheu\u00b7et", "kei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn gleich Welt, Teufel, Fleisch und mehr beisammen seind.", "tokens": ["Wenn", "gleich", "Welt", ",", "Teu\u00b7fel", ",", "Fleisch", "und", "mehr", "bei\u00b7sam\u00b7men", "seind", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "$,", "NN", "$,", "NN", "KON", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein andrer laufe hin, zerstreu sich mit der Welt,", "tokens": ["Ein", "an\u00b7drer", "lau\u00b7fe", "hin", ",", "zer\u00b7streu", "sich", "mit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dies ist das Leben und die Bahn, so mir gef\u00e4llt. ", "tokens": ["Dies", "ist", "das", "Le\u00b7ben", "und", "die", "Bahn", ",", "so", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}