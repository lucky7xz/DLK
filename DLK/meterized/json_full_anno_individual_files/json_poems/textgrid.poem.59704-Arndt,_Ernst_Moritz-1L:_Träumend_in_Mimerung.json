{"textgrid.poem.59704": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00e4umend in Mimerung", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4umend in Mimerung", "tokens": ["Tr\u00e4u\u00b7mend", "in", "Mi\u00b7me\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Deutschesten Hains ich sturmbewegter Eichen,", "tokens": ["Deut\u00b7sches\u00b7ten", "Hains", "ich", "sturm\u00b7be\u00b7weg\u00b7ter", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und wie sie rauschten, rauschten mir Gedanken", "tokens": ["Und", "wie", "sie", "rauschten", ",", "rauschten", "mir", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wild durch die Seele,", "tokens": ["Wild", "durch", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dunkle Gedanken \u2013 Wie der Blitz, auf schwarzen", "tokens": ["Dunk\u00b7le", "Ge\u00b7dan\u00b7ken", "\u2013", "Wie", "der", "Blitz", ",", "auf", "schwar\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "PWAV", "ART", "NN", "$,", "APPR", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wolken sich w\u00e4lzend, schaurig durch die Luft schie\u00dft,", "tokens": ["Wol\u00b7ken", "sich", "w\u00e4l\u00b7zend", ",", "schau\u00b7rig", "durch", "die", "Luft", "schie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Scho\u00df es mit Blitzesleuchtung mir mit scharfem", "tokens": ["Scho\u00df", "es", "mit", "Blit\u00b7zes\u00b7leuch\u00b7tung", "mir", "mit", "schar\u00b7fem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "APPR", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Weh durch die Seele.", "tokens": ["Weh", "durch", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hundert und tausend, wie des Blitzes Funken", "tokens": ["Hun\u00b7dert", "und", "tau\u00b7send", ",", "wie", "des", "Blit\u00b7zes", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Fliegen, so flogen V\u00f6gel hei\u00dfer Schwingen", "tokens": ["Flie\u00b7gen", ",", "so", "flo\u00b7gen", "V\u00f6\u00b7gel", "hei\u00b7\u00dfer", "Schwin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mir um den Busen, hiehin, dahin flatternd,", "tokens": ["Mir", "um", "den", "Bu\u00b7sen", ",", "hie\u00b7hin", ",", "da\u00b7hin", "flat\u00b7ternd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PAV", "$,", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "M\u00e4chtige W\u00fchler.", "tokens": ["M\u00e4ch\u00b7ti\u00b7ge", "W\u00fch\u00b7ler", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "W\u00fchler, aufrei\u00dfend tiefsten Grund des Herzens,", "tokens": ["W\u00fch\u00b7ler", ",", "auf\u00b7rei\u00b7\u00dfend", "tiefs\u00b7ten", "Grund", "des", "Her\u00b7zens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Rei\u00dfend der gl\u00fccklich dicht verh\u00fcllten Zukunft", "tokens": ["Rei\u00b7\u00dfend", "der", "gl\u00fcck\u00b7lich", "dicht", "ver\u00b7h\u00fcll\u00b7ten", "Zu\u00b7kunft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJD", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dunkles Gew\u00f6lk auf, wo es wie gespenstisch", "tokens": ["Dunk\u00b7les", "Ge\u00b7w\u00f6lk", "auf", ",", "wo", "es", "wie", "ge\u00b7spens\u00b7tisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "PWAV", "PPER", "KOKOM", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mitternachtspiel spielt.", "tokens": ["Mit\u00b7ter\u00b7nacht\u00b7spiel", "spielt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Mitternachtspiel; denn gleich entbundnen Geistern,", "tokens": ["Mit\u00b7ter\u00b7nacht\u00b7spiel", ";", "denn", "gleich", "ent\u00b7bund\u00b7nen", "Geis\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nicht wie aus Windeln in der Zukunft Wiege,", "tokens": ["Nicht", "wie", "aus", "Win\u00b7deln", "in", "der", "Zu\u00b7kunft", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "APPR", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nein, wie aus Gr\u00e4bern, tanzten vor mir grausig", "tokens": ["Nein", ",", "wie", "aus", "Gr\u00e4\u00b7bern", ",", "tanz\u00b7ten", "vor", "mir", "grau\u00b7sig"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PWAV", "APPR", "NN", "$,", "VVFIN", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "S\u00e4keln den Tanz ab.", "tokens": ["S\u00e4\u00b7keln", "den", "Tanz", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbweh mir der Zeichen!\u00ab rief ich, \u00bbdu gewaltiges", "tokens": ["\u00bb", "weh", "mir", "der", "Zei\u00b7chen", "!", "\u00ab", "rief", "ich", ",", "\u00bb", "du", "ge\u00b7wal\u00b7ti\u00b7ges"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "$,", "$(", "PPER", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wehen des Geistes! Schone deiner Blitze!", "tokens": ["We\u00b7hen", "des", "Geis\u00b7tes", "!", "Scho\u00b7ne", "dei\u00b7ner", "Blit\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Schone des Donners! Denn er donnert Schrecken,", "tokens": ["Scho\u00b7ne", "des", "Don\u00b7ners", "!", "Denn", "er", "don\u00b7nert", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Geistesverwirrung.", "tokens": ["Geis\u00b7tes\u00b7ver\u00b7wir\u00b7rung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Weh mir der Zeichen! Weh der Sehnsuchtsfragen", "tokens": ["Weh", "mir", "der", "Zei\u00b7chen", "!", "Weh", "der", "Sehn\u00b7suchts\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "$.", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ahnender Sehnsucht, ob von diesen Eichen", "tokens": ["Ah\u00b7nen\u00b7der", "Sehn\u00b7sucht", ",", "ob", "von", "die\u00b7sen", "Ei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Freie Germanen Siegeskr\u00e4nze flechten?", "tokens": ["Frei\u00b7e", "Ger\u00b7ma\u00b7nen", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7ze", "flech\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Enkel noch flechten?", "tokens": ["En\u00b7kel", "noch", "flech\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Ob, wann Gefahr, wann Kriegsget\u00fcmmel andr\u00e4ngt,", "tokens": ["Ob", ",", "wann", "Ge\u00b7fahr", ",", "wann", "Kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel", "an\u00b7dr\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blut nur der Fremden deutsche Klingen r\u00f6tet?", "tokens": ["Blut", "nur", "der", "Frem\u00b7den", "deut\u00b7sche", "Klin\u00b7gen", "r\u00f6\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Vielheit der F\u00fcrsten wie ein Mann dann vorficht?", "tokens": ["Viel\u00b7heit", "der", "F\u00fcrs\u00b7ten", "wie", "ein", "Mann", "dann", "vor\u00b7ficht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOKOM", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Einheit in Treue?", "tokens": ["Ein\u00b7heit", "in", "Treu\u00b7e", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Ob, wann aus Welschland ein Orkan, aus Ru\u00dfland", "tokens": ["Ob", ",", "wann", "aus", "Wel\u00b7schland", "ein", "Or\u00b7kan", ",", "aus", "Ru\u00df\u00b7land"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "APPR", "NN", "ART", "NN", "$,", "APPR", "NE"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Brausend ein zweiter Deutschlands Mitte fasset,", "tokens": ["Brau\u00b7send", "ein", "zwei\u00b7ter", "Deutschlands", "Mit\u00b7te", "fas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Fern kein Arminius sein wird und kein zweiter", "tokens": ["Fern", "kein", "Ar\u00b7mi\u00b7nius", "sein", "wird", "und", "kein", "zwei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "VAINF", "VAFIN", "KON", "PIAT", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gneisenau-Bl\u00fccher?\u00ab", "tokens": ["Gnei\u00b7se\u00b7nau\u00b7B\u00b7l\u00fc\u00b7cher", "?", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Da hat's gelispelt: \u00bb", "tokens": ["Da", "hat's", "ge\u00b7lis\u00b7pelt", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Augen, du k\u00f6nntest sie am Born der Weisheit", "tokens": ["Au\u00b7gen", ",", "du", "k\u00f6nn\u00b7test", "sie", "am", "Born", "der", "Weis\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mimern verpf\u00e4nden, vollen Trunk der Seele", "tokens": ["Mi\u00b7mern", "ver\u00b7pf\u00e4n\u00b7den", ",", "vol\u00b7len", "Trunk", "der", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "ADJA", 