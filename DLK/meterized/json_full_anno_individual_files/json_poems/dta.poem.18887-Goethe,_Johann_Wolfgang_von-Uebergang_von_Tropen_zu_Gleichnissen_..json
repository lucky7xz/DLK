{"dta.poem.18887": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Uebergang von Tropen  \n  zu Gleichnissen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "That und Leben mir die Brust durchdringen,", "tokens": ["That", "und", "Le\u00b7ben", "mir", "die", "Brust", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wieder auf den F\u00fcssen steh\u2019 ich fest:", "tokens": ["Wie\u00b7der", "auf", "den", "F\u00fcs\u00b7sen", "steh'", "ich", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn der goldne Falke, breiter Schwingen,", "tokens": ["Denn", "der", "gold\u00b7ne", "Fal\u00b7ke", ",", "brei\u00b7ter", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ueberschwebet sein azurnes Nest.", "tokens": ["Ue\u00b7ber\u00b7schwe\u00b7bet", "sein", "a\u00b7zur\u00b7nes", "Nest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}