{"dta.poem.9391": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Von der Eitelen Welt-freude/  \n da\u00df sie fl\u00fcchtig sey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wie eitel seyn die sachen/", "tokens": ["Ach", "wie", "ei\u00b7tel", "seyn", "die", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAINF", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die uns sollen lustig machen!", "tokens": ["die", "uns", "sol\u00b7len", "lus\u00b7tig", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dieses lachen", "tokens": ["die\u00b7ses", "la\u00b7chen"], "token_info": ["word", "word"], "pos": ["PDS", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "und das bittre Zucker-wort", "tokens": ["und", "das", "bitt\u00b7re", "Zu\u00b7cke\u00b7rwort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das uns an ein solches ort", "tokens": ["das", "uns", "an", "ein", "sol\u00b7ches", "ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ziher fort/", "tokens": ["zi\u00b7her", "fort", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$("], "meter": "---", "measure": "unknown.measure.zero"}, "line.7": {"text": "da die schn\u00f6den L\u00fcste stehen/", "tokens": ["da", "die", "schn\u00f6\u00b7den", "L\u00fcs\u00b7te", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "das mu\u00df mit der zeit vergehen", "tokens": ["das", "mu\u00df", "mit", "der", "zeit", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "f\u00fcr und f\u00fcr.", "tokens": ["f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Mein? wo bleiben die geberden?", "tokens": ["Mein", "?", "wo", "blei\u00b7ben", "die", "ge\u00b7ber\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PWAV", "VVFIN", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die so weit geholet werden", "tokens": ["die", "so", "weit", "ge\u00b7ho\u00b7let", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVPP", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hier auf Erden?", "tokens": ["hier", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wird nicht blass der rothe Mund/", "tokens": ["Wird", "nicht", "blass", "der", "ro\u00b7the", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "der so manches Hertz verwundt", "tokens": ["der", "so", "man\u00b7ches", "Hertz", "ver\u00b7wundt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "auf den grund/", "tokens": ["auf", "den", "grund", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "dem die sch\u00f6nsten Rosen wichen", "tokens": ["dem", "die", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen", "wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und vor jhm so gar verblichen?", "tokens": ["und", "vor", "jhm", "so", "gar", "ver\u00b7bli\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "er mu\u00df fort.", "tokens": ["er", "mu\u00df", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Alle Lust und Freud\u2019 ist fl\u00fcchtig/", "tokens": ["Al\u00b7le", "Lust", "und", "Freud'", "ist", "fl\u00fcch\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles was wir thun/ ist nichtig/", "tokens": ["Al\u00b7les", "was", "wir", "thun", "/", "ist", "nich\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VVINF", "$(", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "gantz unt\u00fcchtig/", "tokens": ["gantz", "un\u00b7t\u00fcch\u00b7tig", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Selbst der Leib/ ob er gleich sch\u00f6n/", "tokens": ["Selbst", "der", "Leib", "/", "ob", "er", "gleich", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "kann die l\u00e4nge nicht bestehn/", "tokens": ["kann", "die", "l\u00e4n\u00b7ge", "nicht", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "mu\u00df vergehn.", "tokens": ["mu\u00df", "ver\u00b7gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "ja der gantze Krei\u00df der Erden", "tokens": ["ja", "der", "gant\u00b7ze", "Krei\u00df", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mu\u00df durch gluth zu nichte werden", "tokens": ["mu\u00df", "durch", "gluth", "zu", "nich\u00b7te", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "APPR", "PIS", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "mit der zeit.", "tokens": ["mit", "der", "zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wohl! was wollen wir noch prangen", "tokens": ["Wohl", "!", "was", "wol\u00b7len", "wir", "noch", "pran\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und der eitlen Ehr\u2019 anhangen", "tokens": ["und", "der", "eit\u00b7len", "Ehr'", "an\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mit verlangen?", "tokens": ["mit", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da doch alles nur verschwindt", "tokens": ["Da", "doch", "al\u00b7les", "nur", "ver\u00b7schwindt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "was mann hier auff Erden find/", "tokens": ["was", "mann", "hier", "auff", "Er\u00b7den", "find", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wie der wind;", "tokens": ["wie", "der", "wind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "dr\u00fcmb will ich nach jenem trachten", "tokens": ["dr\u00fcmb", "will", "ich", "nach", "je\u00b7nem", "trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und das Weltliche verachten/", "tokens": ["und", "das", "Welt\u00b7li\u00b7che", "ver\u00b7ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welt ade!", "tokens": ["Welt", "a\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}