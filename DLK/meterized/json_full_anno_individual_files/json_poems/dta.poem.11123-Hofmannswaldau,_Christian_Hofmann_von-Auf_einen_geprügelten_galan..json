{"dta.poem.11123": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen gepr\u00fcgelten galan.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst braucht man holtz dazu, wenn man will feuer machen,", "tokens": ["Sonst", "braucht", "man", "holtz", "da\u00b7zu", ",", "wenn", "man", "will", "feu\u00b7er", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "PAV", "$,", "KOUS", "PIS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, wie man sieht, bey dir sind es verkehrte sachen;", "tokens": ["Doch", ",", "wie", "man", "sieht", ",", "bey", "dir", "sind", "es", "ver\u00b7kehr\u00b7te", "sa\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIS", "VVFIN", "$,", "APPR", "PPER", "VAFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du bringst was neues auf, und zeigst mit zittern an,", "tokens": ["Du", "bringst", "was", "neu\u00b7es", "auf", ",", "und", "zeigst", "mit", "zit\u00b7tern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ADJA", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df man das feuer auch mit holtze leschen kan.", "tokens": ["Da\u00df", "man", "das", "feu\u00b7er", "auch", "mit", "holt\u00b7ze", "le\u00b7schen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "APPR", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}