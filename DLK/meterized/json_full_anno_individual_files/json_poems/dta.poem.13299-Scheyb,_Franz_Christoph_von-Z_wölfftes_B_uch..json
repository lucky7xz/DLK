{"dta.poem.13299": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eanstatt des Seegel-Tuchs schwingt sich ein Purpur-Dach", "tokens": ["\u201e", "an\u00b7statt", "des", "See\u00b7gel\u00b7Tuchs", "schwingt", "sich", "ein", "Pur\u00b7pur\u00b7Dach"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201egeschlungen in die Luft; dem schwebt ein anders nach,", "tokens": ["\u201e", "ge\u00b7schlun\u00b7gen", "in", "die", "Luft", ";", "dem", "schwebt", "ein", "an\u00b7ders", "nach", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "APPR", "ART", "NN", "$.", "ART", "VVFIN", "ART", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "195\u201dUnd deckt des Schiffs Geb\u00e4u durch sein gelindes Wehen,", "tokens": ["\"", "Und", "deckt", "des", "Schiffs", "Ge\u00b7b\u00e4u", "durch", "sein", "ge\u00b7lin\u00b7des", "We\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edoch da\u00df man das Gepr\u00e4ng der Hof-Stadt k\u00f6nne sehen.", "tokens": ["\u201e", "doch", "da\u00df", "man", "das", "Ge\u00b7pr\u00e4ng", "der", "Hof\u00b7Stadt", "k\u00f6n\u00b7ne", "se\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PIS", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edas Schiff erhebet sich in aufgeth\u00fcrmtem Stolz,", "tokens": ["\u201e", "das", "Schiff", "er\u00b7he\u00b7bet", "sich", "in", "auf\u00b7ge\u00b7th\u00fcrm\u00b7tem", "Stolz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund pranget mit dem Glanz des Kunst-geschnizten Golds.", "tokens": ["\u201e", "und", "pran\u00b7get", "mit", "dem", "Glanz", "des", "Kunst\u00b7ge\u00b7schniz\u00b7ten", "Golds", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eein wahres Pracht-Geb\u00e4u! ein Wohlfart-reiches Wesen!", "tokens": ["\u201e", "ein", "wah\u00b7res", "Pracht\u00b7Ge\u00b7b\u00e4u", "!", "ein", "Wohlf\u00b7ar\u00b7trei\u00b7ches", "We\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "200\u201dEin starcker Haupt-Beweis die Fragen aufzul\u00f6sen!", "tokens": ["\"", "Ein", "star\u00b7cker", "Haup\u00b7tBe\u00b7weis", "die", "Fra\u00b7gen", "auf\u00b7zu\u00b7l\u00f6\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eda schallt gewi\u00df der Ruff: je blutiger der Krieg,", "tokens": ["\u201e", "da", "schallt", "ge\u00b7wi\u00df", "der", "Ruff", ":", "je", "blu\u00b7ti\u00b7ger", "der", "Krieg", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eum so Ruhmw\u00fcrdiger und gr\u00f6sser ist der Sieg!", "tokens": ["\u201e", "um", "so", "Ruhm\u00b7w\u00fcr\u00b7di\u00b7ger", "und", "gr\u00f6s\u00b7ser", "ist", "der", "Sieg", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "ADV", "NN", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "\u201eje schwerer K\u00f6nige der Krone Recht erfechten,", "tokens": ["\u201e", "je", "schwe\u00b7rer", "K\u00f6\u00b7ni\u00b7ge", "der", "Kro\u00b7ne", "Recht", "er\u00b7fech\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eje pr\u00e4chtiger sieht man ihr Haupt in Lorber flechten.", "tokens": ["\u201e", "je", "pr\u00e4ch\u00b7ti\u00b7ger", "sieht", "man", "ihr", "Haupt", "in", "Lor\u00b7ber", "flech\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVFIN", "PIS", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "205\u201dEs wird ein Ebenbild der st\u00e4rcksten Kriegerinn,", "tokens": ["\"", "Es", "wird", "ein", "E\u00b7ben\u00b7bild", "der", "st\u00e4rcks\u00b7ten", "Krie\u00b7ge\u00b7rinn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eder ersten Frau der Welt, der gr\u00f6\u00dften Siegerinn,", "tokens": ["\u201e", "der", "ers\u00b7ten", "Frau", "der", "Welt", ",", "der", "gr\u00f6\u00df\u00b7ten", "Sie\u00b7ge\u00b7rinn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eden Augen dieses Volcks und allen Erden-Kreisen,", "tokens": ["\u201e", "den", "Au\u00b7gen", "die\u00b7ses", "Volcks", "und", "al\u00b7len", "Er\u00b7den\u00b7Krei\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PDAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eja gar der Nachwelt selbst sich durch die Schifffart weisen.", "tokens": ["\u201e", "ja", "gar", "der", "Nach\u00b7welt", "selbst", "sich", "durch", "die", "Schiff\u00b7fart", "wei\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}