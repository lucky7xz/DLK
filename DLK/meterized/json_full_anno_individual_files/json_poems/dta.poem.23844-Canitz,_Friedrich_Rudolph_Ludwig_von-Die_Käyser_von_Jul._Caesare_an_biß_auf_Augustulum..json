{"dta.poem.23844": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00e4yser von   Jul. Caesare   an bi\u00df auf  \n  Augustulum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.57", "af:0.42"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "A. C.ERst lehret Julius die R\u00f6mer dienstbar seyn/", "tokens": ["A.", "C.", "Erst", "leh\u00b7ret", "Ju\u00b7lius", "die", "R\u00f6\u00b7mer", "dienst\u00b7bar", "seyn", "/"], "token_info": ["abbreviation", "abbreviation", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "NE", "VVFIN", "NE", "ART", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bi\u00df ins verwirrte Reich Octavian tritt ein.", "tokens": ["Bi\u00df", "ins", "ver\u00b7wirr\u00b7te", "Reich", "Oc\u00b7ta\u00b7vi\u00b7an", "tritt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tiberius nach ihm ist voll von b\u00f6sen T\u00fccken/", "tokens": ["Ti\u00b7be\u00b7ri\u00b7us", "nach", "ihm", "ist", "voll", "von", "b\u00f6\u00b7sen", "T\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "An dem Caligula ist wenig zu erblicken", "tokens": ["An", "dem", "Ca\u00b7li\u00b7gu\u00b7la", "ist", "we\u00b7nig", "zu", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "VAFIN", "PIS", "PTKZU", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Als Grimm und Aberwitz. Der dumme Claudius", "tokens": ["Als", "Grimm", "und", "A\u00b7ber\u00b7witz", ".", "Der", "dum\u00b7me", "Clau\u00b7di\u00b7us"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NE", "KON", "NN", "$.", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So gleichfals ein Tyrann/ erlebet den Verdru\u00df/", "tokens": ["So", "gleich\u00b7fals", "ein", "Ty\u00b7rann", "/", "er\u00b7le\u00b7bet", "den", "Ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da\u00df sein verhurtes Weib mit andern sich verm\u00e4h-", "tokens": ["Da\u00df", "sein", "ver\u00b7hur\u00b7tes", "Weib", "mit", "an\u00b7dern", "sich", "ver\u00b7m\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PIS", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie wird der Christen-Schaar zu Nerons Zeit ge-", "tokens": ["Wie", "wird", "der", "Chris\u00b7ten\u00b7Schaar", "zu", "Ne\u00b7rons", "Zeit", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "NE", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der sich durch Mutter-Mord/ durch angelegten", "tokens": ["Der", "sich", "durch", "Mut\u00b7ter\u00b7Mord", "/", "durch", "an\u00b7ge\u00b7leg\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PRF", "APPR", "NN", "$(", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und tausend Grausamkeit der Nachwelt macht be-", "tokens": ["Und", "tau\u00b7send", "Grau\u00b7sam\u00b7keit", "der", "Nach\u00b7welt", "macht", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als Galba f\u00e4llt durch Geitz/ wird Sylvius erkohren/", "tokens": ["Als", "Gal\u00b7ba", "f\u00e4llt", "durch", "Geitz", "/", "wird", "Syl\u00b7vius", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPR", "NN", "$(", "VAFIN", "NE", "VVPP", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Der aus Verzweifelung/ nachdem die Schlacht ver-", "tokens": ["Der", "aus", "Ver\u00b7zwei\u00b7fe\u00b7lung", "/", "nach\u00b7dem", "die", "Schlacht", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "$(", "KOUS", "ART", "NN", "TRUNC"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.13": {"text": "Ihm selbst sein M\u00f6rder ist. Vitellius verha\u00dft/", "tokens": ["Ihm", "selbst", "sein", "M\u00f6r\u00b7der", "ist", ".", "Vi\u00b7tel\u00b7lius", "ver\u00b7ha\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$.", "NE", "ADJD", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Weil er in Schlemmerey viel Gut und Blut ver-", "tokens": ["Weil", "er", "in", "Schlem\u00b7me\u00b7rey", "viel", "Gut", "und", "Blut", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ADJD", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wird wie ein Aa\u00df geschlept. Vespasianus G\u00fcte", "tokens": ["Wird", "wie", "ein", "Aa\u00df", "ge\u00b7schlept", ".", "Ves\u00b7pa\u00b7si\u00b7a\u00b7nus", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "VVPP", "$.", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Begl\u00fcckt das K\u00e4yserthum/ in Titus sein Gem\u00fcthe", "tokens": ["Be\u00b7gl\u00fcckt", "das", "K\u00e4y\u00b7ser\u00b7thum", "/", "in", "Ti\u00b7tus", "sein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$(", "APPR", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ist alle Welt verliebt/ wiewol die heilge Stadt", "tokens": ["Ist", "al\u00b7le", "Welt", "ver\u00b7liebt", "/", "wie\u00b7wol", "die", "heil\u00b7ge", "Stadt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des Himmels schweren Zorn durch ihn empfunden", "tokens": ["Des", "Him\u00b7mels", "schwe\u00b7ren", "Zorn", "durch", "ihn", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ihm folgt Domitian sein Bruder/ der am Blute", "tokens": ["Ihm", "folgt", "Do\u00b7mi\u00b7ti\u00b7an", "sein", "Bru\u00b7der", "/", "der", "am", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "PPOSAT", "NN", "$(", "ART", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der B\u00fcrger sich erg\u00f6tzt/ der Christen andre Ruthe/", "tokens": ["Der", "B\u00fcr\u00b7ger", "sich", "er\u00b7g\u00f6tzt", "/", "der", "Chris\u00b7ten", "and\u00b7re", "Ru\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bi\u00df endlich Nerva kommt/ gleich da die Zeit ver-", "tokens": ["Bi\u00df", "end\u00b7lich", "Ner\u00b7va", "kommt", "/", "gleich", "da", "die", "Zeit", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NE", "VVFIN", "$(", "ADV", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "100Der ersten hundert Jahr/ die er mit Ruhm be-\nschlie\u00dft.", "tokens": ["ers\u00b7ten", "hun\u00b7dert", "Jahr", "/", "die", "er", "mit", "Ruhm", "be", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "TRUNC", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.23": {"text": "Trajan ist zwar ein Held den selbst das Gl\u00fccke", "tokens": ["Tra\u00b7jan", "ist", "zwar", "ein", "Held", "den", "selbst", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "ART", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Nur da\u00df die Christen er/ zum dritten mahl be-", "tokens": ["Nur", "da\u00df", "die", "Chris\u00b7ten", "er", "/", "zum", "drit\u00b7ten", "mahl", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "$(", "APPRART", "ADJA", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Der K\u00e4yser Adrian schreckt sie zum vierten mahl/", "tokens": ["Der", "K\u00e4y\u00b7ser", "Ad\u00b7ri\u00b7an", "schreckt", "sie", "zum", "vier\u00b7ten", "mahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "APPRART", "ADJA", "ADV", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Und schl\u00e4gt das Juden-Volck in einer grossen", "tokens": ["Und", "schl\u00e4gt", "das", "Ju\u00b7den\u00b7Volck", "in", "ei\u00b7ner", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Dem frommen Antonin gef\u00e4llt der edle Friede.", "tokens": ["Dem", "from\u00b7men", "An\u00b7to\u00b7nin", "ge\u00b7f\u00e4llt", "der", "ed\u00b7le", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sein Folger Antonin der Weise/ wird bald", "tokens": ["Sein", "Fol\u00b7ger", "An\u00b7to\u00b7nin", "der", "Wei\u00b7se", "/", "wird", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "ART", "NN", "$(", "VAFIN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Der Kirchen Feind zu seyn/ als durch des Betens", "tokens": ["Der", "Kir\u00b7chen", "Feind", "zu", "seyn", "/", "als", "durch", "des", "Be\u00b7tens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$(", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der Christen Legion ihm Sieg und Regen schafft.", "tokens": ["Der", "Chris\u00b7ten", "Le\u00b7gi\u00b7on", "ihm", "Sieg", "und", "Re\u00b7gen", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sein Sohn der Commodus stirbt wie ein W\u00fctrich", "tokens": ["Sein", "Sohn", "der", "Com\u00b7mo\u00b7dus", "stirbt", "wie", "ein", "W\u00fct\u00b7rich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Kaum hat der Pertinax den Purpur angeleget/", "tokens": ["Kaum", "hat", "der", "Per\u00b7tin\u00b7ax", "den", "Pur\u00b7pur", "an\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Als ihn sein eignes Heer erw\u00fcrget. Didius", "tokens": ["Als", "ihn", "sein", "eig\u00b7nes", "Heer", "er\u00b7w\u00fcr\u00b7get", ".", "Di\u00b7dius"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.34": {"text": "Erkaufft das K\u00e4yserthum/ stirbt durch des Rathes", "tokens": ["Er\u00b7kaufft", "das", "K\u00e4y\u00b7ser\u00b7thum", "/", "stirbt", "durch", "des", "Ra\u00b7thes"], "token_info": ["w