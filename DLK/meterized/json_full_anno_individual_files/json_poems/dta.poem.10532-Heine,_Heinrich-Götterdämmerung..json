{"dta.poem.10532": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der May ist da mit seinen goldnen Lichtern,", "tokens": ["Der", "May", "ist", "da", "mit", "sei\u00b7nen", "gold\u00b7nen", "Lich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seidnen L\u00fcften und gew\u00fcrzten D\u00fcften,", "tokens": ["Und", "seid\u00b7nen", "L\u00fcf\u00b7ten", "und", "ge\u00b7w\u00fcrz\u00b7ten", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und freundlich lockt er mit den wei\u00dfen Bl\u00fcthen,", "tokens": ["Und", "freund\u00b7lich", "lockt", "er", "mit", "den", "wei\u00b7\u00dfen", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gr\u00fc\u00dft aus tausend blauen Veilchenaugen,", "tokens": ["Und", "gr\u00fc\u00dft", "aus", "tau\u00b7send", "blau\u00b7en", "Veil\u00b7chen\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und breitet aus den blumreich gr\u00fcnen Teppich,", "tokens": ["Und", "brei\u00b7tet", "aus", "den", "blum\u00b7reich", "gr\u00fc\u00b7nen", "Tep\u00b7pich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durchwebt mit Sonnenschein und Morgenthau,", "tokens": ["Durch\u00b7webt", "mit", "Son\u00b7nen\u00b7schein", "und", "Mor\u00b7gent\u00b7hau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ruft herbei die lieben Menschenkinder.", "tokens": ["Und", "ruft", "her\u00b7bei", "die", "lie\u00b7ben", "Men\u00b7schen\u00b7kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das bl\u00f6de Volk gehorcht dem ersten Ruf;", "tokens": ["Das", "bl\u00f6\u00b7de", "Volk", "ge\u00b7horcht", "dem", "ers\u00b7ten", "Ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die M\u00e4nner ziehn die Nankinhosen an,", "tokens": ["Die", "M\u00e4n\u00b7ner", "ziehn", "die", "Nan\u00b7kin\u00b7ho\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Sonntagsr\u00f6ck' mit goldnen Spiegelkn\u00f6pfen;", "tokens": ["Und", "Sonn\u00b7tags\u00b7r\u00f6ck'", "mit", "gold\u00b7nen", "Spie\u00b7gel\u00b7kn\u00f6p\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Frauen kleiden sich in Unschuldwei\u00df,", "tokens": ["Die", "Frau\u00b7en", "klei\u00b7den", "sich", "in", "Un\u00b7schuld\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "J\u00fcnglinge kr\u00e4useln sich den Fr\u00fchlingsschnurrbart,", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "kr\u00e4u\u00b7seln", "sich", "den", "Fr\u00fch\u00b7lings\u00b7schnurr\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.13": {"text": "Jungfrauen lassen ihre Busen wallen,", "tokens": ["Jung\u00b7frau\u00b7en", "las\u00b7sen", "ih\u00b7re", "Bu\u00b7sen", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Stadtpoeten stecken in die Tasche", "tokens": ["Die", "Stadt\u00b7po\u00b7e\u00b7ten", "ste\u00b7cken", "in", "die", "Ta\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Papier und Bleistift und Lorgnett'; und jubelnd", "tokens": ["Pa\u00b7pier", "und", "Blei\u00b7stift", "und", "Lor\u00b7gnett'", ";", "und", "ju\u00b7belnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "$.", "KON", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Zieht nach dem Thor die krausbewegte Schaar,", "tokens": ["Zieht", "nach", "dem", "Thor", "die", "kraus\u00b7be\u00b7weg\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und lagert drau\u00dfen sich auf gr\u00fcnem Rasen,", "tokens": ["Und", "la\u00b7gert", "drau\u00b7\u00dfen", "sich", "auf", "gr\u00fc\u00b7nem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewundert, wie die B\u00e4ume flei\u00dfig wachsen,", "tokens": ["Be\u00b7wun\u00b7dert", ",", "wie", "die", "B\u00e4u\u00b7me", "flei\u00b7\u00dfig", "wach\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Spielt mit den bunten, zarten Bl\u00fcmelein,", "tokens": ["Spielt", "mit", "den", "bun\u00b7ten", ",", "zar\u00b7ten", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Horcht auf den Sang der lust'gen V\u00f6gelein,", "tokens": ["Horcht", "auf", "den", "Sang", "der", "lust'\u00b7gen", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jauchzt hinauf zum blauen Himmelszelt.", "tokens": ["Und", "jauchzt", "hin\u00b7auf", "zum", "blau\u00b7en", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu mir kam auch der Mai. Er klopfte dreimal", "tokens": ["Zu", "mir", "kam", "auch", "der", "Mai", ".", "Er", "klopf\u00b7te", "drei\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "An meine Th\u00fcr', und rief: Ich bin der Mai,", "tokens": ["An", "mei\u00b7ne", "Th\u00fcr'", ",", "und", "rief", ":", "Ich", "bin", "der", "Mai", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bleicher Tr\u00e4umer, komm, ich will dich k\u00fcssen!", "tokens": ["Du", "blei\u00b7cher", "Tr\u00e4u\u00b7mer", ",", "komm", ",", "ich", "will", "dich", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich hielt verriegelt meine Th\u00fcr', und rief:", "tokens": ["Ich", "hielt", "ver\u00b7rie\u00b7gelt", "mei\u00b7ne", "Th\u00fcr'", ",", "und", "rief", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vergebens lockst du mich, du schlimmer Gast;", "tokens": ["Ver\u00b7ge\u00b7bens", "lockst", "du", "mich", ",", "du", "schlim\u00b7mer", "Gast", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich habe dich durchschaut, ich hab' durchschaut", "tokens": ["Ich", "ha\u00b7be", "dich", "durch\u00b7schaut", ",", "ich", "hab'", "durch\u00b7schaut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Bau der Welt, und hab' zu viel geschaut,", "tokens": ["Den", "Bau", "der", "Welt", ",", "und", "hab'", "zu", "viel", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "VAFIN", "PTKA", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und viel zu tief, und hin ist alle Freude,", "tokens": ["Und", "viel", "zu", "tief", ",", "und", "hin", "ist", "al\u00b7le", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "$,", "KON", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und ew'ge Qualen zogen in mein Herz.", "tokens": ["Und", "ew'\u00b7ge", "Qua\u00b7len", "zo\u00b7gen", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich schaue durch die steinern harten Rinden", "tokens": ["Ich", "schau\u00b7e", "durch", "die", "stei\u00b7nern", "har\u00b7ten", "Rin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Menschenh\u00e4user und der Menschenherzen,", "tokens": ["Der", "Men\u00b7schen\u00b7h\u00e4u\u00b7ser", "und", "der", "Men\u00b7schen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und schau' in beiden Lug und Trug und Elend.", "tokens": ["Und", "schau'", "in", "bei\u00b7den", "Lug", "und", "Trug", "und", "E\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Auf den Gesichtern les' ich die Gedanken,", "tokens": ["Auf", "den", "Ge\u00b7sich\u00b7tern", "les'", "ich", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Viel schlimme. In der Jungfrau Scham-Err\u00f6then", "tokens": ["Viel", "schlim\u00b7me", ".", "In", "der", "Jung\u00b7frau", "Scham\u00b7Er\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Seh' ich geheime Lust begehrlich zittern;", "tokens": ["Seh'", "ich", "ge\u00b7hei\u00b7me", "Lust", "be\u00b7gehr\u00b7lich", "zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Auf dem begeistert stolzen J\u00fcnglingshaupt'", "tokens": ["Auf", "dem", "be\u00b7geis\u00b7tert", "stol\u00b7zen", "J\u00fcng\u00b7lings\u00b7haupt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Seh' ich die bunte Schellenkappe sitzen;", "tokens": ["Seh'", "ich", "die", "bun\u00b7te", "Schel\u00b7len\u00b7kap\u00b7pe", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und Fratzenbilder nur und sieche Schatten", "tokens": ["Und", "Frat\u00b7zen\u00b7bil\u00b7der", "nur", "und", "sie\u00b7che", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Seh' ich auf dieser Erde, und ich wei\u00df nicht,", "tokens": ["Seh'", "ich", "auf", "die\u00b7ser", "Er\u00b7de", ",", "und", "ich", "wei\u00df", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Ist sie ein Tollhaus oder Krankenhaus.", "tokens": ["Ist", "sie", "ein", "Toll\u00b7haus", "o\u00b7der", "Kran\u00b7ken\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich sehe durch den Grund der alten Erde,", "tokens": ["Ich", "se\u00b7he", "durch", "den", "Grund", "der", "al\u00b7ten", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als sey sie von Kristall, und seh' das Grausen,", "tokens": ["Als", "sey", "sie", "von", "Kris\u00b7tall", ",", "und", "seh'", "das", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das mit dem freud'gen Gr\u00fcne zu bedecken", "tokens": ["Das", "mit", "dem", "freu\u00b7d'\u00b7gen", "Gr\u00fc\u00b7ne", "zu", "be\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Mai vergeblich strebt. Ich seh' die Todten,", "tokens": ["Der", "Mai", "ver\u00b7geb\u00b7lich", "strebt", ".", "Ich", "seh'", "die", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie liegen unten in den schmalen S\u00e4rgen,", "tokens": ["Sie", "lie\u00b7gen", "un\u00b7ten", "in", "den", "schma\u00b7len", "S\u00e4r\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die H\u00e4nd' gefaltet und die Augen offen,", "tokens": ["Die", "H\u00e4nd'", "ge\u00b7fal\u00b7tet", "und", "die", "Au\u00b7gen", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word"