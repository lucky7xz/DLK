{"dta.poem.2853": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00e4me dich, zur\u00fcck von einem Wandelgang", "tokens": ["O", "sch\u00e4\u00b7me", "dich", ",", "zu\u00b7r\u00fcck", "von", "ei\u00b7nem", "Wan\u00b7del\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu kommen durch den Wald, die Fr\u00fchlingsflur entlang,", "tokens": ["Zu", "kom\u00b7men", "durch", "den", "Wald", ",", "die", "Fr\u00fch\u00b7lings\u00b7flur", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und nicht in deiner Brust ein Lied mit dir zu bringen,", "tokens": ["Und", "nicht", "in", "dei\u00b7ner", "Brust", "ein", "Lied", "mit", "dir", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag es nun oder nicht hervor nach au\u00dfen klingen.", "tokens": ["Mag", "es", "nun", "o\u00b7der", "nicht", "her\u00b7vor", "nach", "au\u00b7\u00dfen", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "KON", "PTKNEG", "PTKVZ", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das sch\u00f6nste Lied ist ja nicht das man druckt und schreibt,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Lied", "ist", "ja", "nicht", "das", "man", "druckt", "und", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "ART", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr das wie die Perl' in seiner Muschel bleibt.", "tokens": ["Viel\u00b7mehr", "das", "wie", "die", "Perl'", "in", "sei\u00b7ner", "Mu\u00b7schel", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}