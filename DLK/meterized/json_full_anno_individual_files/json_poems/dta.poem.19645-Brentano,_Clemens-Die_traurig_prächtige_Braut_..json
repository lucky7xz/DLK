{"dta.poem.19645": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die traurig pr\u00e4chtige Braut .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm heraus, komm heraus du sch\u00f6ne, sch\u00f6ne Braut,               ", "tokens": ["Komm", "he\u00b7raus", ",", "komm", "he\u00b7raus", "du", "sch\u00f6\u00b7ne", ",", "sch\u00f6\u00b7ne", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Deine gute Tage sind alle alle aus.", "tokens": ["Dei\u00b7ne", "gu\u00b7te", "Ta\u00b7ge", "sind", "al\u00b7le", "al\u00b7le", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "PIS", "PTKVZ", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "O Weyele Weh! O Weyele Weh!", "tokens": ["O", "We\u00b7ye\u00b7le", "Weh", "!", "O", "We\u00b7ye\u00b7le", "Weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$.", "NE", "PIAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was weinet die sch\u00f6ne Braut so sehr,", "tokens": ["Was", "wei\u00b7net", "die", "sch\u00f6\u00b7ne", "Braut", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mu\u00dft die Jungfern lassen stehn,", "tokens": ["Mu\u00dft", "die", "Jung\u00b7fern", "las\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu den Weibern must du gehn.", "tokens": ["Zu", "den", "Wei\u00b7bern", "must", "du", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lege an, lege an auf kurze kurze Zeit", "tokens": ["Le\u00b7ge", "an", ",", "le\u00b7ge", "an", "auf", "kur\u00b7ze", "kur\u00b7ze", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "APPR", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Darfst du ja wohl tragen das sch\u00f6ne Hochzeitskleid.", "tokens": ["Darfst", "du", "ja", "wohl", "tra\u00b7gen", "das", "sch\u00f6\u00b7ne", "Hoch\u00b7zeits\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O Weyele weh! o Weyele weh!", "tokens": ["O", "We\u00b7ye\u00b7le", "weh", "!", "o", "We\u00b7ye\u00b7le", "weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$.", "FM", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach was weinet die sch\u00f6ne Braut so sehr!", "tokens": ["Ach", "was", "wei\u00b7net", "die", "sch\u00f6\u00b7ne", "Braut", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Must dein H\u00e4rlein schlie\u00dfen ein", "tokens": ["Must", "dein", "H\u00e4r\u00b7lein", "schlie\u00b7\u00dfen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem weissen H\u00e4ubelein.", "tokens": ["In", "dem", "weis\u00b7sen", "H\u00e4u\u00b7be\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lache nicht, lache nicht, deine rothe rothe Schuh", "tokens": ["La\u00b7che", "nicht", ",", "la\u00b7che", "nicht", ",", "dei\u00b7ne", "ro\u00b7the", "ro\u00b7the", "Schuh"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$,", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Werden dich wohl dr\u00fccken, sind eng genug dazu.", "tokens": ["Wer\u00b7den", "dich", "wohl", "dr\u00fc\u00b7cken", ",", "sind", "eng", "ge\u00b7nug", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$,", "VAFIN", "ADJD", "ADV", "PAV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "O Weyele weh, o Weyele weh!", "tokens": ["O", "We\u00b7ye\u00b7le", "weh", ",", "o", "We\u00b7ye\u00b7le", "weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "FM", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach was weinet die sch\u00f6ne Braut so sehr.", "tokens": ["Ach", "was", "wei\u00b7net", "die", "sch\u00f6\u00b7ne", "Braut", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wenn die andern tanzen gehn", "tokens": ["Wenn", "die", "an\u00b7dern", "tan\u00b7zen", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirst du bei der Wiege stehn.", "tokens": ["Wirst", "du", "bei", "der", "Wie\u00b7ge", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wincke nur wincke nicht, sind gar leichte leichte Winck", "tokens": ["Win\u00b7cke", "nur", "win\u00b7cke", "nicht", ",", "sind", "gar", "leich\u00b7te", "leich\u00b7te", "Win\u00b7ck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PTKNEG", "$,", "VAFIN", "ADV", "ADJA", "ADJA", "NN"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Bis du an dem Finger einen goldnen Hochzeit-Ring.", "tokens": ["Bis", "du", "an", "dem", "Fin\u00b7ger", "ei\u00b7nen", "gold\u00b7nen", "Hoch\u00b7zeit\u00b7Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "O Weyele weh, o Weyele weh!", "tokens": ["O", "We\u00b7ye\u00b7le", "weh", ",", "o", "We\u00b7ye\u00b7le", "weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "FM", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach was weinet die sch\u00f6ne Braut so sehr!", "tokens": ["Ach", "was", "wei\u00b7net", "die", "sch\u00f6\u00b7ne", "Braut", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Goldne Ketten legst du an,", "tokens": ["Gold\u00b7ne", "Ket\u00b7ten", "legst", "du", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dft in ein Gef\u00e4ngni\u00df gahn.", "tokens": ["Mu\u00dft", "in", "ein", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "gahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Springe heut, springe heut deinen letzten letzten Tanz,", "tokens": ["Sprin\u00b7ge", "heut", ",", "sprin\u00b7ge", "heut", "dei\u00b7nen", "letz\u00b7ten", "letz\u00b7ten", "Tanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ADV", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Morgen kannst du weinen auf den sch\u00f6nen Hochzeitkranz,", "tokens": ["Mor\u00b7gen", "kannst", "du", "wei\u00b7nen", "auf", "den", "sch\u00f6\u00b7nen", "Hoch\u00b7zeit\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "O Weyele weh, o Weyele weh!", "tokens": ["O", "We\u00b7ye\u00b7le", "weh", ",", "o", "We\u00b7ye\u00b7le", "weh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "FM", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach wa\u00df weinet die sch\u00f6ne Braut so sehr!", "tokens": ["Ach", "wa\u00df", "wei\u00b7net", "die", "sch\u00f6\u00b7ne", "Braut", "so", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Must die Blumen lassen stehn.", "tokens": ["Must", "die", "Blu\u00b7men", "las\u00b7sen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Acker must du gehn.", "tokens": ["Auf", "den", "A\u00b7cker", "must", "du", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}