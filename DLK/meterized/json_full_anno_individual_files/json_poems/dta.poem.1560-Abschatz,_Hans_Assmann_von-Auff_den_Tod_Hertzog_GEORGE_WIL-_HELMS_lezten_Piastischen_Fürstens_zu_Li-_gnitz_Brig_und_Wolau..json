{"dta.poem.1560": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff den Tod Hertzog GEORGE WIL-  \n HELMS/ lezten Piastischen F\u00fcrstens zu Li-  \n gnitz/ Brig/ und Wolau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Fliesst nasse Thr\u00e4nen/ fliesst auff Wangen und Papier/ ", "tokens": ["Fliesst", "nas\u00b7se", "Thr\u00e4\u00b7nen", "/", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das lezte K\u00f6nigs-Blutt Sarmatiens liegt hier:", "tokens": ["Das", "lez\u00b7te", "K\u00f6\u00b7nigs\u00b7Blutt", "Sar\u00b7ma\u00b7ti\u00b7ens", "liegt", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Purpur/ der ihm war von Ahnen angeerbt/", "tokens": ["Der", "Pur\u00b7pur", "/", "der", "ihm", "war", "von", "Ah\u00b7nen", "an\u00b7ge\u00b7erbt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat seinen siechen Leib umkleidet und gef\u00e4rbt.", "tokens": ["Hat", "sei\u00b7nen", "sie\u00b7chen", "Leib", "um\u00b7klei\u00b7det", "und", "ge\u00b7f\u00e4rbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Fliesst milde Thr\u00e4nen fliesst auff Wangen und Papier/", "tokens": ["Fliesst", "mil\u00b7de", "Thr\u00e4\u00b7nen", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "iastens lezter Zweig/ der Edle F\u00fcrst/ liegt hier/", "tokens": ["i\u00b7as\u00b7tens", "lez\u00b7ter", "Zweig", "/", "der", "Ed\u00b7le", "F\u00fcrst", "/", "liegt", "hier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der hohe Cedern-Baum versinckt mit ihm ins Grab/", "tokens": ["Der", "ho\u00b7he", "Ce\u00b7dern\u00b7Baum", "ver\u00b7sinckt", "mit", "ihm", "ins", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der durch achthundert Jahr dem Lande Schatten gab.", "tokens": ["Der", "durch", "acht\u00b7hun\u00b7dert", "Jahr", "dem", "Lan\u00b7de", "Schat\u00b7ten", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fliesst tr\u00fcbe Thr\u00e4nen/ fliesst auff Wangen und Papier/", "tokens": ["Fliesst", "tr\u00fc\u00b7be", "Thr\u00e4\u00b7nen", "/", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Gro\u00dfen K\u00e4ysers Lust/ der kluge F\u00fcrst/ liegt hier.", "tokens": ["Des", "Gro\u00b7\u00dfen", "K\u00e4y\u00b7sers", "Lust", "/", "der", "klu\u00b7ge", "F\u00fcrst", "/", "liegt", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "itz hub ihn vor der Zeit auff seinen F\u00fcrsten-Thron: ", "tokens": ["itz", "hub", "ihn", "vor", "der", "Zeit", "auff", "sei\u00b7nen", "F\u00fcrs\u00b7ten\u00b7Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Blitz des Todes f\u00fchrt ihn vor der Zeit darvon.", "tokens": ["Der", "Blitz", "des", "To\u00b7des", "f\u00fchrt", "ihn", "vor", "der", "Zeit", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Fliesst dicke Thr\u00e4nen/ fliesst auff Wangen und Papier/", "tokens": ["Fliesst", "di\u00b7cke", "Thr\u00e4\u00b7nen", "/", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Auge Schlesiens/ der theure F\u00fcrst/ liegt hier/ ", "tokens": ["Das", "Au\u00b7ge", "Schle\u00b7si\u00b7ens", "/", "der", "theu\u00b7re", "F\u00fcrst", "/", "liegt", "hier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der tr\u00fcbe Himmel weint/ die Sonne birgt ihr Licht/ ", "tokens": ["Der", "tr\u00fc\u00b7be", "Him\u00b7mel", "weint", "/", "die", "Son\u00b7ne", "birgt", "ihr", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil dir/ Erlauchtes Haubt/ die Lebens-Sonne bricht. ", "tokens": ["Weil", "dir", "/", "Er\u00b7lauch\u00b7tes", "Haubt", "/", "die", "Le\u00b7bens\u00b7Son\u00b7ne", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Fliesst hei\u00dfe Thr\u00e4nen/ fliesst auff Wangen und Papier/ ", "tokens": ["Fliesst", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "/", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der treuen L\u00e4nder Schutz/ der werthe F\u00fcrst/ liegt hier. ", "tokens": ["Der", "treu\u00b7en", "L\u00e4n\u00b7der", "Schutz", "/", "der", "wert\u00b7he", "F\u00fcrst", "/", "liegt", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der seinem Ur-Ahn solt an Jahren gleiche gehn/ ", "tokens": ["Der", "sei\u00b7nem", "Ur\u00b7Ahn", "solt", "an", "Jah\u00b7ren", "glei\u00b7che", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "APPR", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sst uns an Jahren jung betr\u00fcbte W\u00e4ysen stehn. ", "tokens": ["sst", "uns", "an", "Jah\u00b7ren", "jung", "be\u00b7tr\u00fcb\u00b7te", "W\u00e4y\u00b7sen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Fliesst bittre Thr\u00e4nen/ fliesst auff Wangen und Papier:", "tokens": ["Fliesst", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "/", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unterthanen Schatz/ der liebste F\u00fcrst/ liegt hier/", "tokens": ["Der", "Un\u00b7ter\u00b7tha\u00b7nen", "Schatz", "/", "der", "liebs\u00b7te", "F\u00fcrst", "/", "liegt", "hier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Hertze neben ihm. Es decket eine Grofft/", "tokens": ["Ihr", "Hert\u00b7ze", "ne\u00b7ben", "ihm", ".", "Es", "de\u00b7cket", "ei\u00b7ne", "Grofft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Was sie geliebt/ geehrt/ gew\u00fcnschet und gehofft.", "tokens": ["Was", "sie", "ge\u00b7liebt", "/", "ge\u00b7ehrt", "/", "ge\u00b7w\u00fcn\u00b7schet", "und", "ge\u00b7hofft", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$(", "VVPP", "$(", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Fliesst bluttge Thr\u00e4nen/ fliesst auff Wangen und Papier/", "tokens": ["Fliesst", "blutt\u00b7ge", "Thr\u00e4\u00b7nen", "/", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tugend Ebenbild/ der fromme F\u00fcrst liegt hier:", "tokens": ["Der", "Tu\u00b7gend", "E\u00b7ben\u00b7bild", "/", "der", "from\u00b7me", "F\u00fcrst", "liegt", "hier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Kirche GOttes weint/ der hohe Richtstuhl klagt/", "tokens": ["Die", "Kir\u00b7che", "Got\u00b7tes", "weint", "/", "der", "ho\u00b7he", "Richt\u00b7stuhl", "klagt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das gantze Land erseuffzt/ erbebet/ \u00e4chzt und zagt.", "tokens": ["Das", "gant\u00b7ze", "Land", "er\u00b7seuffzt", "/", "er\u00b7be\u00b7bet", "/", "\u00e4chzt", "und", "zagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Fliesst herbe Thr\u00e4nen/ fliesst auff Wangen und Papier/", "tokens": ["Fliesst", "her\u00b7be", "Thr\u00e4\u00b7nen", "/", "fliesst", "auff", "Wan\u00b7gen", "und", "Pa\u00b7pier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der jungen Helden Prei\u00df/ der tapffre F\u00fcrst/ liegt hier.", "tokens": ["Der", "jun\u00b7gen", "Hel\u00b7den", "Prei\u00df", "/", "der", "tapf\u00b7fre", "F\u00fcrst", "/", "liegt", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch hat der Tod nur d