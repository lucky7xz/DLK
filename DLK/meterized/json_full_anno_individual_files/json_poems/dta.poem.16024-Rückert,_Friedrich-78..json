{"dta.poem.16024": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begreif, o Sohn, der Mensch ist eine kleine Welt,", "tokens": ["Be\u00b7greif", ",", "o", "Sohn", ",", "der", "Mensch", "ist", "ei\u00b7ne", "klei\u00b7ne", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Enthaltend alles was die gro\u00dfe nur enth\u00e4lt.", "tokens": ["Ent\u00b7hal\u00b7tend", "al\u00b7les", "was", "die", "gro\u00b7\u00dfe", "nur", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wie am Spiegelbild sich Rechts und Links umkehren,", "tokens": ["Doch", "wie", "am", "Spie\u00b7gel\u00b7bild", "sich", "Rechts", "und", "Links", "um\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gilt f\u00fcr Mensch und Welt Verschiedenheit der Lehren.", "tokens": ["So", "gilt", "f\u00fcr", "Mensch", "und", "Welt", "Ver\u00b7schie\u00b7den\u00b7heit", "der", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn Freundschaft Einheit ist, wenn Feindschaft ist Entzweiung,", "tokens": ["Wenn", "Freund\u00b7schaft", "Ein\u00b7heit", "ist", ",", "wenn", "Feind\u00b7schaft", "ist", "Ent\u00b7zwei\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAFIN", "$,", "KOUS", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hilft die Feindschaft erst dem Leben zur Befreiung.", "tokens": ["So", "hilft", "die", "Feind\u00b7schaft", "erst", "dem", "Le\u00b7ben", "zur", "Be\u00b7frei\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie bricht, da\u00df Vieles sei, das starre Eins entzwei;", "tokens": ["Sie", "bricht", ",", "da\u00df", "Vie\u00b7les", "sei", ",", "das", "star\u00b7re", "Eins", "ent\u00b7zwei", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "VAFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschieden ist was lebt, der Tod ist einerlei.", "tokens": ["Ver\u00b7schie\u00b7den", "ist", "was", "lebt", ",", "der", "Tod", "ist", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du la\u00df im Reich der Welt die hehre Zwietracht walten,", "tokens": ["Du", "la\u00df", "im", "Reich", "der", "Welt", "die", "heh\u00b7re", "Zwiet\u00b7racht", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lern' in deinem Zelt ihr Gegenbild entfalten.", "tokens": ["Und", "lern'", "in", "dei\u00b7nem", "Zelt", "ihr", "Ge\u00b7gen\u00b7bild", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "La\u00df aus der Kr\u00e4fte Kampf des Lebens F\u00fclle sprie\u00dfen,", "tokens": ["La\u00df", "aus", "der", "Kr\u00e4f\u00b7te", "Kampf", "des", "Le\u00b7bens", "F\u00fcl\u00b7le", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um Frieden still mit dir und Gott und Welt zu schlie\u00dfen.", "tokens": ["Um", "Frie\u00b7den", "still", "mit", "dir", "und", "Gott", "und", "Welt", "zu", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADJD", "APPR", "PPER", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}