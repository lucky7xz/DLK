{"textgrid.poem.46160": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wan der welt aug ganz fruchtbar seinen schein", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wan der welt aug ganz fruchtbar seinen schein", "tokens": ["Wan", "der", "welt", "aug", "ganz", "frucht\u00b7bar", "sei\u00b7nen", "schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "breit \u00fcber das erdreich ausstrecket,", "tokens": ["breit", "\u00fc\u00b7ber", "das", "er\u00b7dreich", "aus\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "alsdan mein ganz ruhlose pein", "tokens": ["als\u00b7dan", "mein", "ganz", "ruh\u00b7lo\u00b7se", "pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein solche klag erwecket:", "tokens": ["ein", "sol\u00b7che", "klag", "er\u00b7we\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbo sch\u00f6nes liecht, von jedem, nur von mir", "tokens": ["\u00bb", "o", "sch\u00f6\u00b7nes", "liecht", ",", "von", "je\u00b7dem", ",", "nur", "von", "mir"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "APPR", "PIS", "$,", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nicht gern, allsehend gern gesehen!", "tokens": ["nicht", "gern", ",", "all\u00b7se\u00b7hend", "gern", "ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ach, lieber! bracht man dir je f\u00fcr", "tokens": ["ach", ",", "lie\u00b7ber", "!", "bracht", "man", "dir", "je", "f\u00fcr"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["XY", "$,", "ADV", "$.", "VVFIN", "PIS", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein kl\u00e4glicheres flehen!", "tokens": ["ein", "kl\u00e4g\u00b7li\u00b7che\u00b7res", "fle\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwar deines liechts gesicht-geliebter lust", "tokens": ["Zwar", "dei\u00b7nes", "liechts", "ge\u00b7sicht\u00b7ge\u00b7lieb\u00b7ter", "lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "kan leichtlich die gesch\u00f6pf erlaben,", "tokens": ["kan", "leicht\u00b7lich", "die", "ge\u00b7sch\u00f6pf", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und doch das herz in meiner brust", "tokens": ["und", "doch", "das", "herz", "in", "mei\u00b7ner", "brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit keinem trost begaben.", "tokens": ["mit", "kei\u00b7nem", "trost", "be\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vol qual und angst ein schrecklich-schwarze nacht", "tokens": ["Vol", "qual", "und", "angst", "ein", "schreck\u00b7lich\u00b7schwa\u00b7rze", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mein herz, seel und gesicht beschweret,", "tokens": ["mein", "herz", ",", "seel", "und", "ge\u00b7sicht", "be\u00b7schwe\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "VVPP", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "die dan mit meines schmerzens macht", "tokens": ["die", "dan", "mit", "mei\u00b7nes", "schmer\u00b7zens", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den weg des tods mich lehret.", "tokens": ["den", "weg", "des", "tods", "mich", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Geheime ort such ich voll finsternus,", "tokens": ["Ge\u00b7hei\u00b7me", "ort", "such", "ich", "voll", "fins\u00b7ter\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die ich erf\u00fcll mit meinen klagen,", "tokens": ["die", "ich", "er\u00b7f\u00fcll", "mit", "mei\u00b7nen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADJD", "APPR", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df sie mir doppeln den verdru\u00df", "tokens": ["da\u00df", "sie", "mir", "dop\u00b7peln", "den", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "wan sie die widersagen.", "tokens": ["wan", "sie", "die", "wi\u00b7der\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Ein end hat nu mein lust, freud und gesang,", "tokens": ["Ein", "end", "hat", "nu", "mein", "lust", ",", "freud", "und", "ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "nichts kan mein elend von mir wenden;", "tokens": ["nichts", "kan", "mein", "e\u00b7lend", "von", "mir", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "darum wart ich nu des tods gang,", "tokens": ["da\u00b7rum", "wart", "ich", "nu", "des", "tods", "gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mein leid und klag zu enden.\u00ab", "tokens": ["mein", "leid", "und", "klag", "zu", "en\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "VVFIN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wan dan die nacht mit ihrem schwarzen kleid", "tokens": ["Wan", "dan", "die", "nacht", "mit", "ih\u00b7rem", "schwar\u00b7zen", "kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "den sterblichen die schlafstund bringet,", "tokens": ["den", "sterb\u00b7li\u00b7chen", "die", "schlaf\u00b7stund", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "alsdan mein wachend-frisches leid", "tokens": ["als\u00b7dan", "mein", "wa\u00b7chen\u00b7dfri\u00b7sches", "leid"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "also den luft durchdringet:", "tokens": ["al\u00b7so", "den", "luft", "durch\u00b7drin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "\u00bbo sch\u00f6ne stern, der nacht liechtreicher pracht,", "tokens": ["\u00bb", "o", "sch\u00f6\u00b7ne", "stern", ",", "der", "nacht", "liech\u00b7trei\u00b7cher", "pracht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "VVINF", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die ihr, bleich, meine not beweinet", "tokens": ["die", "ihr", ",", "bleich", ",", "mei\u00b7ne", "not", "be\u00b7wei\u00b7net"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "ADJD", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und meiner seelen letzten nacht", "tokens": ["und", "mei\u00b7ner", "see\u00b7len", "letz\u00b7ten", "nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zu dem grab traurig scheinet.", "tokens": ["zu", "dem", "grab", "trau\u00b7rig", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr k\u00f6nt zwar wol der schlafsucht s\u00fc\u00dfen saft", "tokens": ["Ihr", "k\u00f6nt", "zwar", "wol", "der", "schlaf\u00b7sucht", "s\u00fc\u00b7\u00dfen", "saft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der sterblichen gesicht eingie\u00dfen,", "tokens": ["der", "sterb\u00b7li\u00b7chen", "ge\u00b7sicht", "ein\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ihr aber habt nicht so vil kraft", "tokens": ["ihr", "a\u00b7ber", "habt", "nicht", "so", "vil", "kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein aug mir zu beschlie\u00dfen.", "tokens": ["ein", "aug", "mir", "zu", "be\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du nacht thust mir mit scheuzlich-schwerer hand", "tokens": ["Du", "nacht", "thust", "mir", "mit", "scheuz\u00b7lich\u00b7schwe\u00b7rer", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "geduld und ruh gar hinweg raufen,", "tokens": ["ge\u00b7duld", "und", "ruh", "gar", "hin\u00b7weg", "rau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "ADV", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die Furien mit ihrem brand", "tokens": ["die", "Fu\u00b7ri\u00b7en", "mit", "ih\u00b7rem", "brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "stets meinem geist nachlaufen.\u00ab", "tokens": ["stets", "mei\u00b7nem", "geist", "nach\u00b7lau\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVIZU", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Also hab ich kein liecht, trost noch geduld", "tokens": ["Al\u00b7so", "hab", "ich", "kein", "liecht", ",", "trost", "noch", "ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,", "VVFIN", "ADV", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "in meinem geist, gesicht und herzen", "tokens": ["i