{"dta.poem.23522": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Staatsinquisition.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hei\u00dft die schrecklichste der Listen,", "tokens": ["Wie", "hei\u00dft", "die", "schreck\u00b7lichs\u00b7te", "der", "Lis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00e4rger ist als Feindeslist,", "tokens": ["Die", "\u00e4r\u00b7ger", "ist", "als", "Fein\u00b7des\u00b7list", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auch sogar den span'schen Christen", "tokens": ["Und", "auch", "so\u00b7gar", "den", "span'\u00b7schen", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch unbekannt geblieben ist?", "tokens": ["Noch", "un\u00b7be\u00b7kannt", "ge\u00b7blie\u00b7ben", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich will dir deine Neugier stillen:", "tokens": ["Ich", "will", "dir", "dei\u00b7ne", "Neu\u00b7gier", "stil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Conduitenliste hei\u00dft die List,", "tokens": ["Con\u00b7dui\u00b7ten\u00b7lis\u00b7te", "hei\u00dft", "die", "List", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worin du durch Beh\u00f6rdenbrillen", "tokens": ["Wo\u00b7rin", "du", "durch", "Be\u00b7h\u00f6r\u00b7den\u00b7bril\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6n abdaguerrotypet bist. \u2014", "tokens": ["Sch\u00f6n", "ab\u00b7da\u00b7gu\u00b7er\u00b7ro\u00b7ty\u00b7pet", "bist", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "VAFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "O w\u00e4r' ich dann ein Troglodyte,", "tokens": ["O", "w\u00e4r'", "ich", "dann", "ein", "Tro\u00b7glo\u00b7dy\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Berg' und W\u00e4lder wilder Sohn!", "tokens": ["Der", "Ber\u00b7g'", "und", "W\u00e4l\u00b7der", "wil\u00b7der", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch leider bin ich von Conduite,", "tokens": ["Doch", "lei\u00b7der", "bin", "ich", "von", "Con\u00b7dui\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein einzig Wort verfehmt mich schon.", "tokens": ["Ein", "ein\u00b7zig", "Wort", "ver\u00b7fehmt", "mich", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}