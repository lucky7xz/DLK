{"dta.poem.10341": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Der Hans und die Grete tanzen herum,", "tokens": ["Der", "Hans", "und", "die", "Gre\u00b7te", "tan\u00b7zen", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und jauchzen vor lauter Freude.", "tokens": ["Und", "jauch\u00b7zen", "vor", "lau\u00b7ter", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Peter steht so still und stumm,", "tokens": ["Der", "Pe\u00b7ter", "steht", "so", "still", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist so bla\u00df wie Kreide.", "tokens": ["Und", "ist", "so", "bla\u00df", "wie", "Krei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Hans und die Grete sind Br\u00e4ut'gam und Braut,", "tokens": ["Der", "Hans", "und", "die", "Gre\u00b7te", "sind", "Br\u00e4ut'\u00b7gam", "und", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NN", "VAFIN", "NE", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und blitzen im Hochzeitgeschmeide.", "tokens": ["Und", "blit\u00b7zen", "im", "Hoch\u00b7zeit\u00b7ge\u00b7schmei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der arme Peter die N\u00e4gel kau't", "tokens": ["Der", "ar\u00b7me", "Pe\u00b7ter", "die", "N\u00e4\u00b7gel", "kau't"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und geht im Werkeltagskleide.", "tokens": ["Und", "geht", "im", "Wer\u00b7kel\u00b7tags\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Peter spricht leise vor sich her,", "tokens": ["Der", "Pe\u00b7ter", "spricht", "lei\u00b7se", "vor", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schaut betr\u00fcbet auf beide:", "tokens": ["Und", "schaut", "be\u00b7tr\u00fc\u00b7bet", "auf", "bei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach! wenn ich nicht gar zu vern\u00fcnftig w\u00e4r',", "tokens": ["Ach", "!", "wenn", "ich", "nicht", "gar", "zu", "ver\u00b7n\u00fcnf\u00b7tig", "w\u00e4r'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PTKNEG", "ADV", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich th\u00e4t' mir was zu leide.", "tokens": ["Ich", "th\u00e4t'", "mir", "was", "zu", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}