{"textgrid.poem.51136": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf Maler Distelis Tod", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben Ruh, die Kutten braun und schwarz,", "tokens": ["Sie", "ha\u00b7ben", "Ruh", ",", "die", "Kut\u00b7ten", "braun", "und", "schwarz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Flederm\u00e4use, Raben-, Eulenk\u00f6pfe,", "tokens": ["Die", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", ",", "Ra\u00b7ben", ",", "Eu\u00b7len\u00b7k\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "TRUNC", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Spie\u00dfb\u00fcrger alle, mit und ohne Z\u00f6pfe,", "tokens": ["Spie\u00df\u00b7b\u00fcr\u00b7ger", "al\u00b7le", ",", "mit", "und", "oh\u00b7ne", "Z\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "APPR", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was da klebt im z\u00e4hen Pech und Harz!", "tokens": ["Und", "was", "da", "klebt", "im", "z\u00e4\u00b7hen", "Pech", "und", "Harz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er hat sie drangsaliert und lie\u00df sie tanzen,", "tokens": ["Er", "hat", "sie", "drang\u00b7sa\u00b7lier