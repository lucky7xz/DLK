{"textgrid.poem.38673": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fr\u00e4ulein klagt' im finstern Thurm", "tokens": ["Ein", "Fr\u00e4u\u00b7lein", "klagt'", "im", "fins\u00b7tern", "Thurm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Seegestad' erbaut.", "tokens": ["Am", "See\u00b7ge\u00b7stad'", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es rauscht' und heulte Wog' und Sturm", "tokens": ["Es", "rauscht'", "und", "heul\u00b7te", "Wo\u00b7g'", "und", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In ihres Jammers Laut.", "tokens": ["In", "ih\u00b7res", "Jam\u00b7mers", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Rosalia von Montanvert", "tokens": ["Ro\u00b7sa\u00b7lia", "von", "Mon\u00b7tan\u00b7vert"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hie\u00df manchem Troubadour", "tokens": ["Hie\u00df", "man\u00b7chem", "Trou\u00b7ba\u00b7dour"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und einem ganzen Ritterheer", "tokens": ["Und", "ei\u00b7nem", "gan\u00b7zen", "Rit\u00b7ter\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Krone der Natur.", "tokens": ["Die", "Kro\u00b7ne", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch ehe noch ihr Herz die Macht", "tokens": ["Doch", "e\u00b7he", "noch", "ihr", "Herz", "die", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der s\u00fc\u00dfen Minn' empfand,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Minn'", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erlag der Vater in der Schlacht", "tokens": ["Er\u00b7lag", "der", "Va\u00b7ter", "in", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Sarazenenstrand.", "tokens": ["Am", "Sa\u00b7ra\u00b7ze\u00b7nen\u00b7strand", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Ohm, ein Ritter Manfry, ward", "tokens": ["Der", "Ohm", ",", "ein", "Rit\u00b7ter", "Manf\u00b7ry", ",", "ward"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "NE", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Schirmvogt ihr bestellt;", "tokens": ["Zum", "Schirm\u00b7vogt", "ihr", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem lacht' ins Herz, wie Felsen hart,", "tokens": ["Dem", "lacht'", "ins", "Herz", ",", "wie", "Fel\u00b7sen", "hart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Fr\u00e4uleins Gut und Geld.", "tokens": ["Des", "Fr\u00e4u\u00b7leins", "Gut", "und", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bald \u00fcberall im Lande ging", "tokens": ["Bald", "\u00fc\u00b7be\u00b7rall", "im", "Lan\u00b7de", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Trauerkund' umher:", "tokens": ["Die", "Trau\u00b7er\u00b7kund'", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdes Todes kalte Nacht umfieng", "tokens": ["\u00bb", "des", "To\u00b7des", "kal\u00b7te", "Nacht", "um\u00b7fi\u00b7eng"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Rose Montanvert.\u00ab", "tokens": ["Die", "Ro\u00b7se", "Mon\u00b7tan\u00b7vert", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein schwarzes Todtenf\u00e4hnlein wallt", "tokens": ["Ein", "schwar\u00b7zes", "Tod\u00b7ten\u00b7f\u00e4hn\u00b7lein", "wallt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch auf des Fr\u00e4uleins Burg;", "tokens": ["Hoch", "auf", "des", "Fr\u00e4u\u00b7leins", "Burg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die dumpfe Leichenglocke schallt", "tokens": ["Die", "dump\u00b7fe", "Lei\u00b7chen\u00b7glo\u00b7cke", "schallt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drei Tag' und N\u00e4cht' hindurch.", "tokens": ["Drei", "Tag'", "und", "N\u00e4cht'", "hin\u00b7durch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf ewig hin, auf ewig todt,", "tokens": ["Auf", "e\u00b7wig", "hin", ",", "auf", "e\u00b7wig", "todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PTKVZ", "$,", "APPR", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Rose Montanvert!", "tokens": ["O", "Ro\u00b7se", "Mon\u00b7tan\u00b7vert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun milderst du der Wittwe Noth,", "tokens": ["Nun", "mil\u00b7derst", "du", "der", "Witt\u00b7we", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Waise Schmerz nicht mehr.", "tokens": ["Der", "Wai\u00b7se", "Schmerz", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So klagt' einm\u00fcthig Alt und Jung,", "tokens": ["So", "klagt'", "ein\u00b7m\u00fct\u00b7hig", "Alt", "und", "Jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Blick von Thr\u00e4nen schwer,", "tokens": ["Den", "Blick", "von", "Thr\u00e4\u00b7nen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Fr\u00fchroth bis zur D\u00e4mmerung,", "tokens": ["Vom", "Fr\u00fch\u00b7roth", "bis", "zur", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die Rose Montanvert.", "tokens": ["Die", "Ro\u00b7se", "Mon\u00b7tan\u00b7vert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Ohm in einem Thurm sie barg", "tokens": ["Der", "Ohm", "in", "ei\u00b7nem", "Thurm", "sie", "barg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcllt mit Moderduft.", "tokens": ["Er\u00b7f\u00fcllt", "mit", "Mo\u00b7der\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drauf senkte man den leeren Sarg", "tokens": ["Drauf", "senk\u00b7te", "man", "den", "lee\u00b7ren", "Sarg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl in der V\u00e4ter Gruft.", "tokens": ["Wohl", "in", "der", "V\u00e4\u00b7ter", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das Fr\u00e4ulein horchte, still und bang,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "horch\u00b7te", ",", "still", "und", "bang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Priester Litaney'n;", "tokens": ["Der", "Pries\u00b7ter", "Li\u00b7ta\u00b7ney'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tr\u00fcb' in des Kerkers Gitter drang", "tokens": ["Tr\u00fcb'", "in", "des", "Ker\u00b7kers", "Git\u00b7ter", "drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fackeln rother Schein.", "tokens": ["Der", "Fa\u00b7ckeln", "ro\u00b7ther", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie ahnte schaudernd ihr Geschick,", "tokens": ["Sie", "ahn\u00b7te", "schau\u00b7dernd", "ihr", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr ward so dumpf und schwer;", "tokens": ["Ihr", "ward", "so", "dumpf", "und", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Todesgraun erstarb ihr Blick,", "tokens": ["Im", "To\u00b7des\u00b7graun", "er\u00b7starb", "ihr", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sank und war nicht mehr.", "tokens": ["Sie", "sank", "und", "war", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Des Thurms Ruinen an der See", "tokens": ["Des", "Thurms", "Ru\u00b7i\u00b7nen", "an", "der", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind heute noch zuschaun.", "tokens": ["Sind", "heu\u00b7te", "noch", "zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Wandrer fa\u00dft in ihrer N\u00e4h'", "tokens": ["Den", "Wand\u00b7rer", "fa\u00dft", "in", "ih\u00b7rer", "N\u00e4h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wundersames Graun.", "tokens": ["Ein", "wun\u00b7der\u00b7sa\u00b7mes", "Graun", "."], "token_in