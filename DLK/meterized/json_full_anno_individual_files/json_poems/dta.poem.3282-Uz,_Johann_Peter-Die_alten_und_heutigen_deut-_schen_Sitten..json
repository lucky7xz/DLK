{"dta.poem.3282": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die alten und heutigen deut-  \n schen Sitten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wenig gleichen wir den Alten!", "tokens": ["Wie", "we\u00b7nig", "glei\u00b7chen", "wir", "den", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was wir f\u00fcr ungesittet halten,", "tokens": ["Was", "wir", "f\u00fcr", "un\u00b7ge\u00b7sit\u00b7tet", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen M\u00e4nnlichkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "M\u00e4nn\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur wenig \u00e4chte deutsche Br\u00e4uche", "tokens": ["Nur", "we\u00b7nig", "\u00e4ch\u00b7te", "deut\u00b7sche", "Br\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sind unverj\u00e4hrt im deutschen Reiche", "tokens": ["Sind", "un\u00b7ver\u00b7j\u00e4hrt", "im", "deut\u00b7schen", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPRART", "ADJA", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsrer Zeit.", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Zusammen kommen, um zu zechen,", "tokens": ["Zu\u00b7sam\u00b7men", "kom\u00b7men", ",", "um", "zu", "ze\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.2": {"text": "Bis alle Zungen stammelnd sprechen,", "tokens": ["Bis", "al\u00b7le", "Zun\u00b7gen", "stam\u00b7melnd", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Fr\u00f6hlichkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch schwingt bey manchem Freudenmahle", "tokens": ["Noch", "schwingt", "bey", "man\u00b7chem", "Freu\u00b7den\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ly\u00e4us drohende Pocale", "tokens": ["Ly\u00b7\u00e4us", "dro\u00b7hen\u00b7de", "Po\u00b7ca\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu unsrer Zeit.", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch Recht und Menschheit nicht verletzen,", "tokens": ["Doch", "Recht", "und", "Menschheit", "nicht", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auch bey ermangelnden Gesetzen,", "tokens": ["Auch", "bey", "er\u00b7man\u00b7geln\u00b7den", "Ge\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Billigkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Bil\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich finde mehr gelehrt Geschw\u00e4tze,", "tokens": ["Ich", "fin\u00b7de", "mehr", "ge\u00b7lehrt", "Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sehr wenig Tugend, viel Gesetze", "tokens": ["Sehr", "we\u00b7nig", "Tu\u00b7gend", ",", "viel", "Ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsrer Zeit.", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Da\u00df sich getreue Weiber funden,", "tokens": ["Da\u00df", "sich", "ge\u00b7treu\u00b7e", "Wei\u00b7ber", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die auch dem Golde widerstunden,", "tokens": ["Die", "auch", "dem", "Gol\u00b7de", "wi\u00b7der\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df keine Seltenheit.", "tokens": ["Hie\u00df", "kei\u00b7ne", "Sel\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man sagt, zur Schande karger Reichen,", "tokens": ["Man", "sagt", ",", "zur", "Schan\u00b7de", "kar\u00b7ger", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es geb auch etliche dergleichen", "tokens": ["Es", "geb", "auch", "et\u00b7li\u00b7che", "derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsrer Zeit.", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Doch auch, wann Reiz und Jugend bl\u00fchen,", "tokens": ["Doch", "auch", ",", "wann", "Reiz", "und", "Ju\u00b7gend", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Ku\u00df nichts wissen, ihm entfliehen,", "tokens": ["Vom", "Ku\u00df", "nichts", "wis\u00b7sen", ",", "ihm", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "VVINF", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Ehrbarkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Ehr\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ist nur eine Sch\u00e4fertugend", "tokens": ["Die", "ist", "nur", "ei\u00b7ne", "Sch\u00e4\u00b7fer\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und abgeschmackt an muntrer Jugend", "tokens": ["Und", "ab\u00b7ge\u00b7schmackt", "an", "mun\u00b7trer", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsrer Zeit.", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Da\u00df stets der k\u00fchne Junker jagte,", "tokens": ["Da\u00df", "stets", "der", "k\u00fch\u00b7ne", "Jun\u00b7ker", "jag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch eh es auf den Bergen tagte,", "tokens": ["Auch", "eh", "es", "auf", "den", "Ber\u00b7gen", "tag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Streitbarkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Streit\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch jagt und schmaust er um die Wette,", "tokens": ["Noch", "jagt", "und", "schmaust", "er", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Inde\u00df besorgt ein Freund sein Bette,", "tokens": ["In\u00b7de\u00df", "be\u00b7sorgt", "ein", "Freund", "sein", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsrer Zeit.", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Doch Ansehn und erhabne W\u00fcrden", "tokens": ["Doch", "An\u00b7sehn", "und", "er\u00b7hab\u00b7ne", "W\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur auf verdiente Schultern b\u00fcrden,", "tokens": ["Nur", "auf", "ver\u00b7dien\u00b7te", "Schul\u00b7tern", "b\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Schuldigkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Schul\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu Aemtern kann ein ieder kommen,", "tokens": ["Zu", "A\u00b7em\u00b7tern", "kann", "ein", "ie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "ART", "PIS", "VVINF", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Die W\u00fcrdigen blo\u00df ausgenommen,", "tokens": ["Die", "W\u00fcr\u00b7di\u00b7gen", "blo\u00df", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsrer Zeit.", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Die prophezeyenden Matronen", "tokens": ["Die", "pro\u00b7phe\u00b7ze\u00b7yen\u00b7den", "Mat\u00b7ro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "F\u00fcr ihre L\u00fcgen noch belohnen,", "tokens": ["F\u00fcr", "ih\u00b7re", "L\u00fc\u00b7gen", "noch", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen sehr gescheidt.", "tokens": ["Hie\u00df", "ih\u00b7nen", "sehr", "ge\u00b7scheidt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sagt, kluge Frauen! Zeichendeuter!", "tokens": ["Sagt", ",", "klu\u00b7ge", "Frau\u00b7en", "!", "Zei\u00b7chen\u00b7deu\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zige\u00fcner! sagt: sind wir gescheidter", "tokens": ["Zi\u00b7ge\u00b7\u00fc\u00b7ner", "!", "sagt", ":", "sind", "wir", "ge\u00b7scheid\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "$.", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zu unsrer Zeit?", "tokens": ["Zu", "uns\u00b7rer", "Zeit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Doch edler Vorzug grauer Alten!", "tokens": ["Doch", "ed\u00b7ler", "Vor\u00b7zug", "grau\u00b7er", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Treue, Wort und Bund zu halten,", "tokens": ["Die", "Treu\u00b7e", ",", "Wort", "und", "Bund", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Redlichkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Red\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die schlummert auf best\u00e4ubtem Boden,", "tokens": ["Die", "schlum\u00b7mert", "auf", "be\u00b7st\u00e4ub\u00b7tem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bey andern abgelebten Moden,", "tokens": ["Bey", "an\u00b7dern", "ab\u00b7ge\u00b7leb\u00b7ten", "Mo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsrer Zeit", "tokens": ["Zu", "uns\u00b7rer", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}}}}}