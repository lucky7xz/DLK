{"textgrid.poem.67188": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf Gefilden neues Entz\u00fccken keimt", "tokens": ["Wenn", "auf", "Ge\u00b7fil\u00b7den", "neu\u00b7es", "Ent\u00b7z\u00fc\u00b7cken", "keimt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sich die Ansicht wieder versch\u00f6nt und sich", "tokens": ["Und", "sich", "die", "An\u00b7sicht", "wie\u00b7der", "ver\u00b7sch\u00f6nt", "und", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN", "ADV", "VVPP", "KON", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An Bergen, wo die B\u00e4ume gr\u00fcnen,", "tokens": ["An", "Ber\u00b7gen", ",", "wo", "die", "B\u00e4u\u00b7me", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hellere L\u00fcfte, Gew\u00f6lke zeigen,", "tokens": ["Hel\u00b7le\u00b7re", "L\u00fcf\u00b7te", ",", "Ge\u00b7w\u00f6l\u00b7ke", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O! welche Freude haben die Menschen! froh", "tokens": ["O", "!", "wel\u00b7che", "Freu\u00b7de", "ha\u00b7ben", "die", "Men\u00b7schen", "!", "froh"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$.", "PWAT", "NN", "VAFIN", "ART", "NN", "$.", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gehn an Gestaden Einsame, Ruh und Lust", "tokens": ["Gehn", "an", "Ge\u00b7sta\u00b7den", "Ein\u00b7sa\u00b7me", ",", "Ruh", "und", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und Wonne der Gesundheit bl\u00fchet,", "tokens": ["Und", "Won\u00b7ne", "der", "Ge\u00b7sund\u00b7heit", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundliches Lachen ist auch nicht ferne.", "tokens": ["Freund\u00b7li\u00b7ches", "La\u00b7chen", "ist", "auch", "nicht", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn auf Gefilden neues Entz\u00fccken keimt", "tokens": ["Wenn", "auf", "Ge\u00b7fil\u00b7den", "neu\u00b7es", "Ent\u00b7z\u00fc\u00b7cken", "keimt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sich die Ansicht wieder versch\u00f6nt und sich", "tokens": ["Und", "sich", "die", "An\u00b7sicht", "wie\u00b7der", "ver\u00b7sch\u00f6nt", "und", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN", "ADV", "VVPP", "KON", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An Bergen, wo die B\u00e4ume gr\u00fcnen,", "tokens"