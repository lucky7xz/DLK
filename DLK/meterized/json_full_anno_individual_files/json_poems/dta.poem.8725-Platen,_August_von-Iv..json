{"dta.poem.8725": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und dieses ist das lezte Wort?", "tokens": ["Und", "die\u00b7ses", "ist", "das", "lez\u00b7te", "Wort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich lockt ein ferner Himmelsbogen,", "tokens": ["Dich", "lockt", "ein", "fer\u00b7ner", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es treibt dich in die Fremde fort?", "tokens": ["Es", "treibt", "dich", "in", "die", "Frem\u00b7de", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wird geliebt, wer liebt und bleibet,", "tokens": ["Doch", "wird", "ge\u00b7liebt", ",", "wer", "liebt", "und", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer flieht, verkannt; und glaube mir,", "tokens": ["Wer", "flieht", ",", "ver\u00b7kannt", ";", "und", "glau\u00b7be", "mir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVPP", "$.", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dich die Sehnsucht f\u00fcrder treibet,", "tokens": ["Wenn", "dich", "die", "Sehn\u00b7sucht", "f\u00fcr\u00b7der", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bleibt die Liebe hinter dir!", "tokens": ["So", "bleibt", "die", "Lie\u00b7be", "hin\u00b7ter", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mag umwuchern dich das sch\u00f6ne", "tokens": ["Und", "mag", "um\u00b7wu\u00b7chern", "dich", "das", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVFIN", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hesperien voll milder Au'n,", "tokens": ["Hes\u00b7pe\u00b7ri\u00b7en", "voll", "mil\u00b7der", "Au'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo findest du die deutschen T\u00f6ne?", "tokens": ["Wo", "fin\u00b7dest", "du", "die", "deut\u00b7schen", "T\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo findest du die deutschen Frau'n?", "tokens": ["Wo", "fin\u00b7dest", "du", "die", "deut\u00b7schen", "Frau'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}