{"textgrid.poem.40049": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie gl\u00fccklich ist der Mensch, den keine Menschen kennen,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00fccklich ist der Mensch, den keine Menschen kennen,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "ist", "der", "Mensch", ",", "den", "kei\u00b7ne", "Men\u00b7schen", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, mit sich selbst vergn\u00fcg't, in einem Winckel leb't!", "tokens": ["Der", ",", "mit", "sich", "selbst", "ver\u00b7gn\u00fcg't", ",", "in", "ei\u00b7nem", "Win\u00b7ckel", "le\u00b7b't", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Der dem gesch\u00e4tzten Nichts, das wir die Ehre nennen,", "tokens": ["Der", "dem", "ge\u00b7sch\u00e4tz\u00b7ten", "Nichts", ",", "das", "wir", "die", "Eh\u00b7re", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Hoffahrts-Rauch berauscht, niemahlen nachgestreb't,", "tokens": ["Vom", "Hof\u00b7fahrts\u00b7Rauch", "be\u00b7rauscht", ",", "nie\u00b7mah\u00b7len", "nach\u00b7ge\u00b7stre\u00b7b't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der keine Freude find't, als nur im freyen Leben,", "tokens": ["Der", "kei\u00b7ne", "Freu\u00b7de", "find't", ",", "als", "nur", "im", "frey\u00b7en", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "KOUS", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und niemand, als ihm selbst, von sich darf Rechnung geben.", "tokens": ["Und", "nie\u00b7mand", ",", "als", "ihm", "selbst", ",", "von", "sich", "darf", "Rech\u00b7nung", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "KOUS", "PPER", "ADV", "$,", "APPR", "PRF", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie gl\u00fccklich ist der Mensch, den keine Menschen kennen,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "ist", "der", "Mensch", ",", "den", "kei\u00b7ne", "Men\u00b7schen", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, mit sich selbst vergn\u00fcg't, in einem Winckel leb't!", "tokens": ["Der", ",", "mit", "sich", "selbst", "ver\u00b7gn\u00fcg't", ",", "in", "ei\u00b7nem", "Win\u00b7ckel", "le\u00b7b't", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": 