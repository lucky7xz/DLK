{"dta.poem.7920": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vorw\u00e4rts !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Vorw\u00e4rts! Fort und immerfort!", "tokens": ["Vor\u00b7w\u00e4rts", "!", "Fort", "und", "im\u00b7mer\u00b7fort", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ru\u00dfland rief das stolze Wort:", "tokens": ["Ru\u00df\u00b7land", "rief", "das", "stol\u00b7ze", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Preu\u00dfen h\u00f6rt das stolze Wort,", "tokens": ["Preu\u00b7\u00dfen", "h\u00f6rt", "das", "stol\u00b7ze", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt es gern und hallt es fort:", "tokens": ["H\u00f6rt", "es", "gern", "und", "hallt", "es", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Auf, gewalt\u2019ges Oesterreich!", "tokens": ["Auf", ",", "ge\u00b7walt'\u00b7ges", "O\u00b7es\u00b7ter\u00b7reich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vorw\u00e4rts! thu\u2019s den andern gleich!", "tokens": ["Vor\u00b7w\u00e4rts", "!", "thu's", "den", "an\u00b7dern", "gleich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Auf, du altes Sachsenland!", "tokens": ["Auf", ",", "du", "al\u00b7tes", "Sach\u00b7sen\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer vorw\u00e4rts, Hand in Hand!", "tokens": ["Im\u00b7mer", "vor\u00b7w\u00e4rts", ",", "Hand", "in", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Baiern, Hessen, schlaget ein!", "tokens": ["Bai\u00b7ern", ",", "Hes\u00b7sen", ",", "schla\u00b7get", "ein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwaben, Franken, vor zum Rhein!", "tokens": ["Schwa\u00b7ben", ",", "Fran\u00b7ken", ",", "vor", "zum", "Rhein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "APPRART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Vorw\u00e4rts, Holland, Niederland!", "tokens": ["Vor\u00b7w\u00e4rts", ",", "Hol\u00b7land", ",", "Nie\u00b7der\u00b7land", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch das Schwerdt in freier Hand,", "tokens": ["Hoch", "das", "Schwerdt", "in", "frei\u00b7er", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Gr\u00fc\u00df euch Gott, du Schweizerbund,", "tokens": ["Gr\u00fc\u00df", "euch", "Gott", ",", "du", "Schwei\u00b7zer\u00b7bund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Elsa\u00df, Lothringen, Burgund!", "tokens": ["El\u00b7sa\u00df", ",", "Loth\u00b7rin\u00b7gen", ",", "Bur\u00b7gund", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Vorw\u00e4rts, Spanien, Engelland!", "tokens": ["Vor\u00b7w\u00e4rts", ",", "Spa\u00b7ni\u00b7en", ",", "En\u00b7gel\u00b7land", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Reicht den Br\u00fcdern bald die Hand!", "tokens": ["Reicht", "den", "Br\u00fc\u00b7dern", "bald", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Vorw\u00e4rts, fort und immer fort!", "tokens": ["Vor\u00b7w\u00e4rts", ",", "fort", "und", "im\u00b7mer", "fort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "KON", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Guter Wind und naher Port!", "tokens": ["Gu\u00b7ter", "Wind", "und", "na\u00b7her", "Port", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Vorw&#228;rts hei\u00dft ein Feldmarschall.", "tokens": ["Vorw", "&#228;", "rts", "hei\u00dft", "ein", "Feld\u00b7mar\u00b7schall", "."], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorw\u00e4rts, tapfre Streiter all!", "tokens": ["Vor\u00b7w\u00e4rts", ",", "tapf\u00b7re", "Strei\u00b7ter", "all", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vorw\u00e4rts!", "tokens": ["Vor\u00b7w\u00e4rts", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}