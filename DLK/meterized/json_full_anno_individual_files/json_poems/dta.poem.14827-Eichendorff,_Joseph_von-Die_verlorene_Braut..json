{"dta.poem.14827": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die verlorene Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr schauert's durch die Glieder:", "tokens": ["Ihr", "schau\u00b7ert's", "durch", "die", "Glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201edu bist mein todtes Kind!", "tokens": ["\u201e", "du", "bist", "mein", "tod\u00b7tes", "Kind", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie funkeln die Sterne nieder,", "tokens": ["Wie", "fun\u00b7keln", "die", "Ster\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jetzt wei\u00df ich, wo wir sind.\u201c \u2014", "tokens": ["Jetzt", "wei\u00df", "ich", ",", "wo", "wir", "sind", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}