{"dta.poem.19602": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Goldtuch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eihr M\u00e4gde, schaut, was ihr im Schreine habt!", "tokens": ["\u201e", "ihr", "M\u00e4g\u00b7de", ",", "schaut", ",", "was", "ihr", "im", "Schrei\u00b7ne", "habt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht darfst du mir von hinnen unbegabt,", "tokens": ["Nicht", "darfst", "du", "mir", "von", "hin\u00b7nen", "un\u00b7be\u00b7gabt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein blonder Enkel, der der Ahne bot", "tokens": ["Mein", "blon\u00b7der", "En\u00b7kel", ",", "der", "der", "Ah\u00b7ne", "bot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit priesterlichen H\u00e4nden Gott im Brot!\u201c", "tokens": ["Mit", "pries\u00b7ter\u00b7li\u00b7chen", "H\u00e4n\u00b7den", "Gott", "im", "Brot", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mathilde sprach's die F\u00fcrstin, sterbeschwach.", "tokens": ["Ma\u00b7thil\u00b7de", "sprach's", "die", "F\u00fcrs\u00b7tin", ",", "ster\u00b7be\u00b7schwach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Richburg die Schaffnerin seufzt' Weh und Ach!", "tokens": ["Rich\u00b7burg", "die", "Schaff\u00b7ne\u00b7rin", "seufzt'", "Weh", "und", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u201ehin gabst den Armen Alles du! Allein", "tokens": ["\u201e", "hin", "gabst", "den", "Ar\u00b7men", "Al\u00b7les", "du", "!", "Al\u00b7lein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "PIS", "PPER", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein goldgewoben Bahrtuch liegt im Schrein!\u201c", "tokens": ["Dein", "gold\u00b7ge\u00b7wo\u00b7ben", "Bahr\u00b7tuch", "liegt", "im", "Schrein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u2014 \u201eDie goldne Decke! Gebt dem Bischof die!", "tokens": ["\u201e", "Die", "gold\u00b7ne", "De\u00b7cke", "!", "Gebt", "dem", "Bi\u00b7schof", "die", "!"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ART", "ADJA", "NN", "$.", "VVIMP", "ART", "NN", "ART", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Brauthemd und Bahrtuch fehlt den Frauen nie!\u201c ...", "tokens": ["Brau\u00b7themd", "und", "Bahr\u00b7tuch", "fehlt", "den", "Frau\u00b7en", "nie", "!", "\u201c", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der J\u00fcngling zaudert ... \u201eNimm die Decke! Kr\u00e4nk'", "tokens": ["Der", "J\u00fcng\u00b7ling", "zau\u00b7dert", "...", "\u201e", "Nimm", "die", "De\u00b7cke", "!", "Kr\u00e4nk'"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "$(", "NE", "ART", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich nicht!\u201c Er nimmt und zieht mit dem Geschenk.", "tokens": ["Mich", "nicht", "!", "\u201c", "Er", "nimmt", "und", "zieht", "mit", "dem", "Ge\u00b7schenk", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "$(", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie athmet aus. Es l\u00e4utet lang und sch\u00f6n", "tokens": ["Sie", "ath\u00b7met", "aus", ".", "Es", "l\u00e4u\u00b7tet", "lang", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit allen Glocken von des M\u00fcnsters H\u00f6hn.", "tokens": ["Mit", "al\u00b7len", "Glo\u00b7cken", "von", "des", "M\u00fcns\u00b7ters", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was wandert dort im letzten Sonnenblick?", "tokens": ["Was", "wan\u00b7dert", "dort", "im", "letz\u00b7ten", "Son\u00b7nen\u00b7blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mathildens Bahrtuch kehrt zu ihr zur\u00fcck.", "tokens": ["Ma\u00b7thil\u00b7dens", "Bahr\u00b7tuch", "kehrt", "zu", "ihr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Abspringt ein Reiter, der den Thurm ersteigt.", "tokens": ["Ab\u00b7springt", "ein", "Rei\u00b7ter", ",", "der", "den", "Thurm", "er\u00b7steigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u201eden Bischof warf das Ro\u00df. Ein Todter schweigt.", "tokens": ["\u201e", "den", "Bi\u00b7schof", "warf", "das", "Ro\u00df", ".", "Ein", "Tod\u00b7ter", "schweigt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir bringen ihn! Verdoppelt das Gel\u00e4ut!", "tokens": ["Wir", "brin\u00b7gen", "ihn", "!", "Ver\u00b7dop\u00b7pelt", "das", "Ge\u00b7l\u00e4ut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Gl\u00f6ckner, zwier bekommt ihr L\u00f6hnung heut!\u201c", "tokens": ["Ihr", "Gl\u00f6ck\u00b7ner", ",", "zwier", "be\u00b7kommt", "ihr", "L\u00f6h\u00b7nung", "heut", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}