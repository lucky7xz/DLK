{"textgrid.poem.24177": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Meinen werten Feinden", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Feinde haben mich weise gemacht,", "tokens": ["Die", "Fein\u00b7de", "ha\u00b7ben", "mich", "wei\u00b7se", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013 Die guten Feinde!", "tokens": ["\u2013", "Die", "gu\u00b7ten", "Fein\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Erst hab ich gebrummt, dann hab ich gelacht", "tokens": ["Erst", "hab", "ich", "ge\u00b7brummt", ",", "dann", "hab", "ich", "ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der grimmen Gemeinde.", "tokens": ["Der", "grim\u00b7men", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie haben mir, was ich bin, gezeigt,", "tokens": ["Sie", "ha\u00b7ben", "mir", ",", "was", "ich", "bin", ",", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013 Die lieben Leute!", "t