{"dta.poem.1710": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Drey K\u00f6nge zahlen mir verdienten Mutter-Sold;", "tokens": ["Drey", "K\u00f6n\u00b7ge", "zah\u00b7len", "mir", "ver\u00b7dien\u00b7ten", "Mut\u00b7ter\u00b7Sold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Spanien regiert/ giebt Indianisch Gold/", "tokens": ["Der", "Spa\u00b7ni\u00b7en", "re\u00b7giert", "/", "giebt", "In\u00b7di\u00b7a\u00b7nisch", "Gold", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$(", "VVFIN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Weyhrauch schickt der Herr der Britten/ Schotten/", "tokens": ["Den", "Wey\u00b7hrauch", "schickt", "der", "Herr", "der", "Brit\u00b7ten", "/", "Schot\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Irren/", "tokens": ["Ir\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Aus Franckreich kommen mir des Sohnes herbe Myrrhen.", "tokens": ["Aus", "Fran\u00b7ck\u00b7reich", "kom\u00b7men", "mir", "des", "Soh\u00b7nes", "her\u00b7be", "Myr\u00b7rhen", "."], "token_info": ["word", "word", "word