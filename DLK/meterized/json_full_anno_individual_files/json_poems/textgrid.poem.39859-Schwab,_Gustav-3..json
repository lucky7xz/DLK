{"textgrid.poem.39859": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Mond, wie leget sich so sch\u00f6n und breit,", "tokens": ["O", "Mond", ",", "wie", "le\u00b7get", "sich", "so", "sch\u00f6n", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel weicher als auf Gassen und Pall\u00e4ste,", "tokens": ["Viel", "wei\u00b7cher", "als", "auf", "Gas\u00b7sen", "und", "Pal\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um diese Berge, diese vollen Aeste,", "tokens": ["Um", "die\u00b7se", "Ber\u00b7ge", ",", "die\u00b7se", "vol\u00b7len", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf dieses Gras dein lichtgesponnen Kleid!", "tokens": ["Auf", "die\u00b7ses", "Gras", "dein", "licht\u00b7ge\u00b7spon\u00b7nen", "Kleid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Mond, o Sonne der Vergangenheit!", "tokens": ["O", "Mond", ",", "o", "Son\u00b7ne", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie dringst du auch in meines Busens Veste,", "tokens": ["Wie", "dringst", "du", "auch", "in", "mei\u00b7nes", "Bu\u00b7sens", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie wirfst du Glanz und Schatten auf die Reste", "tokens": ["Wie", "wirfst", "du", "Glanz", "und", "Schat\u00b7ten", "auf", "die", "Res\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Lebenstr\u00e4umen ferner J\u00fcnglingszeit.", "tokens": ["Von", "Le\u00b7ben\u00b7str\u00e4u\u00b7men", "fer\u00b7ner", "J\u00fcng\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Aus diesen Tr\u00fcmmern hebt sich leis' empor", "tokens": ["Aus", "die\u00b7sen", "Tr\u00fcm\u00b7mern", "hebt", "sich", "leis'", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Stral der Nacht ein Lilienangesicht", "tokens": ["Im", "Stral", "der", "Nacht", "ein", "Li\u00b7li\u00b7en\u00b7an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit blauer Augen