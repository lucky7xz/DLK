{"dta.poem.18558": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A ls er bey ihr zu gaste ge-  \n wesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6ne Margaris! ich bin dir schlecht verbunden/", "tokens": ["Du", "sch\u00f6\u00b7ne", "Mar\u00b7ga\u00b7ris", "!", "ich", "bin", "dir", "schlecht", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$.", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob du mich gestern gleich so wohl bewirthet hast/", "tokens": ["Ob", "du", "mich", "ge\u00b7stern", "gleich", "so", "wohl", "be\u00b7wirt\u00b7het", "hast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ob ich der G\u00f6tter Kost an deinen Tisch gefunden/", "tokens": ["Ob", "ich", "der", "G\u00f6t\u00b7ter", "Kost", "an", "dei\u00b7nen", "Tisch", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und war ich gleich bey dir ein angenehmer Gast.", "tokens": ["Und", "war", "ich", "gleich", "bey", "dir", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe zwar die Lust in Uberflu\u00df geschmecket/", "tokens": ["Ich", "ha\u00b7be", "zwar", "die", "Lust", "in", "U\u00b7ber\u00b7flu\u00df", "ge\u00b7schme\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So lange noch mein Aug' an deinen Lippen hing:", "tokens": ["So", "lan\u00b7ge", "noch", "mein", "Aug'", "an", "dei\u00b7nen", "Lip\u00b7pen", "hing", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch nur der Abschied hat mich ungemein erschrecket/", "tokens": ["Doch", "nur", "der", "Ab\u00b7schied", "hat", "mich", "un\u00b7ge\u00b7mein", "er\u00b7schre\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich wei\u00df nicht/ wie der Schmertz mit mir zur\u00fccke gieng.", "tokens": ["Ich", "wei\u00df", "nicht", "/", "wie", "der", "Schmertz", "mit", "mir", "zu\u00b7r\u00fc\u00b7cke", "gieng", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOKOM", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach losse Margaris! ich kan dirs nicht verhehlen/", "tokens": ["Ach", "los\u00b7se", "Mar\u00b7ga\u00b7ris", "!", "ich", "kan", "dirs", "nicht", "ver\u00b7heh\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NE", "$.", "PPER", "VMFIN", "PIS", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch dencke/ da\u00df es dir mit nichten r\u00fchmlich steht/", "tokens": ["Doch", "den\u00b7cke", "/", "da\u00df", "es", "dir", "mit", "nich\u00b7ten", "r\u00fchm\u00b7lich", "steht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "PPER", "APPR", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So offt man einen Gast zuletzte will bestehlen/", "tokens": ["So", "offt", "man", "ei\u00b7nen", "Gast", "zu\u00b7letz\u00b7te", "will", "be\u00b7steh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ART", "NN", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und da\u00df er reichlich kommt/ und arm zur\u00fccke geht.", "tokens": ["Und", "da\u00df", "er", "reich\u00b7lich", "kommt", "/", "und", "arm", "zu\u00b7r\u00fc\u00b7cke", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$(", "KON", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den Diebstahl haben nun die wundersch\u00f6nen Wangen", "tokens": ["Den", "Dieb\u00b7stahl", "ha\u00b7ben", "nun", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deiner Augenstrahl an meiner Brust begangen.", "tokens": ["Und", "dei\u00b7ner", "Au\u00b7gen\u00b7strahl", "an", "mei\u00b7ner", "Brust", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}