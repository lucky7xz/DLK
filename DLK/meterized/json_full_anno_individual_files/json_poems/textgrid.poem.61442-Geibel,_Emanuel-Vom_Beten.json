{"textgrid.poem.61442": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Vom Beten", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, du magst nicht beten, denn es sei", "tokens": ["Du", "sagst", ",", "du", "magst", "nicht", "be\u00b7ten", ",", "denn", "es", "sei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch alles vorbestimmt. \u2013 Wie? Ist dein Gott", "tokens": ["Doch", "al\u00b7les", "vor\u00b7be\u00b7stimmt", ".", "\u2013", "Wie", "?", "Ist", "dein", "Gott"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "PWAV", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn schon gestorben, seine heil'ge Vorsicht", "tokens": ["Denn", "schon", "ge\u00b7stor\u00b7ben", ",", "sei\u00b7ne", "heil'\u00b7ge", "Vor\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein blo\u00dfes Uhrwerk, das an F\u00e4den schnurrt,", "tokens": ["Ein", "blo\u00b7\u00dfes", "Uhr\u00b7werk", ",", "das", "an", "F\u00e4\u00b7den", "schnurrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der tote Nachla\u00df eines gro\u00dfen K\u00fcnstlers?", "tokens": ["Der", "to\u00b7te", "Nachl\u00b7a\u00df", "ei\u00b7nes", "gro\u00b7\u00dfen", "K\u00fcnst\u00b7lers", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist er nicht heut noch da und webt und schafft", "tokens": ["Ist", "er", "nicht", "heut", "noch", "da", "und", "webt", "und", "schafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADV", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Am nimmer fert'gen Werk? Gibt dieser Duft", "tokens": ["Am", "nim\u00b7mer", "fert'\u00b7gen", "Werk", "?", "Gibt", "die\u00b7ser", "Duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADV", "ADJA", "NN", "$.", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von jungen Rosen, der durchs Fenster quillt,", "tokens": ["Von", "jun\u00b7gen", "Ro\u00b7sen", ",", "der", "durchs", "Fens\u00b7ter", "quillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nicht holde B\u00fcrgschaft seiner Gegenwart,", "tokens": ["Nicht", "hol\u00b7de", "B\u00fcrg\u00b7schaft", "sei\u00b7ner", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und da\u00df er lebt und liebt? Und ", "tokens": ["Und", "da\u00df", "er", "lebt", "und", "liebt", "?", "Und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER"