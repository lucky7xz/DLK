{"dta.poem.12634": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.  \n  Die erste Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O wie viel Leben, wie viel Zeit", "tokens": ["O", "wie", "viel", "Le\u00b7ben", ",", "wie", "viel", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hab ich, als kaum beseelt, verlohren,", "tokens": ["Hab", "ich", ",", "als", "kaum", "be\u00b7seelt", ",", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh mich die Gunst der Z\u00e4rtlichkeit", "tokens": ["Eh", "mich", "die", "Gunst", "der", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begeistert und f\u00fcr dich erkohren!", "tokens": ["Be\u00b7geis\u00b7tert", "und", "f\u00fcr", "dich", "er\u00b7koh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun mich dein s\u00fcsser Ku\u00df erfreut,", "tokens": ["Nun", "mich", "dein", "s\u00fcs\u00b7ser", "Ku\u00df", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O nun belebt sich meine Zeit!", "tokens": ["O", "nun", "be\u00b7lebt", "sich", "mei\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun bin ich erst gebohren!", "tokens": ["Nun", "bin", "ich", "erst", "ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}