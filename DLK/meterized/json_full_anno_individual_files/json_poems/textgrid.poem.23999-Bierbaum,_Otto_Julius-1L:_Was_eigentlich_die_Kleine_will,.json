{"textgrid.poem.23999": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was eigentlich die Kleine will,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was eigentlich die Kleine will,", "tokens": ["Was", "ei\u00b7gent\u00b7lich", "die", "Klei\u00b7ne", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "VMFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Das mag der Teufel wissen!", "tokens": ["Das", "mag", "der", "Teu\u00b7fel", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald guckt sie mich gar gl\u00fchend an,", "tokens": ["Bald", "guckt", "sie", "mich", "gar", "gl\u00fc\u00b7hend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r sie hingerissen.", "tokens": ["Als", "w\u00e4r", "sie", "hin\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wovon? Wozu? Ich ahn es nicht;", "tokens": ["Wo\u00b7von", "?", "Wo\u00b7zu", "?", "Ich", "ahn", "es", "nicht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Teufel mag es wissen.", "tokens": ["Der", "Teu\u00b7fel", "mag", "es", "wis\u00b7sen", "."]