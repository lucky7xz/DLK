{"textgrid.poem.53878": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Pfeifen anrauchen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das tut sich wohl des \u00f6ftern begeben:", "tokens": ["Das", "tut", "sich", "wohl", "des", "\u00f6f\u00b7tern", "be\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mal beginnt jeder sein ganzes Leben", "tokens": ["Mal", "be\u00b7ginnt", "je\u00b7der", "sein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "von neuem. Wirft hin, was er nur kann,", "tokens": ["von", "neu\u00b7em", ".", "Wirft", "hin", ",", "was", "er", "nur", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$.", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und f\u00e4ngt alles wieder von vorne an,", "tokens": ["und", "f\u00e4ngt", "al\u00b7les", "wie\u00b7der", "von", "vor\u00b7ne", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "mit g\u00e4nzlich neuer Melodie . . .", "tokens": ["mit", "g\u00e4nz\u00b7lich", "neu\u00b7er", "Me\u00b7lo\u00b7die", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Franzosen nennens \u203arefaire sa vie\u2039.", "tokens": ["Die", "Fran\u00b7zo\u00b7sen", "nen\u00b7nens", "\u203a", "re\u00b7fai\u00b7re", "sa", "vie", "\u2039", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Refaire sa vie . . . das ist gar nicht einfach.", "tokens": ["Re\u00b7fai\u00b7re", "sa", "vie", ".", ".", ".", "das", "ist", "gar", "nicht", "ein\u00b7fach", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$.", "$.", "PDS", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Refaire sa vie . . . ist leider mein Fach.", "tokens": ["Re\u00b7fai\u00b7re", "sa", "vie", ".", ".", ".", "ist", "lei\u00b7der", "mein", "Fach", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$.", "$.", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dazu sind wir zu gebrauchen . . .", "tokens": ["Da\u00b7zu", "sind", "wir", "zu", "ge\u00b7brau\u00b7chen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$.", "$.", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Refaire sa vie \u2013 ist wie Pfeifen anrauchen.", "tokens": ["Re\u00b7fai\u00b7re", "sa", "vie", "\u2013", "ist", "wie", "Pfei\u00b7fen", "an\u00b7rau\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "VAFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Du glaubst erst gar nicht, da\u00df es sich lohnt.", "tokens": ["Du", "glaubst", "erst", "gar", "nicht", ",", "da\u00df", "es", "sich", "lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Tabak schmeckt schwer und ungewohnt \u2013", "tokens": ["Der", "Ta\u00b7bak", "schmeckt", "schwer", "und", "un\u00b7ge\u00b7wohnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "es legt sich das Nikotin auf den Magen,", "tokens": ["es", "legt", "sich", "das", "Ni\u00b7ko\u00b7tin", "auf", "den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "du hast \u00fcber Seelen- und Bauchweh zu klagen;", "tokens": ["du", "hast", "\u00fc\u00b7ber", "See\u00b7len", "und", "Bauch\u00b7weh", "zu", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "TRUNC", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "das macht:", "tokens": ["das", "macht", ":"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "das Ding ist nicht abgenutzt,", "tokens": ["das", "Ding", "ist", "nicht", "ab\u00b7ge\u00b7nutzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "und die Pfeife ist viel zu wenig verschmutzt.", "tokens": ["und", "die", "Pfei\u00b7fe", "ist", "viel", "zu", "we\u00b7nig", "ver\u00b7schmutzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Aber so eine zwei, drei Jahr \u2013", "tokens": ["A\u00b7ber", "so", "ei\u00b7ne", "zwei", ",", "drei", "Jahr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "CARD", "$,", "CARD", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "da schmeckt die Pfeife wunderbar.", "tokens": ["da", "schmeckt", "die", "Pfei\u00b7fe", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Hals ist dir so vertraut gebogen,", "tokens": ["Ihr", "Hals", "ist", "dir", "so", "ver\u00b7traut", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "das Holz ist voller Tabak gesogen", "tokens": ["das", "Holz", "ist", "vol\u00b7ler", "Ta\u00b7bak", "ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "bis zur letzten Faser. Und du kratzst nichts ab.", "tokens": ["bis", "zur", "letz\u00b7ten", "Fa\u00b7ser", ".", "Und", "du", "kratz\u00b7st", "nichts", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Diese Pfeife nimmst du ins Grab . . .", "tokens": ["Die\u00b7se", "Pfei\u00b7fe", "nimmst", "du", "ins", "Grab", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "$.", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Bis zur n\u00e4chsten. Bis zur n\u00e4chsten Ecke.", "tokens": ["Bis", "zur", "n\u00e4chs\u00b7ten", ".", "Bis", "zur", "n\u00e4chs\u00b7ten", "E\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "$.", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Da krauchst du hervor aus deinem Verstecke,", "tokens": ["Da", "krauchst", "du", "her\u00b7vor", "aus", "dei\u00b7nem", "Ver\u00b7ste\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "der Boden bekommt eine neue Schichtung,", "tokens": ["der", "Bo\u00b7den", "be\u00b7kommt", "ei\u00b7ne", "neu\u00b7e", "Schich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "das Leben nimmt eine andere Richtung \u2013", "tokens": ["das", "Le\u00b7ben", "nimmt", "ei\u00b7ne", "an\u00b7de\u00b7re", "Rich\u00b7tung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und du bist ein Kerl und ganzer", "tokens": ["Und", "du", "bist", "ein", "Kerl", "und", "gan\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mann und steckst eine neue Pfeife an.", "tokens": ["Mann", "und", "steckst", "ei\u00b7ne", "neu\u00b7e", "Pfei\u00b7fe", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Wenn du einmal am Ende stehst,", "tokens": ["Wenn", "du", "ein\u00b7mal", "am", "En\u00b7de", "stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn du die letzte Wende gehst,", "tokens": ["wenn", "du", "die", "letz\u00b7te", "Wen\u00b7de", "gehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wenn du dann klug bist, blickst du zur\u00fcck,", "tokens": ["wenn", "du", "dann", "klug", "bist", ",", "blickst", "du", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "auf das ganze geschl\u00e4ngelte St\u00fcck.", "tokens": ["auf", "das", "gan\u00b7ze", "ge\u00b7schl\u00e4n\u00b7gel\u00b7te", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "So viel Pfeifen! Viel \u00c4nderungen!", "tokens": ["So", "viel", "Pfei\u00b7fen", "!", "Viel", "\u00c4n\u00b7de\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "so oft hast du eine neue geschwungen!", "tokens": ["so", "oft", "hast", "du", "ei\u00b7ne", "neu\u00b7e", "ge\u00b7schwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und hat die Neue gen\u00fctzt?", "tokens": ["Und", "hat", "die", "Neu\u00b7e", "ge\u00b7n\u00fctzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Seife.", "tokens": ["Sei\u00b7fe", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Es war immer dieselbe Pfeife.", "tokens": ["Es", "war", "im\u00b7mer", "die\u00b7sel\u00b7be", "Pfei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PDAT", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Das tut sich wohl des \u00f6ftern begeben:", "tokens": ["Das", "tut", "sich", "wohl", "de