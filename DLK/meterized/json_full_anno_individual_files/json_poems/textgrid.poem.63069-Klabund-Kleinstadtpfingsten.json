{"textgrid.poem.63069": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Kleinstadtpfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um eine sch\u00f6ne Pfingststimmung zu bewirken,", "tokens": ["Um", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Pfingsts\u00b7tim\u00b7mung", "zu", "be\u00b7wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stellt man in den kleinen St\u00e4dten Birken", "tokens": ["Stellt", "man", "in", "den", "klei\u00b7nen", "St\u00e4d\u00b7ten", "Bir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vor die T\u00fcr. Und am Vorabend singen", "tokens": ["Vor", "die", "T\u00fcr", ".", "Und", "am", "Vor\u00b7a\u00b7bend", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "KON", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die M\u00e4dchen s\u00fc\u00dfsonderbare Lieder, die den Sommer herbeizwingen Sollen.", "tokens": ["Die", "M\u00e4d\u00b7chen", "s\u00fc\u00df\u00b7son\u00b7der\u00b7ba\u00b7re", "Lie\u00b7der", ",", "die", "den", "Som\u00b7mer", "her\u00b7bei\u00b7zwin\u00b7gen", "Sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-++-+-+-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.5": {"text": "Die Buben zwitschern auf ihren Kalmusstauden wie Nachtigallen.", "tokens": ["Die", "Bu\u00b7ben", "zwit\u00b7schern", "auf", "ih\u00b7ren", "Kal\u00b7mus\u00b7stau\u00b7den", "wie", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+--+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Aber vor allen", "tokens": ["A\u00b7ber", "vor", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "PIAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dingen verge\u00dft", "tokens": ["Din\u00b7gen", "ver\u00b7ge\u00dft"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Nicht: wir feiern Pfingsten das Sch\u00fctzenfest.", "tokens": ["Nicht", ":", "wir", "fei\u00b7ern", "Pfings\u00b7ten", "das", "Sch\u00fct\u00b7zen\u00b7fest", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$.", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "In grasgr\u00fcner Uniform wie die F\u00f6rster, mit Fahnen, Fl\u00f6ten, Pauken, und unter Applaus", "tokens": ["In", "gras\u00b7gr\u00fc\u00b7ner", "U\u00b7ni\u00b7form", "wie", "die", "F\u00f6rs\u00b7ter", ",", "mit", "Fah\u00b7nen", ",", "Fl\u00f6\u00b7ten", ",", "Pau\u00b7ken", ",", "und", "un\u00b7ter", "Ap\u00b7plaus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "ART", "NN", "$,", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "APPR", "NN"], "meter": "-+--+-+--+--+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Des Publikums, marschiert die Sch\u00fctzengilde (63 Mann) zum Sch\u00fctzenhaus.", "tokens": ["Des", "Pub\u00b7li\u00b7kums", ",", "mar\u00b7schiert", "die", "Sch\u00fct\u00b7zen\u00b7gil\u00b7de", "(", "63", "Mann", ")", "zum", "Sch\u00fct\u00b7zen\u00b7haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "number", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "$(", "CARD", "NN", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Mein Vater ist Sch\u00fctzenmajor \u2013 er tr\u00e4gt einen Ehrendegen", "tokens": ["Mein", "Va\u00b7ter", "ist", "Sch\u00fct\u00b7zen\u00b7ma\u00b7jor", "\u2013", "er", "tr\u00e4gt", "ei\u00b7nen", "Eh\u00b7ren\u00b7de\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-++-+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Und mu\u00df an solchem Fest- und Ehrentage auch seinen Kronenorden vierter Klasse anlegen,", "tokens": ["Und", "mu\u00df", "an", "sol\u00b7chem", "Fest", "und", "Eh\u00b7ren\u00b7ta\u00b7ge", "auch", "sei\u00b7nen", "Kro\u00b7ne\u00b7nor\u00b7den", "vier\u00b7ter", "Klas\u00b7se", "an\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "TRUNC", "KON", "NN", "ADV", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.13": {"text": "Sowie die hohenzollern-sigmaringsche Verdienstmedaille. \u2013", "tokens": ["So\u00b7wie", "die", "ho\u00b7hen\u00b7zol\u00b7lern\u00b7sig\u00b7ma\u00b7ring\u00b7sche", "Ver\u00b7dienst\u00b7me\u00b7dail\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Die M\u00e4dchen gehen alle schon in wei\u00dfer Taille,", "tokens": ["Die", "M\u00e4d\u00b7chen", "ge\u00b7hen", "al\u00b7le", "schon", "in", "wei\u00b7\u00dfer", "Tail\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und am Abend tanzt man im Sch\u00fctzenhaussaal bis zum Verr\u00fccktwerden...", "tokens": ["Und", "am", "A\u00b7bend", "tanzt", "man", "im", "Sch\u00fct\u00b7zen\u00b7haus\u00b7saal", "bis", "zum", "Ver\u00b7r\u00fcckt\u00b7wer\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PIS", "APPRART", "NN", "APPR", "APPRART", "NN", "$("], "meter": "--+-+--+-++-+-+--", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Dann drau\u00dfen unter den B\u00e4umen... im Grase... von deinem Munde begl\u00fcckt werden.", "tokens": ["Dann", "drau\u00b7\u00dfen", "un\u00b7ter", "den", "B\u00e4u\u00b7men", "...", "im", "Gra\u00b7se", "...", "von", "dei\u00b7nem", "Mun\u00b7de", "be\u00b7gl\u00fcckt", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$(", "APPRART", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+--+--+--+-+--++-", "measure": "iambic.octa.plus.relaxed"}, "line.17": {"text": "... K\u00fcsse... Musik von ferne.. am Abendhimmel die Venus glei\u00dft...", "tokens": ["...", "K\u00fcs\u00b7se", "...", "Mu\u00b7sik", "von", "fer\u00b7ne", "..", "am", "A\u00b7bend\u00b7him\u00b7mel", "die", "Ve\u00b7nus", "glei\u00dft", "..."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "NN", "APPR", "ADJA", "$(", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Und wir reden jauchzend irr mit fremden Zungen,", "tokens": ["Und", "wir", "re\u00b7den", "jauch\u00b7zend", "irr", "mit", "frem\u00b7den", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVPP", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Unsere Herzen sind wie Bl\u00fcten aufgesprungen,", "tokens": ["Un\u00b7se\u00b7re", "Her\u00b7zen", "sind", "wie", "Bl\u00fc\u00b7ten", "auf\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Nieder fuhr durchs Dunkel wie ein Blitz singend der heilige Geist...", "tokens": ["Nie\u00b7der", "fuhr", "durchs", "Dun\u00b7kel", "wie", "ein", "Blitz", "sin\u00b7gend", "der", "hei\u00b7li\u00b7ge", "Geist", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "KOKOM", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+---+--+", "measure": "iambic.septa.chol"}}, "stanza.2": {"line.1": {"text": "Um eine sch\u00f6ne Pfingststimmung zu bewirken,", "tokens": ["Um", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Pfingsts\u00b7tim\u00b7mung", "zu", "be\u00b7wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text"