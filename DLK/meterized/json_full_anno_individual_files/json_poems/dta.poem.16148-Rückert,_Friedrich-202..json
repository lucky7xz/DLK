{"dta.poem.16148": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "202.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Sinnbild des Vereins der Schale mit dem Kerne", "tokens": ["Ein", "Sinn\u00b7bild", "des", "Ver\u00b7eins", "der", "Scha\u00b7le", "mit", "dem", "Ker\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist die Vereinigung des Lichts und der Laterne.", "tokens": ["Ist", "die", "Ver\u00b7ei\u00b7ni\u00b7gung", "des", "Lichts", "und", "der", "La\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer die Laterne tr\u00e4gt, und hat kein Licht darinn,", "tokens": ["Wer", "die", "La\u00b7ter\u00b7ne", "tr\u00e4gt", ",", "und", "hat", "kein", "Licht", "da\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "KON", "VAFIN", "PIAT", "NN", "PAV", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Davon hat weder er noch irgendwer Gewinn.", "tokens": ["Da\u00b7von", "hat", "we\u00b7der", "er", "noch", "ir\u00b7gend\u00b7wer", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "KON", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer offen tr\u00e4gt sein Licht, von keinem Schirm umwacht,", "tokens": ["Wer", "of\u00b7fen", "tr\u00e4gt", "sein", "Licht", ",", "von", "kei\u00b7nem", "Schirm", "um\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat unverl\u00e4ssiges Geleit bei wind'ger Nacht.", "tokens": ["Hat", "un\u00b7ver\u00b7l\u00e4s\u00b7si\u00b7ges", "Ge\u00b7leit", "bei", "win\u00b7d'\u00b7ger", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "Nur wem das Licht zugleich und die Latern' ist eigen,", "tokens": ["Nur", "wem", "das", "Licht", "zu\u00b7gleich", "und", "die", "La\u00b7tern'", "ist", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "ADV", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht selber seinen Weg, und kann ihn andern zeigen.", "tokens": ["Sieht", "sel\u00b7ber", "sei\u00b7nen", "Weg", ",", "und", "kann", "ihn", "an\u00b7dern", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}