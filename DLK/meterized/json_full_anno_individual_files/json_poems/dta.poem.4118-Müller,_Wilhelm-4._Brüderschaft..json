{"dta.poem.4118": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "4. Br\u00fcderschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da kehrt' ich durstig ein:", "tokens": ["Da", "kehrt'", "ich", "durs\u00b7tig", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da sa\u00df ein Wandrer drinnen", "tokens": ["Da", "sa\u00df", "ein", "Wand\u00b7rer", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Tisch bei k\u00fchlem Wein.", "tokens": ["Am", "Tisch", "bei", "k\u00fch\u00b7lem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Glas war eingegossen,", "tokens": ["Ein", "Glas", "war", "ein\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das wurde nimmer leer;", "tokens": ["Das", "wur\u00b7de", "nim\u00b7mer", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Haupt ruht' auf dem B\u00fcndel,", "tokens": ["Sein", "Haupt", "ruht'", "auf", "dem", "B\u00fcn\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4r's ihm viel zu schwer.", "tokens": ["Als", "w\u00e4r's", "ihm", "viel", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich th\u00e4t mich zu ihm setzen,", "tokens": ["Ich", "th\u00e4t", "mich", "zu", "ihm", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sah ihm in's Gesicht,", "tokens": ["Ich", "sah", "ihm", "in's", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das schien mir gar befreundet,", "tokens": ["Das", "schien", "mir", "gar", "be\u00b7freun\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dennoch kannt' ich's nicht.", "tokens": ["Und", "den\u00b7noch", "kannt'", "ich's", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da sah auch mir in's Auge", "tokens": ["Da", "sah", "auch", "mir", "in's", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der fremde Wandersmann,", "tokens": ["Der", "frem\u00b7de", "Wan\u00b7ders\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und f\u00fcllte meinen Becher,", "tokens": ["Und", "f\u00fcll\u00b7te", "mei\u00b7nen", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sah mich wieder an.", "tokens": ["Und", "sah", "mich", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hei, was die Becher klangen,", "tokens": ["Hei", ",", "was", "die", "Be\u00b7cher", "klan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie brannte Hand in Hand:", "tokens": ["Wie", "brann\u00b7te", "Hand", "in", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00abes lebe die Liebste deine,", "tokens": ["\u00ab", "es", "le\u00b7be", "die", "Liebs\u00b7te", "dei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00abherzbruder, im Vaterland!", "tokens": ["\u00ab", "herz\u00b7bru\u00b7der", ",", "im", "Va\u00b7ter\u00b7land", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}