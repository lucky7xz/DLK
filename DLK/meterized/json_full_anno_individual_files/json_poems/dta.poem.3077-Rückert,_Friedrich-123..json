{"dta.poem.3077": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "123.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Singv\u00f6gel sind es nicht, die lernen W\u00f6rter sprechen,", "tokens": ["Sing\u00b7v\u00f6\u00b7gel", "sind", "es", "nicht", ",", "die", "ler\u00b7nen", "W\u00f6r\u00b7ter", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sind die schreienden, die Rede radebrechen,", "tokens": ["Es", "sind", "die", "schrei\u00b7en\u00b7den", ",", "die", "Re\u00b7de", "ra\u00b7de\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Papagei, dem man vorh\u00e4ngt die Spiegelwand,", "tokens": ["Der", "Pa\u00b7pa\u00b7gei", ",", "dem", "man", "vor\u00b7h\u00e4ngt", "die", "Spie\u00b7gel\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Elster, wenn man ihr gel\u00f6st der Zunge Band.", "tokens": ["Die", "Els\u00b7ter", ",", "wenn", "man", "ihr", "ge\u00b7l\u00f6st", "der", "Zun\u00b7ge", "Band", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "PPER", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch die mit freier Kunst dichten die freien Strofen", "tokens": ["Doch", "die", "mit", "frei\u00b7er", "Kunst", "dich\u00b7ten", "die", "frei\u00b7en", "Stro\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "ADJA", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Im dichten freien Wald, sind nicht Schulfilosofen.", "tokens": ["Im", "dich\u00b7ten", "frei\u00b7en", "Wald", ",", "sind", "nicht", "Schul\u00b7fi\u00b7lo\u00b7so\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}