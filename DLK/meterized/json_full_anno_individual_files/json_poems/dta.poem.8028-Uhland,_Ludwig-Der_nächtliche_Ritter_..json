{"dta.poem.8028": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der n\u00e4chtliche Ritter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.85", "nl:0.14"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In der mondlos stillen Nacht", "tokens": ["In", "der", "mond\u00b7los", "stil\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand er unter dem Altane,", "tokens": ["Stand", "er", "un\u00b7ter", "dem", "Al\u00b7ta\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sang mit himmlisch s\u00fc\u00dfer Stimme", "tokens": ["Sang", "mit", "himm\u00b7lisch", "s\u00fc\u00b7\u00dfer", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Minnelieder zur Guitarre.", "tokens": ["Min\u00b7ne\u00b7lie\u00b7der", "zur", "Gui\u00b7tar\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann auch mit den Nebenbuhlern", "tokens": ["Dann", "auch", "mit", "den", "Ne\u00b7ben\u00b7buh\u00b7lern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Hat er tapfer sich geschlagen,", "tokens": ["Hat", "er", "tap\u00b7fer", "sich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die hellen Funken stoben,", "tokens": ["Da\u00df", "die", "hel\u00b7len", "Fun\u00b7ken", "sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df die Mauern wiederhallten.", "tokens": ["Da\u00df", "die", "Mau\u00b7ern", "wie\u00b7der\u00b7hall\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und so \u00fcbt\u2019 er jeden Dienst,", "tokens": ["Und", "so", "\u00fcbt'", "er", "je\u00b7den", "Dienst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Den man weihet edeln Damen,", "tokens": ["Den", "man", "wei\u00b7het", "e\u00b7deln", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df mein Herz in Lieb\u2019 ergl\u00fchte", "tokens": ["Da\u00df", "mein", "Herz", "in", "Lieb'", "er\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fcr den theuern Unbekannten.", "tokens": ["F\u00fcr", "den", "theu\u00b7ern", "Un\u00b7be\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Als ich drauf am fr\u00fchen Morgen", "tokens": ["Als", "ich", "drauf", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Bebend blickte vom Altane:", "tokens": ["Be\u00b7bend", "blick\u00b7te", "vom", "Al\u00b7ta\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Blieb mir nichts von ihm zu schauen,", "tokens": ["Blieb", "mir", "nichts", "von", "ihm", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Als sein Blut, f\u00fcr mich gelassen.", "tokens": ["Als", "sein", "Blut", ",", "f\u00fcr", "mich", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}