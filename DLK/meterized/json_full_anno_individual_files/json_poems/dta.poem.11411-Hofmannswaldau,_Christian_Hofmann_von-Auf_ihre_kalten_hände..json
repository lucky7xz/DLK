{"dta.poem.11411": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre kalten h\u00e4nde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du liebst, mein schatz! und bist doch immer kalt;", "tokens": ["Du", "liebst", ",", "mein", "schatz", "!", "und", "bist", "doch", "im\u00b7mer", "kalt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$.", "KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist denn bey dir allein die lieb ein frostig wesen?", "tokens": ["Ist", "denn", "bey", "dir", "al\u00b7lein", "die", "lieb", "ein", "fros\u00b7tig", "we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADV", "ART", "ADJD", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie? oder soll mein aug aus dieser k\u00e4lte lesen,", "tokens": ["Wie", "?", "o\u00b7der", "soll", "mein", "aug", "aus", "die\u00b7ser", "k\u00e4l\u00b7te", "le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VMFIN", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df keine wahre lieb in deinen adern wallt?", "tokens": ["Da\u00df", "kei\u00b7ne", "wah\u00b7re", "lieb", "in", "dei\u00b7nen", "a\u00b7dern", "wallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein, Flavia! dein ku\u00df und die getreuen blicke", "tokens": ["Nein", ",", "Fla\u00b7via", "!", "dein", "ku\u00df", "und", "die", "ge\u00b7treu\u00b7en", "bli\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$.", "PPOSAT", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Verjagen hier den argwohn aller r\u00fccke,", "tokens": ["Ver\u00b7ja\u00b7gen", "hier", "den", "arg\u00b7wohn", "al\u00b7ler", "r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und zeigen, da\u00df dein geist in allem ernste liebt;", "tokens": ["Und", "zei\u00b7gen", ",", "da\u00df", "dein", "geist", "in", "al\u00b7lem", "erns\u00b7te", "liebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch, da\u00df dein zarter leib so wenig w\u00e4rme giebt,", "tokens": ["Doch", ",", "da\u00df", "dein", "zar\u00b7ter", "leib", "so", "we\u00b7nig", "w\u00e4r\u00b7me", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Macht blos die reine scham: Denn diese zwingt die flammen", "tokens": ["Macht", "blos", "die", "rei\u00b7ne", "scham", ":", "Denn", "die\u00b7se", "zwingt", "die", "flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$.", "KON", "PDS", "VVFIN", "ART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In deinem hertzen so zusammen,", "tokens": ["In", "dei\u00b7nem", "hert\u00b7zen", "so", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df man auf deiner glieder bahn", "tokens": ["Da\u00df", "man", "auf", "dei\u00b7ner", "glie\u00b7der", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nur schnee, und keine spuhr des feuers, finden kan.", "tokens": ["Nur", "schnee", ",", "und", "kei\u00b7ne", "spuhr", "des", "feu\u00b7ers", ",", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PIAT", "VVFIN", "ART", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}