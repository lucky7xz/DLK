tra"}, "line.3": {"text": "Sollen f\u00fcrder keinen Sieg mir bringen,", "tokens": ["Sol\u00b7len", "f\u00fcr\u00b7der", "kei\u00b7nen", "Sieg", "mir", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seit mir Chloe ward zu Theil.", "tokens": ["Seit", "mir", "Chloe", "ward", "zu", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NE", "VAFIN", "APPR", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.15": {"line.1": {"text": "Keine Herzen mehr verwunden", "tokens": ["Kei\u00b7ne", "Her\u00b7zen", "mehr", "ver\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich, bleibet ", "tokens": ["Will", "ich", ",", "blei\u00b7bet"], "token_info": ["word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Alle meine Lebens-Tag' und Stunden", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Le\u00b7bens\u00b7Tag'", "und", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Will ich ihr Gefangner sein.", "tokens": ["Will", "ich", "ihr", "Ge\u00b7fang\u00b7ner", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Deine Fackel? Ach, die Liebe", "tokens": ["Dei\u00b7ne", "Fa\u00b7ckel", "?", "Ach", ",", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOS