{"textgrid.poem.67388": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die G\u00f6ttergabe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, mit welcher holden Gabe", "tokens": ["H\u00f6rt", ",", "mit", "wel\u00b7cher", "hol\u00b7den", "Ga\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich die Liebe j\u00fcngst begl\u00fcckt!", "tokens": ["Mich", "die", "Lie\u00b7be", "j\u00fcngst", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich nie entz\u00fcckt gesungen habe,", "tokens": ["Wenn", "ich", "nie", "ent\u00b7z\u00fcckt", "ge\u00b7sun\u00b7gen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sing' ich jetzt von ihr entz\u00fcckt.", "tokens": ["Sing'", "ich", "jetzt", "von", "ihr", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Amor, als im sch\u00f6nsten Liede", "tokens": ["A\u00b7mor", ",", "als", "im", "sch\u00f6ns\u00b7ten", "Lie\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich des Gottes Siege sang,", "tokens": ["Ich", "des", "Got\u00b7tes", "Sie\u00b7ge", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trat zu mir und bot mir Gru\u00df und Friede,", "tokens": ["Trat", "zu", "mir", "und", "bot", "mir", "Gru\u00df", "und", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was er hatte, mir zum Dank.", "tokens": ["Was", "er", "hat\u00b7te", ",", "mir", "zum", "Dank", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbamor,\u00ab sprach ich, \u00bbDeine Schwingen", "tokens": ["\u00bb", "a\u00b7mor", ",", "\u00ab", "sprach", "ich", ",", "\u00bb", "Dei\u00b7ne", "Schwin\u00b7gen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "FM.la", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Dein K\u00f6cher und Dein Pfeil", "tokens": ["Und", "Dein", "K\u00f6\u00b7cher", "und", "Dein", "Pfeil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollen f\u00fcrder keinen Sieg mir bringen,", "tokens": ["Sol\u00b7len", "f\u00fcr\u00b7der", "kei\u00b7nen", "Sieg", "mir", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seit mir Chloe ward zu Theil.", "tokens": ["Seit", "mir", "Chloe", "ward", "zu", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NE", "VAFIN", "APPR", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Keine Herzen mehr verwunden", "tokens": ["Kei\u00b7ne", "Her\u00b7zen", "mehr", "ver\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich, bleibet ", "tokens": ["Will", "ich", ",", "blei\u00b7bet"], "token_info": ["word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Alle meine Lebens-Tag' und Stunden", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Le\u00b7bens\u00b7Tag'", "und", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Will ich ihr Gefangner sein.", "tokens": ["Will", "ich", "ihr", "Ge\u00b7fang\u00b7ner", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine Fackel? Ach, die Liebe", "tokens": ["Dei\u00b7ne", "Fa\u00b7ckel", "?", "Ach", ",", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ITJ", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehet ein zu helles Licht!", "tokens": ["Flie\u00b7het", "ein", "zu", "hel\u00b7les", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie, wenn Chloe mir nicht Chloe bliebe?", "tokens": ["Wie", ",", "wenn", "Chloe", "mir", "nicht", "Chloe", "blie\u00b7be", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "NE", "PPER", "PTKNEG", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Amor, nein! die Fackel nicht!\u00ab", "tokens": ["A\u00b7mor", ",", "nein", "!", "die", "Fa\u00b7ckel", "nicht", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PTKANT", "$.", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbnun, Du Sohn der T\u00e4uschereien,", "tokens": ["\u00bb", "nun", ",", "Du", "Sohn", "der", "T\u00e4u\u00b7sche\u00b7rei\u00b7en", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm die Binde denn von mir!", "tokens": ["Nimm", "die", "Bin\u00b7de", "denn", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mehr als Alles wird sie Dich erfreuen,", "tokens": ["Mehr", "als", "Al\u00b7les", "wird", "sie", "Dich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PIS", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vieles schenk' ich Dir mit ihr:", "tokens": ["Vie\u00b7les", "schenk'", "ich", "Dir", "mit", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "S\u00fc\u00dfen Trug und holdes W\u00e4hnen,", "tokens": ["S\u00fc\u00b7\u00dfen", "Trug", "und", "hol\u00b7des", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das f\u00fcr mehr als Wahrheit gilt,", "tokens": ["Das", "f\u00fcr", "mehr", "als", "Wahr\u00b7heit", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein immer wachsend neues Sehnen,", "tokens": ["Und", "ein", "im\u00b7mer", "wach\u00b7send", "neu\u00b7es", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Das die Seele hebt und f\u00fcllt.", "tokens": ["Das", "die", "See\u00b7le", "hebt", "und", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Tr\u00e4ume sind in ihr verborgen", "tokens": ["Tr\u00e4u\u00b7me", "sind", "in", "ihr", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "(freund, Du kennest sie noch kaum),", "tokens": ["(", "freund", ",", "Du", "ken\u00b7nest", "sie", "noch", "kaum", ")", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffnungen, mit jedem neuen Morgen", "tokens": ["Hoff\u00b7nun\u00b7gen", ",", "mit", "je\u00b7dem", "neu\u00b7en", "Mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dir ein neuer Jugendtraum.", "tokens": ["Dir", "ein", "neu\u00b7er", "Ju\u00b7gend\u00b7traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weise Blindheit, nicht zu sehen,", "tokens": ["Wei\u00b7se", "Blind\u00b7heit", ",", "nicht", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was Du froh nicht sehen willt,", "tokens": ["Was", "Du", "froh", "nicht", "se\u00b7hen", "willt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00fcchternheit, nicht Fehler aufzusp\u00e4hen,", "tokens": ["N\u00fcch\u00b7tern\u00b7heit", ",", "nicht", "Feh\u00b7ler", "auf\u00b7zu\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die der Liebreiz Dir verh\u00fcllt.", "tokens": ["Die", "der", "Lieb\u00b7reiz", "Dir", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schonung lieget in der Binde,", "tokens": ["Scho\u00b7nung", "lie\u00b7get", "in", "der", "Bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh und Warten und Geduld.", "tokens": ["Ruh", "und", "War\u00b7ten", "und", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm sie und sei selig gleich dem Kinde,", "tokens": ["Nimm", "sie", "und", "sei", "se\u00b7lig", "gleich", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VAFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Oder \u2013 es ist Deine Schuld!\u00ab", "tokens": ["O\u00b7der", "\u2013", "es", "ist", "Dei\u00b7ne", "Schuld", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.11": {"line.1": {"text": "Seit mit dieser G\u00f6ttergabe", "tokens": ["Seit", "mit", "die\u00b7ser", "G\u00f6t\u00b7ter\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Amor mich zum Gott entz\u00fcckt,", "tokens": ["A\u00b7mor", "mich", "zum", "Gott", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist sie wechselnd mein' und Chloens Habe,", "tokens": ["Ist", "sie", "wech\u00b7selnd", "mein'", "und", "Chloens", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVFIN", "KON", "NE", "NN", "$,"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und wir tragen sie begl\u00fcckt.", "tokens": ["Und", "wir", "tra\u00b7gen", "sie", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "H\u00f6rt, mit welcher holden Gabe", "tokens": ["H\u00f6rt", ",", "mit", "wel\u00b7cher", "hol\u00b7den", "Ga\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich die Liebe j\u00fcngst begl\u00fcckt!", "tokens": ["Mich", "die", "Lie\u00b7be", "j\u00fcngst", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich nie entz\u00fcckt gesungen habe,", "tokens": ["Wenn", "ich", "nie", "ent\u00b7z\u00fcckt", "ge\u00b7sun\u00b7gen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sing' ich jetzt von ihr entz\u00fcckt.", "tokens": ["Sing'", "ich", "jetzt", "von", "ihr", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Amor, als im sch\u00f6nst