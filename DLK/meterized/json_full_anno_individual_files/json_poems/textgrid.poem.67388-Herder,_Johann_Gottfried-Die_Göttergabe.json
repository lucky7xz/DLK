word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PIS", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vieles schenk' ich Dir mit ihr:", "tokens": ["Vie\u00b7les", "schenk'", "ich", "Dir", "mit", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "S\u00fc\u00dfen Trug und holdes W\u00e4hnen,", "tokens": ["S\u00fc\u00b7\u00dfen", "Trug", "und", "hol\u00b7des", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das f\u00fcr mehr als Wahrheit gilt,", "tokens": ["Das", "f\u00fcr", "mehr", "als", "Wahr\u00b7heit", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein immer wachsend neues Sehnen,", "tokens": ["Und", "ein", "im\u00b7mer", "wach\u00b7send", "neu\u00b7es", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Das die Seele hebt und f\u00fcllt.", "tokens": ["Das", "die", "See\u00b7le", "hebt", "und", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Tr\u00e4ume sind in ihr verborgen", "tokens": ["Tr\u00e4u\u00b7me", "sind", "in", "ihr", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VA