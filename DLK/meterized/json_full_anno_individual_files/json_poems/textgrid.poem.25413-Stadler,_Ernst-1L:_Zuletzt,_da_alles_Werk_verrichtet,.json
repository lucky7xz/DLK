{"textgrid.poem.25413": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zuletzt, da alles Werk verrichtet,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuletzt, da alles Werk verrichtet,", "tokens": ["Zu\u00b7letzt", ",", "da", "al\u00b7les", "Werk", "ver\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "meinen Gott zu loben,", "tokens": ["mei\u00b7nen", "Gott", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat meine Hand die beiden Frauenbilder", "tokens": ["Hat", "mei\u00b7ne", "Hand", "die", "bei\u00b7den", "Frau\u00b7en\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "aus dem Stein gehoben.", "tokens": ["aus", "dem", "Stein", "ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die eine aufgerichtet,", "tokens": ["Die", "ei\u00b7ne", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "frei und unerschrocken \u2013", "tokens": ["frei", "und", "un\u00b7er\u00b7schro\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ihr Blick ist Sieg,", "tokens": ["Ihr", "Blick", "ist", "Sieg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "ihr Schreiten gl\u00e4nzt Frohlocken.", "tokens": ["ihr", "Schrei\u00b7ten", "gl\u00e4nzt", "Froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu zeigen, wie sie freudig", "tokens": ["Zu", "zei\u00b7gen", ",", "wie", "sie", "freu\u00b7dig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "\u00fcber allem Erdenm\u00fchsal throne,", "tokens": ["\u00fc\u00b7ber", "al\u00b7lem", "Er\u00b7den\u00b7m\u00fch\u00b7sal", "thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Gab ich ihr Kelch und Kreuzesfahne und", "tokens": ["Gab", "ich", "ihr", "Kelch", "und", "Kreu\u00b7zes\u00b7fah\u00b7ne", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "die Krone.", "tokens": ["die", "Kro\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Aber meine Seele, Sch\u00f6nheit ferner Kindertage", "tokens": ["A\u00b7ber", "mei\u00b7ne", "See\u00b7le", ",", "Sch\u00f6n\u00b7heit", "fer\u00b7ner", "Kin\u00b7der\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "ADV", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "und mein tief verstecktes Leben", "tokens": ["und", "mein", "tief", "ver\u00b7steck\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Hab ich der Besiegten,", "tokens": ["Hab", "ich", "der", "Be\u00b7sieg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "der Versto\u00dfenen gegeben.", "tokens": ["der", "Ver\u00b7sto\u00b7\u00dfe\u00b7nen", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "--+---+-", "measure": "anapaest.init"}, "line.17": {"text": "Und was ich in mir trug an Stille,", "tokens": ["Und", "was", "ich", "in", "mir", "trug", "an", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "sanfter Trauer und dem\u00fctigem Verlangen", "tokens": ["sanf\u00b7ter", "Trau\u00b7er", "und", "de\u00b7m\u00fc\u00b7ti\u00b7gem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Hab ich sehns\u00fcchtig", "tokens": ["Hab", "ich", "sehn\u00b7s\u00fcch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "\u00fcber ihren Kinderleib gehangen:", "tokens": ["\u00fc\u00b7ber", "ih\u00b7ren", "Kin\u00b7der\u00b7leib", "ge\u00b7han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Die schlanken H\u00fcften ausgebuchtet,", "tokens": ["Die", "schlan\u00b7ken", "H\u00fcf\u00b7ten", "aus\u00b7ge\u00b7buch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "die der lockre G\u00fcrtel h\u00e4lt,", "tokens": ["die", "der", "lock\u00b7re", "G\u00fcr\u00b7tel", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Die H\u00fcgel ihrer Br\u00fcste", "tokens": ["Die", "H\u00fc\u00b7gel", "ih\u00b7rer", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "z\u00e4rtlich aus dem Linnen ausgewellt,", "tokens": ["z\u00e4rt\u00b7lich", "aus", "dem", "Lin\u00b7nen", "aus\u00b7ge\u00b7wellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Lie\u00df ihre Haare \u00fcber Schultern hin", "tokens": ["Lie\u00df", "ih\u00b7re", "Haa\u00b7re", "\u00fc\u00b7ber", "Schul\u00b7tern", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "wie einen blonden Regen flie\u00dfen,", "tokens": ["wie", "ei\u00b7nen", "blon\u00b7den", "Re\u00b7gen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Liebkoste ihre H\u00e4nde, die das alte Buch", "tokens": ["Lieb\u00b7kos\u00b7te", "ih\u00b7re", "H\u00e4n\u00b7de", ",", "die", "das", "al\u00b7te", "Buch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "und den zerknickten Schaft umschlie\u00dfen,", "tokens": ["und", "den", "zer\u00b7knick\u00b7ten", "Schaft", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Gab ihren schlaffen Armen die gebeugte Schwermut", "tokens": ["Gab", "ih\u00b7ren", "schlaf\u00b7fen", "Ar\u00b7men", "die", "ge\u00b7beug\u00b7te", "Schwer\u00b7mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "gelber Weizenfelder, die in Julisonne schwellen,", "tokens": ["gel\u00b7ber", "Wei\u00b7zen\u00b7fel\u00b7der", ",", "die", "in", "Ju\u00b7li\u00b7son\u00b7ne", "schwel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.31": {"text": "Dem Wandeln ihrer F\u00fc\u00dfe die Musik von Orgeln,", "tokens": ["Dem", "Wan\u00b7deln", "ih\u00b7rer", "F\u00fc\u00b7\u00dfe", "die", "Mu\u00b7sik", "von", "Or\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "die an Sonntagen aus Kirchent\u00fcren quellen.", "tokens": ["die", "an", "Sonn\u00b7ta\u00b7gen", "aus", "Kir\u00b7chen\u00b7t\u00fc\u00b7ren", "quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.33": {"text": "Die s\u00fc\u00dfen Augen", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "mu\u00dften eine Binde tragen,", "tokens": ["mu\u00df\u00b7ten", "ei\u00b7ne", "Bin\u00b7de", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Da\u00df r\u00fchrender durch d\u00fcnne Seide", "tokens": ["Da\u00df", "r\u00fch\u00b7ren\u00b7der", "durch", "d\u00fcn\u00b7ne", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "wehe ihrer Wimpern Schlagen.", "tokens": ["we\u00b7he", "ih\u00b7rer", "Wim\u00b7pern", "Schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und Lieblichkeit der Glieder,", "tokens": ["Und", "Lieb\u00b7lich\u00b7keit", "der", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "die ihr weiches Hemd erf\u00fcllt,", "tokens": ["die", "ihr", "wei\u00b7ches", "Hemd", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Hab ich mit Demut", "tokens": ["Hab", "ich", "mit", "De\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.40": {"text": "ganz und gar umh\u00fcllt,", "tokens": ["ganz", "und", "gar", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.41": {"text": "Da\u00df wunderbar", "tokens": ["Da\u00df", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word"], "pos": ["KOUS", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "in Gottes Brudern\u00e4he", "tokens": ["in", "Got\u00b7tes", "Bru\u00b7der\u00b7n\u00e4\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Von Niedrigkeit umgl\u00e4nzt", "tokens": ["Von", "Nied\u00b7rig\u00b7keit", "um\u00b7gl\u00e4nzt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "ihr reines Bildnis stehe.", "tokens": ["ihr", "rei\u00b7nes", "Bild\u00b7nis", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zuletzt, da alles Werk verrichtet,", "tokens": ["Zu\u00b7letzt", ",", "da", "al\u00b7les", "Werk", "ver\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "meinen Gott zu loben,", "tokens": ["mei\u00b7nen", "Gott", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "me