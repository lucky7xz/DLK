{"dta.poem.15700": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht sch\u00f6ner ist es jetzt, als einst es war, auf Erden,", "tokens": ["Nicht", "sch\u00f6\u00b7ner", "ist", "es", "jetzt", ",", "als", "einst", "es", "war", ",", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "PPER", "VAFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch besser; besser einst und sch\u00f6ner wird es werden.", "tokens": ["Noch", "bes\u00b7ser", ";", "bes\u00b7ser", "einst", "und", "sch\u00f6\u00b7ner", "wird", "es", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADJD", "ADV", "KON", "ADJD", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vom Blumenh\u00fcgel ist die Weltgeschicht' entstiegen;", "tokens": ["Vom", "Blu\u00b7men\u00b7h\u00fc\u00b7gel", "ist", "die", "Welt\u00b7ge\u00b7schicht'", "ent\u00b7stie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Fl\u00fcgeln wird sie einst den G\u00f6tterberg erfliegen:", "tokens": ["Mit", "Fl\u00fc\u00b7geln", "wird", "sie", "einst", "den", "G\u00f6t\u00b7ter\u00b7berg", "er\u00b7flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Am Boden kriecht sie jetzt; wann wird sie Fl\u00fcgel kriegen?", "tokens": ["Am", "Bo\u00b7den", "kriecht", "sie", "jetzt", ";", "wann", "wird", "sie", "Fl\u00fc\u00b7gel", "krie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$.", "PWAV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}