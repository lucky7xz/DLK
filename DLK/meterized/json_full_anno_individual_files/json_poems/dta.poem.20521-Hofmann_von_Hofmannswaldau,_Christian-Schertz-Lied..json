{"dta.poem.20521": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie n\u00e4rrisch lebt ein kerl doch in der welt/", "tokens": ["Wie", "n\u00e4r\u00b7risch", "lebt", "ein", "kerl", "doch", "in", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn er erst in das garn der liebe f\u00e4llt?", "tokens": ["Wenn", "er", "erst", "in", "das", "garn", "der", "lie\u00b7be", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn er den muth f\u00fcr einen griff verhandelt/", "tokens": ["Wenn", "er", "den", "muth", "f\u00fcr", "ei\u00b7nen", "griff", "ver\u00b7han\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in den stricken des verderbens wandelt?", "tokens": ["Und", "in", "den", "stri\u00b7cken", "des", "ver\u00b7der\u00b7bens", "wan\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bald sitzt ihm der kragen am halse nicht recht/", "tokens": ["Bald", "sitzt", "ihm", "der", "kra\u00b7gen", "am", "hal\u00b7se", "nicht", "recht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "PTKNEG", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Bald ist ihm die d\u00fcnne Paruqve zu schlecht/", "tokens": ["Bald", "ist", "ihm", "die", "d\u00fcn\u00b7ne", "Pa\u00b7ruq\u00b7ve", "zu", "schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "PTKA", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Bald zieht er den degen/ bald steckt er ihn ein;", "tokens": ["Bald", "zieht", "er", "den", "de\u00b7gen", "/", "bald", "steckt", "er", "ihn", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Bald denckt er ein bauer/ bald k\u00f6nig zu seyn.", "tokens": ["Bald", "denckt", "er", "ein", "bau\u00b7er", "/", "bald", "k\u00f6\u00b7nig", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wie mancher tag und manche sch\u00f6ne nacht", "tokens": ["Wie", "man\u00b7cher", "tag", "und", "man\u00b7che", "sch\u00f6\u00b7ne", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird doch mit gram und sorgen zugebracht?", "tokens": ["Wird", "doch", "mit", "gram", "und", "sor\u00b7gen", "zu\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn wir endlich nur zu lange harren/", "tokens": ["Und", "wenn", "wir", "end\u00b7lich", "nur", "zu", "lan\u00b7ge", "har\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So werden wir zu thoren und zu narren.", "tokens": ["So", "wer\u00b7den", "wir", "zu", "tho\u00b7ren", "und", "zu", "nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn gehn wir und geben dem gl\u00fccke die schuld/", "tokens": ["Denn", "gehn", "wir", "und", "ge\u00b7ben", "dem", "gl\u00fc\u00b7cke", "die", "schuld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PDS", "VVFIN", "ART", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Das gl\u00fccke spricht: Habe nur ferner gedult.", "tokens": ["Das", "gl\u00fc\u00b7cke", "spricht", ":", "Ha\u00b7be", "nur", "fer\u00b7ner", "ge\u00b7dult", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Gedultig seyn bringt uns in leiden und pein/", "tokens": ["Ge\u00b7dul\u00b7tig", "seyn", "bringt", "uns", "in", "lei\u00b7den", "und", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VVFIN", "PPER", "APPR", "VVINF", "KON", "NE", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Bi\u00df da\u00df wir so m\u00fcrbe wie pflaumen-brey seyn.", "tokens": ["Bi\u00df", "da\u00df", "wir", "so", "m\u00fcr\u00b7be", "wie", "pflau\u00b7men\u00b7brey", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADJA", "KOKOM", "NN", "VAINF", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch geht es mit der zeit auch endlich an/", "tokens": ["Doch", "geht", "es", "mit", "der", "zeit", "auch", "end\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df man zur noth mit m\u00e4dgen l\u00f6ffeln kan;", "tokens": ["Da\u00df", "man", "zur", "noth", "mit", "m\u00e4d\u00b7gen", "l\u00f6f\u00b7feln", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach Gott! mit was vor m\u00fch und schwerem lauffen", "tokens": ["Ach", "Gott", "!", "mit", "was", "vor", "m\u00fch", "und", "schwe\u00b7rem", "lauf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "APPR", "PRELS", "APPR", "ADJD", "KON", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df man alsdenn ihm seine lust erkauffen!", "tokens": ["Mu\u00df", "man", "als\u00b7denn", "ihm", "sei\u00b7ne", "lust", "er\u00b7kauf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da nimmt man/ so viel man bekommen nur kan/", "tokens": ["Da", "nimmt", "man", "/", "so", "viel", "man", "be\u00b7kom\u00b7men", "nur", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "ADV", "ADV", "PIS", "VVFIN", "ADV", "VMFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und leget den wechsel an spitzen-werck an;", "tokens": ["Und", "le\u00b7get", "den", "wech\u00b7sel", "an", "spit\u00b7zen\u00b7\u00b7werck", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Indessen sitzt vater und mutter zu hau\u00df/", "tokens": ["In\u00b7des\u00b7sen", "sitzt", "va\u00b7ter", "und", "mut\u00b7ter", "zu", "hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und zahlen das theure Collegium aus.", "tokens": ["Und", "zah\u00b7len", "das", "theu\u00b7re", "Col\u00b7le\u00b7gi\u00b7um", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn man nun so das geld hat angewandt/", "tokens": ["Wenn", "man", "nun", "so", "das", "geld", "hat", "an\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So machen sich denn mund und mund bekandt/", "tokens": ["So", "ma\u00b7chen", "sich", "denn", "mund", "und", "mund", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADJD", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schliessen sich vor heissen liebes-flammen/", "tokens": ["Und", "schlies\u00b7sen", "sich", "vor", "heis\u00b7sen", "lie\u00b7bes\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie fetter leim und junges wachs zusammen.", "tokens": ["Wie", "fet\u00b7ter", "leim", "und", "jun\u00b7ges", "wachs", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn macht man dem m\u00e4dgen die br\u00fcstgen erst blo\u00df/", "tokens": ["Denn", "macht", "man", "dem", "m\u00e4d\u00b7gen", "die", "br\u00fcst\u00b7gen", "erst", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "ADJA", "ART", "ADJA", "ADV", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die br\u00fcste verleiten uns endlich im schoo\u00df/", "tokens": ["Die", "br\u00fcs\u00b7te", "ver\u00b7lei\u00b7ten", "uns", "end\u00b7lich", "im", "schoo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "APPRART", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Im schop\u00dfe w\u00e4chst unser verderben durchs kind/", "tokens": ["Im", "schop\u00b7\u00dfe", "w\u00e4chst", "un\u00b7ser", "ver\u00b7der\u00b7ben", "durchs", "kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPOSAT", "VVFIN", "APPRART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Denn stehn wir im ", "tokens": ["Denn", "stehn", "wir", "im"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wohl dem! der so/ wie goldschmids-junge denckt/", "tokens": ["Wohl", "dem", "!", "der", "so", "/", "wie", "gold\u00b7schmids\u00b7jun\u00b7ge", "denckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$.", "ART", "ADV", "$(", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und eher sich nicht zu der liebe lenckt/", "tokens": ["Und", "e\u00b7her", "sich", "nicht", "zu", "der", "lie\u00b7be", "lenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "PTKNEG", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als bi\u00df er nach vollbrachten jugend-jahren", "tokens": ["Als", "bi\u00df", "er", "nach", "voll\u00b7brach\u00b7ten", "ju\u00b7gend\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich kan in ehren mit der liebsten paaren.", "tokens": ["Sich", "kan", "in", "eh\u00b7ren", "mit", "der", "liebs\u00b7ten", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "APPR", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich habe die regel: Ich liebe nicht viel/", "tokens": ["Ich", "ha\u00b7be", "die", "re\u00b7gel", ":", "Ich", "lie\u00b7be", "nicht", "viel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Und wenn ich gleich liebe/ so ist es nur spiel/", "tokens": ["Und", "wenn", "ich", "gleich", "lie\u00b7be", "/", "so", "ist", "es", "nur", "spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und ist es im ernste/ so bin ich vergn\u00fcgt/", "tokens": ["Und", "ist", "es", "im", "erns\u00b7te", "/", "so", "bin", "ich", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "ADJA", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Im fall ich nur freundschafft und k\u00fcsse gekriegt.", "tokens": ["Im", "fall", "ich", "nur", "freund\u00b7schafft", "und", "k\u00fcs\u00b7se", "ge\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}