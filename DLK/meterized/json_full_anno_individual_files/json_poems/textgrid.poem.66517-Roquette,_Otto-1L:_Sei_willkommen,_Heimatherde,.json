{"textgrid.poem.66517": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei willkommen, Heimatherde,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei willkommen, Heimatherde,", "tokens": ["Sei", "will\u00b7kom\u00b7men", ",", "Hei\u00b7ma\u00b7ther\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich fast verloren gab!", "tokens": ["Die", "ich", "fast", "ver\u00b7lo\u00b7ren", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchre zu dem alten Herde", "tokens": ["F\u00fch\u00b7re", "zu", "dem", "al\u00b7ten", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich, mein treuer Wanderstab!", "tokens": ["Mich", ",", "mein", "treu\u00b7er", "Wan\u00b7der\u00b7stab", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die vor Jahren ich verlassen", "tokens": ["Die", "vor", "Jah\u00b7ren", "ich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr die Welt, mit leichtem Fu\u00df,", "tokens": ["F\u00fcr", "die", "Welt", ",", "mit", "leich\u00b7tem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", 