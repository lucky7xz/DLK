{"dta.poem.11019": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schw\u00e4che menschlicher Begriffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da die Gelehrten, wie vorhin, auch noch die Widerspr\u00fc-\nche h\u00e4ufen:", "tokens": ["Da", "die", "Ge\u00b7lehr\u00b7ten", ",", "wie", "vor\u00b7hin", ",", "auch", "noch", "die", "Wi\u00b7der\u00b7spr\u00fc", "che", "h\u00e4u\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "ADV", "$,", "ADV", "ADV", "ART", "TRUNC", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "So scheint es eine klare Probe, da\u00df alle wirklich nichts be-", "tokens": ["So", "scheint", "es", "ei\u00b7ne", "kla\u00b7re", "Pro\u00b7be", ",", "da\u00df", "al\u00b7le", "wirk\u00b7lich", "nichts", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "PIS", "ADJD", "PIS", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ich bin dadurch in meiner Meynung best\u00e4rkt, und bleibe bey", "tokens": ["Ich", "bin", "da\u00b7durch", "in", "mei\u00b7ner", "Mey\u00b7nung", "be\u00b7st\u00e4rkt", ",", "und", "blei\u00b7be", "bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "APPR", "PPOSAT", "NN", "VVPP", "$,", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Da\u00df das Begreifen f\u00fcr die Engel, Bewundern blo\u00df f\u00fcr uns", "tokens": ["Da\u00df", "das", "Be\u00b7grei\u00b7fen", "f\u00fcr", "die", "En\u00b7gel", ",", "Be\u00b7wun\u00b7dern", "blo\u00df", "f\u00fcr", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}