{"dta.poem.5585": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ich ersinn', ist ungemein,", "tokens": ["Was", "ich", "er\u00b7sinn'", ",", "ist", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df, da\u00df mein Entwurf, der mir so wol gef\u00e4llt,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "mein", "Ent\u00b7wurf", ",", "der", "mir", "so", "wol", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da er in Franckreich neu wird vorgestellt,", "tokens": ["Da", "er", "in", "Fran\u00b7ck\u00b7reich", "neu", "wird", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Verwegen scheinen wird, und kaum gef\u00e4llig seyn.", "tokens": ["Ver\u00b7we\u00b7gen", "schei\u00b7nen", "wird", ",", "und", "kaum", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "$,", "KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich f\u00fcrcht\u2019 und schmeichle mir zugleich: mein Hoffen gr\u00fcnt,", "tokens": ["Ich", "f\u00fcrcht'", "und", "schmeich\u00b7le", "mir", "zu\u00b7gleich", ":", "mein", "Hof\u00b7fen", "gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df, wie mein Vorsatz neu, er so auch Nuhm verdient.", "tokens": ["Da\u00df", ",", "wie", "mein", "Vor\u00b7satz", "neu", ",", "er", "so", "auch", "Nuhm", "ver\u00b7dient", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOKOM", "PPOSAT", "NN", "ADJD", "$,", "PPER", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}