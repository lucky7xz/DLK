{"textgrid.poem.60027": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein fr\u00f6hlicher Sylphe,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein fr\u00f6hlicher Sylphe,", "tokens": ["Ein", "fr\u00f6h\u00b7li\u00b7cher", "Syl\u00b7phe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der, wenn die Lerche sich hebt,", "tokens": ["Der", ",", "wenn", "die", "Ler\u00b7che", "sich", "hebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit ihr in blauen Luften schwebt;", "tokens": ["Mit", "ihr", "in", "blau\u00b7en", "Luf\u00b7ten", "schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Fr\u00fchlings treuer Geh\u00fclfe,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "treu\u00b7er", "Ge\u00b7h\u00fcl\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der ihm das F\u00fcllhorn reicht,", "tokens": ["Der", "ihm", "das", "F\u00fcll\u00b7horn", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und \u00fcber zarte Knospen schleicht,", "tokens": ["Und", "\u00fc\u00b7ber", "zar\u00b7te", "Knos\u00b7pen", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Knospen Wohlger\u00fcche giebt,", "tokens": ["Den", "Knos\u00b7pen", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jede Blume des Grases liebt;", "tokens": ["Und", "je\u00b7de", "Blu\u00b7me", "des", "Gra\u00b7ses", "liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ein Gott voll Unschuld, wie die Bl\u00fcthe", "tokens": ["Ein", "Gott", "voll", "Un\u00b7schuld", ",", "wie", "die", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Linden im Thal,", "tokens": ["Der", "Lin\u00b7den", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Und voll bescheidner G\u00fcte,", "tokens": ["Und", "voll", "be\u00b7scheid\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wie n\u00e4chtlicher Thau, bey Mondesstrahl;", "tokens": ["Wie", "n\u00e4cht\u00b7li\u00b7cher", "Thau", ",", "bey", "Mon\u00b7des\u00b7strahl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der sah im letzten May, von seinem Rasenbette;", "tokens": ["Der", "sah", "im", "letz\u00b7ten", "May", ",", "von", "sei\u00b7nem", "Ra\u00b7sen\u00b7bet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo neben ihm ein Sylphenm\u00e4dchen schlief,", "tokens": ["Wo", "ne\u00b7ben", "ihm", "ein", "Syl\u00b7phen\u00b7m\u00e4d\u00b7chen", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.15": {"text": "Dich Freuden athmende Lenette!", "tokens": ["Dich", "Freu\u00b7den", "ath\u00b7men\u00b7de", "Le\u00b7net\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wie Geister sehen, sah er tief", "tokens": ["Wie", "Geis\u00b7ter", "se\u00b7hen", ",", "sah", "er", "tief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "VVINF", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "In deine liebliche Seele,", "tokens": ["In", "dei\u00b7ne", "lieb\u00b7li\u00b7che", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "So lieblich, wie die gr\u00fcne H\u00f6hle,", "tokens": ["So", "lieb\u00b7lich", ",", "wie", "die", "gr\u00fc\u00b7ne", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "In deren Innersten ein Kind, das nie gewacht,", "tokens": ["In", "de\u00b7ren", "In\u00b7ners\u00b7ten", "ein", "Kind", ",", "das", "nie", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Das keine b\u00f6se That gedacht,", "tokens": ["Das", "kei\u00b7ne", "b\u00f6\u00b7se", "That", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ein kaum gebohrner Amor lacht.", "tokens": ["Ein", "kaum", "ge\u00b7bohr\u00b7ner", "A\u00b7mor", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der fr\u00f6hliche Sylphe,", "tokens": ["Der", "fr\u00f6h\u00b7li\u00b7che", "Syl\u00b7phe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Der Gott voll Unschuld, w\u00fcnschte sich, dein,", "tokens": ["Der", "Gott", "voll", "Un\u00b7schuld", ",", "w\u00fcnschte", "sich", ",", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "VVFIN", "PRF", "$,", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und nicht des Fr\u00fchlings Geh\u00fclfe,", "tokens": ["Und", "nicht", "des", "Fr\u00fch\u00b7lings", "Ge\u00b7h\u00fcl\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Nicht seiner Sylphide Liebling zu seyn.", "tokens": ["Nicht", "sei\u00b7ner", "Syl\u00b7phi\u00b7de", "Lieb\u00b7ling", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Er eilte durch den Hain,", "tokens": ["Er", "eil\u00b7te", "durch", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Mit frischen Kr\u00e4nzen sch\u00f6n behangen,", "tokens": ["Mit", "fri\u00b7schen", "Kr\u00e4n\u00b7zen", "sch\u00f6n", "be\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und k\u00fc\u00dfte dich.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Jedoch auf deinen sterblichen Wangen", "tokens": ["Je\u00b7doch", "auf", "dei\u00b7nen", "sterb\u00b7li\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Verloren seine K\u00fcsse sich", "tokens": ["Ver\u00b7lo\u00b7ren", "sei\u00b7ne", "K\u00fcs\u00b7se", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "In einen Rosenduft, in eines Windes Wehen.", "tokens": ["In", "ei\u00b7nen", "Ro\u00b7sen\u00b7duft", ",", "in", "ei\u00b7nes", "Win\u00b7des", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Nun stand der Gott, vom Lenz' allein gesehen,", "tokens": ["Nun", "stand", "der", "Gott", ",", "vom", "Lenz'", "al\u00b7lein", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und weinte bitterlich;", "tokens": ["Und", "wein\u00b7te", "bit\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Denn alle K\u00fcsse verloren sich.", "tokens": ["Denn", "al\u00b7le", "K\u00fcs\u00b7se", "ver\u00b7lo\u00b7ren", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "PRF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Allein, wo seine Thr\u00e4nen fielen,", "tokens": ["Al\u00b7lein", ",", "wo", "sei\u00b7ne", "Thr\u00e4\u00b7nen", "fie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Da spro\u00df ein Veilchen empor,", "tokens": ["Da", "spro\u00df", "ein", "Veil\u00b7chen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Und eine von seinen Gespielen,", "tokens": ["Und", "ei\u00b7ne", "von", "sei\u00b7nen", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.38": {"text": "Aus der Nymphen Chor,", "tokens": ["Aus", "der", "Nym\u00b7phen", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.39": {"text": "Lispelt ihm ins Ohr:", "tokens": ["Lis\u00b7pelt", "ihm", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.40": {"text": "Deine K\u00fcsse f\u00fchlen", "tokens": ["Dei\u00b7ne", "K\u00fcs\u00b7se", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.41": {"text": "Kann das Erdenm\u00e4dchen nie;", "tokens": ["Kann", "das", "Er\u00b7den\u00b7m\u00e4d\u00b7chen", "nie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Aber, du Gl\u00fccklicher, sieh", "tokens": ["A\u00b7ber", ",", "du", "Gl\u00fcck\u00b7li\u00b7cher", ",", "sieh"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "PPER", "NN", "$,", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.43": {"text": "Deinen Thr\u00e4nen dieses Veilchen entbl\u00fchn!", "tokens": ["Dei\u00b7nen", "Thr\u00e4\u00b7nen", "die\u00b7ses", "Veil\u00b7chen", "ent\u00b7bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.44": {"text": "Liebe will es auferziehn,", "tokens": ["Lie\u00b7be", "will", "es", "auf\u00b7er\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Und es lebt noch, wenn die Farben", "tokens": ["Und", "es", "lebt", "noch", ",", "wenn", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Jedes Blumenbeets erstarben;", "tokens": ["Je\u00b7des", "Blu\u00b7men\u00b7beets", "er\u00b7star\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Dann, des Herbstes auch sich freuend, irrt", "tokens": ["Dann", ",", "des", "Herbs\u00b7tes", "auch", "sich", "freu\u00b7end", ",", "irrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "ART", "NN", "ADV", "PRF", "VVPP", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "Deine Sch\u00f6ne hier, wo rauhe Winde schw\u00e4rmen,", "tokens": ["Dei\u00b7ne", "Sch\u00f6\u00b7ne", "hier", ",", "wo", "rau\u00b7he", "Win\u00b7de", "schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.49": {"text": "Bricht das Veilchen, und es wird", "tokens": ["Bricht", "das", "Veil\u00b7chen", ",", "und", "es", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Sich an ihrem Busen w\u00e4rmen.", "tokens": ["Sich", "an", "ih\u00b7rem", "Bu\u00b7sen", "w\u00e4r\u0