{"dta.poem.3793": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Die best\u00e4ndige Liebe.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Unsre keuschen Liebes-Flammen,", "tokens": ["Uns\u00b7re", "keu\u00b7schen", "Lie\u00b7bes\u00b7Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So vom reinsten Himmel stam\u0303en,", "tokens": ["So", "vom", "reins\u00b7ten", "Him\u00b7mel", "stam\u0303en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00e4hret die Best\u00e4ndigkeit;", "tokens": ["N\u00e4h\u00b7ret", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese redlich frommen Triebe,", "tokens": ["Die\u00b7se", "red\u00b7lich", "from\u00b7men", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese Sehnsucht, treuer Liebe,", "tokens": ["Die\u00b7se", "Sehn\u00b7sucht", ",", "treu\u00b7er", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gr\u00fcnet endlich mit der Zeit.", "tokens": ["Gr\u00fc\u00b7net", "end\u00b7lich", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebster Schatz! du mein Vergn\u00fcgen!", "tokens": ["Liebs\u00b7ter", "Schatz", "!", "du", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeit und Treu wird endlich siegen.", "tokens": ["Zeit", "und", "Treu", "wird", "end\u00b7lich", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist \u00fcberzeugt, o! Sch\u00f6ne,", "tokens": ["Du", "bist", "\u00fc\u00b7berz\u00b7eugt", ",", "o", "!", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "FM", "$.", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meine schwachen Dichter-Th\u00f6ne", "tokens": ["Mei\u00b7ne", "schwa\u00b7chen", "Dich\u00b7ter\u00b7\u00b7T\u00b7h\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Singen nur von deinem Werth;", "tokens": ["Sin\u00b7gen", "nur", "von", "dei\u00b7nem", "Werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine reitzend sch\u00f6ne Jugend,", "tokens": ["Dei\u00b7ne", "reit\u00b7zend", "sch\u00f6\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Anmuth, Fr\u00f6mmigkeit und Tugend", "tokens": ["An\u00b7muth", ",", "Fr\u00f6m\u00b7mig\u00b7keit", "und", "Tu\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist, was Geist und Kiel verehrt.", "tokens": ["Ist", ",", "was", "Geist", "und", "Kiel", "ver\u00b7ehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "NN", "KON", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fc\u00dft ich nur in \u00e4chten Bildern", "tokens": ["W\u00fc\u00dft", "ich", "nur", "in", "\u00e4ch\u00b7ten", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich nach W\u00fcrden abzuschildern!", "tokens": ["Dich", "nach", "W\u00fcr\u00b7den", "ab\u00b7zu\u00b7schil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unsre Liebe stammt von oben,", "tokens": ["Uns\u00b7re", "Lie\u00b7be", "stammt", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df so Neid, als Mi\u00dfgunst, toben,", "tokens": ["La\u00df", "so", "Neid", ",", "als", "Mi\u00df\u00b7gunst", ",", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sie immer Zeder schreyn;", "tokens": ["La\u00df", "sie", "im\u00b7mer", "Ze\u00b7der", "schreyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kind, erdulde doch gelassen", "tokens": ["Kind", ",", "er\u00b7dul\u00b7de", "doch", "ge\u00b7las\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr verdammt und freches Rasen,", "tokens": ["Ihr", "ver\u00b7dammt", "und", "fre\u00b7ches", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du bist doch und bleibest mein.", "tokens": ["Du", "bist", "doch", "und", "blei\u00b7best", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Endlich nach Gedult und Warten", "tokens": ["End\u00b7lich", "nach", "Ge\u00b7dult", "und", "War\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bl\u00fcht der sch\u00f6ne Hoffnungs-Garten.", "tokens": ["Bl\u00fcht", "der", "sch\u00f6\u00b7ne", "Hoff\u00b7nungs\u00b7Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar, die \u00fcberh\u00e4uffte Kr\u00e4nckni\u00df", "tokens": ["Zwar", ",", "die", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7te", "Kr\u00e4nck\u00b7ni\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt uns offt ein Leich-Beg\u00e4ngni\u00df,", "tokens": ["H\u00e4lt", "uns", "offt", "ein", "Leich\u00b7Be\u00b7g\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehe man es kaum gedacht;", "tokens": ["E\u00b7he", "man", "es", "kaum", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, der blinde Ball vom Gl\u00fccke,", "tokens": ["Ja", ",", "der", "blin\u00b7de", "Ball", "vom", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, das widrige Geschicke", "tokens": ["Ja", ",", "das", "wid\u00b7ri\u00b7ge", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat es offt zunicht gemacht,", "tokens": ["Hat", "es", "offt", "zu\u00b7nicht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein blosses Nichts auf Erden", "tokens": ["Und", "ein", "blos\u00b7ses", "Nichts", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kan an uns zum Hencker werden.", "tokens": ["Kan", "an", "uns", "zum", "Hen\u00b7cker", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "APPRART", "NN", "VAINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Doch, ein fromm gela\u00dfnes Schweigen", "tokens": ["Doch", ",", "ein", "fromm", "ge\u00b7la\u00df\u00b7nes", "Schwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird uns Trost und Labsal zeigen,", "tokens": ["Wird", "uns", "Trost", "und", "Lab\u00b7sal", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da auch unsre Z\u00e4rtlichkeit", "tokens": ["Da", "auch", "uns\u00b7re", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch derselben Ancker sieget,", "tokens": ["Durch", "der\u00b7sel\u00b7ben", "An\u00b7cker", "sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in stillster Ruhe lieget,", "tokens": ["Und", "in", "stills\u00b7ter", "Ru\u00b7he", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn wird unser Hertz erfreut,", "tokens": ["Denn", "wird", "un\u00b7ser", "Hertz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so treu verliebten Seelen", "tokens": ["Und", "so", "treu", "ver\u00b7lieb\u00b7ten", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kan ihr Wunsch ohnm\u00f6glich fehlen.", "tokens": ["Kan", "ihr", "Wunsch", "ohn\u00b7m\u00f6g\u00b7lich", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Scheinen unsrer Wohlfahrt Sterne", "tokens": ["Schei\u00b7nen", "uns\u00b7rer", "Wohl\u00b7fahrt", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jtzo gleich in weiter Ferne,", "tokens": ["Jt\u00b7zo", "gleich", "in", "wei\u00b7ter", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Endlich kommt das frohe Licht,", "tokens": ["End\u00b7lich", "kommt", "das", "fro\u00b7he", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn nach Hagel, Blitz und Wettern,", "tokens": ["Wenn", "nach", "Ha\u00b7gel", ",", "Blitz", "und", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn nach Donner, Schlag und Schmettern,", "tokens": ["Wenn", "nach", "Don\u00b7ner", ",", "Schlag", "und", "Schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Phaeton die Wolcken bricht.", "tokens": ["Phae\u00b7ton", "die", "Wol\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wenn, eh man es kaum gemeinet,", "tokens": ["Wenn", ",", "eh", "man", "es", "kaum", "ge\u00b7mei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Uns die hellste Sonne scheinet.", "tokens": ["Uns", "die", "hells\u00b7te", "Son\u00b7ne", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Siehe nun, mein Hertz! und Leben!", "tokens": ["Sie\u00b7he", "nun", ",", "mein", "Hertz", "!", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$.", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich mich dir gantz ergeben,", "tokens": ["Wie", "ich", "mich", "dir", "gantz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich gegen dir gesinnt;", "tokens": ["Wie", "ich", "ge\u00b7gen", "dir", "ge\u00b7sinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O! ich wollte mehr erwehnen,", "tokens": ["O", "!", "ich", "woll\u00b7te", "mehr", "er\u00b7weh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ich scheue deine Thr\u00e4nen,", "tokens": ["Doch", "ich", "scheu\u00b7e", "dei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allerliebster Schatz! mein Kind!", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7ter", "Schatz", "!", "mein", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ich wei\u00df, mein Angedencken", "tokens": ["Denn", "ich", "wei\u00df", ",", "mein", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird dich offt empfindlich kr\u00e4ncken.", "tokens": ["Wird", "dich", "offt", "emp\u00b7find\u00b7lich", "kr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun wohlan! ich bin der Deine", "tokens": ["Nun", "wo\u00b7hlan", "!", "ich", "bin", "der", "Dei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "PPER",