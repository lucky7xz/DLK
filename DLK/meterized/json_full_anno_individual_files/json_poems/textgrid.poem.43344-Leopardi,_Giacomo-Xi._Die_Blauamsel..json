{"textgrid.poem.43344": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "Xi. Die Blauamsel.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herab von jenes alten Thurmes Zinne", "tokens": ["Her\u00b7ab", "von", "je\u00b7nes", "al\u00b7ten", "Thur\u00b7mes", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Singst du ins Feld hinaus, einsamer Vogel,", "tokens": ["Singst", "du", "ins", "Feld", "hin\u00b7aus", ",", "ein\u00b7sa\u00b7mer", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "APZR", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und erst des Tags Verscheiden macht dich stumm.", "tokens": ["Und", "erst", "des", "Tags", "Ver\u00b7schei\u00b7den", "macht", "dich", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der s\u00fc\u00dfe Wohllaut schweift durch dieses Thal;", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Wohl\u00b7laut", "schweift", "durch", "die\u00b7ses", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In L\u00fcften gl\u00e4nzt ringsum", "tokens": ["In", "L\u00fcf\u00b7ten", "gl\u00e4nzt", "ring\u00b7sum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Lenz und zieht frohlockend durch die Fluren,", "tokens": ["Der", "Lenz", "und", "zieht", "froh\u00b7lo\u00b7ckend", "durch", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df uns der Anblick z\u00e4rtlich r\u00fchrt die Brust.", "tokens": ["Da\u00df", "uns", "der", "An\u00b7blick", "z\u00e4rt\u00b7lich", "r\u00fchrt", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du h\u00f6rst die Schafe bl\u00f6ken, Rinder br\u00fcllen,", "tokens": ["Du", "h\u00f6rst", "die", "Scha\u00b7fe", "bl\u00f6\u00b7ken", ",", "Rin\u00b7der", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die andern frohen V\u00f6gel um die Wette", "tokens": ["Die", "an\u00b7dern", "fro\u00b7hen", "V\u00f6\u00b7gel", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In tausend Kreisen schw\u00e4rmen unterm Himmel,", "tokens": ["In", "tau\u00b7send", "Krei\u00b7sen", "schw\u00e4r\u00b7men", "un\u00b7term", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Frohlockend dieser Zeit, der lustgeweihten.", "tokens": ["Froh\u00b7lo\u00b7ckend", "die\u00b7ser", "Zeit", ",", "der", "lust\u00b7ge\u00b7weih\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du blickst von fern nachdenklich ins Get\u00fcmmel;", "tokens": ["Du", "blickst", "von", "fern", "nach\u00b7denk\u00b7lich", "ins", "Ge\u00b7t\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nicht an Gef\u00e4hrten, Fl\u00fcgen", "tokens": ["Nicht", "an", "Ge\u00b7f\u00e4hr\u00b7ten", ",", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Und heiterm Spiel magst du Gefallen finden.", "tokens": ["Und", "hei\u00b7term", "Spiel", "magst", "du", "Ge\u00b7fal\u00b7len", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Du singst, \u2013 und so entschwinden", "tokens": ["Du", "singst", ",", "\u2013", "und", "so", "ent\u00b7schwin\u00b7den"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "$(", "KON", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Dir deine wie des Jahres Bl\u00fctezeiten.", "tokens": ["Dir", "dei\u00b7ne", "wie", "des", "Jah\u00b7res", "Bl\u00fc\u00b7te\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie \u00e4hnlich, ach, verrinnt", "tokens": ["Wie", "\u00e4hn\u00b7lich", ",", "ach", ",", "ver\u00b7rinnt"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "$,", "ITJ", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Tag dem deinen! Muntrer Scherz und Lachen,", "tokens": ["Mein", "Tag", "dem", "dei\u00b7nen", "!", "Mun\u00b7trer", "Scherz", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPOSAT", "$.", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die stets der Jugendzeit Gespielen sind,", "tokens": ["Die", "stets", "der", "Ju\u00b7gend\u00b7zeit", "Ge\u00b7spie\u00b7len", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und du, der Jugend holde Schwester, Liebe,", "tokens": ["Und", "du", ",", "der", "Ju\u00b7gend", "hol\u00b7de", "Schwes\u00b7ter", ",", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du bittrer Seufzer unsrer reifern Tage,", "tokens": ["Du", "bit\u00b7trer", "Seuf\u00b7zer", "uns\u00b7rer", "rei\u00b7fern", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich r\u00fchrt ihr nicht; warum? ich wei\u00df es nicht;", "tokens": ["Mich", "r\u00fchrt", "ihr", "nicht", ";", "wa\u00b7rum", "?", "ich", "wei\u00df", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ja, euch entfl\u00f6h' ich gerne.", "tokens": ["Ja", ",", "euch", "ent\u00b7fl\u00f6h'", "ich", "ger\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fast allen Menschen ferne,", "tokens": ["Fast", "al\u00b7len", "Men\u00b7schen", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Fremd meinem Heimathort,", "tokens": ["Fremd", "mei\u00b7nem", "Hei\u00b7ma\u00b7thort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Seh' ich, wie meines Lebens Lenz verstreicht.", "tokens": ["Seh'", "ich", ",", "wie", "mei\u00b7nes", "Le\u00b7bens", "Lenz", "ver\u00b7streicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie pflegen diesen Tag, der nun sich neigt,", "tokens": ["Sie", "pfle\u00b7gen", "die\u00b7sen", "Tag", ",", "der", "nun", "sich", "neigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In unserm St\u00e4dtchen festlich zu begehn.", "tokens": ["In", "un\u00b7serm", "St\u00e4dt\u00b7chen", "fest\u00b7lich", "zu", "be\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Horch, wie durch klare Luft das Gl\u00f6ckchen t\u00f6nt,", "tokens": ["Horch", ",", "wie", "durch", "kla\u00b7re", "Luft", "das", "Gl\u00f6ck\u00b7chen", "t\u00f6nt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Horch, wie dazwischen oft aus Eisenr\u00f6hren", "tokens": ["Horch", ",", "wie", "da\u00b7zwi\u00b7schen", "oft", "aus", "Ei\u00b7sen\u00b7r\u00f6h\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PAV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ein Donnern fern von Haus zu Haus erdr\u00f6hnt.", "tokens": ["Ein", "Don\u00b7nern", "fern", "von", "Haus", "zu", "Haus", "er\u00b7dr\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Des Ortes Jugend heut", "tokens": ["Des", "Or\u00b7tes", "Ju\u00b7gend", "heut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "In ihren Feierkleidern", "tokens": ["In", "ih\u00b7ren", "Fei\u00b7er\u00b7klei\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Verl\u00e4\u00dft die H\u00e4user, wandelt hier- und dorthin", "tokens": ["Ver\u00b7l\u00e4\u00dft", "die", "H\u00e4u\u00b7ser", ",", "wan\u00b7delt", "hier", "und", "dor\u00b7thin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "TRUNC", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und schaut und l\u00e4\u00dft sich schau'n und ist vergn\u00fcgt.", "tokens": ["Und", "schaut", "und", "l\u00e4\u00dft", "sich", "schau'n", "und", "ist", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "VVINF", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ich geh' in Einsamkeit", "tokens": ["Ich", "geh'", "in", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Hinaus hier diesen abgelegnen Pfad.", "tokens": ["Hin\u00b7aus", "hier", "die\u00b7sen", "ab\u00b7ge\u00b7leg\u00b7nen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ach, alle Lust und Freude", "tokens": ["Ach", ",", "al\u00b7le", "Lust", "und", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Vertag' ich auf die Zukunft, und inde\u00df ich", "tokens": ["Ver\u00b7tag'", "ich", "auf", "die", "Zu\u00b7kunft", ",", "und", "in\u00b7de\u00df", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,", "KON", "ADV", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Den Blick ins Helle lenke,", "tokens": ["Den", "Blick", "ins", "Hel\u00b7le", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Trifft mich die Sonne, die von fernen Bergen", "tokens": ["Trifft", "mich", "die", "Son\u00b7ne", ",", "die", "von", "fer\u00b7nen", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So klar her\u00fcbersieht", "tokens": ["So", "klar", "her\u00b7\u00fc\u00b7ber\u00b7sieht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und scheidend mir zu sagen scheint: gedenke,", "tokens": ["Und", "schei\u00b7dend", "mir", "zu", "sa\u00b7gen", "scheint", ":", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PTKZU", "VVINF", "VVFIN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wie bald die sel'ge Jugendzeit entflieht.", "tokens": ["Wie", "bald", "die", "sel'\u00b7ge", "Ju\u00b7gend\u00b7zeit", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du, einsam V\u00f6gelchen, wenn sich zum Abend", "tokens": ["Du", ",", "ein\u00b7sam", "V\u00f6\u00b7gel\u00b7chen", ",", "wenn", "sich", "zum", "A\u00b7bend"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADJD", "NN", "$,", "KOUS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Leben neigt, das dir die Sterne g\u00f6nnen,", "tokens": ["Das", "Le\u00b7ben", "neigt", ",", "das", "dir", "die", "Ster\u00b7ne", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirst nicht beklagen dies", "tokens": ["Wirst", "nicht", "be\u00b7kla\u00b7gen", "dies"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "PDS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein stilles Dasein; denn aus der Natur", "tokens": ["Dein", "stil\u00b7les", "Da\u00b7sein", ";", "denn", "aus", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bl\u00fcht euch all euer Gl\u00fcck.", "tokens": ["Bl\u00fcht", "euch", "all", "eu\u00b7er", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch ich \u2013 l\u00e4\u00dft mein Geschick", "tokens": ["Doch", "ich", "\u2013", "l\u00e4\u00dft", "mein", "Ge\u00b7schick"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich zur verha\u00dften Schwelle", "tokens": ["Mich", "zur", "ver\u00b7ha\u00df\u00b7ten", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA"