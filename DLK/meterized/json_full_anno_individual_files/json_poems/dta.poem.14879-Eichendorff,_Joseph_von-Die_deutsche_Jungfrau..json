{"dta.poem.14879": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Jungfrau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da kam ein r\u00f6m'scher Rittersmann,", "tokens": ["Da", "kam", "ein", "r\u00f6m'\u00b7scher", "Rit\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ritt keck an die Burg hinan,", "tokens": ["Der", "ritt", "keck", "an", "die", "Burg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es blitzt sein Helm gar mannigfach,", "tokens": ["Es", "blitzt", "sein", "Helm", "gar", "man\u00b7nig\u00b7fach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6ne Ritter also sprach:", "tokens": ["Der", "sch\u00f6\u00b7ne", "Rit\u00b7ter", "al\u00b7so", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}