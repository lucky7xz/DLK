{"textgrid.poem.38446": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Was m\u00f6chtest du nicht?", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6cht vor tausend Thaler nicht,", "tokens": ["Ich", "m\u00f6cht", "vor", "tau\u00b7send", "Tha\u00b7ler", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "CARD", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mir der Kopf ab w\u00e4r,", "tokens": ["Da\u00df", "mir", "der", "Kopf", "ab", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da spr\u00e4ng ich mit dem Rumpf herum,", "tokens": ["Da", "spr\u00e4ng", "ich", "mit", "dem", "Rumpf", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fc\u00dft nicht, wo ich w\u00e4r,", "tokens": ["Und", "w\u00fc\u00dft", "nicht", ",", "wo", "ich", "w\u00e4r", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Leut schrien all und blieben stehn:", "tokens": ["Die", "Leut", "schri\u00b7en", "all", "und", "blie\u00b7ben", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "KON", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "lin