{"textgrid.poem.39359": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heil sei dir, Fried' und Wonne,", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil sei dir, Fried' und Wonne,", "tokens": ["Heil", "sei", "dir", ",", "Fried'", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Alles, was mir gebricht!", "tokens": ["Al\u00b7les", ",", "was", "mir", "ge\u00b7bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auch diese heitre Sonne", "tokens": ["Auch", "die\u00b7se", "heit\u00b7re", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hemmt meine Klage nicht.", "tokens": ["Hemmt", "mei\u00b7ne", "Kla\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6chte dir nur leben,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dir", "nur", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ganz dein eigen sein.", "tokens": ["Und", "ganz", "dein", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was hilft das eitle Streben?", "tokens": ["Was", "hilft", "das", "eit\u00b7le", "Stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin selber ja nicht mein.", "tokens": ["Bin", "sel\u00b7ber", "ja", "nicht", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich h\u00e4lt mein Loo\u00df gebunden,", "tokens": ["Mich", "h\u00e4lt", "mein", "Loo\u00df", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl ist es mir bewu\u00dft;", "tokens": ["Wohl", "ist", "es", "mir", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum sind die fl\u00fccht'gen Stunden", "tokens": ["Drum", "sind", "die", "fl\u00fccht'\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur qualenvolle Lust.", "tokens": ["Nur", "qua\u00b7len\u00b7vol\u00b7le", "Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist voll Lieb' und Milde,", "tokens": ["Du", "bist", "voll", "Lieb'", "und", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "mea