{"textgrid.poem.40464": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was dr\u00e4ngt das junge Laub der Eichen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dr\u00e4ngt das junge Laub der Eichen", "tokens": ["Was", "dr\u00e4ngt", "das", "jun\u00b7ge", "Laub", "der", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So frisch aus Maienlicht sich heute,", "tokens": ["So", "frisch", "aus", "Mai\u00b7en\u00b7licht", "sich", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieht doch unten Seinesgleichen,", "tokens": ["Und", "sieht", "doch", "un\u00b7ten", "Sei\u00b7nes\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des letzten Herbstwinds d\u00fcrre Beute!", "tokens": ["Des", "letz\u00b7ten", "Herbst\u00b7winds", "d\u00fcr\u00b7re", "Beu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was jauchzt die Nachtigall sanglodernd,", "tokens": ["Was", "jauchzt", "die", "Nach\u00b7ti\u00b7gall", "sang\u00b7lo\u00b7dernd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Als ob ihr horchten Ewigkeiten,", "tokens": ["Als", "ob", "ihr", "horch\u00b7ten", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieht doch ihre Schwester modernd,", "tokens": ["Und", "sieht", "doch", "ih\u00b7re", "Schwes\u00b7ter", "mo\u00b7dernd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Schnee sein Bahrtuch l\u00e4\u00dft entgleiten!", "tokens": ["Wenn", "Schnee", "sein", "Bahr\u00b7tuch", "l\u00e4\u00dft", "ent\u00b7glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was dr\u00e4ngt ihr, Lieder, euch vermessen,", "tokens": ["Was", "dr\u00e4ngt", "ihr", ",", "Lie\u00b7der", ",", "euch", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Dichtersaal Geh\u00f6r zu fordern,", "tokens": ["Im", "Dich\u00b7ter\u00b7saal", "Ge\u00b7h\u00f6r", "zu", "for\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und seht doch l\u00e4ngst verhallt, vergessen", "tokens": ["Und", "seht", "doch", "l\u00e4ngst", "ver\u00b7hallt", ",", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lieder edler Sangesvordern! \u2013", "tokens": ["Die", "Lie\u00b7der", "ed\u00b7ler", "San\u00b7ges\u00b7vor\u00b7dern", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und w\u00fc\u00dft' ich auch, ein Schutzgeist schreibe", "tokens": ["Und", "w\u00fc\u00dft'", "ich", "auch", ",", "ein", "Schutz\u00b7geist", "schrei\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Lied in Felsen unverdrossen,", "tokens": ["Mein", "Lied", "in", "Fel\u00b7sen", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aufbewahrt es Enkeln bleibe, \u2013", "tokens": ["Da\u00df", "auf\u00b7be\u00b7wahrt", "es", "En\u00b7keln", "blei\u00b7be", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVPP", "PPER", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hielte fest den Mund verschlossen.", "tokens": ["Ich", "hiel\u00b7te", "fest", "den", "Mund", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und w\u00fc\u00dft' ich, da\u00df zu fernen Zeiten", "tokens": ["Und", "w\u00fc\u00dft'", "ich", ",", "da\u00df", "zu", "fer\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jeglich Bild aus meinen S\u00e4ngen", "tokens": ["Ein", "jeg\u00b7lich", "Bild", "aus", "mei\u00b7nen", "S\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als Marmorbildni\u00df w\u00fcrde schreiten, \u2013", "tokens": ["Als", "Mar\u00b7mor\u00b7bild\u00b7ni\u00df", "w\u00fcr\u00b7de", "schrei\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "VAFIN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fest w\u00fcrd' ich zu die Lippen zw\u00e4ngen.", "tokens": ["Fest", "w\u00fcrd'", "ich", "zu", "die", "Lip\u00b7pen", "zw\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn freud'ge Ahnung im Gem\u00fcthe", "tokens": ["Denn", "freu\u00b7d'\u00b7ge", "Ah\u00b7nung", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Hoffnung will mich s\u00fc\u00df durchdringen,", "tokens": ["Und", "Hoff\u00b7nung", "will", "mich", "s\u00fc\u00df", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es werde unsres Daseins Bl\u00fcthe", "tokens": ["Es", "wer\u00b7de", "uns\u00b7res", "Da\u00b7seins", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einem neu'n Geschlecht sich j\u00fcngen;", "tokens": ["In", "ei\u00b7nem", "neu'n", "Ge\u00b7schlecht", "sich", "j\u00fcn\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das, Manneskraft im starken Busen", "tokens": ["Das", ",", "Man\u00b7nes\u00b7kraft", "im", "star\u00b7ken", "Bu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gotteslieb' im warmen Herzen,", "tokens": ["Und", "Got\u00b7tes\u00b7lieb'", "im", "war\u00b7men", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einst l\u00e4cheln mu\u00df ob unsrer Musen", "tokens": ["Einst", "l\u00e4\u00b7cheln", "mu\u00df", "ob", "uns\u00b7rer", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VMFIN", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fruchtlosen K\u00e4mpfen, m\u00fc\u00df'gen Scherzen.", "tokens": ["Frucht\u00b7lo\u00b7sen", "K\u00e4mp\u00b7fen", ",", "m\u00fc\u00df'\u00b7gen", "Scher\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Doch w\u00fcrden, wend' es Gott! die S\u00f6hne", "tokens": ["Doch", "w\u00fcr\u00b7den", ",", "wend'", "es", "Gott", "!", "die", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht edler als die V\u00e4ter wieder,", "tokens": ["Nicht", "ed\u00b7ler", "als", "die", "V\u00e4\u00b7ter", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "KOKOM", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann sind sie unsrer Schmerzenst\u00f6ne", "tokens": ["Dann", "sind", "sie", "uns\u00b7rer", "Schmer\u00b7zen\u00b7st\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht werth und unsrer Kampfeslieder.", "tokens": ["Nicht", "werth", "und", "uns\u00b7rer", "Kamp\u00b7fes\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und s\u00fc\u00dfer als ein ruhmlos Leben", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "als", "ein", "ruh\u00b7mlos", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im weiten, todesstillen Raume,", "tokens": ["Im", "wei\u00b7ten", ",", "to\u00b7des\u00b7stil\u00b7len", "Rau\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's, zu verklingen, zu verschweben,", "tokens": ["Ist's", ",", "zu", "ver\u00b7klin\u00b7gen", ",", "zu", "ver\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Blatt und Vogel sinkt vom Baume.", "tokens": ["Wie", "Blatt", "und", "Vo\u00b7gel", "sinkt", "vom", "Bau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn ihr nur einen Ast zersplittert,", "tokens": ["Wenn", "ihr", "nur", "ei\u00b7nen", "Ast", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bl\u00e4ttlein rei\u00dft vom Zweigesrande,", "tokens": ["Ein", "Bl\u00e4tt\u00b7lein", "rei\u00dft", "vom", "Zwei\u00b7ges\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Traun, ihr verletzt und ihr zerknittert", "tokens": ["Traun", ",", "ihr", "ver\u00b7letzt", "und", "ihr", "zer\u00b7knit\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVPP", "KON", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Lenz ein St\u00fcck vom Festgewande!", "tokens": ["Dem", "Lenz", "ein", "St\u00fcck", "vom", "Fest\u00b7ge\u00b7wan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Schie\u00dft ihr ein V\u00f6glein, leicht zu missen,", "tokens": ["Schie\u00dft", "ihr", "ein", "V\u00f6\u00b7glein", ",", "leicht", "zu", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Eines aus dem Schwarme nieder,", "tokens": ["Nur", "Ei\u00b7nes", "aus", "dem", "Schwar\u00b7me", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Fr\u00fchlings Lied habt ihr zerrissen,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Lied", "habt", "ihr", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ganze Vollklang ist's nicht wieder!", "tokens": ["Der", "gan\u00b7ze", "Voll\u00b7klang", "ist's", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "So ist mein Lied im Dichterlenze", "tokens": ["So", "ist", "mein", "Lied", "im", "Dich\u00b7ter\u00b7len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vogel nur, ein Blatt, ein Schimmer,", "tokens": ["Ein", "Vo\u00b7gel", "nur", ",", "ein", "Blatt", ",", "ein", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und fehlt es, bleibt noch g'nug dem Lenze,", "tokens": ["Und", "fehlt", "es", ",", "bleibt", "noch", "g'\u00b7nug", "dem", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch ist der ganze Lenz es nimmer.", "tokens": ["Doch", "ist", "der", "gan\u00b7ze", "Lenz", "es", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Drum gr\u00fcne k\u00fchn, Baum meiner Lieder,", "tokens": ["Drum", "gr\u00fc\u00b7ne", "k\u00fchn", ",", "Baum", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im Haine deutschen Sangs ein Sprosse,", "tokens": ["Im", "Hai\u00b7ne", "deut\u00b7schen", "Sangs", "ein", "Spros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Inmitten deiner sch\u00f6nern Br\u00fcder", "tokens": ["In\u00b7mit\u00b7ten", "dei\u00b7ner", "sch\u00f6\u00b7nern", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein treuer, heiterer Genosse.", "tokens": ["Ein", "treu\u00b7er", ",", "hei\u00b7te\u00b7rer", "Ge\u00b7nos\u00b7se", "."], "