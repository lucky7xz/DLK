{"dta.poem.7268": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Weiher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Er liegt so still im Morgenlicht,", "tokens": ["Er", "liegt", "so", "still", "im", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So friedlich, wie ein fromm Gewissen;", "tokens": ["So", "fried\u00b7lich", ",", "wie", "ein", "fromm", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Weste seinen Spiegel k\u00fcssen,", "tokens": ["Wenn", "Wes\u00b7te", "sei\u00b7nen", "Spie\u00b7gel", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ufers Blume f\u00fchlt es nicht;", "tokens": ["Des", "U\u00b7fers", "Blu\u00b7me", "f\u00fchlt", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Libellen zittern \u00fcber ihn,", "tokens": ["Li\u00b7bel\u00b7len", "zit\u00b7tern", "\u00fc\u00b7ber", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Blaugoldne St\u00e4bchen und Karmin,", "tokens": ["Blau\u00b7gold\u00b7ne", "St\u00e4b\u00b7chen", "und", "Kar\u00b7min", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und auf des Sonnenbildes Glanz", "tokens": ["Und", "auf", "des", "Son\u00b7nen\u00b7bil\u00b7des", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Wasserspinne f\u00fchrt den Tanz;", "tokens": ["Die", "Was\u00b7ser\u00b7spin\u00b7ne", "f\u00fchrt", "den", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schwertlilienkranz am Ufer steht", "tokens": ["Schwert\u00b7li\u00b7li\u00b7en\u00b7kranz", "am", "U\u00b7fer", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und horcht des Schilfes Schlummerliede;", "tokens": ["Und", "horcht", "des", "Schil\u00b7fes", "Schlum\u00b7mer\u00b7lie\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein lindes S\u00e4useln kommt und geht,", "tokens": ["Ein", "lin\u00b7des", "S\u00e4u\u00b7seln", "kommt", "und", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als fl\u00fcstr' es: Friede! Friede! Friede! \u2014", "tokens": ["Als", "fl\u00fcstr'", "es", ":", "Frie\u00b7de", "!", "Frie\u00b7de", "!", "Frie\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}