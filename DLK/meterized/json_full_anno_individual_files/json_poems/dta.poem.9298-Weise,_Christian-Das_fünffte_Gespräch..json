{"dta.poem.9298": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Marindgen hat ein solches hertze/", "tokens": ["Ma\u00b7rind\u00b7gen", "hat", "ein", "sol\u00b7ches", "hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das allen noch belieben kan/", "tokens": ["Das", "al\u00b7len", "noch", "be\u00b7lie\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie stehet auch in ernst und schertze", "tokens": ["Sie", "ste\u00b7het", "auch", "in", "ernst", "und", "schert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der compagnie nicht \u00fcbel an/", "tokens": ["Der", "com\u00b7pag\u00b7nie", "nicht", "\u00fc\u00b7bel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKNEG", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn mich sonsten nichts vertreibt/", "tokens": ["Und", "wenn", "mich", "sons\u00b7ten", "nichts", "ver\u00b7treibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bleib ich gerne wo sie bleibt.", "tokens": ["So", "bleib", "ich", "ger\u00b7ne", "wo", "sie", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Sie hat zwar allzeit ihres gleichen/", "tokens": ["Sie", "hat", "zwar", "all\u00b7zeit", "ih\u00b7res", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ist die allersch\u00f6nste nicht;", "tokens": ["Und", "ist", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wird auch deinen ruhm erreichen", "tokens": ["Sie", "wird", "auch", "dei\u00b7nen", "ruhm", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ein beliebtes angesicht:", "tokens": ["Durch", "ein", "be\u00b7lieb\u00b7tes", "an\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch lieb ich ihren sch\u00f6nen sinn", "tokens": ["Doch", "lieb", "ich", "ih\u00b7ren", "sch\u00f6\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mit ihr zu frieden bin.", "tokens": ["Da\u00df", "ich", "mit", "ihr", "zu", "frie\u00b7den", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Nur dieses will mir nicht gefallen/", "tokens": ["Nur", "die\u00b7ses", "will", "mir", "nicht", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich ein bi\u00dfgen h\u00f6fflich thu/", "tokens": ["Wenn", "ich", "ein", "bi\u00df\u00b7gen", "h\u00f6ff\u00b7lich", "thu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lacht das lose kind zu allen/", "tokens": ["So", "lacht", "das", "lo\u00b7se", "kind", "zu", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und denckt im hertzen wohl darzu/", "tokens": ["Und", "denckt", "im", "hert\u00b7zen", "wohl", "dar\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein mensch der solche worte giebt/", "tokens": ["Ein", "mensch", "der", "sol\u00b7che", "wor\u00b7te", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sey unfehlbar recht verliebt.", "tokens": ["Der", "sey", "un\u00b7fehl\u00b7bar", "recht", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ach stich dich nicht in meine liebe/", "tokens": ["Ach", "stich", "dich", "nicht", "in", "mei\u00b7ne", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrwahr es ist gefahr dabey:", "tokens": ["F\u00fcr\u00b7wahr", "es", "ist", "ge\u00b7fahr", "da\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mir das wasser aussen bliebe/", "tokens": ["Wenn", "mir", "das", "was\u00b7ser", "aus\u00b7sen", "blie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVIZU", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So brennte mir das hertz entzwey.", "tokens": ["So", "brenn\u00b7te", "mir", "das", "hertz", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun kenn\u2019 ich auch den rechten geist/", "tokens": ["Nun", "kenn'", "ich", "auch", "den", "rech\u00b7ten", "geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mich so sehr im leben reist.", "tokens": ["Der", "mich", "so", "sehr", "im", "le\u00b7ben", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ach nein es mu\u00df mir besser kommen/", "tokens": ["Ach", "nein", "es", "mu\u00df", "mir", "bes\u00b7ser", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe mich der l\u00f6ffeley", "tokens": ["Ich", "ha\u00b7be", "mich", "der", "l\u00f6f\u00b7fe\u00b7ley"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur aus gewonheit angenommen:", "tokens": ["Nur", "aus", "ge\u00b7won\u00b7heit", "an\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darumb erhalt ich mich darbey/", "tokens": ["Da\u00b7rumb", "er\u00b7halt", "ich", "mich", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und leid es gerne/ wenn ein kind", "tokens": ["Und", "leid", "es", "ger\u00b7ne", "/", "wenn", "ein", "kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "ADV", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir eine freundligkeit verg\u00fcnnt.", "tokens": ["Mir", "ei\u00b7ne", "freund\u00b7lig\u00b7keit", "ver\u00b7g\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ich nehm es leichtlich an und wandre", "tokens": ["Ich", "nehm", "es", "leicht\u00b7lich", "an", "und", "wand\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem durch die gantze stadt/", "tokens": ["Mit", "ei\u00b7nem", "durch", "die", "gant\u00b7ze", "stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein mensch ist warlich wie das andre/", "tokens": ["Ein", "mensch", "ist", "war\u00b7lich", "wie", "das", "and\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "KOKOM", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man nur was zu thalen hat:", "tokens": ["Wenn", "man", "nur", "was", "zu", "tha\u00b7len", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch kommt mir eine vors gesicht/", "tokens": ["Doch", "kommt", "mir", "ei\u00b7ne", "vors", "ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So la\u00df ich meine possen nicht.", "tokens": ["So", "la\u00df", "ich", "mei\u00b7ne", "pos\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Wiewol die m\u00e4dgen sind im hertzen/", "tokens": ["Wie\u00b7wol", "die", "m\u00e4d\u00b7gen", "sind", "im", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So viel ich schliesse/ gleich also/", "tokens": ["So", "viel", "ich", "schlies\u00b7se", "/", "gleich", "al\u00b7so", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$(", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie suchen ihre lust im schertzen/", "tokens": ["Sie", "su\u00b7chen", "ih\u00b7re", "lust", "im", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sind von gantzer seele froh", "tokens": ["Und", "sind", "von", "gant\u00b7zer", "see\u00b7le", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn jemand seine pflicht erweist/", "tokens": ["Wenn", "je\u00b7mand", "sei\u00b7ne", "pflicht", "er\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er mag auch heissen wie er heist.", "tokens": ["Er", "mag", "auch", "heis\u00b7sen", "wie", "er", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Marindgen darff ich di\u00df bekennen/", "tokens": ["Ma\u00b7rind\u00b7gen", "darff", "ich", "di\u00df", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun so gef\u00e4llt mir alles wohl:", "tokens": ["Nun", "so", "ge\u00b7f\u00e4llt", "mir", "al\u00b7les", "wohl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kan zwar nicht vor liebe brennen/", "tokens": ["Ich", "kan", "zwar", "nicht", "vor", "lie\u00b7be", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch wofern ich schertzen soll/", "tokens": ["Je\u00b7doch", "wo\u00b7fern", "ich", "schert\u00b7zen", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So will ich stets in dem geruhn/", "tokens": ["So", "will", "ich", "stets", "in", "dem", "ge\u00b7ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dirs am allerliebsten thun.", "tokens": ["Und", "dirs", "am", "al\u00b7ler\u00b7liebs\u00b7ten", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}