{"textgrid.poem.64868": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ferne von dem frohen Kreise klagst du, es sei Nacht in dir?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ferne von dem frohen Kreise klagst du, es sei Nacht in dir?", "tokens": ["Fer\u00b7ne", "von", "dem", "fro\u00b7hen", "Krei\u00b7se", "klagst", "du", ",", "es", "sei", "Nacht", "in", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber, sprich! sie zu erhellen, hast du nicht die Macht in dir?", "tokens": ["A\u00b7ber", ",", "sprich", "!", "sie", "zu", "er\u00b7hel\u00b7len", ",", "hast", "du", "nicht", "die", "Macht", "in", "dir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$.", "PPER", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Gib nicht auf das Unverlorne! scheue Arbeit nicht nach M\u00fch,", "tokens": ["Gib", "nicht", "auf", "das", "Un\u00b7ver\u00b7lor\u00b7ne", "!", "scheu\u00b7e", "Ar\u00b7beit", "nicht", "nach", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "APPR", "ART", "NN", "$.", "ADJA", "NN", "PTKNEG", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und durchw\u00fchl' den unersch\u00f6pflich rei