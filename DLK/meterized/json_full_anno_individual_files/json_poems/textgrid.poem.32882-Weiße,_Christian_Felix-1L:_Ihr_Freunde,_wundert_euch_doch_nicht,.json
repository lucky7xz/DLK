{"textgrid.poem.32882": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Freunde, wundert euch doch nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Freunde, wundert euch doch nicht,", "tokens": ["Ihr", "Freun\u00b7de", ",", "wun\u00b7dert", "euch", "doch", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Titan stets so fr\u00fch erwachet,", "tokens": ["Da\u00df", "Ti\u00b7tan", "stets", "so", "fr\u00fch", "er\u00b7wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da mich kaum sein mitt\u00e4glich Licht", "tokens": ["Da", "mich", "kaum", "sein", "mit\u00b7t\u00e4g\u00b7lich", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Mahlzeit wieder munter machet.", "tokens": ["Zur", "Mahl\u00b7zeit", "wie\u00b7der", "mun\u00b7ter", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Freunde, kann das anders seyn?", "tokens": ["Ihr", "Freun\u00b7de", ",", "kann", "das", "an\u00b7ders", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PDS", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambi