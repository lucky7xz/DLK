{"dta.poem.21352": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne ging auf bey Paderborn,", "tokens": ["Die", "Son\u00b7ne", "ging", "auf", "bey", "Pa\u00b7der\u00b7born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit sehr verdross\u2019ner Gebehrde.", "tokens": ["Mit", "sehr", "ver\u00b7dross'\u00b7ner", "Ge\u00b7behr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie treibt in der That ein verdrie\u00dflich Gesch\u00e4ft \u2013", "tokens": ["Sie", "treibt", "in", "der", "That", "ein", "ver\u00b7drie\u00df\u00b7lich", "Ge\u00b7sch\u00e4ft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Beleuchten die dumme Erde!", "tokens": ["Be\u00b7leuch\u00b7ten", "die", "dum\u00b7me", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}