{"textgrid.poem.39049": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit ich Florenz verlie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ich Florenz verlie\u00df", "tokens": ["Seit", "ich", "Flo\u00b7renz", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vermi\u00df' ich Italien,", "tokens": ["Ver\u00b7mi\u00df'", "ich", "I\u00b7ta\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Alle Berge d\u00fcnken mir klein,", "tokens": ["Al\u00b7le", "Ber\u00b7ge", "d\u00fcn\u00b7ken", "mir", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Alle Formen der Landschaft beengend:", "tokens": ["Al\u00b7le", "For\u00b7men", "der", "Land\u00b7schaft", "be\u00b7en\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "O wie sehnt sich mein Auge nach dir", "tokens": ["O", "wie", "sehnt", "sich", "mein", "Au\u00b7ge", "nach", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PPER"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Du erhabene r\u00f6mische Ferne,", "tokens": ["Du", "er\u00b7ha\u00b7be\u00b7ne", "r\u00f6\u00b7mi\u00b7sche", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Mit deinen hochschwebenden Gebirgen,", "tokens": ["Mit", "dei\u00b7nen", "hoch\u00b7schwe\u00b7ben\u00b7den", "Ge\u00b7bir\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Der weiten, ausgedehnten Landschaft.", "tokens": ["Der", "wei\u00b7ten", ",", "aus\u00b7ge\u00b7dehn\u00b7ten", "Land\u00b7schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Todt nennen sie deine Natur?", "tokens": ["Todt", "nen\u00b7nen", "sie", "dei\u00b7ne", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Noch immer ruhen, wie liebliche Tr\u00e4ume,", "tokens": ["Noch", "im\u00b7mer", "ru\u00b7hen", ",", "wie", "lieb\u00b7li\u00b7che", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Deine d\u00e4mmernden Gestalten vor den Augen meines Geistes,", "tokens": ["Dei\u00b7ne", "d\u00e4m\u00b7mern\u00b7den", "Ge\u00b7stal\u00b7ten", "vor", "den", "Au\u00b7gen", "mei\u00b7nes", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und wie man nach langem, innigen Gespr\u00e4ch,", "tokens": ["Und", "wie", "man", "nach", "lan\u00b7gem", ",", "in\u00b7ni\u00b7gen", "Ge\u00b7spr\u00e4ch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "In der Versammlung der Fremden den Freund vermi\u00dft,", "tokens": ["In", "der", "Ver\u00b7samm\u00b7lung", "der", "Frem\u00b7den", "den", "Freund", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "So erseufz' ich nach Roms Gegenwart", "tokens": ["So", "er\u00b7seuf\u00b7z'", "ich", "nach", "Roms", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wenn man mir dieses wohl und jenes sch\u00f6n will nennen.", "tokens": ["Wenn", "man", "mir", "die\u00b7ses", "wohl", "und", "je\u00b7nes", "sch\u00f6n", "will", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PDS", "ADV", "KON", "PDS", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Seit ich Florenz verlie\u00df", "tokens": ["Seit", "ich", "Flo\u00b7renz", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vermi\u00df' ich Italien,", "tokens": ["Ver\u00b7mi\u00df'", "ich", "I\u00b7ta\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "