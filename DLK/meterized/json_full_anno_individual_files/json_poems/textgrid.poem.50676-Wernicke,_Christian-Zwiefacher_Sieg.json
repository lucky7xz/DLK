{"textgrid.poem.50676": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Zwiefacher Sieg", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie leicht ist Einer \u00fcberwunden,", "tokens": ["Wie", "leicht", "ist", "Ei\u00b7ner", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man die rechte Zeit zur Rache hat gefunden! \u2013", "tokens": ["Wenn", "man", "die", "rech\u00b7te", "Zeit", "zur", "Ra\u00b7che", "hat", "ge\u00b7fun\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erdr\u00fcck' ihn! er verdient's! Es steht dir Alles frei;", "tokens": ["Er\u00b7dr\u00fcck", "ihn", "!", "er", "ver\u00b7dient's", "!", "Es", "steht", "dir", "Al\u00b7les", "frei", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch wo du ihm vergiebst, so \u00fcberwind'st du ", "tokens": ["Doch", "wo", "du", "ihm", "ver\u00b7giebst", ",", "so", "\u00fc\u00b7ber\u00b7win\u00b7d'st", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2"