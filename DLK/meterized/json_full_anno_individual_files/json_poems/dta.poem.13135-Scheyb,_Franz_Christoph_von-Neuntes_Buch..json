{"dta.poem.13135": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu, Dicht-Kunst! prahl dich nur! was wei\u00df man von der Zeit", "tokens": ["\u201e", "du", ",", "Dicht\u00b7Kunst", "!", "prahl", "dich", "nur", "!", "was", "wei\u00df", "man", "von", "der", "Zeit"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eda nichts als dein Gesang die Helden eingeweiht?", "tokens": ["\u201e", "da", "nichts", "als", "dein", "Ge\u00b7sang", "die", "Hel\u00b7den", "ein\u00b7ge\u00b7weiht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eh\u00e4tt man sich meines Kiels, nicht deiner Leyr bedienet;", "tokens": ["\u201e", "h\u00e4tt", "man", "sich", "mei\u00b7nes", "Kiels", ",", "nicht", "dei\u00b7ner", "Leyr", "be\u00b7die\u00b7net", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "PRF", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso wu\u00dfte man, was dort zum Heil des Lands gegr\u00fcnet.", "tokens": ["\u201e", "so", "wu\u00df\u00b7te", "man", ",", "was", "dort", "zum", "Heil", "des", "Lands", "ge\u00b7gr\u00fc\u00b7net", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "$,", "PRELS", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas hilfft der Cither Klang? wo ist dein Ehrenmahl?", "tokens": ["\u201e", "was", "hilfft", "der", "Cit\u00b7her", "Klang", "?", "wo", "ist", "dein", "Eh\u00b7ren\u00b7mahl", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "50\u201dWas wei\u00df man jezt davon? zeig jener Helden Zahl", "tokens": ["\"", "Was", "wei\u00df", "man", "jezt", "da\u00b7von", "?", "zeig", "je\u00b7ner", "Hel\u00b7den", "Zahl"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PIS", "ADV", "PAV", "$.", "VVFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewoher der Deutschen Herz, Treu, Gro\u00dfmuth, Ruhm entsprungen;", "tokens": ["\u201e", "wo\u00b7her", "der", "Deut\u00b7schen", "Herz", ",", "Treu", ",", "Gro\u00df\u00b7muth", ",", "Ruhm", "ent\u00b7sprun\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eich h\u00e4tte sie dem Fra\u00df der Zeiten abgezwungen.", "tokens": ["\u201e", "ich", "h\u00e4t\u00b7te", "sie", "dem", "Fra\u00df", "der", "Zei\u00b7ten", "ab\u00b7ge\u00b7zwun\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201emein Schall verliehrt sich nicht, er klinget immerfort,", "tokens": ["\u201e", "mein", "Schall", "ver\u00b7liehrt", "sich", "nicht", ",", "er", "klin\u00b7get", "im\u00b7mer\u00b7fort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edie Zeit verzehrt ihn nicht, sie sch\u00e4rfft ein jedes Wort;", "tokens": ["\u201e", "die", "Zeit", "ver\u00b7zehrt", "ihn", "nicht", ",", "sie", "sch\u00e4rfft", "ein", "je\u00b7des", "Wort", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "55\u201dVon mir bekommt der Ruhm stets Kraft und neues Leben,", "tokens": ["\"", "Von", "mir", "be\u00b7kommt", "der", "Ruhm", "stets", "Kraft", "und", "neu\u00b7es", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "ART", "NN", "ADV", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eselbst die Unsterblichkeit pflegt mir das Lob zu geben.", "tokens": ["\u201e", "selbst", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "pflegt", "mir", "das", "Lob", "zu", "ge\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--++--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}