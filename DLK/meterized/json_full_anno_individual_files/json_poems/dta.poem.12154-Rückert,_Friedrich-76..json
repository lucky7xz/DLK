{"dta.poem.12154": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein weites Zimmer macht weit die Gedankenwelt,", "tokens": ["Ein", "wei\u00b7tes", "Zim\u00b7mer", "macht", "weit", "die", "Ge\u00b7dan\u00b7ken\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein sch\u00f6nes helles hat den Sinn versch\u00f6nt, erhellt.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "hel\u00b7les", "hat", "den", "Sinn", "ver\u00b7sch\u00f6nt", ",", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da kann kein Filosof ein dumpf System erbau'n,", "tokens": ["Da", "kann", "kein", "Fi\u00b7lo\u00b7sof", "ein", "dumpf", "Sys\u00b7tem", "er\u00b7bau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und kein Poet darin tr\u00fcbsel'ge Verse brau'n.", "tokens": ["Und", "kein", "Po\u00b7et", "da\u00b7rin", "tr\u00fcb\u00b7sel'\u00b7ge", "Ver\u00b7se", "brau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PIAT", "NN", "PAV", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}