{"textgrid.poem.26555": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4ngst, du freundliches Nachtgestirn,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ngst, du freundliches Nachtgestirn,", "tokens": ["L\u00e4ngst", ",", "du", "freund\u00b7li\u00b7ches", "Nacht\u00b7ges\u00b7tirn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ist dein Geheimni\u00df verweht.", "tokens": ["Ist", "dein", "Ge\u00b7heim\u00b7ni\u00df", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Erkenntni\u00dfstolz blickt der Knabe schon", "tokens": ["Er\u00b7kennt\u00b7ni\u00df\u00b7stolz", "blickt", "der", "Kna\u00b7be", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu dir empor;", "tokens": ["Zu", "dir", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Denn verfallen bist du, wie Alles jetzt,", "tokens": ["Denn", "ver\u00b7fal\u00b7len", "bist", "du", ",", "wie", "Al\u00b7les", "jetzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "$,", "PWAV", "PIS", "ADV", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der Wissenschaft,", "tokens": ["Der", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die deine H\u00f6hen und Tiefen mi\u00dft \u2013", "tokens": ["Die", "dei\u00b7ne", "H\u00f6\u00b7hen", "und", "Tie\u00b7fen", "mi\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wer wei\u00df, ob du nicht endlich doch noch", "tokens": ["Und", "wer", "wei\u00df", ",", "ob", "du", "nicht", "end\u00b7lich", "doch", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Erstiegen wirst auf der M\u00fcnchhausenleiter", "tokens": ["Er\u00b7stie\u00b7gen", "wirst", "auf", "der", "M\u00fcnch\u00b7hau\u00b7sen\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Der Hypothesen.", "tokens": ["Der", "Hy\u00b7po\u00b7the\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Dennoch, du alter, treuer Begleiter der Erde,", "tokens": ["Den\u00b7noch", ",", "du", "al\u00b7ter", ",", "treu\u00b7er", "Be\u00b7glei\u00b7ter", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Webt und wirkt dein alter Zauber fort,", "tokens": ["Webt", "und", "wirkt", "dein", "al\u00b7ter", "Zau\u00b7ber", "fort", ","], "token_info": ["word", "wor