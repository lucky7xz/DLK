etra"}}, "stanza.33": {"line.1": {"text": "Sonst wird sie Venus auff dem Wagn", "tokens": ["Sonst", "wird", "sie", "Ve\u00b7nus", "auff", "dem", "Wagn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hoch als Sonn' und Monde tragn;", "tokens": ["So", "hoch", "als", "Sonn'", "und", "Mon\u00b7de", "tragn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst wird mein' arme Seele stehn,", "tokens": ["Sonst", "wird", "mein'", "ar\u00b7me", "See\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo jetzund die Gestirn' auffgehn.", "tokens": ["Wo", "je\u00b7tzund", "die", "Ge\u00b7stirn'", "auff\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.34": {"line.1": {"text": "Die Sternen umb de\u00df Himmels Feldt,", "tokens": ["Die", "Ster\u00b7nen", "umb", "de\u00df", "Him\u00b7mels", "Feldt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So n\u00e4chtlich leuchten aller Welt,", "tokens": ["So", "n\u00e4cht\u00b7lich", "leuch\u00b7ten", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die waren Buhler vor der Zeit,", "tokens": ["Die", "wa\u00b7ren", "Buh\u00b7ler", "vor", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt stehen sie von Noth befreyt.", "tokens": ["Jetzt", "ste\u00b7hen", "sie", "von", "Noth", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.35": {"line.1": {"text": "Sie stehn und haben fleissig Acht,", "tokens": ["Sie", "stehn", "und", "ha\u00b7ben", "fleis\u00b7sig", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJD", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Pein mir wird von dir gemacht;", "tokens": ["Was", "Pein", "mir", "wird", "von", "dir", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch sie erfehrts der Venus Sohn,", "tokens": ["Durch", "sie", "er\u00b7fehrts", "der", "Ve\u00b7nus", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird dir geben rechten Lohn.", "tokens": ["Der", "wird", "dir", "ge\u00b7ben", "rech\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.36": {"line.1": {"text": "Die Namen werden auffgesetzt;", "tokens": ["Die", "Na\u00b7men", "wer\u00b7den", "auff\u00b7ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer denn mit Zehren sich genetzt", "tokens": ["Wer", "denn", "mit", "Zeh\u00b7ren", "sich", "ge\u00b7netzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am meisten auff der Welt allhier,", "tokens": ["Am", "meis\u00b7ten", "auff", "der", "Welt", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den zeucht man Andern dorte f\u00fcr.", "tokens": ["Den", "zeucht", "man", "An\u00b7dern", "dor\u00b7te", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.37": {"line.1": {"text": "Die beste Stell' ist wol f\u00fcr mich,", "tokens": ["Die", "bes\u00b7te", "Stell'", "ist", "wol", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drumb sterb' ich jetzund williglich;", "tokens": ["Drumb", "sterb'", "ich", "je\u00b7tzund", "wil\u00b7lig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wil ich seyn dein h\u00f6chster Spott,", "tokens": ["Da", "wil", "ich", "seyn", "dein", "h\u00f6chs\u00b7ter", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die du mich bringest in den Todt.", "tokens": ["Die", "du", "mich", "brin\u00b7gest", "in", "den", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.38": {"line.1": {"text": "Ich gehe nun und la\u00df' allhier", "tokens": ["Ich", "ge\u00b7he", "nun", "und", "la\u00df'", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVIMP", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die heissen Threnen vor der Th\u00fcr;", "tokens": ["Die", "heis\u00b7sen", "Thre\u00b7nen", "vor", "der", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch soll ich fort, denck' auch vorhin,", "tokens": ["Doch", "soll", "ich", "fort", ",", "denck", "auch", "vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich des Todtes schuldig bin.", "tokens": ["Ob", "ich", "des", "Tod\u00b7tes", "schul\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}