{"textgrid.poem.49184": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "8. Nachtklage", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt blicken durch de\u00df Himmels Saal", "tokens": ["Jetzt", "bli\u00b7cken", "durch", "de\u00df", "Him\u00b7mels", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die g\u00fcldnen Sternen allzumal,", "tokens": ["Die", "g\u00fcld\u00b7nen", "Ster\u00b7nen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ohn' Hoffnung gantz allein,", "tokens": ["Ich", "bin", "ohn'", "Hoff\u00b7nung", "gantz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wach', und Andre schlaffen ein.", "tokens": ["Ich", "wach'", ",", "und", "And\u00b7re", "schlaf\u00b7fen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, Jungfrau, liegest in der Rhu", "tokens": ["Du", ",", "Jung\u00b7frau", ",", "lie\u00b7gest", "in", "der", "Rhu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hast die stoltzen Augen zu;", "tokens": ["Und", "hast", "die", "stolt\u00b7zen", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bl\u00e4sest durch den rothen Mundt", "tokens": ["Du", "bl\u00e4\u00b7sest", "durch", "den", "ro\u00b7then", "Mundt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das s\u00fcsse Gifft, so mich verwundt.", "tokens": ["Das", "s\u00fcs\u00b7se", "Gifft", ",", "so", "mich", "ver\u00b7wundt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du denckest nicht an meine Noth,", "tokens": ["Du", "den\u00b7ckest", "nicht", "an", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch an den s\u00fcssen Liebesgott,", "tokens": ["Noch", "an", "den", "s\u00fcs\u00b7sen", "Lie\u00b7bes\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mein Gem\u00fct' und Sinn hat bracht", "tokens": ["Der", "mein", "Ge\u00b7m\u00fct'", "und", "Sinn", "hat", "bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deine Hand und grosse Macht.", "tokens": ["In", "dei\u00b7ne", "Hand", "und", "gros\u00b7se", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich lieg' an deiner tauben Th\u00fcr,", "tokens": ["Ich", "lieg'", "an", "dei\u00b7ner", "tau\u00b7ben", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob ich doch m\u00f6ge kommen f\u00fcr", "tokens": ["Ob", "ich", "doch", "m\u00f6\u00b7ge", "kom\u00b7men", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diesen unbewegten Sinn", "tokens": ["Und", "die\u00b7sen", "un\u00b7be\u00b7weg\u00b7ten", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch meine Bitte zu mir ziehn.", "tokens": ["Durch", "mei\u00b7ne", "Bit\u00b7te", "zu", "mir", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was sonst bey Tag' irrt hin und her,", "tokens": ["Was", "sonst", "bey", "Tag'", "irrt", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schnellen Fisch' und auch ihr Meer,", "tokens": ["Die", "schnel\u00b7len", "Fisch'", "und", "auch", "ihr", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind sicher, geben sich zu Rhu,", "tokens": ["Sind", "si\u00b7cher", ",", "ge\u00b7ben", "sich", "zu", "Rhu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "PRF", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bringe nur kein Auge zu.", "tokens": ["Ich", "brin\u00b7ge", "nur", "kein", "Au\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Threnen ruff' ich Zeugen an", "tokens": ["Die", "Thre\u00b7nen", "ruff'", "ich", "Zeu\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Damit ich dich nicht zwingen kan,", "tokens": ["Da\u00b7mit", "ich", "dich", "nicht", "zwin\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Threnen, so ich dir zur Schand'", "tokens": ["Die", "Thre\u00b7nen", ",", "so", "ich", "dir", "zur", "Schand'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier la\u00df' als meiner Liebe Pfand.", "tokens": ["Hier", "la\u00df'", "als", "mei\u00b7ner", "Lie\u00b7be", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein jeglichs Ding hat seine Zeit;", "tokens": ["Ein", "jeg\u00b7lichs", "Ding", "hat", "sei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann es gefroren und geschneyt,", "tokens": ["Wann", "es", "ge\u00b7fro\u00b7ren", "und", "ge\u00b7schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht sich der Westwind auff die Bahn,", "tokens": ["Macht", "sich", "der", "West\u00b7wind", "auff", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Legt allem neue Kleider an.", "tokens": ["Legt", "al\u00b7lem", "neu\u00b7e", "Klei\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das eine fellt, das andre steht;", "tokens": ["Das", "ei\u00b7ne", "fellt", ",", "das", "and\u00b7re", "steht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann Phebus auff die Wache geht", "tokens": ["Wann", "Phe\u00b7bus", "auff", "die", "Wa\u00b7che", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weicht Luna weg; will sie entstehn,", "tokens": ["Weicht", "Lu\u00b7na", "weg", ";", "will", "sie", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df Phebus dann zu Bette gehn.", "tokens": ["Mu\u00df", "Phe\u00b7bus", "dann", "zu", "Bet\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es geht doch alles nach Geb\u00fchr;", "tokens": ["Es", "geht", "doch", "al\u00b7les", "nach", "Ge\u00b7b\u00fchr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwey Dinge bleiben f\u00fcr und f\u00fcr;", "tokens": ["Zwey", "Din\u00b7ge", "blei\u00b7ben", "f\u00fcr", "und", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein harter Sinn und meine Pein", "tokens": ["Dein", "har\u00b7ter", "Sinn", "und", "mei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die musten gantz unendlich seyn.", "tokens": ["Die", "mus\u00b7ten", "gantz", "un\u00b7end\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Ursach ist mein treuer Sinn,", "tokens": ["Die", "Ur\u00b7sach", "ist", "mein", "treu\u00b7er", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich best\u00e4ndig bey dir bin", "tokens": ["Weil", "ich", "be\u00b7st\u00e4n\u00b7dig", "bey", "dir", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und liebe dich noch diesen Tag", "tokens": ["Und", "lie\u00b7be", "dich", "noch", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sehr als ich vor lengst schon pflag.", "tokens": ["So", "sehr", "als", "ich", "vor", "lengst", "schon", "pflag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "APPR", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich bin kein Schiff nicht in der See,", "tokens": ["Ich", "bin", "kein", "Schiff", "nicht", "in", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das nach de\u00df Windes Wellen geh;", "tokens": ["Das", "nach", "de\u00df", "Win\u00b7des", "Wel\u00b7len", "geh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich halt' allein bey dir fest' an", "tokens": ["Ich", "halt'", "al\u00b7lein", "bey", "dir", "fest'", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Glauben, der nicht wancken kan.", "tokens": ["Mit", "Glau\u00b7ben", ",", "der", "nicht", "wan\u00b7cken", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Und di\u00df ist, Jungfrau, meine Schuld.", "tokens": ["Und", "di\u00df", "ist", ",", "Jung\u00b7frau", ",", "mei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wolan, ich gehe mit Gedult", "tokens": ["Wo\u00b7lan", ",", "ich", "ge\u00b7he", "mit", "Ge\u00b7dult"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und such' ein Ende meiner Pein;", "tokens": ["Und", "such'", "ein", "En\u00b7de", "mei\u00b7ner", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will dir nicht beschwerlich seyn.", "tokens": ["Ich", "will", "dir", "nicht", "be\u00b7schwer\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Gehab dich wol, ich scheid' jetzt ab,", "tokens": ["Ge\u00b7hab", "dich", "wol", ",", "ich", "scheid'", "jetzt", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gehab dich wol, ich eil' ins Grab.", "tokens": ["Ge\u00b7hab", "dich", "wol", ",", "ich", "eil'", "ins", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimb meine Seel' allein und blo\u00df", "tokens": ["Nimb", "mei\u00b7ne", "Seel'", "al\u00b7lein", "und", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KON", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zu dir in deine zarte Scho\u00df.", "tokens": ["Zu", "dir", "in", "dei\u00b7ne", "zar\u00b7te", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sonst wird sie Venus auff dem Wagn", "tokens": ["Sonst", "wird", "sie", "Ve\u00b7nus", "auff", "dem", "Wagn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hoch als Sonn' und Monde tragn;", "tokens": ["So", "hoch", "als", "Sonn'", "und", "Mon\u00b7de", "tragn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst wird mein' arme Seele stehn,", "tokens": ["Sonst", "wird", "mein'", "ar\u00b7me", "See\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo jetzund die Gestirn' auffgehn.", "tokens": ["Wo", "je\u00b7tzund", "die", "Ge\u00b7stirn'", "auff\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Die Sternen umb de\u00df Himmels Feldt,", "tokens": ["Die", "Ster\u00b7nen", "umb", "de\u00df", "Him\u00b7mels", "Feldt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So n\u00e4chtlich leuchten aller Welt,", "tokens": ["So", "n\u00e4cht\u00b7lich", "leuch\u00b7ten", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die waren Buhler vor der Zeit,", "tokens": ["Die", "wa\u00b7ren", "Buh\u00b7ler", "vor", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt stehen sie von Noth befreyt.", "tokens": ["Jetzt", "ste\u00b7hen", "sie", "von", "Noth", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Sie stehn und haben fleissig Acht,", "tokens": ["Sie", "stehn", "und", "ha\u00b7ben", "fleis\u00b7sig", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJD", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Pein mir wird von dir gemacht;", "tokens": ["Was", "Pein", "mir", "wird", "von", "dir", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch sie erfehrts der Venus Sohn,", "tokens": ["Durch", "sie", "er\u00b7fehrts", "der", "Ve\u00b7nus", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wird dir geben rechten Lohn.", "tokens": ["Der", "wird", "dir", "ge\u00b7ben", "rech\u00b7ten", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Die Namen werden auffgesetzt;", "tokens": ["Die", "Na\u00b7men", "wer\u00b7den", "auff\u00b7ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer denn mit Zehren sich genetzt", "tokens": ["Wer", "denn", "mit", "Zeh\u00b7ren", "sich", "ge\u00b7netzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am meisten auff der Welt allhier,", "tokens": ["Am", "meis\u00b7ten", "auff", "der", "Welt", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den zeucht man Andern dorte f\u00fcr.", "tokens": ["Den", "zeucht", "man", "An\u00b7dern", "dor\u00b7te", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Die beste Stell' ist wol f\u00fcr mich,", "tokens": ["Die", "bes\u00b7te", "Stell'", "ist", "wol", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drumb sterb' ich jetzund williglich;", "tokens": ["Drumb", "sterb'", "ich", "je\u00b7tzund", "wil\u00b7lig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wil ich seyn dein h\u00f6chster Spott,", "tokens": ["Da", "wil", "ich", "seyn", "dein", "h\u00f6chs\u00b7ter", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die du mich bringest in den Todt.", "tokens": ["Die", "du", "mich", "brin\u00b7gest", "in", "den", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Ich gehe nun und la\u00df' allhier", "tokens": ["Ich", "ge\u00b7he", "nun", "und", "la\u00df'", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVIMP", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die heissen Threnen vor der Th\u00fcr;", "tokens": ["Die", "heis\u00b7sen", "Thre\u00b7nen", "vor", "der", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch soll ich fort, denck' auch vorhin,", "tokens": ["Doch", "soll", "ich", "fort", ",", "denck", "auch", "vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich des Todtes schuldig bin.", "tokens": ["Ob", "ich", "des", "Tod\u00b7tes", "schul\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Jetzt blicken durch de\u00df Himmels Saal", "tokens": ["Jetzt", "bli\u00b7cken", "durch", "de\u00df", "Him\u00b7mels", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die g\u00fcldnen Sternen allzumal,", "tokens": ["Die", "g\u00fcld\u00b7nen", "Ster\u00b7nen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ohn' Hoffnung gantz allein,", "tokens": ["Ich", "bin", "ohn'", "Hoff\u00b7nung", "gantz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wach', und Andre schlaffen ein.", "tokens": ["Ich", "wach'", ",", "und", "And\u00b7re", "schlaf\u00b7fen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Du, Jungfrau, liegest in der Rhu", "tokens": ["Du", ",", "Jung\u00b7frau", ",", "lie\u00b7gest", "in", "der", "Rhu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hast die stoltzen Augen zu;", "tokens": ["Und", "hast", "die", "stolt\u00b7zen", "Au\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bl\u00e4sest durch den rothen Mundt", "tokens": ["Du", "bl\u00e4\u00b7sest", "durch", "den", "ro\u00b7then", "Mundt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das s\u00fcsse Gifft, so mich verwundt.", "tokens": ["Das", "s\u00fcs\u00b7se", "Gifft", ",", "so", "mich", "ver\u00b7wundt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Du denckest nicht an meine Noth,", "tokens": ["Du", "den\u00b7ckest", "nicht", "an", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text":