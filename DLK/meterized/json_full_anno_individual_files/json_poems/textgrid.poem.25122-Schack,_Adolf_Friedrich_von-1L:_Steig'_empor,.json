{"textgrid.poem.25122": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steig' empor,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steig' empor,", "tokens": ["Steig'", "em\u00b7por", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Herrlichste der Sonnen,", "tokens": ["Herr\u00b7lichs\u00b7te", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Die \u00fcber Deutschland geleuchtet!", "tokens": ["Die", "\u00fc\u00b7ber", "Deutschland", "ge\u00b7leuch\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O den Tag, den du bringst,", "tokens": ["O", "den", "Tag", ",", "den", "du", "bringst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ganz und voll zu genie\u00dfen,", "tokens": ["Ganz", "und", "voll", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Ist es genug nicht des Gl\u00fccks f\u00fcr ein Leben?", "tokens": ["Ist", "es", "ge\u00b7nug", "nicht", "des", "Gl\u00fccks", "f\u00fcr", "ein", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Den sterbenden Greis", "tokens": ["Den", "ster\u00b7ben\u00b7den", "Greis"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "La\u00df das Auge nicht schlie\u00dfen,", "tokens": ["La\u00df", "das", "Au\u00b7ge", "nicht", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Bevor er ihn erblickt,", "tokens": ["Be\u00b7vor", "er", "ihn", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und in der Wiege dem S\u00e4ugling", "tokens": ["Und", "in", "der", "Wie\u00b7ge", "dem", "S\u00e4ug\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Oeffne des Geistes Sehkraft,", "tokens": ["Oeff\u00b7ne", "des", "Geis\u00b7tes", "Seh\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Da\u00df sein Gedanke ihn fasse,", "tokens": ["Da\u00df", "sein", "Ge\u00b7dan\u00b7ke", "ihn", "fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Und er einst noch den Enkeln k\u00fcnde:", "tokens": ["Und", "er", "einst", "noch", "den", "En\u00b7keln", "k\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Ich habe den gro\u00dfen Tag erlebt.", "tokens": ["Ich", "ha\u00b7be", "den", "gro\u00b7\u00dfen", "Tag", "er\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Horch! Trommelwirbel", "tokens": ["Horch", "!", "Trom\u00b7mel\u00b7wir\u00b7bel"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$.", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Und Fall von hunderttausend Tritten!", "tokens": ["Und", "Fall", "von", "hun\u00b7dert\u00b7tau\u00b7send", "Trit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie sind es, sie nahen,", "tokens": ["Sie", "sind", "es", ",", "sie", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPER", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Die durch den Donner der Schlachten", "tokens": ["Die", "durch", "den", "Don\u00b7ner", "der", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Ueber st\u00fcrzender Br\u00fcder Leichen dahin", "tokens": ["Ue\u00b7ber", "st\u00fcr\u00b7zen\u00b7der", "Br\u00fc\u00b7der", "Lei\u00b7chen", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "PAV"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Deutschlands Banner getragen!", "tokens": ["Deutschlands", "Ban\u00b7ner", "ge\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Noch scheinen ihre Lanzen", "tokens": ["Noch", "schei\u00b7nen", "ih\u00b7re", "Lan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Vom Wirbelsturm des Kampfes zu zittern.", "tokens": ["Vom", "Wir\u00b7bels\u00b7turm", "des", "Kamp\u00b7fes", "zu", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Doch Hoch! erschallt es, Hoch!", "tokens": ["Doch", "Hoch", "!", "er\u00b7schallt", "es", ",", "Hoch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$.", "VVFIN", "PPER", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Durch des Volkes wogende Reihen,", "tokens": ["Durch", "des", "Vol\u00b7kes", "wo\u00b7gen\u00b7de", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Und mit dem Gr\u00fcn des Friedens bekr\u00e4nzt,", "tokens": ["Und", "mit", "dem", "Gr\u00fcn", "des", "Frie\u00b7dens", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Wallen durchs Thor die Siegesfahnen.", "tokens": ["Wal\u00b7len", "durchs", "Thor", "die", "Sie\u00b7ges\u00b7fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Gen Himmel flackert", "tokens": ["Gen", "Him\u00b7mel", "fla\u00b7ckert"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Im Sonnenlichte der Glanz", "tokens": ["Im", "Son\u00b7nen\u00b7lich\u00b7te", "der", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Der wogenden Helme und Waffen,", "tokens": ["Der", "wo\u00b7gen\u00b7den", "Hel\u00b7me", "und", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Wie durch die geschm\u00fcckten Stra\u00dfen", "tokens": ["Wie", "durch", "die", "ge\u00b7schm\u00fcck\u00b7ten", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Der Zug der Krieger sich w\u00e4lzt,", "tokens": ["Der", "Zug", "der", "Krie\u00b7ger", "sich", "w\u00e4lzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Und Fanfarengeschmetter nun", "tokens": ["Und", "Fan\u00b7fa\u00b7ren\u00b7ge\u00b7schmet\u00b7ter", "nun"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und hochaufhallender Jubelruf;", "tokens": ["Und", "hoch\u00b7auf\u00b7hal\u00b7len\u00b7der", "Ju\u00b7bel\u00b7ruf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Sie kommen, die glorreichen F\u00fchrer,", "tokens": ["Sie", "kom\u00b7men", ",", "die", "glor\u00b7rei\u00b7chen", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Die Lieblinge des Ruhmes,", "tokens": ["Die", "Lieb\u00b7lin\u00b7ge", "des", "Ruh\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Die noch nach Jahrtausenden", "tokens": ["Die", "noch", "nach", "Jahr\u00b7tau\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "In ungeborner V\u00f6lker", "tokens": ["In", "un\u00b7ge\u00b7bor\u00b7ner", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Ges\u00e4ngen leben werden!", "tokens": ["Ge\u00b7s\u00e4n\u00b7gen", "le\u00b7ben", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Aus ihrer Mitte hervor,", "tokens": ["Aus", "ih\u00b7rer", "Mit\u00b7te", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "Wie Orion unter den anderen Sternen,", "tokens": ["Wie", "O\u00b7rion", "un\u00b7ter", "den", "an\u00b7de\u00b7ren", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Leuchtet der Herrliche,", "tokens": ["Leuch\u00b7tet", "der", "Herr\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.42": {"text": "Der Retter Deutschlands!", "tokens": ["Der", "Ret\u00b7ter", "Deutschlands", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "La\u00dft Platz f\u00fcr sein Ro\u00df,", "tokens": ["La\u00dft", "Platz", "f\u00fcr", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.44": {"text": "Ihr Weiber, die mit euren Kleinen", "tokens": ["Ihr", "Wei\u00b7ber", ",", "die", "mit", "eu\u00b7ren", "Klei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Heran ihr euch dr\u00e4ngt,", "tokens": ["He\u00b7ran", "ihr", "euch", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.46": {"text": "Um, seine Kniee umklammernd, ihm zu danken,", "tokens": ["Um", ",", "sei\u00b7ne", "Kni\u00b7ee", "um\u00b7klam\u00b7mernd", ",", "ihm", "zu", "dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "PPOSAT", "NN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Da\u00df er euch Haus und Herd", "tokens": ["Da\u00df", "er", "euch", "Haus", "und", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Vor Schande gesch\u00fctzt!", "tokens": ["Vor", "Schan\u00b7de", "ge\u00b7sch\u00fctzt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "Wohl mehr, als des Krieges Gew\u00fchl,", "tokens": ["Wohl", "mehr", ",", "als", "des", "Krie\u00b7ges", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.50": {"text": "Liebt er, Kinder um sich spielen zu sehen;", "tokens": ["Liebt", "er", ",", "Kin\u00b7der", "um", "sich", "spie\u00b7len", "zu", "se\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "APPR", "PRF", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.51": {"text": "Aber noch ", "tokens": ["A\u00b7ber", "noch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.52": {"text": "Eh zur Pflugschar das Schwert sich wandelt,", "tokens": ["Eh", "zur", "Pflug\u00b7schar", "das", "Schwert", "sich", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.53": {"text": "In seines Heeres Mitte", "tokens": ["In", "sei\u00b7nes", "Hee\u00b7res", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Mit den krachenden Feuerschl\u00fcnden", "tokens": ["Mit", "den", "kra\u00b7chen\u00b7den", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.55": {"text": "Mu\u00df er Zwiesprach' halten.", "tokens": ["Mu\u00df", "er", "Zwie\u00b7sprach'", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.56": {"text": "Horch! das sind die ehernen Stimmen \u2013", "tokens": ["Horch", "!", "das", "sind", "die", "e\u00b7her\u00b7nen", "Stim\u00b7men", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.57": {"text": "Er kennt sie \u2013,", "tokens": ["Er", "kennt", "sie", "\u2013", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.58": {"text": "Die ihn in zwanzig Siegesschlachten umdonnert,", "tokens": ["Die", "ihn", "in", "zwan\u00b7zig", "Sie\u00b7ges\u00b7schlach\u00b7ten", "um\u00b7don\u00b7nert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.59": {"text": "Vor denen hundert Vesten", "tokens": ["Vor", "de\u00b7nen", "hun\u00b7dert", "Ves\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Und ein Reich in Tr\u00fcmmer gesunken.", "tokens": ["Und", "ein", "Reich", "in", "Tr\u00fcm\u00b7mer", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "wor