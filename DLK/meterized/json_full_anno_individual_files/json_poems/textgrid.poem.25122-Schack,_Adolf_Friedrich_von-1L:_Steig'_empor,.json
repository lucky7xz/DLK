"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Der Zug der Krieger sich w\u00e4lzt,", "tokens": ["Der", "Zug", "der", "Krie\u00b7ger", "sich", "w\u00e4lzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Und Fanfarengeschmetter nun", "tokens": ["Und", "Fan\u00b7fa\u00b7ren\u00b7ge\u00b7schmet\u00b7ter", "nun"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und hochaufhallender Jubelruf;", "tokens": ["Und", "hoch\u00b7auf\u00b7hal\u00b7len\u00b7der", "Ju\u00b7bel\u00b7ruf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Sie kommen, die glorreichen F\u00fchrer,", "tokens": ["Sie", "kom\u00b7men", ",", "die", "glor\u00b7rei\u00b7chen", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Die Lieblinge des Ruhmes,", "tokens": ["Die", "Lieb\u00b7lin\u00b7ge", "des", "Ruh\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Die noch nach Jahrtausenden", "tokens": ["Die", "noch", "nach", "Jahr\u00b7tau\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "In ungeborner V\u00f6lker", "tokens": ["In", "un\u00b7ge\u00b7bor\u00b7ner", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Ges\u00e4ngen leben werden!", "tokens": ["Ge\u00b7s\u00e4n\u00b7gen", "le\u00b7ben", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Aus ihrer Mitte hervor,", "tokens": ["Aus", "ih\u00b7rer", "Mit\u00b7te", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "Wie Orion unter den anderen Sternen,", "tokens": ["Wie", "O\u00b7rion", "un\u00b7ter", "den", "an\u00b7de\u00b7ren", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Leuchtet der Herrliche,", "tokens": ["Leuch\u00b7tet", "der", "Herr\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.42": {"text": "Der Retter Deutschlands!", "tokens": ["Der", "Ret\u00b7ter", "Deutschlands", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "La\u00dft Platz f\u00fcr sein Ro\u00df,", "tokens": ["La\u00dft", "Platz", "f\u00fcr", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.44": {"text": "Ihr Weiber, die mit euren Kleinen", "tokens": ["Ihr", "Wei\u00b7ber", ",", "die", "mit", "eu\u00b7ren", "Klei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Heran ihr euch dr\u00e4ngt,", "tokens": ["He\u00b7ran", "ihr", "euch", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.46": {"text": "Um, seine Kniee umklammernd, ihm zu danken,", "tokens": ["Um", ",", "sei\u00b7ne", "Kni\u00b7ee", "um\u00b7klam\u00b7mernd", ",", "ihm", "zu", "dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "PPOSAT", "NN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "Da\u00df er euch Haus und Herd", "tokens": ["Da\u00df", "er", "euch", "Haus", "und", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Vor Schande gesch\u00fctzt!", "tokens": ["Vor", "Schan\u00b7de", "ge\u00b7sch\u00fctzt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "Wohl mehr, als des Krieges Gew\u00fchl,", "tokens": ["Wohl", "mehr", ",", "als", "des", "Krie\u00b7ges", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.50": {"text": "Liebt er, Kinder um sich spielen zu sehen;", "tokens": ["Liebt", "er", ",", "Kin\u00b7der", "um", "sich", "spie\u00b7len", "zu", "se\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "APPR", "PRF", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.51": {"text": "Aber noch ", "tokens": ["A\u00b7ber", "noch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.52": {"text": "Eh zur Pflugschar das Schwert sich wandelt,", "tokens": ["Eh", "zur", "Pflug\u00b7schar", "das", "Schwert", "sich", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.53": {"text": "In seines Heeres Mitte", "tokens": ["In", "sei\u00b7nes", "Hee\u00b7res", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Mit den krachenden Feuerschl\u00fcnden", "tokens": ["Mit", "den", "kra\u00b7chen\u00b7den", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.55": {"text": "Mu\u00df er Zwiesprach' halten.", "tokens": ["Mu\u00df", "er", "Zwie\u00b7sprach'", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.56": {"text": "Horch! das sind die ehernen Stimmen \u2013", "tokens": ["Horch", "!", "das", "sind", "die", "e\u00b7her\u00b7nen", "Stim\u00b7men", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.57": {"text": "Er kennt sie \u2013,", "tokens": ["Er", "kennt", "sie", "\u2013", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.58": {"text": "Die ihn in zwanzig Siegesschlachten umdonnert,", "tokens": ["Die", "ihn", "in", "zwan\u00b7zig", "Sie\u00b7ges\u00b7schlach\u00b7ten", "um\u00b7don\u00b7nert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.59": {"text": "Vor denen hundert Vesten", "tokens": ["Vor", "de\u00b7nen", "hun\u00b7dert", "Ves\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Und ein Reich in Tr\u00fcmmer gesunken.", "tokens": ["Und", "ein", "Reich", "in", "Tr\u00fcm\u00b7mer", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.61": {"text": "Von allen T\u00fcrmen die Glocken fallen ein,", "tokens": ["Von", "al\u00b7len", "T\u00fcr\u00b7men", "die", "Glo\u00b7cken", "fal\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.62": {"text": "O! und weiter dahin, durch den Blumenregen,", "tokens": ["O", "!", "und", "wei\u00b7ter", "da\u00b7hin", ",", "durch", "den", "Blu\u00b7men\u00b7re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "ADV", "PAV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.63": {"text": "Der von Fenstern und D\u00e4chern niederst\u00e4ubt,", "tokens": ["Der", "von", "Fens\u00b7tern", "und", "D\u00e4\u00b7chern", "nie\u00b7der\u00b7st\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.64": {"text": "Zieht er achtlos vor\u00fcber an uns,", "tokens": ["Zieht", "er", "acht\u00b7los", "vor\u00b7\u00fc\u00b7ber", "an", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.65": {"text": "Denen an der Wimper die Freudenthr\u00e4ne zittert,", "tokens": ["De\u00b7nen", "an", "der", "Wim\u00b7per", "die", "Freu\u00b7dent\u00b7hr\u00e4\u00b7ne", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.66": {"text": "W\u00e4hrend die Lippe verstummt", "tokens": ["W\u00e4h\u00b7rend", "die", "Lip\u00b7pe", "ver\u00b7stummt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.67": {"text": "Und nur des Herzens Klopfen", "tokens": ["Und", "nur", "des", "Her\u00b7zens", "Klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Dank ihm stammelt,", "tokens": ["Dank", "ihm", "stam\u00b7melt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.69": {"text": "Da\u00df er uns ein Vaterland geschenkt.", "tokens": ["Da\u00df", "er", "uns", "ein", "Va\u00b7ter\u00b7land", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "tro