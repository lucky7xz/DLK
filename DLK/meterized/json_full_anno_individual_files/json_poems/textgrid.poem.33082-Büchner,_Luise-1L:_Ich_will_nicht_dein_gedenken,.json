er": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es trotzt ihr jede Stund \u2013", "tokens": ["Es", "trotzt", "ihr", "je\u00b7de", "Stund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie wird des Herzens Gl\u00fchen", "tokens": ["Nie", "wird", "des", "Her\u00b7zens", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Besiegt vom weisen Mund!", "tokens": ["Be\u00b7siegt", "vom", "wei\u00b7sen", "Mund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich will nicht dein gedenken,", "tokens": ["Ich", "will", "nicht", "dein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sollst nicht mehr bei mir sein", "tokens": ["Sollst", "nicht", "mehr", "bei", "mir", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADV", "APPR", "PPER", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In allem meinem Denken,", "tokens": ["In", "al\u00b7lem", "mei\u00b7nem", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinem ganzen Sein.", "tokens": ["In", "mei\u00b7nem", "gan\u00b7zen", "Sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line