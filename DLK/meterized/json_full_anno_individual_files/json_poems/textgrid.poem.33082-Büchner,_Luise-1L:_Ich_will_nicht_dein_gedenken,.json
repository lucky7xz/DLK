{"textgrid.poem.33082": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will nicht dein gedenken,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will nicht dein gedenken,", "tokens": ["Ich", "will", "nicht", "dein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sollst nicht mehr bei mir sein", "tokens": ["Sollst", "nicht", "mehr", "bei", "mir", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADV", "APPR", "PPER", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In allem meinem Denken,", "tokens": ["In", "al\u00b7lem", "mei\u00b7nem", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinem ganzen Sein.", "tokens": ["In", "mei\u00b7nem", "gan\u00b7zen", "Sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Rose wird gepfl\u00fccket", "tokens": ["Die", "Ro\u00b7se", "wird", "ge\u00b7pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Sturm, an einem Tag,", "tokens": ["Vom", "Sturm", ",", "an", "ei\u00b7nem", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Felsen selbst zerst\u00fccket", "tokens": ["Den", "Fel\u00b7sen", "selbst", "zer\u00b7st\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein einz'ger Donnerschlag.", "tokens": ["Ein", "einz'\u00b7ger", "Don\u00b7ner\u00b7schlag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So will ich's auch erringen,", "tokens": ["So", "will", "ich's", "auch", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Alles ist geweiht \u2013", "tokens": ["Dem", "Al\u00b7les", "ist", "ge\u00b7weiht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell soll dies Herz erzwingen", "tokens": ["Schnell", "soll", "dies", "Herz", "er\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PDS", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich die Verge\u00dflichkeit!", "tokens": ["Sich", "die", "Ver\u00b7ge\u00df\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nicht, wie ja Alles m\u00fcde", "tokens": ["Nicht", ",", "wie", "ja", "Al\u00b7les", "m\u00fc\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PWAV", "ADV", "PIS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Grabe endlich schwankt,", "tokens": ["Zu", "Gra\u00b7be", "end\u00b7lich", "schwa