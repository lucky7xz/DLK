{"textgrid.poem.62989": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der L\u00f6we war an Kr\u00e4ften ganz ersch\u00f6pft,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we war an Kr\u00e4ften ganz ersch\u00f6pft,", "tokens": ["Der", "L\u00f6\u00b7we", "war", "an", "Kr\u00e4f\u00b7ten", "ganz", "er\u00b7sch\u00f6pft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die kalte Gicht durchw\u00fchlte seine Glieder,", "tokens": ["Die", "kal\u00b7te", "Gicht", "durch\u00b7w\u00fchl\u00b7te", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst ward er gerieben und geschr\u00f6pft,", "tokens": ["Um\u00b7sonst", "ward", "er", "ge\u00b7rie\u00b7ben", "und", "ge\u00b7schr\u00f6pft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Quell des Lebens flo\u00df nicht wieder.", "tokens": ["Der", "Quell", "des", "Le\u00b7bens", "flo\u00df", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Hofstaat lie\u00df sich Tag vor Tag", "tokens": ["Sein", "Hof\u00b7staat", "lie\u00df", "sich", "Tag", "vor", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit traurigem Gesicht vor seinem Bette sehen,", "tokens": ["Mit", "trau\u00b7ri\u00b7gem", "Ge\u00b7sicht", "vor", "sei\u00b7nem", "Bet\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Um ihm mit Rath und H\u00fclfe beyzustehen.", "tokens": ["Um", "ihm", "mit", "Rath", "und", "H\u00fcl\u00b7fe", "bey\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Einst mi\u00dfte man den Fuchs. Ein voller Taubenschlag", "tokens": ["Einst", "mi\u00df\u00b7te", "man", "den", "Fuchs", ".", "Ein", "vol\u00b7ler", "Tau\u00b7ben\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NE", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gab ihm auf einem Dorf zu schaffen.", "tokens": ["Gab", "ihm", "auf", "ei\u00b7nem", "Dorf", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da sieht mans, sprach der Wolf zum Affen,", "tokens": ["Da", "sieht", "mans", ",", "sprach", "der", "Wolf", "zum", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "VVFIN", "ART", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So leise da\u00df dem Schach kein Wort entgieng,", "tokens": ["So", "lei\u00b7se", "da\u00df", "dem", "Schach", "kein", "Wort", "ent\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der B\u00f6sewicht fragt einen Pfifferling", "tokens": ["Der", "B\u00f6\u00b7se\u00b7wicht", "fragt", "ei\u00b7nen", "Pfif\u00b7fer\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nach seines guten K\u00f6nigs Qualen.", "tokens": ["Nach", "sei\u00b7nes", "gu\u00b7ten", "K\u00f6\u00b7nigs", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Hier ward die Wuth des L\u00f6wen aufgeweckt;", "tokens": ["Hier", "ward", "die", "Wuth", "des", "L\u00f6\u00b7wen", "auf\u00b7ge\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Er schwur: das soll er mir mit seinem Blut bezahlen;", "tokens": ["Er", "schwur", ":", "das", "soll", "er", "mir", "mit", "sei\u00b7nem", "Blut", "be\u00b7zah\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Nachricht wird dem Fuchs gesteckt;", "tokens": ["Die", "Nach\u00b7richt", "wird", "dem", "Fuchs", "ge\u00b7steckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Er kam des andern Tags mit heitern Mienen", "tokens": ["Er", "kam", "des", "an\u00b7dern", "Tags", "mit", "hei\u00b7tern", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Zum alten Schach. Was hielt dich gestern ab,", "tokens": ["Zum", "al\u00b7ten", "Schach", ".", "Was", "hielt", "dich", "ge\u00b7stern", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Verr\u00e4ther? \u2013 Sir! der Eifer dir zu dienen:", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", "?", "\u2013", "Sir", "!", "der", "Ei\u00b7fer", "dir", "zu", "die\u00b7nen", ":"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$.", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ich lief nach Epidaur, dem Helfer Aeskulap", "tokens": ["Ich", "lief", "nach", "E\u00b7pi\u00b7daur", ",", "dem", "Hel\u00b7fer", "A\u00b7es\u00b7ku\u00b7lap"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "NE"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Durch mein Gebet ein Mittel abzudringen,", "tokens": ["Durch", "mein", "Ge\u00b7bet", "ein", "Mit\u00b7tel", "ab\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Das deine Gicht zerstreun, die Kr\u00e4fte wiederbringen,", "tokens": ["Das", "dei\u00b7ne", "Gicht", "zer\u00b7streun", ",", "die", "Kr\u00e4f\u00b7te", "wie\u00b7der\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja gleich dem Ph\u00f6nix dich verj\u00fcngen kann.", "tokens": ["Ja", "gleich", "dem", "Ph\u00f6\u00b7nix", "dich", "ver\u00b7j\u00fcn\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ists m\u00f6glich! rief der Schach; ha, bester Freund, sag an! \u2013", "tokens": ["Ists", "m\u00f6g\u00b7lich", "!", "rief", "der", "Schach", ";", "ha", ",", "bes\u00b7ter", "Freund", ",", "sag", "an", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$.", "VVFIN", "ART", "NN", "$.", "ITJ", "$,", "ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du darfst dich, sprach er, nur nach des Orakels Willen", "tokens": ["Du", "darfst", "dich", ",", "sprach", "er", ",", "nur", "nach", "des", "O\u00b7ra\u00b7kels", "Wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In eine warme Wolfshaut h\u00fcllen,", "tokens": ["In", "ei\u00b7ne", "war\u00b7me", "Wolfs\u00b7haut", "h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "So ist das ganze Werk gethan.", "tokens": ["So", "ist", "das", "gan\u00b7ze", "Werk", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ey, ey, rief Isegrim, Gott Aeskulap will spassen,", "tokens": ["Ey", ",", "ey", ",", "rief", "I\u00b7seg\u00b7rim", ",", "Gott", "A\u00b7es\u00b7ku\u00b7lap", "will", "spas\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VVFIN", "NE", "$,", "NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+---+-+-", "measure": "unknown.measure.hexa"}, "line.29": {"text": "Und schlich der Th\u00fcre zu. Der L\u00f6we winkt dem B\u00e4r,", "tokens": ["Und", "schlich", "der", "Th\u00fc\u00b7re", "zu", ".", "Der", "L\u00f6\u00b7we", "winkt", "dem", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PTKVZ", "$.", "ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dem Tiger und dem Hund, den Sp\u00f6tter anzufassen,", "tokens": ["Dem", "Ti\u00b7ger", "und", "dem", "Hund", ",", "den", "Sp\u00f6t\u00b7ter", "an\u00b7zu\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und kurz, er mu\u00dfte sich, trotz aller Gegenwehr,", "tokens": ["Und", "kurz", ",", "er", "mu\u00df\u00b7te", "sich", ",", "trotz", "al\u00b7ler", "Ge\u00b7gen\u00b7wehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VMFIN", "PRF", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auf seiner Majest\u00e4t Gesundheit schinden lassen.", "tokens": ["Auf", "sei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", "Ge\u00b7sund\u00b7heit", "schin\u00b7den", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der L\u00f6we war an Kr\u00e4ften ganz ersch\u00f6pft,", "tokens": ["Der", "L\u00f6\u00b7we", "war", "an", "Kr\u00e4f\u00b7ten", "ganz", "er\u00b7sch\u00f6pft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die kalte Gicht durchw\u00fchlte seine Glieder,", "tokens": ["Die", "kal\u00b7te", "Gicht", "durch\u00b7w\u00fchl\u00b7te", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst ward er gerieben und geschr\u00f6pft,", "tokens": ["Um\u00b7sonst", "ward", "er", "ge\u00b7rie\u00b7ben", "und", "ge\u00b7schr\u00f6pft", ","], "token_info": ["word", "word", "word", "word", 