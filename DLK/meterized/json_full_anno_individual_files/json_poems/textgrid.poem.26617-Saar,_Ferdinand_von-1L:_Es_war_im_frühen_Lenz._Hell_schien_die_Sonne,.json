{"textgrid.poem.26617": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war im fr\u00fchen Lenz. Hell schien die Sonne,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war im fr\u00fchen Lenz. Hell schien die Sonne,", "tokens": ["Es", "war", "im", "fr\u00fc\u00b7hen", "Lenz", ".", "Hell", "schien", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$.", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beleuchtend warm das zarte junge Gr\u00fcn;", "tokens": ["Be\u00b7leuch\u00b7tend", "warm", "das", "zar\u00b7te", "jun\u00b7ge", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Zittern rings, ein Athmen still in Wonne \u2013", "tokens": ["Ein", "Zit\u00b7tern", "rings", ",", "ein", "Ath\u00b7men", "still", "in", "Won\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und duft'ger Veilchen tief verborg'nes Bl\u00fch'n.", "tokens": ["Und", "duft'\u00b7ger", "Veil\u00b7chen", "tief", "ver\u00b7bor\u00b7g'\u00b7nes", "Bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJA", "NN", "ADJD", "ADJA", "NN", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Ich hatte froh gen\u00fctzt den holden Morgen", "tokens": ["Ich", "hat\u00b7te", "froh", "ge\u00b7n\u00fctzt", "den", "hol\u00b7den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu einem Gang in's weit gedehnte Land \u2013", "tokens": ["Zu", "ei\u00b7nem", "Gang", "in's", "weit", "ge\u00b7dehn\u00b7te", "Land", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun kehrt' ich wieder zu den alten Sorgen,", "tokens": ["Nun", "kehrt'", "ich", "wie\u00b7der", "zu", "den", "al\u00b7ten", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Stadt zur\u00fcck l\u00e4ngs eines Baches Rand.", "tokens": ["Zur", "Stadt", "zu\u00b7r\u00fcck", "l\u00e4ngs", "ei\u00b7nes", "Ba\u00b7ches", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die ersten H\u00e4user! Fast schon im Verfallen,", "tokens": ["Die", "ers\u00b7ten", "H\u00e4u\u00b7ser", "!", "Fast", "schon", "im", "Ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Obgleich man sie erst k\u00fcrzlich aufgebaut!", "tokens": ["Ob\u00b7gleich", "man", "sie", "erst", "k\u00fcrz\u00b7lich", "auf\u00b7ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus hohen Schloten sah ich's d\u00fcster wallen", "tokens": ["Aus", "ho\u00b7hen", "Schlo\u00b7ten", "sah", "ich's", "d\u00fcs\u00b7ter", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00f6rte der Fabriken Arbeitslaut.", "tokens": ["Und", "h\u00f6r\u00b7te", "der", "Fab\u00b7ri\u00b7ken", "Ar\u00b7beits\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O welche Luft, beklemmend und mephitisch,", "tokens": ["O", "wel\u00b7che", "Luft", ",", "be\u00b7klem\u00b7mend", "und", "me\u00b7phi\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da schon mein Fu\u00df die Gassen jetzt beschritt!", "tokens": ["Da", "schon", "mein", "Fu\u00df", "die", "Gas\u00b7sen", "jetzt", "be\u00b7schritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Halbnackte Kinder, blutleer und rhachitisch", "tokens": ["Halb\u00b7nack\u00b7te", "Kin\u00b7der", ",", "blut\u00b7leer", "und", "rhac\u00b7hi\u00b7tisch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor jeder Schwelle und bei jedem Tritt!", "tokens": ["Vor", "je\u00b7der", "Schwel\u00b7le", "und", "bei", "je\u00b7dem", "Tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und Buden rechts und links mit schlechten Waaren,", "tokens": ["Und", "Bu\u00b7den", "rechts", "und", "links", "mit", "schlech\u00b7ten", "Waa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf die selbst Hunger nur mit Ekel trifft;", "tokens": ["Auf", "die", "selbst", "Hun\u00b7ger", "nur", "mit", "E\u00b7kel", "trifft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zahlreiche Schenken f\u00fcr verkomm'ne Schaaren,", "tokens": ["Zahl\u00b7rei\u00b7che", "Schen\u00b7ken", "f\u00fcr", "ver\u00b7kom\u00b7m'\u00b7ne", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die sich bet\u00e4uben mit des Branntweins Gift.", "tokens": ["Die", "sich", "be\u00b7t\u00e4u\u00b7ben", "mit", "des", "Brannt\u00b7weins", "Gift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dazwischen \u00f6des kleines Handwerktreiben", "tokens": ["Da\u00b7zwi\