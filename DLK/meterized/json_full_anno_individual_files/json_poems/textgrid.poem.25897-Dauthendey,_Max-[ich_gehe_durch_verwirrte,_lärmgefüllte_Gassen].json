{"textgrid.poem.25897": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[ich gehe durch verwirrte, l\u00e4rmgef\u00fcllte Gassen]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gehe durch verwirrte, l\u00e4rmgef\u00fcllte Gassen", "tokens": ["Ich", "ge\u00b7he", "durch", "ver\u00b7wirr\u00b7te", ",", "l\u00e4rm\u00b7ge\u00b7f\u00fcll\u00b7te", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ratlos hin, zur\u00fcck, und trete in ein unbekanntes Haus.", "tokens": ["Rat\u00b7los", "hin", ",", "zu\u00b7r\u00fcck", ",", "und", "tre\u00b7te", "in", "ein", "un\u00b7be\u00b7kann\u00b7tes", "Haus", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Durch Korridore, T\u00fcren, Zimmer finde \u00f6den Weg", "tokens": ["Durch", "Kor\u00b7ri\u00b7do\u00b7re", ",", "T\u00fc\u00b7ren", ",", "Zim\u00b7mer", "fin\u00b7de", "\u00f6\u00b7den", "Weg"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und komme in den alten, hohen B\u00fcchersaal,", "tokens": ["Und", "kom\u00b7me", "in", "den", "al\u00b7ten", ",", "ho\u00b7hen", "B\u00fc\u00b7cher\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Still, weltfern lebte hier nur sanfter Staub,", "tokens": ["Still", ",", "welt\u00b7fern", "leb\u00b7te", "hier", "nur", "sanf\u00b7ter", "Staub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAT", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Geistesabwesend schien das Saalgesicht.", "tokens": ["Geis\u00b7tes\u00b7ab\u00b7we\u00b7send", "schien", "das", "Sa\u00b7al\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "An allen W\u00e4nden standen wei\u00dfe Schr\u00e4nke.", "tokens": ["An", "al\u00b7len", "W\u00e4n\u00b7den", "stan\u00b7den", "wei\u00b7\u00dfe", "Schr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich will die B\u00fccher sehen,", "tokens": ["Ich", "will", "die", "B\u00fc\u00b7cher", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich \u00f6ffne von den stillen Schr\u00e4nken einen,", "tokens": ["Ich", "\u00f6ff\u00b7ne", "von", "den", "stil\u00b7len", "Schr\u00e4n\u00b7ken", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es stehen gro\u00dfe dunkle Herzen in Regalen,", "tokens": ["Es", "ste\u00b7hen", "gro\u00b7\u00dfe", "dunk\u00b7le", "Her\u00b7zen", "in", "Re\u00b7ga\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Herzen wie Menschen gro\u00df und mumienhaft gedorrt.", "tokens": ["Her\u00b7zen", "wie", "Men\u00b7schen", "gro\u00df", "und", "mu\u00b7mi\u00b7en\u00b7haft", "ge\u00b7dorrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Ich wu\u00dfte nur noch, da\u00df ich lesen wollte,", "tokens": ["Ich", "wu\u00df\u00b7te", "nur", "noch", ",", "da\u00df", "ich", "le\u00b7sen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich lege mir ein Herz auf einen Tisch, und es bricht auf.", "tokens": ["Ich", "le\u00b7ge", "mir", "ein", "Herz", "auf", "ei\u00b7nen", "Tisch", ",", "und", "es", "bricht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Es war verstaubtes, altes Blut darin.", "tokens": ["Es", "war", "ver\u00b7staub\u00b7tes", ",", "al\u00b7tes", "Blut", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "$,", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u00c4lter und stiller wurde es im Saal.", "tokens": ["\u00c4l\u00b7ter", "und", "stil\u00b7ler", "wur\u00b7de", "es", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Es ist aus jenem Herzen jemand eingetreten.", "tokens": ["Es", "ist", "aus", "je\u00b7nem", "Her\u00b7zen", "je\u00b7mand", "ein\u00b7ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Schr\u00e4nke an den W\u00e4nden stehen alle offen,", "tokens": ["Die", "Schr\u00e4n\u00b7ke", "an", "den", "W\u00e4n\u00b7den", "ste\u00b7hen", "al\u00b7le", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und vor mir dichte Reihen dunkler Herzen.", "tokens": ["Und", "vor", "mir", "dich\u00b7te", "Rei\u00b7hen", "dunk\u00b7ler", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die Luft wuchs eng, unsichtbar f\u00fcllen Menschen dicht den Saal.", "tokens": ["Die", "Luft", "wuchs", "eng", ",", "un\u00b7sicht\u00b7bar", "f\u00fcl\u00b7len", "Men\u00b7schen", "dicht", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADJD", "ADJA", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Ich sehne mich hinaus, dort an der T\u00fcre sitzt ein Mensch, gelb und verdorrt,", "tokens": ["Ich", "seh\u00b7ne", "mich", "hin\u00b7aus", ",", "dort", "an", "der", "T\u00fc\u00b7re", "sitzt", "ein", "Mensch", ",", "gelb", "und", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.21": {"text": "Ohne Iris und Pupillen sieht er mich wartend an.", "tokens": ["Oh\u00b7ne", "I\u00b7ris", "und", "Pu\u00b7pil\u00b7len", "sieht", "er", "mich", "war\u00b7tend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Vergr\u00e4mt und einsam sieht er aus", "tokens": ["Ver\u00b7gr\u00e4mt", "und", "ein\u00b7sam", "sieht", "er", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und war Jahrhunderte allein.", "tokens": ["Und", "war", "Jahr\u00b7hun\u00b7der\u00b7te", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Er sieht mich wartend an mit leeren Augen.", "tokens": ["Er", "sieht", "mich", "war\u00b7tend", "an", "mit", "lee\u00b7ren", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ich komme fast erw\u00fcrgt an ihm vorbei.", "tokens": ["Ich", "kom\u00b7me", "fast", "er\u00b7w\u00fcrgt", "an", "ihm", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.26": {"text": "Dann, als ich Haus und Stra\u00dfe l\u00e4ngst verlor,", "tokens": ["Dann", ",", "als", "ich", "Haus", "und", "Stra\u00b7\u00dfe", "l\u00e4ngst", "ver\u00b7lor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Erst weit fort, wu\u00dfte ich, das war der Mensch,", "tokens": ["Erst", "weit", "fort", ",", "wu\u00df\u00b7te", "ich", ",", "das", "war", "der", "Mensch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Des Herz ich brach.", "tokens": ["Des", "Herz", "ich", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Er wollte einzig eine Tr\u00e4ne nur, und alle Herzen wollten eine Tr\u00e4ne,", "tokens": ["Er", "woll\u00b7te", "ein\u00b7zig", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "nur", ",", "und", "al\u00b7le", "Her\u00b7zen", "woll\u00b7ten", "ei\u00b7ne", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "ADV", "$,", "KON", "PIAT", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.30": {"text": "Sie alle warten seit Jahrhunderten.", "tokens": ["Sie", "al\u00b7le", "war\u00b7ten", "seit", "Jahr\u00b7hun\u00b7der\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--++--", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich gehe durch verwirrte, l\u00e4rmgef\u00fcllte Gassen", "tokens": ["Ich", "ge\u00b7he", "durch", "ver\u00b7wirr\u00b7te", ",", "l\u00e4rm\u00b7ge\u00b7f\u00fcll\u00b7te", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ratlos hin, zur\u00fcck, und trete in ein unbekanntes Haus.", "tokens": ["Rat\u00b7los", "hin", ",", "zu\u00b7r\u00fcck", ",", "und", "tre\u00b7te", "in", "ein", "un\u00b7be\u00b7kann\u00b7tes", "Haus", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Durch Korridore, T\u00fcren, Zimmer finde \u00f6den Weg", "tokens": ["Durch", "Kor\u00b7ri\u00b7do\u00b7re", ",", "T\u00fc\u00b7ren", ",", "Zim\u00b7mer", "fin\u00b7de", "\u00f6\u00b7den", "Weg"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und komme in den alten, hohen B\u00fcchersaal,", "tokens": ["Und", "kom\u00b7me", "in", "den", "al\u00b7ten", ",", "ho\u00b7