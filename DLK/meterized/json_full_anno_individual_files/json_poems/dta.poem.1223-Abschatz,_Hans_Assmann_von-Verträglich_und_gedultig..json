{"dta.poem.1223": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vertr\u00e4glich und gedultig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ardenia/ mein Licht/ was wilt du weiter sagen?", "tokens": ["Ar\u00b7de\u00b7nia", "/", "mein", "Licht", "/", "was", "wilt", "du", "wei\u00b7ter", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ich k\u00fcsse mit Gedult die Rutte/ die mich schl\u00e4gt/", "tokens": ["Ich", "k\u00fcs\u00b7se", "mit", "Ge\u00b7dult", "die", "Rut\u00b7te", "/", "die", "mich", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bet in Demutt an den Feind/ der mich erlegt/", "tokens": ["Und", "bet", "in", "De\u00b7mutt", "an", "den", "Feind", "/", "der", "mich", "er\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$(", "PRELS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verzehre mich in mir mit Leiden und nicht klagen.", "tokens": ["Ver\u00b7zeh\u00b7re", "mich", "in", "mir", "mit", "Lei\u00b7den", "und", "nicht", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "APPR", "NN", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ein Hylas will alsbald das volle Jawort wissen/", "tokens": ["Ein", "Hy\u00b7las", "will", "als\u00b7bald", "das", "vol\u00b7le", "Ja\u00b7wort", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich warte bi\u00df dein Mund es von sich selber spricht.", "tokens": ["Ich", "war\u00b7te", "bi\u00df", "dein", "Mund", "es", "von", "sich", "sel\u00b7ber", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Filadon vertr\u00e4gt sich mit Gesellschafft nicht/", "tokens": ["Ein", "Fi\u00b7la\u00b7don", "ver\u00b7tr\u00e4gt", "sich", "mit", "Ge\u00b7sell\u00b7schafft", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will/ was er noch nicht hat/ bereits allein gen\u00fcssen.", "tokens": ["Will", "/", "was", "er", "noch", "nicht", "hat", "/", "be\u00b7reits", "al\u00b7lein", "ge\u00b7n\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "PWS", "PPER", "ADV", "PTKNEG", "VAFIN", "$(", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Viel andre lieben dich; ich la\u00df es frey geschehen:", "tokens": ["Viel", "and\u00b7re", "lie\u00b7ben", "dich", ";", "ich", "la\u00df", "es", "frey", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ieder sucht sein Gl\u00fcck/ und liebt was Liebens werth.", "tokens": ["Ein", "ie\u00b7der", "sucht", "sein", "Gl\u00fcck", "/", "und", "liebt", "was", "Lie\u00b7bens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PIS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist doch einem nur zu seiner Zeit beschert:", "tokens": ["Du", "bist", "doch", "ei\u00b7nem", "nur", "zu", "sei\u00b7ner", "Zeit", "be\u00b7schert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man wird mich nie inde\u00df zu dienen m\u00fcde sehen.", "tokens": ["Man", "wird", "mich", "nie", "in\u00b7de\u00df", "zu", "die\u00b7nen", "m\u00fc\u00b7de", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein kluges Urtheil mag ohn allen Zwang erkennen/", "tokens": ["Dein", "klu\u00b7ges", "Ur\u00b7theil", "mag", "ohn", "al\u00b7len", "Zwang", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer deiner Gegen-Gunst am besten w\u00fcrdig sey.", "tokens": ["Wer", "dei\u00b7ner", "Ge\u00b7gen\u00b7Gunst", "am", "bes\u00b7ten", "w\u00fcr\u00b7dig", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKA", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch suchet deine Wahl ein Hertze voller Treu/", "tokens": ["Doch", "su\u00b7chet", "dei\u00b7ne", "Wahl", "ein", "Hert\u00b7ze", "vol\u00b7ler", "Treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bin ich schon gewi\u00df/ du wirst Silvandern nennen.", "tokens": ["So", "bin", "ich", "schon", "ge\u00b7wi\u00df", "/", "du", "wirst", "Sil\u00b7van\u00b7dern", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$(", "PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Soll gleich der Ausspruch nicht auff meine Seite fallen/", "tokens": ["Soll", "gleich", "der", "Aus\u00b7spruch", "nicht", "auff", "mei\u00b7ne", "Sei\u00b7te", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich werde dir darob nicht abhold k\u00f6nnen seyn.", "tokens": ["Ich", "wer\u00b7de", "dir", "da\u00b7rob", "nicht", "ab\u00b7hold", "k\u00f6n\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PAV", "PTKNEG", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will die keusche Brunst ins Hertze schl\u00fcssen ein/", "tokens": ["Ich", "will", "die", "keu\u00b7sche", "Brunst", "ins", "Hert\u00b7ze", "schl\u00fcs\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPRART", "VVFIN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bleibe bi\u00df ins Grab dein Treuster unter allen.", "tokens": ["Und", "blei\u00b7be", "bi\u00df", "ins", "Grab", "dein", "Treus\u00b7ter", "un\u00b7ter", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}