{"textgrid.poem.25149": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[die Glocken l\u00e4uten Tr\u00fcbsinn in die Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Glocken l\u00e4uten Tr\u00fcbsinn in die Welt.", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "Tr\u00fcb\u00b7sinn", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Laut mahnt ihr Ruf zu anderen Gebeten;", "tokens": ["Laut", "mahnt", "ihr", "Ruf", "zu", "an\u00b7de\u00b7ren", "Ge\u00b7be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit wilden Zungen, f\u00fcrchterlich beredten,", "tokens": ["Mit", "wil\u00b7den", "Zun\u00b7gen", ",", "f\u00fcrch\u00b7ter\u00b7lich", "be\u00b7red\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Grauen, Schmerz und Schreck ihr Toben gellt.", "tokens": ["Von", "Grau\u00b7en", ",", "Schmerz", "und", "Schreck", "ihr", "To\u00b7ben", "gellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und machtvoll ist ihr Ruf, der z\u00fcrnend bellt,", "tokens": ["Und", "macht\u00b7voll", "ist", "ihr", "Ruf", ",", "der", "z\u00fcr\u00b7nend", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn Menschen folgen ihm, fliehn angstbetreten", "tokens": ["Denn", "Men\u00b7schen", "fol\u00b7gen", "ihm", ",", "fliehn", "angst\u00b7be\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom stillen Herd, wo edelste Poeten", "tokens": ["Vom", "stil\u00b7len", "Herd", ",", "wo", "e\u00b7dels\u00b7te", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Wort und Werken ihren Geist erhellt.", "tokens": ["Mit", "Wort", "und", "Wer\u00b7ken", "ih\u00b7ren", "Geist", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch \u2013 noch ihr L\u00e4uten! Und wie Grabesschauer", "tokens": ["Noch", "\u2013", "noch", "ihr", "L\u00e4u\u00b7ten", "!", "Und", "wie", "Gra\u00b7bes\u00b7schau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "ADV", "PPOSAT", "NN", "$.", "KON", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00fcrd' mich Verzweiflung fassen, w\u00fc\u00dft' ich nicht,", "tokens": ["W\u00fcrd'", "mich", "Ver\u00b7zwei\u00b7flung", "fas\u00b7sen", ",", "w\u00fc\u00dft'", "ich", "nicht", ","], "token_info": ["word", "word"