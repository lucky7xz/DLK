{"dta.poem.9120": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Stern mit silbernen Zacken", "tokens": ["Auf", "ei\u00b7nem", "Stern", "mit", "sil\u00b7ber\u00b7nen", "Za\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sitz ich und lach ich \u2014 ein kleines Kind.", "tokens": ["sitz", "ich", "und", "lach", "ich", "ein", "klei\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "PPER", "$(", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "V\u00f6gel und Blumen haben mich lieb,", "tokens": ["V\u00f6\u00b7gel", "und", "Blu\u00b7men", "ha\u00b7ben", "mich", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "blonde Engel spielen mit mir.", "tokens": ["blon\u00b7de", "En\u00b7gel", "spie\u00b7len", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Unten gr\u00e4mt sich der Vater,", "tokens": ["Un\u00b7ten", "gr\u00e4mt", "sich", "der", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "unten schluchzt die Mutter,", "tokens": ["un\u00b7ten", "schluchzt", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "ich sitze und flechte mir einen Kranz aus Himmelsschl\u00fcsselchen.", "tokens": ["ich", "sit\u00b7ze", "und", "flech\u00b7te", "mir", "ei\u00b7nen", "Kranz", "aus", "Him\u00b7mels\u00b7schl\u00fcs\u00b7sel\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Lieber Vater! Liebe Mutter!", "tokens": ["Lie\u00b7ber", "Va\u00b7ter", "!", "Lie\u00b7be", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weint nicht!", "tokens": ["Weint", "nicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Seht:", "tokens": ["Seht", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "hier wachsen Blumen,", "tokens": ["hier", "wach\u00b7sen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "L\u00e4mmer springen,", "tokens": ["L\u00e4m\u00b7mer", "sprin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "und an jedem blanken Zacken", "tokens": ["und", "an", "je\u00b7dem", "blan\u00b7ken", "Za\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "h\u00e4ngt ein Zuckerherz!", "tokens": ["h\u00e4ngt", "ein", "Zu\u00b7cker\u00b7herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}