{"textgrid.poem.39972": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Einladung", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Folget meines Liedes Stimme", "tokens": ["Fol\u00b7get", "mei\u00b7nes", "Lie\u00b7des", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem allerstillsten Thal,", "tokens": ["Nach", "dem", "al\u00b7ler\u00b7stills\u00b7ten", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sicher vor des Sturmes Grimme,", "tokens": ["Si\u00b7cher", "vor", "des", "Stur\u00b7mes", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht verbrannt vom Sonnenstral,", "tokens": ["Nicht", "ver\u00b7brannt", "vom", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruh' und K\u00fchlung zwischen H\u00fcgeln,", "tokens": ["Ruh'", "und", "K\u00fch\u00b7lung", "zwi\u00b7schen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Matten gr\u00fcn und Himmel hell;", "tokens": ["Mat\u00b7ten", "gr\u00fcn", "und", "Him\u00b7mel", "hell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kommt, la\u00dft uns den Schritt befl\u00fcgeln,", "tokens": ["Kommt", ",", "la\u00dft", "uns", "den", "Schritt", "be\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bis wir sind im Appenzell.", "tokens": ["Bis", "wir", "sind", "im", "Ap\u00b7pen\u00b7zell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00fche weiden, Bienen saugen,", "tokens": ["K\u00fc\u00b7he", "wei\u00b7den", ",", "Bie\u00b7nen", "sau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gras und Blume stehn so dicht,", "tokens": ["Gras", "und", "Blu\u00b7me", "stehn", "so", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4ttigt die vergn\u00fcgten Augen,", "tokens": ["S\u00e4t\u00b7tigt", "die", "ver\u00b7gn\u00fcg\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Suchet Baum und Rebe nicht!", "tokens": ["Su\u00b7chet", "Baum", "und", "Re\u00b7be", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ihr von den Bergen kommet", "tokens": ["Wenn", "ihr", "von", "den", "Ber\u00b7gen", "kom\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Fehlt euch Speise nicht und Trank,", "tokens": ["Fehlt", "euch", "Spei\u00b7se", "nicht", "und", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKNEG", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Milch und Honig \u2013 was euch frommet,", "tokens": ["Milch", "und", "Ho\u00b7nig", "\u2013", "was", "euch", "from\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Harret auf der Ruhebank.", "tokens": ["Har\u00b7ret", "auf", "der", "Ru\u00b7he\u00b7bank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Satt und fr\u00f6hlich sollt ihr werden,", "tokens": ["Satt", "und", "fr\u00f6h\u00b7lich", "sollt", "ihr", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Setzt euch vor das kleine Haus;", "tokens": ["Setzt", "euch", "vor", "das", "klei\u00b7ne", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fctten breiten sich, wie Heerden", "tokens": ["H\u00fct\u00b7ten", "brei\u00b7ten", "sich", ",", "wie", "Heer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem gr\u00fcnen Anger aus.", "tokens": ["Auf", "dem", "gr\u00fc\u00b7nen", "An\u00b7ger", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Niedrig und geborgen stehen", "tokens": ["Nied\u00b7rig", "und", "ge\u00b7bor\u00b7gen", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie auf friedevollen Au'n,", "tokens": ["Sie", "auf", "frie\u00b7de\u00b7vol\u00b7len", "Au'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer es siehet, mu\u00df gestehen:", "tokens": ["Wer", "es", "sie\u00b7het", ",", "mu\u00df", "ge\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hier ist lieblich H\u00fctten bau'n.", "tokens": ["Hier", "ist", "lieb\u00b7lich", "H\u00fct\u00b7ten", "bau'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VAFIN", "ADJD", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier wohnt Hochmut nicht, noch Schande,", "tokens": ["Hier", "wohnt", "Hoch\u00b7mut", "nicht", ",", "noch", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKNEG", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh ist Alles, Alles gleich;", "tokens": ["Froh", "ist", "Al\u00b7les", ",", "Al\u00b7les", "gleich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "$,", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist K\u00f6nig hier im Lande,", "tokens": ["Wer", "ist", "K\u00f6\u00b7nig", "hier", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht es in der Armut reich?", "tokens": ["Macht", "es", "in", "der", "Ar\u00b7mut", "reich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ihr nach dem K\u00f6nig fraget,", "tokens": ["Wenn", "ihr", "nach", "dem", "K\u00f6\u00b7nig", "fra\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruft das Volk euch lachend zu:", "tokens": ["Ruft", "das", "Volk", "euch", "la\u00b7chend", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbhinten sitzt er, wo's mittaget,", "tokens": ["\u00bb", "hin\u00b7ten", "sitzt", "er", ",", "wo's", "mit\u00b7ta\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PWAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Herrscht schon lang in guter Ruh'!", "tokens": ["Herrscht", "schon", "lang", "in", "gu\u00b7ter", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort auf dem granitnen Throne", "tokens": ["Dort", "auf", "dem", "gra\u00b7nit\u00b7nen", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tausendj\u00e4hrig sitzt der Greis,", "tokens": ["Tau\u00b7send\u00b7j\u00e4h\u00b7rig", "sitzt", "der", "Greis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4gt von Felsen eine Krone,", "tokens": ["Tr\u00e4gt", "von", "Fel\u00b7sen", "ei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnee f\u00e4rbt seine Scheitel wei\u00df.", "tokens": ["Schnee", "f\u00e4rbt", "sei\u00b7ne", "Schei\u00b7tel", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der beschirmet unsre Samen,", "tokens": ["Der", "be\u00b7schir\u00b7met", "uns\u00b7re", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deckt mit seinem Leib das Land,", "tokens": ["Deckt", "mit", "sei\u00b7nem", "Leib", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist mit edlem F\u00fcrstennamen:", "tokens": ["Ist", "mit", "ed\u00b7lem", "F\u00fcrs\u00b7ten\u00b7na\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seltsam Volk, de\u00df H\u00fctten W\u00e4lle,", "tokens": ["Selt\u00b7sam", "Volk", ",", "de\u00df", "H\u00fct\u00b7ten", "W\u00e4l\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen Reichtum Schaf und Rind,", "tokens": ["Des\u00b7sen", "Reich\u00b7tum", "Schaf", "und", "Rind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schatz und Vorratskammer St\u00e4lle,", "tokens": ["Schatz", "und", "Vor\u00b7rats\u00b7kam\u00b7mer", "St\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen F\u00fcrsten Berge sind!", "tokens": ["Des\u00b7sen", "F\u00fcrs\u00b7ten", "Ber\u00b7ge", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer hat dir dein Loos geschaffen,", "tokens": ["Wer", "hat", "dir", "dein", "Loos", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ohne Wunsch und ohne Harm?", "tokens": ["Oh\u00b7ne", "Wunsch", "und", "oh\u00b7ne", "Harm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh, da hei\u00dft es: unsre Waffen!", "tokens": ["Sieh", ",", "da", "hei\u00dft", "es", ":", "uns\u00b7re", "Waf\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh, da ruft es: unser Arm!", "tokens": ["Sieh", ",", "da", "ruft", "es", ":", "un\u00b7ser", "Arm", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und in's Wort der braunen Hirten", "tokens": ["Und", "in's", "Wort", "der", "brau\u00b7nen", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stimmt der Mund der Weiber ein;", "tokens": ["Stimmt", "der", "Mund", "der", "Wei\u00b7ber", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die den Wandrer mild bewirten,", "tokens": ["Die", "den", "Wand\u00b7rer", "mild", "be\u00b7wir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen nicht vergessen sein:", "tokens": ["Wol\u00b7len", "nicht", "ver\u00b7ges\u00b7sen", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es siegten mit die Frauen,", "tokens": ["Denn", "es", "sieg\u00b7ten", "mit", "die", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wenn's auch ihr Arm nicht that,", "tokens": ["Und", "wenn's", "auch", "ihr", "Arm", "nicht", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "That's ihr Antlitz, streute Grauen", "tokens": ["That's", "ihr", "Ant\u00b7litz", ",", "streu\u00b7te", "Grau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf des Feindes fl\u00fccht'gen Pfad.", "tokens": ["Auf", "des", "Fein\u00b7des", "fl\u00fccht'\u00b7gen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun, bereitet ist die Kunde:", "tokens": ["Nun", ",", "be\u00b7rei\u00b7tet", "ist", "die", "Kun\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcnes Thal, so sei uns hold!", "tokens": ["Gr\u00fc\u00b7nes", "Thal", ",", "so", "sei", "uns", "hold", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", 