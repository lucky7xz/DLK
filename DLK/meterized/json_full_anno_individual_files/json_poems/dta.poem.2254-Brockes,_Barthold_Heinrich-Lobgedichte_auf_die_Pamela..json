{"dta.poem.2254": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lobgedichte  \n auf die Pamela.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das, was man, von der wahren Tugend, in hun-", "tokens": ["Das", ",", "was", "man", ",", "von", "der", "wah\u00b7ren", "Tu\u00b7gend", ",", "in", "hun"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PIS", "$,", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "dert tausend B\u00fcchern lehret,", "tokens": ["dert", "tau\u00b7send", "B\u00fc\u00b7chern", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird, durch der Pamela Betragen, auf eine solche", "tokens": ["Wird", ",", "durch", "der", "Pa\u00b7me\u00b7la", "Be\u00b7tra\u00b7gen", ",", "auf", "ei\u00b7ne", "sol\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "APPR", "ART", "NE", "NN", "$,", "APPR", "ART", "PIAT"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Weis' erkl\u00e4ret,", "tokens": ["Weis'", "er\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df der nicht nur kein tugendhaftes, kein menschlichs", "tokens": ["Da\u00df", "der", "nicht", "nur", "kein", "tu\u00b7gend\u00b7haf\u00b7tes", ",", "kein", "menschlichs"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "PTKNEG", "ADV", "PIAT", "ADJA", "$,", "PIAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Herz im Busen hegt,", "tokens": ["Herz", "im", "Bu\u00b7sen", "hegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Den diese tugendhafte Sch\u00f6ne zur Tugendliebe nicht be-", "tokens": ["Den", "die\u00b7se", "tu\u00b7gend\u00b7haf\u00b7te", "Sch\u00f6\u00b7ne", "zur", "Tu\u00b7gend\u00b7lie\u00b7be", "nicht", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "ADJA", "NN", "APPRART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "wegt.", "tokens": ["wegt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+", "measure": "single.up"}}}}}