{"textgrid.poem.42191": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Das Opfer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich bei Tag mein M\u00e4del mir beseh,", "tokens": ["Wenn", "ich", "bei", "Tag", "mein", "M\u00e4\u00b7del", "mir", "be\u00b7seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann seh ich einen kahlen Totensch\u00e4del,", "tokens": ["Dann", "seh", "ich", "ei\u00b7nen", "kah\u00b7len", "To\u00b7ten\u00b7sch\u00e4\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Darunter ein Skelett, und seh mein M\u00e4del", "tokens": ["Da\u00b7run\u00b7ter", "ein", "Ske\u00b7lett", ",", "und", "seh", "mein", "M\u00e4\u00b7del"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebrochen knien von schauerlichem Weh.", "tokens": ["Ge\u00b7bro\u00b7chen", "kni\u00b7en", "von", "schau\u00b7er\u00b7li\u00b7chem", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie schreit zum Sch\u00f6pfer: \u00bbLa\u00df mich Freudenquell", "tokens": ["Sie", "schreit", "zum", "Sch\u00f6p\u00b7fer", ":", "\u00bb", "La\u00df", "mich", "Freu\u00b7den\u00b7quell"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "VVIMP", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur schleunigst jetzt an ihm vor\u00fcbergehen!", "tokens": ["Nur", "schleu\u00b7nigst", "jetzt", "an", "ihm", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sechs Monde noch, dann w\u00e4r's um ihn geschehen.", "tokens": ["Sechs", "Mon\u00b7de", "noch", ",", "dann", "w\u00e4r's", "um", "ihn", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Mark wird m\u00fcrb, der Tod vergafft sich schnell.\u00ab", "tokens": ["Sein", "Mark", "wird", "m\u00fcrb", ",", "der", "Tod", "ver\u00b7gafft", "sich", "schnell", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PRF", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbmich wirft man auf den Mist, das ist normal;", "tokens": ["\u00bb", "mich", "wirft", "man", "auf", "den", "Mist", ",", "das", "ist", "nor\u00b7mal", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das Fleisch auf meinen Rippen ist Schim\u00e4re.", "tokens": ["Da