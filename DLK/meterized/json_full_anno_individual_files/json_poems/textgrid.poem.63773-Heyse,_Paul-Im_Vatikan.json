{"textgrid.poem.63773": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Im Vatikan", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00dft du, statt einsam durch dies Haus zu schweifen,", "tokens": ["Mu\u00dft", "du", ",", "statt", "ein\u00b7sam", "durch", "dies", "Haus", "zu", "schwei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUI", "ADJD", "APPR", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Deutschen wandern oder Britenscharen,", "tokens": ["Mit", "Deut\u00b7schen", "wan\u00b7dern", "o\u00b7der", "Bri\u00b7ten\u00b7scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirst du in gl\u00fch'ndem Unmut oft gewahren,", "tokens": ["Wirst", "du", "in", "gl\u00fch'n\u00b7dem", "Un\u00b7mut", "oft", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sie betasten, was sie nicht begreifen.", "tokens": ["Da\u00df", "sie", "be\u00b7tas\u00b7ten", ",", "was", "sie", "nicht", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mag auch der Strom der Zeit an ihnen schleifen,", "tokens": ["Mag", "auch", "der", "Strom", "der", "Zeit", "an", "ih\u00b7nen", "schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie bleiben doch im Herzensgrund Barbaren,", "tokens": ["Sie", "blei\u00b7ben", "doch", "im", "Her\u00b7zens\u00b7grund", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die frech dem Zeusbild in die Locken fahren", "tokens": ["Die", "frech", "dem", "Zeus\u00b7bild", "in", "die", "Lo\u00b7cken", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und vor dem Torso Gassenhauer pfeifen.", "tokens": ["Und", "vor", "dem", "Tor\u00b7so", "Gas\u00b7sen\u00b7hau\u00b7er", "pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch mitten im Gew\u00fchl der Stumpfgebornen", "tokens": ["Doch", "mit\u00b7ten", "im", "Ge\u00b7w\u00fchl", "der", "Stumpf\u00b7ge\u00b7bor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trifft dich ein Blitz aus nord'schem Augenlid", "tokens": ["Trifft", "dich", "ein", "Blitz", "aus", "nord'\u00b7schem", "Au\u00b7gen\u00b7lid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN"], "me