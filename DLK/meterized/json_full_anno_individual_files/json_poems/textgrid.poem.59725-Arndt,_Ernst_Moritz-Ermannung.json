{"textgrid.poem.59725": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Ermannung", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df du die Dinge nur rennen und rinnen,", "tokens": ["La\u00df", "du", "die", "Din\u00b7ge", "nur", "ren\u00b7nen", "und", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Blitzet es drau\u00dfen, so blitze du drinnen,", "tokens": ["Blit\u00b7zet", "es", "drau\u00b7\u00dfen", ",", "so", "blit\u00b7ze", "du", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Brauche den g\u00f6ttlichgeborenen Blitz!", "tokens": ["Brau\u00b7che", "den", "g\u00f6tt\u00b7lich\u00b7ge\u00b7bo\u00b7re\u00b7nen", "Blitz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Rasen die St\u00fcrme und brausen die Fluten,", "tokens": ["Ra\u00b7sen", "die", "St\u00fcr\u00b7me", "und", "brau\u00b7sen", "die", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Z\u00fcnden die Blitze mit fressenden Gluten,", "tokens": ["Z\u00fcn\u00b7den", "die", "Blit\u00b7ze", "mit", "fres\u00b7sen\u00b7den", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Halte, Prometheus, den Geist auf dem Sitz!", "tokens": ["Hal\u00b7te", ",", "Pro\u00b7me\u00b7theus", ",", "den", "Geist", "auf", "dem", "Sitz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Mutig gleich schlachtenbegeisterten Rossen,", "tokens": ["Mu\u00b7tig", "gleich", "schlach\u00b7ten\u00b7be\u00b7geis\u00b7ter\u00b7ten", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wiehernd entgegen den Donnergeschossen", "tokens": ["Wie\u00b7hernd", "ent\u00b7ge\u00b7gen", "den", "Don\u00b7ner\u00b7ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Streite und schreite entgegen dem Sturm!", "tokens": ["Strei\u00b7te", "und", "schrei\u00b7te", "ent\u00b7ge\u00b7gen", "dem", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Streite und schreite, und, gilt es zu stehen,", "tokens": ["Strei\u00b7te", "und", "schrei\u00b7te", ",", "und", ",", "gilt", "es", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KON", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text"