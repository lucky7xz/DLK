{"dta.poem.7103": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stell\u2019 ich den Menschen mir", "tokens": ["Stell'", "ich", "den", "Men\u00b7schen", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In dieser Welt, nach seinem Wesen, f\u00fcr;", "tokens": ["In", "die\u00b7ser", "Welt", ",", "nach", "sei\u00b7nem", "We\u00b7sen", ",", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So scheint die Meinung mir nicht fern vom Zweck zu", "tokens": ["So", "scheint", "die", "Mei\u00b7nung", "mir", "nicht", "fern", "vom", "Zweck", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADJD", "APPRART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zielen,", "tokens": ["zie\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wenn man vor dem den Stand der seel\u2019gen Welt", "tokens": ["Wenn", "man", "vor", "dem", "den", "Stand", "der", "seel'\u00b7gen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sich etwann folgends vorgestellt:", "tokens": ["Sich", "et\u00b7wann", "fol\u00b7gends", "vor\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "GoTT h\u00e4tt\u2019 uns solchen Trieb vergebens nicht gegeben,", "tokens": ["GoTT", "h\u00e4tt'", "uns", "sol\u00b7chen", "Trieb", "ver\u00b7ge\u00b7bens", "nicht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In stetem Wechsel Lust zu finden,", "tokens": ["In", "ste\u00b7tem", "Wech\u00b7sel", "Lust", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und in Ver\u00e4nderung Vergn\u00fcgen zu empfinden,", "tokens": ["Und", "in", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nein, eben dadurch nur noch mehr geschickt gemacht,", "tokens": ["Nein", ",", "e\u00b7ben", "da\u00b7durch", "nur", "noch", "mehr", "ge\u00b7schickt", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PAV", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von seiner Herrlichkeit, Macht, Majest\u00e4t und Pracht,", "tokens": ["Von", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", ",", "Macht", ",", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die unersch\u00f6pfflich sind, in jenem seel\u2019gen Leben,", "tokens": ["Die", "un\u00b7er\u00b7sch\u00f6pf\u00b7flich", "sind", ",", "in", "je\u00b7nem", "seel'\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Unendlichen Beweis auf nie ersch\u00f6pffte Art,", "tokens": ["Un\u00b7end\u00b7li\u00b7chen", "Be\u00b7weis", "auf", "nie", "er\u00b7sch\u00f6pff\u00b7te", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mehr, als wir ewig k\u00f6nnen dencken;", "tokens": ["Mehr", ",", "als", "wir", "e\u00b7wig", "k\u00f6n\u00b7nen", "den\u00b7cken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Uns sonder End und Ziel zu schencken.", "tokens": ["Uns", "son\u00b7der", "End", "und", "Ziel", "zu", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Vermuthlich w\u00fcrden wir so schnell nicht w\u00fcnschen k\u00f6nnen,", "tokens": ["Ver\u00b7muth\u00b7lich", "w\u00fcr\u00b7den", "wir", "so", "schnell", "nicht", "w\u00fcn\u00b7schen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "GoTT w\u00fcrde den Besitz uns augenblicklich g\u00f6nnen.", "tokens": ["GoTT", "w\u00fcr\u00b7de", "den", "Be\u00b7sitz", "uns", "au\u00b7gen\u00b7blick\u00b7lich", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Wollust einer gantzen Welt", "tokens": ["Die", "Wol\u00b7lust", "ei\u00b7ner", "gant\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die w\u00fcrde sie nach eigenen Verlangen,", "tokens": ["Die", "w\u00fcr\u00b7de", "sie", "nach", "ei\u00b7ge\u00b7nen", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Mit einer, die ihr mehr gef\u00e4llt,", "tokens": ["Mit", "ei\u00b7ner", ",", "die", "ihr", "mehr", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Vertauschen; und derselben Prangen", "tokens": ["Ver\u00b7tau\u00b7schen", ";", "und", "der\u00b7sel\u00b7ben", "Pran\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Kaum bis zur S\u00e4ttigung versp\u00fcren,", "tokens": ["Kaum", "bis", "zur", "S\u00e4t\u00b7ti\u00b7gung", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Da sie im Augenblick von neuen", "tokens": ["Da", "sie", "im", "Au\u00b7gen\u00b7blick", "von", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sich einer andern kann erfreuen.", "tokens": ["Sich", "ei\u00b7ner", "an\u00b7dern", "kann", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}