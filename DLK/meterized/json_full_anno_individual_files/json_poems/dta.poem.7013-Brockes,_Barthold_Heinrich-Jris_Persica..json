{"dta.poem.7013": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jris Persica.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat, gesch\u00e4tzter Schenck, dein liebliches Geschencke", "tokens": ["Es", "hat", ",", "ge\u00b7sch\u00e4tz\u00b7ter", "Schenck", ",", "dein", "lieb\u00b7li\u00b7ches", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zusammt der wohlgesetzten Schrifft,", "tokens": ["Zu\u00b7sammt", "der", "wohl\u00b7ge\u00b7setz\u00b7ten", "Schrifft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dasjenige, was du bey mir verlangt, gestifft:", "tokens": ["Das\u00b7je\u00b7ni\u00b7ge", ",", "was", "du", "bey", "mir", "ver\u00b7langt", ",", "ge\u00b7stifft", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es macht, da\u00df ich aufs neu an GOTTES Wunder dencke.", "tokens": ["Es", "macht", ",", "da\u00df", "ich", "aufs", "neu", "an", "GoT\u00b7TES", "Wun\u00b7der", "den\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "ADJD", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin mit neuer Lust erf\u00fcllt,", "tokens": ["Ich", "bin", "mit", "neu\u00b7er", "Lust", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ich der Wolcken Gnaden-Bild,", "tokens": ["Da", "ich", "der", "Wol\u00b7cken", "Gna\u00b7den\u00b7Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Nahmen und den Farben nach,", "tokens": ["Dem", "Nah\u00b7men", "und", "den", "Far\u00b7ben", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Zeit, als Lentz und Winter brach,", "tokens": ["Zur", "Zeit", ",", "als", "Lentz", "und", "Win\u00b7ter", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nach Regen, Sturm, nach Frost und Schnee,", "tokens": ["Nach", "Re\u00b7gen", ",", "Sturm", ",", "nach", "Frost", "und", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In deiner Wunder-Bluhme seh\u2019.", "tokens": ["In", "dei\u00b7ner", "Wun\u00b7der\u00b7Bluh\u00b7me", "seh'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das seltene Gew\u00e4chs die Jris Persiea,", "tokens": ["Das", "sel\u00b7te\u00b7ne", "Ge\u00b7w\u00e4chs", "die", "Jris", "Per\u00b7si\u00b7ea", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "NE", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Als ich sie mit Aufmercksamkeit besah,", "tokens": ["Als", "ich", "sie", "mit", "Auf\u00b7merck\u00b7sam\u00b7keit", "be\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Bestand, dem Ansehn nach, aus dreyen Bl\u00e4ttern nur,", "tokens": ["Be\u00b7stand", ",", "dem", "An\u00b7sehn", "nach", ",", "aus", "drey\u00b7en", "Bl\u00e4t\u00b7tern", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKVZ", "$,", "APPR", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Worunter drey, die sich auf wei\u00df, auf blau, auf gr\u00fcn,", "tokens": ["Wo\u00b7run\u00b7ter", "drey", ",", "die", "sich", "auf", "wei\u00df", ",", "auf", "blau", ",", "auf", "gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "CARD", "$,", "PRELS", "PRF", "APPR", "VVFIN", "$,", "APPR", "ADJD", "$,", "APPR", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "In sanfft- und lieblicher Vermischung ziehn:", "tokens": ["In", "sanf\u00b7ft", "und", "lieb\u00b7li\u00b7cher", "Ver\u00b7mi\u00b7schung", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Dieselben sind an bunt- und zartem Glantz,", "tokens": ["Die\u00b7sel\u00b7ben", "sind", "an", "bunt", "und", "zar\u00b7tem", "Glantz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Den klaren Perlen-Muscheln gantz,", "tokens": ["Den", "kla\u00b7ren", "Per\u00b7len\u00b7Mu\u00b7scheln", "gantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Was sag ich, selbst den Perlen gleich,", "tokens": ["Was", "sag", "ich", ",", "selbst", "den", "Per\u00b7len", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "An ihren Seiten sind dieselbigen durchschnitten,", "tokens": ["An", "ih\u00b7ren", "Sei\u00b7ten", "sind", "die\u00b7sel\u00b7bi\u00b7gen", "durch\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die andern drey sind Gold und Purpur reich,", "tokens": ["Die", "an\u00b7dern", "drey", "sind", "Gold", "und", "Pur\u00b7pur", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Es strahlt ein gelber Strich in ihrer Mitten,", "tokens": ["Es", "strahlt", "ein", "gel\u00b7ber", "Strich", "in", "ih\u00b7rer", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Der oben gantz erh\u00f6ht, und unten gantz besprengt", "tokens": ["Der", "o\u00b7ben", "gantz", "er\u00b7h\u00f6ht", ",", "und", "un\u00b7ten", "gantz", "be\u00b7sprengt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVPP", "$,", "KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mit P\u00fcnctgen, die mit Strichen untermengt,", "tokens": ["Mit", "P\u00fcnct\u00b7gen", ",", "die", "mit", "Stri\u00b7chen", "un\u00b7ter\u00b7mengt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "In einem Purpurfarbnen-Schein.", "tokens": ["In", "ei\u00b7nem", "Pur\u00b7pur\u00b7fa\u00b7rb\u00b7nen\u00b7Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Bey dieser Sch\u00f6nheit fiel mir ein:", "tokens": ["Bey", "die\u00b7ser", "Sch\u00f6n\u00b7heit", "fiel", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Was nimmst du eitler Mensch dir nicht f\u00fcr Sorg und\nM\u00fch,", "tokens": ["Was", "nimmst", "du", "eit\u00b7ler", "Mensch", "dir", "nicht", "f\u00fcr", "Sorg", "und", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "In Purpur, Gold und Silber dich zu zeigen,", "tokens": ["In", "Pur\u00b7pur", ",", "Gold", "und", "Sil\u00b7ber", "dich", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Dein Schmuck ist nur geborgt. Hingegen hie", "tokens": ["Dein", "Schmuck", "ist", "nur", "ge\u00b7borgt", ".", "Hin\u00b7ge\u00b7gen", "hie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$.", "ADV", "ADV"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Ist alle Pracht und Zier der Bluhmen eigen,", "tokens": ["Ist", "al\u00b7le", "Pracht", "und", "Zier", "der", "Bluh\u00b7men", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Selbst die Natur hat sie so sch\u00f6n geschm\u00fcckt,", "tokens": ["Selbst", "die", "Na\u00b7tur", "hat", "sie", "so", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Der allerd\u00fcnnste Tafft ist dick und grob bey ihnen;", "tokens": ["Der", "al\u00b7ler\u00b7d\u00fcnns\u00b7te", "Tafft", "ist", "dick", "und", "grob", "bey", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Durchsichtig wie ein Glas. Es spielt im holden Gr\u00fcnen", "tokens": ["Durch\u00b7sich\u00b7tig", "wie", "ein", "Glas", ".", "Es", "spielt", "im", "hol\u00b7den", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein angenehmes weislich Blau,", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mes", "weis\u00b7lich", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Das ich Verwundrungs-voll beschau.", "tokens": ["Das", "ich", "Ver\u00b7wun\u00b7drungs\u00b7voll", "be\u00b7schau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ein wiederscheinender ver\u00e4nderlicher Glantz,", "tokens": ["Ein", "wie\u00b7der\u00b7schei\u00b7nen\u00b7der", "ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7cher", "Glantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Gleicht fast dem Tauben-Hals, gleicht einem Pfauen-\nSchwantz.", "tokens": ["Gleicht", "fast", "dem", "Tau\u00b7ben\u00b7Hals", ",", "gleicht", "ei\u00b7nem", "Pfau\u00b7en", "Schwantz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "TRUNC", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So sanfft als deiner Farben Schein;", "tokens": ["So", "sanfft", "als", "dei\u00b7ner", "Far\u00b7ben", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "So sanfft pflegt dein Geruch zu seyn.", "tokens": ["So", "sanfft", "pflegt", "dein", "Ge\u00b7ruch", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Wie zart nun dein Geruch wie zart auch deine Bl\u00e4tter,", "tokens": ["Wie", "zart", "nun", "dein", "Ge\u00b7ruch", "wie", "zart", "auch", "dei\u00b7ne", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPOSAT", "NN", "KOKOM", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie zart die Farben seyn, so tritt dein sanffter Flor,", "tokens": ["Wie", "zart", "die", "Far\u00b7ben", "seyn", ",", "so", "tritt", "dein", "sanff\u00b7ter", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAINF", "$,", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Doch mitten in dem Schnee hervor,", "tokens": ["Doch", "mit\u00b7ten", "in", "dem", "Schnee", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und scheuet weder Sturm noch sonst ein rauhes Wetter.", "tokens": ["Und", "scheu\u00b7et", "we\u00b7der", "Sturm", "noch", "sonst", "ein", "rau\u00b7hes", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ach m\u00f6gt' auch ich ohn' eckle Z\u00e4rtlichkeit,", "tokens": ["Ach", "m\u00f6gt'", "auch", "ich", "ohn'", "ec\u00b7kle", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Zu deinem Ruhm, mein Sch\u00f6pfer, bl\u00fchen!", "tokens": ["Zu", "dei\u00b7nem", "Ruhm", ",", "mein", "Sch\u00f6p\u00b7fer", ",", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Ach m\u00f6cht auch ich der Erden mich entziehen,", "tokens": ["Ach", "m\u00f6cht", "auch", "ich", "der", "Er\u00b7den", "mich", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Und stets in fr\u00f6licher Gelassenheit,", "tokens": ["Und", "stets", "in", "fr\u00f6\u00b7li\u00b7cher", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Dich zu verehren mich bem\u00fchen!", "tokens": ["Dich", "zu", "ver\u00b7eh\u00b7ren", "mich", "be\u00b7m\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}