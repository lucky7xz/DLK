{"dta.poem.17916": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "159.  Das Hertze mu\u00df scharff geladen seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "cy:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ey lad doch recht und scharff/ was paffstu in die Lufft?", "tokens": ["Ey", "lad", "doch", "recht", "und", "scharff", "/", "was", "paff\u00b7stu", "in", "die", "Lufft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "ADJD", "KON", "VVFIN", "$(", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was blind geladen ist das heisset nur gepufft.", "tokens": ["Was", "blind", "ge\u00b7la\u00b7den", "ist", "das", "heis\u00b7set", "nur", "ge\u00b7pufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "VAFIN", "PDS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}