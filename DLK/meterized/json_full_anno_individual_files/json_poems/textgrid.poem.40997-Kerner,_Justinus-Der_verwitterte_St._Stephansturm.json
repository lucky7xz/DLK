info": ["word", "word", "word"], "pos": ["VVPP", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schwanenlied den Sternen singe!", "tokens": ["Ein", "Schwa\u00b7nen\u00b7lied", "den", "Ster\u00b7nen", "sin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Dann aus dem Schutte, Turmes Geist!", "tokens": ["Dann", "aus", "dem", "Schut\u00b7te", ",", "Tur\u00b7mes", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flieg' eine Wolk' in Himmels Fernen,", "tokens": ["Flieg'", "ei\u00b7ne", "Wol\u00b7k'", "in", "Him\u00b7mels", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "ADJA", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Vom Felsenaare noch umkreist,", "tokens": ["Vom", "Fel\u00b7se\u00b7naa\u00b7re", "noch", "um\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihm verschwindend unter Sternen!", "tokens": ["Mit", "ihm", "ver\u00b7schwin\u00b7dend", "un\u00b7ter", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}