{"textgrid.poem.40997": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der verwitterte St. Stephansturm", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Turm, der du viele hundert Jahr'", "tokens": ["Turm", ",", "der", "du", "vie\u00b7le", "hun\u00b7dert", "Jahr'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "PIAT", "CARD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aufstrebtest stolz ins Reich der L\u00fcfte,", "tokens": ["Auf\u00b7streb\u00b7test", "stolz", "ins", "Reich", "der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um dessen Haupt der Felsenaar", "tokens": ["Um", "des\u00b7sen", "Haupt", "der", "Fel\u00b7se\u00b7naar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie um den Mast die M\u00f6we schiffte,", "tokens": ["Wie", "um", "den", "Mast", "die", "M\u00f6\u00b7we", "schiff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch dich zerfra\u00df der Zeiten Zahn.", "tokens": ["Auch", "dich", "zer\u00b7fra\u00df", "der", "Zei\u00b7ten", "Zahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um dir noch Leben zu erzwingen,", "tokens": ["Um", "dir", "noch", "Le\u00b7ben", "zu", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Strich man dich Sterbenden noch an", "tokens": ["Strich", "man", "dich", "Ster\u00b7ben\u00b7den", "noch", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIS", "PRF", "NN", "ADV", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und band den Leib mit Eisenringen.", "tokens": ["Und", "band", "den", "Leib", "mit", "Ei\u00b7sen\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So hatte man Eid von Vivar,", "tokens": ["So", "hat\u00b7te", "man", "Eid", "von", "Vi\u00b7var", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Den greisen, noch aufs Ro\u00df gebunden,", "tokens": ["Den", "grei\u00b7sen", ",", "noch", "aufs", "Ro\u00df", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als er schon eine Leiche war,", "tokens": ["Als", "er", "schon", "ei\u00b7ne", "Lei\u00b7che", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Stahlkleid \u00fcber alte Wunden.", "tokens": ["Das", "Stahl\u00b7kleid", "\u00fc\u00b7ber", "al\u00b7te", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So lagen Ringe schwer von Erz", "tokens": ["So", "la\u00b7gen", "Rin\u00b7ge", "schwer", "von", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem treuen Heinrich in der Sage", "tokens": ["Dem", "treu\u00b7en", "Hein\u00b7rich", "in", "der", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ums alte, gramerf\u00fcllte Herz,", "tokens": ["Ums", "al\u00b7te", ",", "gra\u00b7mer\u00b7f\u00fcll\u00b7te", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf da\u00df aus ihm nicht brach die Klage.", "tokens": ["Auf", "da\u00df", "aus", "ihm", "nicht", "brach", "die", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wo ist die Glocke, riesiggro\u00df,", "tokens": ["Wo", "ist", "die", "Glo\u00b7cke", ",", "rie\u00b7sig\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die oft die Luft gesetzt in Wogen,", "tokens": ["Die", "oft", "die", "Luft", "ge\u00b7setzt", "in", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gu\u00df aus des Christenfeinds Gescho\u00df,", "tokens": ["Gu\u00df", "aus", "des", "Chris\u00b7ten\u00b7feinds", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das deinen Nacken nicht gebogen?", "tokens": ["Das", "dei\u00b7nen", "Na\u00b7cken", "nicht", "ge\u00b7bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie ruhet hinter morschen Gittern;", "tokens": ["Sie", "ru\u00b7het", "hin\u00b7ter", "mor\u00b7schen", "Git\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es m\u00f6cht' die Donnermelodie", "tokens": ["Es", "m\u00f6cht'", "die", "Don\u00b7ner\u00b7me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sehr den alten Leib ersch\u00fcttern.", "tokens": ["Zu", "sehr", "den", "al\u00b7ten", "Leib", "er\u00b7sch\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df deinem Haupt ein Kreuz man bot,", "tokens": ["Da\u00df", "dei\u00b7nem", "Haupt", "ein", "Kreuz", "man", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf da\u00df dein Wuchs noch h\u00f6her reiche,", "tokens": ["Auf", "da\u00df", "dein", "Wuchs", "noch", "h\u00f6\u00b7her", "rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist ein Strecken vor dem Tod,", "tokens": ["Das", "ist", "ein", "Stre\u00b7cken", "vor", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist das Wachstum einer Leiche.", "tokens": ["Das", "ist", "das", "Wachs\u00b7tum", "ei\u00b7ner", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Von Hagel, Sturm und Regengu\u00df,", "tokens": ["Von", "Ha\u00b7gel", ",", "Sturm", "und", "Re\u00b7gen\u00b7gu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Blitz und Bomben oft getroffen,", "tokens": ["Von", "Blitz", "und", "Bom\u00b7ben", "oft", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gesteinigt wie dein Stephanus,", "tokens": ["Ge\u00b7stei\u00b7nigt", "wie", "dein", "Ste\u00b7pha\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Siehst wohl auch du den Himmel offen.", "tokens": ["Siehst", "wohl", "auch", "du", "den", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was soll Scheinleben dir und Zwang?", "tokens": ["Was", "soll", "Schein\u00b7le\u00b7ben", "dir", "und", "Zwang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mein Turm! zerspreng die Eisenringe!", "tokens": ["Mein", "Turm", "!", "zer\u00b7spreng", "die", "Ei\u00b7sen\u00b7rin\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einst\u00fcrzend unterm Glockenklang", "tokens": ["Ein\u00b7st\u00fcr\u00b7zend", "un\u00b7term", "Glo\u00b7cken\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schwanenlied den Sternen singe!", "tokens": ["Ein", "Schwa\u00b7nen\u00b7lied", "den", "Ster\u00b7nen", "sin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dann aus dem Schutte, Turmes Geist!", "tokens": ["Dann", "aus", "dem", "Schut\u00b7te", ",", "Tur\u00b7mes", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADJA", 