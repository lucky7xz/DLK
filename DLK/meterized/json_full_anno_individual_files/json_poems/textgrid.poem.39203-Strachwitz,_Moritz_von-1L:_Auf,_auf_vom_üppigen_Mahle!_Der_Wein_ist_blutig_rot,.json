{"textgrid.poem.39203": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf, auf vom \u00fcppigen Mahle! Der Wein ist blutig rot,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf vom \u00fcppigen Mahle! Der Wein ist blutig rot,", "tokens": ["Auf", ",", "auf", "vom", "\u00fcp\u00b7pi\u00b7gen", "Mah\u00b7le", "!", "Der", "Wein", "ist", "blu\u00b7tig", "rot", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "APPRART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es grinst aus jedem Pokale, aus jeder Sch\u00fcssel der Tod;", "tokens": ["Es", "grinst", "aus", "je\u00b7dem", "Po\u00b7ka\u00b7le", ",", "aus", "je\u00b7der", "Sch\u00fcs\u00b7sel", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob Eurem Haupte blitzen seh' ich am Haar das Schwert,", "tokens": ["Ob", "Eu\u00b7rem", "Haup\u00b7te", "blit\u00b7zen", "seh'", "ich", "am", "Haar", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ihr bleibt behaglich sitzen, bis es hernieder f\u00e4hrt.", "tokens": ["Ihr", "bleibt", "be\u00b7hag\u00b7lich", "sit\u00b7zen", ",", "bis", "es", "her\u00b7nie\u00b7der", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die alte schottische Sitte, ist sie Euch nicht bekannt,", "tokens": ["Die", "al\u00b7te", "schot\u00b7ti\u00b7sche", "Sit\u00b7te", ",", "ist", "sie", "Euch", "nicht", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn in des Tisches Mitte der blutige Stierkopf stand?", "tokens": ["Wenn", "in", "des", "Ti\u00b7sches", "Mit\u00b7te", "der", "blu\u00b7ti\u00b7ge", "Stier\u00b7kopf", "stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es stand in roter Lache des schwarzen B\u00fcffels Haupt,", "tokens": ["Es", "stand", "in", "ro\u00b7ter", "La\u00b7che", "des", "schwar\u00b7zen", "B\u00fcf\u00b7fels", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das war der Ruf der Rache, da kam der Tod geschnaubt.", "tokens": ["Das", "war", "der", "Ruf", "der", "Ra\u00b7che", ",", "da", "kam", "der", "Tod", "ge\u00b7schnaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sprangen von den Sitzen der Schlo\u00dfherr und sein Klan,", "tokens": ["Da", "spran\u00b7gen", "von", "den", "Sit\u00b7zen", "der", "Schlo\u00df\u00b7herr", "und", "sein", "Klan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das Blut begann zu spritzen, die Rache ward getan;", "tokens": ["Das", "Blut", "be\u00b7gann", "zu", "sprit\u00b7zen", ",", "die", "Ra\u00b7che", "ward", "ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie schnitt die Faust vom Stumpfe, die eben den Becher nahm,", "tokens": ["Sie", "schnitt", "die", "Faust", "vom", "Stump\u00b7fe", ",", "die", "e\u00b7ben", "den", "Be\u00b7cher", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie hieb den Kopf vom Rumpfe, eh' die Lippe zum Rande kam.", "tokens": ["Sie", "hieb", "den", "Kopf", "vom", "Rump\u00b7fe", ",", "eh'", "die", "Lip\u00b7pe", "zum", "Ran\u00b7de", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf, auf vom vollen Becher, dem Tode sei getrotzt,", "tokens": ["Auf", ",", "auf", "vom", "vol\u00b7len", "Be\u00b7cher", ",", "dem", "To\u00b7de", "sei", "ge\u00b7trotzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schaut, wie der stumme R\u00e4cher, der gr\u00e4\u00dfliche Stierkopf glotzt!", "tokens": ["Schaut", ",", "wie", "der", "stum\u00b7me", "R\u00e4\u00b7cher", ",", "der", "gr\u00e4\u00df\u00b7li\u00b7che", "Stier\u00b7kopf", "glotzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schon lange hat's gegoren, und wenn Ihr Euch nicht r\u00fchrt,", "tokens": ["Schon", "lan\u00b7ge", "hat's", "ge\u00b7go\u00b7ren", ",", "und", "wenn", "Ihr", "Euch", "nicht", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$,", "KON", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So ist der Kopf verloren, eh' der Kelch zur Lippe gef\u00fchrt.", "tokens": ["So", "ist", "der", "Kopf", "ver\u00b7lo\u00b7ren", ",", "eh'", "der", "Kelch", "zur", "Lip\u00b7pe", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "KOUS", "ART", "