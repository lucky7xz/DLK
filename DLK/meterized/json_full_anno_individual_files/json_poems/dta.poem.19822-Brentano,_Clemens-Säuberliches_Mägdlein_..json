{"dta.poem.19822": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4uberliches M\u00e4gdlein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo find ich deines Vaters Haus               ", "tokens": ["Wo", "find", "ich", "dei\u00b7nes", "Va\u00b7ters", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "S\u00e4uberliches M\u00e4gdlein?", "tokens": ["S\u00e4u\u00b7ber\u00b7li\u00b7ches", "M\u00e4gd\u00b7lein", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Geh das G\u00e4\u00dflein ab und aus,", "tokens": ["Geh", "das", "G\u00e4\u00df\u00b7lein", "ab", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweig still und la\u00df dein Fragen seyn.", "tokens": ["Schweig", "still", "und", "la\u00df", "dein", "Fra\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo bellet dann das H\u00fcndlein dein: S\u00e4ub. etc.", "tokens": ["Wo", "bel\u00b7let", "dann", "das", "H\u00fcnd\u00b7lein", "dein", ":", "S\u00e4ub", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "$.", "NN", "$.", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ruf den W\u00e4chter leise ein: Schweig etc.", "tokens": ["Ruf", "den", "W\u00e4ch\u00b7ter", "lei\u00b7se", "ein", ":", "Schweig", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["NN", "ART", "NE", "ADJD", "PTKVZ", "$.", "VVFIN", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Wo knarret dann das Th\u00fcrlein dein, S\u00e4ub. etc.", "tokens": ["Wo", "knar\u00b7ret", "dann", "das", "Th\u00fcr\u00b7lein", "dein", ",", "S\u00e4ub", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "$,", "NN", "$.", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Nimm den Haspen in die Hand: Schweig etc.", "tokens": ["Nimm", "den", "Has\u00b7pen", "in", "die", "Hand", ":", "Schweig", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$.", "VVFIN", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Wo schimmert dann das Feuer dein: S\u00e4ub. etc.", "tokens": ["Wo", "schim\u00b7mert", "dann", "das", "Feu\u00b7er", "dein", ":", "S\u00e4ub", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "$.", "NN", "$.", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Geu\u00df ein wenig Wasser drein: Schweig etc.", "tokens": ["Geu\u00df", "ein", "we\u00b7nig", "Was\u00b7ser", "drein", ":", "Schweig", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["NN", "ART", "PIAT", "NN", "PTKVZ", "$.", "VVFIN", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wo find ich dann das K\u00e4mmerlein: S\u00e4ub. etc.", "tokens": ["Wo", "find", "ich", "dann", "das", "K\u00e4m\u00b7mer\u00b7lein", ":", "S\u00e4ub", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "NN", "$.", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bey der K\u00fcche an der Wand: Schweig etc.", "tokens": ["Bey", "der", "K\u00fc\u00b7che", "an", "der", "Wand", ":", "Schweig", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$.", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo leg ich hin dein Hemdelein: S\u00e4ub. etc.", "tokens": ["Wo", "leg", "ich", "hin", "dein", "Hem\u00b7del\u00b7ein", ":", "S\u00e4ub", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["PWAV", "NE", "PPER", "ADV", "PPOSAT", "NN", "$.", "NN", "$.", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wei\u00dft dus nit, so nimms nicht rein: Schweig etc.", "tokens": ["Wei\u00dft", "dus", "nit", ",", "so", "nimms", "nicht", "rein", ":", "Schweig", "et\u00b7c."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["VVFIN", "NE", "PTKNEG", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "$.", "VVFIN", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Wie soll ich auf den Morgen thun: S\u00e4ub. etc.", "tokens": ["Wie", "soll", "ich", "auf", "den", "Mor\u00b7gen", "thun", ":", "S\u00e4ub", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "NN", "$.", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zieh dich an und geh darvon: Schweig. etc.", "tokens": ["Zieh", "dich", "an", "und", "geh", "dar\u00b7von", ":", "Schweig", ".", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "abbreviation"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PAV", "$.", "VVFIN", "$.", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}