{"textgrid.poem.62688": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Kluger Dichter. Ein Send-Schreiben", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kluger Dichter", "tokens": ["Klu\u00b7ger", "Dich\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Z\u00fcrne nicht, wenn ich hier nicht zierlich schreibe,", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "wenn", "ich", "hier", "nicht", "zier\u00b7lich", "schrei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und die Dichtkunst nicht so hoch, wie ich doch wohl solte, treibe.", "tokens": ["Und", "die", "Dicht\u00b7kunst", "nicht", "so", "hoch", ",", "wie", "ich", "doch", "wohl", "sol\u00b7te", ",", "trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,", "PWAV", "PPER", "ADV", "ADV", "VMFIN", "$,", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "H\u00e4tt ich deine sch\u00f6ne Gaben, h\u00e4tt ich deinen netten Kiel,", "tokens": ["H\u00e4tt", "ich", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Ga\u00b7ben", ",", "h\u00e4tt", "ich", "dei\u00b7nen", "net\u00b7ten", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "O! so schrieb ich schnell und sauber, k\u00fcnstlich, rein, galant, und viel.", "tokens": ["O", "!", "so", "schrieb", "ich", "schnell", "und", "sau\u00b7ber", ",", "k\u00fcnst\u00b7lich", ",", "rein", ",", "ga\u00b7lant", ",", "und", "viel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "KON", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Vor der Zieglerin ihr Lied dank ich dir nach allen Kr\u00e4ften,", "tokens": ["Vor", "der", "Zieg\u00b7le\u00b7rin", "ihr", "Lied", "dank", "ich", "dir", "nach", "al\u00b7len", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "APPR", "PPER", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "Und weil mirs so wohl gef\u00e4llt, will ichs noch an eins mit heften,", "tokens": ["Und", "weil", "mirs", "so", "wohl", "ge\u00b7f\u00e4llt", ",", "will", "ichs", "noch", "an", "eins", "mit", "hef\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ADV", "ADV", "VVPP", "$,", "VMFIN", "PIS", "ADV", "APPR", "PIS", "APPR", "ADJA", "$,"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Das sie auch mit Ruhm geschrieben. Hier komt auch dein Sch\u00e4fer-Lied,", "tokens": ["Das", "sie", "auch", "mit", "Ruhm", "ge\u00b7schrie\u00b7ben", ".", "Hier", "komt", "auch", "dein", "Sch\u00e4\u00b7fer\u00b7Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPR", "NN", "VVPP", "$.", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Das Dir, wie dein Brief mich lehret, einesmahls so bald gerieth.", "tokens": ["Das", "Dir", ",", "wie", "dein", "Brief", "mich", "leh\u00b7ret", ",", "ei\u00b7nes\u00b7mahls", "so", "bald", "ge\u00b7rieth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Ich versteh das R\u00e4zel nicht, darum kan ich nicht ergr\u00fcnden,", "tokens": ["Ich", "ver\u00b7steh", "das", "R\u00e4\u00b7zel", "nicht", ",", "da\u00b7rum", "kan", "ich", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wo die sch\u00f6nste Sch\u00f6nheit steckt. Ob man solte Fehler finden;", "tokens": ["Wo", "die", "sch\u00f6ns\u00b7te", "Sch\u00f6n\u00b7heit", "steckt", ".", "Ob", "man", "sol\u00b7te", "Feh\u00b7ler", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$.", "KOUS", "PIS", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Das wirst Du am besten wissen. Ey! was gehen sie mich an?", "tokens": ["Das", "wirst", "Du", "am", "bes\u00b7ten", "wis\u00b7sen", ".", "Ey", "!", "was", "ge\u00b7hen", "sie", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKA", "ADJD", "VVINF", "$.", "NN", "$.", "PWS", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Irr ich; so fehlst Du mit Willen, oder hasts aus Scherz gethan.", "tokens": ["Irr", "ich", ";", "so", "fehlst", "Du", "mit", "Wil\u00b7len", ",", "o\u00b7der", "hasts", "aus", "Scherz", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+--++-+-+-+-+-+", "measure": "dactylic.init"}, "line.14": {"text": "Ich behalt mein Sch\u00e4fer-St\u00fcck; denn Du m\u00f6chtest Glossen machen,", "tokens": ["Ich", "be\u00b7halt", "mein", "Sch\u00e4\u00b7fer\u00b7St\u00fcck", ";", "denn", "Du", "m\u00f6ch\u00b7test", "Glos\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "KON", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Und vieleicht auch h\u00f6hnisch sehn, oder Dich zu Tode lachen.", "tokens": ["Und", "vie\u00b7leicht", "auch", "h\u00f6h\u00b7nisch", "sehn", ",", "o\u00b7der", "Dich", "zu", "To\u00b7de", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVINF", "$,", "KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Di\u00df ist wieder mein Gewissen, und die Strafen w\u00fcrden mehr.", "tokens": ["Di\u00df", "ist", "wie\u00b7der", "mein", "Ge\u00b7wis\u00b7sen", ",", "und", "die", "Stra\u00b7fen", "w\u00fcr\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Und wo krigten wir denn wieder einen solchen Dichter her?", "tokens": ["Und", "wo", "krig\u00b7ten", "wir", "denn", "wie\u00b7der", "ei\u00b7nen", "sol\u00b7chen", "Dich\u00b7ter", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "PIAT", "NN", "PTKVZ", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.18": {"text": "Weil auch deine H\u00f6flichkeit mir im letzten Brief geschrieben,", "tokens": ["Weil", "auch", "dei\u00b7ne", "H\u00f6f\u00b7lich\u00b7keit", "mir", "im", "letz\u00b7ten", "Brief", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Da\u00df ich von der Poesie, die du schicktest, nach Belieben", "tokens": ["Da\u00df", "ich", "von", "der", "Poe\u00b7sie", ",", "die", "du", "schick\u00b7test", ",", "nach", "Be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "Mir was draus erwehlen m\u00f6chte: So hab ichs mit Dir getheilt.", "tokens": ["Mir", "was", "draus", "er\u00b7weh\u00b7len", "m\u00f6ch\u00b7te", ":", "So", "hab", "ichs", "mit", "Dir", "ge\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PAV", "VVINF", "VMFIN", "$.", "ADV", "VAFIN", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Sey so g\u00fctig und verzeihe wo mich hier ein Fehl ereilt.", "tokens": ["Sey", "so", "g\u00fc\u00b7tig", "und", "ver\u00b7zei\u00b7he", "wo", "mich", "hier", "ein", "Fehl", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "VVFIN", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Ich bedank mich vor das Buch so mir deine Hand verehret,", "tokens": ["Ich", "be\u00b7dank", "mich", "vor", "das", "Buch", "so", "mir", "dei\u00b7ne", "Hand", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Sonst zu nichts, nur darzu n\u00fczts, da\u00df es meine B\u00fccher mehret,", "tokens": ["Sonst", "zu", "nichts", ",", "nur", "dar\u00b7zu", "n\u00fczts", ",", "da\u00df", "es", "mei\u00b7ne", "B\u00fc\u00b7cher", "meh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "PIS", "$,", "ADV", "PAV", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-++-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Du verh\u00f6hnst die \u2013 \u2013 \u2013 ja du lachst \u2013 \u2013 \u2013 aus;", "tokens": ["Du", "ver\u00b7h\u00f6hnst", "die", "\u2013", "\u2013", "\u2013", "ja", "du", "lachst", "\u2013", "\u2013", "\u2013", "aus", ";"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$(", "$(", "$(", "PTKANT", "PPER", "VVFIN", "$(", "$(", "$(", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Zwar \u2013 \u2013 hats verdient. Und ich sage frey heraus,", "tokens": ["Zwar", "\u2013", "\u2013", "hats", "ver\u00b7dient", ".", "Und", "ich", "sa\u00b7ge", "frey", "he\u00b7raus", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "$(", "VAFIN", "VVPP", "$.", "KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Wie du auch gestehen must, da\u00df die \u2013 \u2013 \u2013 \u2013", "tokens": ["Wie", "du", "auch", "ge\u00b7ste\u00b7hen", "must", ",", "da\u00df", "die", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "ART", "$(", "$(", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Sich beym Ph\u00f6bus noch nicht darf um den Ruhm und Lorber melden.", "tokens": ["Sich", "beym", "Ph\u00f6\u00b7bus", "noch", "nicht", "darf", "um", "den", "Ruhm", "und", "Lor\u00b7ber", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ADV", "PTKNEG", "VMFIN", "APPR", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Doch der Druck ist nicht zu tadeln, damit man den Unterschied", "tokens": ["Doch", "der", "Druck", "ist", "nicht", "zu", "ta\u00b7deln", ",", "da\u00b7mit", "man", "den", "Un\u00b7ter\u00b7schied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "--+-+-+-++--+-+", "measure": "iambic.septa.relaxed"}, "line.29": {"text": "Zwischen einem gutem Dichter, und auch einen schlechten sieht", "tokens": ["Zwi\u00b7schen", "ei\u00b7nem", "gu\u00b7tem", "Dich\u00b7ter", ",", "und", "auch", "ei\u00b7nen", "schlech\u00b7ten", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "ADV", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Nun wie lauft der Pegasus? Hast du bald dem Berg erstiegen?", "tokens": ["Nun", "wie", "lauft", "der", "Pe\u00b7ga\u00b7sus", "?", "Hast", "du", "bald", "dem", "Berg", "er\u00b7stie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "ART", "NE", "$.", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.31": {"text": "Soll ich auf mein Thema bald eine nette Antwort kriegen?", "tokens": ["Soll", "ich", "auf", "mein", "The\u00b7ma", "bald", "ei\u00b7ne", "net\u00b7te", "Ant\u00b7wort", "krie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.32": {"text": "Jetzo wird das Meine kommen, da\u00df du mir gegeben hast.", "tokens": ["Jet\u00b7zo", "wird", "das", "Mei\u00b7ne", "kom\u00b7men", ",", "da\u00df", "du", "mir", "ge\u00b7ge\u00b7ben", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Doch ich schick es mit Err\u00f6hten, denn ich sch\u00e4me mich noch fast,", "tokens": ["Doch", "ich", "schick", "es", "mit", "Er\u00b7r\u00f6h\u00b7ten", ",", "denn", "ich", "sch\u00e4\u00b7me", "mich", "noch", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Da\u00df ich einen Musen Sohn wie du bist nicht besser ehre.", "tokens": ["Da\u00df", "ich", "ei\u00b7nen", "Mu\u00b7sen", "Sohn", "wie", "du", "bist", "nicht", "bes\u00b7ser", "eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KOKOM", "PPER", "VAFIN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Doch wenn ich einmahl dein Spiel selbst mit meinen Ohren h\u00f6re,", "tokens": ["Doch", "wenn", "ich", "ein\u00b7mahl", "dein", "Spiel", "selbst", "mit", "mei\u00b7nen", "Oh\u00b7ren", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.36": {"text": "(schweig! Du spielest nur vor Kenner, und dieweil dein Geist zu gro\u00df", "tokens": ["(", "schweig", "!", "Du", "spie\u00b7lest", "nur", "vor", "Ken\u00b7ner", ",", "und", "die\u00b7weil", "dein", "Geist", "zu", "gro\u00df"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "KON", "KOUS", "PPOSAT", "NN", "PTKA", "ADJD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.37": {"text": "Zeigst du deine Frau Gemahlin auch nicht einem jeden blo\u00df.)", "tokens": ["Zeigst", "du", "dei\u00b7ne", "Frau", "Ge\u00b7mah\u00b7lin", "auch", "nicht", "ei\u00b7nem", "je\u00b7den", "blo\u00df", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "ADV", "PTKNEG", "ART", "PIAT", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.38": {"text": "Soll die Feder besser seyn. Es ist mir sehr schlecht gelungen,", "tokens": ["Soll", "die", "Fe\u00b7der", "bes\u00b7ser", "seyn", ".", "Es", "ist", "mir", "sehr", "schlecht", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VAINF", "$.", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.39": {"text": "Und ich habe die Music nicht nach der W\u00fcrdigkeit besungen.", "tokens": ["Und", "ich", "ha\u00b7be", "die", "Mu\u00b7sic", "nicht", "nach", "der", "W\u00fcr\u00b7dig\u00b7keit", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.40": {"text": "Aber wenn mich Ph\u00f6bus liebet und mir Hand und Feder f\u00fchrt,", "tokens": ["A\u00b7ber", "wenn", "mich", "Ph\u00f6\u00b7bus", "lie\u00b7bet", "und", "mir", "Hand", "und", "Fe\u00b7der", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "KON", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.41": {"text": "Will ich auch geschickter schreiben. Nun ich bin wie sichs geb\u00fchrt.", "tokens": ["Will", "ich", "auch", "ge\u00b7schick\u00b7ter", "schrei\u00b7ben", ".", "Nun", "ich", "bin", "wie", "sichs", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$.", "ADV", "PPER", "VAFIN", "KOKOM", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Kluger Dichter", "tokens": ["Klu\u00b7ger", "Dich\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Z\u00fcrne nicht, wenn ich hier nicht zierlich schreibe,", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "wenn", "ich", "hier", "nicht", "zier\u00b7lich", "schrei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und die Dichtkunst nicht so hoch, wie ich doch wohl solte, treibe.", "tokens": ["Und", "die", "Dicht\u00b7kunst", "nicht", "so", "hoch", ",", "wie", "ich", "doch", "wohl", "sol\u00b7te", ",", "trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,", "PWAV", "PPER", "ADV", "ADV", "VMFIN", "$,", "VVFIN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "H\u00e4tt ich deine sch\u00f6ne Gaben, h\u00e4tt ich deinen netten Kiel,", "tokens": ["H\u00e4tt", "ich", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Ga\u00b7ben", ",", "h\u00e4tt", "ich", "dei\u00b7nen", "net\u00b7ten", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "O! so schrieb ich schnell und sauber, k\u00fcnstlich, rein, galant, und viel.", "tokens": ["O", "!", "so", "schrieb", "ich", "schnell", "und", "sau\u00b7ber", ",", "k\u00fcnst\u00b7lich", ",", "rein", ",", "ga\u00b7lant", ",", "und", "viel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "KON", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Vor der Zieglerin ihr Lied dank ich dir nach allen Kr\u00e4ften,", "tokens": ["Vor", "der", "Zieg\u00b7le\u00b7rin", "ihr", "Lied", "dank", "ich", "dir", "nach", "al\u00b7len", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "APPR", "PPER", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "Und weil mirs so wohl gef\u00e4llt, will ichs noch an eins mit heften,", "tokens": ["Und", "weil", "mirs", "so", "wohl", "ge\u00b7f\u00e4llt", ",", "will", "ichs", "noch", "an", "eins", "mit", "hef\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ADV", "ADV", "VVPP", "$,", "VMFIN", "PIS", "ADV", "APPR", "PIS", "APPR", "ADJA", "$,"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Das sie auch mit Ruhm geschrieben. Hier komt auch dein Sch\u00e4fer-Lied,", "tokens": ["Das", "sie", "auch", "mit", "Ruhm", "ge\u00b7schrie\u00b7ben", ".", "Hier", "komt", "auch", "dein", "Sch\u00e4\u00b7fer\u00b7Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPR", "NN", "VVPP", "$.", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Das Dir, wie dein Brief mich lehret, einesmahls so bald gerieth.", "tokens": ["Das", "Dir", ",", "wie", "dein", "Brief", "mich", "leh\u00b7ret", ",", "ei\u00b7nes\u00b7mahls", "so", "bald", "ge\u00b7rieth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Ich versteh das R\u00e4zel nicht, darum kan ich nicht ergr\u00fcnden,", "tokens": ["Ich", "ver\u00b7steh", "das", "R\u00e4\u00b7zel", "nicht", ",", "da\u00b7rum", "kan", "ich", "nicht", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wo die sch\u00f6nste Sch\u00f6nheit steckt. Ob man solte Fehler finden;", "tokens": ["Wo", "die", "sch\u00f6ns\u00b7te", "Sch\u00f6n\u00b7heit", "steckt", ".", "Ob", "man", "sol\u00b7te", "Feh\u00b7ler", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$.", "KOUS", "PIS", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Das wirst Du am besten wissen. Ey! was gehen sie mich an?", "tokens": ["Das", "wirst", "Du", "am", "bes\u00b7ten", "wis\u00b7sen", ".", "Ey", "!", "was", "ge\u00b7hen", "sie", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKA", "ADJD", "VVINF", "$.", "NN", "$.", "PWS", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Irr ich; so fehlst Du mit Willen, oder hasts aus Scherz gethan.", "tokens": ["Irr", "ich", ";", "so", "fehlst", "Du", "mit", "Wil\u00b7len", ",", "o\u00b7der", "hasts", "aus", "Scherz", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+--++-+-+-+-+-+", "measure": "dactylic.init"}, "line.14": {"text": "Ich behalt mein Sch\u00e4fer-St\u00fcck; denn Du m\u00f6chtest Glossen machen,", "tokens": ["Ich", "be\u00b7halt", "mein", "Sch\u00e4\u00b7fer\u00b7St\u00fcck", ";", "denn", "Du", "m\u00f6ch\u00b7test", "Glos\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "KON", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Und vieleicht auch h\u00f6hnisch sehn, oder Dich zu Tode lachen.", "tokens": ["Und", "vie\u00b7leicht", "auch", "h\u00f6h\u00b7nisch", "sehn", ",", "o\u00b7der", "Dich", "zu", "To\u00b7de", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVINF", "$,", "KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Di\u00df ist wieder mein Gewissen, und die Strafen w\u00fcrden mehr.", "tokens": ["Di\u00df", "ist", "wie\u00b7der", "mein", "Ge\u00b7wis\u00b7sen", ",", "und", "die", "Stra\u00b7fen", "w\u00fcr\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Und wo krigten wir denn wieder einen solchen Dichter her?", "tokens": ["Und", "wo", "krig\u00b7ten", "wir", "denn", "wie\u00b7der", "ei\u00b7nen", "sol\u00b7chen", "Dich\u00b7ter", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "PIAT", "NN", "PTKVZ", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.18": {"text": "Weil auch deine H\u00f6flichkeit mir im letzten Brief geschrieben,", "tokens": ["Weil", "auch", "dei\u00b7ne", "H\u00f6f\u00b7lich\u00b7keit", "mir", "im", "letz\u00b7ten", "Brief", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Da\u00df ich von der Poesie, die du schicktest, nach Belieben", "tokens": ["Da\u00df", "ich", "von", "der", "Poe\u00b7sie", ",", "die", "du", "schick\u00b7test", ",", "nach", "Be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "Mir was draus erwehlen m\u00f6chte: So hab ichs mit Dir getheilt.", "tokens": ["Mir", "was", "draus", "er\u00b7weh\u00b7len", "m\u00f6ch\u00b7te", ":", "So", "hab", "ichs", "mit", "Dir", "ge\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PAV", "VVINF", "VMFIN", "$.", "ADV", "VAFIN", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Sey so g\u00fctig und verzeihe wo mich hier ein Fehl ereilt.", "tokens": ["Sey", "so", "g\u00fc\u00b7tig", "und", "ver\u00b7zei\u00b7he", "wo", "mich", "hier", "ein", "Fehl", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "VVFIN", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Ich bedank mich vor das Buch so mir deine Hand verehret,", "tokens": ["Ich", "be\u00b7dank", "mich", "vor", "das", "Buch", "so", "mir", "dei\u00b7ne", "Hand", "ver\u00b7eh\u00b7ret", ","], 