{"dta.poem.4037": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Parzen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Nur Einen Sommer g\u00f6nnt, ihr Gewaltigen!", "tokens": ["Nur", "Ei\u00b7nen", "Som\u00b7mer", "g\u00f6nnt", ",", "ihr", "Ge\u00b7wal\u00b7ti\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Einen Herbst zu reifem Gesange mir,", "tokens": ["Und", "Ei\u00b7nen", "Herbst", "zu", "rei\u00b7fem", "Ge\u00b7san\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df williger mein Herz, vom s\u00fcssen", "tokens": ["Da\u00df", "wil\u00b7li\u00b7ger", "mein", "Herz", ",", "vom", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "PPOSAT", "NN", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spiele ges\u00e4ttiget, dann mir sterbe!", "tokens": ["Spie\u00b7le", "ge\u00b7s\u00e4t\u00b7ti\u00b7get", ",", "dann", "mir", "ster\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Seele, der im Leben ihr g\u00f6ttlich Recht", "tokens": ["Die", "See\u00b7le", ",", "der", "im", "Le\u00b7ben", "ihr", "g\u00f6tt\u00b7lich", "Recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht ward, sie ruht auch drunten im Orkus nicht;", "tokens": ["Nicht", "ward", ",", "sie", "ruht", "auch", "drun\u00b7ten", "im", "Or\u00b7kus", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch ist mir einst das Heil'ge, das am", "tokens": ["Doch", "ist", "mir", "einst", "das", "Heil'\u00b7ge", ",", "das", "am"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herzen mir liegt, das Gedicht gelungen:", "tokens": ["Her\u00b7zen", "mir", "liegt", ",", "das", "Ge\u00b7dicht", "ge\u00b7lun\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Willkommen dann, o Stille der Schattenwelt!", "tokens": ["Will\u00b7kom\u00b7men", "dann", ",", "o", "Stil\u00b7le", "der", "Schat\u00b7ten\u00b7welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zufrieden bin ich, wenn auch mein Saitenspiel", "tokens": ["Zu\u00b7frie\u00b7den", "bin", "ich", ",", "wenn", "auch", "mein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich nicht hinabgeleitet; Einmal", "tokens": ["Mich", "nicht", "hin\u00b7ab\u00b7ge\u00b7lei\u00b7tet", ";", "Ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKNEG", "VVPP", "$.", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebt' ich, wie G\u00f6tter, und mehr bedarf's nicht.", "tokens": ["Lebt'", "ich", ",", "wie", "G\u00f6t\u00b7ter", ",", "und", "mehr", "be\u00b7da\u00b7rf's", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "$,", "KON", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}}}}