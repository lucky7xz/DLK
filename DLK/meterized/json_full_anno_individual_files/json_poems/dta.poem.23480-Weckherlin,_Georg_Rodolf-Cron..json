{"dta.poem.23480": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Djser welt lieb/ hofnung/ wollust vnd", "tokens": ["Djser", "welt", "lieb", "/", "hof\u00b7nung", "/", "wol\u00b7lust", "vnd"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "ADJD", "$(", "NN", "$(", "VMFIN", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "prei\u00df", "tokens": ["prei\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wirt Er billich allein geachtet/", "tokens": ["Wirt", "Er", "bil\u00b7lich", "al\u00b7lein", "ge\u00b7ach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "VVPP", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wan man allein seine ", "tokens": ["Wan", "man", "al\u00b7lein", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "(als der Tugenten haubt) betrachtet.", "tokens": ["(", "als", "der", "Tu\u00b7gen\u00b7ten", "haubt", ")", "be\u00b7trach\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "VVFIN", "$(", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Durch Sie kan Er/ wei\u00df/ f\u00fcrsichtig vnd klug", "tokens": ["Durch", "Sie", "kan", "Er", "/", "wei\u00df", "/", "f\u00fcr\u00b7sich\u00b7tig", "vnd", "klug"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "$(", "VVFIN", "$(", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Wol in dem grund den verst\u00f6lten Betrug/", "tokens": ["Wol", "in", "dem", "grund", "den", "ver\u00b7st\u00f6l\u00b7ten", "Be\u00b7trug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Vnd die verborgne Warheit sehen/", "tokens": ["Vnd", "die", "ver\u00b7borg\u00b7ne", "War\u00b7heit", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd jenem vnd der Rew entgehen.", "tokens": ["Vnd", "je\u00b7nem", "vnd", "der", "Rew", "ent\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durch Sie bef\u00fcrdert Er die gleich", "tokens": ["Durch", "Sie", "be\u00b7f\u00fcr\u00b7dert", "Er", "die", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Deren verdienst Jhre bit nicht verneinet;", "tokens": ["De\u00b7ren", "ver\u00b7dienst", "Ih\u00b7re", "bit", "nicht", "ver\u00b7nei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Durch Sie ist Er Jhm selbs stehts gleich/", "tokens": ["Durch", "Sie", "ist", "Er", "Jhm", "selbs", "stehts", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dz er nicht spricht/ dz h\u00e4t ich nicht vermeinet.", "tokens": ["Dz", "er", "nicht", "spricht", "/", "dz", "h\u00e4t", "ich", "nicht", "ver\u00b7mei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$(", "KOUS", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Ja auf diser Welt wildem M\u00f6hr/", "tokens": ["Ja", "auf", "di\u00b7ser", "Welt", "wil\u00b7dem", "M\u00f6hr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PDAT", "NN", "ADJA", "NN", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Kan Er durch Sie ohn forcht aller gefahre\u0303", "tokens": ["Kan", "Er", "durch", "Sie", "ohn", "forcht", "al\u00b7ler", "ge\u00b7fahr\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Mit seinen Tugenten vmbfahren", "tokens": ["Mit", "sei\u00b7nen", "Tu\u00b7gen\u00b7ten", "vmb\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.17": {"text": "Vnverwundlich vnder der Feinden H\u00f6hr.", "tokens": ["Vn\u00b7ver\u00b7wund\u00b7lich", "vn\u00b7der", "der", "Fein\u00b7den", "H\u00f6hr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}