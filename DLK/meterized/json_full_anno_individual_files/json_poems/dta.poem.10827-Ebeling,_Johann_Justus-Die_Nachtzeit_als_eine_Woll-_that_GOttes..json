{"dta.poem.10827": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtzeit als eine Woll-  \n that GOttes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist der war vom Denken m\u00fcde,\nund auch die Kraft der Sinnen mat,", "tokens": ["Mein", "Geist", "der", "war", "vom", "Den\u00b7ken", "m\u00fc\u00b7de", ",", "und", "auch", "die", "Kraft", "der", "Sin\u00b7nen", "mat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "APPRART", "NN", "ADJD", "$,", "KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+----+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Als sich das helle Licht der Sonnen,", "tokens": ["Als", "sich", "das", "hel\u00b7le", "Licht", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Abendgrenzen n\u00e4her zog. Die A-", "tokens": ["Den", "A\u00b7bend\u00b7gren\u00b7zen", "n\u00e4\u00b7her", "zog", ".", "Die", "A"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ehe man es sich versah, war alles", "tokens": ["Und", "e\u00b7he", "man", "es", "sich", "ver\u00b7sah", ",", "war", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PIS", "PPER", "PRF", "VVFIN", "$,", "VAFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Augen konnten nicht mehr sehen. Ich sprach:", "tokens": ["Die", "Au\u00b7gen", "konn\u00b7ten", "nicht", "mehr", "se\u00b7hen", ".", "Ich", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "VVINF", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Ist nun auch abermahl vollendet: Nun ist die s\u00fcsse", "tokens": ["Ist", "nun", "auch", "a\u00b7ber\u00b7mahl", "voll\u00b7en\u00b7det", ":", "Nun", "ist", "die", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$.", "ADV", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+----+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ich legte Buch und Feder hin, und bei der Rast", "tokens": ["Ich", "leg\u00b7te", "Buch", "und", "Fe\u00b7der", "hin", ",", "und", "bei", "der", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erholte sich der matte Geist, in einer stillen Ruhe", "tokens": ["Er\u00b7hol\u00b7te", "sich", "der", "mat\u00b7te", "Geist", ",", "in", "ei\u00b7ner", "stil\u00b7len", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Ich wurde recht in ", "tokens": ["Ich", "wur\u00b7de", "recht", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und breitete die schwarzen Schatten; es wurden an", "tokens": ["Und", "brei\u00b7te\u00b7te", "die", "schwar\u00b7zen", "Schat\u00b7ten", ";", "es", "wur\u00b7den", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Die Nachtlaternen, die Gestirn uns sichtbahr in den", "tokens": ["Die", "Nacht\u00b7la\u00b7ter\u00b7nen", ",", "die", "Ge\u00b7stirn", "uns", "sicht\u00b7bahr", "in", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "PRF", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich sah sie an dem Firmament, in einer stillen Klar-", "tokens": ["Ich", "sah", "sie", "an", "dem", "Fir\u00b7ma\u00b7ment", ",", "in", "ei\u00b7ner", "stil\u00b7len", "Kla\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Der Schlaf durchkroch zulezt die Glieder; die Stil-", "tokens": ["Der", "Schlaf", "durch\u00b7kroch", "zu\u00b7lezt", "die", "Glie\u00b7der", ";", "die", "Stil"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Es st\u00f6hrte mich nicht in der Ruhe, der hellen Sonne", "tokens": ["Es", "st\u00f6hr\u00b7te", "mich", "nicht", "in", "der", "Ru\u00b7he", ",", "der", "hel\u00b7len", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.15": {"text": "Es war der Schatten schwarzer Nacht, an dem be-", "tokens": ["Es", "war", "der", "Schat\u00b7ten", "schwar\u00b7zer", "Nacht", ",", "an", "dem", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Als wie ein Vorhang ausgespannt, und um mein", "tokens": ["Als", "wie", "ein", "Vor\u00b7hang", "aus\u00b7ge\u00b7spannt", ",", "und", "um", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVPP", "$,", "KON", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Ich leg in denen weichen Federn, und schlief bei die-", "tokens": ["Ich", "leg", "in", "de\u00b7nen", "wei\u00b7chen", "Fe\u00b7dern", ",", "und", "schlief", "bei", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "APPR", "PRELS", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Bis de\u00df die Morgenr\u00f6th erwachte, die mich erwek-", "tokens": ["Bis", "de\u00df", "die", "Mor\u00b7gen\u00b7r\u00f6th", "er\u00b7wach\u00b7te", ",", "die", "mich", "er\u00b7wek"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Mein Geist war munter und der Leib versp\u00fcrte zu", "tokens": ["Mein", "Geist", "war", "mun\u00b7ter", "und", "der", "Leib", "ver\u00b7sp\u00fcr\u00b7te", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da sich die Nerven aufgespannt, auch wieder die", "tokens": ["Da", "sich", "die", "Ner\u00b7ven", "auf\u00b7ge\u00b7spannt", ",", "auch", "wie\u00b7der", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$,", "ADV", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es fiel mr zu des Sch\u00f6pfers Preise, des Morgens", "tokens": ["Es", "fiel", "mr", "zu", "des", "Sch\u00f6p\u00b7fers", "Prei\u00b7se", ",", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Sehr weislich zu des Menschen Leben und zu der", "tokens": ["Sehr", "weis\u00b7lich", "zu", "des", "Men\u00b7schen", "Le\u00b7ben", "und", "zu", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN", "KON", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Dieweil der Schlaf uns n\u00fczlich ist, des Leibes Kr\u00e4fte", "tokens": ["Die\u00b7weil", "der", "Schlaf", "uns", "n\u00fcz\u00b7lich", "ist", ",", "des", "Lei\u00b7bes", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sonst w\u00fcrde unsers Leibes Bau gar bald ermatten", "tokens": ["Sonst", "w\u00fcr\u00b7de", "un\u00b7sers", "Lei\u00b7bes", "Bau", "gar", "bald", "er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sonst k\u00f6nnten unsre \u00e4usre Sinnen, auch nicht so", "tokens": ["Sonst", "k\u00f6nn\u00b7ten", "uns\u00b7re", "\u00e4us\u00b7re", "Sin\u00b7nen", ",", "auch", "nicht", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es m\u00fcsten aller Nerven Kr\u00e4fte, wenn sie stets auf-", "tokens": ["Es", "m\u00fcs\u00b7ten", "al\u00b7ler", "Ner\u00b7ven", "Kr\u00e4f\u00b7te", ",", "wenn", "sie", "stets", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "NN", "$,", "KOUS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Die Nacht ist gut auch vor dem Geist; der wird", "tokens": ["Die", "Nacht", "ist", "gut", "auch", "vor", "dem", "Geist", ";", "der", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$.", "ART", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Auf eine wunderbahre Art beim stillen Schlummer", "tokens": ["Auf", "ei\u00b7ne", "wun\u00b7der\u00b7bah\u00b7re", "Art", "beim", "stil\u00b7len", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Sinnen unsrer Seelen W\u00e4chter, die geben.", "tokens": ["Die", "Sin\u00b7nen", "uns\u00b7rer", "See\u00b7len", "W\u00e4ch\u00b7ter", ",", "die", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "NE", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Weil alles in der Stille lieget, was auser uns vor-", "tokens": ["Weil", "al\u00b7les", "in", "der", "Stil\u00b7le", "lie\u00b7get", ",", "was", "au\u00b7ser", "uns", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Der Augen Vorhang schliest sich zu: Wir k\u00f6nnen", "tokens": ["Der", "Au\u00b7gen", "Vor\u00b7hang", "schliest", "sich", "zu", ":", "Wir", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "PTKVZ", "$.", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Sonst w\u00fcrde bei der stillen Ruh, verwirrte Unruh", "tokens": ["Sonst", "w\u00fcr\u00b7de", "bei", "der", "stil\u00b7len", "Ruh", ",", "ver\u00b7wirr\u00b7te", "Un\u00b7ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Nacht und deren ihr Geselle, der Schlaf bringt", "tokens": ["Die", "Nacht", "und", "de\u00b7ren", "ihr", "Ge\u00b7sel\u00b7le", ",", "der", "Schlaf", "bringt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PDS", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Und dienet, wie wir schon geh\u00f6ret, auch der Gesund-", "tokens": ["Und", "die\u00b7net", ",", "wie", "wir", "schon", "ge\u00b7h\u00f6\u00b7ret", ",", "auch", "der", "Ge\u00b7sun\u00b7d"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Der Schlaf ist zu dem Wachsthum gut der Jugend;", "tokens": ["Der", "Schlaf", "ist", "zu", "dem", "Wach\u00b7sthum", "gut", "der", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Bei ihrer guten Leibeskraft, noch l\u00e4nger daurhaft", "tokens": ["Bei", "ih\u00b7rer", "gu\u00b7ten", "Lei\u00b7bes\u00b7kraft", ",", "noch", "l\u00e4n\u00b7ger", "daur\u00b7haft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Schlaf ist bei gesunden Tagen ein Mittel da-", "tokens": ["Der", "Schlaf", "ist", "bei", "ge\u00b7sun\u00b7den", "Ta\u00b7gen", "ein", "Mit\u00b7tel", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "In einem warmen Bette kommen; da wir wenn un-", "tokens": ["In", "ei\u00b7nem", "war\u00b7men", "Bet\u00b7te", "kom\u00b7men", ";", "da", "wir", "wenn", "un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$.", "KOUS", "PPER", "KOUS", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Die sanfte Ausd\u00fcnstung versp\u00fcrn, dadurch der Un-", "tokens": ["Die", "sanf\u00b7te", "Aus\u00b7d\u00fcns\u00b7tung", "ver\u00b7sp\u00fcrn", ",", "da\u00b7durch", "der", "Un"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "PAV", "ART", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Der unsern K\u00f6rper tr\u00e4ge macht, wenn er in Haut", "tokens": ["Der", "un\u00b7sern", "K\u00f6r\u00b7per", "tr\u00e4\u00b7ge", "macht", ",", "wenn", "er", "in", "Haut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Schlaf ist gut auch f\u00fcr die Kranken, und dienet", "tokens": ["Der", "Schlaf", "ist", "gut", "auch", "f\u00fcr", "die", "Kran\u00b7ken", ",", "und", "die\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Wenn sie nur erst zum Schlummer kommen; so merkt", "tokens": ["Wenn", "sie", "nur", "erst", "zum", "Schlum\u00b7mer", "kom\u00b7men", ";", "so", "merkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "l