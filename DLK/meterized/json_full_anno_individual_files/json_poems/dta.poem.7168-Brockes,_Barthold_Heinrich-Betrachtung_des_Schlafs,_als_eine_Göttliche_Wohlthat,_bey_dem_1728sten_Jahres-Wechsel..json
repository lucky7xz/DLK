{"dta.poem.7168": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Inde\u00df gieb, da\u00df so offt wir uns zur Ruhe legen,", "tokens": ["In\u00b7de\u00df", "gieb", ",", "da\u00df", "so", "offt", "wir", "uns", "zur", "Ru\u00b7he", "le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ADV", "ADV", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir Dein so herrlich Werck, an unsern Schlaf, er-", "tokens": ["Wir", "Dein", "so", "herr\u00b7lich", "Werck", ",", "an", "un\u00b7sern", "Schlaf", ",", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PPOSAT", "ADV", "ADJD", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wegen,", "tokens": ["we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["APPR", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "La\u00df unser Hertz davor durch Dancken Dich erh\u00f6hn,", "tokens": ["La\u00df", "un\u00b7ser", "Hertz", "da\u00b7vor", "durch", "Dan\u00b7cken", "Dich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PAV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da Du so Nutz, als Lust im Schlaffen uns beschieden,", "tokens": ["Da", "Du", "so", "Nutz", ",", "als", "Lust", "im", "Schlaf\u00b7fen", "uns", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "$,", "KOUS", "NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sprechen wir mit Recht, wenn wir zur Ruhe gehn,", "tokens": ["So", "spre\u00b7chen", "wir", "mit", "Recht", ",", "wenn", "wir", "zur", "Ru\u00b7he", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich lieg' und schlafe gantz mit Frieden.", "tokens": ["Ich", "lieg'", "und", "schla\u00b7fe", "gantz", "mit", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}