{"dta.poem.16249": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "303.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn mit Gef\u00e4lligkeit du einen willst verbinden,", "tokens": ["Wenn", "mit", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "du", "ei\u00b7nen", "willst", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df ihn zusehr dabei dein Ansehn nicht empfinden.", "tokens": ["La\u00df", "ihn", "zu\u00b7sehr", "da\u00b7bei", "dein", "An\u00b7sehn", "nicht", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PAV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft ihm f\u00fcr die Gunst erniedrigende Bitt'", "tokens": ["Du", "mu\u00dft", "ihm", "f\u00fcr", "die", "Gunst", "er\u00b7nied\u00b7ri\u00b7gen\u00b7de", "Bitt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ersparen, oder er h\u00e4lt sich des Dankes quitt.", "tokens": ["Er\u00b7spa\u00b7ren", ",", "o\u00b7der", "er", "h\u00e4lt", "sich", "des", "Dan\u00b7kes", "quitt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VVFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relax