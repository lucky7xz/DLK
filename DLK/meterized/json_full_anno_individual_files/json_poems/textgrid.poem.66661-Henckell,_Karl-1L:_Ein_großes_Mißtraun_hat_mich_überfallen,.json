{"textgrid.poem.66661": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gro\u00dfes Mi\u00dftraun hat mich \u00fcberfallen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfes Mi\u00dftraun hat mich \u00fcberfallen,", "tokens": ["Ein", "gro\u00b7\u00dfes", "Mi\u00df\u00b7traun", "hat", "mich", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Herz und Nieren pr\u00fcf' ich jedes Wort.", "tokens": ["Auf", "Herz", "und", "Nie\u00b7ren", "pr\u00fcf'", "ich", "je\u00b7des", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man sieht es blitzen, h\u00f6rt den Donner schallen,", "tokens": ["Man", "sieht", "es", "blit\u00b7zen", ",", "h\u00f6rt", "den", "Don\u00b7ner", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einmal schiebt sich die Kulisse fort,", "tokens": ["Auf", "ein\u00b7mal", "schiebt", "sich", "die", "Ku\u00b7lis\u00b7se", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da liegt das Donnerblech in ganzen Ballen,", "tokens": ["Da", "liegt", "das", "Don\u00b7ner\u00b7blech", "in", "gan\u00b7zen", "Bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der B\u00fchne Feuerzungen sind verdorrt \u2013", "tokens": ["Der", "B\u00fch\u00b7ne", "Feu\u00b7er\u00b7zun\u00b7gen", "sind", "ver\u00b7dorrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Pyrotechniker von Geistes Gnaden", "tokens": ["Der", "Py\u00b7ro\u00b7tech\u00b7ni\u00b7ker", "von", "Geis\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schleicht fort mit Z\u00fcndschnur und verbranntem Faden.", "tokens": ["Schleicht", "fort", "mit", "Z\u00fcnd\u00b7schnur", "und", "ver\u00b7brann\u00b7tem", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Ehrlichkeit der Seele will ich wittern,", "tokens": ["Die", "Ehr\u00b7lich\u00b7keit", "der", "See\u00b7le", "will", "ich", "wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit oder ohne Reim, blo\u00df das Getu,", "tokens": ["Mit", "o\u00b7der", "oh\u00b7ne", "Reim", ",", "blo\u00df", "das", "Ge\u00b7tu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dies modisch unsolide Phrasenflittern", "tokens": ["Dies", "mo\u00b7disch", "un\u00b7so\u00b7li\u00b7de", "Phra\u00b7sen\u00b7flit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ber\u00fchrt mich kl\u00e4glich wie gestohlne Schuh", "tokens": ["Be\u00b7r\u00fchrt", "mich", "kl\u00e4g\u00b7lich", "wie", "ge\u00b7stohl\u00b7ne", "Schuh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von K\u00f6nigen an sch\u00e4bigen Heckenrittern,", "tokens": ["Von", "K\u00f6\u00b7ni\u00b7gen", "an", "sch\u00e4\u00b7bi\u00b7gen", "He\u00b7cken\u00b7rit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vor Prunkfu\u00df halt' ich mir die ", "tokens": ["Vor", "Prunk\u00b7fu\u00df", "halt'", "ich", "mir", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die Pose t\u00e4uscht, mein Gott, welch fauler Zauber!", "tokens": ["Die", "Po\u00b7se", "t\u00e4uscht", ",", "mein", "Gott", ",", "welch", "fau\u00b7ler", "Zau\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Man glaubt an Prinzen \u2013 und das Hemd nicht sauber!", "tokens": ["Man", "glaubt", "an", "Prin\u00b7zen", "\u2013", "und", "das", "Hemd", "nicht", "sau\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$(", "KON", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ist Dichtkunst denn ein Kom\u00f6diantenkasten,", "tokens": ["Ist", "Dicht\u00b7kunst", "denn", "ein", "Ko\u00b7m\u00f6\u00b7di\u00b7an\u00b7ten\u00b7kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "In dem der Mensch sich f\u00fcrchterlich drapiert?", "tokens": ["In", "dem", "der", "Mensch", "sich", "f\u00fcrch\u00b7ter\u00b7lich", "dra\u00b7piert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald \u00dcbermensch mit plumpen Nietzschequasten,", "tokens": ["Bald", "\u00dc\u00b7ber\u00b7mensch", "mit", "plum\u00b7pen", "Nietz\u00b7sche\u00b7quas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der kra\u00df mit Renaissance kokettiert,", "tokens": ["Der", "kra\u00df", "mit", "Re\u00b7nais\u00b7san\u00b7ce", "ko\u00b7ket\u00b7tiert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald S\u00e4useler auf Symbolistentasten,", "tokens": ["Bald", "S\u00e4u\u00b7se\u00b7ler", "auf", "Sym\u00b7bo\u00b7lis\u00b7ten\u00b7tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Romantik mit Verr\u00fccktheit impr\u00e4gniert,", "tokens": ["Ro\u00b7man\u00b7tik", "mit", "Ver\u00b7r\u00fcckt\u00b7heit", "im\u00b7pr\u00e4g\u00b7niert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Bald Junkersporen, D\u00f6rpertanzgeb\u00e4rde,", "tokens": ["Bald", "Jun\u00b7ker\u00b7spo\u00b7ren", ",", "D\u00f6r\u00b7per\u00b7tanz\u00b7ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Selbst Eichendorff auf dem Theaterpferde!", "tokens": ["Selbst", "Ei\u00b7chen\u00b7dorff", "auf", "dem", "The\u00b7a\u00b7ter\u00b7pfer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Br\u00fcderchen, warum mehr scheinen wollen,", "tokens": ["Ihr", "Br\u00fc\u00b7der\u00b7chen", ",", "wa\u00b7rum", "mehr", "schei\u00b7nen", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als man nun grade mal von Haus besitzt?", "tokens": ["Als", "man", "nun", "gra\u00b7de", "mal", "von", "Haus", "be\u00b7sitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll man dem Pomp von Pumpus Gnaden zollen", "tokens": ["Soll", "man", "dem", "Pomp", "von", "Pum\u00b7pus", "Gna\u00b7den", "zol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "NN", "APPR", "NE", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Ehre, die ihr schwindelnd euch stibitzt?", "tokens": ["Die", "Eh\u00b7re", ",", "die", "ihr", "schwin\u00b7delnd", "euch", "sti\u00b7bitzt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hochstapler seid ihr, tut wie aus dem Vollen,", "tokens": ["Hoch\u00b7stap\u00b7ler", "seid", "ihr", ",", "tut", "wie", "aus", "dem", "Vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVFIN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Indes ihr \u00bbNichts!\u00ab aus allen Poren schwitzt ...", "tokens": ["In\u00b7des", "ihr", "\u00bb", "Nichts", "!", "\u00ab", "aus", "al\u00b7len", "Po\u00b7ren", "schwitzt", "..."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$(", "PIS", "$.", "$(", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Man kennt euch doch und eure nobeln Schliche,", "tokens": ["Man", "kennt", "euch", "doch", "und", "eu\u00b7re", "no\u00b7beln", "Schli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Grandiose falsche Grafen \u2013 Tinteriche!", "tokens": ["Gran\u00b7di\u00b7o\u00b7se", "fal\u00b7sche", "Gra\u00b7fen", "\u2013", "Tin\u00b7te\u00b7ri\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "NE", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach, h\u00e4ttet ihr zu Wert und Ma\u00df der Dinge", "tokens": ["Ach", ",", "h\u00e4t\u00b7tet", "ihr", "zu", "Wert", "und", "Ma\u00df", "der", "Din\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "APPR", "NE", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schlichte Liebe nur \u2013 es st\u00e4nde gut.", "tokens": ["Die", "schlich\u00b7te", "Lie\u00b7be", "nur", "\u2013", "es", "st\u00e4n\u00b7de", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$(", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So aber seid ihr nichts als Dichterlinge,", "tokens": ["So", "a\u00b7ber", "seid", "ihr", "nichts", "als", "Dich\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob ihr auch hochmodern spottwichtig tut.", "tokens": ["Ob", "ihr", "auch", "hoch\u00b7mo\u00b7dern", "spott\u00b7wich\u00b7tig", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich kreuze nicht mit Hinz und Kunz die Klinge,", "tokens": ["Ich", "kreu\u00b7ze", "nicht", "mit", "Hinz", "und", "Kunz", "die", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn epidemisch wuchert diese Brut.", "tokens": ["Denn", "e\u00b7pi\u00b7de\u00b7misch", "wu\u00b7chert", "die\u00b7se", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf manchen setzt' ich anfangs viel Vertrauen", "tokens": ["Auf", "man\u00b7chen", "setzt'", "ich", "an\u00b7fangs", "viel", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mu\u00df ihn jetzt beim Volk der Gaukler schauen.", "tokens": ["Und", "mu\u00df", "ihn", "jetzt", "beim", "Volk", "der", "Gauk\u00b7ler", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein gro\u00dfes Mi\u00dftraun hat mich \u00fcberfallen,", "tokens": ["Ein", "gro\u00b7\u00dfes", "Mi\u00df\u00b7traun", "hat", "mich", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Herz und Nieren pr\u00fcf' ich jedes Wort.", "tokens": ["Auf", "Herz", "und", "Nie\u00b7ren", "pr\u00fcf'", "ich", "je\u00b7des", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man sieht es blitzen, h\u00f6rt den Donner schallen,", "tokens": ["Man", "sieht", "es", "blit\u00b7zen", ",", "h\u00f6rt", "den", "Don\u00b7ner", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einmal schiebt sich die Kulisse fort,", "tokens": ["Auf", "ein\u00b7mal", "schiebt", "sich", "die", "Ku\u00b7lis\u00b7se", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da liegt das Donnerblech in ganzen Ballen,", "tokens": ["Da", "liegt", "das", "Don\u00b7ner\u00b7blech", "in", "gan\u00b7zen", "Bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der B\u00fchne Feuerzungen sind verdorrt \u2013", "tokens": ["Der", "B\u00fch\u00b7ne", "Feu\u00b7er\u00b7zun\u00b7gen", "sind", "ver\u00b7dorrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Pyrotechniker von Geistes Gnaden", "tokens": ["Der", "Py\u00b7ro\u00b7tech\u00b7ni\u00b7ker", "von", "Geis\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schleicht fort mit Z\u00fcndschnur und verbranntem Faden.", "tokens": ["Schleicht", "fort", "mit", "Z\u00fcnd\u00b7schnur", "und", "ver\u00b7bra