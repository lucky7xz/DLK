{"dta.poem.6522": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Phoebus sich von uns entfernet, (wie man meinet)", "tokens": ["Wenn", "Phoe\u00b7bus", "sich", "von", "uns", "ent\u00b7fer\u00b7net", ",", "(", "wie", "man", "mei\u00b7net", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "APPR", "PPER", "VVFIN", "$,", "$(", "PWAV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und er mit schr\u00e4gem Blick auf unsre Fl\u00e4che scheinet;", "tokens": ["Und", "er", "mit", "schr\u00e4\u00b7gem", "Blick", "auf", "uns\u00b7re", "Fl\u00e4\u00b7che", "schei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So treffen uns von seinem Licht", "tokens": ["So", "tref\u00b7fen", "uns", "von", "sei\u00b7nem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr viele Strahlen nicht:", "tokens": ["Sehr", "vie\u00b7le", "Strah\u00b7len", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da eine grosse Meng\u2019 in L\u00fcfften sich verlieret,", "tokens": ["Da", "ei\u00b7ne", "gros\u00b7se", "Meng'", "in", "L\u00fcff\u00b7ten", "sich", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wodurch man minder Licht und minder W\u00e4rme sp\u00fcret.", "tokens": ["Wo\u00b7durch", "man", "min\u00b7der", "Licht", "und", "min\u00b7der", "W\u00e4r\u00b7me", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hingegen in der Sommer-Zeit,", "tokens": ["Hin\u00b7ge\u00b7gen", "in", "der", "Som\u00b7mer\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn unser Horizont von Glantz und Heiterkeit", "tokens": ["Wenn", "un\u00b7ser", "Ho\u00b7ri\u00b7zont", "von", "Glantz", "und", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Fast angez\u00fcndet ist, durch seinen Flammen-Wagen,", "tokens": ["Fast", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "ist", ",", "durch", "sei\u00b7nen", "Flam\u00b7men\u00b7Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der \u00fcber unserm Haupt scheint in den Krebs zu jagen;", "tokens": ["Der", "\u00fc\u00b7ber", "un\u00b7serm", "Haupt", "scheint", "in", "den", "Krebs", "zu", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So dringet auf uns zu, von allen Seiten,", "tokens": ["So", "drin\u00b7get", "auf", "uns", "zu", ",", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sein Strahlen-Heer mit regen Heiterkeiten,", "tokens": ["Sein", "Strah\u00b7len\u00b7Heer", "mit", "re\u00b7gen", "Hei\u00b7ter\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nichts hemmet ihren Lauf, nichts mindert ihre Pracht,", "tokens": ["Nichts", "hem\u00b7met", "ih\u00b7ren", "Lauf", ",", "nichts", "min\u00b7dert", "ih\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wir empfinden mehr der Sonnen Licht und Macht.", "tokens": ["Und", "wir", "emp\u00b7fin\u00b7den", "mehr", "der", "Son\u00b7nen", "Licht", "und", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}