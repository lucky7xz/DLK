, "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit blauem Himmel, frischem Gr\u00fcn,", "tokens": ["Mit", "blau\u00b7em", "Him\u00b7mel", ",", "fri\u00b7schem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesang und Blumenprangen!", "tokens": ["Ge\u00b7sang", "und", "Blu\u00b7men\u00b7pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ja, zweierlei ist Schal' und Kern!", "tokens": ["Ja", ",", "zwei\u00b7er\u00b7lei", "ist", "Schal'", "und", "Kern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Spruch hab' ich erwandert!", "tokens": ["Den", "Spruch", "hab'", "ich", "er\u00b7wan\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und zweifelt wer an ihm, ihr Herrn,", "tokens": ["Und", "zwei\u00b7felt", "wer", "an", "ihm", ",", "ihr", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Knackt N\u00fcsse, oder wandert!", "tokens": ["Knackt", "N\u00fcs\u00b7se", ",", "o\u00b7der", "wan\u00b7dert", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}