{"dta.poem.2312": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Doris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcnstlerinn! wir k\u00fcnsteln beide,", "tokens": ["K\u00fcnst\u00b7le\u00b7rinn", "!", "wir", "k\u00fcns\u00b7teln", "bei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du kannst stikken, ich kann malen.", "tokens": ["Du", "kannst", "stik\u00b7ken", ",", "ich", "kann", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber stikkst du denn nur Blumen?", "tokens": ["A\u00b7ber", "stikkst", "du", "denn", "nur", "Blu\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kanst du nicht mit goldnen Faden", "tokens": ["Kanst", "du", "nicht", "mit", "gold\u00b7nen", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Knaben oder M\u00e4dchen stikken?", "tokens": ["Kna\u00b7ben", "o\u00b7der", "M\u00e4d\u00b7chen", "stik\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wag\u2019 es nur, du wirst es k\u00f6nnen.", "tokens": ["Wag'", "es", "nur", ",", "du", "wirst", "es", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber erstlich stikke Knaben.", "tokens": ["A\u00b7ber", "erst\u00b7lich", "stik\u00b7ke", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stikke solche, wie ich male,", "tokens": ["Stik\u00b7ke", "sol\u00b7che", ",", "wie", "ich", "ma\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PWAV", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ohne Perlen, ohne Purpur,", "tokens": ["Oh\u00b7ne", "Per\u00b7len", ",", "oh\u00b7ne", "Pur\u00b7pur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie sie sich im Gr\u00fcnen iagen,", "tokens": ["Wie", "sie", "sich", "im", "Gr\u00fc\u00b7nen", "i\u00b7a\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.11": {"text": "Oder wie sie sich das Hemde", "tokens": ["O\u00b7der", "wie", "sie", "sich", "das", "Hem\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Vor den Augen bl\u00f6der Nimfen", "tokens": ["Vor", "den", "Au\u00b7gen", "bl\u00f6\u00b7der", "Nim\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Vorwerts auf die Knie halten.", "tokens": ["Vor\u00b7werts", "auf", "die", "Knie", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.14": {"text": "Sieh\u2019 sie selbst, hier sind im Buche", "tokens": ["Sieh'", "sie", "selbst", ",", "hier", "sind", "im", "Bu\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADV", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Zwanzig Knaben abgeschildert,", "tokens": ["Zwan\u00b7zig", "Kna\u00b7ben", "ab\u00b7ge\u00b7schil\u00b7dert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "W\u00e4hle dir den besten Knaben,", "tokens": ["W\u00e4h\u00b7le", "dir", "den", "bes\u00b7ten", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Nimm den Losen, der so l\u00e4chelt,", "tokens": ["Nimm", "den", "Lo\u00b7sen", ",", "der", "so", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Oder ienen, mit dem Bogen,", "tokens": ["O\u00b7der", "ie\u00b7nen", ",", "mit", "dem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Der dich mit dem Pfeile drohet,", "tokens": ["Der", "dich", "mit", "dem", "Pfei\u00b7le", "dro\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nimm sie nicht, hier sind noch andre,", "tokens": ["Nimm", "sie", "nicht", ",", "hier", "sind", "noch", "and\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sieh sie an, und w\u00e4hle selber,", "tokens": ["Sieh", "sie", "an", ",", "und", "w\u00e4h\u00b7le", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "KON", "VVIMP", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Ich will sehn, wie gut du w\u00e4hlest.", "tokens": ["Ich", "will", "sehn", ",", "wie", "gut", "du", "w\u00e4h\u00b7lest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Diesen Knaben wilst du stikken?", "tokens": ["Die\u00b7sen", "Kna\u00b7ben", "wilst", "du", "stik\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Diesen, der nach K\u00fcssen schmachtet?", "tokens": ["Die\u00b7sen", ",", "der", "nach", "K\u00fcs\u00b7sen", "schmach\u00b7tet", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Der halbnakkend sich nicht sch\u00e4met?", "tokens": ["Der", "halb\u00b7nak\u00b7kend", "sich", "nicht", "sch\u00e4\u00b7met", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Doris! dieses bin ich selber.", "tokens": ["Do\u00b7ris", "!", "die\u00b7ses", "bin", "ich", "sel\u00b7ber", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Hat mein Pinsel mich getroffen?", "tokens": ["Hat", "mein", "Pin\u00b7sel", "mich", "ge\u00b7trof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Kennst du mich an diesen Z\u00fcgen?", "tokens": ["Kennst", "du", "mich", "an", "die\u00b7sen", "Z\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Gut! du solst mich selber stikken.", "tokens": ["Gut", "!", "du", "solst", "mich", "sel\u00b7ber", "stik\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Aber erst must du mich schildern.", "tokens": ["A\u00b7ber", "erst", "must", "du", "mich", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "H\u00f6re nur, wir wollen tauschen.", "tokens": ["H\u00f6\u00b7re", "nur", ",", "wir", "wol\u00b7len", "tau\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Ich will stikken, du solst malen.", "tokens": ["Ich", "will", "stik\u00b7ken", ",", "du", "solst", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Hurtig gib mir Gold und Nadel,", "tokens": ["Hur\u00b7tig", "gib", "mir", "Gold", "und", "Na\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIMP", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Diese Rose will ich enden;", "tokens": ["Die\u00b7se", "Ro\u00b7se", "will", "ich", "en\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Denn sie wird in blauer Seide", "tokens": ["Denn", "sie", "wird", "in", "blau\u00b7er", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Einst auf deinem Busen bl\u00fchen.", "tokens": ["Einst", "auf", "dei\u00b7nem", "Bu\u00b7sen", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Unterde\u00df kanst du mich malen,", "tokens": ["Un\u00b7ter\u00b7de\u00df", "kanst", "du", "mich", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.38": {"text": "Und sobald du mich gemalet,", "tokens": ["Und", "so\u00b7bald", "du", "mich", "ge\u00b7ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Solst du das Gem\u00e4lde stikken.", "tokens": ["Solst", "du", "das", "Ge\u00b7m\u00e4l\u00b7de", "stik\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Da! hier hast du meine Farben!", "tokens": ["Da", "!", "hier", "hast", "du", "mei\u00b7ne", "Far\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}