{"textgrid.poem.50926": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Zeit geht nicht, sie stehet still,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit geht nicht, sie stehet still,", "tokens": ["Die", "Zeit", "geht", "nicht", ",", "sie", "ste\u00b7het", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist ein Karawanserei,", "tokens": ["Sie", "ist", "ein", "Ka\u00b7ra\u00b7wan\u00b7se\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind die Pilger drin.", "tokens": ["Wir", "sind", "die", "Pil\u00b7ger", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Etwas, form- und farbenlos,", "tokens": ["Ein", "Et\u00b7was", ",", "for\u00b7m", "und", "far\u00b7ben\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das nur Gestalt gewinnt,", "tokens": ["Das", "nur", "Ge\u00b7stalt", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ihr drin auf und nieder taucht,", "tokens": ["Wo", "ihr", "drin", "auf", "und", "nie\u00b7der", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis wieder ihr zerrinnt.", "tokens": ["Bis", "wie\u00b7der", "ihr", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es blitzt ein Tropfen Morgentau", "tokens": ["Es", "blitzt", "ein", "Trop\u00b7fen", "Mor\u00b7gen\u00b7tau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Strahl des Sonnenlichts \u2013", "tokens": ["Im", "Strahl", "des", "Son\u00b7nen\u00b7lichts", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hundert Jahre \u2013 nichts!", "tokens": ["Und", "hun\u00b7dert", "Jah\u00b7re", "\u2013", "nichts", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "CARD", "NN", "$(", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist ein wei\u00dfes Pergament", "tokens": ["Es", "ist", "ein", "wei\u00b7\u00dfes", "Per\u00b7ga\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit, und jeder schreibt", "tokens": ["Die", "Zeit", ",", "und", "je\u00b7der", "schreibt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinem besten Blut darauf,", "tokens": ["Mit", "sei\u00b7nem", "bes\u00b7ten", "Blut", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ihn der Strom vertreibt.", "tokens": ["Bis", "ihn", "der", "Strom", "ver\u00b7treibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "An dich, du wunderbare Welt,", "tokens": ["An", "dich", ",", "du", "wun\u00b7der\u00b7ba\u00b7re", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Sch\u00f6nheit ohne End,", "tokens": ["Du", "Sch\u00f6n\u00b7heit", "oh\u00b7ne", "End", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schreib ich 'nen kurzen Liebesbrief", "tokens": ["Schreib", "ich", "'nen", "kur\u00b7zen", "Lie\u00b7bes\u00b7brief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dieses Pergament,", "tokens": ["Auf", "die\u00b7ses", "Per\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Froh bin ich, da\u00df ich aufgetaucht", "tokens": ["Froh", "bin", "ich", ",", "da\u00df", "ich", "auf\u00b7ge\u00b7taucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deinem runden Kranz;", "tokens": ["In", "dei\u00b