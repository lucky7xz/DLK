word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Ahnungsvollen, meine Bahn nicht,", "tokens": ["Die", "Ah\u00b7nungs\u00b7vol\u00b7len", ",", "mei\u00b7ne", "Bahn", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber gewisser ins Meer hin eilen.", "tokens": ["A\u00b7ber", "ge\u00b7wis\u00b7ser", "ins", "Meer", "hin", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "Denn selbstvergessen, allzubereit, den Wunsch", "tokens": ["Denn", "selbst\u00b7ver\u00b7ges\u00b7sen", ",", "all\u00b7zu\u00b7be\u00b7reit", ",", "den", "Wunsch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "$,", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der G\u00f6tter zu erf\u00fcllen, ergreift zu gern,", "tokens": ["Der", "G\u00f6t\u00b7ter", "zu", "er\u00b7f\u00fcl\u00b7len", ",", "er\u00b7greift", "zu", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was sterblich ist und einmal offnen", "tokens": ["Was", "sterb\u00b7lich", "ist", "und", "ein\u00b7mal", "off\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auges auf eigenem Pfade wandelt,", "tokens": ["Au\u00b7ges", "auf", "ei\u00b7ge\u00b7nem", "Pfa\u00b7de", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Ins All zur\u00fcck die k\u00fcrzeste Bahn, so st\u00fcrzt", "tokens": ["Ins", "All", "zu\u00b7r\u00fcck", "die", "k\u00fcr\u00b7zes\u00b7te", "Bahn", ",", "so", "st\u00fcrzt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Strom hinab, er suchet die Ruh, es rei\u00dft,", "tokens": ["Der", "Strom", "hin\u00b7ab", ",", "er", "su\u00b7chet", "die", "Ruh", ",", "es", "rei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es ziehet wider Willen ihn von", "tokens": ["Es", "zie\u00b7het", "wi\u00b7der", "Wil\u00b7len", "ihn", "von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Klippe zu Klippe, den Steuerlosen,", "tokens": ["Klip\u00b7pe", "zu", "Klip\u00b7pe", ",", "den", "Steu\u00b7er\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Das wunderbare Sehnen dem Abgrund zu,", "tokens": ["Das", "wun\u00b7der\u00b7ba\u00b7re", "Seh\u00b7nen", "dem", "Ab\u00b7grund", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und kaum der Erd entstiegen, desselben Tags", "tokens": ["Und", "kaum", "der", "Erd", "ent\u00b7stie\u00b7gen", ",", "des\u00b7sel\u00b7ben", "Tags"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,", "PRELAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kehrt weinend zum Geburtort schon aus", "tokens": ["Kehrt", "wei\u00b7nend", "zum", "Ge\u00b7bur\u00b7tort", "schon", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPRART", "NN", "ADV", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Purpurner H\u00f6he die Wolke wieder.", "tokens": ["Pur\u00b7pur\u00b7ner", "H\u00f6\u00b7he", "die", "Wol\u00b7ke", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Und V\u00f6lker auch ergreifet die Todeslust,", "tokens": ["Und", "V\u00f6l\u00b7ker", "auch", "er\u00b7grei\u00b7fet", "die", "To\u00b7des\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Heldenst\u00e4dte sinken; die Erde gr\u00fcnt", "tokens": ["Und", "Hel\u00b7den\u00b7st\u00e4d\u00b7te", "sin\u00b7ken", ";", "die", "Er\u00b7de", "gr\u00fcnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und stille vor den Sternen liegt, den", "tokens": ["Und", "stil\u00b7le", "vor", "den", "Ster\u00b7nen", "liegt", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Betenden gleich, in den Staub geworfen,", "tokens": ["Be\u00b7ten\u00b7den", "gleich", ",", "in", "den", "Staub", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "Freiwillig \u00fcberwunden die lange Kunst", "tokens": ["Frei\u00b7wil\u00b7lig", "\u00fc\u00b7berw\u00b7un\u00b7den", "die", "lan\u00b7ge", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor jenen Unnachahmbaren da; er selbst,", "tokens": ["Vor", "je\u00b7nen", "Un\u00b7nac\u00b7hahm\u00b7ba\u00b7ren", "da", ";", "er", "selbst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$.", "PPER", "ADV", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der Mensch, mit eigner Hand zerbrach, die", "tokens": ["Der", "Mensch", ",", "mit", "eig\u00b7ner", "Hand", "zer\u00b7brach", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hohen zu ehren, sein Werk, der K\u00fcnstler.", "tokens": ["Ho\u00b7hen", "zu", "eh\u00b7ren", ",", "sein", "Werk", ",", "der", "K\u00fcnst\u00b7ler", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.21": {"line.1": {"text": "Doch minder nicht sind jene den Menschen hold,", "tokens": ["Doch", "min\u00b7der", "nicht", "sind", "je\u00b7ne", "den", "Men\u00b7schen", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VAFIN", "PDS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie lieben wieder, so, wie geliebt sie sind,", "tokens": ["Sie", "lie\u00b7ben", "wie\u00b7der", ",", "so", ",", "wie", "ge\u00b7liebt", "sie", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "$,", "PWAV", "VVPP", "PPER", "VAFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und hemmen \u00f6fters, da\u00df er lang im", "tokens": ["Und", "hem\u00b7men", "\u00f6f\u00b7ters", ",", "da\u00df", "er", "lang", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lichte sich freue, die Bahn des Menschen.", "tokens": ["Lich\u00b7te", "sich", "freu\u00b7e", ",", "die", "Bahn", "des", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.22": {"line.1": {"text": "Und wie des Adlers Jungen, er wirft sie selbst,", "tokens": ["Und", "wie", "des", "Ad\u00b7lers", "Jun\u00b7gen", ",", "er", "wirft", "sie", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Vater, aus dem Neste, damit sie sich", "tokens": ["Der", "Va\u00b7ter", ",", "aus", "dem", "Nes\u00b7te", ",", "da\u00b7mit", "sie", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Felde Beute suchen, so auch", "tokens": ["Im", "Fel\u00b7de", "Beu\u00b7te", "su\u00b7chen", ",", "so", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Treiben uns l\u00e4chelnd hinaus die G\u00f6tter.", "tokens": ["Trei\u00b7ben", "uns", "l\u00e4\u00b7chelnd", "hin\u00b7aus", "die", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.23": {"line.1": {"text": "Wohl allen, die zur Ruhe gegangen sind", "tokens": ["Wohl", "al\u00b7len", ",", "die", "zur", "Ru\u00b7he", "ge\u00b7gan\u00b7gen", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "PRELS", "APPRART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und vor der Zeit gefallen, auch sie, auch sie", "tokens": ["Und", "vor", "der", "Zeit", "ge\u00b7fal\u00b7len", ",", "auch", "sie", ",", "auch", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,", "ADV", "PPER", "$,", "ADV", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Geopfert gleich den Erstlingen der", "tokens": ["Ge\u00b7o\u00b7pfert", "gleich", "den", "Erst\u00b7lin\u00b7gen", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ernte, sie haben ihr Teil gewonnen!", "tokens": ["Ern\u00b7te", ",", "sie", "ha\u00b7ben", "ihr", "Teil", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.24": {"line.1": {"text": "Nicht, o ihr Teuern, ohne die Wonnen all", "tokens": ["Nicht", ",", "o", "ihr", "Teu\u00b7ern", ",", "oh\u00b7ne", "die", "Won\u00b7nen", "all"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "FM", "PPOSAT", "NN", "$,", "KOUI", "ART", "NN", "PIAT"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Lebens gingt ihr unter, ein Festtag ward", "tokens": ["Des", "Le\u00b7bens", "gingt", "ihr", "un\u00b7ter", ",", "ein", "Fest\u00b7tag", "ward"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch Einer euch zuvor, und dem gleich", "tokens": ["Noch", "Ei\u00b7ner", "euch", "zu\u00b7vor", ",", "und", "dem", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "PPER", "ADV", "$,", "KON", "ART", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Haben die anderen keins gefunden.", "tokens": ["Ha\u00b7ben", "die", "an\u00b7de\u00b7ren", "keins", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.25": {"line.1": {"text": "Doch sichrer ists und gr\u00f6\u00dfer und ihrer mehr,", "tokens": ["Doch", "sich\u00b7rer", "ists", "und", "gr\u00f6\u00b7\u00dfer", "und", "ih\u00b7rer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KON", "ADJD", "KON", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die allen alles ist, der Mutter wert,", "tokens": ["Die", "al\u00b7len", "al\u00b7les", "ist", ",", "der", "Mut\u00b7ter", "wert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VAFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Eile z\u00f6gernd, mit des Adlers", "tokens": ["In", "Ei\u00b7le", "z\u00f6\u00b7gernd", ",", "mit", "des", "Ad\u00b7lers"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lust die geschwungnere Bahn zu wandeln.", "tokens": ["Lust", "die", "ge\u00b7schwung\u00b7ne\u00b7re", "Bahn", "zu", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKZU", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.26": {"line.1": {"text": "Drum weil sie fromm ist, ehr ich den Himmlischen", "tokens": ["Drum", "weil", "sie", "fromm", "ist", ",", "ehr", "ich", "den", "Himm\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu lieb des Volkes Stimme, die ruhige,", "tokens": ["Zu", "lieb", "des", "Vol\u00b7kes", "Stim\u00b7me", ",", "die", "ru\u00b7hi\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch um der G\u00f6tter und der Menschen", "tokens": ["Doch", "um", "der", "G\u00f6t\u00b7ter", "und", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Willen, sie ruhe zu gern nicht immer!", "tokens": ["Wil\u00b7len", ",", "sie", "ru\u00b7he", "zu", "gern", "nicht", "im\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "APPR", "ADV", "PTKNEG", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}