{"dta.poem.9343": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Als die Buchdruckerey-Verwanten jhr  \n Jubelfest  Anno 1640.  am Johannsta-  \n ge begiengen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es seyn zwar wohl die Alten hochgestiegen;", "tokens": ["Es", "seyn", "zwar", "wohl", "die", "Al\u00b7ten", "hoch\u00b7ges\u00b7tie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein h\u00f6ltzern bild gab Dedalus herf\u00fcr/", "tokens": ["Ein", "h\u00f6lt\u00b7zern", "bild", "gab", "De\u00b7da\u00b7lus", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das selbsten gieng lebhafftig wie ein Thier.", "tokens": ["das", "selbs\u00b7ten", "gieng", "leb\u00b7haff\u00b7tig", "wie", "ein", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Architas holtz kont\u2019 in den L\u00fcfften fl\u00fcgen", "tokens": ["Ar\u00b7chi\u00b7tas", "holtz", "kont'", "in", "den", "L\u00fcff\u00b7ten", "fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "nach tauben-art; Homerus helffte liegen", "tokens": ["nach", "tau\u00b7ben\u00b7art", ";", "Ho\u00b7me\u00b7rus", "helff\u00b7te", "lie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$.", "NE", "ADJA", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "in einer Nu\u00df geschrieben auff papier;", "tokens": ["in", "ei\u00b7ner", "Nu\u00df", "ge\u00b7schrie\u00b7ben", "auff", "pa\u00b7pier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Himmels-lauff im glase mit manier", "tokens": ["Des", "Him\u00b7mels\u00b7lauff", "im", "gla\u00b7se", "mit", "ma\u00b7nier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zeigt Archimed: Doch war damals im kriegen", "tokens": ["zeigt", "Ar\u00b7chi\u00b7med", ":", "Doch", "war", "da\u00b7mals", "im", "krie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$.", "KON", "VAFIN", "ADV", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "kein Jupiter/ der auf der Erden blitzt/", "tokens": ["kein", "Ju\u00b7pi\u00b7ter", "/", "der", "auf", "der", "Er\u00b7den", "blitzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "kein Alchimist/ kein M\u00fcnch der Pulver n\u00fctzt;", "tokens": ["kein", "Al\u00b7chi\u00b7mist", "/", "kein", "M\u00fcnch", "der", "Pul\u00b7ver", "n\u00fctzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Gallen ward auch Tanto nicht versp\u00fcret/", "tokens": ["Zu", "Gal\u00b7len", "ward", "auch", "Tan\u00b7to", "nicht", "ver\u00b7sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "NE", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "der uns gelehrt den werthen glockengu\u00df;", "tokens": ["der", "uns", "ge\u00b7lehrt", "den", "wert\u00b7hen", "glo\u00b7cken\u00b7gu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Druckerey/ so jtzund trotzen mu\u00df", "tokens": ["Die", "Dru\u00b7cke\u00b7rey", "/", "so", "jt\u00b7zund", "trot\u00b7zen", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Athen und Rom/ ward damals nicht gef\u00fchret.", "tokens": ["A\u00b7then", "und", "Rom", "/", "ward", "da\u00b7mals", "nicht", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$(", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}