{"dta.poem.20984": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII.  \n Eines Gesandten.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein K\u00f6nig sandte mich in ein ber\u00fchmtes Land/", "tokens": ["Mein", "K\u00f6\u00b7nig", "sand\u00b7te", "mich", "in", "ein", "be\u00b7r\u00fchm\u00b7tes", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich hoffte frey zu seyn/ von aller V\u00f6lcker Hand.", "tokens": ["Ich", "hoff\u00b7te", "frey", "zu", "seyn", "/", "von", "al\u00b7ler", "V\u00f6l\u00b7cker", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VAINF", "$(", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Hoffen war umbsonst/ der Tod hat mich er-", "tokens": ["Mein", "Hof\u00b7fen", "war", "um\u00b7bsonst", "/", "der", "Tod", "hat", "mich", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "VAFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schlagen/", "tokens": ["schla\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der Flegel wuste nicht/ was die Juristen sagen.", "tokens": ["Der", "Fle\u00b7gel", "wus\u00b7te", "nicht", "/", "was", "die", "Ju\u00b7ris\u00b7ten", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "PWS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}