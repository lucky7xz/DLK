{"textgrid.poem.35503": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht mit Trauben, nicht mit Rosen", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mit Trauben, nicht mit Rosen", "tokens": ["Nicht", "mit", "Trau\u00b7ben", ",", "nicht", "mit", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward die Laute mir umwunden \u2013", "tokens": ["Ward", "die", "Lau\u00b7te", "mir", "um\u00b7wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zum Reigen, nicht zum Kosen", "tokens": ["Nicht", "zum", "Rei\u00b7gen", ",", "nicht", "zum", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich Ton und Wort gefunden.", "tokens": ["Hab'", "ich", "Ton", "und", "Wort", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was die Seele mir erf\u00fcllt hat,", "tokens": ["Was", "die", "See\u00b7le", "mir", "er\u00b7f\u00fcllt", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klang mir aus Gewitterpsalmen,", "tokens": ["Klang", "mir", "aus", "Ge\u00b7wit\u00b7ter\u00b7psal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mein brennend Weh zermalmen", "tokens": ["Und", "mein", "bren\u00b7nend", "Weh", "zer\u00b7mal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Konnte nur, was sich enth\u00fcllt hat ...", "tokens": ["Konn\u00b7te", "nur", ",", "was", "sich", "ent\u00b7h\u00fcllt", "hat", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "PRELS", "PRF", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht mit Scherzen, federleichten,", "tokens": ["Nicht", "mit", "Scher\u00b7zen", ",", "fe\u00b7der\u00b7leich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich mir die Welt versch\u00e4ndet \u2013", "tokens": ["Hab'", "ich", "mir", "die", "Welt", "ver\u00b7sch\u00e4n\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu den Armen, Gramgebleichten,", "tokens": ["Zu", "den", "Ar\u00b7men", ",", "Gram\u00b7ge\u00b7bleich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War mein Sinnen hingewendet.", "tokens": ["War", "mein", "Sin\u00b7nen", "hin\u00b7ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich sah in ihre Augen,", "tokens": ["Und", "ich", "sah", "in", "ih\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zorn und Trauer tief im Herzen, \u2013", "tokens": ["Zorn", "und", "Trau\u00b7er", "tief", "im", "Her\u00b7zen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "APPRART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich las von ew'gen Schmerzen,", "tokens": ["Und", "ich", "las", "von", "ew'\u00b7gen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die zu lust'gem Spiel nicht taugen ...", "tokens": ["Die", "zu", "lust'\u00b7gem", "Spiel", "nicht", "tau\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "ADJA", "NN", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich sa\u00df an ihren Lagern:", "tokens": ["Und", "ich", "sa\u00df", "an", "ih\u00b7ren", "La\u00b7gern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klappernd w\u00e4lzten sich zur Seite", "tokens": ["Klap\u00b7pernd", "w\u00e4lz\u00b7ten", "sich", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tod und Schande \u2013 mit den magern", "tokens": ["Tod", "und", "Schan\u00b7de", "\u2013", "mit", "den", "ma\u00b7gern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4usten ringend um die Beute!", "tokens": ["F\u00e4us\u00b7ten", "rin\u00b7gend", "um", "die", "Beu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die Beute: D\u00fcrre Glieder \u2013", "tokens": ["Um", "die", "Beu\u00b7te", ":", "D\u00fcr\u00b7re", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$.", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcrbe, notzerfre\u00dfne Knochen \u2013", "tokens": ["M\u00fcr\u00b7be", ",", "not\u00b7zer\u00b7fre\u00df\u00b7ne", "Kno\u00b7chen", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da hat es mich j\u00e4h durchstochen:", "tokens": ["Da", "hat", "es", "mich", "j\u00e4h", "durch\u00b7sto\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Und ich sah ein fruchtlos M\u00fchen", "tokens": ["Und", "ich", "sah", "ein", "frucht\u00b7los", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Besten jede Stunde \u2013", "tokens": ["Al\u00b7ler", "Bes\u00b7ten", "je\u00b7de", "Stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hellster Flammen bleich Vergl\u00fchen \u2013", "tokens": ["Hells\u00b7ter", "Flam\u00b7men", "bleich", "Ver\u00b7gl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am trotzigsten die Wunde,", "tokens": ["Und", "am", "trot\u00b7zigs\u00b7ten", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die ein gro\u00dfes Streben schl\u00e4gt,", "tokens": ["Die", "ein", "gro\u00b7\u00dfes", "Stre\u00b7ben", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das sich bricht in engen Schranken:", "tokens": ["Das", "sich", "bricht", "in", "en\u00b7gen", "Schran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich begriff es tiefbewegt ...", "tokens": ["Ich", "be\u00b7griff", "es", "tief\u00b7be\u00b7wegt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur das