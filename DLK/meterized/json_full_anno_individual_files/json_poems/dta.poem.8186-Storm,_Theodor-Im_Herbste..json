{"dta.poem.8186": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Am Himmel steht ein falber Schein;", "tokens": ["Am", "Him\u00b7mel", "steht", "ein", "fal\u00b7ber", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du schauerst leis, und dr\u00fcckst dich fester", "tokens": ["Du", "schau\u00b7erst", "leis", ",", "und", "dr\u00fcckst", "dich", "fes\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In deines Mannes Arm hinein.", "tokens": ["In", "dei\u00b7nes", "Man\u00b7nes", "Arm", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was nun von Halm zu Halme wandelt,", "tokens": ["Was", "nun", "von", "Halm", "zu", "Hal\u00b7me", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was nach den letzten Blumen greift,", "tokens": ["Was", "nach", "den", "letz\u00b7ten", "Blu\u00b7men", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat heimlich im Vor\u00fcbergehen", "tokens": ["Hat", "heim\u00b7lich", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch dein geliebtes Haupt gestreift.", "tokens": ["Auch", "dein", "ge\u00b7lieb\u00b7tes", "Haupt", "ge\u00b7streift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch rei\u00dfen auch die zarten F\u00e4den,", "tokens": ["Doch", "rei\u00b7\u00dfen", "auch", "die", "zar\u00b7ten", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die warme Nacht auf Wiesen spann \u2014", "tokens": ["Die", "war\u00b7me", "Nacht", "auf", "Wie\u00b7sen", "spann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist der Sommer nur, der scheidet;", "tokens": ["Es", "ist", "der", "Som\u00b7mer", "nur", ",", "der", "schei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was geht denn uns der Sommer an!", "tokens": ["Was", "geht", "denn", "uns", "der", "Som\u00b7mer", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du legst die Hand an meine Stirne,", "tokens": ["Du", "legst", "die", "Hand", "an", "mei\u00b7ne", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaust mir pr\u00fcfend in's Gesicht;", "tokens": ["Und", "schaust", "mir", "pr\u00fc\u00b7fend", "in's", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus deinen milden Frauenaugen", "tokens": ["Aus", "dei\u00b7nen", "mil\u00b7den", "Frau\u00b7en\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bricht gar zu melancholisch Licht.", "tokens": ["Bricht", "gar", "zu", "me\u00b7lan\u00b7cho\u00b7lisch", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADJD", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Erlosch auch hier ein Duft, ein Schimmer,", "tokens": ["Er\u00b7losch", "auch", "hier", "ein", "Duft", ",", "ein", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein R\u00e4thsel, das dich einst bewegt,", "tokens": ["Ein", "R\u00e4th\u00b7sel", ",", "das", "dich", "einst", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du in meine Hand gefangen", "tokens": ["Da\u00df", "du", "in", "mei\u00b7ne", "Hand", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die freie M\u00e4dchenhand gelegt?", "tokens": ["Die", "frei\u00b7e", "M\u00e4d\u00b7chen\u00b7hand", "ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O schaudre nicht! Ob auch unmerklich", "tokens": ["O", "schaud\u00b7re", "nicht", "!", "Ob", "auch", "un\u00b7mer\u00b7klich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "KOUS", "ADV", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der sch\u00f6nste Sonnenschein verrann \u2014", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Son\u00b7nen\u00b7schein", "ver\u00b7rann"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist der Sommer nur, der scheidet;", "tokens": ["Es", "ist", "der", "Som\u00b7mer", "nur", ",", "der", "schei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was geht denn uns der Sommer an!", "tokens": ["Was", "geht", "denn", "uns", "der", "Som\u00b7mer", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}