{"dta.poem.10529": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht liegt auf den fremden Wegen, \u2014", "tokens": ["Nacht", "liegt", "auf", "den", "frem\u00b7den", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krankes Herz und m\u00fcde Glieder; \u2014", "tokens": ["Kran\u00b7kes", "Herz", "und", "m\u00fc\u00b7de", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, da flie\u00dft, wie stiller Segen,", "tokens": ["Ach", ",", "da", "flie\u00dft", ",", "wie", "stil\u00b7ler", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfer Mond, dein Licht hernieder.", "tokens": ["S\u00fc\u00b7\u00dfer", "Mond", ",", "dein", "Licht", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00dfer Mond, mit deinen Strahlen", "tokens": ["S\u00fc\u00b7\u00dfer", "Mond", ",", "mit", "dei\u00b7nen", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheuchest du das n\u00e4cht'ge Grauen;", "tokens": ["Scheu\u00b7chest", "du", "das", "n\u00e4cht'\u00b7ge", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es zerrinnen meine Qualen,", "tokens": ["Es", "zer\u00b7rin\u00b7nen", "mei\u00b7ne", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Augen \u00fcberthauen.", "tokens": ["Und", "die", "Au\u00b7gen", "\u00fc\u00b7bert\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}