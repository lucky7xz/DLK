{"dta.poem.23596": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein sch\u00f6ner Zug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr nicht frei euch f\u00fchlt zu Haus,", "tokens": ["Wenn", "ihr", "nicht", "frei", "euch", "f\u00fchlt", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlan, so ziehet gleich hinaus!", "tokens": ["Wo\u00b7hlan", ",", "so", "zie\u00b7het", "gleich", "hin\u00b7aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frei k\u00f6nnt ihr ziehn aus allen deutschen Landen,", "tokens": ["Frei", "k\u00f6nnt", "ihr", "ziehn", "aus", "al\u00b7len", "deut\u00b7schen", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Freiz\u00fcgigkeit ist auch f\u00fcr euch vorhanden.", "tokens": ["Frei\u00b7z\u00fc\u00b7gig\u00b7keit", "ist", "auch", "f\u00fcr", "euch", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein sch\u00f6ner Zug von unsrer Zeit!", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Zug", "von", "uns\u00b7rer", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sch\u00f6ner Zug: Freiz\u00fcgigkeit!", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Zug", ":", "Frei\u00b7z\u00fc\u00b7gig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir fehlt ein n an deines Gl\u00fcckes Sterne:", "tokens": ["Dir", "fehlt", "ein", "n", "an", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "XY", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Freiz\u00fcgig Volk, freiz\u00fcngig w\u00e4rst du gerne!", "tokens": ["Frei\u00b7z\u00fc\u00b7gig", "Volk", ",", "frei\u00b7z\u00fcn\u00b7gig", "w\u00e4rst", "du", "ger\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}