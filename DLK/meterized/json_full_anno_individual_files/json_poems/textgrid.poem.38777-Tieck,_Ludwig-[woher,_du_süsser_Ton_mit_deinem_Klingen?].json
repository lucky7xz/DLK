{"textgrid.poem.38777": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[woher, du s\u00fcsser Ton mit deinem Klingen?]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013 \u00bbWoher, du s\u00fcsser Ton mit deinem Klingen?", "tokens": ["\u2013", "\u00bb", "Wo\u00b7her", ",", "du", "s\u00fcs\u00b7ser", "Ton", "mit", "dei\u00b7nem", "Klin\u00b7gen", "?"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PWAV", "$,", "PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der wie ein Zauber blitzend in mich schl\u00e4get,", "tokens": ["Der", "wie", "ein", "Zau\u00b7ber", "blit\u00b7zend", "in", "mich", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df furchtsam sich das Herz nur zitternd reget,", "tokens": ["Da\u00df", "furcht\u00b7sam", "sich", "das", "Herz", "nur", "zit\u00b7ternd", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor Sehnsucht, Lust vom Auge Thr\u00e4nen entrinnen?\u00ab \u2013", "tokens": ["Vor", "Sehn\u00b7sucht", ",", "Lust", "vom", "Au\u00b7ge", "Thr\u00e4\u00b7nen", "ent\u00b7rin\u00b7nen", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPRART", "NN", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u2013 Siehst du denn nicht, wie dieses holde Singen", "tokens": ["\u2013", "Siehst", "du", "denn", "nicht", ",", "wie", "die\u00b7ses", "hol\u00b7de", "Sin\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich von dem Glanz der Lippen herbeweget,", "tokens": ["Sich", "von", "dem", "Glanz", "der", "Lip\u00b7pen", "her\u00b7be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Mund der rothen Liebreiz in sich heget?", "tokens": ["Vom", "Mund", "der", "ro\u00b7then", "Lieb\u00b7reiz", "in", "sich", "he\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den s\u00fcssen Flammen mu\u00df sich Feu'r entschwingen. \u2013", "tokens": ["Den", "s\u00fcs\u00b7sen", "Flam\u00b7men", "mu\u00df", "sich", "Feu'r", "ent\u00b7schwin\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u2013 \u00bbJa, peinigend versehrst du, doch die Flammen", "tokens": ["\u2013", "\u00bb", "Ja", ",", "pei\u00b7ni\u00b7gend", "ver\u00b7sehrst", "du", ",", "doch", "die", "Flam\u00b7men"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "$(", "PTKANT", "$,", "ADJD", "VVFIN", "PPER", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tr\u00f6sten im Brande, Tod wird zum Entz\u00fccken,", "tokens": ["Tr\u00f6s\u00b7ten", "im", "Bran\u00b7de", ",", "Tod", "wird", "zum", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "In diesen Gluthen f\u00fchlt mein Herz sein Leben,\u00ab \u2013", "tokens": ["In", "die\u00b7sen", "Glu\u00b7then", "f\u00fchlt", "mein", "Herz", "sein", "Le\u00b7ben", ",", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u2013 \u00bbWeil diese T\u00f6n' dem Geiste selbst entschweben;", "tokens": ["\u2013", "\u00bb", "Weil", "die\u00b7se", "T\u00f6n'", "dem", "Geis\u00b7te", "selbst", "ent\u00b7schwe\u00b7ben", ";"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KOUS", "PDAT", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ist ein Liebesothem, Trost, Erquicken:", "tokens": ["Der", "ist", "ein", "Lie\u00b7be\u00b7sot\u00b7hem", ",", "Trost", ",", "Er\u00b7qui\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Liebe nur kann Ton der Liebe stammen!\u00ab \u2013", "tokens": ["Aus", "Lie\u00b