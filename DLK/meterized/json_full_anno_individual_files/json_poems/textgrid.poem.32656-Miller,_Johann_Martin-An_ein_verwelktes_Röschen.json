{"textgrid.poem.32656": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An ein verwelktes R\u00f6schen", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6schen, wie beneid' ich dich!", "tokens": ["R\u00f6\u00b7schen", ",", "wie", "be\u00b7neid'", "ich", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fccklich warest du vor allen;", "tokens": ["Gl\u00fcck\u00b7lich", "wa\u00b7rest", "du", "vor", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Daphnen hattest du gefallen,", "tokens": ["Daph\u00b7nen", "hat\u00b7test", "du", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Daphne sah und w\u00e4hlte dich!", "tokens": ["Daph\u00b7ne", "sah", "und", "w\u00e4hl\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Allen J\u00fcnglingen zur Lust,", "tokens": ["Al\u00b7len", "J\u00fcng\u00b7lin\u00b7gen", "zur", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bl\u00fchtest du an ihrer Brust;", "tokens": ["Bl\u00fch\u00b7test", "du", "an", "ih\u00b7rer", "Brust", ";"], "token_info": ["word", "word"