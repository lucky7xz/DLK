{"textgrid.poem.35653": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "16. An die Herrn Franzosen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmt die Zither, und r\u00fchmt, mutiges Halls, S\u00f6hne Lutetias,", "tokens": ["Nehmt", "die", "Zi\u00b7ther", ",", "und", "r\u00fchmt", ",", "mu\u00b7ti\u00b7ges", "Halls", ",", "S\u00f6h\u00b7ne", "Lu\u00b7te\u00b7ti\u00b7as", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,", "ADJA", "NN", "$,", "NN", "NE", "$,"], "meter": "+-+--+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Selber, also geb\u00fchrts! euren Gesang durch die erstaunte Welt!", "tokens": ["Sel\u00b7ber", ",", "al\u00b7so", "ge\u00b7b\u00fchrts", "!", "eu\u00b7ren", "Ge\u00b7sang", "durch", "die", "er\u00b7staun\u00b7te", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$.", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Trotzt dem dorischen Schwung, welcher, im \u00d6lschatten, Olympias", "tokens": ["Trotzt", "dem", "do\u00b7ri\u00b7schen", "Schwung", ",", "wel\u00b7cher", ",", "im", "\u00d6l\u00b7schat\u00b7ten", ",", "O\u00b7lym\u00b7pias"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "$,", "APPRART", "NN", "$,", "NE"], "meter": "+-+--+--+++-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Reigen fl\u00fcgelt', und trotzt jenem, der Roms W\u00fctrich gen Himmel ri\u00df!", "tokens": ["Rei\u00b7gen", "fl\u00fc\u00b7gelt'", ",", "und", "trotzt", "je\u00b7nem", ",", "der", "Roms", "W\u00fct\u00b7rich", "gen", "Him\u00b7mel", "ri\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "PDAT", "$,", "ART", "NE", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "+-+---+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Herrlich habt ihr erf\u00fcllt Ludewigs Wunsch! Nicht der lombardischen", "tokens": ["Herr\u00b7lich", "habt", "ihr", "er\u00b7f\u00fcllt", "Lu\u00b7de\u00b7wigs", "Wunsch", "!", "Nicht", "der", "lom\u00b7bar\u00b7di\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "NE", "NN", "$.", "PTKNEG", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Pickelheringe Kunst r\u00fcttelte so, so des Monarchen Wanst", "tokens": ["Pi\u00b7ckel\u00b7he\u00b7rin\u00b7ge", "Kunst", "r\u00fct\u00b7tel\u00b7te", "so", ",", "so", "des", "Mon\u00b7ar\u00b7chen", "Wanst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADV", "$,", "ADV", "ART", "NN", "NN"], "meter": "+--+-++--+---+-+", "measure": "iambic.septa.invert"}, "line.7": {"text": "Nicht der lustigste Schwank Roquelaurs selbst, als, wenn dem Atmenden", "tokens": ["Nicht", "der", "lus\u00b7tigs\u00b7te", "Schwank", "Ro\u00b7que\u00b7laurs", "selbst", ",", "als", ",", "wenn", "dem", "At\u00b7men\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "NE", "ADV", "$,", "KOUS", "$,", "KOUS", "ART", "NN"], "meter": "--+--+-+-+-+-+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "Most und Nymphen ihr s\u00e4ngt! Taumelnder noch tobt und centaurischer", "tokens": ["Most", "und", "Nym\u00b7phen", "ihr", "s\u00e4ngt", "!", "Tau\u00b7meln\u00b7der", "noch", "tobt", "und", "cen\u00b7tau\u00b7ri\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$.", "NN", "ADV", "VVFIN", "KON", "ADJA"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Euch der H\u00f6flinge Tanz! Hei\u00dferes Bluts, birgt es die Schminke gleich,", "tokens": ["Euch", "der", "H\u00f6f\u00b7lin\u00b7ge", "Tanz", "!", "Hei\u00b7\u00dfe\u00b7res", "Bluts", ",", "birgt", "es", "die", "Schmin\u00b7ke", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$.", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+--+--+--+---+-+", "measure": "dactylic.tetra.plus"}, "line.10": {"text": "St\u00fcrzen M\u00e4dchen im Flug tief in die Nacht fl\u00fcsternder Myrten sich!", "tokens": ["St\u00fcr\u00b7zen", "M\u00e4d\u00b7chen", "im", "Flug", "tief", "in", "die", "Nacht", "fl\u00fcs\u00b7tern\u00b7der", "Myr\u00b7ten", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "PRF", "$."], "meter": "+-+--++--++--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Billig schielet ihr Grimm \u00fcber den Rhein in das abscheuliche", "tokens": ["Bil\u00b7lig", "schie\u00b7let", "ihr", "Grimm", "\u00fc\u00b7ber", "den", "Rhein", "in", "das", "ab\u00b7scheu\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NE", "APPR", "ART", "NE", "APPR", "ART", "ADJA"], "meter": "+-+--++--+--+---", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Land, wo H\u00f6chst\u00e4dt vom Mord, Ro\u00dfbach vom Mord feiner Franzosen raucht!", "tokens": ["Land", ",", "wo", "H\u00f6chs\u00b7t\u00e4dt", "vom", "Mord", ",", "Ro\u00df\u00b7bach", "vom", "Mord", "fei\u00b7ner", "Fran\u00b7zo\u00b7sen", "raucht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "APPRART", "NN", "$,", "NN", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+---+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Billig schimpft ihr den witzlosen Gesang, welcher mit rauhem Schall,", "tokens": ["Bil\u00b7lig", "schimpft", "ihr", "den", "witz\u00b7lo\u00b7sen", "Ge\u00b7sang", ",", "wel\u00b7cher", "mit", "rau\u00b7hem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--++--+---+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gott, dies M\u00e4rchen! und ha! Freiheitsgew\u00e4sch t\u00f6net, und Vaterland!", "tokens": ["Gott", ",", "dies", "M\u00e4r\u00b7chen", "!", "und", "ha", "!", "Frei\u00b7heits\u00b7ge\u00b7w\u00e4sch", "t\u00f6\u00b7net", ",", "und", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "NN", "$.", "KON", "NE", "$.", "NN", "VVFIN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Selbst die Ersten des Volks \u2013 selbst der Barbar, dessen geschliffnes Schwert", "tokens": ["Selbst", "die", "Ers\u00b7ten", "des", "Volks", "\u2013", "selbst", "der", "Bar\u00b7bar", ",", "des\u00b7sen", "ge\u00b7schliff\u00b7nes", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$(", "ADV", "ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "+-+--++-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "So unmenschlich euch schlug, schm\u00e4het den Kranz, welchen die Sprache Teuts", "tokens": ["So", "un\u00b7menschlich", "euch", "schlug", ",", "schm\u00e4\u00b7het", "den", "Kranz", ",", "wel\u00b7chen", "die", "Spra\u00b7che", "Teuts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "PWAT", "ART", "NN", "NN"], "meter": "-+---+--+---+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Seinen Siegen umwand, bettelt um den, der an der Marne spro\u00dft!", "tokens": ["Sei\u00b7nen", "Sie\u00b7gen", "um\u00b7wand", ",", "bet\u00b7telt", "um", "den", ",", "der", "an", "der", "Mar\u00b7ne", "spro\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ART", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Holder Amor, den oft, leider! der Ernst meines teutonischen", "tokens": ["Hol\u00b7der", "A\u00b7mor", ",", "den", "oft", ",", "lei\u00b7der", "!", "der", "Ernst", "mei\u00b7nes", "teu\u00b7to\u00b7ni\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PRELS", "ADV", "$,", "ADV", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Lieds scheuchte; (wo nicht jetzt du im Arm deines Poeten schl\u00e4fst,", "tokens": ["Lieds", "scheuch\u00b7te", ";(", "wo", "nicht", "jetzt", "du", "im", "Arm", "dei\u00b7nes", "Po\u00b7et\u00b7en", "schl\u00e4fst", ","], "token_info": ["word", "word", "emoticon", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "PWAV", "PTKNEG", "ADV", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+----+-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Nicht der Schmetterlingsjagd Mattung, im Tau s\u00fc\u00dfer Aurikeln, k\u00fchlst;)", "tokens": ["Nicht", "der", "Schmet\u00b7ter\u00b7lings\u00b7jagd", "Mat\u00b7tung", ",", "im", "Tau", "s\u00fc\u00b7\u00dfer", "Au\u00b7ri\u00b7keln", ",", "k\u00fchlst", ";)"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "emoticon"], "pos": ["PTKNEG", "ART", "NN", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+--++-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Schau der bittersten Reu' Thr\u00e4nen, und gieb, gieb mir des goldenen", "tokens": ["Schau", "der", "bit\u00b7ters\u00b7ten", "Reu'", "Thr\u00e4\u00b7nen", ",", "und", "gieb", ",", "gieb", "mir", "des", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$,", "KON", "VVIMP", "$,", "VVIMP", "PPER", "ART", "ADJA"], "meter": "+-+---+--+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Bogens Saite, da\u00df k\u00fchn folge mein Spiel Galliens Harmonie!", "tokens": ["Bo\u00b7gens", "Sai\u00b7te", ",", "da\u00df", "k\u00fchn", "fol\u00b7ge", "mein", "Spiel", "Gal\u00b7li\u00b7ens", "Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ADJD", "VVFIN", "PPOSAT", "NN", "NE", "NE", "$."], "meter": "+-+--++-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Dann, o Grazien, tanzt, f\u00e4lschlichverh\u00fcllt, nach dem Parisertakt,", "tokens": ["Dann", ",", "o", "Gra\u00b7zi\u00b7en", ",", "tanzt", ",", "f\u00e4lschlich\u00b7ver\u00b7h\u00fcllt", ",", "nach", "dem", "Pa\u00b7ri\u00b7ser\u00b7takt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Tanzt zu meinem Gesang! sch\u00fcttet den Kelch f\u00fcnfmalgel\u00e4uterten", "tokens": ["Tanzt", "zu", "mei\u00b7nem", "Ge\u00b7sang", "!", "sch\u00fct\u00b7tet", "den", "Kelch", "f\u00fcnf\u00b7mal\u00b7ge\u00b7l\u00e4u\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$.", "VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+--++--+-+-+--", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Nektars \u00fcber ihn aus, da\u00df mit Begier sch\u00f6pfe des J\u00fcnglings Brust", "tokens": ["Nek\u00b7tars", "\u00fc\u00b7ber", "ihn", "aus", ",", "da\u00df", "mit", "Be\u00b7gier", "sch\u00f6p\u00b7fe", "des", "J\u00fcng\u00b7lings", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,", "KOUS", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Meinen zaubernden Trank; da\u00df, ob vor Scham gl\u00fchend die Sch\u00f6ne sich", "tokens": ["Mei\u00b7nen", "zau\u00b7bern\u00b7den", "Trank", ";", "da\u00df", ",", "ob", "vor", "Scham", "gl\u00fc\u00b7hend", "die", "Sch\u00f6\u00b7ne", "sich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "KOUS", "$,", "KOUS", "APPR", "NN", "ADJD", "ART", "NN", "PRF"], "meter": "+-+--+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Mit dem F\u00e4cher beschirmt, hinter dem Schirm l\u00fcstern sie l\u00e4chele!", "tokens": ["Mit", "dem", "F\u00e4\u00b7cher", "be\u00b7schirmt", ",", "hin\u00b7ter", "dem", "Schirm", "l\u00fcs\u00b7tern", "sie", "l\u00e4\u00b7che\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "--+---+--++--+--", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "O der Freuden! Auch mir l\u00e4chelt, auch mir, S\u00e4nger Lutetias!", "tokens": ["O", "der", "Freu\u00b7den", "!", "Auch", "mir", "l\u00e4\u00b7chelt", ",", "auch", "mir", ",", "S\u00e4n\u00b7ger", "Lu\u00b7te\u00b7ti\u00b7as", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "ADV", "PPER", "VVFIN", "$,", "ADV", "PPER", "$,", "NN", "NE", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.12": {"text": "Eine Sch\u00f6ne, das Haar gallischget\u00fcrmt, blitzend von Edelstein,", "tokens": ["Ei\u00b7ne", "Sch\u00f6\u00b7ne", ",", "das", "Haar", "gal\u00b7lischge\u00b7t\u00fcrmt", ",", "blit\u00b7zend", "von", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+--+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Und die Wange mit Sch\u00f6nfleckchen bes\u00e4't! Mich! in dem z\u00e4rtli