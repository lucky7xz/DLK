 ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "PRF", "$."], "meter": "+-+--++--++--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Billig schielet ihr Grimm \u00fcber den Rhein in das abscheuliche", "tokens": ["Bil\u00b7lig", "schie\u00b7let", "ihr", "Grimm", "\u00fc\u00b7ber", "den", "Rhein", "in", "das", "ab\u00b7scheu\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NE", "APPR", "ART", "NE", "APPR", "ART", "ADJA"], "meter": "+-+--++--+--+---", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Land, wo H\u00f6chst\u00e4dt vom Mord, Ro\u00dfbach vom Mord feiner Franzosen raucht!", "tokens": ["Land", ",", "wo", "H\u00f6chs\u00b7t\u00e4dt", "vom", "Mord", ",", "Ro\u00df\u00b7bach", "vom", "Mord", "fei\u00b7ner", "Fran\u00b7zo\u00b7sen", "raucht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "APPRART", "NN", "$,", "NN", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+---+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Billig schimpft ihr den witzlosen Gesang, welcher mit rauhem Schall,", "tokens": ["Bil\u00b7lig", "schimpft", "ihr", "den", "witz\u00b7lo\u00b7sen", "Ge\u00b7sang", ",", "wel\u00b7cher", "mit", "rau\u00b7hem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--++--+---+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gott, dies M\u00e4rchen! und ha! Freiheitsgew\u00e4sch t\u00f6net, und Vaterland!", "tokens": ["Gott", ",", "