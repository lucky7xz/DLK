{"dta.poem.10875": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgte Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich geh im Garten hin und her,", "tokens": ["Ich", "geh", "im", "Gar\u00b7ten", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um, an des gro\u00dfen Sch\u00f6pfers Werken", "tokens": ["Um", ",", "an", "des", "gro\u00b7\u00dfen", "Sch\u00f6p\u00b7fers", "Wer\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "$,", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Lust, und Ehrfurcht, zu bemerken,", "tokens": ["Mit", "Lust", ",", "und", "Ehr\u00b7furcht", ",", "zu", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie nichts von seinen Wundern leer.", "tokens": ["Wie", "nichts", "von", "sei\u00b7nen", "Wun\u00b7dern", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich denn, mit Vergn\u00fcgen schau,", "tokens": ["Da", "ich", "denn", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "schau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie, durch der Hecken holdes Gr\u00fcn,", "tokens": ["Wie", ",", "durch", "der", "He\u00b7cken", "hol\u00b7des", "Gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des glatten Wassers holdes Blau,", "tokens": ["Des", "glat\u00b7ten", "Was\u00b7sers", "hol\u00b7des", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Voll kleiner Silberblitze, schien.", "tokens": ["Voll", "klei\u00b7ner", "Sil\u00b7ber\u00b7blit\u00b7ze", ",", "schien", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die\u00df machte nun, in dem Geb\u00fcsche,", "tokens": ["Die\u00df", "mach\u00b7te", "nun", ",", "in", "dem", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein solch anmuthiges Gemische,", "tokens": ["Ein", "solch", "an\u00b7mut\u00b7hi\u00b7ges", "Ge\u00b7mi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Da\u00df meine Seele, durchs Gesicht,", "tokens": ["Da\u00df", "mei\u00b7ne", "See\u00b7le", ",", "durchs", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Recht angestralt von Farb und Licht,", "tokens": ["Recht", "an\u00b7ge\u00b7stralt", "von", "Farb", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dem, der sie, f\u00fcr so manche Pracht", "tokens": ["Dem", ",", "der", "sie", ",", "f\u00fcr", "so", "man\u00b7che", "Pracht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "$,", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Creatur, empfindlich macht,", "tokens": ["Der", "Crea\u00b7tur", ",", "emp\u00b7find\u00b7lich", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und ihr so manch Vergn\u00fcgen schenket,", "tokens": ["Und", "ihr", "so", "manch", "Ver\u00b7gn\u00fc\u00b7gen", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit frohem Dank, zu Ehren, folgends denket.", "tokens": ["Mit", "fro\u00b7hem", "Dank", ",", "zu", "Eh\u00b7ren", ",", "fol\u00b7gends", "den\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "\u201dsch\u00f6pfer! den allein die Liebe,", "tokens": ["\"", "sch\u00f6p\u00b7fer", "!", "den", "al\u00b7lein", "die", "Lie\u00b7be", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "ART", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "\u201dum mit den erschaffnen Sch\u00e4tzen,", "tokens": ["\"", "um", "mit", "den", "er\u00b7schaff\u00b7nen", "Sch\u00e4t\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "\u201dcreaturen zu ergetzen,", "tokens": ["\"", "crea\u00b7tu\u00b7ren", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "\u201dund sie inniglich zu r\u00fchren,", "tokens": ["\"", "und", "sie", "in\u00b7nig\u00b7lich", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "\u201dcreaturen zu formiren,", "tokens": ["\"", "crea\u00b7tu\u00b7ren", "zu", "for\u00b7mi\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "\u201ddie betrachten k\u00f6nnen, triebe!", "tokens": ["\"", "die", "be\u00b7trach\u00b7ten", "k\u00f6n\u00b7nen", ",", "trie\u00b7be", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "VVINF", "VMINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "\u201dla\u00df mich, da dein gn\u00e4digs Wollen", "tokens": ["\"", "la\u00df", "mich", ",", "da", "dein", "gn\u00e4\u00b7digs", "Wol\u00b7len"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "\u201dwill, da\u00df wir uns freuen sollen,", "tokens": ["\"", "will", ",", "da\u00df", "wir", "uns", "freu\u00b7en", "sol\u00b7len", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "\u201ddich, in Freuden zu erheben,", "tokens": ["\"", "dich", ",", "in", "Freu\u00b7den", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "\u201dmich, nach M\u00f6glichkeit, bestreben!", "tokens": ["\"", "mich", ",", "nach", "M\u00f6g\u00b7lich\u00b7keit", ",", "be\u00b7stre\u00b7ben", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "$,", "APPR", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}