{"textgrid.poem.47468": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es hauset der Gedanke]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hauset der Gedanke", "tokens": ["Es", "hau\u00b7set", "der", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im unendlichen Weltensaal;", "tokens": ["Im", "un\u00b7end\u00b7li\u00b7chen", "Wel\u00b7ten\u00b7saal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nicht bannt ihn eine Schranke,", "tokens": ["Nicht", "bannt", "ihn", "ei\u00b7ne", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht hemmt ihn eine Kette,", "tokens": ["Nicht", "hemmt", "ihn", "ei\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ringet H\u00f6h' und Thal", "tokens": ["Es", "rin\u00b7get", "H\u00f6h'", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sein Fittig aus, verbrannte Zon' und Pol,", "tokens": ["Sein", "Fit\u00b7tig", "aus", ",", "ver\u00b7brann\u00b7te", "Zon'", "und", "Pol", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-