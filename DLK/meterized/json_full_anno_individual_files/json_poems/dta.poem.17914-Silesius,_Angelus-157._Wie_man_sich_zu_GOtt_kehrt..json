{"dta.poem.17914": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "157.  Wie man sich zu GOtt kehrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Heiliger Begihr/ und nicht mit blossem bethen;", "tokens": ["Mit", "Hei\u00b7li\u00b7ger", "Be\u00b7gihr", "/", "und", "nicht", "mit", "blos\u00b7sem", "be\u00b7then", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "PTKNEG", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Heilgem Lebenslauff komt man zu GOtt gethreten.", "tokens": ["Mit", "Heil\u00b7gem", "Le\u00b7bens\u00b7lauff", "komt", "man", "zu", "Gott", "ge\u00b7th\u00b7re\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+----", "measure": "iambic.tetra.relaxed"}}}}}