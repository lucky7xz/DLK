{"dta.poem.16021": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sechs W\u00f6rtchen nehmen mich in Anspruch jeden Tag:", "tokens": ["Sechs", "W\u00f6rt\u00b7chen", "neh\u00b7men", "mich", "in", "An\u00b7spruch", "je\u00b7den", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich soll, ich mu\u00df, ich kann, ich will, ich darf, ich mag.", "tokens": ["Ich", "soll", ",", "ich", "mu\u00df", ",", "ich", "kann", ",", "ich", "will", ",", "ich", "darf", ",", "ich", "mag."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich soll, ist das Gesetz, von Gott ins Herz geschrieben,", "tokens": ["Ich", "soll", ",", "ist", "das", "Ge\u00b7setz", ",", "von", "Gott", "ins", "Herz", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ziel, nach welchem ich bin von mir selbst getrieben.", "tokens": ["Das", "Ziel", ",", "nach", "wel\u00b7chem", "ich", "bin", "von", "mir", "selbst", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df, das ist die Schrank', in welcher mich die Welt", "tokens": ["Ich", "mu\u00df", ",", "das", "ist", "die", "Schrank'", ",", "in", "wel\u00b7cher", "mich", "die", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einer, die Natur von andrer Seite h\u00e4lt.", "tokens": ["Von", "ei\u00b7ner", ",", "die", "Na\u00b7tur", "von", "an\u00b7drer", "Sei\u00b7te", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich kann, das ist das Ma\u00df der mir verliehnen Kraft,", "tokens": ["Ich", "kann", ",", "das", "ist", "das", "Ma\u00df", "der", "mir", "ver\u00b7lieh\u00b7nen", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PDS", "VAFIN", "ART", "NN", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der That, der Fertigkeit, der Kunst und Wissenschaft.", "tokens": ["Der", "That", ",", "der", "Fer\u00b7tig\u00b7keit", ",", "der", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich will, die h\u00f6chste Kron' ist dieses, die mich schm\u00fcckt,", "tokens": ["Ich", "will", ",", "die", "h\u00f6chs\u00b7te", "Kron'", "ist", "die\u00b7ses", ",", "die", "mich", "schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Freiheit Siegel, das mein Geist sich aufgedr\u00fcckt.", "tokens": ["Der", "Frei\u00b7heit", "Sie\u00b7gel", ",", "das", "mein", "Geist", "sich", "auf\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich darf, das ist zugleich die Inschrift bei dem Siegel,", "tokens": ["Ich", "darf", ",", "das", "ist", "zu\u00b7gleich", "die", "In\u00b7schrift", "bei", "dem", "Sie\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PDS", "VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim aufgethanen Thor der Freiheit auch ein Riegel.", "tokens": ["Beim", "auf\u00b7ge\u00b7tha\u00b7nen", "Thor", "der", "Frei\u00b7heit", "auch", "ein", "Rie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich mag, das endlich ist, was zwischen allen schwimmt,", "tokens": ["Ich", "mag", ",", "das", "end\u00b7lich", "ist", ",", "was", "zwi\u00b7schen", "al\u00b7len", "schwimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "ADV", "VAFIN", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Unbestimmtes, das der Augenblick bestimmt.", "tokens": ["Ein", "Un\u00b7be\u00b7stimm\u00b7tes", ",", "das", "der", "Au\u00b7gen\u00b7blick", "be\u00b7stimmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich soll, ich mu\u00df, ich kann, ich will, ich darf, ich mag,", "tokens": ["Ich", "soll", ",", "ich", "mu\u00df", ",", "ich", "kann", ",", "ich", "will", ",", "ich", "darf", ",", "ich", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sechse nehmen mich in Anspruch jeden Tag.", "tokens": ["Die", "sech\u00b7se", "neh\u00b7men", "mich", "in", "An\u00b7spruch", "je\u00b7den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Nur wenn du stets mich lehrst, wei\u00df ich, was jeden Tag", "tokens": ["Nur", "wenn", "du", "stets", "mich", "lehrst", ",", "wei\u00df", "ich", ",", "was", "je\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich soll, ich mu\u00df, ich kann, ich will, ich darf, ich mag.", "tokens": ["Ich", "soll", ",", "ich", "mu\u00df", ",", "ich", "kann", ",", "ich", "will", ",", "ich", "darf", ",", "ich", "mag."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "abbreviation"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}