{"dta.poem.11924": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn jener Funke Licht in dir vom h\u00f6chsten Licht", "tokens": ["Wenn", "je\u00b7ner", "Fun\u00b7ke", "Licht", "in", "dir", "vom", "h\u00f6chs\u00b7ten", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "NN", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergisset seiner Pflicht und seines Ursprungs nicht;", "tokens": ["Ver\u00b7gis\u00b7set", "sei\u00b7ner", "Pflicht", "und", "sei\u00b7nes", "Ur\u00b7sprungs", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn er das dunkle Haus, das er bewohnen soll,", "tokens": ["Wenn", "er", "das", "dunk\u00b7le", "Haus", ",", "das", "er", "be\u00b7woh\u00b7nen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In stiller Freudigkeit macht Himmelsglanzes voll;", "tokens": ["In", "stil\u00b7ler", "Freu\u00b7dig\u00b7keit", "macht", "Him\u00b7mels\u00b7glan\u00b7zes", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn seine Spitze treu er stets zur H\u00f6he lenkt,", "tokens": ["Wenn", "sei\u00b7ne", "Spit\u00b7ze", "treu", "er", "stets", "zur", "H\u00f6\u00b7he", "lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eigenwillig nicht sich in die Tiefe senkt;", "tokens": ["Und", "ei\u00b7gen\u00b7wil\u00b7lig", "nicht", "sich", "in", "die", "Tie\u00b7fe", "senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht gleich der Pflanze will im Boden Wurzel schlagen,", "tokens": ["Nicht", "gleich", "der", "Pflan\u00b7ze", "will", "im", "Bo\u00b7den", "Wur\u00b7zel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VMFIN", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch gleich dem Thier am Staub nach niederm Raube jagen;", "tokens": ["Noch", "gleich", "dem", "Thier", "am", "Staub", "nach", "nie\u00b7derm", "Rau\u00b7be", "ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nein, wie die Blume sich dem Licht er\u00f6ffnet gern,", "tokens": ["Nein", ",", "wie", "die", "Blu\u00b7me", "sich", "dem", "Licht", "er\u00b7\u00f6ff\u00b7net", "gern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "PRF", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer aufzugehn bereit ist wie ein Stern;", "tokens": ["Und", "im\u00b7mer", "auf\u00b7zu\u00b7gehn", "be\u00b7reit", "ist", "wie", "ein", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIZU", "ADJD", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ja Zeugnis, da\u00df im Licht er lebt in dunklen Schranken,", "tokens": ["Ja", "Zeug\u00b7nis", ",", "da\u00df", "im", "Licht", "er", "lebt", "in", "dunk\u00b7len", "Schran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets gibt mit lichter That, Lichtwort und Lichtgedanken:", "tokens": ["Stets", "gibt", "mit", "lich\u00b7ter", "That", ",", "Licht\u00b7wort", "und", "Licht\u00b7ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann wird von oben gern das Licht mit ihm verkehren,", "tokens": ["Dann", "wird", "von", "o\u00b7ben", "gern", "das", "Licht", "mit", "ihm", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im gesunknen Stern den hohen Ursprung ehren;", "tokens": ["Und", "im", "ge\u00b7sun\u00b7knen", "Stern", "den", "ho\u00b7hen", "Ur\u00b7sprung", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihm helfen, wenn er sicht, bis er die Schranke bricht,", "tokens": ["Ihm", "hel\u00b7fen", ",", "wenn", "er", "sicht", ",", "bis", "er", "die", "Schran\u00b7ke", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus der Scheitel tritt ein Licht hervor ins Licht:", "tokens": ["Und", "aus", "der", "Schei\u00b7tel", "tritt", "ein", "Licht", "her\u00b7vor", "ins", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dann wird ein Sonnenstral, und w\u00e4r' es in der Nacht,", "tokens": ["Dann", "wird", "ein", "Son\u00b7nen\u00b7stral", ",", "und", "w\u00e4r'", "es", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird ein Mondenstral, beliehn mit Sonnenmacht,", "tokens": ["So", "wird", "ein", "Mon\u00b7dens\u00b7tral", ",", "be\u00b7liehn", "mit", "Son\u00b7nen\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sich unterbreiten ihm, und heben ihn und tragen", "tokens": ["Sich", "un\u00b7ter\u00b7brei\u00b7ten", "ihm", ",", "und", "he\u00b7ben", "ihn", "und", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Lichtreich sicher, da\u00df kein Sturm ihn kann verschlagen,", "tokens": ["Ins", "Lich\u00b7treich", "si\u00b7cher", ",", "da\u00df", "kein", "Sturm", "ihn", "kann", "ver\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "KOUS", "PIAT", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Vorbei dem Wirbel, der die schwerern Geister zieht,", "tokens": ["Vor\u00b7bei", "dem", "Wir\u00b7bel", ",", "der", "die", "schwe\u00b7rern", "Geis\u00b7ter", "zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tiefe, die er floh, und der er jetzt entflieht.", "tokens": ["Der", "Tie\u00b7fe", ",", "die", "er", "floh", ",", "und", "der", "er", "jetzt", "ent\u00b7flieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}