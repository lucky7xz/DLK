{"dta.poem.21437": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Des Jahrhunderts verlorene Kinder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ein freudlos erl\u00f6sungheischend Geschlecht,               ", "tokens": ["Ein", "freud\u00b7los", "er\u00b7l\u00f6\u00b7sun\u00b7ghei\u00b7schend", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Jahrhunderts verlorene Kinder,", "tokens": ["Des", "Jahr\u00b7hun\u00b7derts", "ver\u00b7lo\u00b7re\u00b7ne", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So taumeln wir hin! we\u00df Schmerzen sind echt?", "tokens": ["So", "tau\u00b7meln", "wir", "hin", "!", "we\u00df", "Schmer\u00b7zen", "sind", "echt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PWAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "We\u00df Lust ist kein Rausch? wer kein S\u00fcnder? \u2026", "tokens": ["We\u00df", "Lust", "ist", "kein", "Rausch", "?", "wer", "kein", "S\u00fcn\u00b7der", "?", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "VAFIN", "PIAT", "NN", "$.", "PWS", "PIAT", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Selbstsucht treibt Alle, wilde Gier nach Gold,", "tokens": ["Selbst\u00b7sucht", "treibt", "Al\u00b7le", ",", "wil\u00b7de", "Gier", "nach", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uners\u00e4ttlich Sinnengel\u00fcste,", "tokens": ["Un\u00b7er\u00b7s\u00e4tt\u00b7lich", "Sin\u00b7nen\u00b7ge\u00b7l\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Keinem Einzigen ist Mutter Erde hold \u2014", "tokens": ["Kei\u00b7nem", "Ein\u00b7zi\u00b7gen", "ist", "Mut\u00b7ter", "Er\u00b7de", "hold"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "NN", "ADJD", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Rings graut nur unendliche W\u00fcste!", "tokens": ["Rings", "graut", "nur", "un\u00b7end\u00b7li\u00b7che", "W\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Chaotische Brandung wirr uns umtost;", "tokens": ["Chao\u00b7ti\u00b7sche", "Bran\u00b7dung", "wirr", "uns", "um\u00b7tost", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verzehrt von d\u00e4monischen Gluthen,", "tokens": ["Ver\u00b7zehrt", "von", "d\u00e4\u00b7mo\u00b7ni\u00b7schen", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von keinem Strahl ewigen Lichts umkost,", "tokens": ["Von", "kei\u00b7nem", "Strahl", "e\u00b7wi\u00b7gen", "Lichts", "um\u00b7kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "M\u00fcssen wir elend verbluten \u2026", "tokens": ["M\u00fcs\u00b7sen", "wir", "e\u00b7lend", "ver\u00b7blu\u00b7ten", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}