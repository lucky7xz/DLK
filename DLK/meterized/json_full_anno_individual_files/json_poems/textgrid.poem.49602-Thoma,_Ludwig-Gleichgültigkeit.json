{"textgrid.poem.49602": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gleichg\u00fcltigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich gestern lag in meinem Bette,", "tokens": ["Als", "ich", "ge\u00b7stern", "lag", "in", "mei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klopfte es so gegen Mitternacht.", "tokens": ["Klopf\u00b7te", "es", "so", "ge\u00b7gen", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Meinung war, es sei Jeannette,", "tokens": ["Mei\u00b7ne", "Mei\u00b7nung", "war", ",", "es", "sei", "Jean\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PPER", "VAFIN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nat\u00fcrlich hab' ich aufgemacht.", "tokens": ["Und", "na\u00b7t\u00fcr\u00b7lich", "hab'", "ich", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Leise kam es jetzt hereingeschlichen,", "tokens": ["Lei\u00b7se", "kam", "es", "jetzt", "her\u00b7ein\u00b7ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Setzte sich an meines Bettes Rand,", "tokens": ["Setz\u00b7te", "sich", "an", "mei\u00b7nes", "Bet\u00b7tes", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Hat mir \u00fcber meinen Kopf gestrichen", "tokens": ["Hat", "mir", "\u00fc\u00b7ber", "mei\u00b7nen", "Kopf", "ge\u00b7stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mit der ziemlich gro\u00dfen, dicken Hand.", "tokens": ["Mit", "der", "ziem\u00b7lich", "gro\u00b7\u00dfen", ",", "di\u00b7cken", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Doch ich merkte bald an ihren Formen:", "tokens": ["Doch", "ich", "merk\u00b7te", "bald", "an", "ih\u00b7ren", "For\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Dieses Weib ist ja Jeannette nicht,", "tokens": ["Die\u00b7ses", "Weib", "ist", "ja", "Jean\u00b7net\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "NE", "PTKNEG", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Deren H\u00fcften nicht von so enormem", "tokens": ["De\u00b7ren", "H\u00fcf\u00b7ten", "nicht", "von", "so", "en\u00b7or\u00b7mem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "PTKNEG", "APPR", "ADV", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Umfang sind und solchem Schwergewicht.", "tokens": ["Um\u00b7fang", "sind", "und", "sol\u00b7chem", "Schwer\u00b7ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Trotzdem schwieg ich. Denn ich \u00fcberlegte:", "tokens": ["Trotz\u00b7dem", "schwieg", "ich", ".", "Denn", "ich", "\u00fc\u00b7ber\u00b7leg\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Nicht das ", "tokens": ["Nicht", "das"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Au\u00dferdem, die Absicht, die sie hegte,", "tokens": ["Au\u00b7\u00dfer\u00b7dem", ",", "die", "Ab\u00b7sicht", ",", "die", "sie", "heg\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "War entschieden l\u00f6blich ausgedacht.", "tokens": ["War", "ent\u00b7schie\u00b7den", "l\u00f6b\u00b7lich", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Was bedeutet dieserhalb ein Name?", "tokens": ["Was", "be\u00b7deu\u00b7tet", "die\u00b7ser\u00b7halb", "ein", "Na\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "In der Liebe ist das einerlei.", "t