{"textgrid.poem.47071": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, Kn\u00e4bchen, klingend mit dem Sporne,", "tokens": ["Wer", "bist", "du", ",", "Kn\u00e4b\u00b7chen", ",", "klin\u00b7gend", "mit", "dem", "Spor\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit dem t\u00f6nereichen Horn am Munde,", "tokens": ["Und", "mit", "dem", "t\u00f6\u00b7ne\u00b7rei\u00b7chen", "Horn", "am", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier ruhend auf der Liebsten Tisch? Gib Kunde!", "tokens": ["Hier", "ru\u00b7hend", "auf", "der", "Liebs\u00b7ten", "Tisch", "?", "Gib", "Kun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$.", "VVIMP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbich bin der Knabe mit dem Wunderhorne.", "tokens": ["\u00bb", "ich", "bin", "der", "Kna\u00b7be", "mit", "dem", "Wun\u00b7der\u00b7hor\u00b7ne", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier lie\u00dfest du mich ja, als du im Zorne", "tokens": ["Hier", "lie\u00b7\u00dfest", "du", "mich", "ja", ",", "als", "du", "im", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damals von hinnen gingst; und seit der Stunde", "tokens": ["Da\u00b7mals", "von", "hin\u00b7nen", "gingst", ";", "und", "seit", "der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "VVFIN", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat sich dein einsam Lieb von Herzensgrunde", "tokens": ["Hat", "sich", "dein", "ein\u00b7sam", "Lieb", "von", "Her\u00b7zens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "PPOSAT", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gar oft erquickt aus meinem Liederborne.\u00ab", "tokens": ["Gar", "oft", "er\u00b7quickt", "aus", "mei\u00b7nem", "Lie\u00b7der\u00b7bor\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bist du also, seit ich aus gewesen,", "tokens": ["So", "bist", "du", "al\u00b7so", ",", "seit", "ich", "aus", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geblieben, Gl\u00fccklicher, am alten Platze?", "tokens": ["Ge\u00b7blie\u00b7ben", ",", "Gl\u00fcck\u00b7li\u00b7cher", ",", "am