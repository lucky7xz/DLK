{"dta.poem.14213": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Schon wird es drau\u00dfen licht auf Berg und Thalen;", "tokens": ["Schon", "wird", "es", "drau\u00b7\u00dfen", "licht", "auf", "Berg", "und", "Tha\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aurora, stille Braut, ihr sch\u00f6nen Strahlen,", "tokens": ["Au\u00b7ro\u00b7ra", ",", "stil\u00b7le", "Braut", ",", "ihr", "sch\u00f6\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die farb'gen Rauch aus Flu\u00df und W\u00e4ldern saugen,", "tokens": ["Die", "fa\u00b7rb'\u00b7gen", "Rauch", "aus", "Flu\u00df", "und", "W\u00e4l\u00b7dern", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Euch gr\u00fc\u00dfen neu die halbverschlaf'nen Augen.", "tokens": ["Euch", "gr\u00fc\u00b7\u00dfen", "neu", "die", "halb\u00b7ver\u00b7schlaf'\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verr\u00e4th'risch, sagt man, sei des Zimmers Schw\u00fcle", "tokens": ["Ver\u00b7r\u00e4th'\u00b7risch", ",", "sagt", "man", ",", "sei", "des", "Zim\u00b7mers", "Schw\u00fc\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PIS", "$,", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo Nachts ein M\u00e4dchen tr\u00e4umte vom Geliebten:", "tokens": ["Wo", "Nachts", "ein", "M\u00e4d\u00b7chen", "tr\u00e4um\u00b7te", "vom", "Ge\u00b7lieb\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So komm herein, du rothe, frische K\u00fchle,", "tokens": ["So", "komm", "her\u00b7ein", ",", "du", "ro\u00b7the", ",", "fri\u00b7sche", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fliegt in die blaue Luft, ihr sch\u00f6nen Tr\u00e4ume!", "tokens": ["Fliegt", "in", "die", "blau\u00b7e", "Luft", ",", "ihr", "sch\u00f6\u00b7nen", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}