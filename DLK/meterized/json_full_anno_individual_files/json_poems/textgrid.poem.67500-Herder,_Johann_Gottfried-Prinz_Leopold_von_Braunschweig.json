{"textgrid.poem.67500": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Prinz Leopold von Braunschweig", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbla\u00dft uns helfen den Armen! Auch wir sind Menschen!\u00ab So sprach er", "tokens": ["\u00bb", "la\u00dft", "uns", "hel\u00b7fen", "den", "Ar\u00b7men", "!", "Auch", "wir", "sind", "Men\u00b7schen", "!", "\u00ab", "So", "sprach", "er"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "VVFIN", "ART", "NN", "$.", "ADV", "PPER", "VAFIN", "NN", "$.", "$(", "ADV", "VVFIN", "PPER"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und stieg muthig voran in den errettenden Kahn.", "tokens": ["Und", "stieg", "mut\u00b7hig", "vo\u00b7ran", "in", "den", "er\u00b7ret\u00b7ten\u00b7den", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Und da sprachen die G\u00f6tter: \u00bbDem menschenfreundlichen Helden", "tokens": ["Und", "da", "spra\u00b7chen", "die", "G\u00f6t\u00b7ter", ":", "\u00bb", "Dem", "men\u00b7schen\u00b7freund\u00b7li\u00b7chen", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ziemt ein h\u00f6heres Loos! Komm zum Olympus hinauf,", "tokens": ["Ziemt", "ein", "h\u00f6\u00b7he\u00b7res", "Loos", "!", "Komm", "zum", "O\u00b7lym\u00b7pus", "hin\