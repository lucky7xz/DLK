{"textgrid.poem.47723": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach, Schwesterlein, ach, Schwesterlein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Schwesterlein, ach, Schwesterlein,", "tokens": ["Ach", ",", "Schwes\u00b7ter\u00b7lein", ",", "ach", ",", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ITJ", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hast du dich so weit hinaus", "tokens": ["Wie", "hast", "du", "dich", "so", "weit", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versprochen aus dem Vaterhaus!", "tokens": ["Ver\u00b7spro\u00b7chen", "aus", "dem", "Va\u00b7ter\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Br\u00fcder haben ein gro\u00df Begehr", "tokens": ["Die", "Br\u00fc\u00b7der", "ha\u00b7ben", "ein", "gro\u00df", "Be\u00b7gehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und m\u00f6chten gerne bei dir sein,", "tokens": ["Und", "m\u00f6ch\u00b7ten", "ger\u00b7ne", "bei", "dir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tragen es nicht l\u00e4nger mehr", "tokens": ["Und", "tra\u00b7gen", "es", "nicht", "l\u00e4n\u00b7ger", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und reiten in die Welt hinein,", "tokens": ["Und", "rei\u00b7ten", "in", "die", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kennen, ach, die Wege nicht,", "tokens": ["Und", "ken\u00b7nen", ",", "ach", ",", "die", "We\u00b7ge", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ITJ", "$,", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und finden, ach, die Stege nicht,", "tokens": ["Und", "fin\u00b7den", ",", "ach", ",", "die", "Ste\u00b7ge", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ITJ", "$,", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und irren fragend im Land umher.", "tokens": ["Und", "ir\u00b7ren", "fra\u00b7gend", "im", "Land", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, Schwesterlein, ach, Schwesterlein,", "tokens": ["Ach", ",", "Schwes\u00b7ter\u00b7lein", ",", "ach", ",", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ITJ", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie