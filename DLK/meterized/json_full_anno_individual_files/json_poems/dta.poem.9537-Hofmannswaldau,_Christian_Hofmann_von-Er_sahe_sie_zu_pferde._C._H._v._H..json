{"dta.poem.9537": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er sahe sie zu pferde.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die lange Lesbia/ so meine freyheit bindet/", "tokens": ["Die", "lan\u00b7ge", "Les\u00b7bia", "/", "so", "mei\u00b7ne", "frey\u00b7heit", "bin\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erk\u00fchnte sich nechst hin zu schreiten auf ein pferd.", "tokens": ["Er\u00b7k\u00fchn\u00b7te", "sich", "nechst", "hin", "zu", "schrei\u00b7ten", "auf", "ein", "pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Trug gleich ihr sch\u00f6ner leib nicht bogen/ spie\u00df und schwerdt/", "tokens": ["Trug", "gleich", "ihr", "sch\u00f6\u00b7ner", "leib", "nicht", "bo\u00b7gen", "/", "spie\u00df", "und", "schwerdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$(", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fchrt sie doch den blitz/ der alle welt entz\u00fcndet/", "tokens": ["So", "f\u00fchrt", "sie", "doch", "den", "blitz", "/", "der", "al\u00b7le", "welt", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$(", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein etwas/ so man f\u00fchlt und keiner recht ergr\u00fcndet/", "tokens": ["Ein", "et\u00b7was", "/", "so", "man", "f\u00fchlt", "und", "kei\u00b7ner", "recht", "er\u00b7gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$(", "ADV", "PIS", "VVFIN", "KON", "PIS", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem kein Bucephalus sich recht und wohl erwehrt/", "tokens": ["Dem", "kein", "Bu\u00b7ce\u00b7pha\u00b7lus", "sich", "recht", "und", "wohl", "er\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PRF", "ADJD", "KON", "ADV", "VVPP", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "So Alexandern selbst und seinen muth verzehrt/", "tokens": ["So", "A\u00b7lex\u00b7an\u00b7dern", "selbst", "und", "sei\u00b7nen", "muth", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Macht da\u00df ihr pferd den trieb/ der himmlisch ist/ empfindet;", "tokens": ["Macht", "da\u00df", "ihr", "pferd", "den", "trieb", "/", "der", "himm\u00b7lisch", "ist", "/", "emp\u00b7fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KOUS", "PPOSAT", "NN", "ART", "VVFIN", "$(", "ART", "ADJD", "VAFIN", "$(", "VVFIN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.9": {"text": "Wie wirstu Heldin denn itzund von mir genant/", "tokens": ["Wie", "wirs\u00b7tu", "Hel\u00b7din", "denn", "it\u00b7zund", "von", "mir", "ge\u00b7nant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der ich das erste mahl durch deine glut entbrant/", "tokens": ["Der", "ich", "das", "ers\u00b7te", "mahl", "durch", "dei\u00b7ne", "glut", "ent\u00b7brant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich/ dessen asche noch soll deine wahlstatt zieren.", "tokens": ["Ich", "/", "des\u00b7sen", "asc\u00b7he", "noch", "soll", "dei\u00b7ne", "wahl\u00b7statt", "zie\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PDS", "VVFIN", "ADV", "VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Reit/ reit/ Amazonin/ getrost durch wald und feld:", "tokens": ["Reit", "/", "reit", "/", "A\u00b7ma\u00b7zo\u00b7nin", "/", "ge\u00b7trost", "durch", "wald", "und", "feld", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NE", "$(", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch wiltu da\u00df dein knecht die sehnen steiff beh\u00e4lt/", "tokens": ["Doch", "wil\u00b7tu", "da\u00df", "dein", "knecht", "die", "seh\u00b7nen", "steiff", "be\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So mustu/ merck es wol/ die br\u00fcste nicht verlieren.", "tokens": ["So", "mus\u00b7tu", "/", "merck", "es", "wol", "/", "die", "br\u00fcs\u00b7te", "nicht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "VVFIN", "PPER", "ADV", "$(", "ART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}