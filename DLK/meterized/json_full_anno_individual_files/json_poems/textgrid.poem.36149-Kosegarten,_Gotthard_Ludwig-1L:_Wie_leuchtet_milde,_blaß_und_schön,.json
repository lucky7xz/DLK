 "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Verh\u00e4ngni\u00df hat mir sie angetrau't.", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "hat", "mir", "sie", "an\u00b7ge\u00b7trau'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "NE", "NE"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "O M\u00e4dchen, M\u00e4dchen, bleib nur mein!", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "M\u00e4d\u00b7chen", ",", "bleib", "nur", "mein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist mir Welt und Schicksal klein!", "tokens": ["So", "ist", "mir", "Welt", "und", "Schick\u00b7sal", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So rei\u00dft mich von dir nicht Gewalt noch Noth,", "tokens": ["So", "rei\u00dft", "mich", "von", "dir", "nicht", "Ge\u00b7walt", "noch", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Selbst nicht der eiserne grimmige Tod.", "tokens": ["Selbst", "nicht", "der", "ei\u00b7ser\u00b7ne", "grim\u00b7mi\u00b7ge", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}