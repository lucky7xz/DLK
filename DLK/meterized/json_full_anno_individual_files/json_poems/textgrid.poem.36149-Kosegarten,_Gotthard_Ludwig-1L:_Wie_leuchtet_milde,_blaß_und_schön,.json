{"textgrid.poem.36149": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie leuchtet milde, bla\u00df und sch\u00f6n,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie leuchtet milde, bla\u00df und sch\u00f6n,", "tokens": ["Wie", "leuch\u00b7tet", "mil\u00b7de", ",", "bla\u00df", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Abendsonne! Sieh, wie wehn", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "!", "Sieh", ",", "wie", "wehn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "$,", "PWAV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00fcthen, r\u00f6thlich, wei\u00df und bunt,", "tokens": ["Die", "Bl\u00fc\u00b7then", ",", "r\u00f6th\u00b7lich", ",", "wei\u00df", "und", "bunt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcberschnei'n den Gartengrund!", "tokens": ["Und", "\u00fc\u00b7ber\u00b7schnei'n", "den", "Gar\u00b7ten\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie schwimmt die k\u00fchl'ge Abendluft", "tokens": ["Wie", "schwimmt", "die", "k\u00fchl'\u00b7ge", "A\u00b7ben\u00b7dluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Mai- und Nachtviolenduft!", "tokens": ["In", "Mai", "und", "Nacht\u00b7vi\u00b7o\u00b7len\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie w\u00f6lkt sich die Laube bl\u00e4tterschwer", "tokens": ["Wie", "w\u00f6lkt", "sich", "die", "Lau\u00b7be", "bl\u00e4t\u00b7ter\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So dunkel freundlich um uns her.", "tokens": ["So", "dun\u00b7kel", "freund\u00b7lich", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und, horch! durch Garten, Busch und Thal,", "tokens": ["Und", ",", "horch", "!", "durch", "Gar\u00b7ten", ",", "Busch", "und", "Thal", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVIMP", "$.", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt ihren Schlag die Nachtigall!", "tokens": ["Schl\u00e4gt", "ih\u00b7ren", "Schlag", "die", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Schlag schl\u00e4gt mir durch Mark und Bein \u2013", "tokens": ["Dein", "Schlag", "schl\u00e4gt", "mir", "durch", "Mark", "und", "Bein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Nachtigall, Nachtigall, schone mein!", "tokens": ["O", "Nach\u00b7ti\u00b7gall", ",", "Nach\u00b7ti\u00b7gall", ",", "scho\u00b7ne", "mein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und, ach! in ihrer Lieblichkeit,", "tokens": ["Und", ",", "ach", "!", "in", "ih\u00b7rer", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer Sch\u00f6nheit Feierkleid,", "tokens": ["In", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Fei\u00b7er\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wallt neben mir das M\u00e4dchen mein!", "tokens": ["Wallt", "ne\u00b7ben", "mir", "das", "M\u00e4d\u00b7chen", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "O M\u00e4dchen, M\u00e4dchen, schone mein!", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "M\u00e4d\u00b7chen", ",", "scho\u00b7ne", "mein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O schone mein, du bist so hold,", "tokens": ["O", "scho\u00b7ne", "mein", ",", "du", "bist", "so", "hold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel holder als der Sonne Gold,", "tokens": ["Viel", "hol\u00b7der", "als", "der", "Son\u00b7ne", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel sch\u00f6ner als die Bl\u00fcthen all',", "tokens": ["Viel", "sch\u00f6\u00b7ner", "als", "die", "Bl\u00fc\u00b7then", "all'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel s\u00fc\u00dfer als die Nachtigall.", "tokens": ["Viel", "s\u00fc\u00b7\u00dfer", "als", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dein Aug' ist blau und freundlich gut,", "tokens": ["Dein", "Aug'", "ist", "blau", "und", "freund\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Mund in seiner Rosengluth!", "tokens": ["Dein", "Mund", "in", "sei\u00b7ner", "Ro\u00b7sen\u00b7gluth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Blick so lieb! dein Busen rein!", "tokens": ["Dein", "Blick", "so", "lieb", "!", "dein", "Bu\u00b7sen", "rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$.", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Herzensm\u00e4dchen, schone mein!", "tokens": ["O", "Her\u00b7zens\u00b7m\u00e4d\u00b7chen", ",", "scho\u00b7ne", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In meiner Seele lebt's und webt's.", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "lebt's", "und", "webt'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "NE", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem Herzen strebt's und bebt's.", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "strebt's", "und", "bebt'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wogt und wirbelt Fluth auf Fluth.", "tokens": ["Es", "wogt", "und", "wir\u00b7belt", "Fluth", "auf", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es blitzt und lodert Gluth auf Gluth.", "tokens": ["Es", "blitzt", "und", "lo\u00b7dert", "Gluth", "auf", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und, horch! durch Busch und Bl\u00fcthen all'", "tokens": ["Und", ",", "horch", "!", "durch", "Busch", "und", "Bl\u00fc\u00b7then", "all'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVIMP", "$.", "APPR", "NN", "KON", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt noch einmal die Nachtigall.", "tokens": ["Schl\u00e4gt", "noch", "ein\u00b7mal", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Schlag schl\u00e4gt mir durch Leben und Sein.", "tokens": ["Dein", "Schlag", "schl\u00e4gt", "mir", "durch", "Le\u00b7ben", "und", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "PPOSAT", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "O Nachtigall, Nachtigall, schone mein!", "tokens": ["O", "Nach\u00b7ti\u00b7gall", ",", "Nach\u00b7ti\u00b7gall", ",", "scho\u00b7ne", "mein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Mir wird so hei\u00df! Mir wird so weh,", "tokens": ["Mir", "wird", "so", "hei\u00df", "!", "Mir", "wird", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um dich, du innig Innige!", "tokens": ["Um", "dich", ",", "du", "in\u00b7nig", "In\u00b7ni\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ist, wie ich, so stark, so held!", "tokens": ["Wer", "ist", ",", "wie", "ich", ",", "so", "stark", ",", "so", "held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schl\u00fcge f\u00fcr dich mit der ganzen Welt.", "tokens": ["Ich", "schl\u00fc\u00b7ge", "f\u00fcr", "dich", "mit", "der", "gan\u00b7zen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich st\u00fcrbe f\u00fcr dich den hei\u00dfesten Tod!", "tokens": ["Ich", "st\u00fcr\u00b7be", "f\u00fcr", "dich", "den", "hei\u00b7\u00dfes\u00b7ten", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zehntausendfachen grimmigen Tod!", "tokens": ["Zehn\u00b7tau\u00b7send\u00b7fa\u00b7chen", "grim\u00b7mi\u00b7gen", "Tod", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wol grimmig, d\u00fcster, wild ist er!", "tokens": ["Wol", "grim\u00b7mig", ",", "d\u00fcs\u00b7ter", ",", "wild", "ist", "er", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Doch ist die Liebe noch grimmiger!", "tokens": ["Doch", "ist", "die", "Lie\u00b7be", "noch", "grim\u00b7mi\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Wer will mir rauben das M\u00e4dchen mein?", "tokens": ["Wer", "will", "mir", "rau\u00b7ben", "das", "M\u00e4d\u00b7chen", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu Staub soll stieben sein Gebein!", "tokens": ["Zu", "Staub", "soll", "stie\u00b7ben", "sein", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hadert um meine erw\u00e4hlte Braut?", "tokens": ["Wer", "ha\u00b7dert", "um", "mei\u00b7ne", "er\u00b7w\u00e4hl\u00b7te", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Verh\u00e4ngni\u00df hat mir sie angetrau't.", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "hat", "mir", "sie", "an\u00b7ge\u00b7trau'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "NE", "NE"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "O M\u00e4dchen, M\u00e4dchen, bleib nur mein!", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "M\u00e4d\u00b7chen", ",", "bleib", "nur", "mein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VVFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist mir Welt und Schicksal klein!", "tokens": ["So", "ist", "mir", "Welt", "und", "Schick\u00b7sal", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So rei\u00dft mich von dir nicht Gewalt noch Noth,", "tokens": ["So", "rei\u00dft", "mich", "von", "dir", "nicht", "Ge\u00b7walt", "noch", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Selbst nicht der eiserne grimmige Tod.", "tokens": ["Selbst", "nicht", "der", "ei\u00b7ser\u00b7ne", "grim\u00b7mi\u00b7ge", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie leuchtet milde, bla\u00df und sch\u00f6n,", "tokens": ["Wie", "leuch\u00b7tet", "mil\u00b7de", ",", "bla\u00df", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Abendsonne! Sieh, wie wehn", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "!", "Sieh", ",", "wie", "wehn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "$,", "PWAV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bl\u00fcthen, r\u00f6thlich, wei\u00df und bunt,", "tokens": ["Die", "Bl\u00fc\u00b7then", ",", "r\u00f6th\u00b7lich", ",", "wei\u00df"