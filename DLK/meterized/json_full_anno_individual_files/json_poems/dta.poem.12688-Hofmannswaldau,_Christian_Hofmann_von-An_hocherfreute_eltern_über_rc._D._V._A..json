{"dta.poem.12688": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An hocherfreute eltern \u00fcber rc.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Empfangt den lieben sohn, als euren werthen gast,", "tokens": ["Emp\u00b7fangt", "den", "lie\u00b7ben", "sohn", ",", "als", "eu\u00b7ren", "wert\u00b7hen", "gast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und last die tochter hin zu ihren liebsten ziehen:", "tokens": ["Und", "last", "die", "toch\u00b7ter", "hin", "zu", "ih\u00b7ren", "liebs\u00b7ten", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird auf einen tag viel sorge von euch fliehen,", "tokens": ["So", "wird", "auf", "ei\u00b7nen", "tag", "viel", "sor\u00b7ge", "von", "euch", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihrer jugend lust schw\u00e4cht eures alters last.", "tokens": ["Und", "ih\u00b7rer", "ju\u00b7gend", "lust", "schw\u00e4cht", "eu\u00b7res", "al\u00b7ters", "last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVFIN", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott steh euch ferner bey! so wird allhier auf erden", "tokens": ["Gott", "steh", "euch", "fer\u00b7ner", "bey", "!", "so", "wird", "all\u00b7hier", "auf", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das gl\u00fcck der kinder alt, der eltern j\u00fcnger werden.", "tokens": ["Das", "gl\u00fcck", "der", "kin\u00b7der", "alt", ",", "der", "el\u00b7tern", "j\u00fcn\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}