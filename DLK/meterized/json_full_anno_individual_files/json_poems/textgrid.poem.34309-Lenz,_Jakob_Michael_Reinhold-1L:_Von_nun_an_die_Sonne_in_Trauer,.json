{"textgrid.poem.34309": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von nun an die Sonne in Trauer,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von nun an die Sonne in Trauer,", "tokens": ["Von", "nun", "an", "die", "Son\u00b7ne", "in", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von nun an finster der Tag,", "tokens": ["Von", "nun", "an", "fins\u00b7ter", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des Himmels Thore verschlossen!", "tokens": ["Des", "Him\u00b7mels", "Tho\u00b7re", "ver\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wer ist der wieder er\u00f6ffnen", "tokens": ["Wer", "ist", "der", "wie\u00b7der", "er\u00b7\u00f6ff\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mir wieder entschlie\u00dfen sie mag?", "tokens": ["Mir", "wie\u00b7der", "ent\u00b7schlie\u00b7\u00dfen", "sie", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "VMFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Hier ausgesperret, verloren,", "tokens": ["Hier", "aus\u00b7ge\u00b7sper\u00b7ret", ",", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sitzt der Verworfne und weint,", "tokens": ["Sitzt", "der", "Ver\u00b7worf\u00b7ne", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Und kennt im Himmel, auf Erden", "tokens": ["Und", "kennt", "im", "Him\u00b7mel", ",", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Geh\u00e4\u00dfiger nichts als sich selber,", "tokens": ["Ge\u00b7h\u00e4\u00b7\u00dfi\u00b7ger", "nichts", "als", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "KOKOM", "PRF", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Und ist im Himmel, auf Erden", "tokens": ["Und", "ist", "im", "Him\u00b7mel", ",", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Sein unvers\u00f6hnlichster Feind.", "tokens": ["Sein", "un\u00b7ver\u00b7s\u00f6hn\u00b7lichs\u00b7ter", "Feind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Aufgiengen die Thore,", "tokens": ["Auf\u00b7gien\u00b7gen", "die", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ich sah die Erscheinung.", "tokens": ["Ich", "sah", "die", "Er\u00b7schei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und war's kein Traum?", "tokens": ["Und", "wa\u00b7r's", "kein", "Traum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und war's so fremd mir? \u2013", "tokens": ["Und", "wa\u00b7r's", "so", "fremd", "mir", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "PPER", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Tochter, die Freude,", "tokens": ["Die", "Toch\u00b7ter", ",", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Segen des Himmels,", "tokens": ["Der", "Se\u00b7gen", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In wei\u00dfen Gew\u00f6lken", "tokens": ["In", "wei\u00b7\u00dfen", "Ge\u00b7w\u00f6l\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit Rosen umschattet,", "tokens": ["Mit", "Ro\u00b7sen", "um\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Duftete sie hin\u00fcber zu mir.", "tokens": ["Duf\u00b7te\u00b7te", "sie", "hin\u00b7\u00fc\u00b7ber", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "In Liebe hingesunken,", "tokens": ["In", "Lie\u00b7be", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie schrecklich in Reizen geschm\u00fcckt,", "tokens": ["Wie", "schreck\u00b7lich", "in", "Rei\u00b7zen", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Schon hatt' ich so selig, so trunken", "tokens": ["Schon", "hatt'", "ich", "so", "se\u00b7lig", ",", "so", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Fest an mein Herz sie gedr\u00fcckt.", "tokens": ["Fest", "an", "mein", "Herz", "sie", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Ich lag im Geist ihr zu F\u00fc\u00dfen,", "tokens": ["Ich", "lag", "im", "Geist", "ihr", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Mein Mund schwebt' \u00fcber ihr \u2013", "tokens": ["Mein", "Mund", "schwebt'", "\u00fc\u00b7ber", "ihr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ach! diese Lippen zu k\u00fcssen", "tokens": ["Ach", "!", "die\u00b7se", "Lip\u00b7pen", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PDAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Und dann mit ewiger M\u00fch", "tokens": ["Und", "dann", "mit", "e\u00b7wi\u00b7ger", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Den s\u00fc\u00dfen Frevel zu b\u00fc\u00dfen! \u2013", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Fre\u00b7vel", "zu", "b\u00fc\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "In dem einzigen Augenblick,", "tokens": ["In", "dem", "ein\u00b7zi\u00b7gen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gro\u00dfe G\u00f6tter! was hielt mich zur\u00fcck?", "tokens": ["Gro\u00b7\u00dfe", "G\u00f6t\u00b7ter", "!", "was", "hielt", "mich", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Kommt er nicht wieder? \u2013", "tokens": ["Kommt", "er", "nicht", "wie\u00b7der", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$.", "$("], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Er kehrt nicht wieder,", "tokens": ["Er", "kehrt", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ach er ist hin, der Augenblick", "tokens": ["Ach", "er", "ist", "hin", ",", "der", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPER", "VAFIN", "ADV", "$,", "ART", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und der Tod mein einziges Gl\u00fcck! \u2013", "tokens": ["Und", "der", "Tod", "mein", "ein\u00b7zi\u00b7ges", "Gl\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Da\u00df er k\u00e4me! \u2013", "tokens": ["Da\u00df", "er", "k\u00e4\u00b7me", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mit bebender Seele", "tokens": ["Mit", "be\u00b7ben\u00b7der", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Wollt' ich ihn fa\u00dfen,", "tokens": ["Wollt'", "ich", "ihn", "fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wollte mit Angst ihn", "tokens": ["Woll\u00b7te", "mit", "Angst", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und mit Entz\u00fccken", "tokens": ["Und", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Halten ihn, halten", "tokens": ["Hal\u00b7ten", "ihn", ",", "hal\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und ihn nicht la\u00dfen,", "tokens": ["Und", "ihn", "nicht", "la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und drohte die Erde mir", "tokens": ["Und", "droh\u00b7te", "die", "Er\u00b7de", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Unter mir zu brechen,", "tokens": ["Un\u00b7ter", "mir", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Und drohte der Himmel mir,", "tokens": ["Und", "droh\u00b7te", "der", "Him\u00b7mel", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die K\u00fchnheit zu r\u00e4chen \u2013", "tokens": ["Die", "K\u00fchn\u00b7heit", "zu", "r\u00e4\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Ich hielte, ich fa\u00dfte dich,", "tokens": ["Ich", "hiel\u00b7te", ",", "ich", "fa\u00df\u00b7te", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Heilige, Einzige,", "tokens": ["Hei\u00b7li\u00b7ge", ",", "Ein\u00b7zi\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.14": {"text": "Mit all deiner Wonne,", "tokens": ["Mit", "all", "dei\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Mit all deinem Schmerz!", "tokens": ["Mit", "all", "dei\u00b7nem", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Presst' an den Busen dich,", "tokens": ["Presst'", "an", "den", "Bu\u00b7sen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.17": {"text": "S\u00e4ttigte einmal mich \u2013", "tokens": ["S\u00e4t\u00b7tig\u00b7te", "ein\u00b7mal", "mich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.18": {"text": "W\u00e4hnte du w\u00e4rst f\u00fcr mich \u2013", "tokens": ["W\u00e4hn\u00b7te", "du", "w\u00e4rst", "f\u00fcr", "mich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "APPR", "PPER", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "Und in dem Wonnerausch,", "tokens": ["Und", "in", "dem", "Won\u00b7ne\u00b7rausch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "In den Entz\u00fcckungen,", "tokens": ["In", "den", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Br\u00e4che mein Herz!", "tokens": ["Br\u00e4\u00b7che", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Von nun an die Sonne in Trauer,", "tokens": ["Von", "nun", "an", "die", "Son\u00b7ne", "in", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von nun an finster der Tag,", "tokens": ["Von", "nun", "an", "fins\u00b7ter", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Des Himmels Thore verschlossen!", "tokens": ["Des", "Him\u00b7mels", "Tho\u00b7re", "ver\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wer ist der wieder er\u00f6ffnen", "tokens": ["Wer", "ist", "der", "wie\u00b7der", "er\u00b7\u00f6ff\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mir wieder entschlie\u00dfen sie mag?", "tokens": ["Mir", "wie\u00b7der", "ent\u00b7schlie\u00b7\u00dfen", "sie", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "VMFIN", "$."], "meter": "-+--+--+",