{"textgrid.poem.52143": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gl\u00f6ckner dieser Welt, es wird nichts guts bedeuten,", "tokens": ["Der", "Gl\u00f6ck\u00b7ner", "die\u00b7ser", "Welt", ",", "es", "wird", "nichts", "guts", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00e4ngt die gego\u00dfne Glock in Welschland an zu la\u00fcten:", "tokens": ["F\u00e4ngt", "die", "ge\u00b7go\u00df\u00b7ne", "Glock", "in", "Wel\u00b7schland", "an", "zu", "la\u00fc\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nu Campanella tr\u00e4gt den Klang in Franckreich feil,", "tokens": ["Nu", "Cam\u00b7pa\u00b7nel\u00b7la", "tr\u00e4gt", "den", "Klang", "in", "Fran\u00b7ck\u00b7reich", "feil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ART", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Und der gefrorne Belt kriegt auch davon sein Theil.", "tokens": ["Und", "der", "ge\u00b7fror\u00b7ne", "Belt", "kriegt", "auch", "da\u00b7von", "sein", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Vater, Oesterreich und Spanien ingleichen", "tokens": ["O", "Va\u00b7ter", ",", "O\u00b7es\u00b7ter\u00b7reich", "und", "Spa\u00b7ni\u00b7en", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NE", "KON", "NE", "NE"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hat Euch von Alters her die H\u00e4nde helffen reichen,", "tokens": ["Hat", "Euch", "von", "Al\u00b7ters", "her", "die", "H\u00e4n\u00b7de", "helf\u00b7fen", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APZR", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O la\u00fctet ihnen nicht, der Klang m\u00f6cht euch beth\u00f6rn,", "tokens": ["O", "la\u00fc\u00b7tet", "ih\u00b7nen", "nicht", ",", "der", "Klang", "m\u00f6cht", "euch", "be\u00b7th\u00f6rn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr w\u00fcst, wie Carl der F\u00fcnfft euch konte beten lehrn.", "tokens": ["Ihr", "w\u00fcst", ",", "wie", "Carl", "der", "F\u00fcnfft", "euch", "kon\u00b7te", "be\u00b7ten", "lehrn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "ART", "NN", "PPER", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Gl\u00f6ckner dieser Welt, es wird nichts guts 