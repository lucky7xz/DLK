{"textgrid.poem.46157": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung an etliche canzleiherren", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr herren, damit ich ja euch", "tokens": ["Ihr", "her\u00b7ren", ",", "da\u00b7mit", "ich", "ja", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nenn eben gleich", "tokens": ["nenn", "e\u00b7ben", "gleich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "wie g\u00fcnstig ihr euch selbs intitulieret,", "tokens": ["wie", "g\u00fcns\u00b7tig", "ihr", "euch", "selbs", "in\u00b7ti\u00b7tu\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ihr, deren grob verderbtes blut", "tokens": ["ihr", ",", "de\u00b7ren", "grob", "ver\u00b7derb\u00b7tes", "blut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJD", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "sich, gleichsam ab des fiebers wut,", "tokens": ["sich", ",", "gleich\u00b7sam", "ab", "des", "fie\u00b7bers", "wut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ab meiner schrift erhitzet und gefrieret.", "tokens": ["ab", "mei\u00b7ner", "schrift", "er\u00b7hit\u00b7zet", "und", "ge\u00b7frie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr mischet teutsch, welsch und latein,", "tokens": ["Ihr", "mi\u00b7schet", "teutsch", ",", "welsch", "und", "la\u00b7tein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "doch keines rein,", "tokens": ["doch", "kei\u00b7nes", "rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "weil eure kunst ihr nicht gern wolt verhehlen,", "tokens": ["weil", "eu\u00b7re", "kunst", "ihr", "nicht", "gern", "wolt", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und sprechet mit zu weiser schmach,", "tokens": ["und", "spre\u00b7chet", "mit", "zu", "wei\u00b7ser", "schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df ich verderb die teutsche sprach,", "tokens": ["da\u00df", "ich", "ver\u00b7derb", "die", "teut\u00b7sche", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "weil fremde wort ich nicht, wie ihr, mag qu\u00e4len.", "tokens": ["weil", "frem\u00b7de", "wort", "ich", "nicht", ",", "wie", "ihr", ",", "mag", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar wan man ja welsch reden soll,", "tokens": ["Zwar", "wan", "man", "ja", "welsch", "re\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PIS", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "so m\u00fcst ihr wol,", "tokens": ["so", "m\u00fcst", "ihr", "wol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "da\u00df besser ich, dan ihr, es red, gestehen;", "tokens": ["da\u00df", "bes\u00b7ser", "ich", ",", "dan", "ihr", ",", "es", "red", ",", "ge\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "$,", "ADV", "PPER", "$,", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "kan also auch ein blinder tropf", "tokens": ["kan", "al\u00b7so", "auch", "ein", "blin\u00b7der", "tropf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "nicht so vil witz in euerm kopf", "tokens": ["nicht", "so", "vil", "witz", "in", "eu\u00b7erm", "kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "als neid und ha\u00df in euern herzen sehen.", "tokens": ["als", "neid", "und", "ha\u00df", "in", "eu\u00b7ern", "her\u00b7zen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVIMP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Demnach dan euers hirns gefahr", "tokens": ["Dem\u00b7nach", "dan", "eu\u00b7ers", "hirns", "ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so offenbar,", "tokens": ["so", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "warum solt ich in versen euch bedenken?", "tokens": ["wa\u00b7rum", "solt", "ich", "in", "ver\u00b7sen", "euch", "be\u00b7den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "w\u00e4r ich nicht kr\u00e4nker selbs, dan ihr,", "tokens": ["w\u00e4r", "ich", "nicht", "kr\u00e4n\u00b7ker", "selbs", ",", "dan", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "ADV", "$,", "ADV", "PPER", "$,"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "und auch ein vernunftloses thier,", "tokens": ["und", "auch", "ein", "ver\u00b7nunft\u00b7lo\u00b7ses", "thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "wan ich euch wolt mit schriften mehr bekr\u00e4nken?", "tokens": ["wan", "ich", "euch", "wolt", "mit", "schrif\u00b7ten", "mehr", "be\u00b7kr\u00e4n\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "APPR", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nein. Euer argwohn ist umsunst", "tokens": ["Nein", ".", "Eu\u00b7er", "arg\u00b7wohn", "ist", "um\u00b7sunst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und nur ein dunst,", "tokens": ["und", "nur", "ein", "dunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "der euch das hirn, so vorhin schwach, verletzet.", "tokens": ["der", "euch", "das", "hirn", ",", "so", "vor\u00b7hin", "schwach", ",", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "ADV", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ich w\u00e4r wie ihr, wan ich die hand,", "tokens": ["ich", "w\u00e4r", "wie", "ihr", ",", "wan", "ich", "die", "hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "f\u00fcr oder wider eure schand", "tokens": ["f\u00fcr", "o\u00b7der", "wi\u00b7der", "eu\u00b7re", "schand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "APPR", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zu schreiben, nur auf das papier gesetzet.", "tokens": ["zu", "schrei\u00b7ben", ",", "nur", "auf", "das", "pa\u00b7pier", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Dan w\u00fcrden alle weisen nicht", "tokens": ["Dan", "w\u00fcr\u00b7den", "al\u00b7le", "wei\u00b7sen", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bald das gedicht,", "tokens": ["bald", "das", "ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "das euch fuchsschw\u00e4nzen wolt, verlachen?", "tokens": ["das", "euch", "fuchs\u00b7schw\u00e4n\u00b7zen", "wolt", ",", "ver\u00b7la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wie dan euch schelten, w\u00e4r auch kaum", "tokens": ["wie", "dan", "euch", "schel\u00b7ten", ",", "w\u00e4r", "auch", "kaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ein weisers werk, dan einen baum,", "tokens": ["ein", "wei\u00b7sers", "werk", ",", "dan", "ei\u00b7nen", "baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der d\u00fcrr und faul, noch fruchtbar wollen machen.", "tokens": ["der", "d\u00fcrr", "und", "faul", ",", "noch", "frucht\u00b7bar", "wol\u00b7len", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wan ich die zeit schadlos vertreib", "tokens": ["Wan", "ich", "die", "zeit", "schad\u00b7los", "ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "und fr\u00f6lich schreib,", "tokens": ["und", "fr\u00f6\u00b7lich", "schreib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "so schreib ich doch nicht an, f\u00fcr, noch von allen,", "tokens": ["so", "schreib", "ich", "doch", "nicht", "an", ",", "f\u00fcr", ",", "noch", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,", "APPR", "$,", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und meine vers, kunstreich und wert,", "tokens": ["und", "mei\u00b7ne", "vers", ",", "kuns\u00b7treich", "und", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die sollen denen, die gelehrt,", "tokens": ["die", "sol\u00b7len", "de\u00b7nen", ",", "die", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "PDS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und nur, hoff ich, verst\u00e4ndigen gefallen.", "tokens": ["und", "nur", ",", "hoff", "ich", ",", "ver\u00b7st\u00e4n\u00b7di\u00b7gen", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Zu k\u00f6stlich und zu rein und frisch", "tokens": ["Zu", "k\u00f6st\u00b7lich", "und", "zu", "rein", "und", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "KON", "PTKA", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr euern tisch", "tokens": ["f\u00fcr", "eu\u00b7ern", "tisch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und magen seind die trachten meiner schriften;", "tokens": ["und", "ma\u00b7gen", "seind", "die", "trach\u00b7ten", "mei\u00b7ner", "schrif\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "den bauren taugt ein hafenk\u00e4s,", "tokens": ["den", "bau\u00b7ren", "taugt", "ein", "ha\u00b7fen\u00b7k\u00e4s", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die pomeranzen seind zu r\u00e4\u00df,", "tokens": ["die", "po\u00b7me\u00b7ran\u00b7zen", "seind", "zu", "r\u00e4\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "damit sie sich wol f\u00f6rchten zu vergiften.", "tokens": ["da\u00b7mit", "sie", "sich", "wol", "f\u00f6rch\u00b7ten", "zu", "ver\u00b7gif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich will nicht die torechte m\u00fch,", "tokens": ["Ich", "will", "nicht", "die", "to\u00b7rech\u00b7te", "m\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so ich alhie,", "tokens": ["so", "ich", "al\u00b7hie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "jemals von euch zu schreiben ferners haben;", "tokens": ["je\u00b7mals", "von", "euch", "zu", "schrei\u00b7ben", "fer\u00b7ners", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKZU", "VVINF", "ADV", "VAFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "darum so gebt euch nu zu ruh,", "tokens": ["da\u00b7rum", "so", "gebt", "euch", "nu", "zu", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ich sag euch bei den Musen zu:", "tokens": ["ich", "sag", "euch", "bei", "den", "Mu\u00b7sen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": 