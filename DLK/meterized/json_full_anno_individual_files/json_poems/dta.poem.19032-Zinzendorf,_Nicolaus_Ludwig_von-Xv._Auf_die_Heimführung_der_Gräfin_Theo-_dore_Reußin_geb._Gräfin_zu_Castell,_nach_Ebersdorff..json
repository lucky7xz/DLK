{"dta.poem.19032": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  Auf die Heimf\u00fchrung der Gr\u00e4fin Theo-  \n dore Reu\u00dfin geb. Gr\u00e4fin zu Castell,  \n nach Ebersdorff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Bruder, g\u00f6nne mir, da\u00df sich von ferne her", "tokens": ["Mein", "Bru\u00b7der", ",", "g\u00f6n\u00b7ne", "mir", ",", "da\u00df", "sich", "von", "fer\u00b7ne", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PRF", "APPR", "ADV", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch eines Freundes Hertz mit deinem Geist ver binde,", "tokens": ["Noch", "ei\u00b7nes", "Freun\u00b7des", "Hertz", "mit", "dei\u00b7nem", "Geist", "ver", "bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dich mit einem Blatt voll lautrer Liebe ehr:", "tokens": ["Und", "dich", "mit", "ei\u00b7nem", "Blatt", "voll", "laut\u00b7rer", "Lie\u00b7be", "ehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es kommt nicht eher an, damit es Musse finde.", "tokens": ["Es", "kommt", "nicht", "e\u00b7her", "an", ",", "da\u00b7mit", "es", "Mus\u00b7se", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So hat dich Ebersdorff nun wiederum erlangt,", "tokens": ["So", "hat", "dich", "E\u00b7bers\u00b7dorff", "nun", "wie\u00b7de\u00b7rum", "er\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und schlie\u00dft dich feste ein mit deiner Theodoren;", "tokens": ["Und", "schlie\u00dft", "dich", "fes\u00b7te", "ein", "mit", "dei\u00b7ner", "Theo\u00b7do\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dein liebes ", "tokens": ["Dein", "lie\u00b7bes"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Erfreut sich, da\u00df dir GOtt das Beste selbst erkohren.", "tokens": ["Er\u00b7freut", "sich", ",", "da\u00df", "dir", "Gott", "das", "Bes\u00b7te", "selbst", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Und jeder Redliche; ein Danck-Lied beyzuf\u00fcgen.", "tokens": ["Und", "je\u00b7der", "Red\u00b7li\u00b7che", ";", "ein", "Dan\u00b7ck\u00b7Lied", "bey\u00b7zu\u00b7f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Es fehlet auch gewi\u00df an guten W\u00fcnschen nicht;", "tokens": ["Es", "feh\u00b7let", "auch", "ge\u00b7wi\u00df", "an", "gu\u00b7ten", "W\u00fcn\u00b7schen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und wenn du beten hilffst, GOtt wird sie alle h\u00f6ren:", "tokens": ["Und", "wenn", "du", "be\u00b7ten", "hilffst", ",", "Gott", "wird", "sie", "al\u00b7le", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VVFIN", "$,", "NN", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich leistete vorhin die br\u00fcderliche Pflicht,", "tokens": ["Ich", "leis\u00b7te\u00b7te", "vor\u00b7hin", "die", "br\u00fc\u00b7der\u00b7li\u00b7che", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ietzo soll dis Wort auch seinen Leser lehren.", "tokens": ["Und", "iet\u00b7zo", "soll", "dis", "Wort", "auch", "sei\u00b7nen", "Le\u00b7ser", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PDS", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es war das Lobe-Lied der Sache gantz gem\u00e4\u00df,", "tokens": ["Es", "war", "das", "Lo\u00b7be\u00b7Lied", "der", "Sa\u00b7che", "gantz", "ge\u00b7m\u00e4\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Darinn ich zu Castell die Wunder GOttes priese,", "tokens": ["Da\u00b7rinn", "ich", "zu", "Cas\u00b7tell", "die", "Wun\u00b7der", "Got\u00b7tes", "prie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "NE", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Der eine tadelte, der andre r\u00fchmte es,", "tokens": ["Der", "ei\u00b7ne", "ta\u00b7del\u00b7te", ",", "der", "and\u00b7re", "r\u00fchm\u00b7te", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Womit sich der Verstand von solchen Sachen wiese.", "tokens": ["Wo\u00b7mit", "sich", "der", "Ver\u00b7stand", "von", "sol\u00b7chen", "Sa\u00b7chen", "wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indessen weil es doch noch Kinder GOttes hat,", "tokens": ["In\u00b7des\u00b7sen", "weil", "es", "doch", "noch", "Kin\u00b7der", "Got\u00b7tes", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die ihres Vaters Hand zu k\u00fcssen sich bequemen;", "tokens": ["Die", "ih\u00b7res", "Va\u00b7ters", "Hand", "zu", "k\u00fcs\u00b7sen", "sich", "be\u00b7que\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So wiederhole ichs mit wohlbedachtem Rath,", "tokens": ["So", "wie\u00b7der\u00b7ho\u00b7le", "ichs", "mit", "wohl\u00b7be\u00b7dach\u00b7tem", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}