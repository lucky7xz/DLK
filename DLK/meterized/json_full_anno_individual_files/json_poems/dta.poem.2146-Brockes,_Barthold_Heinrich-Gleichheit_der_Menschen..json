{"dta.poem.2146": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gleichheit der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, was die Stimme der Natur zu einem jeden Men-", "tokens": ["H\u00f6rt", ",", "was", "die", "Stim\u00b7me", "der", "Na\u00b7tur", "zu", "ei\u00b7nem", "je\u00b7den", "Men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "ART", "NN", "APPR", "ART", "PIAT", "TRUNC"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "schen spricht:", "tokens": ["schen", "spricht", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Verachte niemand, auch sogar den aller\u00e4rmsten Sklaven", "tokens": ["Ver\u00b7ach\u00b7te", "nie\u00b7mand", ",", "auch", "so\u00b7gar", "den", "al\u00b7le\u00b7r\u00e4rms\u00b7ten", "Skla\u00b7ven"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "nicht,", "tokens": ["nicht", ","], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Er ist sowohl als du ein Glied an meiner Kette. Bey-", "tokens": ["Er", "ist", "so\u00b7wohl", "als", "du", "ein", "Glied", "an", "mei\u00b7ner", "Ket\u00b7te", ".", "Bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "der Ende", "tokens": ["der", "En\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ist, so wie euer Ursprung, gleich. Jhr lebet beyde,", "tokens": ["Ist", ",", "so", "wie", "eu\u00b7er", "Ur\u00b7sprung", ",", "gleich", ".", "Ihr", "le\u00b7bet", "bey\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$,", "ADV", "$.", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gottes Willen", "tokens": ["Got\u00b7tes", "Wil\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und seine Absicht zu erf\u00fcllen.", "tokens": ["Und", "sei\u00b7ne", "Ab\u00b7sicht", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Unterthan und der Monarch, die werden endlich", "tokens": ["Der", "Un\u00b7ter\u00b7than", "und", "der", "Mon\u00b7arch", ",", "die", "wer\u00b7den", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "beyde gleich,", "tokens": ["bey\u00b7de", "gleich", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Und die Verwesung schlinget beyde, so bald ihr in dem", "tokens": ["Und", "die", "Ver\u00b7we\u00b7sung", "schlin\u00b7get", "bey\u00b7de", ",", "so", "bald", "ihr", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "$,", "ADV", "ADV", "PPER", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Todtenreich,", "tokens": ["Tod\u00b7ten\u00b7reich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Ohn Ansehn eines Ranges ein.", "tokens": ["Ohn", "An\u00b7sehn", "ei\u00b7nes", "Ran\u00b7ges", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Was hat ein Sterblicher vor Recht, so \u00fcberm\u00fcthig stolz", "tokens": ["Was", "hat", "ein", "Sterb\u00b7li\u00b7cher", "vor", "Recht", ",", "so", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "zu seyn?", "tokens": ["zu", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$."], "meter": "-+", "measure": "iambic.single"}}}}}