{"textgrid.poem.48945": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Creatur ist sch\u00f6n/ noch sch\u00f6ner ist mein Freund/", "tokens": ["Die", "Crea\u00b7tur", "ist", "sch\u00f6n", "/", "noch", "sch\u00f6\u00b7ner", "ist", "mein", "Freund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "So geht mirs manches mal/ bleib ich nicht bey dem Einen/", "tokens": ["So", "geht", "mirs", "man\u00b7ches", "mal", "/", "bleib", "ich", "nicht", "bey", "dem", "Ei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PIS", "ADV", "$(", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ART", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Jesus selber ist/ so hab ich nichts als Pein/", "tokens": ["Das", "Je\u00b7sus", "sel\u00b7ber", "ist", "/", "so", "hab", "ich", "nichts", "als", "Pein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan aber seine Lieb in mir alleine scheinen/", "tokens": ["Kan", "a\u00b7ber", "sei\u00b7ne", "Lieb", "in", "mir", "al\u00b7lei\u00b7ne", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So pflegt er/ wie er ist/ auff einmal all's zu seyn.", "tokens": ["So", "pflegt", "er", "/", "wie", "er", "ist", "/", "auff", "ein\u00b7mal", "all's", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VAFIN", "$(", "APPR", "ADV", "PIS", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kurtz/ Vielheit ist mein Schad/ das Eine meine Ruh/", "tokens": ["Kurtz", "/", "Viel\u00b7heit", "ist", "mein", "Schad", "/", "das", "Ei\u00b7ne", "mei\u00b7ne", "Ruh", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "NN", "VAFIN", "PPOSAT", "NN", "$(", "PDS", "ART", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Welt hat eine Sonn/ mein Leib ein Hertz und Seele:", "tokens": ["Die", "Welt", "hat", "ei\u00b7ne", "Sonn", "/", "mein", "Leib", "ein", "Hertz", "und", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was h\u00e4lt mich/ da\u00df ich nicht gleich lauff dem Einen zu?", "tokens": ["Was", "h\u00e4lt", "mich", "/", "da\u00df", "ich", "nicht", "gleich", "lauff", "dem", "Ei\u00b7nen", "zu", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], 