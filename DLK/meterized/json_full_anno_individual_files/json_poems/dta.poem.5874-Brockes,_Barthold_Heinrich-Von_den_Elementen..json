{"dta.poem.5874": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch nicht geschickt, da\u00df er, was wahr ist, lehrt:", "tokens": ["Auch", "nicht", "ge\u00b7schickt", ",", "da\u00df", "er", ",", "was", "wahr", "ist", ",", "lehrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "$,", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und durch verwirrte Wort\u2019 allein sind wir beth\u00f6rt.", "tokens": ["Und", "durch", "ver\u00b7wirr\u00b7te", "Wort'", "al\u00b7lein", "sind", "wir", "be\u00b7th\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was man nun sonst zu ", "tokens": ["Was", "man", "nun", "sonst", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "ADV", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Gerad und Ungerad, die Zahl, die Harmonie,", "tokens": ["Ge\u00b7rad", "und", "Un\u00b7ge\u00b7rad", ",", "die", "Zahl", ",", "die", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die k\u00f6nnen gleichfals nicht daf\u00fcr gerechnet seyn,", "tokens": ["Die", "k\u00f6n\u00b7nen", "gleich\u00b7fals", "nicht", "da\u00b7f\u00fcr", "ge\u00b7rech\u00b7net", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PTKNEG", "PAV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem, da\u00df sie", "tokens": ["In\u00b7dem", ",", "da\u00df", "sie"], "token_info": ["word", "punct", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nur gar zu sehr vertiefft, zu dunckel und zu fein.", "tokens": ["Nur", "gar", "zu", "sehr", "ver\u00b7tiefft", ",", "zu", "dun\u00b7ckel", "und", "zu", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADV", "VVPP", "$,", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}