{"textgrid.poem.43836": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Bl\u00fcmlein Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In unsers Vaters Garten", "tokens": ["In", "un\u00b7sers", "Va\u00b7ters", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da war's noch gestern gr\u00fcn,", "tokens": ["Da", "wa\u00b7r's", "noch", "ge\u00b7stern", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sah ich noch so mancherlei,", "tokens": ["Da", "sah", "ich", "noch", "so", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6ne Blumen bl\u00fch'n.", "tokens": ["So", "sch\u00f6\u00b7ne", "Blu\u00b7men", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und heut ist Alles anders,", "tokens": ["Und", "heut", "ist", "Al\u00b7les", "an\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und heut ist Alles todt:", "tokens": ["Und", "heut", "ist", "Al\u00b7les", "todt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo seid ihr hin, ihr Bl\u00fcmelein,", "tokens": ["Wo", "seid", "ihr", "hin", ",", "ihr", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Bl\u00fcmlein gelb und roth?", "tokens": ["Ihr", "Bl\u00fcm\u00b7lein", "gelb", "und", "roth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbo liebes Kind, wir schlafen", "tokens": ["\u00bb", "o", "lie\u00b7bes", "Kind", ",", "wir", "schla\u00b7fen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Gottes Willen hier,", "tokens": ["Nach", "Got\u00b7tes", "Wil\u00b7len", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis Er uns seinen Fr\u00fchling schickt,", "tokens": ["Bis", "Er", "uns", "sei\u00b7nen", "Fr\u00fch\u00b7ling", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann erwachen wir.", "tokens": ["Und", "dann", "er\u00b7wa\u00b7chen", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja, deine Bl\u00fcmlein schlafen:", "tokens": ["Ja", ",", "dei\u00b7ne", "Bl\u00fcm\u00b7lein", "schla\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wirst auch schlafen du,", "tokens": ["So", "wirst", "auch", "schla\u00b7fen", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis dich erweckt ein Fr\u00fchlingstag", "tokens": ["Bis", "dich", "er\u00b7weckt", "ein", "Fr\u00fch\u00b7lings\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus deiner langen Ruh.", "tokens": ["Aus", "dei\u00b7ner", "lan\u00b7gen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn du dann erwachest,", "tokens": ["Und", "wenn", "du", "dann", "er\u00b7wa\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O m\u00f6chtest du dann sein", "tokens": ["O", "m\u00f6ch\u00b7test", "du", "dann", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So heiter und so fr\u00fchlingsfroh", "tokens": ["So", "hei\u00b7ter", "und", "so", "fr\u00fch\u00b7lings\u00b7froh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie deine Bl\u00fcmelein!\u00ab", "tokens": ["Wie", "dei\u00b7ne", "Bl\u00fc\u00b7me\u00b7lein", "!", "\u00ab"], "toke