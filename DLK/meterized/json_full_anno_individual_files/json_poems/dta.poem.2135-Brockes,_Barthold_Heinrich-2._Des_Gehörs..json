{"dta.poem.2135": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Des Geh\u00f6rs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein, durch der hellen V\u00f6gel Ch\u00f6re,", "tokens": ["Mein", ",", "durch", "der", "hel\u00b7len", "V\u00f6\u00b7gel", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem empfindenden Geh\u00f6re,", "tokens": ["In", "dem", "emp\u00b7fin\u00b7den\u00b7den", "Ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ger\u00fchrtes und vergn\u00fcgtes Herz,", "tokens": ["Ge\u00b7r\u00fchr\u00b7tes", "und", "ver\u00b7gn\u00fcg\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bet\u00e4ubt durch ihren s\u00fc\u00dfen Scherz,", "tokens": ["Be\u00b7t\u00e4ubt", "durch", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denkt, \u00fcberleget und erw\u00e4get", "tokens": ["Denkt", ",", "\u00fc\u00b7ber\u00b7le\u00b7get", "und", "er\u00b7w\u00e4\u00b7get"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Weisheit, welche solchen Klang,", "tokens": ["Die", "Weis\u00b7heit", ",", "wel\u00b7che", "sol\u00b7chen", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und herzentz\u00fcckenden Gesang,", "tokens": ["Und", "her\u00b7zent\u00b7z\u00fc\u00b7cken\u00b7den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In solcher Thierchen Schn\u00e4bel leget.", "tokens": ["In", "sol\u00b7cher", "Thier\u00b7chen", "Schn\u00e4\u00b7bel", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der solche schnelle Dehnungskraft,", "tokens": ["Der", "sol\u00b7che", "schnel\u00b7le", "Deh\u00b7nungs\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So lieblichschallend\u2019 Eigenschaft", "tokens": ["So", "lieb\u00b7lich\u00b7schal\u00b7lend'", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den d\u00fcnnen L\u00fcften eingepr\u00e4get.", "tokens": ["Den", "d\u00fcn\u00b7nen", "L\u00fcf\u00b7ten", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der uns zu dem Genu\u00df erkohren,", "tokens": ["Der", "uns", "zu", "dem", "Ge\u00b7nu\u00df", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und uns das Werkzeug unsrer Ohren", "tokens": ["Und", "uns", "das", "Werk\u00b7zeug", "uns\u00b7rer", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dazu formiret und geschenkt,", "tokens": ["Da\u00b7zu", "for\u00b7mi\u00b7ret", "und", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Wodurch sich, sonder alle M\u00fch,", "tokens": ["Wo\u00b7durch", "sich", ",", "son\u00b7der", "al\u00b7le", "M\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die liederreiche Harmonie", "tokens": ["Die", "lie\u00b7der\u00b7rei\u00b7che", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "In die ger\u00fchrte Seele senkt.", "tokens": ["In", "die", "ge\u00b7r\u00fchr\u00b7te", "See\u00b7le", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Will man nun recht, und menschlich h\u00f6ren;", "tokens": ["Will", "man", "nun", "recht", ",", "und", "menschlich", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "$,", "KON", "ADJD", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Mu\u00df man des Wohllauts Ursprung ehren.", "tokens": ["Mu\u00df", "man", "des", "Wohl\u00b7lauts", "Ur\u00b7sprung", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}