rd", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.105": {"text": "\u201egro\u00dfes Unrecht thatet Ihr,", "tokens": ["\u201e", "gro\u00b7\u00dfes", "Un\u00b7recht", "tha\u00b7tet", "Ihr", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.106": {"text": "Euer war ich ohne Wanken,", "tokens": ["Eu\u00b7er", "war", "ich", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.107": {"text": "Aber solch ein Herz genie\u00dfen", "tokens": ["A\u00b7ber", "solch", "ein", "Herz", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.108": {"text": "Wendet leichtlich die Gedanken.", "tokens": ["Wen\u00b7det", "leicht\u00b7lich", "die", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.109": {"text": "Manches tritt mir vor die Seele,", "tokens": ["Man\u00b7ches", "tritt", "mir", "vor", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.110": {"text": "Was vorl\u00e4ngst die Lieder sangen,", "tokens": ["Was", "vor\u00b7l\u00e4ngst", "die", "Lie\u00b7der", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.111": {"text": "Der mir lebend fremd geblieben,", "tokens": ["Der", "mir", "le\u00b7bend", "fremd", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.112": {"text": "Hat als Todter mich befangen.", "tokens": ["Hat", "als", "Tod\u00b7ter", "mich", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.113": {"text": "Ja! ich bin dem Tod geweihet,", "tokens": ["Ja", "!", "ich", "bin", "dem", "Tod", "ge\u00b7wei\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.114": {"text": "Jedes Mahl ist mir verwehret,", "tokens": ["Je\u00b7des", "Mahl", "ist", "mir", "ver\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.115": {"text": "Nicht geziemt mir andre Speise", "tokens": ["Nicht", "ge\u00b7ziemt", "mir", "and\u00b7re", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.116": {"text": "Seit mich dieses Herz gen\u00e4hret.", "tokens": ["Seit", "mich", "die\u00b7ses", "Herz", "ge\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.117": {"text": "Aber Euch w\u00fcnsch\u2019 ich zum Letzten", "tokens": ["A\u00b7ber", "Euch", "w\u00fcn\u00b7sch'", "ich", "zum", "Letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.118": {"text": "Milden Spruch des ew\u2019gen Richters.\u201c \u2014", "tokens": ["Mil\u00b7den", "Spruch", "des", "ew'\u00b7gen", "Rich\u00b7ters", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.119": {"text": "Dieses alles ist geschehen", "tokens": ["Die\u00b7ses", "al\u00b7les", "ist", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.120": {"text": "Mit dem Herzen eines Dichters.", "tokens": ["Mit", "dem", "Her\u00b7zen", "ei\u00b7nes", "Dich\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}