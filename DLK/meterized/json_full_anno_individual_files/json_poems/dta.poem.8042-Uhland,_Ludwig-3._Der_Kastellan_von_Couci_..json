{"dta.poem.8042": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.  Der Kastellan von Couci .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.85", "nl:0.14"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wie der Kastellan von Couci", "tokens": ["Wie", "der", "Kas\u00b7tel\u00b7lan", "von", "Cou\u00b7ci"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnell die Hand zum Herzen dr\u00fcckte,", "tokens": ["Schnell", "die", "Hand", "zum", "Her\u00b7zen", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Dame von Favel", "tokens": ["Als", "die", "Da\u00b7me", "von", "Fa\u00b7vel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Er zum ersten Mal erblickte!", "tokens": ["Er", "zum", "ers\u00b7ten", "Mal", "er\u00b7blick\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seit demselben Augenblicke", "tokens": ["Seit", "dem\u00b7sel\u00b7ben", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drang durch alle seine Lieder,", "tokens": ["Drang", "durch", "al\u00b7le", "sei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter allen Weisen stets", "tokens": ["Un\u00b7ter", "al\u00b7len", "Wei\u00b7sen", "stets"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jener erste Herzschlag wieder.", "tokens": ["Je\u00b7ner", "ers\u00b7te", "Herz\u00b7schlag", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber wenig mocht\u2019 ihm frommen", "tokens": ["A\u00b7ber", "we\u00b7nig", "mocht'", "ihm", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "All die s\u00fc\u00dfe Liederklage,", "tokens": ["All", "die", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nimmer darf er dieses hoffen,", "tokens": ["Nim\u00b7mer", "darf", "er", "die\u00b7ses", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df sein Herz an ihrem schlage.", "tokens": ["Da\u00df", "sein", "Herz", "an", "ih\u00b7rem", "schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wenn sie auch mit zartem Sinn", "tokens": ["Wenn", "sie", "auch", "mit", "zar\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Eines sch\u00f6nen Lieds sich freute,", "tokens": ["Ei\u00b7nes", "sch\u00f6\u00b7nen", "Lieds", "sich", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Streng und stille ging sie immer", "tokens": ["Streng", "und", "stil\u00b7le", "ging", "sie", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.16": {"text": "An des stolzen Gatten Seite.", "tokens": ["An", "des", "stol\u00b7zen", "Gat\u00b7ten", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Da beschlie\u00dft der Kastellan,", "tokens": ["Da", "be\u00b7schlie\u00dft", "der", "Kas\u00b7tel\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Seine Brust in Stahl zu h\u00fcllen", "tokens": ["Sei\u00b7ne", "Brust", "in", "Stahl", "zu", "h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und mit drauf geheft\u2019tem Kreutz", "tokens": ["Und", "mit", "drauf", "ge\u00b7heft'\u00b7tem", "Kreutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PAV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Seines Herzens Schlag zu stillen.", "tokens": ["Sei\u00b7nes", "Her\u00b7zens", "Schlag", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Als er schon im heil\u2019gen Lande", "tokens": ["Als", "er", "schon", "im", "heil'\u00b7gen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Manchen hei\u00dfen Tag gestritten,", "tokens": ["Man\u00b7chen", "hei\u00b7\u00dfen", "Tag", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "F\u00e4hrt ein Pfeil durch Kreutz und Panzer,", "tokens": ["F\u00e4hrt", "ein", "Pfeil", "durch", "Kreutz", "und", "Pan\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Trifft ihm noch das Herze mitten.", "tokens": ["Trifft", "ihm", "noch", "das", "Her\u00b7ze", "mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "\u201eh\u00f6rst du mich, getreuer Knappe?", "tokens": ["\u201e", "h\u00f6rst", "du", "mich", ",", "ge\u00b7treu\u00b7er", "Knap\u00b7pe", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Wann dies Herz nun ausgeschlagen,", "tokens": ["Wann", "dies", "Herz", "nun", "aus\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Zu der Dame von Fayel", "tokens": ["Zu", "der", "Da\u00b7me", "von", "Fa\u00b7yel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Sollt du es hin\u00fcbertragen!\u201c", "tokens": ["Sollt", "du", "es", "hin\u00b7\u00fc\u00b7ber\u00b7tra\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "In geweihter, k\u00fchler Erde", "tokens": ["In", "ge\u00b7weih\u00b7ter", ",", "k\u00fch\u00b7ler", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Wird der edle Leib begraben;", "tokens": ["Wird", "der", "ed\u00b7le", "Leib", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Nur das Herz, das m\u00fcde Herz,", "tokens": ["Nur", "das", "Herz", ",", "das", "m\u00fc\u00b7de", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Soll noch keine Ruhe haben.", "tokens": ["Soll", "noch", "kei\u00b7ne", "Ru\u00b7he", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Schon in einer goldnen Urne", "tokens": ["Schon", "in", "ei\u00b7ner", "gold\u00b7nen", "Ur\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Liegt es, wohl einbalsamiret,", "tokens": ["Liegt", "es", ",", "wohl", "ein\u00b7bal\u00b7sa\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und zu Schiffe steigt der Diener,", "tokens": ["Und", "zu", "Schif\u00b7fe", "steigt", "der", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Der es sorgsam mit sich f\u00fchret.", "tokens": ["Der", "es", "sorg\u00b7sam", "mit", "sich", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "St\u00fcrme brausen, Wogen schlagen,", "tokens": ["St\u00fcr\u00b7me", "brau\u00b7sen", ",", "Wo\u00b7gen", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Blitze zucken, Maste splittern,", "tokens": ["Blit\u00b7ze", "zu\u00b7cken", ",", "Mas\u00b7te", "split\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Aengstlich klopfen alle Herzen,", "tokens": ["A\u00b7engst\u00b7lich", "klop\u00b7fen", "al\u00b7le", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Eines nur ist ohne Zittern.", "tokens": ["Ei\u00b7nes", "nur", "ist", "oh\u00b7ne", "Zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Golden stralt die Sonne wieder,", "tokens": ["Gol\u00b7den", "stralt", "die", "Son\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Frankreichs K\u00fcste gl\u00e4nzet dr\u00fcben,", "tokens": ["Fran\u00b7kreichs", "K\u00fcs\u00b7te", "gl\u00e4n\u00b7zet", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Freudig schlagen alle Herzen,", "tokens": ["Freu\u00b7dig", "schla\u00b7gen", "al\u00b7le", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Eines nur ist still geblieben.", "tokens": ["Ei\u00b7nes", "nur", "ist", "still", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Schon im Walde von Fayel", "tokens": ["Schon", "im", "Wal\u00b7de", "von", "Fa\u00b7yel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Schreitet rasch der Urne Tr\u00e4ger,", "tokens": ["Schrei\u00b7tet", "rasch", "der", "Ur\u00b7ne", "Tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Pl\u00f6tzlich schallt ein lustig Horn", "tokens": ["Pl\u00f6tz\u00b7lich", "schallt", "ein", "lus\u00b7tig", "Horn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Sammt dem Rufe wilder J\u00e4ger.", "tokens": ["Sammt", "dem", "Ru\u00b7fe", "wil\u00b7der", "J\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Aus den B\u00fcschen rauscht ein Hirsch,", "tokens": ["Aus", "den", "B\u00fc\u00b7schen", "rauscht", "ein", "Hirsch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Dem ein Pfeil im Herzen stecket,", "tokens": ["Dem", "ein", "Pfeil", "im", "Her\u00b7zen", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "B\u00e4umt sich auf und st\u00fcrzt und liegt", "tokens": ["B\u00e4umt", "sich", "auf", "und", "st\u00fcrzt", "und", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Vor dem Knappen hingestrecket.", "tokens": ["Vor", "dem", "Knap\u00b7pen", "hin\u00b7ge\u00b7stre\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Sieh! der Ritter von Fayel,", "tokens": ["Sieh", "!", "der", "Rit\u00b7ter", "von", "Fa\u00b7yel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.54": {"text": "Der das Wild in\u2019s Herz geschossen,", "tokens": ["Der", "das", "Wild", "in's", "Herz", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Sprengt heran mit Jagdgefolg", "tokens": ["Sprengt", "he\u00b7ran", "mit", "Jagd\u00b7ge\u00b7folg"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Und der Knapp\u2019 ist rings umschlossen.", "tokens": ["Und", "der", "Knapp'", "ist", "rings", "um\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.57": {"text": "Nach dem blanken Goldgef\u00e4\u00df", "tokens": ["Nach", "dem", "blan\u00b7ken", "Gold\u00b7ge\u00b7f\u00e4\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Tasten gleich des Ritters Knechte,", "tokens": ["Tas\u00b7ten", "gleich", "des", "Rit\u00b7ters", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Doch der Knappe tritt zur\u00fcck,", "tokens": 