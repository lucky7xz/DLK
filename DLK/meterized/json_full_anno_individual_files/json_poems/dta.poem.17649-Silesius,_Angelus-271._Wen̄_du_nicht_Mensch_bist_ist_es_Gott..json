{"dta.poem.17649": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "271.  Wen\u0304 du nicht Mensch bist/ ist es Gott.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du nicht Mensch mehr bist/ und dich verl\u00e4ugnet", "tokens": ["Wenn", "du", "nicht", "Mensch", "mehr", "bist", "/", "und", "dich", "ver\u00b7l\u00e4ug\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "ADV", "VAFIN", "$(", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ist Gott selber Mensch/ un\u0304 tr\u00e4get deine Last. (hast/", "tokens": ["So", "ist", "Gott", "sel\u00b7ber", "Mensch", "/", "un\u0304", "tr\u00e4\u00b7get", "dei\u00b7ne", "Last", ".", "(", "hast", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$.", "$(", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}