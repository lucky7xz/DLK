{"textgrid.poem.48836": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "61. An Dulcamaren", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kan ich ohne Ha\u00df, dich, ", "tokens": ["Wie", "kan", "ich", "oh\u00b7ne", "Ha\u00df", ",", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "du Bitters\u00fc\u00dfe du? Bald bist du gar zu gut,", "tokens": ["du", "Bit\u00b7ter\u00b7s\u00fc\u00b7\u00dfe", "du", "?", "Bald", "bist", "du", "gar", "zu", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$.", "ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bald, wenn ein schlechter Wahn ersteiget deinen Mut,", "tokens": ["bald", ",", "wenn", "ein", "schlech\u00b7ter", "Wahn", "er\u00b7stei\u00b7get", "dei\u00b7nen", "Mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so steht mein naher Tod an deiner Stirn geschrieben.", "tokens": ["so", "steht", "mein", "na\u00b7her", "Tod", "an", "dei\u00b7ner", "Stirn", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So lange 