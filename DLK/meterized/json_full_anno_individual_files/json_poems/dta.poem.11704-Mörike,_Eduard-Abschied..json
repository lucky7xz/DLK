{"dta.poem.11704": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich habe die Ehr', Ihr Recensent zu seyn.\u201c", "tokens": ["\u201e", "ich", "ha\u00b7be", "die", "Ehr'", ",", "Ihr", "Re\u00b7cen\u00b7sent", "zu", "seyn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VAINF", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sofort nimmt er das Licht in die Hand,", "tokens": ["So\u00b7fort", "nimmt", "er", "das", "Licht", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Besieht lang meinen Schatten an der Wand,", "tokens": ["Be\u00b7sieht", "lang", "mei\u00b7nen", "Schat\u00b7ten", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "R\u00fcckt nah und fern: \u201eNun, lieber junger Mann,", "tokens": ["R\u00fcckt", "nah", "und", "fern", ":", "\u201e", "Nun", ",", "lie\u00b7ber", "jun\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$.", "$(", "ADV", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sehn Sie doch gef\u00e4lligst 'mal Ihre Nas' so von der Seite an!", "tokens": ["Sehn", "Sie", "doch", "ge\u00b7f\u00e4l\u00b7ligst", "'", "mal", "Ih\u00b7re", "Nas", "'", "so", "von", "der", "Sei\u00b7te", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$(", "ADV", "PPOSAT", "NN", "$(", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Sie geben zu, da\u00df das ein Auswuchs is.\u201c", "tokens": ["Sie", "ge\u00b7ben", "zu", ",", "da\u00df", "das", "ein", "Aus\u00b7wuchs", "is", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PDS", "ART", "NN", "FM", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u2014 Das? Alle Wetter \u2014 gewi\u00df!", "tokens": ["Das", "?", "Al\u00b7le", "Wet\u00b7ter", "ge\u00b7wi\u00df", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PDS", "$.", "PIAT", "NN", "$(", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ei Hasen! ich dachte nicht,", "tokens": ["Ei", "Ha\u00b7sen", "!", "ich", "dach\u00b7te", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "All mein Lebtage nicht,", "tokens": ["All", "mein", "Leb\u00b7ta\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df ich so eine Welts-Nase f\u00fchrt' im Gesicht!!", "tokens": ["Da\u00df", "ich", "so", "ei\u00b7ne", "Welts\u00b7Na\u00b7se", "f\u00fchrt'", "im", "Ge\u00b7sicht", "!!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Mann sprach noch Zerschiednes hin und her,", "tokens": ["Der", "Mann", "sprach", "noch", "Zer\u00b7schied\u00b7nes", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NE", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df, auf meine Ehre, nicht mehr;", "tokens": ["Ich", "wei\u00df", ",", "auf", "mei\u00b7ne", "Eh\u00b7re", ",", "nicht", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Meinte vielleicht, ich sollt' ihm beichten.", "tokens": ["Mein\u00b7te", "viel\u00b7leicht", ",", "ich", "sollt'", "ihm", "beich\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zulezt stand er auf; ich that ihm leuchten.", "tokens": ["Zu\u00b7lezt", "stand", "er", "auf", ";", "ich", "that", "ihm", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Wie wir nun an der Treppe sind,", "tokens": ["Wie", "wir", "nun", "an", "der", "Trep\u00b7pe", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da geb' ich ihm, ganz froh gesinnt,", "tokens": ["Da", "geb'", "ich", "ihm", ",", "ganz", "froh", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Einen kleinen Tritt", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Tritt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Nur so von hinten auf's Ges\u00e4\u00dfe mit \u2014", "tokens": ["Nur", "so", "von", "hin\u00b7ten", "auf's", "Ge\u00b7s\u00e4\u00b7\u00dfe", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "APPRART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Alle Hagel! ward das ein Gerumpel,", "tokens": ["Al\u00b7le", "Ha\u00b7gel", "!", "ward", "das", "ein", "Ge\u00b7rum\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "VAFIN", "PDS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ein Gepurzel, ein Gehumpel!", "tokens": ["Ein", "Ge\u00b7pur\u00b7zel", ",", "ein", "Ge\u00b7hum\u00b7pel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dergleichen hab' ich nie gesehn,", "tokens": ["Derg\u00b7lei\u00b7chen", "hab'", "ich", "nie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "All mein Lebtage nicht gesehn", "tokens": ["All", "mein", "Leb\u00b7ta\u00b7ge", "nicht", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Einen Menschen so rasch die Trepp' hinab gehn!", "tokens": ["Ei\u00b7nen", "Men\u00b7schen", "so", "rasch", "die", "Trepp'", "hin\u00b7ab", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}