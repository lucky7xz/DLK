{"textgrid.poem.52269": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Trotz alledem!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war 'ne hei\u00dfe M\u00e4rzenzeit,", "tokens": ["Das", "war", "'ne", "hei\u00b7\u00dfe", "M\u00e4r\u00b7zen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz Regen, Schnee und alledem!", "tokens": ["Trotz", "Re\u00b7gen", ",", "Schnee", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun aber, da es Bl\u00fcten schneit,", "tokens": ["Nun", "a\u00b7ber", ",", "da", "es", "Bl\u00fc\u00b7ten", "schneit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun ist es kalt, trotz alledem!", "tokens": ["Nun", "ist", "es", "kalt", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem \u2013", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Wien, Berlin und alledem \u2013", "tokens": ["Trotz", "Wi\u00b7en", ",", "Ber\u00b7lin", "und", "al\u00b7le\u00b7dem", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "KON", "PIS", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ein schn\u00f6der scharfer Winterwind", "tokens": ["Ein", "schn\u00f6\u00b7der", "schar\u00b7fer", "Win\u00b7ter\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durchfr\u00f6stelt uns trotz alledem!", "tokens": ["Durch\u00b7fr\u00f6s\u00b7telt", "uns", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das ist der Wind der Reaktion", "tokens": ["Das", "ist", "der", "Wind", "der", "Re\u00b7ak\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Meltau, Reif und alledem!", "tokens": ["Mit", "Mel\u00b7tau", ",", "Reif", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist die Bourgeoisie am Thron \u2013", "tokens": ["Das", "ist", "die", "Bour\u00b7ge\u00b7o\u00b7i\u00b7sie", "am", "Thron", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der annoch steht, trotz alledem!", "tokens": ["Der", "an\u00b7noch", "steht", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem \u2013", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Blutschuld, Trug und alledem \u2013", "tokens": ["Trotz", "Blut\u00b7schuld", ",", "Trug", "und", "al\u00b7le\u00b7dem", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er steht noch, und er hudelt uns", "tokens": ["Er", "steht", "noch", ",", "und", "er", "hu\u00b7delt", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie fr\u00fcher fast, trotz alledem!", "tokens": ["Wie", "fr\u00fc\u00b7her", "fast", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Waffen, die der Sieg uns gab,", "tokens": ["Die", "Waf\u00b7fen", ",", "die", "der", "Sieg", "uns", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sieg des Rechts trotz alledem,", "tokens": ["Der", "Sieg", "des", "Rechts", "trotz", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nimmt man sacht uns wieder ab,", "tokens": ["Die", "nimmt", "man", "sacht", "uns", "wie\u00b7der", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Samt Kraut und Lot und alledem,", "tokens": ["Samt", "Kraut", "und", "Lot", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem,", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Parlament und alledem \u2013", "tokens": ["Trotz", "Par\u00b7la\u00b7ment", "und", "al\u00b7le\u00b7dem", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir werden unsre B\u00fcchsen los,", "tokens": ["Wir", "wer\u00b7den", "uns\u00b7re", "B\u00fcch\u00b7sen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Soldatenwild trotz alledem!", "tokens": ["Sol\u00b7da\u00b7ten\u00b7wild", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch sind wir frisch und wohlgemut", "tokens": ["Doch", "sind", "wir", "frisch", "und", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zagen nicht trotz alledem!", "tokens": ["Und", "za\u00b7gen", "nicht", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In tiefer Brust des Zornes Glut,", "tokens": ["In", "tie\u00b7fer", "Brust", "des", "Zor\u00b7nes", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die h\u00e4lt uns warm trotz alledem!", "tokens": ["Die", "h\u00e4lt", "uns", "warm", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem,", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es gilt uns gleich trotz alledem!", "tokens": ["Es", "gilt", "uns", "gleich", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sch\u00fctteln uns: Ein garst'ger Wind,", "tokens": ["Wir", "sch\u00fct\u00b7teln", "uns", ":", "Ein", "gar\u00b7st'\u00b7ger", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Doch weiter nichts trotz alledem!", "tokens": ["Doch", "wei\u00b7ter", "nichts", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn ob der Reichstag sich blamiert", "tokens": ["Denn", "ob", "der", "Reichs\u00b7tag", "sich", "bla\u00b7miert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Professorhaft, trotz alledem!", "tokens": ["Pro\u00b7fes\u00b7sor\u00b7haft", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PIS", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und ob der Teufel regiert", "tokens": ["Und", "ob", "der", "Teu\u00b7fel", "re\u00b7giert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Huf und Horn und alledem \u2013", "tokens": ["Mit", "Huf", "und", "Horn", "und", "al\u00b7le\u00b7dem", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem,", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Dummheit, List und alledem,", "tokens": ["Trotz", "Dumm\u00b7heit", ",", "List", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir wissen doch: die Menschlichkeit", "tokens": ["Wir", "wis\u00b7sen", "doch", ":", "die", "Menschlich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Beh\u00e4lt den Sieg trotz alledem!", "tokens": ["Be\u00b7h\u00e4lt", "den", "Sieg", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So f\u00fcllt denn nur der M\u00f6rser Schlund", "tokens": ["So", "f\u00fcllt", "denn", "nur", "der", "M\u00f6r\u00b7ser", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Eisen, Blei und alledem:", "tokens": ["Mit", "Ei\u00b7sen", ",", "Blei", "und", "al\u00b7le\u00b7dem", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir halten aus auf unserm Grund,", "tokens": ["Wir", "hal\u00b7ten", "aus", "auf", "un\u00b7serm", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wanken nicht trotz alledem!", "tokens": ["Wir", "wan\u00b7ken", "nicht", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem!", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und macht ihr's gar, trotz alledem,", "tokens": ["Und", "macht", "ih\u00b7r's", "gar", ",", "trotz", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "APPR", "PIS", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Wie zu Neapel jener Schuft:", "tokens": ["Wie", "zu", "Nea\u00b7pel", "je\u00b7ner", "Schuft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das hilft erst recht, trotz alledem!", "tokens": ["Das", "hilft", "erst", "recht", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur, was zerf\u00e4llt, vertretet ihr!", "tokens": ["Nur", ",", "was", "zer\u00b7f\u00e4llt", ",", "ver\u00b7tre\u00b7tet", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seid Kasten nur, trotz alledem!", "tokens": ["Seid", "Kas\u00b7ten", "nur", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "NN", "ADV", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind das Volk, die Menschheit wir,", "tokens": ["Wir", "sind", "das", "Volk", ",", "die", "Menschheit", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind ewig drum, trotz alledem!", "tokens": ["Sind", "e\u00b7wig", "drum", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PAV", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem:", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kommt denn an, trotz alledem!", "tokens": ["So", "kommt", "denn", "an", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr hemmt uns, doch ihr zwingt uns nicht \u2013", "tokens": ["Ihr", "hemmt", "uns", ",", "doch", "ihr", "zwingt", "uns", "nicht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Unser die Welt trotz alledem!", "tokens": ["Un\u00b7ser", "die", "Welt", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PIS", "$."], "meter": 