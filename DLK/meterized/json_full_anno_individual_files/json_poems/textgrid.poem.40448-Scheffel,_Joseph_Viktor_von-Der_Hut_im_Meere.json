{"textgrid.poem.40448": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Hut im Meere", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Sorrentiner Marktschiff trug", "tokens": ["Das", "Sor\u00b7ren\u00b7ti\u00b7ner", "Markt\u00b7schiff", "trug"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Orangen \u00fcber Meer", "tokens": ["O\u00b7ran\u00b7gen", "\u00fc\u00b7ber", "Meer"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und flog mit leichtem M\u00f6wenflug,", "tokens": ["Und", "flog", "mit", "leich\u00b7tem", "M\u00f6\u00b7wen\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn's ein Dampfer w\u00e4r'.", "tokens": ["Als", "wenn's", "ein", "Damp\u00b7fer", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel Volk fuhr mit; die Luft war lind", "tokens": ["Viel", "Volk", "fuhr", "mit", ";", "die", "Luft", "war", "lind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alles frohgemut,", "tokens": ["Und", "al\u00b7les", "froh\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann blies von Capri starker Wind \u2013", "tokens": ["Dann", "blies", "von", "Cap\u00b7ri", "star\u00b7ker", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u2013 Fahr' wohl, mein grauer Hut!", "tokens": ["\u2013", "Fahr'", "wohl", ",", "mein", "grau\u00b7er", "Hut", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bis eingerefft das Segel war", "tokens": ["Bis", "ein\u00b7ge\u00b7refft", "das", "Se\u00b7gel", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag Kiel und Mastbaum schief,", "tokens": ["Lag", "Kiel", "und", "Mast\u00b7baum", "schief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Bootsmann schalt, der Weiber Schar", "tokens": ["Der", "Boots\u00b7mann", "schalt", ",", "der", "Wei\u00b7ber", "Schar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Sant' Antonio rief.", "tokens": ["Zum", "Sant'", "An\u00b7to\u00b7nio", "rief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch einmal mir der Freund erschien", "tokens": ["Noch", "ein\u00b7mal", "mir", "der", "Freund", "er\u00b7schien"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Kampf mit Schaum und Flut,", "tokens": ["Im", "Kampf", "mit", "Schaum", "und", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann trieb's ihn gen Pompeji hin", "tokens": ["Dann", "trie\u00b7b's", "ihn", "gen", "Pom\u00b7pe\u00b7ji", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "\u2013 Fahr' wohl, mein grauer Hut!", "tokens": ["\u2013", "Fahr'", "wohl", ",", "mein", "grau\u00b7er", "Hut", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er f\u00fcllte sich, schlug um und sank", "tokens": ["Er", "f\u00fcll\u00b7te", "sich", ",", "schlug", "um", "und", "sank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Salzschwer hinab zum Grund;", "tokens": ["Salz\u00b7schwer", "hin\u00b7ab", "zum", "Grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun tut ihm die Korallenbank", "tokens": ["Nun", "tut", "ihm", "die", "Ko\u00b7ral\u00b7len\u00b7bank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tiefen Wunder kund.", "tokens": ["Der", "Tie\u00b7fen", "Wun\u00b7der", "kund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Astr\u00e4en nisten um ihn her", "tokens": ["A\u00b7str\u00e4\u00b7en", "nis\u00b7ten", "um", "ihn", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Madreporenbrut,", "tokens": ["Und", "Ma\u00b7dre\u00b7po\u00b7ren\u00b7brut", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und der Polypen scheu\u00dflich Heer", "tokens": ["Und", "der", "Po\u00b7ly\u00b7pen", "scheu\u00df\u00b7lich", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u2013 Fahr' wohl, mein grauer Hut!", "tokens": ["\u2013", "Fahr'", "wohl", ",", "mein", "grau\u00b7er", "Hut", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hoch am Vesuviusgipfel stand", "tokens": ["Hoch", "am", "Ve\u00b7su\u00b7vius\u00b7gip\u00b7fel", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein W\u00f6lklein licht gekraust,", "tokens": ["Ein", "W\u00f6lk\u00b7lein", "licht", "ge\u00b7kraust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich den letzten Gru\u00df ihm sandt',", "tokens": ["Als", "ich", "den", "letz\u00b7ten", "Gru\u00df", "ihm", "sandt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Haar vom Wind zerzaust:", "tokens": ["Das", "Haar", "vom", "Wind", "zer\u00b7zaust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbsohn Irions ... im Auge quillt's ...", "tokens": ["\u00bb", "sohn", "I\u00b7rions", "...", "im", "Au\u00b7ge", "quillt's", "..."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "$(", "APPRART", "NN", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Du warst mir weich und gut,", "tokens": ["Du", "warst", "mir", "weich", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Einst Filz und jetzt Salzwasserpilz", "tokens": ["Einst", "Filz", "und", "jetzt", "Salz\u00b7was\u00b7ser\u00b7pilz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u2013 Fahr' wohl, mein grauer Hut!\u00ab", "tokens": ["\u2013", "Fahr'", "wohl", ",", "mein", "grau\u00b7er", "Hut", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "... Graziella fuhr im Schiff wie ich,", "tokens": ["...", "Gra\u00b7zi\u00b7el\u00b7la", "fuhr", "im", "Schiff", "wie", "ich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "APPRART", "NN", "KOKOM", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Ungl\u00fcck nahm sie wahr", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "nahm", "sie", "wahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bot als Schutz vor Sonnenstich", "tokens": ["Und", "bot", "als", "Schutz", "vor", "Son\u00b7nen\u00b7stich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Busenf\u00fcrtuch dar.", "tokens": ["Ihr", "Bu\u00b7sen\u00b7f\u00fcr\u00b7tuch", "dar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und als mein Haupt, derweil sie's kn\u00fcpft,", "tokens": ["Und", "als", "mein", "Haupt", ",", "der\u00b7weil", "sie's", "kn\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ihrem Scho\u00df geruht,", "tokens": ["In", "ih\u00b7rem", "Scho\u00df", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat mir das Herz vor Freud' geh\u00fcpft", "tokens": ["Hat", "mir", "das", "Herz", "vor", "Freud'", "ge\u00b7h\u00fcpft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"t