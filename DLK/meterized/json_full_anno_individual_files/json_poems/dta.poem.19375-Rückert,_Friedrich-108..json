{"dta.poem.19375": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist es, da\u00df du sagst: es hat mich dieses heut,", "tokens": ["Was", "ist", "es", ",", "da\u00df", "du", "sagst", ":", "es", "hat", "mich", "die\u00b7ses", "heut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "PDS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gestern jenes mich, und neulich das gefreut!", "tokens": ["Und", "ge\u00b7stern", "je\u00b7nes", "mich", ",", "und", "neu\u00b7lich", "das", "ge\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "PPER", "$,", "KON", "ADV", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie du dich, armes Herz, mit deinen Freuden qu\u00e4lst,", "tokens": ["Wie", "du", "dich", ",", "ar\u00b7mes", "Herz", ",", "mit", "dei\u00b7nen", "Freu\u00b7den", "qu\u00e4lst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du die einzelnen zusammenrechnend z\u00e4hlst!", "tokens": ["Wenn", "du", "die", "ein\u00b7zel\u00b7nen", "zu\u00b7sam\u00b7men\u00b7rech\u00b7nend", "z\u00e4hlst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVPP", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Die Freude kennst du nicht, wenn du nur Freuden kennest;", "tokens": ["Die", "Freu\u00b7de", "kennst", "du", "nicht", ",", "wenn", "du", "nur", "Freu\u00b7den", "ken\u00b7nest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir fehlt das ganze Licht, wenn du's in Stralen trennest.", "tokens": ["Dir", "fehlt", "das", "gan\u00b7ze", "Licht", ",", "wenn", "du's", "in", "Stra\u00b7len", "tren\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aus all den Freudchen flichst du keinen Freudenkranz;", "tokens": ["Aus", "all", "den", "Freud\u00b7chen", "flichst", "du", "kei\u00b7nen", "Freu\u00b7den\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn eh das eine bl\u00fcht, verwelkt des andern Glanz.", "tokens": ["Denn", "eh", "das", "ei\u00b7ne", "bl\u00fcht", ",", "ver\u00b7welkt", "des", "an\u00b7dern", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIS", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dir frommt auf kurze Rast nicht mancher Freudengast,", "tokens": ["Dir", "frommt", "auf", "kur\u00b7ze", "Rast", "nicht", "man\u00b7cher", "Freu\u00b7den\u00b7gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du nicht Freudendaur im Hause wohnen hast.", "tokens": ["Wenn", "du", "nicht", "Freu\u00b7den\u00b7daur", "im", "Hau\u00b7se", "woh\u00b7nen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "APPRART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}