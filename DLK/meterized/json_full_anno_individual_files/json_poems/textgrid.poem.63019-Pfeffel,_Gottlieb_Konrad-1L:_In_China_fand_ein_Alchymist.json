{"textgrid.poem.63019": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In China fand ein Alchymist", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In China fand ein Alchymist", "tokens": ["In", "Chi\u00b7na", "fand", "ein", "Al\u00b7chy\u00b7mist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt Golds ein Gift, von allen Giften", "tokens": ["Statt", "Golds", "ein", "Gift", ",", "von", "al\u00b7len", "Gif\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das schrecklichste: kein Nektar flie\u00dft", "tokens": ["Das", "schreck\u00b7lichs\u00b7te", ":", "kein", "Nek\u00b7tar", "flie\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$.", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So glatt hinab. In zwanzig Schriften", "tokens": ["So", "glatt", "hin\u00b7ab", ".", "In", "zwan\u00b7zig", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$.", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verk\u00fcndigt er den neuen Fund;", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "er", "den", "neu\u00b7en", "Fund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nachricht gieng von Mund zu Mund", "tokens": ["Die", "Nach\u00b7richt", "gieng", "von", "Mund", "zu", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wirkte schnell. Die Absolone", "tokens": ["Und", "wirk\u00b7te", "schnell", ".", "Die", "Ab\u00b7so\u00b7lo\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verk\u00fcrzten sich den Weg zum Throne", "tokens": ["Ver\u00b7k\u00fcrz\u00b7ten", "sich", "den", "Weg", "zum", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Durch diesen wunderbaren Saft:", "tokens": ["Durch", "die\u00b7sen", "wun\u00b7der\u00b7ba\u00b7ren", "Saft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der philosophische Minister", "tokens": ["Der", "phi\u00b7lo\u00b7so\u00b7phi\u00b7sche", "Mi\u00b7nis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Ward insgeheim vom Hohenpriester", "tokens": ["Ward", "ins\u00b7ge\u00b7heim", "vom", "Ho\u00b7hen\u00b7pries\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Durch ihn vom Ruder weggerafft:", "tokens": ["Durch", "ihn", "vom", "Ru\u00b7der", "weg\u00b7ge\u00b7rafft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Durch ihn vertauschte manche Metze", "tokens": ["Durch", "ihn", "ver\u00b7tauschte", "man\u00b7che", "Met\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Den Gatten gegen den Galan,", "tokens": ["Den", "Gat\u00b7ten", "ge\u00b7gen", "den", "Ga\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und mancher Barnwell griff die Sch\u00e4tze", "tokens": ["Und", "man\u00b7cher", "Barn\u00b7well", "griff", "die", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Des kargen Oheims fr\u00fcher an.", "tokens": ["Des", "kar\u00b7gen", "O\u00b7heims", "fr\u00fc\u00b7her", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Allein so h\u00f6r ich manchen fragen,", "tokens": ["Al\u00b7lein", "so", "h\u00f6r", "ich", "man\u00b7chen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Stellt denn die hohe Policey", "tokens": ["Stellt", "denn", "die", "ho\u00b7he", "Po\u00b7li\u00b7cey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "In China jedem Rekel frey,", "tokens": ["In", "Chi\u00b7na", "je\u00b7dem", "Re\u00b7kel", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Das \u00e4rgste Bubenst\u00fcck zu wagen?", "tokens": ["Das", "\u00e4rgs\u00b7te", "Bu\u00b7ben\u00b7st\u00fcck", "zu", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und warum durfte der Adept", "tokens": ["Und", "wa\u00b7rum", "durf\u00b7te", "der", "A\u00b7dept"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So keck sein h\u00f6llisches Recept", "tokens": ["So", "keck", "sein", "h\u00f6l\u00b7li\u00b7sches", "Re\u00b7cept"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Vor aller Welt zu Markte tragen?", "tokens": ["Vor", "al\u00b7ler", "Welt", "zu", "Mark\u00b7te", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Hier\u00fcber w\u00e4re viel zu sagen;", "tokens": ["Hier\u00b7\u00fc\u00b7ber", "w\u00e4\u00b7re", "viel", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Genug; es war im Geist der Zeit,", "tokens": ["Ge\u00b7nug", ";", "es", "war", "im", "Geist", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df kraft der edeln Pre\u00dffreyheit", "tokens": ["Da\u00df", "kraft", "der", "e\u00b7deln", "Pre\u00df\u00b7frey\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Ein jeder alles schrieb und druckte,", "tokens": ["Ein", "je\u00b7der", "al\u00b7les", "schrieb", "und", "druck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wornach die rechte Faust ihn juckte.", "tokens": ["Wor\u00b7nach", "die", "rech\u00b7te", "Faust", "ihn", "juck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "So schlich der Unfug weit und breit", "tokens": ["So", "schlich", "der", "Un\u00b7fug", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Gleich einer Pest in alle H\u00e4user:", "tokens": ["Gleich", "ei\u00b7ner", "Pest", "in", "al\u00b7le", "H\u00e4u\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ein jeder murrte; nur dem Kaiser", "tokens": ["Ein", "je\u00b7der", "murr\u00b7te", ";", "nur", "dem", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Verbargen seine Schmeichler ihn.", "tokens": ["Ver\u00b7bar\u00b7gen", "sei\u00b7ne", "Schmeich\u00b7ler", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Doch endlich fand ein Mandarin,", "tokens": ["Doch", "end\u00b7lich", "fand", "ein", "Man\u00b7da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ein Menschenfreund, der in der Jugend", "tokens": ["Ein", "Men\u00b7schen\u00b7freund", ",", "der", "in", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Der Quellen Kraft, der Kr\u00e4uter Tugend", "tokens": ["Der", "Quel\u00b7len", "Kraft", ",", "der", "Kr\u00e4u\u00b7ter", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Zu forschen um die Welt geschifft,", "tokens": ["Zu", "for\u00b7schen", "um", "die", "Welt", "ge\u00b7schifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Durch seine Kunst ein Gegengift:", "tokens": ["Durch", "sei\u00b7ne", "Kunst", "ein", "Ge\u00b7gen\u00b7gift", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Er macht es kund: er reicht den Kranken", "tokens": ["Er", "macht", "es", "kund", ":", "er", "reicht", "den", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Das Mittel unentgeldlich dar.", "tokens": ["Das", "Mit\u00b7tel", "un\u00b7ent\u00b7geld\u00b7lich", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Es n\u00fctzte wenig; viele tranken", "tokens": ["Es", "n\u00fctz\u00b7te", "we\u00b7nig", ";", "vie\u00b7le", "tran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Es ungern, weil es bitter war:", "tokens": ["Es", "un\u00b7gern", ",", "weil", "es", "bit\u00b7ter", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Die meisten lachten beym Gedanken", "tokens": ["Die", "meis\u00b7ten", "lach\u00b7ten", "beym", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Von Hinterlist und von Gefahr:", "tokens": ["Von", "Hin\u00b7ter\u00b7list", "und", "von", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Die Siechen aus den fernen Kreisen", "tokens": ["Die", "Sie\u00b7chen", "aus", "den", "fer\u00b7nen", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Erhielten die Arzney des Weisen", "tokens": ["Er\u00b7hiel\u00b7ten", "die", "Arz\u00b7ney", "des", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.46": {"text": "Oft gar nicht, \u00f6fters auch zu sp\u00e4t.", "tokens": ["Oft", "gar", "nicht", ",", "\u00f6f\u00b7ters", "auch", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "$,", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "So wuchs das Uebel alle Tage", "tokens": ["So", "wuchs", "das", "Ue\u00b7bel", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Und endlich drang des Volkes Klage", "tokens": ["Und", "end\u00b7lich", "drang", "des", "Vol\u00b7kes", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Bis vor den Thron der Majest\u00e4t.", "tokens": ["Bis", "vor", "den", "Thron", "der", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Der Kaiser ruft den Unheilstifter", "tokens": ["Der", "Kai\u00b7ser", "ruft", "den", "Un\u00b7heil\u00b7stif\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Zur Rechenschaft vors Blutgericht.", "tokens": ["Zur", "Re\u00b7chen\u00b7schaft", "vors", "Blut\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Monarch, erwiedert der Vergifter", "tokens": ["Mon\u00b7arch", ",", "er\u00b7wie\u00b7dert", "der", "Ver\u00b7gif\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Mit unerschrocknem Angesicht,", "tokens": ["Mit", "un\u00b7er\u00b7schrock\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Ich kenne mein Verbrechen nicht;", "tokens": ["Ich", "ken\u00b7ne", "mein", "Ver\u00b7bre\u00b7chen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Ist nicht die Vollmacht, laut zu denken,", "tokens": ["Ist", "nicht", "die", "Voll\u00b7macht", ",", "laut", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Das Vorrecht der Philosophie?", "tokens": ["Das", "Vor\u00b7recht", "der", "Phi\u00b7lo\u00b7so\u00b7phie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Und dieses, Herr, kannst du nicht kr\u00e4nken,", "tokens": ["Und", "die\u00b7ses", ",", "Herr", ",", "kannst", "du", "nicht", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "w