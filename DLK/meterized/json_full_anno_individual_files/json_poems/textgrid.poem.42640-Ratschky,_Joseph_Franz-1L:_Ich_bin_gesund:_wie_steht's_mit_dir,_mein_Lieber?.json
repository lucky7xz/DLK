{"textgrid.poem.42640": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin gesund: wie steht's mit dir, mein Lieber?", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin gesund: wie steht's mit dir, mein Lieber?", "tokens": ["Ich", "bin", "ge\u00b7sund", ":", "wie", "steht's", "mit", "dir", ",", "mein", "Lie\u00b7ber", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PWAV", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist's wirklich Ernst, dass dich ein b\u00f6ses Fieber", "tokens": ["Ist's", "wirk\u00b7lich", "Ernst", ",", "dass", "dich", "ein", "b\u00f6\u00b7ses", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NE", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drey Tage schon nicht aus dem Bette l\u00e4sst?", "tokens": ["Drey", "Ta\u00b7ge", "schon", "nicht", "aus", "dem", "Bet\u00b7te", "l\u00e4sst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ey, Freund! das ist ein arger Hausarrest.", "tokens": ["Ey", ",", "Freund", "!", "das", "ist", "ein", "ar\u00b7ger", "Haus\u00b7ar\u00b7rest", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und w\u00e4r's nur noch in tr\u00fcben Wintertagen,", "tokens": ["Und", "w\u00e4r's", "nur", "noch", "in", "tr\u00fc\u00b7ben", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da liesse sich's viel leichter \u00fcbertragen:", "tokens": ["Da", "lies\u00b7se", "sich's", "viel", "leich\u00b7ter", "\u00fc\u00b7ber\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Allein im Lenz, im anmuthvollen Lenz", "tokens": ["Al\u00b7lein", "im", "Lenz", ",", "im", "an\u00b7muth\u00b7vol\u00b7len", "Lenz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ist allerdings ein solches Akcidenz", "tokens": ["Ist", "al\u00b7ler\u00b7dings", "ein", "sol\u00b7ches", "Ak\u00b7ci\u00b7denz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein Streich, bey dem selbst Epiktet, die Zierde", "tokens": ["Ein", "Streich", ",", "bey", "dem", "selbst", "E\u00b7pik\u00b7tet", ",", "die", "Zier\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADV", "VVPP", "$,", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der Stoiker, ein Bisschen fluchen w\u00fcrde.", "tokens": ["Der", "Stoi\u00b7ker", ",", "ein", "Bis\u00b7schen", "flu\u00b7chen", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du dauerst mich, o armer Pazient!", "tokens": ["Du", "dau\u00b7erst", "mich", ",", "o", "ar\u00b7mer", "Pa\u00b7zi\u00b7ent", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Indessen wir, das blaue Firmament", "tokens": ["In\u00b7des\u00b7sen", "wir", ",", "das", "blau\u00b7e", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob unserm Haupt, im gr\u00fcnen Prater sitzen,", "tokens": ["Ob", "un\u00b7serm", "Haupt", ",", "im", "gr\u00fc\u00b7nen", "Pra\u00b7ter", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Musst du daheim im warmen Pf\u00fchle schwitzen.", "tokens": ["Musst", "du", "da\u00b7heim", "im", "war\u00b7men", "Pf\u00fch\u00b7le", "schwit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Statt deines Kleists und B\u00fcrgers liegt ein Wisch", "tokens": ["Statt", "dei\u00b7nes", "Kleists", "und", "B\u00fcr\u00b7gers", "liegt", "ein", "Wisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Recipe auf einem Nebentisch:", "tokens": ["Von", "Re\u00b7ci\u00b7pe", "auf", "ei\u00b7nem", "Ne\u00b7ben\u00b7tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Statt Lottens sitzt, mit einer Staatsper\u00fccke", "tokens": ["Statt", "Lot\u00b7tens", "sitzt", ",", "mit", "ei\u00b7ner", "Staats\u00b7pe\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Belastet, dir der Arzt auf dem Genicke:", "tokens": ["Be\u00b7las\u00b7tet", ",", "dir", "der", "Arzt", "auf", "dem", "Ge\u00b7ni\u00b7cke", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Statt eines Tranks von frischem Haberbier", "tokens": ["Statt", "ei\u00b7nes", "Tranks", "von", "fri\u00b7schem", "Ha\u00b7ber\u00b7bier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Bedient man dich mit einem Elixir.", "tokens": ["Be\u00b7dient", "man", "dich", "mit", "ei\u00b7nem", "E\u00b7li\u00b7xir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr G\u00f6tter, helft! Zevs, Juno, Athen\u00e4a,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "helft", "!", "Zevs", ",", "Ju\u00b7no", ",", "A\u00b7the\u00b7n\u00e4a", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$.", "NE", "$,", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Apoll, Merkur, Mars, Bachus, Cytherea,", "tokens": ["A\u00b7poll", ",", "Mer\u00b7kur", ",", "Mars", ",", "Ba\u00b7chus", ",", "Cy\u00b7the\u00b7rea", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,", "NE", "$,", "NE", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und wie ihr bass nach Rang und Dignit\u00e4t", "tokens": ["Und", "wie", "ihr", "bass", "nach", "Rang", "und", "Dig\u00b7ni\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Griechenlands und Roms Legenden steht,", "tokens": ["In", "Grie\u00b7chen\u00b7lands", "und", "Roms", "Le\u00b7gen\u00b7den", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Helft meinem Freund; sonst traun! bey meiner Ehre!", "tokens": ["Helft", "mei\u00b7nem", "Freund", ";", "sonst", "traun", "!", "bey", "mei\u00b7ner", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVINF", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sonst schimpf' ich laut auf eurer Priester Lehre,", "tokens": ["Sonst", "schimpf'", "ich", "laut", "auf", "eu\u00b7rer", "Pries\u00b7ter", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word"