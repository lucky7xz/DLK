{"dta.poem.10768": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma  \n  Vom Thurn zu Stra\u00dfburg/ warumb der  \n andere daruebennit auffgebawet  \n worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr seyt vnrecht daran/ Zuseher/ die jhr schawet", "tokens": ["Ihr", "seyt", "vn\u00b7recht", "da\u00b7ran", "/", "Zu\u00b7se\u00b7her", "/", "die", "jhr", "scha\u00b7wet"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "PAV", "$(", "NN", "$(", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Diesen herrlichen Thurn/ das achte Wunderwerck", "tokens": ["Die\u00b7sen", "herr\u00b7li\u00b7chen", "Thurn", "/", "das", "ach\u00b7te", "Wun\u00b7der\u00b7werck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "De\u00df Irrdischen bezircks/ vnd vber alle Berg", "tokens": ["De\u00df", "Irr\u00b7di\u00b7schen", "be\u00b7zircks", "/", "vnd", "vber", "al\u00b7le", "Berg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Denselben hoch erhebt/ weil er k\u00f6stlich erbawet", "tokens": ["Den\u00b7sel\u00b7ben", "hoch", "er\u00b7hebt", "/", "weil", "er", "k\u00f6st\u00b7lich", "er\u00b7ba\u00b7wet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "ADJD", "VVFIN", "$(", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Bi\u00df an die Wolcken reicht/ da\u00df jhr darbey wolt klagen/", "tokens": ["Bi\u00df", "an", "die", "Wol\u00b7cken", "reicht", "/", "da\u00df", "jhr", "dar\u00b7bey", "wolt", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$(", "KOUS", "PPER", "PAV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es mangelt jhm noch eins/ di\u00df nemlich/ da\u00df der grunde", "tokens": ["Es", "man\u00b7gelt", "jhm", "noch", "eins", "/", "di\u00df", "nem\u00b7lich", "/", "da\u00df", "der", "grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "$(", "PDS", "ADV", "$(", "KOUS", "ART", "NN"], "meter": "--+-+-+-++-+-", "measure": "anapaest.init"}, "line.7": {"text": "Seins Bruders neben jhm leer/ der vff diese stundt", "tokens": ["Seins", "Bru\u00b7ders", "ne\u00b7ben", "jhm", "leer", "/", "der", "vff", "die\u00b7se", "stundt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADJD", "$(", "ART", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nur halb gef\u00fchret auff. Nicht lasset euch mi\u00dfhagen", "tokens": ["Nur", "halb", "ge\u00b7f\u00fch\u00b7ret", "auff", ".", "Nicht", "las\u00b7set", "euch", "mi\u00df\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "PTKVZ", "$.", "PTKNEG", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dieses geheimnus gro\u00df. Natur hats eingestelt", "tokens": ["Die\u00b7ses", "ge\u00b7heim\u00b7nus", "gro\u00df", ".", "Na\u00b7tur", "hats", "ein\u00b7ge\u00b7stelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJD", "$.", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df neben diesem Thurn noch einer solt gefallen/", "tokens": ["Da\u00df", "ne\u00b7ben", "die\u00b7sem", "Thurn", "noch", "ei\u00b7ner", "solt", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "ADV", "PIS", "VMFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dann so ist er allein der sch\u00f6nst vnd h\u00f6chst vor allen/", "tokens": ["Dann", "so", "ist", "er", "al\u00b7lein", "der", "sch\u00f6nst", "vnd", "h\u00f6chst", "vor", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "KON", "ADV", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vnd hat seins gleichen nicht in dieser weiten Welt.", "tokens": ["Vnd", "hat", "seins", "glei\u00b7chen", "nicht", "in", "die\u00b7ser", "wei\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "PTKNEG", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}