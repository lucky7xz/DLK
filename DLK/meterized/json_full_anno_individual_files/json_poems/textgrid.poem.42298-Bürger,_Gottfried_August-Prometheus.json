{"textgrid.poem.42298": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Prometheus", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prometheus hatte kaum herab in Erdennacht", "tokens": ["Pro\u00b7me\u00b7theus", "hat\u00b7te", "kaum", "her\u00b7ab", "in", "Er\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Quell des Lichts, der W\u00e4rm' und alles Lebens,", "tokens": ["Den", "Quell", "des", "Lichts", ",", "der", "W\u00e4rm'", "und", "al\u00b7les", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Feuer, von Olymp gebracht;", "tokens": ["Das", "Feu\u00b7er", ",", "von", "O\u00b7lymp", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh, da verbrannte sich \u2013 denn Warnen war vergebens \u2013", "tokens": ["Sieh", ",", "da", "ver\u00b7brann\u00b7te", "sich", "\u2013", "denn", "War\u00b7nen", "war", "ver\u00b7ge\u00b7bens", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PRF", "$(", "KON", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Manch dummes J\u00fcngelchen die Faust aus Unbedacht.", "tokens": ["Manch", "dum\u00b7mes", "J\u00fcn\u00b7gel\u00b7chen", "die", "Faust", "aus", "Un\u00b7be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mein Gott! Was f\u00fcr Geschrei erhuben", "tokens": ["Mein", "Gott", "!", "Was", "f\u00fcr", "Ge\u00b7schrei", "er\u00b7hu\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWS", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht da so manches dummen Buben", "tokens": ["Nicht", "da", "so", "man\u00b7ches", "dum\u00b7men", "Bu\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erzdummer Papa,", "tokens": ["Erz\u00b7dum\u00b7mer", "Pa\u00b7pa", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Erzdumme Mama,", "tokens": ["Erz\u00b7dum\u00b7me", "Ma\u00b7ma", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Erzdumme Leibs- und Seelenamme!", "tokens": ["Erz\u00b7dum\u00b7me", "Leibs", "und", "See\u00b7len\u00b7am\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Welch G\u00e4nsegeschnatter die Klerisei,", "tokens": ["Welch", "G\u00e4n\u00b7se\u00b7ge\u00b7schnat\u00b7ter", "die", "Kle\u00b7ri\u00b7sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Welch Trutha