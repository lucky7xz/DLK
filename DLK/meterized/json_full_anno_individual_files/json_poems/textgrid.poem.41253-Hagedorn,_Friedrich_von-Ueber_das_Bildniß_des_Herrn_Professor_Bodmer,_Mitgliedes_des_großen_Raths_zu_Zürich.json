{"textgrid.poem.41253": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ueber das Bildni\u00df des Herrn Professor Bodmer, Mitgliedes des gro\u00dfen Raths zu Z\u00fcrich", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dieser Bildung herrscht der sch\u00f6pferische Geist,", "tokens": ["In", "die\u00b7ser", "Bil\u00b7dung", "herrscht", "der", "sch\u00f6p\u00b7fe\u00b7ri\u00b7sche", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der neuen Witz und Muth im Noah uns beweist.", "tokens": ["Der", "neu\u00b7en", "Witz", "und", "Muth", "im", "Noah", "uns", "be\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPRART", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sein Auge lebt und denkt, und weissagt Meisterst\u00fccke.", "tokens": ["Sein", "Au\u00b7ge", "lebt", "und", "denkt", ",", "und", "weis\u00b7sagt", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie reizt mich's, da\u00df ich hier auch einen Freund erblicke,", "tokens": ["Wie", "reizt", "mich's", ",", "da\u00df", "ich", "hier", "auch", "ei\u00b7nen", "Freund", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der mich so lange liebt, und daher fast vergi\u00dft,", "tokens": ["Der", "mich", "so", "lan\u00b7ge", "liebt", ",", "und", "da\u00b7her", "fast", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,", "KON", "PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df meine Dichterei dem Reim noch dienstbar ist!", "tokens": ["Da\u00df", "mei\u00b7ne", "Dich\u00b7te\u00b7rei", "dem", "Reim", "noch", "dienst\u00b7bar", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In dieser Bildung herrscht der sch\u00f6pferische Geist,", "tokens": ["In", "die\u00b7ser", "Bil\u00b7dung", "herrscht", "der", "sch\u00f6p\u00b7fe\u00b7ri\u00b7sche", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der neuen Witz und Muth im Noah uns beweist.", "tokens": ["Der", "neu\u00b7en", "Witz", "und", "Muth", "im", "Noah", "uns", "be\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPRART", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sein Auge lebt und denkt, und weissagt Meisterst\u00fccke.", "tokens": ["Sein", "Au\u00b7ge", "lebt", "und", "denk