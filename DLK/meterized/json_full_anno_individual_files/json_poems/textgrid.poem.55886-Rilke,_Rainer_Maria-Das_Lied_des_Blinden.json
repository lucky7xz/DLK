{"textgrid.poem.55886": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied des Blinden", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin blind, ihr drau\u00dfen, das ist ein Fluch,", "tokens": ["Ich", "bin", "blind", ",", "ihr", "drau\u00b7\u00dfen", ",", "das", "ist", "ein", "Fluch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "ADV", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "ein Widerwillen, ein Widerspruch,", "tokens": ["ein", "Wi\u00b7der\u00b7wil\u00b7len", ",", "ein", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "etwas t\u00e4glich Schweres.", "tokens": ["et\u00b7was", "t\u00e4g\u00b7lich", "Schwe\u00b7res", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ich leg meine Hand auf den Arm der Frau,", "tokens": ["Ich", "leg", "mei\u00b7ne", "Hand", "auf", "den", "Arm", "der", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "meine graue Hand auf ihr graues Grau,", "tokens": ["mei\u00b7ne", "grau\u00b7e", "Hand", "auf", "ihr", "grau\u00b7es", "Grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT",