{"dta.poem.8376": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Des ritters rh\u00fcm ich mich/ die weil ich obgesieget:", "tokens": ["Des", "rit\u00b7ters", "rh\u00fcm", "ich", "mich", "/", "die", "weil", "ich", "ob\u00b7ge\u00b7sie\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PRF", "$(", "ART", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin kein kriegesinann der niemals hatt gekrieget/", "tokens": ["Ich", "bin", "kein", "krie\u00b7ge\u00b7si\u00b7nann", "der", "nie\u00b7mals", "hatt", "ge\u00b7krie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein ritter ohne feindt/ kein reuter ohne pferdt;", "tokens": ["Kein", "rit\u00b7ter", "oh\u00b7ne", "feindt", "/", "kein", "reu\u00b7ter", "oh\u00b7ne", "pferdt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJD", "$(", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer von mir wi\u00dfen wil/ der frage noch mein schwerdt.", "tokens": ["Wer", "von", "mir", "wi\u00b7\u00dfen", "wil", "/", "der", "fra\u00b7ge", "noch", "mein", "schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "punc