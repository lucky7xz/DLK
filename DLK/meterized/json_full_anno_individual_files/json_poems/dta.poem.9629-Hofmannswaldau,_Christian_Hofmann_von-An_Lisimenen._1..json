{"dta.poem.9629": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Lisimenen.  \n 1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljsimene liebst du nicht?", "tokens": ["Ljsi\u00b7me\u00b7ne", "liebst", "du", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wilstu ewig einsam leben?", "tokens": ["Wils\u00b7tu", "e\u00b7wig", "ein\u00b7sam", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll der augen sternend licht", "tokens": ["Soll", "der", "au\u00b7gen", "ster\u00b7nend", "licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir nicht die vergn\u00fcgung geben?", "tokens": ["Mir", "nicht", "die", "ver\u00b7gn\u00fc\u00b7gung", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach die sonnen sind zu sch\u00f6ne/", "tokens": ["Ach", "die", "son\u00b7nen", "sind", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "ADJA", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ist der lippen ihr rubin", "tokens": ["Ist", "der", "lip\u00b7pen", "ihr", "ru\u00b7bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor die todten nur erkohren?", "tokens": ["Vor", "die", "tod\u00b7ten", "nur", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gibst du so die rosen hin/", "tokens": ["Gibst", "du", "so", "die", "ro\u00b7sen", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die vor g\u00f6tter sind gebehren?", "tokens": ["Die", "vor", "g\u00f6t\u00b7ter", "sind", "ge\u00b7beh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach der purpur ist zu sch\u00f6ne/", "tokens": ["Ach", "der", "pur\u00b7pur", "ist", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "ADJD", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Tr\u00e4gt nicht deine zarte hand/", "tokens": ["Tr\u00e4gt", "nicht", "dei\u00b7ne", "zar\u00b7te", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die perle nicht zu gleichen/", "tokens": ["Der", "die", "per\u00b7le", "nicht", "zu", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein verwundtes hertz im band?", "tokens": ["Ein", "ver\u00b7wund\u00b7tes", "hertz", "im", "band", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kan dich dieses nicht erweichen?", "tokens": ["Kan", "dich", "die\u00b7ses", "nicht", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PDS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach die h\u00e4nde sind zu sch\u00f6ne/", "tokens": ["Ach", "die", "h\u00e4n\u00b7de", "sind", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "ADJA", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Es h\u00e4ngt an dem weissen schild/", "tokens": ["Es", "h\u00e4ngt", "an", "dem", "weis\u00b7sen", "schild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der die runden h\u00fcgel tr\u00e4get/", "tokens": ["Der", "die", "run\u00b7den", "h\u00fc\u00b7gel", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem halse ja ein bild/", "tokens": ["An", "dem", "hal\u00b7se", "ja", "ein", "bild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da Cupido drauff gepr\u00e4get.", "tokens": ["Da", "Cu\u00b7pi\u00b7do", "drauff", "ge\u00b7pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PAV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach der hals der ist zu sch\u00f6ne/", "tokens": ["Ach", "der", "hals", "der", "ist", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "ART", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Soll der marmor deiner brust/", "tokens": ["Soll", "der", "mar\u00b7mor", "dei\u00b7ner", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welchen du mit flei\u00df verh\u00fcllet/", "tokens": ["Wel\u00b7chen", "du", "mit", "flei\u00df", "ver\u00b7h\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zum lieben tragen lust/", "tokens": ["Nicht", "zum", "lie\u00b7ben", "tra\u00b7gen", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er auff und nieder quillet?", "tokens": ["Wenn", "er", "auff", "und", "nie\u00b7der", "quil\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach die \u00e4pffel sind zu sch\u00f6ne/", "tokens": ["Ach", "die", "\u00e4pf\u00b7fel", "sind", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Ach ich bitte/ z\u00fcrne nicht/", "tokens": ["Ach", "ich", "bit\u00b7te", "/", "z\u00fcr\u00b7ne", "nicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADV", "$(", "VVFIN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich neulich auff der wiesen", "tokens": ["Da\u00df", "ich", "neu\u00b7lich", "auff", "der", "wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gantz verstohlen/ o mein licht!", "tokens": ["Gantz", "ver\u00b7stoh\u00b7len", "/", "o", "mein", "licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$(", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Konte deinen leib erkiesen.", "tokens": ["Kon\u00b7te", "dei\u00b7nen", "leib", "er\u00b7kie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach der leib der war zu sch\u00f6ne/", "tokens": ["Ach", "der", "leib", "der", "war", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "ART", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Es trit dein geschickter fu\u00df", "tokens": ["Es", "trit", "dein", "ge\u00b7schick\u00b7ter", "fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "PTKVZ"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die betr\u00fcbten einsamkeiten/", "tokens": ["Die", "be\u00b7tr\u00fcb\u00b7ten", "ein\u00b7sam\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich glaube/ mit verdru\u00df/", "tokens": ["Wie", "ich", "glau\u00b7be", "/", "mit", "ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$(", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich ihn n\u00e4chst sahe gleiten.", "tokens": ["Weil", "ich", "ihn", "n\u00e4chst", "sa\u00b7he", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Ach die f\u00fcsse sind zu sch\u00f6ne/", "tokens": ["Ach", "die", "f\u00fcs\u00b7se", "sind", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Sch\u00f6nste/ nimst du den nicht an/", "tokens": ["Sch\u00f6ns\u00b7te", "/", "nimst", "du", "den", "nicht", "an", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "ART", "PTKNEG", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich itzt vor deinem throne", "tokens": ["Der", "sich", "itzt", "vor", "dei\u00b7nem", "thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leget als dein unterthan?", "tokens": ["Le\u00b7get", "als", "dein", "un\u00b7ter\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebst du dich ihm nicht zu lohne?", "tokens": ["Giebst", "du", "dich", "ihm", "nicht", "zu", "loh\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPER", "PTKNEG", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein! ach nein! du bist zu sch\u00f6ne/", "tokens": ["Nein", "!", "ach", "nein", "!", "du", "bist", "zu", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "XY", "PTKANT", "$.", "PPER", "VAFIN", "APPR", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lisimene!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}