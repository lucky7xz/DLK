{"dta.poem.6130": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was auch noch deutlicher uns den Zusammenhang,", "tokens": ["Was", "auch", "noch", "deut\u00b7li\u00b7cher", "uns", "den", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Des Mondes mit der Lufft, der Lufft mit unserm Meer", "tokens": ["Des", "Mon\u00b7des", "mit", "der", "Lufft", ",", "der", "Lufft", "mit", "un\u00b7serm", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor Augen legt, ist dies: wenn sich des Mondes Gang", "tokens": ["Vor", "Au\u00b7gen", "legt", ",", "ist", "dies", ":", "wenn", "sich", "des", "Mon\u00b7des", "Gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "VAFIN", "PDS", "$.", "KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In beyden Tropiets befindet, wird der Drang", "tokens": ["In", "bey\u00b7den", "Tro\u00b7piets", "be\u00b7fin\u00b7det", ",", "wird", "der", "Drang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "In weiten Schoo\u00df des Welt-Meers starck: daher", "tokens": ["In", "wei\u00b7ten", "Schoo\u00df", "des", "Welt\u00b7Meers", "starck", ":", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$.", "PAV"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Entsteht daselbst ein Wallen, dessen Schwall,", "tokens": ["Ent\u00b7steht", "da\u00b7selbst", "ein", "Wal\u00b7len", ",", "des\u00b7sen", "Schwall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "$,", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als allgemein in Welt-Meer, \u00fcberall,", "tokens": ["Als", "all\u00b7ge\u00b7mein", "in", "Welt\u00b7Meer", ",", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Ebb und Fluht des Tages zweymal bringet.", "tokens": ["Die", "Ebb", "und", "Fluht", "des", "Ta\u00b7ges", "zwey\u00b7mal", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Wasser gleiten dann gen S\u00fcd- und Norden fort,", "tokens": ["Die", "Was\u00b7ser", "glei\u00b7ten", "dann", "gen", "S\u00fcd", "und", "Nor\u00b7den", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "TRUNC", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man sp\u00fcrt sie weniger und mehr; nachdem ein Ort,", "tokens": ["Man", "sp\u00fcrt", "sie", "we\u00b7ni\u00b7ger", "und", "mehr", ";", "nach\u00b7dem", "ein", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "KON", "ADV", "$.", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch weniger und mehr von Ufern ist umringet.", "tokens": ["Auch", "we\u00b7ni\u00b7ger", "und", "mehr", "von", "U\u00b7fern", "ist", "um\u00b7rin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}