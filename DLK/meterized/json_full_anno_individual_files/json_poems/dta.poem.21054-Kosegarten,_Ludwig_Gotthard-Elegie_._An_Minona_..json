{"dta.poem.21054": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie .  \n  An Minona .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum bist du so ernst? und warum thauet die               ", "tokens": ["Wa\u00b7rum", "bist", "du", "so", "ernst", "?", "und", "wa\u00b7rum", "thau\u00b7et", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "KON", "PWAV", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thr\u00e4ne", "tokens": ["Thr\u00e4\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Deine gl\u00fchende Wang', edeles M\u00e4dchen,", "tokens": ["Dei\u00b7ne", "gl\u00fc\u00b7hen\u00b7de", "Wang'", ",", "e\u00b7de\u00b7les", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "herab?", "tokens": ["her\u00b7ab", "?"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Warum h\u00fcllet dich Dunkel, und warum w\u00f6lket", "tokens": ["Wa\u00b7rum", "h\u00fcl\u00b7let", "dich", "Dun\u00b7kel", ",", "und", "wa\u00b7rum", "w\u00f6l\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "KON", "PWAV", "VVFIN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "dich Tr\u00fcbsinn,", "tokens": ["dich", "Tr\u00fcb\u00b7sinn", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Da der Morgen dich weckt, welcher ins", "tokens": ["Da", "der", "Mor\u00b7gen", "dich", "weckt", ",", "wel\u00b7cher", "ins"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "APPRART"], "meter": "--+--++-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Daseyn dich rief? \u2014", "tokens": ["Da\u00b7seyn", "dich", "rief", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wallen etwa die Schatten der abgeschiedenen Stun-", "tokens": ["Wal\u00b7len", "et\u00b7wa", "die", "Schat\u00b7ten", "der", "ab\u00b7ge\u00b7schie\u00b7de\u00b7nen", "Stun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "den", "tokens": ["den"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Deine Seele vorbey? Siehst du die fl\u00fcch-", "tokens": ["Dei\u00b7ne", "See\u00b7le", "vor\u00b7bey", "?", "Siehst", "du", "die", "fl\u00fcch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "tige Schaar", "tokens": ["ti\u00b7ge", "Schaar"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.13": {"text": "Deiner Monden und Jahre sich st\u00fcrzen in dr\u00e4ngen-", "tokens": ["Dei\u00b7ner", "Mon\u00b7den", "und", "Jah\u00b7re", "sich", "st\u00fcr\u00b7zen", "in", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "PRF", "VVFIN", "APPR", "TRUNC"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "den Wogen,", "tokens": ["den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "In der Vergangenheit alles verschlingendes", "tokens": ["In", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "al\u00b7les", "ver\u00b7schlin\u00b7gen\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "ADJA"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Meer?", "tokens": ["Meer", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Rufst du die Freuden zur\u00fcck, die dir im Schoosse", "tokens": ["Rufst", "du", "die", "Freu\u00b7den", "zu\u00b7r\u00fcck", ",", "die", "dir", "im", "Schoos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "der Vorzeit", "tokens": ["der", "Vor\u00b7zeit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Bl\u00fchten? Winkst du dem Gram, welcher", "tokens": ["Bl\u00fch\u00b7ten", "?", "Winkst", "du", "dem", "Gram", ",", "wel\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.20": {"text": "dich tr\u00fcbte, zur\u00fcck?", "tokens": ["dich", "tr\u00fcb\u00b7te", ",", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Lass sie ruhen! Sie sind verschwunden, wie nich-", "tokens": ["Lass", "sie", "ru\u00b7hen", "!", "Sie", "sind", "ver\u00b7schwun\u00b7den", ",", "wie", "nich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "PPER", "VAFIN", "VVPP", "$,", "PWAV", "TRUNC"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "tige Schatten.", "tokens": ["ti\u00b7ge", "Schat\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.23": {"text": "Lass ihn schlummern! Er schl\u00e4ft in der", "tokens": ["Lass", "ihn", "schlum\u00b7mern", "!", "Er", "schl\u00e4ft", "in", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "PPER", "VVFIN", "APPR", "ART"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.24": {"text": "Vergessenheit Nacht.", "tokens": ["Ver\u00b7ges\u00b7sen\u00b7heit", "Nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Freue dich deiner Jugend. Noch g\u00fcrtet ihr rosiger", "tokens": ["Freu\u00b7e", "dich", "dei\u00b7ner", "Ju\u00b7gend", ".", "Noch", "g\u00fcr\u00b7tet", "ihr", "ro\u00b7si\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA"], "meter": "---+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "G\u00fcrtel", "tokens": ["G\u00fcr\u00b7tel"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Deine H\u00fcfte; dein Haar ringelt die Freund-", "tokens": ["Dei\u00b7ne", "H\u00fcf\u00b7te", ";", "dein", "Haar", "rin\u00b7gelt", "die", "Freun\u00b7d"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "liche noch.", "tokens": ["li\u00b7che", "noch", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Freue dich deines Seyns; denn s\u00fcss ist Seyn, und", "tokens": ["Freu\u00b7e", "dich", "dei\u00b7nes", "Seyns", ";", "denn", "s\u00fcss", "ist", "Seyn", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "KON", "ADJD", "VAFIN", "PPOSAT", "$,", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "erfreulich", "tokens": ["er\u00b7freu\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Ist es zu athmen. Und sch\u00f6n l\u00e4chelst du,", "tokens": ["Ist", "es", "zu", "ath\u00b7men", ".", "Und", "sch\u00f6n", "l\u00e4\u00b7chelst", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "$.", "KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "seliges Licht.", "tokens": ["se\u00b7li\u00b7ges", "Licht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Perlen streuet die Sonn' im Aufgang, Perlen im", "tokens": ["Per\u00b7len", "streu\u00b7et", "die", "Sonn'", "im", "Auf\u00b7gang", ",", "Per\u00b7len", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "NN", "APPRART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Abend.", "tokens": ["A\u00b7bend", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Freundlich leuchtet der Mond \u00fcber die", "tokens": ["Freund\u00b7lich", "leuch\u00b7tet", "der", "Mond", "\u00fc\u00b7ber", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "schlummernde Welt.", "tokens": ["schlum\u00b7mern\u00b7de", "Welt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "L\u00fcstern l\u00e4chelt die Erd' am Br\u00e4utigamsbusen des", "tokens": ["L\u00fcs\u00b7tern", "l\u00e4\u00b7chelt", "die", "Erd'", "am", "Br\u00e4u\u00b7ti\u00b7gams\u00b7bu\u00b7sen", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "ART"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Fr\u00fchlings.", "tokens": ["Fr\u00fch\u00b7lings", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Leise pfl\u00fccket der Herbst ihre Verwelkun-", "tokens": ["Lei\u00b7se", "pfl\u00fc\u00b7cket", "der", "Herbst", "ih\u00b7re", "Ver\u00b7wel\u00b7kun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "gen weg.", "tokens": ["gen", "weg", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "Feyerlich ruht sie im Leichengewande des blenden-", "tokens": ["Fey\u00b7er\u00b7lich", "ruht", "sie", "im", "Lei\u00b7chen\u00b7ge\u00b7wan\u00b7de", "des", "blen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART", "TRUNC"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.42": {"text": "den Schneees,", "tokens": ["den", "Schne\u00b7e\u00b7es", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Bis sie verj\u00fcngt und versch\u00f6nt wieder den", "tokens": ["Bis", "sie", "ver\u00b7j\u00fcngt", "und", "ver\u00b7sch\u00f6nt", "wie\u00b7der", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "ART"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Gr\u00e4bern entbl\u00fcht.", "tokens": ["Gr\u00e4\u00b7bern", "ent\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.45": {"text": "Ja, es ist s\u00fcss zu athmen auf Gottes herrlich be-", "tokens": ["Ja", ",", "es", "ist", "s\u00fcss", "zu", "ath\u00b7men", "auf", "Got\u00b7tes", "herr\u00b7lich", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "PTKZU", "VVINF", "APPR", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "gabter", "tokens": ["gab\u00b7ter"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Sch\u00f6ner Erd'. Es ist Wonne, zu wandeln", "tokens": ["Sch\u00f6\u00b7ner", "Erd'", ".", "Es", "ist", "Won\u00b7ne", ",", "zu", "wan\u00b7deln"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.48": {"text": "auf ihr.", "tokens": ["auf", "ihr", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$."], "meter": "++", "measure": "spondeus"}, "line.49": {"text": "Wonne ist es, zu schaun des Fr\u00fchlings funkelnden", "tokens": ["Won\u00b7ne", "ist", "es", ",", "zu", "schaun", "des", "Fr\u00fch\u00b7lings", "fun\u00b7keln\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PTKZU", "VVINF", "ART", "NN", "VVINF"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.50": {"text": "Brautschmuck,", "tokens": ["Brautsc\u00b7hmuck", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.51": {"text": "Wonne zu schauen den Wald glimmen im", "tokens": ["Won\u00b7ne", "zu", "schau\u00b7en", "den", "Wald", "glim\u00b7men", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "ART", "NN", "VVFIN", "APPRART"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.52": {"text": "silbernen Reif.", "tokens": ["sil\u00b7ber\u00b7nen", "Reif", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.53": {"text": "Hohe Wonn' ists zu schaun des Menschen g\u00f6ttliches", "tokens": ["Ho\u00b7he", "Wonn'", "ists", "zu", "schaun", "des", "Men\u00b7schen", "g\u00f6tt\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "ART", "NN", "ADJA"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.54": {"text": "Antlitz,", "tokens": ["Ant\u00b7litz", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.55": {"text": "Gl\u00fchend von tiefem Gef\u00fchl, feuernd von", "tokens": ["Gl\u00fc\u00b7hend", "von", "tie\u00b7fem", "Ge\u00b7f\u00fchl", ",", "feu\u00b7ernd", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "ADJD", "APPR"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.56": {"text": "Thatenbegier.", "tokens": ["Tha\u00b7ten\u00b7be\u00b7gier", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.57": {"text": "H\u00f6here Wonn' ists, Herzen erobern, am Busen der", "tokens": ["H\u00f6\u00b7he\u00b7re", "Wonn'", "ists", ",", "Her\u00b7zen", "er\u00b7o\u00b7bern", ",", "am", "Bu\u00b7sen", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "NN", "VVINF", "$,", "APPRART", "NN", "ART"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.58": {"text": "Freundschaft", "tokens": ["Freund\u00b7schaft"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.59": {"text": "Hoch aufathmen, im Arm liebender Lieb-", "tokens": ["Hoch", "auf\u00b7ath\u00b7men", ",", "im", "Arm", "lie\u00b7ben\u00b7der", "Lie\u00b7b"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "APPRART", "NN", "ADJA", "TRUNC"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.60": {"text": "linge ruhn.", "tokens": ["lin\u00b7ge", "ruhn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.61": {"text": "Aber die h\u00f6chste der hohen, der seligen Wonnen", "tokens": ["A\u00b7ber", "die", "h\u00f6chs\u00b7te", "der", "ho\u00b7hen", ",", "der", "se\u00b7li\u00b7gen", "Won\u00b7nen"], "token_info": ["word", "word", "word