{"dta.poem.19991": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsklage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00e4nger frei sich schwingen", "tokens": ["Die", "S\u00e4n\u00b7ger", "frei", "sich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus diesem Thr\u00e4nenthal.", "tokens": ["Aus", "die\u00b7sem", "Thr\u00e4\u00b7nen\u00b7thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fr\u00f6hlich im Sonnenstral", "tokens": ["Fr\u00f6h\u00b7lich", "im", "Son\u00b7nen\u00b7stral"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ein helles Lied zu singen.", "tokens": ["Ein", "hel\u00b7les", "Lied", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich blick' empor mit Sehnen,", "tokens": ["Ich", "blick'", "em\u00b7por", "mit", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Befangen schl\u00e4gt das Herz,", "tokens": ["Be\u00b7fan\u00b7gen", "schl\u00e4gt", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Lied erzeugt der Schmerz,", "tokens": ["Mein", "Lied", "er\u00b7zeugt", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schnell stirbt es hin in Thr\u00e4nen.", "tokens": ["Schnell", "stirbt", "es", "hin", "in", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die S\u00e4nger ruhn mit Wonne", "tokens": ["Die", "S\u00e4n\u00b7ger", "ruhn", "mit", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im gr\u00fcngew\u00f6lbten Baum,", "tokens": ["Im", "gr\u00fcn\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tr\u00e4umen hellen Traum,", "tokens": ["Sie", "tr\u00e4u\u00b7men", "hel\u00b7len", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Sternen, Mond und Sonne.", "tokens": ["Von", "Ster\u00b7nen", ",", "Mond", "und", "Son\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich sitz' in enger Zelle,", "tokens": ["Ich", "sitz'", "in", "en\u00b7ger", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Traum l\u00f6s't meinen Harm,", "tokens": ["Kein", "Traum", "l\u00f6s't", "mei\u00b7nen", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich sitze krank und arm,", "tokens": ["Ich", "sit\u00b7ze", "krank", "und", "arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schmerz macht mir jede Helle.", "tokens": ["Schmerz", "macht", "mir", "je\u00b7de", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}