{"dta.poem.12709": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines reisenden.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck auf den weg von hier! So w\u00fcnschte j\u00fcngst sein hau\u00df,", "tokens": ["Gl\u00fcck", "auf", "den", "weg", "von", "hier", "!", "So", "w\u00fcnschte", "j\u00fcngst", "sein", "hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADV", "APPR", "ADV", "$.", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Als er nach Weimar zog, es hat auch eingetroffen,", "tokens": ["Als", "er", "nach", "Wei\u00b7mar", "zog", ",", "es", "hat", "auch", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weit \u00fcber aller wunsch und sein selbst eigen hoffen,", "tokens": ["Weit", "\u00fc\u00b7ber", "al\u00b7ler", "wunsch", "und", "sein", "selbst", "ei\u00b7gen", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "ADJD", "KON", "PPOSAT", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein weg gieng himmel-an und aus der welt hinaus.", "tokens": ["Sein", "weg", "gieng", "him\u00b7mel\u00b7an", "und", "aus", "der", "welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVFIN", "NE", "KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}