{"textgrid.poem.42019": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es regt in wechselnden Gestalten", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es regt in wechselnden Gestalten", "tokens": ["Es", "regt", "in", "wech\u00b7seln\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich rasch des Lebens buntes Bild,", "tokens": ["Sich", "rasch", "des", "Le\u00b7bens", "bun\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und andre Farben sehn wir walten,", "tokens": ["Und", "and\u00b7re", "Far\u00b7ben", "sehn", "wir", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Je mehr der Faden sich enth\u00fcllt.", "tokens": ["Je", "mehr", "der", "Fa\u00b7den", "sich", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Jungfrau hofft auf sch\u00f6n're Lenze,", "tokens": ["Die", "Jung\u00b7frau", "hofft", "auf", "sch\u00f6n'\u00b7re", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aufstrebend lenkt der Mann sein Gl\u00fcck,", "tokens": ["Auf\u00b7stre\u00b7bend", "lenkt", "der", "Mann", "sein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf die einst errungnen Kr\u00e4nze", "tokens": ["Und", "auf", "die", "einst", "er\u00b7rung\u00b7nen", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Blickt ruhig ernst der Greis zur\u00fcck.", "tokens": ["Blickt", "ru\u00b7hig", "ernst", "der", "Greis", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch Hoffnung, Kraft und Ruhe weben", "tokens": ["Doch", "Hoff\u00b7nung", ",", "Kraft", "und", "Ru\u00b7he", "we\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ganzen traulichen Verein,", "tokens": ["Des", "Gan\u00b7zen", "trau\u00b7li\u00b7chen", "Ver\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimmer kann das volle Leben", "tokens": ["Und", "nim\u00b7mer", "kann", "das", "vol\u00b7le", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schoo\u00df des Einzelnen gedeihn.", "tokens": ["Im", "Schoo\u00df", "des", "Ein\u00b7zel\u00b7nen", "ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Darum soll liebend sich begr\u00fc\u00dfen,", "tokens": ["Da\u00b7rum", "soll", "lie\u00b7bend", "sich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was fern der Flug der Zeiten schied,", "tokens": ["Was", "fern", "der", "Flug", "der", "Zei\u00b7ten", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Lust gew\u00e4hren, Lust genie\u00dfen", "tokens": ["Und", "Lust", "ge\u00b7w\u00e4h\u00b7ren", ",", "Lust", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVINF", "$,", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit friedlich freundlichem Gem\u00fcth.", "tokens": ["Mit", "fried\u00b7lich", "freund\u00b7li\u00b7chem", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So hie\u00df des Menschen erste Weihe,", "tokens": ["So", "hie\u00df", "des", "Men\u00b7schen", "ers\u00b7te", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als er die fremde Bahn betrat:", "tokens": ["Als", "er", "die", "frem\u00b7de", "Bahn", "be\u00b7trat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey weis' und gl\u00fccklich und verzeihe", "tokens": ["Sey", "weis'", "und", "gl\u00fcck\u00b7lich", "und", "ver\u00b7zei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und duld' und liebe was dir naht.", "tokens": ["Und", "duld'", "und", "lie\u00b7be", "was", "dir", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum weiche fern aus unserm Kreise,", "tokens": ["Drum", "wei\u00b7che", "fern", "aus", "un\u00b7serm", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer trotzig nur sein eignes Bild", "tokens": ["Wer", "trot\u00b7zig", "nur", "sein", "eig\u00b7nes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verg\u00f6ttert und mit ew'gem Eise", "tokens": ["Ver\u00b7g\u00f6t\u00b7tert", "und", "mit", "ew'\u00b7gem", "Ei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Mitgef\u00fchl die Brust verh\u00fcllt.", "tokens": ["Dem", "Mit\u00b7ge\u00b7f\u00fchl", "die", "Brust", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn menschlich soll der Mensch empfinden,", "tokens": ["Denn", "menschlich", "soll", "der", "Mensch", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht einsam gehn auf eigner Bahn,", "tokens": ["Nicht", "ein\u00b7sam", "gehn", "auf", "eig\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll suchen, was er lieb', und finden,", "tokens": ["Soll", "su\u00b7chen", ",", "was", "er", "lieb'", ",", "und", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bittend sich und helfend nahn.", "tokens": ["Und", "bit\u00b7tend", "sich", "und", "hel\u00b7fend", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "KON", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl ist mit mannigfalt'gem Glanze", "tokens": ["Wohl", "ist", "mit", "man\u00b7nig\u00b7falt'\u00b7gem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lenzes bunter Pfad verkl\u00e4rt:", "tokens": ["Des", "Len\u00b7zes", "bun\u00b7ter", "Pfad", "ver\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch in der Liebe gro\u00dfem Kranze", "tokens": ["Doch", "in", "der", "Lie\u00b7be", "gro\u00b7\u00dfem", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hat jede Blume gleichen Werth.", "tokens": ["Hat", "je\u00b7de", "Blu\u00b7me", "glei\u00b7chen", "Werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn auch einst die hellen Stunden", "tokens": ["Und", "wenn", "auch", "einst", "die", "hel\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schmerzes d\u00fcstre Wolke tr\u00fcbt,", "tokens": ["Des", "Schmer\u00b7zes", "d\u00fcst\u00b7re", "Wol\u00b7ke", "tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn unsre Hoffnung hingeschwunden", "tokens": ["Wenn", "uns\u00b7re", "Hoff\u00b7nung", "hin\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und still verwelkt, was wir geliebt,", "tokens": ["Und", "still", "ver\u00b7welkt", ",", "was", "wir", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann soll den letzten Trost der Thr\u00e4nen", "tokens": ["Dann", "soll", "den", "letz\u00b7ten", "Trost", "der", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht stolz und kalt das Herz verschm\u00e4hn,", "tokens": ["Nicht", "stolz", "und", "kalt", "das", "Herz", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sch\u00f6ner im Erinnrungssehnen", "tokens": ["Und", "sch\u00f6\u00b7ner", "im", "E\u00b7rinn\u00b7rungs\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das holde Bild uns auferstehn.", "tokens": ["Das", "hol\u00b7de", "Bild", "uns", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, heiter ist schon hier das Leben,", "tokens": ["Ja", ",", "hei\u00b7ter", "ist", "schon", "hier", "das", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eine heitre Zukunft naht;", "tokens": ["Und", "ei\u00b7ne", "heit\u00b7re", "Zu\u00b7kunft", "naht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir bl\u00fchn in froher Kraft und streben,", "tokens": ["Wir", "bl\u00fchn", "in", "fro\u00b7her", "Kraft", "und", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des sch\u00f6nern Lenzes heil'ge Saat;", "tokens": ["Des", "sch\u00f6\u00b7nern", "Len\u00b7zes", "heil'\u00b7ge", "Saat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob fr\u00fcher eine Bl\u00fcthe falle,", "tokens": ["Ob", "fr\u00fc\u00b7her", "ei\u00b7ne", "Bl\u00fc\u00b7the", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Darf nicht des Menschen Sorge seyn;", "tokens": ["Darf", "nicht", "des", "Men\u00b7schen", "Sor\u00b7ge", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Einst schlie\u00dft ", "tokens": ["Einst", "schlie\u00dft"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "In seine reinen Wonnen ein.", "tokens": ["In", "sei\u00b7ne", "rei\u00b7nen", "Won\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es regt in wechselnden Gestalten", "tokens": ["Es", "regt", "in", "wech\u00b7seln\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"t