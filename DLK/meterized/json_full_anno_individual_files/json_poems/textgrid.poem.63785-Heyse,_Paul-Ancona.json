{"textgrid.poem.63785": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Ancona", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeigst du dich denn noch immer deutschen Dichtern", "tokens": ["Zeigst", "du", "dich", "denn", "noch", "im\u00b7mer", "deut\u00b7schen", "Dich\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im schlimmsten Licht? Es w\u00e4lzte Nebelmassen", "tokens": ["Im", "schlimms\u00b7ten", "Licht", "?", "Es", "w\u00e4lz\u00b7te", "Ne\u00b7bel\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch mir Scirocco durch die schmutz'gen Gassen,", "tokens": ["Auch", "mir", "Sci\u00b7roc\u00b7co", "durch", "die", "schmutz'\u00b7gen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und selbst der Bau Trajans stand grau und n\u00fcchtern.", "tokens": ["Und", "selbst", "der", "Bau", "Tra\u00b7jans", "stand", "grau", "und", "n\u00fcch\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NE", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was fabelt hier von sch\u00f6nen Fraungesichtern", "tokens": ["Was", "fa\u00b7belt", "hier", "von", "sch\u00f6\u00b7nen", "Fraun\u00b7ge\u00b7sich\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Reisebuch? Zu diesen fieberblassen,", "tokens": ["Das", "Rei\u00b7se\u00b7buch", "?", "Zu", "die\u00b7sen", "fie\u00b7ber\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkommnen Weibern will das Lob nicht passen;", "tokens": ["Ver\u00b7komm\u00b7nen", "Wei\u00b7bern", "will", "das", "Lob", "nicht", "pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ahnten sie's, so gehn sie stumm und sch\u00fcchtern.", "tokens": ["Als", "ahn\u00b7ten", "sie's", ",", "so", "gehn", "sie", "stumm", "und", "sch\u00fcch\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "NE", "$,", "ADV", "VVFIN", "PPER", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch ferne sei's, von deinem tr\u00fcbsten Tage", "tokens": ["Doch", "fer\u00b7ne", "sei's", ",", "von", "dei\u00b7nem", "tr\u00fcbs\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf all die hellen, die dir bl\u00fchn, zu schlie\u00dfen", "tokens": ["Auf", "all", "die", "hel\u00b7len", ",", "die", "dir", "bl\u00fchn", ",", "zu", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "$,", "PRELS", "PPER", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Leopardis Heimatflur zu schelten,", "tokens": ["Und", "Le\u00b7o\u00b7par\u00b7dis", "Hei\u00b7mat\u00b7flur", "zu", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gleich ihm, dem hohen Genius der Klage,", "tokens": ["Gleich", "ihm", ",", "dem", "ho\u00b7hen", "Ge\u00b7nius", "der", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dem, was ihm selbst versagt war zu genie\u00dfen,", "tokens": ["Dem", ",", "was", "ihm", "selbst", "ver\u00b7sagt", "war", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "VVPP", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Gl\u00fcck der Welt, als Irrwahn mu\u00dfte gelten.", "tokens": ["Das", "Gl\u00fcck", "der", "Welt", ",", "als", "Irr\u00b7wahn", "mu\u00df\u00b7te", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zeigst du dich denn noch immer deutschen Dichtern", "tokens": ["Zeigst", "du", "dich", "denn", "noch", "im\u00b7mer", "deut\u00b7schen", "Dich\u00b7tern"], "token_info": ["word