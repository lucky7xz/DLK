{"dta.poem.20406": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff den Geburts-Tag  \n Sr. Hochfl. Durchl. Herrn Anthon  \n Ulrichs/ Hertzogen zu Braunschweig  \n und L\u00fcneburg rc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vertr\u00e4gt dein kluges ohr auch schlechter saiten-klang/", "tokens": ["Ver\u00b7tr\u00e4gt", "dein", "klu\u00b7ges", "ohr", "auch", "schlech\u00b7ter", "sai\u00b7ten\u00b7klang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchlauchtigster Anthon/ du f\u00fcrst der Castalinnen/", "tokens": ["Durch\u00b7lauch\u00b7tigs\u00b7ter", "An\u00b7thon", "/", "du", "f\u00fcrst", "der", "Cas\u00b7ta\u00b7lin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um dessen lagerstatt das chor der Pierinnen", "tokens": ["Um", "des\u00b7sen", "la\u00b7ger\u00b7statt", "das", "chor", "der", "Pie\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PDS", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bey zarter kindheit schon als seinen Pindus sprang?", "tokens": ["Bey", "zar\u00b7ter", "kind\u00b7heit", "schon", "als", "sei\u00b7nen", "Pin\u00b7dus", "sprang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vergleicht sich deine burg der k\u00e4yserin der wellen/", "tokens": ["Ver\u00b7gleicht", "sich", "dei\u00b7ne", "burg", "der", "k\u00e4y\u00b7se\u00b7rin", "der", "wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "ART", "NN", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die in der blauen schoo\u00df viel purpur-muscheln hegt/", "tokens": ["Die", "in", "der", "blau\u00b7en", "schoo\u00df", "viel", "pur\u00b7pur\u00b7mu\u00b7scheln", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "VVFIN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit perlen und corall den erden-krey\u00df verlegt/", "tokens": ["Mit", "per\u00b7len", "und", "co\u00b7rall", "den", "er\u00b7den\u00b7krey\u00df", "ver\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "KON", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dennoch nicht verschm\u00e4ht gemeine wasser-qvellen?", "tokens": ["Und", "den\u00b7noch", "nicht", "ver\u00b7schm\u00e4ht", "ge\u00b7mei\u00b7ne", "was\u00b7ser\u00b7qvel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Legt sich ein funcken glut von dir auff meinen geist/", "tokens": ["Legt", "sich", "ein", "fun\u00b7cken", "glut", "von", "dir", "auff", "mei\u00b7nen", "geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der ihn der erden ab zu Titans-h\u00f6hen reist?", "tokens": ["Der", "ihn", "der", "er\u00b7den", "ab", "zu", "Ti\u00b7tans\u00b7h\u00f6\u00b7hen", "reist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PTKVZ", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}