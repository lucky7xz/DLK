{"textgrid.poem.36663": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Classisches in Tibur", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jagst du dem Classischen nach, und ist's dem Barbaren Entz\u00fccken,", "tokens": ["Jagst", "du", "dem", "Clas\u00b7si\u00b7schen", "nach", ",", "und", "ist's", "dem", "Bar\u00b7ba\u00b7ren", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "KON", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Nun so sieh, wie mich hier Vorwelt und Mitwelt erfreut!", "tokens": ["Nun", "so", "sieh", ",", "wie", "mich", "hier", "Vor\u00b7welt", "und", "Mit\u00b7welt", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ueber der Grotte Neptuns wird gezecht, im purpurnen Becher", "tokens": ["Ue\u00b7ber", "der", "Grot\u00b7te", "Nep\u00b7tuns", "wird", "ge\u00b7zecht", ",", "im", "pur\u00b7pur\u00b7nen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VAFIN", "VVPP", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+-++-+-", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Spiegelt sich Tempel und Berg, Hain und die Villa Lukulls.", "tokens": ["Spie\u00b7gelt", "sich", "Tem\u00b7pel", "und", "Berg", ",", "Hain", "und", "die", "Vil\u00b7la", "Lu\u00b7kulls", "."], "token_info": ["