{"dta.poem.23595": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Veredelung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur das Vollblut l\u00e4sst man gelten,", "tokens": ["Nur", "das", "Voll\u00b7blut", "l\u00e4sst", "man", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drum erzielt man's hie und da,", "tokens": ["Drum", "er\u00b7zielt", "man's", "hie", "und", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, man schicket auch nicht selten", "tokens": ["Ja", ",", "man", "schi\u00b7cket", "auch", "nicht", "sel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "ADV", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber noch Arabia.", "tokens": ["Sel\u00b7ber", "noch", "A\u00b7ra\u00b7bia", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wer kann das Beginnen schimpfen?", "tokens": ["Wer", "kann", "das", "Be\u00b7gin\u00b7nen", "schimp\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist es auch nicht practisch sehr,", "tokens": ["Ist", "es", "auch", "nicht", "prac\u00b7tisch", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So den Adel einzuimpfen,", "tokens": ["So", "den", "A\u00b7del", "ein\u00b7zu\u00b7imp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebt's doch etwas Adel mehr.", "tokens": ["Giebt's", "doch", "et\u00b7was", "A\u00b7del", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was kann mit der Zeit noch werden,", "tokens": ["Was", "kann", "mit", "der", "Zeit", "noch", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "ADV", "VAINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind vereinte Kr\u00e4ft' im Bund!", "tokens": ["Sind", "ver\u00b7ein\u00b7te", "Kr\u00e4ft'", "im", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt man's so schon mit den Pferden,", "tokens": ["Treibt", "man's", "so", "schon", "mit", "den", "Pfer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kommt man bald auch auf den Hund.", "tokens": ["Kommt", "man", "bald", "auch", "auf", "den", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}