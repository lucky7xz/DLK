{"dta.poem.13069": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eun\u00fcberwindlichkeit ist zwar ihr Eigenthum;", "tokens": ["\u201e", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich\u00b7keit", "ist", "zwar", "ihr", "Ei\u00b7gen\u00b7thum", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edoch lie\u00df ihr grosser Geist dem Ehgemahl den Ruhm", "tokens": ["\u201e", "doch", "lie\u00df", "ihr", "gros\u00b7ser", "Geist", "dem", "Eh\u00b7ge\u00b7mahl", "den", "Ruhm"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "675\u201dDa\u00df niemand in der Welt, als er, ihr Herz besiegte;", "tokens": ["\"", "Da\u00df", "nie\u00b7mand", "in", "der", "Welt", ",", "als", "er", ",", "ihr", "Herz", "be\u00b7sieg\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201everlust, der ihren Sinn mehr als ein Sieg vergn\u00fcgte.", "tokens": ["\u201e", "ver\u00b7lust", ",", "der", "ih\u00b7ren", "Sinn", "mehr", "als", "ein", "Sieg", "ver\u00b7gn\u00fcg\u00b7te", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eje mehr man sie verehrt, je mehr wird er erh\u00f6ht;", "tokens": ["\u201e", "je", "mehr", "man", "sie", "ver\u00b7ehrt", ",", "je", "mehr", "wird", "er", "er\u00b7h\u00f6ht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PIS", "PPER", "VVPP", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201esie schm\u00fcckt und zieret ihn durch ihre Majest\u00e4t.", "tokens": ["\u201e", "sie", "schm\u00fcckt", "und", "zie\u00b7ret", "ihn", "durch", "ih\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewer kann dahero nicht aus allen ihren Wercken,", "tokens": ["\u201e", "wer", "kann", "da\u00b7he\u00b7ro", "nicht", "aus", "al\u00b7len", "ih\u00b7ren", "Wer\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PAV", "PTKNEG", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "680\u201dDa\u00df sie nur ihn verlangt geehrt zu sehen, mercken?", "tokens": ["\"", "Da\u00df", "sie", "nur", "ihn", "ver\u00b7langt", "ge\u00b7ehrt", "zu", "se\u00b7hen", ",", "mer\u00b7cken", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "VVPP", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}