{"textgrid.poem.49781": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Friede", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Heide geht der Wind;", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "geht", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Es fl\u00fcstert im Gras, es rauscht in den B\u00e4umen.", "tokens": ["Es", "fl\u00fcs\u00b7tert", "im", "Gras", ",", "es", "rauscht", "in", "den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die dort unten erschlagen sind,", "tokens": ["Die", "dort", "un\u00b7ten", "er\u00b7schla\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die vielen Toten, sie schweigen und tr\u00e4umen.", "tokens": ["Die", "vie\u00b7len", "To\u00b7ten", ",", "sie", "schwei\u00b7gen", "und", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "H\u00f6ren sie nicht den Glockenklang?", "tokens": ["H\u00f6\u00b7ren", "sie", "nicht", "den", "Glo\u00b7cken\u00b7klang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dringt nicht zu ihnen aus heiligen R\u00e4umen", "tokens": ["Dringt", "nicht", "zu", "ih\u00b7nen", "aus", "hei\u00b7li\u00b7gen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Halleluja und Friedenssang?", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "und", "Frie\u00b7dens\u00b7sang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Die vielen Toten, sie schweigen und tr\u00e4umen.", "tokens": ["Die", "vie\u00b7len", "To\u00b7ten", ",", "sie", "schwei\u00b7gen", "und", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Voll des Dankes ist alle Welt,", "tokens": ["Voll", "des", "Dan\u00b7kes", "ist", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Sie darf mit dem Lobe des Herrn nicht s\u00e4umen;", "tokens": ["Sie", "darf", "mit", "dem", "Lo\u00b7be", "des", "Herrn", "nicht", "s\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wer im Kampfe fiel, hei\u00dft ein Held.", "tokens": ["Wer", "im", "Kamp\