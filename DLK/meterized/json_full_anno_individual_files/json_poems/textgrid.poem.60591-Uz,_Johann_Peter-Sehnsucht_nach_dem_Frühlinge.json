{"textgrid.poem.60591": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach dem Fr\u00fchlinge", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verlange nur nicht allzusehr", "tokens": ["Ver\u00b7lan\u00b7ge", "nur", "nicht", "all\u00b7zu\u00b7sehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des holden Fr\u00fchlings Wiederkehr!", "tokens": ["Des", "hol\u00b7den", "Fr\u00fch\u00b7lings", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wird er, unter jungen Rosen,", "tokens": ["Bald", "wird", "er", ",", "un\u00b7ter", "jun\u00b7gen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Grazien liebkosen,", "tokens": ["Den", "Gra\u00b7zi\u00b7en", "lieb\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und im belaubten Hayn", "tokens": ["Und", "im", "be\u00b7laub\u00b7ten", "Hayn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bey Nymphen und Cytheren seyn.", "tokens": ["Bey", "Nym\u00b7phen", "und", "Cy\u00b7the\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Des Winters trauriges Gewand", "tokens": ["Des", "Win\u00b7ters", "trau\u00b7ri\u00b7ges", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deckt noch die W\u00e4lder, noch das Land:", "tokens": ["Deckt", "noch", "die", "W\u00e4l\u00b7der", ",", "noch", "das", "Land", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Ph\u00f6bus jagt die raschen Pferde", "tokens": ["Doch", "Ph\u00f6\u00b7bus", "jagt", "die", "ra\u00b7schen", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon n\u00e4her an der Erde,", "tokens": ["Schon", "n\u00e4\u00b7her", "an", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch eine steilre Bahn,", "tokens": ["Durch", "ei\u00b7ne", "steil\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des Himmels rund Gew\u00f6lb hinan.", "tokens": ["Des", "Him\u00b7mels", "rund", "Ge\u00b7w\u00f6lb", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf schnellem Wagen ist er schon", "tokens": ["Auf", "schnel\u00b7lem", "Wa\u00b7gen", "ist", "er", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem wilden Capricorn entflohn;", "tokens": ["Dem", "wil\u00b7den", "Cap\u00b7ri\u00b7corn", "ent\u00b7flohn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von den schwarzen St\u00fcrmen schwellen", "tokens": ["Und", "von", "den", "schwar\u00b7zen", "St\u00fcr\u00b7men", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die aufgebrachten Wellen:", "tokens": ["Die", "auf\u00b7ge\u00b7brach\u00b7ten", "Wel\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Winde k\u00e4mpfend Heer", "tokens": ["Der", "Win\u00b7de", "k\u00e4mp\u00b7fend", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00e4llt rasend aufs gest\u00e4upte Meer.", "tokens": ["F\u00e4llt", "ra\u00b7send", "aufs", "ge\u00b7st\u00e4up\u00b7te", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weh ihm, wenn sich der Handelsmann", "tokens": ["Weh", "ihm", ",", "wenn", "sich", "der", "Han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Heimreis' ietzt entschlie\u00dfen kann,", "tokens": ["Zur", "Heim\u00b7reis'", "ietzt", "ent\u00b7schlie\u00b7\u00dfen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bereichert mit Aegyptens Waaren", "tokens": ["Be\u00b7rei\u00b7chert", "mit", "A\u00b7e\u00b7gyp\u00b7tens", "Waa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Creter Meer durchfahren,", "tokens": ["Der", "Cre\u00b7ter", "Meer", "durch\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und k\u00fchn dem Africus", "tokens": ["Und", "k\u00fchn", "dem", "A\u00b7fri\u00b7cus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf schwachem Schiffe trotzen mu\u00df!", "tokens": ["Auf", "schwa\u00b7chem", "Schif\u00b7fe", "trot\u00b7zen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die junge Gattinn harrt am Strand,", "tokens": ["Die", "jun\u00b7ge", "Gat\u00b7tinn", "harrt", "am", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ihr Geliebter ihr verschwand,", "tokens": ["Wo", "ihr", "Ge\u00b7lieb\u00b7ter", "ihr", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und herzt den Sohn mit bangem Sehnen,", "tokens": ["Und", "herzt", "den", "Sohn", "mit", "ban\u00b7gem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den unter s\u00fcssen Thr\u00e4nen", "tokens": ["Den", "un\u00b7ter", "s\u00fcs\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An ihrer Brust sie n\u00e4hrt,", "tokens": ["An", "ih\u00b7rer", "Brust", "sie", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ein Willkommen stammeln lehrt.", "tokens": ["Und", "ein", "Will\u00b7kom\u00b7men", "stam\u00b7meln", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Umsonst! Kein Gott erh\u00f6rt ihr Flehn!", "tokens": ["Um\u00b7sonst", "!", "Kein", "Gott", "er\u00b7h\u00f6rt", "ihr", "Flehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PIAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird ihn, ach! nicht wieder sehn.", "tokens": ["Sie", "wird", "ihn", ",", "ach", "!", "nicht", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ITJ", "$.", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird, in tiefer See begraben,", "tokens": ["Er", "wird", ",", "in", "tie\u00b7fer", "See", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die giergen Fische laben:", "tokens": ["Die", "gier\u00b7gen", "Fi\u00b7sche", "la\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn die erz\u00fcrnte Fluth", "tokens": ["Denn", "die", "er\u00b7z\u00fcrn\u00b7te", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verschlingt lautbr\u00fcllend Schiff und Gut.", "tokens": ["Ver\u00b7schlingt", "laut\u00b7br\u00fcl\u00b7lend", "Schiff", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du aber, wann ein sanfter West", "tokens": ["Du", "a\u00b7ber", ",", "wann", "ein", "sanf\u00b7ter", "West"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun durch die ersten Veilchen bl\u00e4st,", "tokens": ["Nun", "durch", "die", "ers\u00b7ten", "Veil\u00b7chen", "bl\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verweile nicht, dich zu entschlie\u00dfen,", "tokens": ["Ver\u00b7wei\u00b7le", "nicht", ",", "dich", "zu", "ent\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und Tage zu genie\u00dfen,", "tokens": ["Und", "Ta\u00b7ge", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die uns die karge Zeit", "tokens": ["Die", "uns", "die", "kar\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur wenig, wenig Monden leiht!", "tokens": ["Nur", "we\u00b7nig", ",", "we\u00b7nig", "Mon\u00b7den", "leiht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Mensch verfolgt mit starrem Blick", "tokens": ["Der", "Mensch", "ver\u00b7folgt", "mit", "star\u00b7rem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ihm entfliehend l\u00e4chelnd Gl\u00fcck:", "tokens": ["Ein", "ihm", "ent\u00b7flie\u00b7hend", "l\u00e4\u00b7chelnd", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Er jammert um versagte Freuden.", "tokens": ["Er", "jam\u00b7mert", "um", "ver\u00b7sag\u00b7te", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erst wann sie fl\u00fcchtig scheiden,", "tokens": ["Erst", "wann", "sie", "fl\u00fcch\u00b7tig", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erkennt und sch\u00e4tzt er sie:", "tokens": ["Er\u00b7kennt", "und", "sch\u00e4tzt", "er", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch, was er hat, genie\u00dft er nie.", "tokens": ["Doch", ",", "was", "er", "hat", ",", "ge\u00b7nie\u00dft", "er", "nie", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Verlange nur nicht allzusehr", "tokens": ["Ver\u00b7lan\u00b7ge", "nur", "nicht", "all\u00b7zu\u00b7sehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des holden Fr\u00fchlings Wiederkehr!", "tokens": ["Des", "hol\u00b7den", "Fr\u00fch\u00b7lings", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wird er, unter jungen Rosen,", "tokens": ["Bald", "wird", "er", ",", "un\u00b7ter", "jun\u00b7gen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Grazien liebkosen,", "tokens": ["Den", "Gra\u00b7zi\u00b7en", "lieb\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und im belaubten Hayn", "tokens": ["Und", "im", "be\u00b7laub\u00b7ten", "Hayn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bey Nymphen und Cytheren seyn.", "tokens": ["Bey", "Nym\u00b7phen", "und", "Cy\u00b7the\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Des Winters trauriges Gewand", "tokens": ["Des", "Win\u00b7ters", "trau\u00b7ri\u00b7ges", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deckt noch die W\u00e4lder, noch das Land:", "tokens": ["Deckt", "noch", "die", "W\u00e4l\u00b7der", ",", "noch", "das", "Land", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Ph\u00f6bus jagt die raschen Pferde", "tokens": ["Doch", "Ph\u00f6\u00b7bus", "jagt", "die", "ra\u00b7schen", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon n\u00e4her an der Erde,", "tokens": ["Schon", "n\u00e4\u00b7her", "an", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch eine steilre Bahn,", "tokens": ["Durch", "ei\u00b7ne", "steil\u00b7re", "Bahn", ","]