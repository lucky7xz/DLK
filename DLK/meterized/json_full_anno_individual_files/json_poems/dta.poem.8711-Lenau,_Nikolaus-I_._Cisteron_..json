{"dta.poem.8711": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "I .  \n  Cisteron .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Welche Freude f\u00fchlt der Wandrer,", "tokens": ["Wel\u00b7che", "Freu\u00b7de", "f\u00fchlt", "der", "Wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht er so im Fr\u00fchlingsstrahle", "tokens": ["Zieht", "er", "so", "im", "Fr\u00fch\u00b7lings\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die sch\u00f6nen, liedervollen,", "tokens": ["Durch", "die", "sch\u00f6\u00b7nen", ",", "lie\u00b7der\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wonnigen Provencerthale!", "tokens": ["Won\u00b7ni\u00b7gen", "Pro\u00b7ven\u00b7cer\u00b7tha\u00b7le", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hei\u00dfer gl\u00fcht der Ku\u00df der Sonne", "tokens": ["Hei\u00b7\u00dfer", "gl\u00fcht", "der", "Ku\u00df", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den blumenreichen Matten;", "tokens": ["Auf", "den", "blu\u00b7men\u00b7rei\u00b7chen", "Mat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00df're Labung rauscht die Quelle,", "tokens": ["S\u00fc\u00df'\u00b7re", "La\u00b7bung", "rauscht", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchler s\u00e4useln hier die Schatten.", "tokens": ["K\u00fch\u00b7ler", "s\u00e4u\u00b7seln", "hier", "die", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Voller t\u00f6nt des Donners Stimme,", "tokens": ["Vol\u00b7ler", "t\u00f6nt", "des", "Don\u00b7ners", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sterne blinken heller,", "tokens": ["Und", "die", "Ster\u00b7ne", "blin\u00b7ken", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rascher bl\u00fcht die Frucht und reifet,", "tokens": ["Ra\u00b7scher", "bl\u00fcht", "die", "Frucht", "und", "rei\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Liebe z\u00fcndet schneller.", "tokens": ["Und", "die", "Lie\u00b7be", "z\u00fcn\u00b7det", "schnel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unbesiegbar und unendlich", "tokens": ["Un\u00b7be\u00b7sieg\u00b7bar", "und", "un\u00b7end\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+-++-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist der Liebe banges Sehnen,", "tokens": ["Ist", "der", "Lie\u00b7be", "ban\u00b7ges", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es nagen in die Herzen", "tokens": ["Und", "es", "na\u00b7gen", "in", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefer ihre Spur die Thr\u00e4nen.", "tokens": ["Tie\u00b7fer", "ih\u00b7re", "Spur", "die", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber f\u00fchrt der Weg den Wandrer", "tokens": ["A\u00b7ber", "f\u00fchrt", "der", "Weg", "den", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Ort, den ich besinge,", "tokens": ["An", "den", "Ort", ",", "den", "ich", "be\u00b7sin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann er nicht dem Schauder wehren,", "tokens": ["Kann", "er", "nicht", "dem", "Schau\u00b7der", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er ihm das Herz durchdringe.", "tokens": ["Da\u00df", "er", "ihm", "das", "Herz", "durch\u00b7drin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Am Gestade der Durance", "tokens": ["Am", "Ge\u00b7sta\u00b7de", "der", "Du\u00b7ran\u00b7ce"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht er eines St\u00e4dtchens Mauern,", "tokens": ["Sieht", "er", "ei\u00b7nes", "St\u00e4dt\u00b7chens", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Grauber\u00e4uchert, hin und wieder", "tokens": ["Grau\u00b7be\u00b7r\u00e4u\u00b7chert", ",", "hin", "und", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine stillen H\u00e4user trauern.", "tokens": ["Sei\u00b7ne", "stil\u00b7len", "H\u00e4u\u00b7ser", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Grausenhafte Felsenschl\u00fcnde", "tokens": ["Grau\u00b7sen\u00b7haf\u00b7te", "Fel\u00b7sen\u00b7schl\u00fcn\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht der Wandrer dicht daneben,", "tokens": ["Sieht", "der", "Wand\u00b7rer", "dicht", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selten auf granitnem Blocke", "tokens": ["Sel\u00b7ten", "auf", "gra\u00b7nit\u00b7nem", "Blo\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Strauch im Winde beben.", "tokens": ["Ei\u00b7nen", "Strauch", "im", "Win\u00b7de", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "In dem n\u00e4chtlichen Reviere", "tokens": ["In", "dem", "n\u00e4cht\u00b7li\u00b7chen", "Re\u00b7vie\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scheint der Tod sich zu ergehen,", "tokens": ["Scheint", "der", "Tod", "sich", "zu", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Leben nachzusinnen,", "tokens": ["Und", "den", "Le\u00b7ben", "nach\u00b7zu\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sein Odem wird verwehen.", "tokens": ["Die", "sein", "O\u00b7dem", "wird", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Von den Klippen, wie verzweifelnd,", "tokens": ["Von", "den", "Klip\u00b7pen", ",", "wie", "ver\u00b7zwei\u00b7felnd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzt der Wildbach in die Tiefe,", "tokens": ["St\u00fcrzt", "der", "Wild\u00b7bach", "in", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er brauset in den Schluchten,", "tokens": ["Und", "er", "brau\u00b7set", "in", "den", "Schluch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob er bang nach H\u00fclfe riefe.", "tokens": ["Ob", "er", "bang", "nach", "H\u00fcl\u00b7fe", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Furchtsam ruht am Fu\u00df des Berges", "tokens": ["Furcht\u00b7sam", "ruht", "am", "Fu\u00df", "des", "Ber\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4dtchen Cisteron geschmieget,", "tokens": ["St\u00e4dt\u00b7chen", "Cis\u00b7te\u00b7ron", "ge\u00b7schmie\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie zu des Gebieters F\u00fc\u00dfen", "tokens": ["Wie", "zu", "des", "Ge\u00b7bie\u00b7ters", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinend eine Sklavin lieget.", "tokens": ["Wei\u00b7nend", "ei\u00b7ne", "Skla\u00b7vin", "lie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Auf dem Berge ragt Gem\u00e4uer,", "tokens": ["Auf", "dem", "Ber\u00b7ge", "ragt", "Ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in l\u00e4ngstverblichnem Glanze", "tokens": ["Und", "in", "l\u00e4ngst\u00b7ver\u00b7blich\u00b7nem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrschten hier von ihrem Schlosse", "tokens": ["Herrschten", "hier", "von", "ih\u00b7rem", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einst die Grafen der Provence.", "tokens": ["Einst", "die", "Gra\u00b7fen", "der", "Pro\u00b7ven\u00b7ce", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Wie so traurig da dem Wandrer", "tokens": ["Wie", "so", "trau\u00b7rig", "da", "dem", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verfallnen Th\u00fcrme winken:", "tokens": ["Die", "ver\u00b7fall\u00b7nen", "Th\u00fcr\u00b7me", "win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Edle hier auf Erden,", "tokens": ["Al\u00b7les", "Ed\u00b7le", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles mu\u00df am Ende sinken!", "tokens": ["Al\u00b7les", "mu\u00df", "am", "En\u00b7de", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "An den Th\u00fcrmen, steil und pl\u00f6tzlich,", "tokens": ["An", "den", "Th\u00fcr\u00b7men", ",", "steil", "und", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Hebt sich eine Felsenmasse,", "tokens": ["Hebt", "sich", "ei\u00b7ne", "Fel\u00b7sen\u00b7mas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Herberg f\u00fcr die Wolken,", "tokens": ["Ei\u00b7ne", "Her\u00b7berg", "f\u00fcr", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auszuruh'n auf ihrer Stra\u00dfe.", "tokens": ["Aus\u00b7zu\u00b7ruh'n", "auf", "ih\u00b7rer", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und zuh\u00f6chst am Felsenhaupte", "tokens": ["Und", "zu\u00b7h\u00f6chst", "am", "Fel\u00b7sen\u00b7haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht ein H\u00e4uschen, einsam, w\u00fcste,", "tokens": ["Steht", "ein", "H\u00e4usc\u00b7hen", ",", "ein\u00b7sam", ",", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Heide mit dem Opfer", "tokens": ["Wo", "der", "Hei\u00b7de", "mit", "dem", "Op\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine G\u00f6tter einst begr\u00fc\u00dfte.", "tokens": ["Sei\u00b7ne", "G\u00f6t\u00b7ter", "einst", "be\u00b7gr\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Doch in unsern schlimmen Tagen", "tokens": ["Doch", "in", "un\u00b7sern", "schlim\u00b7men", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward der Tempel zum Gef\u00e4ngni\u00df,", "tokens": ["Ward", "der", "Tem\u00b7pel", "zum", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "