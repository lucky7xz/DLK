{"textgrid.poem.59829": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verbittre dir das junge Leben nicht,", "tokens": ["Ver\u00b7bitt\u00b7re", "dir", "das", "jun\u00b7ge", "Le\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verschm\u00e4he, was dir Gott gegeben, nicht!", "tokens": ["Ver\u00b7schm\u00e4\u00b7he", ",", "was", "dir", "Gott", "ge\u00b7ge\u00b7ben", ",", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "NN", "VVPP", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verschlie\u00df dein Herz der Liebe Offenbarung", "tokens": ["Ver\u00b7schlie\u00df", "dein", "Herz", "der", "Lie\u00b7be", "Of\u00b7fen\u00b7ba\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deinen Mund dem Trank der Reben nicht!", "tokens": ["Und", "dei\u00b7nen", "Mund", "dem", "Trank", "der", "Re\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh, sch\u00f6nern Doppellohn, als Wein und Liebe,", "tokens": ["Sieh", ",", "sch\u00f6\u00b7nern", "Dop\u00b7pel\u00b7lohn", ",", "als", "Wein", "und", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beut dir die Erde f\u00fcr dein Streben nicht!", "tokens": ["Beut", "dir", "die", "Er\u00b7de", "f\u00fcr", "dein", "Stre\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Drum ehre sie als deine Erdeng\u00f6tter,", "tokens": ["Drum", "eh\u00b7re", "sie", "als", "dei\u00b7ne", "Er\u00b7den\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und andern huldige daneben nicht!", "tokens": ["Und", "an\u00b7dern", "hul\u00b7di\u00b7ge", "da\u00b7ne\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Toren, die bis zu dem Jenseits schmachten,", "tokens": ["Die", "To\u00b7ren", ",", "die", "bis", "zu", "dem", "Jen\u00b7seits", "schmach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie lassen leben, doch sie leben nicht!", "tokens": ["Sie", "las\u00b7sen", "le\u00b7ben", ",", "doch", "sie", "le\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "