{"textgrid.poem.42054": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchmal hebe ich meine H\u00e4nde von der Decke ins Licht.", "tokens": ["Manch\u00b7mal", "he\u00b7be", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de", "von", "der", "De\u00b7cke", "ins", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nicht lange, denn sie sind schwer; und sehe wie das Licht", "tokens": ["Nicht", "lan\u00b7ge", ",", "denn", "sie", "sind", "schwer", ";", "und", "se\u00b7he", "wie", "das", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KON", "PPER", "VAFIN", "ADJD", "$.", "KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie umflicht mit einem roten Ge\u00e4st von Blut.", "tokens": ["Sie", "um\u00b7flicht", "mit", "ei\u00b7nem", "ro\u00b7ten", "Ge\u00b7\u00e4st", "von", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich f\u00fchle eine fremde W\u00e4rme, die mir nicht wehe tut,", "tokens": ["Ich", "f\u00fch\u00b7le", "ei\u00b7ne", "frem\u00b7de", "W\u00e4r\u00b7me", ",", "die", "mir", "nicht", "we\u00b7he", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Mich in einen milden Schlafschleier spinnen.", "tokens": ["Mich", "in", "ei\u00b7nen", "mil\u00b7den", "Schlaf\u00b7schlei\u00b7er", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Alle Menschen kommen und gehn und sind gut.", "tokens": ["Al\u00b7le", "Men\u00b7schen", "kom\u00b7men", "und", "gehn", "und", "sind", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "KON", "VVINF", "KON", "VAFIN", "ADJD", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie sagen: ich leide. Doch ich verga\u00df das. \u2013 Leid? \u2013", "tokens": ["Sie", "sa\u00b7gen", ":", "ich", "lei\u00b7de", ".", "Doch", "ich", "ver\u00b7ga\u00df", "das", ".", "\u2013", "Leid", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVINF", "$.", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PDS", "$.", "$(", "NN", "$.", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Ich kann mich dunkel immer nur auf eins besinnen:", "tokens": ["Ich", "kann", "mich", "dun\u00b7kel", "im\u00b7mer", "nur", "auf", "eins", "be\u00b7sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "ADV", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Irgendwo in der Ferne vergeht die Zeit.", "tokens": ["Ir\u00b7gend\u00b7wo", "in", "der", "Fer\u00b7ne", "ver\u00b7geht", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Irgendwo in der Ferne mu\u00df mein Leben verrinnen.", "tokens": ["Ir\u00b7gend\u00b7wo", "in", "der", "Fer\u00b7ne", "mu\u00df", "mein", "Le\u00b7ben", "ver\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Manchmal hebe ich meine H\u00e4nde von der Decke ins Licht.", "tokens": ["Manch\u00b7mal", "he\u00b7be", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de", "von", "der", "De\u00b7cke", "ins", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nicht lange, denn sie sind schwer; und sehe wie das Licht", "tokens": ["Nicht", "lan\u00b7ge", ",", "denn", "sie", "sind", "schwer", ";", "und", "se\u00b7he", "wie", "das", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KON", "PPER", "VAFIN", "ADJD", "$.", "KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie umflicht mit einem roten Ge\u00e4st von Blut.", "tokens": ["Sie", "um\u00b7flicht", "mit", "ei\u00b7nem", "ro\u00b7ten", "Ge\u00b7\u00e4st", "von", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "pun