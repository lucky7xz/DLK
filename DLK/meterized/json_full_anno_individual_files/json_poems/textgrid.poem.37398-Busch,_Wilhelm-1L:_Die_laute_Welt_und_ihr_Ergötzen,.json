{"textgrid.poem.37398": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die laute Welt und ihr Erg\u00f6tzen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die laute Welt und ihr Erg\u00f6tzen,", "tokens": ["Die", "lau\u00b7te", "Welt", "und", "ihr", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als eine st\u00f6rende Erscheinung,", "tokens": ["Als", "ei\u00b7ne", "st\u00f6\u00b7ren\u00b7de", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Vermag der Weise nicht zu sch\u00e4tzen.", "tokens": ["Ver\u00b7mag", "der", "Wei\u00b7se", "nicht", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Maulwurf war der gleichen Meinung.", "tokens": ["Ein", "Maul\u00b7wurf", "war", "der", "glei\u00b7chen", "Mei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er fand an L\u00e4rm kein Wohlgefallen,", "tokens": ["Er", "fand", "an", "L\u00e4rm", "kein", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zog sich zur\u00fcck in k\u00fchle Hallen", "tokens": ["Zog", "sich", "zu\u00b7r\u00fcck", "in", "k\u00fch\u00b7le", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ging daselbst in seinem Fach", "tokens": ["Und", "ging", "da\u00b7selbst", "in", "sei\u00b7nem", "Fach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stillflei\u00dfig den Gesch\u00e4ften nach.", "tokens": ["Still\u00b7flei\u00b7\u00dfig", "den", "Ge\u00b7sch\u00e4f\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zwar sehen konnt er da kein bissel,", "tokens": ["Zwar", "se\u00b7hen", "konnt", "er", "da", "kein", "bis\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Indessen sein getreuer R\u00fcssel,", "tokens": ["In\u00b7des\u00b7sen", "sein", "ge\u00b7treu\u00b7er", "R\u00fcs\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein Nervensitz voll Zartgef\u00fchl,", "tokens": ["Ein", "Ner\u00b7ven\u00b7sitz", "voll", "Zart\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "F\u00fchrt sicher zum erw\u00fcnschten Ziel.", "tokens": ["F\u00fchrt", "si\u00b7cher", "zum", "er\u00b7w\u00fcnschten", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Als Nahrung hat er sich erlesen", "tokens": ["Als", "Nah\u00b7rung", "hat", "er", "sich", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Leckerbissen der Chinesen,", "tokens": ["Die", "Le\u00b7cker\u00b7bis\u00b7sen", "der", "Chi\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Den Regenwurm und Engerling,", "tokens": ["Den", "Re\u00b7gen\u00b7wurm", "und", "En\u00b7ger\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wovon er vielfach fette fing.", "tokens": ["Wo\u00b7von", "er", "viel\u00b7fach", "fet\u00b7te", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die Folge war, was ja kein Wunder,", "tokens": ["Die", "Fol\u00b7ge", "war", ",", "was", "ja", "kein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sein B\u00e4uchlein wurde t\u00e4glich runder,", "tokens": ["Sein", "B\u00e4uch\u00b7lein", "wur\u00b7de", "t\u00e4g\u00b7lich", "run\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und wie das h\u00e4ufig so der Brauch,", "tokens": ["Und", "wie", "das", "h\u00e4u\u00b7fig", "so", "der", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Stolz wuchs mit dem Bauche auch.", "tokens": ["Der", "Stolz", "wuchs", "mit", "dem", "Bau\u00b7che", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wohl ist er stattlich von Person", "tokens": ["Wohl", "ist", "er", "statt\u00b7lich", "von", "Per\u00b7son"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und kleidet sich wie ein Baron,", "tokens": ["Und", "klei\u00b7det", "sich", "wie", "ein", "Ba\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nur schad, ihn und sein Sammetkleid", "tokens": ["Nur", "schad", ",", "ihn", "und", "sein", "Sam\u00b7met\u00b7kleid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sah niemand in der Dunkelheit.", "tokens": ["Sah", "nie\u00b7mand", "in", "der", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So trieb ihn denn der H\u00f6hensinn,", "tokens": ["So", "trieb", "ihn", "denn", "der", "H\u00f6\u00b7hen\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Von unten her nach oben hin,", "tokens": ["Von", "un\u00b7ten", "her", "nach", "o\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Zehn Zoll hoch oder gar noch mehr,", "tokens": ["Zehn", "Zoll", "hoch", "o\u00b7der", "gar", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KON", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Zu seines Namens Ruhm und Ehr", "tokens": ["Zu", "sei\u00b7nes", "Na\u00b7mens", "Ruhm", "und", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Gew\u00f6lbte Tempel zu entwerfen,", "tokens": ["Ge\u00b7w\u00f6lb\u00b7te", "Tem\u00b7pel", "zu", "ent\u00b7wer\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Um denen drau\u00dfen einzusch\u00e4rfen,", "tokens": ["Um", "de\u00b7nen", "drau\u00b7\u00dfen", "ein\u00b7zu\u00b7sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Da\u00df innerhalb noch einer wohne,", "tokens": ["Da\u00df", "in\u00b7ner\u00b7halb", "noch", "ei\u00b7ner", "woh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der etwas kann, was nicht so ohne.", "tokens": ["Der", "et\u00b7was", "kann", ",", "was", "nicht", "so", "oh\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "$,", "PRELS", "PTKNEG", "ADV", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Mit Baulichkeiten ist es mi\u00dflich.", "tokens": ["Mit", "Bau\u00b7lich\u00b7kei\u00b7ten", "ist", "es", "mi\u00df\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ob man sie sch\u00e4tzt, ist ungewi\u00dflich.", "tokens": ["Ob", "man", "sie", "sch\u00e4tzt", ",", "ist", "un\u00b7ge\u00b7wi\u00df\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Ein Mensch von andrem Kunstgeschmacke,", "tokens": ["Ein", "Mensch", "von", "an\u00b7drem", "Kunst\u00b7ge\u00b7schma\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Ein G\u00e4rtner, kam mit einer Hacke.", "tokens": ["Ein", "G\u00e4rt\u00b7ner", ",", "kam", "mit", "ei\u00b7ner", "Ha\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Durch kurzen Hieb nach langer Lauer", "tokens": ["Durch", "kur\u00b7zen", "Hieb", "nach", "lan\u00b7ger", "Lau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Zieht er ans Licht den Tempelbauer", "tokens": ["Zieht", "er", "ans", "Licht", "den", "Tem\u00b7pel\u00b7bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Und haut so derb ihn \u00fcbers Ohr,", "tokens": ["Und", "haut", "so", "derb", "ihn", "\u00fc\u00b7bers", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da\u00df er den Lebensgeist verlor.", "tokens": ["Da\u00df", "er", "den", "Le\u00b7bens\u00b7geist", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Da liegt er nun, der stolze Mann.", "tokens": ["Da", "liegt", "er", "nun", ",", "der", "stol\u00b7ze", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Wer tut die letzte Ehr ihm an?", "tokens": ["Wer", "tut", "die", "letz\u00b7te", "Ehr", "ihm", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Drei K\u00e4fer, schwarz und gelb gefleckt,", "tokens": ["Drei", "K\u00e4\u00b7fer", ",", "schwarz", "und", "gelb", "ge\u00b7fleckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Die haben ihn mit Sand bedeckt.", "tokens": ["Die", "ha\u00b7ben", "ihn", "mit", "Sand", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die laute Welt und ihr Erg\u00f6tzen,", "tokens": ["Die", "lau\u00b7te", "Welt", "und", "ihr", "Er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als eine st\u00f6rende Erscheinung,", "tokens": ["Als", "ei\u00b7ne", "st\u00f6\u00b7ren\u00b7de", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Vermag der Weise nicht zu sch\u00e4tzen.", "tokens": ["Ver\u00b7mag", "der", "Wei\u00b7se", "nicht", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Maulwurf war der gleichen Meinung.", "tokens": ["Ein", "Maul\u00b7wurf", "war", "der", "glei\u00b7chen", "Mei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er fand an L\u00e4rm kein Wohlgefallen,", "tokens": ["Er", "fand", "an", "L\u00e4rm", "kein", "Wohl\u