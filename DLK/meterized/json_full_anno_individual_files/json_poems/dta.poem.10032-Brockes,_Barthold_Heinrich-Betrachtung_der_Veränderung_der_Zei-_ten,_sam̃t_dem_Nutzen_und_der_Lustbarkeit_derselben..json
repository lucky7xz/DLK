{"dta.poem.10032": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung der Ver\u00e4nderung der Zei-  \n ten, sam\u0303t dem Nutzen und der Lustbarkeit  \n derselben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Winter f\u00fcllt die Lufft ein neblicht falbes Grau.\nJm Fr\u00fchling ist sie hell, warm, heiter, rein und\nblau.\nJm Winter schnaubt der Nord mit st\u00fcrmerischer Wuth,", "tokens": ["Jm", "Win\u00b7ter", "f\u00fcllt", "die", "Lufft", "ein", "neb\u00b7licht", "fal\u00b7bes", "Grau", ".", "Jm", "Fr\u00fch\u00b7ling", "ist", "sie", "hell", ",", "warm", ",", "hei\u00b7ter", ",", "rein", "und", "blau", ".", "Jm", "Win\u00b7ter", "schnaubt", "der", "Nord", "mit", "st\u00fcr\u00b7me\u00b7ri\u00b7scher", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "APPR", "ADJA", "NN", "$.", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$.", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Zerschneidet uns die Haut, durchdringet Marck und Blut;", "tokens": ["Zer\u00b7schnei\u00b7det", "uns", "die", "Haut", ",", "durch\u00b7drin\u00b7get", "Marck", "und", "Blut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Balsam-reichem Dufft, die schmeichlend lauen Winde.", "tokens": ["Mit", "Bal\u00b7sam\u00b7rei\u00b7chem", "Dufft", ",", "die", "schmeich\u00b7lend", "lau\u00b7en", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein schroff-und starres Eis, durch rauhe Schollen, auf.", "tokens": ["Ein", "schroff\u00b7\u00b7und", "star\u00b7res", "Eis", ",", "durch", "rau\u00b7he", "Schol\u00b7len", ",", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und bildet Wunder-sch\u00f6nden Schmeltz bebl\u00fchmter H\u00fcgel;", "tokens": ["Und", "bil\u00b7det", "Wun\u00b7der\u00b7sch\u00f6n\u00b7den", "Sch\u00b7meltz", "be\u00b7bl\u00fchm\u00b7ter", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sie rauscht und rieselt sanft, bald durch bestrahlte Felder,", "tokens": ["Sie", "rauscht", "und", "rie\u00b7selt", "sanft", ",", "bald", "durch", "be\u00b7strahl\u00b7te", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald durch die gr\u00fcne Nacht und D\u00e4m\u0303rung dunckler W\u00e4lder.", "tokens": ["Bald", "durch", "die", "gr\u00fc\u00b7ne", "Nacht", "und", "D\u00e4m\u0303rung", "dunck\u00b7ler", "W\u00e4l\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Verw\u00fcstet, traurig, schwartz, wild, h\u00f6ckricht, Felsen-hart.", "tokens": ["Ver\u00b7w\u00fcs\u00b7tet", ",", "trau\u00b7rig", ",", "schwartz", ",", "wild", ",", "h\u00f6c\u00b7kricht", ",", "Fel\u00b7sen\u00b7hart", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Korn und Klee bedeckt, und lieblich anzusehn.", "tokens": ["Mit", "Korn", "und", "Klee", "be\u00b7deckt", ",", "und", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "KON", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit lieblichem Geth\u00f6n. Die s\u00e4urlich-s\u00fcssen Fr\u00fcchte", "tokens": ["Mit", "lieb\u00b7li\u00b7chem", "Ge\u00b7th\u00f6n", ".", "Die", "s\u00e4ur\u00b7lich\u00b7s\u00fcs\u00b7sen", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und mancherley den Gaum erquickende Gerichte,", "tokens": ["Und", "man\u00b7cher\u00b7ley", "den", "Gaum", "er\u00b7qui\u00b7cken\u00b7de", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Raubt uns der rauhe ", "tokens": ["Raubt", "uns", "der", "rau\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Mit ihnen Aug\u2019 und Mund und Hertz uns zu erfreuen.", "tokens": ["Mit", "ih\u00b7nen", "Aug'", "und", "Mund", "und", "Hertz", "uns", "zu", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Wechsel blos allein,", "tokens": ["Der", "Wech\u00b7sel", "blos", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zumahl wenn wir darin die Ordnung \u00fcberlegen,", "tokens": ["Zu\u00b7mahl", "wenn", "wir", "da\u00b7rin", "die", "Ord\u00b7nung", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der Natur Ver\u00e4ndrungen erwegen,", "tokens": ["Und", "der", "Na\u00b7tur", "Ver\u00b7\u00e4n\u00b7drun\u00b7gen", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "---+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die unver\u00e4nderlich; sollt uns ein Zeuge seyn", "tokens": ["Die", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", ";", "sollt", "uns", "ein", "Zeu\u00b7ge", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$.", "VMFIN", "PPER", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Richt nur von GOTTES Macht und Weisheit; Auch", "tokens": ["Richt", "nur", "von", "GoT\u00b7TES", "Macht", "und", "Weis\u00b7heit", ";", "Auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "APPR", "NE", "NN", "KON", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Seiner dem Gesch\u00f6pf\u2019 erzeigten Wunder-Liebe", "tokens": ["Von", "Sei\u00b7ner", "dem", "Ge\u00b7sch\u00f6pf'", "er\u00b7zeig\u00b7ten", "Wun\u00b7der\u00b7Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind klar hierin zu sehn: denn da wir leider blind,", "tokens": ["Sind", "klar", "hie\u00b7rin", "zu", "sehn", ":", "denn", "da", "wir", "lei\u00b7der", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$.", "KON", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jm ruhigen Besitz, f\u00fcr alles Gute, sind;", "tokens": ["Jm", "ru\u00b7hi\u00b7gen", "Be\u00b7sitz", ",", "f\u00fcr", "al\u00b7les", "Gu\u00b7te", ",", "sind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VAFIN", "$."], "meter": "+++--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "So scheint der Wechsel uns, mit lieblicher Gewalt,", "tokens": ["So", "scheint", "der", "Wech\u00b7sel", "uns", ",", "mit", "lieb\u00b7li\u00b7cher", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch Neuigkeit, was s\u00fc\u00df, noch s\u00fcsser zu vers\u00fcssen.", "tokens": ["Durch", "Neu\u00b7ig\u00b7keit", ",", "was", "s\u00fc\u00df", ",", "noch", "s\u00fcs\u00b7ser", "zu", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Damit wir der Gesch\u00f6pf Pracht, Farben und Gestalt", "tokens": ["Da\u00b7mit", "wir", "der", "Ge\u00b7sch\u00f6pf", "Pracht", ",", "Far\u00b7ben", "und", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit mehr Empfindlichkeit geniessen.", "tokens": ["Mit", "mehr", "Emp\u00b7find\u00b7lich\u00b7keit", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn sich der Sommer nie von unsrer Gegend trennte,", "tokens": ["Wenn", "sich", "der", "Som\u00b7mer", "nie", "von", "uns\u00b7rer", "Ge\u00b7gend", "trenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ein gerader Strahl die Felder immer brennte;", "tokens": ["Wenn", "ein", "ge\u00b7ra\u00b7der", "Strahl", "die", "Fel\u00b7der", "im\u00b7mer", "brenn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00fcrde, von dem Schaden nichts zu sagen,", "tokens": ["So", "w\u00fcr\u00b7de", ",", "von", "dem", "Scha\u00b7den", "nichts", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns ein verdrie\u00dflichs Einerley,", "tokens": ["Uns", "ein", "ver\u00b7drie\u00df\u00b7lichs", "Ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch bey stets heiterm Wetter, plagen.", "tokens": ["Auch", "bey", "stets", "hei\u00b7term", "Wet\u00b7ter", ",", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Indem ich die\u00df mit Lust und Danck betrachte,", "tokens": ["In\u00b7dem", "ich", "die\u00df", "mit", "Lust", "und", "Danck", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, wie so angenehm der Wechsel sey,", "tokens": ["Und", ",", "wie", "so", "an\u00b7ge\u00b7nehm", "der", "Wech\u00b7sel", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie n\u00f6thig und wie gut, mit frohem Ernst beachte;", "tokens": ["Wie", "n\u00f6\u00b7thig", "und", "wie", "gut", ",", "mit", "fro\u00b7hem", "Ernst", "be\u00b7ach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fchl ich allererst von neuen,", "tokens": ["So", "f\u00fchl", "ich", "al\u00b7le\u00b7rerst", "von", "neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df unser GOtt nicht blo\u00df im Jahr uns vier mahl nur", "tokens": ["Da\u00df", "un\u00b7ser", "Gott", "nicht", "blo\u00df", "im", "Jahr", "uns", "vier", "mahl", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "APPRART", "NN", "PPER", "CARD", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Aenderung der Zeiten woll\u2019 erfreuen:", "tokens": ["Mit", "A\u00b7en\u00b7de\u00b7rung", "der", "Zei\u00b7ten", "woll'", "er\u00b7freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Es g\u00f6nnet auch der Herrscher der Natur", "tokens": ["Es", "g\u00f6n\u00b7net", "auch", "der", "Herr\u00b7scher", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In einem jeden Tag, uns vier mahl diese Lust;", "tokens": ["In", "ei\u00b7nem", "je\u00b7den", "Tag", ",", "uns", "vier", "mahl", "die\u00b7se", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "PPER", "CARD", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da Morgen, Mittag, Abend, Nacht", "tokens": ["Da", "Mor\u00b7gen", ",", "Mit\u00b7tag", ",", "A\u00b7bend", ",", "Nacht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Uns gleichsam sich, wie iedem ja bewust,", "tokens": ["Uns", "gleich\u00b7sam", "sich", ",", "wie", "ie\u00b7dem", "ja", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PRF", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der es betrachten will,", "tokens": ["Der", "es", "be\u00b7trach\u00b7ten", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Zum Leutzen, Sommer, Herbst und Winter macht.", "tokens": ["Zum", "Leut\u00b7zen", ",", "Som\u00b7mer", ",", "Herbst", "und", "Win\u00b7ter", "macht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": 