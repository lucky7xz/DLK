{"textgrid.poem.55552": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Unbest\u00e4ndigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im spielenden Bache da lieg ich, wie helle!", "tokens": ["Im", "spie\u00b7len\u00b7den", "Ba\u00b7che", "da", "lieg", "ich", ",", "wie", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verbreite die Arme der kommenden Welle,", "tokens": ["Ver\u00b7brei\u00b7te", "die", "Ar\u00b7me", "der", "kom\u00b7men\u00b7den", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und buhlerisch dr\u00fcckt sie die sehnende Brust.", "tokens": ["Und", "buh\u00b7le\u00b7risch", "dr\u00fcckt", "sie", "die", "seh\u00b7nen\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann tr\u00e4gt sie ihr Leichtsinn im Strome darnieder;", "tokens": ["Dann", "tr\u00e4gt", "sie", "ihr", "Leicht\u00b7sinn", "im", "Stro\u00b7me", "dar\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "PAV", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Schon naht sich die zweite und streichelt mich wieder,", "tokens": ["Schon", "naht", "sich", "die", "zwei\u00b7te", "und", "strei\u00b7chelt", "mich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da f\u00fchl ich die Freuden der wechselnden Lust.", "tokens": ["Da", "f\u00fchl", "ich", "die", "Freu\u00b7den", "der", "wech\u00b7seln\u00b7den", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "O J\u00fcngling, sei weise, verwein nicht vergebens", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "sei", "wei\u00b7se", ",", "ver\u00b7wein", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "ADV", "$,", "ADV", "PTKNEG", "ADV"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die fr\u00f6hlichsten Stunden des traurigen Lebens,", "tokens": ["Die", "fr\u00f6h\u00b7lichs\u00b7ten", "Stun\u00b7den", "des", "trau\u00b7ri\u00b7gen", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wenn flatterhaft je dich ein M\u00e4dchen vergi\u00dft.", "tokens": ["Wenn", "flat\u00b7ter\u00b7haft", "je", "dich", "ein", "M\u00e4d\u00b7chen", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Geh, ruf sie zur\u00fccke, die vorigen Zeiten!", "tokens": ["Geh", ",", "ruf", "sie", "zu\u00b7r\u00fc\u00b7cke", ",", "die", "vo\u00b7ri\u00b7gen", "Zei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es k\u00fc\u00dft sich so s\u00fc\u00dfe der Busen der zweiten,", "tokens": ["Es", "k\u00fc\u00dft", "sich", "so", "s\u00fc\u00b7\u00dfe", "der", "Bu\u00b7sen", "der", "zwei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Als kaum sich der Busen der ersten gek\u00fc\u00dft.", "tokens": ["Als", "kaum", "sich", "der", "Bu\u00b7sen", "der", "ers\u00b7ten", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Im spielenden Bache da lieg ich, wie helle!", "tokens": ["Im", "spie\u00b7len\u00b7den", "Ba\u00b7che", "da", "lieg", "ich", ",", "wie", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verbreite die Arme der kommenden Welle,", "tokens": ["Ver\u00b7brei\u00b7te", "die", "Ar\u00b7me", "der", "kom\u00b7men\u00b7den", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tet