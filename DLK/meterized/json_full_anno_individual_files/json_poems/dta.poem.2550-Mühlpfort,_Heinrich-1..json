{"dta.poem.2550": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erhell aus dem Schilffedein blaues Gesichte", "tokens": ["Er\u00b7hell", "aus", "dem", "Schilf\u00b7fe\u00b7dein", "blau\u00b7es", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sanfft-wallender Oder-Strohm/ silberner Flu\u00df", "tokens": ["Sanf\u00b7ft\u00b7wal\u00b7len\u00b7der", "O\u00b7der\u00b7Strohm", "/", "sil\u00b7ber\u00b7ner", "Flu\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$(", "ADJA", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Segel zu f\u00f6dern in gleichem Gewichte", "tokens": ["Die", "Se\u00b7gel", "zu", "f\u00f6\u00b7dern", "in", "glei\u00b7chem", "Ge\u00b7wich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "So schwelle die Adern in st\u00e4rckerem Gu\u00df/", "tokens": ["So", "schwel\u00b7le", "die", "A\u00b7dern", "in", "st\u00e4r\u00b7cke\u00b7rem", "Gu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Verz\u00f6gre nicht deine Cristalline Fluth", "tokens": ["Ver\u00b7z\u00f6g\u00b7re", "nicht", "dei\u00b7ne", "Cris\u00b7tal\u00b7li\u00b7ne", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Du f\u00fchrest auff heut ein unsch\u00e4tzbares Gut.", "tokens": ["Du", "f\u00fch\u00b7rest", "auff", "heut", "ein", "un\u00b7sch\u00e4tz\u00b7ba\u00b7res", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Kein st\u00fcrmender Nordwind entb\u00f6hre die Wellen", "tokens": ["Kein", "st\u00fcr\u00b7men\u00b7der", "Nord\u00b7wind", "ent\u00b7b\u00f6h\u00b7re", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Weil Mar garis deinem Gew\u00e4sser vertraut", "tokens": ["Weil", "Mar", "ga\u00b7ris", "dei\u00b7nem", "Ge\u00b7w\u00e4s\u00b7ser", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "PPOSAT", "NN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es m\u00fcsse sich liebliches Wetter erhellen", "tokens": ["Es", "m\u00fcs\u00b7se", "sich", "lieb\u00b7li\u00b7ches", "Wet\u00b7ter", "er\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJA", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da\u00df sie mit gesegneten Regen bethaut/", "tokens": ["Da\u00df", "sie", "mit", "ge\u00b7se\u00b7gne\u00b7ten", "Re\u00b7gen", "be\u00b7thaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der Zefyr-Wind blase Narcissen herf\u00fcr", "tokens": ["Der", "Ze\u00b7fy\u00b7rWind", "bla\u00b7se", "Nar\u00b7cis\u00b7sen", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und mehre der Margaris him\u0303lische Zier.", "tokens": ["Und", "meh\u00b7re", "der", "Mar\u00b7ga\u00b7ris", "him\u0303\u00b7li\u00b7sche", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Auff Franckfurt bereite dich sie zu empfangen/", "tokens": ["Auff", "Fran\u00b7ck\u00b7furt", "be\u00b7rei\u00b7te", "dich", "sie", "zu", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Sohn zeugt begl\u00fccket nach Hause mit Beut/", "tokens": ["Dein", "Sohn", "zeugt", "be\u00b7gl\u00fc\u00b7cket", "nach", "Hau\u00b7se", "mit", "Beut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "APPR", "NN", "APPR", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Jhr Najaden kommt ihm entgegen gegangen", "tokens": ["Ihr", "Na\u00b7ja\u00b7den", "kommt", "ihm", "ent\u00b7ge\u00b7gen", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die ihr umb dieselbigen Ufer rumb seyd", "tokens": ["Die", "ihr", "umb", "die\u00b7sel\u00b7bi\u00b7gen", "U\u00b7fer", "rumb", "seyd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "ADJD", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Verehret die neue willkommene Braut", "tokens": ["Ver\u00b7eh\u00b7ret", "die", "neu\u00b7e", "will\u00b7kom\u00b7me\u00b7ne", "Braut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Mit Gaben/ die euch der Geliebte vertraut.", "tokens": ["Mit", "Ga\u00b7ben", "/", "die", "euch", "der", "Ge\u00b7lieb\u00b7te", "ver\u00b7traut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PRELS", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "So bl\u00fche nun Margaris unter den Reben/", "tokens": ["So", "bl\u00fc\u00b7he", "nun", "Mar\u00b7ga\u00b7ris", "un\u00b7ter", "den", "Re\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPR", "ART", "NN", "$("], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und siege dem Weinstock an Fruchtbarkeit ab/", "tokens": ["Und", "sie\u00b7ge", "dem", "Wein\u00b7stock", "an", "Frucht\u00b7bar\u00b7keit", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wir wollen dir unser Geleite hingeben/", "tokens": ["Wir", "wol\u00b7len", "dir", "un\u00b7ser", "Ge\u00b7lei\u00b7te", "hin\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schencken dir diesen umbwundenen Stab", "tokens": ["Und", "schen\u00b7cken", "dir", "die\u00b7sen", "um\u00b7bwun\u00b7de\u00b7nen", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Zum Zeichen gepflogener Freundschafft und Lust", "tokens": ["Zum", "Zei\u00b7chen", "ge\u00b7pflo\u00b7ge\u00b7ner", "Freund\u00b7schafft", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der Margaris sey nichts als Wohlstand bewust.", "tokens": ["Der", "Mar\u00b7ga\u00b7ris", "sey", "nichts", "als", "Wohl\u00b7stand", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wie dieser Freuden-Wunsch zu Ende war gebracht/", "tokens": ["Wie", "die\u00b7ser", "Freu\u00b7den\u00b7Wunsch", "zu", "En\u00b7de", "war", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt Venus Kammer-Magd/ und saget wie der Wagen", "tokens": ["Kommt", "Ve\u00b7nus", "Kam\u00b7mer\u00b7Magd", "/", "und", "sa\u00b7get", "wie", "der", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "NN", "$(", "KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schon fertig angespannt/ die Nymfen heim zu tragen/", "tokens": ["Schon", "fer\u00b7tig", "an\u00b7ge\u00b7spannt", "/", "die", "Nym\u00b7fen", "heim", "zu", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$(", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie geben wiederumb der Braut noch gute Nacht", "tokens": ["Sie", "ge\u00b7ben", "wie\u00b7de\u00b7rumb", "der", "Braut", "noch", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201cund ziehn was traurig fort/ weil Margaris vergn\u00fcget/", "tokens": ["\u201c", "und", "ziehn", "was", "trau\u00b7rig", "fort", "/", "weil", "Mar\u00b7ga\u00b7ris", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVINF", "PWS", "ADJD", "PTKVZ", "$(", "KOUS", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "&q;Bey ihnen aber Stroh\u2019 und Feur verborgen lieget.", "tokens": ["&", "q", ";", "Bey", "ih\u00b7nen", "a\u00b7ber", "Stroh'", "und", "Feur", "ver\u00b7bor\u00b7gen", "lie\u00b7get", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$.", "APPR", "PPER", "ADV", "NN", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}