{"textgrid.poem.42949": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Drei Tage Tirol", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nach Tirol gereist", "tokens": ["Ich", "bin", "nach", "Ti\u00b7rol", "ge\u00b7reist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hab das Zuhause vergessen.", "tokens": ["Und", "hab", "das", "Zu\u00b7hau\u00b7se", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich habe viel Freiheit gefressen", "tokens": ["Ich", "ha\u00b7be", "viel", "Frei\u00b7heit", "ge\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und viel Gesellschaft gespeist.", "tokens": ["Und", "viel", "Ge\u00b7sell\u00b7schaft", "ge\u00b7speist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Landschaften hab ich gesoffen", "tokens": ["Land\u00b7schaf\u00b7ten", "hab", "ich", "ge\u00b7sof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und Illusionen geraucht.", "tokens": ["Und", "Il\u00b7lu\u00b7si\u00b7o\u00b7nen", "ge\u00b7raucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die Menschen, die ich getroffen,", "tokens": ["Die", "Men\u00b7schen", ",", "die", "ich", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Standen meist so zu den Sternen,", "tokens": ["Stan\u00b7den", "meist", "so", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man, um sie kennenzulernen,", "tokens": ["Da\u00df", "man", ",", "um", "sie", "ken\u00b7nen\u00b7zu\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUI", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nicht erst zu verreisen braucht.", "tokens": ["Nicht", "erst", "zu", "ver\u00b7rei\u00b7sen", "braucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das nennt man Drahtseilbahn: Es hing", "tokens": ["Das", "nennt", "man", "Draht\u00b7seil\u00b7bahn", ":", "Es", "hing"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Z\u00fcndholzsch\u00e4chtelchen an Zwirn.", "tokens": ["Ein", "Z\u00fcnd\u00b7holz\u00b7sch\u00e4ch\u00b7tel\u00b7chen", "an", "Zwirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ein Gewitter 