{"dta.poem.16330": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In diesen Zeiten darfst du Achtung keiner Arten", "tokens": ["In", "die\u00b7sen", "Zei\u00b7ten", "darfst", "du", "Ach\u00b7tung", "kei\u00b7ner", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von keinem, wie er tief steh' unter dir, erwarten,", "tokens": ["Von", "kei\u00b7nem", ",", "wie", "er", "tief", "steh'", "un\u00b7ter", "dir", ",", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "APPR", "PPER", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du nicht \u00e4u\u00dferlich Macht \u00fcber ihn gewannst,", "tokens": ["Wenn", "du", "nicht", "\u00e4u\u00b7\u00dfer\u00b7lich", "Macht", "\u00fc\u00b7ber", "ihn", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihm unmittelbar empfindlich schaden kannst.", "tokens": ["Und", "ihm", "un\u00b7mit\u00b7tel\u00b7bar", "emp\u00b7find\u00b7lich", "scha\u00b7den", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kein Ansehn der Person, wie vorl\u00e4ngst keins bei Gotte", "tokens": ["Kein", "An\u00b7sehn", "der", "Per\u00b7son", ",", "wie", "vor\u00b7l\u00e4ngst", "keins", "bei", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PWAV", "ADV", "PIAT", "APPR", "NN"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gegolten, gilt nunmehr auch keins bei dieser Rotte.", "tokens": ["Ge\u00b7gol\u00b7ten", ",", "gilt", "nun\u00b7mehr", "auch", "keins", "bei", "die\u00b7ser", "Rot\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "PIAT", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nothwendig ist auch das, soll freies Volk erstehn,", "tokens": ["Noth\u00b7wen\u00b7dig", "ist", "auch", "das", ",", "soll", "frei\u00b7es", "Volk", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PDS", "$,", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mu\u00dft du freiem Volk h\u00fcbsch aus dem Wege gehn.", "tokens": ["Doch", "mu\u00dft", "du", "frei\u00b7em", "Volk", "h\u00fcbsch", "aus", "dem", "We\u00b7ge", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}