{"dta.poem.19540": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Gesang des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wolken, meine Kinder, wandern gehen", "tokens": ["Wol\u00b7ken", ",", "mei\u00b7ne", "Kin\u00b7der", ",", "wan\u00b7dern", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wollt ihr? Fahret wohl! Auf Wiedersehen!", "tokens": ["Wollt", "ihr", "?", "Fah\u00b7ret", "wohl", "!", "Auf", "Wie\u00b7der\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$.", "VVFIN", "ADV", "$.", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eure wandellustigen Gestalten", "tokens": ["Eu\u00b7re", "wan\u00b7del\u00b7lus\u00b7ti\u00b7gen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kann ich nicht in Mutterbanden halten.", "tokens": ["Kann", "ich", "nicht", "in", "Mut\u00b7ter\u00b7ban\u00b7den", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Von der Erde seid ihr angezogen:", "tokens": ["Von", "der", "Er\u00b7de", "seid", "ihr", "an\u00b7ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blaue Gipfel! K\u00fcsten weit gebogen!", "tokens": ["Blau\u00b7e", "Gip\u00b7fel", "!", "K\u00fcs\u00b7ten", "weit", "ge\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dort der Stern ist eines Leuchtthurms Feuer!", "tokens": ["Dort", "der", "Stern", "ist", "ei\u00b7nes", "Leucht\u00b7thurms", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ziehet, Kinder! Suchet Abenteuer!", "tokens": ["Zie\u00b7het", ",", "Kin\u00b7der", "!", "Su\u00b7chet", "A\u00b7bent\u00b7eu\u00b7er", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Segelt, k\u00fchne Schiffer, in den L\u00fcften!", "tokens": ["Se\u00b7gelt", ",", "k\u00fch\u00b7ne", "Schif\u00b7fer", ",", "in", "den", "L\u00fcf\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ruhet, sel'ge Geister, \u00fcber Kl\u00fcften!", "tokens": ["Ru\u00b7het", ",", "sel'\u00b7ge", "Geis\u00b7ter", ",", "\u00fc\u00b7ber", "Kl\u00fcf\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bauet Th\u00fcrme! Blitzet! Liefert Schlachten!", "tokens": ["Bau\u00b7et", "Th\u00fcr\u00b7me", "!", "Blit\u00b7zet", "!", "Lie\u00b7fert", "Schlach\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "$.", "NE", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Traget gl\u00fch'nden Kampfes Purpurtrachten!", "tokens": ["Tra\u00b7get", "gl\u00fch'n\u00b7den", "Kamp\u00b7fes", "Pur\u00b7pur\u00b7trach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Rauscht im Regen! Murmelt in den Quellen!", "tokens": ["Rauscht", "im", "Re\u00b7gen", "!", "Mur\u00b7melt", "in", "den", "Quel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcllt die Brunnen! Rieselt in die Wellen!", "tokens": ["F\u00fcllt", "die", "Brun\u00b7nen", "!", "Rie\u00b7selt", "in", "die", "Wel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Braust in Str\u00f6men durch die Lande nieder \u2014", "tokens": ["Braust", "in", "Str\u00f6\u00b7men", "durch", "die", "Lan\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kommet, meine Kinder, kommet wieder!", "tokens": ["Kom\u00b7met", ",", "mei\u00b7ne", "Kin\u00b7der", ",", "kom\u00b7met", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}