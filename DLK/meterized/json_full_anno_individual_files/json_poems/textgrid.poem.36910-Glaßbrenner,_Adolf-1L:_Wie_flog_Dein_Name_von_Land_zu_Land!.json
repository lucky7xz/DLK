{"textgrid.poem.36910": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie flog Dein Name von Land zu Land!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie flog Dein Name von Land zu Land!", "tokens": ["Wie", "flog", "Dein", "Na\u00b7me", "von", "Land", "zu", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie eine s\u00fc\u00dfe Legende!", "tokens": ["Wie", "ei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Le\u00b7gen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Du schwangst Dich auf das gefl\u00fcgelte Ro\u00df", "tokens": ["Du", "schwangst", "Dich", "auf", "das", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So muthig, so behende.", "tokens": ["So", "mut\u00b7hig", ",", "so", "be\u00b7hen\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJA", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Der Freiheit, der Freiheit! erscholl Dein Gesang:", "tokens": ["Der", "Frei\u00b7heit", ",", "der", "Frei\u00b7heit", "!", "er\u00b7scholl", "Dein", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "ADJD", "PPOSAT", "NN