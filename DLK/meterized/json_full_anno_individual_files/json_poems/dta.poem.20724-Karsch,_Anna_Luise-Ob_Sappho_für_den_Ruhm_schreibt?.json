{"dta.poem.20724": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ob Sappho  \n f\u00fcr den Ruhm schreibt?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frau, schreib ich f\u00fcr den Ruhm, und f\u00fcr die Ewigkeit?               ", "tokens": ["Frau", ",", "schreib", "ich", "f\u00fcr", "den", "Ruhm", ",", "und", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein, zum Vergn\u00fcgen meiner Freunde!", "tokens": ["Nein", ",", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "mei\u00b7ner", "Freun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O das Ger\u00fcchte tr\u00e4gt nur eine kurze Zeit", "tokens": ["O", "das", "Ge\u00b7r\u00fcch\u00b7te", "tr\u00e4gt", "nur", "ei\u00b7ne", "kur\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit unserm Ruhme sich; sobald wir von dem Feinde", "tokens": ["Mit", "un\u00b7serm", "Ruh\u00b7me", "sich", ";", "so\u00b7bald", "wir", "von", "dem", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$.", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Menschheit \u00fcberwunden sind,", "tokens": ["Der", "Menschheit", "\u00fc\u00b7berw\u00b7un\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Verflattert er so leicht wie Bl\u00e4tter, die der Wind", "tokens": ["Ver\u00b7flat\u00b7tert", "er", "so", "leicht", "wie", "Bl\u00e4t\u00b7ter", ",", "die", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In irgend einen Flu\u00df gewaltig fortgetrieben,", "tokens": ["In", "ir\u00b7gend", "ei\u00b7nen", "Flu\u00df", "ge\u00b7wal\u00b7tig", "fort\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Homer, Virgil, Horaz und Pindar sind geblieben;", "tokens": ["Ho\u00b7mer", ",", "Vir\u00b7gil", ",", "Ho\u00b7raz", "und", "Pin\u00b7dar", "sind", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Die Griechin aber nicht, die meine Leyer trug,", "tokens": ["Die", "Grie\u00b7ch\u00b7in", "a\u00b7ber", "nicht", ",", "die", "mei\u00b7ne", "Le\u00b7yer", "trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "So z\u00e4rtlich war wie ich, nach ihrem Phaon frug", "tokens": ["So", "z\u00e4rt\u00b7lich", "war", "wie", "ich", ",", "nach", "ih\u00b7rem", "Phaon", "frug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "KOKOM", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und nach dem Leben nicht; sie flog zum Tode wieder.", "tokens": ["Und", "nach", "dem", "Le\u00b7ben", "nicht", ";", "sie", "flog", "zum", "To\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "$.", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nichts blieb uns als ein Brief und zwey beflammte", "tokens": ["Nichts", "blieb", "uns", "als", "ein", "Brief", "und", "zwey", "be\u00b7flamm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "KOUS", "ART", "NN", "KON", "CARD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Lieder.", "tokens": ["Lie\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Die andern schrieb der Neid sich diebisch heimlich ab,", "tokens": ["Die", "an\u00b7dern", "schrieb", "der", "Neid", "sich", "die\u00b7bisch", "heim\u00b7lich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "PRF", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und endlich fanden sie in einem Brand ihr Grab!", "tokens": ["Und", "end\u00b7lich", "fan\u00b7den", "sie", "in", "ei\u00b7nem", "Brand", "ihr", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "O Sapho war ber\u00fchmt! ihr Volk, ein Volk von Prinzen,", "tokens": ["O", "Sa\u00b7pho", "war", "be\u00b7r\u00fchmt", "!", "ihr", "Volk", ",", "ein", "Volk", "von", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Trug seine Dichterin auf viel Ged\u00e4chtni\u00df-M\u00fcnzen,", "tokens": ["Trug", "sei\u00b7ne", "Dich\u00b7te\u00b7rin", "auf", "viel", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7M\u00fcn\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und mancher K\u00fcnstler hieb ihr Bild in Marmor aus,", "tokens": ["Und", "man\u00b7cher", "K\u00fcnst\u00b7ler", "hieb", "ihr", "Bild", "in", "Mar\u00b7mor", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und Kenner redeten ihr Lob bey jedem Schmau\u00df.", "tokens": ["Und", "Ken\u00b7ner", "re\u00b7de\u00b7ten", "ihr", "Lob", "bey", "je\u00b7dem", "Schmau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Halb G\u00f6ttin war das Weib; neun B\u00fccher schrieb sie voll", "tokens": ["Halb", "G\u00f6t\u00b7tin", "war", "das", "Weib", ";", "neun", "B\u00fc\u00b7cher", "schrieb", "sie", "voll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VAFIN", "ART", "NN", "$.", "CARD", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So sch\u00f6n, als w\u00e4ren sie geschrieben vom Apoll.", "tokens": ["So", "sch\u00f6n", ",", "als", "w\u00e4\u00b7ren", "sie", "ge\u00b7schrie\u00b7ben", "vom", "A\u00b7poll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und ach! von alle dem, was sie so sch\u00f6n geschrieben,", "tokens": ["Und", "ach", "!", "von", "al\u00b7le", "dem", ",", "was", "sie", "so", "sch\u00f6n", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "PIS", "ART", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ist nur ein kleiner Rest f\u00fcr unsre Zeit geblieben!", "tokens": ["Ist", "nur", "ein", "klei\u00b7ner", "Rest", "f\u00fcr", "uns\u00b7re", "Zeit", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Frau, solch ein Schicksal trift auch meine Lieder einst!", "tokens": ["Frau", ",", "solch", "ein", "Schick\u00b7sal", "trift", "auch", "mei\u00b7ne", "Lie\u00b7der", "einst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wenn Du voll Z\u00e4rtlichkeit bey meiner Asche weinst.", "tokens": ["Wenn", "Du", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", "bey", "mei\u00b7ner", "A\u00b7sche", "weinst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Noch ehe sich an mir die W\u00fcrmer satt gefressen,", "tokens": ["Noch", "e\u00b7he", "sich", "an", "mir", "die", "W\u00fcr\u00b7mer", "satt", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "APPR", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Dann, Frau, hat schon die Welt mich und mein Buch", "tokens": ["Dann", ",", "Frau", ",", "hat", "schon", "die", "Welt", "mich", "und", "mein", "Buch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "vergessen rc.", "tokens": ["ver\u00b7ges\u00b7sen", "rc", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}