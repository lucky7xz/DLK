{"dta.poem.10935": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Meynung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wir neulich sahn und h\u00f6rten,", "tokens": ["Wie", "wir", "neu\u00b7lich", "sahn", "und", "h\u00f6r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df verschiedne Weisen lehrten,", "tokens": ["Da\u00df", "ver\u00b7schied\u00b7ne", "Wei\u00b7sen", "lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie durch Meynung auf der Erde,", "tokens": ["Wie", "durch", "Mey\u00b7nung", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedermann regieret werde:", "tokens": ["Je\u00b7der\u00b7mann", "re\u00b7gie\u00b7ret", "wer\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dachten wir im Ernst dabey,", "tokens": ["Dach\u00b7ten", "wir", "im", "Ernst", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die Meynung Phantasey.", "tokens": ["Da\u00df", "die", "Mey\u00b7nung", "Phan\u00b7ta\u00b7sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Draus wir denn die Lehre fa\u00dften:", "tokens": ["Draus", "wir", "denn", "die", "Leh\u00b7re", "fa\u00df\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Menschen sind Phantasten;", "tokens": ["Al\u00b7le", "Men\u00b7schen", "sind", "Phan\u00b7tas\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der Allergr\u00f6\u00dfte scheint,", "tokens": ["Und", "der", "Al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Der es nicht zu seyn vermeynt.", "tokens": ["Der", "es", "nicht", "zu", "seyn", "ver\u00b7meynt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "PTKZU", "VAINF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}