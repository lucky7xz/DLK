{"dta.poem.3127": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "172.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Markwart Persiens, als er zum Omar kam,", "tokens": ["Der", "Mark\u00b7wart", "Per\u00b7siens", ",", "als", "er", "zum", "O\u00b7mar", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KOUS", "PPER", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie staunt' er, als er nichts von K\u00f6nigspracht vernahm.", "tokens": ["Wie", "staunt'", "er", ",", "als", "er", "nichts", "von", "K\u00f6\u00b7nig\u00b7spracht", "ver\u00b7nahm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von aller Pracht, die scheint den F\u00fcrsten zu geb\u00fchren,", "tokens": ["Von", "al\u00b7ler", "Pracht", ",", "die", "scheint", "den", "F\u00fcrs\u00b7ten", "zu", "ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War da beim F\u00fcrsten nichts der Gl\u00e4ubigen zu sp\u00fcren.", "tokens": ["War", "da", "beim", "F\u00fcrs\u00b7ten", "nichts", "der", "Gl\u00e4u\u00b7bi\u00b7gen", "zu", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "PIS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er klopft' an Omars Haus. \u201eGrad' ist er ausgegangen.\u201c", "tokens": ["Er", "klopft'", "an", "O\u00b7mars", "Haus", ".", "\u201e", "Grad'", "ist", "er", "aus\u00b7ge\u00b7gan\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "$.", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201e\u201eWohin?\u201c\u201c Die Kunde war von Niemand zu erlangen.", "tokens": ["\u201e", "\u201e", "Wo\u00b7hin", "?", "\u201c", "\u201c", "Die", "Kun\u00b7de", "war", "von", "Nie\u00b7mand", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["punct", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PWAV", "$.", "$(", "$(", "ART", "NN", "VAFIN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Gassen geht er durch, und fragt, wo Omar sei,", "tokens": ["Die", "Gas\u00b7sen", "geht", "er", "durch", ",", "und", "fragt", ",", "wo", "O\u00b7mar", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,", "PWAV", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcberall wird ihm gesagt: Er ging vorbei.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "wird", "ihm", "ge\u00b7sagt", ":", "Er", "ging", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hier hat er das gemacht, hier hat er das befohlen;", "tokens": ["Hier", "hat", "er", "das", "ge\u00b7macht", ",", "hier", "hat", "er", "das", "be\u00b7foh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier hat er was gebracht, hier kam er was zu holen.", "tokens": ["Hier", "hat", "er", "was", "ge\u00b7bracht", ",", "hier", "kam", "er", "was", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Perser Markwart denkt in seinem stolzen Muth:", "tokens": ["Der", "Per\u00b7ser", "Mark\u00b7wart", "denkt", "in", "sei\u00b7nem", "stol\u00b7zen", "Muth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist das f\u00fcr ein F\u00fcrst, der alles selber thut;", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "F\u00fcrst", ",", "der", "al\u00b7les", "sel\u00b7ber", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was f\u00fcr ein F\u00fcrst, der sich bedienen selber mu\u00df,", "tokens": ["Was", "f\u00fcr", "ein", "F\u00fcrst", ",", "der", "sich", "be\u00b7die\u00b7nen", "sel\u00b7ber", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ohne Leibwach' aus dem Hause geht zu Fu\u00df;", "tokens": ["Der", "oh\u00b7ne", "Leib\u00b7wach'", "aus", "dem", "Hau\u00b7se", "geht", "zu", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der \u00fcberall geh\u00f6rt, und nirgends wird gefunden,", "tokens": ["Der", "\u00fc\u00b7be\u00b7rall", "ge\u00b7h\u00f6rt", ",", "und", "nir\u00b7gends", "wird", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dessen Spuren so sind unterm Volk geschwunden? \u2014", "tokens": ["Und", "des\u00b7sen", "Spu\u00b7ren", "so", "sind", "un\u00b7term", "Volk", "ge\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "VAFIN", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Zuletzt umfragend nun kommt er zum Bethaus hin;", "tokens": ["Zu\u00b7letzt", "um\u00b7fra\u00b7gend", "nun", "kommt", "er", "zum", "Be\u00b7thaus", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der F\u00fcrst der Gl\u00e4ubigen, so h\u00f6rt er, schl\u00e4ft darin.", "tokens": ["Der", "F\u00fcrst", "der", "Gl\u00e4u\u00b7bi\u00b7gen", ",", "so", "h\u00f6rt", "er", ",", "schl\u00e4ft", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und schlafen sieht er ihn am Boden in der Ecke,", "tokens": ["Und", "schla\u00b7fen", "sieht", "er", "ihn", "am", "Bo\u00b7den", "in", "der", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wundert sich, da\u00df ihn kein goldner Himmel decke.", "tokens": ["Und", "wun\u00b7dert", "sich", ",", "da\u00df", "ihn", "kein", "gold\u00b7ner", "Him\u00b7mel", "de\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Was ist das f\u00fcr ein F\u00fcrst, spricht er in hehrem Muth,", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "F\u00fcrst", ",", "spricht", "er", "in", "heh\u00b7rem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ohne Menschenhut im Gotteshause ruht?", "tokens": ["Der", "oh\u00b7ne", "Men\u00b7schen\u00b7hut", "im", "Got\u00b7tes\u00b7hau\u00b7se", "ruht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Doch Omar wachet auf, und zeigt in seinem Blicke", "tokens": ["Doch", "O\u00b7mar", "wa\u00b7chet", "auf", ",", "und", "zeigt", "in", "sei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das seiner Macht von Gott vertraute Weltgeschicke.", "tokens": ["Das", "sei\u00b7ner", "Macht", "von", "Gott", "ver\u00b7trau\u00b7te", "Welt\u00b7ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}