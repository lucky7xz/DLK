{"textgrid.poem.25114": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie \u00f6de trauert diese heil'ge Welt", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie \u00f6de trauert diese heil'ge Welt", "tokens": ["Wie", "\u00f6\u00b7de", "trau\u00b7ert", "die\u00b7se", "heil'\u00b7ge", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im zweifelhaften Schein der Tageshelle,", "tokens": ["Im", "zwei\u00b7fel\u00b7haf\u00b7ten", "Schein", "der", "Ta\u00b7ges\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die d\u00e4mmernd durch die Bogenfenster f\u00e4llt", "tokens": ["Die", "d\u00e4m\u00b7mernd", "durch", "die", "Bo\u00b7gen\u00b7fens\u00b7ter", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zitternd schleicht um Altar und Kapelle.", "tokens": ["Und", "zit\u00b7ternd", "schleicht", "um", "Al\u00b7tar", "und", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bisweilen nur, unheimlich wie im Traum,", "tokens": ["Bis\u00b7wei\u00b7len", "nur", ",", "un\u00b7heim\u00b7lich", "wie", "im", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Scheint sich der Tempel wundersam zu regen,", "tokens": ["Scheint", "sich", "der", "Tem\u00b7pel", "wun\u00b7der\u00b7sam", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein innres Atmen den geweihten Raum", "tokens": ["Ein", "inn\u00b7res", "At\u00b7men", "den", "ge\u00b7weih\u00b7ten", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit geisterhaftem Leben zu bewegen.", "tokens": ["Mit", "geis\u00b7ter\u00b7haf\u00b7tem", "Le\u00b7ben", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann h\u00f6rt man durch die Stille dumpf und schwer", "tokens": ["Dann", "h\u00f6rt", "man", "durch", "die", "Stil\u00b7le", "dumpf", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verloren einzle Glockenkl\u00e4nge hallen,", "tokens": ["Ver\u00b7lo\u00b7ren", "einz\u00b7le", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7ge", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie vor dem Sturme auf ein schweigend Meer", "tokens": ["Wie", "vor", "dem", "Stur\u00b7me", "auf", "ein", "schwei\u00b7gend", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "ADJD", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Tropfen der Gewitterwolke fallen.", "tokens": ["Die", "Trop\u00b7fen", "der", "Ge\u00b7wit\u00b7ter\u00b7wol\u00b7ke", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein bleiches Weib, ein Geist vom Ehedem,", "tokens": ["Ein", "blei\u00b7ches", "Weib", ",", "ein", "Geist", "vom", "E\u00b7he\u00b7dem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wallt durch den Dom; gel\u00f6st sind ihre Haare,", "tokens": ["Wallt", "durch", "den", "Dom", ";", "ge\u00b7l\u00f6st", "sind", "ih\u00b7re", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Halb von der Stirne sank das Diadem,", "tokens": ["Halb", "von", "der", "Stir\u00b7ne", "sank", "das", "Di\u00b7a\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein Trauerkleid umflie\u00dft die Wunderbare.", "tokens": ["Ein", "Trau\u00b7er\u00b7kleid", "um\u00b7flie\u00dft", "die", "Wun\u00b7der\u00b7ba\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gebrochnen Schrittes wankt sie hin; sie blickt", "tokens": ["Ge\u00b7broch\u00b7nen", "Schrit\u00b7tes", "wankt", "sie", "hin", ";", "sie", "blickt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kaisers\u00e4rge an mit stummem Harme", "tokens": ["Die", "Kai\u00b7ser\u00b7s\u00e4r\u00b7ge", "an", "mit", "stum\u00b7mem", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hebt mit Klagerufen, halb erstickt,", "tokens": ["Und", "hebt", "mit", "Kla\u00b7ge\u00b7ru\u00b7fen", ",", "halb", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um Rache flehend himmelw\u00e4rts die Arme.", "tokens": ["Um", "Ra\u00b7che", "fle\u00b7hend", "him\u00b7mel\u00b7w\u00e4rts", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVPP", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da aus der Orgel bricht ein m\u00e4cht'ger Schall,", "tokens": ["Da", "aus", "der", "Or\u00b7gel", "bricht", "ein", "m\u00e4cht'\u00b7ger", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sterbeseufzer, ihrer Brust entquollen,", "tokens": ["Ein", "Ster\u00b7be\u00b7seuf\u00b7zer", ",", "ih\u00b7rer", "Brust", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der bei der S\u00e4uleng\u00e4nge Wiederhall", "tokens": ["Der", "bei", "der", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7ge", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch das Gew\u00f6lbe schleicht mit dumpfem Rollen.", "tokens": ["Durch", "das", "Ge\u00b7w\u00f6l\u00b7be", "schleicht", "mit", "dum\u00b7pfem", "Rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Und von dem Riesenklang erbebt das Licht", "tokens": ["Und", "von", "dem", "Rie\u00b7sen\u00b7klang", "er\u00b7bebt", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Lampen, die auf den Alt\u00e4ren schimmern,", "tokens": ["Der", "Lam\u00b7pen", ",", "die", "auf", "den", "Al\u00b7t\u00e4\u00b7ren", "schim\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da\u00df geisterhaft, wohin es zitternd bricht,", "tokens": ["Da\u00df", "geis\u00b7ter\u00b7haft", ",", "wo\u00b7hin", "es", "zit\u00b7ternd", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Kreuze und die Leichensteine flimmern.", "tokens": ["Die", "Kreu\u00b7ze", "und", "die", "Lei\u00b7chen\u00b7stei\u00b7ne", "flim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "In dichtern Tropfen aus den Pfeifen tr\u00e4uft's,", "tokens": ["In", "dich\u00b7tern", "Trop\u00b7fen", "aus", "den", "Pfei\u00b7fen", "tr\u00e4uft's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und durch die Hallen schweben dunkle Schatten,", "tokens": ["Und", "durch", "die", "Hal\u00b7len", "schwe\u00b7ben", "dunk\u00b7le", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zwischendrein vernimmt man das Geseufz", "tokens": ["Und", "zwi\u00b7schen\u00b7drein", "ver\u00b7nimmt", "man", "das", "Ge\u00b7seufz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Toten unter ihren Marmorplatten.", "tokens": ["Der", "To\u00b7ten", "un\u00b7ter", "ih\u00b7ren", "Mar\u00b7mor\u00b7plat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Bald wieder alles stille wie zuvor!", "tokens": ["Bald", "wie\u00b7der", "al\u00b7les", "stil\u00b7le", "wie", "zu\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rings Nacht und Schweigen in den \u00f6den Mauern;", "tokens": ["Rings", "Nacht", "und", "Schwei\u00b7gen", "in", "den", "\u00f6\u00b7den", "Mau\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Kreuze, eingeh\u00fcllt in schwarzen Flor,", "tokens": ["Nur", "Kreu\u00b7ze", ",", "ein\u00b7ge\u00b7h\u00fcllt", "in", "schwar\u00b7zen", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Heil'ge, die in ihren Nischen trauern.", "tokens": ["Und", "Heil'\u00b7ge", ",", "die", "in", "ih\u00b7ren", "Ni\u00b7schen", "trau\u00b7ern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie \u00f6de trauert diese heil'ge Welt", "tokens": ["Wie", "\u00f6\u00b7de", "trau\u00b7ert", "die\u00b7se", "heil'\u00b7ge", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im zweifelhaften Schein der Tageshelle,", "tokens": ["Im", "zwei\u00b7fel\u00b7haf\u00b7ten", "Schein", "der", "Ta\u00b7ges\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punc