{"dta.poem.6476": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ton wird weiter weg und kr\u00e4fftiger gesp\u00fcret,", "tokens": ["Der", "Ton", "wird", "wei\u00b7ter", "weg", "und", "kr\u00e4ff\u00b7ti\u00b7ger", "ge\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem die Handlung starck, aus welcher er entsteht,", "tokens": ["Nach\u00b7dem", "die", "Hand\u00b7lung", "starck", ",", "aus", "wel\u00b7cher", "er", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df der L\u00fcffte Feld, darinn er fortgef\u00fchret,", "tokens": ["Und", "da\u00df", "der", "L\u00fcff\u00b7te", "Feld", ",", "da\u00b7rinn", "er", "fort\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sehr weit ist ausgedehnt. Das Ertz, wenn es erh\u00f6ht", "tokens": ["Sehr", "weit", "ist", "aus\u00b7ge\u00b7dehnt", ".", "Das", "Ertz", ",", "wenn", "es", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$.", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dann geschlagen wird, erschallet starck und weit:", "tokens": ["Und", "dann", "ge\u00b7schla\u00b7gen", "wird", ",", "er\u00b7schal\u00b7let", "starck", "und", "weit", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der in die Lufft von Ertz erhabenen Gef\u00e4sse", "tokens": ["Der", "in", "die", "Lufft", "von", "Ertz", "er\u00b7ha\u00b7be\u00b7nen", "Ge\u00b7f\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Offt wiederholte Schl\u00e4g\u2019 und St\u00f6sse,", "tokens": ["Offt", "wie\u00b7der\u00b7hol\u00b7te", "Schl\u00e4g'", "und", "St\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erregen einen Schall mit heller Hefftigkeit,", "tokens": ["Er\u00b7re\u00b7gen", "ei\u00b7nen", "Schall", "mit", "hel\u00b7ler", "Heff\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von unsern Wart-und Kirchen-Spitzen,", "tokens": ["Von", "un\u00b7sern", "War\u00b7t\u00b7und", "Kir\u00b7chen\u00b7Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Da\u00df alles Volck im Feld und in der Stadt", "tokens": ["Da\u00df", "al\u00b7les", "Volck", "im", "Feld", "und", "in", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verstehet, da\u00df es n\u00f6thig hat", "tokens": ["Ver\u00b7ste\u00b7het", ",", "da\u00df", "es", "n\u00f6\u00b7thig", "hat"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durch Waffen und Gewehr, im Lermen sich zu sch\u00fctzen.", "tokens": ["Durch", "Waf\u00b7fen", "und", "Ge\u00b7wehr", ",", "im", "Ler\u00b7men", "sich", "zu", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auch giebt dies Schallen zu verstehn,", "tokens": ["Auch", "giebt", "dies", "Schal\u00b7len", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Es sey nun Zeit zum Gottes-Dienst zu gehn.", "tokens": ["Es", "sey", "nun", "Zeit", "zum", "Got\u00b7tes\u00b7Dienst", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}