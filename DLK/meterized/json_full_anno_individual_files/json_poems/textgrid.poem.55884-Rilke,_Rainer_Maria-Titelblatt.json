{"textgrid.poem.55884": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Titelblatt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Reichen und Gl\u00fccklichen haben gut schweigen,", "tokens": ["Die", "Rei\u00b7chen", "und", "Gl\u00fcck\u00b7li\u00b7chen", "ha\u00b7ben", "gut", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "niemand will wissen was sie sind.", "tokens": ["nie\u00b7mand", "will", "wis\u00b7sen", "was", "sie", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "PWS", "PPER", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aber die D\u00fcrftigen m\u00fcssen sich zeigen,", "tokens": ["A\u00b7ber", "die", "D\u00fcrf\u00b7ti\u00b7gen", "m\u00fcs\u00b7sen", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "m\u00fcssen sagen: ich bin blind", "tokens": ["m\u00fcs\u00b7sen", "sa\u00b7gen", ":", "ich", "bin", "blind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$.", "PPER", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "oder: ich bin im Begriff es zu werden", "tokens": ["o\u00b7der", ":", "ich", "bin", "im", "Be\u00b7griff", "es", "zu", "wer\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$.", "PPER", "VAFIN", "APPRART", "NN", "PPE