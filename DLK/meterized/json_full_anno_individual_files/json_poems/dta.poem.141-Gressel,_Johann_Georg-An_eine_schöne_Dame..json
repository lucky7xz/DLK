{"dta.poem.141": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine sch\u00f6ne  Dame.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Vollkommne Silvia! wo find' ich ihres gleichen?", "tokens": ["Voll\u00b7komm\u00b7ne", "Sil\u00b7via", "!", "wo", "find'", "ich", "ih\u00b7res", "glei\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jhr hoher Tugend-Ruhm erf\u00fcllt die gantze Welt/", "tokens": ["Ihr", "ho\u00b7her", "Tu\u00b7gen\u00b7dRuhm", "er\u00b7f\u00fcllt", "die", "gant\u00b7ze", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Geist/ der wohnet schon im hohen Stern-Gezelt/", "tokens": ["Ihr", "Geist", "/", "der", "woh\u00b7net", "schon", "im", "ho\u00b7hen", "Stern\u00b7Ge\u00b7zelt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Minerva mu\u00df davor die g\u00fcldnen Seegel streichen.", "tokens": ["Mi\u00b7ner\u00b7va", "mu\u00df", "da\u00b7vor", "die", "g\u00fcld\u00b7nen", "See\u00b7gel", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Die Sch\u00f6nheit hat den Schmuck ihr doppelt zugestelt/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "den", "Schmuck", "ihr", "dop\u00b7pelt", "zu\u00b7ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr sch\u00f6ner Geist belebt durch einen Blick die Leichen.", "tokens": ["Ihr", "sch\u00f6\u00b7ner", "Geist", "be\u00b7lebt", "durch", "ei\u00b7nen", "Blick", "die", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier f\u00e4llt der Laster-Schlu\u00df durch die Gerechtigkeit/", "tokens": ["Hier", "f\u00e4llt", "der", "Las\u00b7ter\u00b7Schlu\u00df", "durch", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Keuschheit ist ihr Schmuck/ und Mildigkeit ihr Kleid.", "tokens": ["Die", "Keuschheit", "ist", "ihr", "Schmuck", "/", "und", "Mil\u00b7dig\u00b7keit", "ihr", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Recht G\u00f6ttlich das Gesicht in Liljen-Wolcken spielet/", "tokens": ["Recht", "G\u00f6tt\u00b7lich", "das", "Ge\u00b7sicht", "in", "Lil\u00b7jen\u00b7Wol\u00b7cken", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jm Haaren hat das Gold ein Berg-Werck angelegt/", "tokens": ["Jm", "Haa\u00b7ren", "hat", "das", "Gold", "ein", "Ber\u00b7g\u00b7Werck", "an\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Diana keinen Glantz und keine Tugend hegt/", "tokens": ["Di\u00b7a\u00b7na", "kei\u00b7nen", "Glantz", "und", "kei\u00b7ne", "Tu\u00b7gend", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil sie gesehn/ da\u00df sie den besten Prei\u00df erziehlet.", "tokens": ["Weil", "sie", "ge\u00b7sehn", "/", "da\u00df", "sie", "den", "bes\u00b7ten", "Prei\u00df", "er\u00b7zieh\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}