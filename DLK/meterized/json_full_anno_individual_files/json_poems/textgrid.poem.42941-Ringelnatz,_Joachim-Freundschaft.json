{"textgrid.poem.42941": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es darf eine Freundschaft formell sein,", "tokens": ["Es", "darf", "ei\u00b7ne", "Freund\u00b7schaft", "for\u00b7mell", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00df aber genau sein.", "tokens": ["Mu\u00df", "a\u00b7ber", "ge\u00b7nau", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Eine Freundschaft kann rauh sein,", "tokens": ["Ei\u00b7ne", "Freund\u00b7schaft", "kann", "rauh", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber mu\u00df hell sein.", "tokens": ["A\u00b7ber", "mu\u00df", "hell", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn Allzuspr\u00f6des vers\u00e4umt oder verdirbt", "tokens": ["Denn", "All\u00b7zu\u00b7spr\u00f6\u00b7des", "ver\u00b7s\u00e4umt", "o\u00b7der", "ver\u00b7dirbt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVPP", "KON", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Viel. Weil manchmal der Partner ganz pl\u00f6tzlich stirbt.", "tokens": ["Viel", ".", "Weil", "manch\u00b7mal", "der", "Part\u00b7ner", "ganz", "pl\u00f6tz\u00b7lich", "stirbt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Mehr m\u00f6chte ich nicht dar\u00fcber sagen.", "tokens": ["Mehr", "m\u00f6ch\u00b7te", "ich", "nicht", "da\u00b7r\u00fc\u00b7ber", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn ich sitze im Speisewagen", "tokens": ["Denn", "ich", "sit\u00b7ze", "im", "Spei\u00b7se\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und f\u00fchle mich aus Freundschaft wohl", "tokens": ["Und", "f\u00fch\u00b7le", "mich", "aus", "Freund\u00b7schaft", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei \u00bbGed\u00e4mpfter Ochsenh\u00fcfte mit Wirsingkohl\u00ab.", "tokens": ["Bei", "\u00bb", "Ge\u00b7d\u00e4mpf\u00b7ter", "Och\u00b7sen\u00b7h\u00fcf\u00b7te", "mit", "Wir\u00b7sing\u00b7kohl", "\u00ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$(", "NN", "NN", "APPR", "NN", "$(", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Es darf eine Freundschaft formell sein,", "tokens": ["Es", "darf", "ei\u00b7ne", "Freund\u00b7schaft", "for\u00b7mell", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD"