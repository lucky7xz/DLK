{"textgrid.poem.26735": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Senke, strahlender Gott, die Fluren d\u00fcrsten", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Senke, strahlender Gott, die Fluren d\u00fcrsten", "tokens": ["Sen\u00b7ke", ",", "strah\u00b7len\u00b7der", "Gott", ",", "die", "Flu\u00b7ren", "d\u00fcrs\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "VMFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nach erquickendem Tau, der Mensch verschmachtet,", "tokens": ["Nach", "er\u00b7qui\u00b7cken\u00b7dem", "Tau", ",", "der", "Mensch", "ver\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Matter ziehen die Rosse,", "tokens": ["Mat\u00b7ter", "zie\u00b7hen", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Senke den Wagen hinab.", "tokens": ["Sen\u00b7ke", "den", "Wa\u00b7gen", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Siehe, wer aus des Meers kristallner Woge", "tokens": ["Sie\u00b7he", ",", "wer", "aus", "des", "Meers", "kris\u00b7tall\u00b7ner", "Wo\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lieblich l\u00e4chelnd dir winkt! Erkennt dein Herz sie?", "tokens": ["Lieb\u00b7lich", "l\u00e4\u00b7chelnd", "dir", "winkt", "!", "Er\u00b7kennt", "dein", "Herz", "sie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Rascher fliegen die Rosse,", "tokens": ["Ra\u00b7scher", "flie\u00b7gen", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tethys, die g\u00f6ttliche, winkt.", "tokens": ["Te\u00b7thys", ",", "die", "g\u00f6tt\u00b7li\u00b7che", ",", "winkt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Schnell vom Wagen herab in ihre Arme", "tokens": ["Schnell", "vom", "Wa\u00b7gen", "her\u00b7ab", "in", "ih\u00b7re", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Springt der F\u00fchrer, den Zaum ergreift Kupido,", "tokens": ["Springt", "der", "F\u00fch\u00b7rer", ",", "den", "Zaum", "er\u00b7greift", "Ku\u00b7pi\u00b7do", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stille halten die Rosse,", "tokens": ["Stil\u00b7le", "hal\u00b7ten", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Trinken die k\u00fchlende Flut.", "tokens": ["Trin\u00b7ken", "die", "k\u00fch\u00b7len\u00b7de", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "An dem Himmel herauf mit leisen Schritten", "tokens": ["An", "dem", "Him\u00b7mel", "her\u00b7auf", "mit", "lei\u00b7sen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kommt die duftende Nacht; ihr folgt die s\u00fc\u00dfe", "tokens": ["Kommt", "die", "duf\u00b7ten\u00b7de", "Nacht", ";", "ihr", "folgt", "die", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Liebe. Ruhet und liebet,", "tokens": ["Lie\u00b7be", ".", "Ru\u00b7het", "und", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ph\u00f6bus, der liebende, ruht.", "tokens": ["Ph\u00f6\u00b7bus", ",", "der", "lie\u00b7ben\u00b7de", ",", "ruht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Senke, strahlender Gott, die Fluren d\u00fcrsten", "tokens": ["Sen\u00b7ke", ",", "strah\u00b7len\u00b7der", "Gott", ",", "die", "Flu\u00b7ren", "d\u00fcrs\u00b7ten"], "toke