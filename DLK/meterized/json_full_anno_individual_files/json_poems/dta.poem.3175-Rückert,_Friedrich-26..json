{"dta.poem.3175": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie unertr\u00e4glich dir die leeren Tage waren,", "tokens": ["Wie", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "dir", "die", "lee\u00b7ren", "Ta\u00b7ge", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die vollen hast du nun zur Uebergn\u00fcg' erfahren.", "tokens": ["Die", "vol\u00b7len", "hast", "du", "nun", "zur", "Ue\u00b7ber\u00b7gn\u00fcg'", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O nie beklage mehr dich \u00fcber Tage leer,", "tokens": ["O", "nie", "be\u00b7kla\u00b7ge", "mehr", "dich", "\u00fc\u00b7ber", "Ta\u00b7ge", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei froh, wenn, wie von Lust, sie sind von Plage leer.", "tokens": ["Sei", "froh", ",", "wenn", ",", "wie", "von", "Lust", ",", "sie", "sind", "von", "Pla\u00b7ge", "leer", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "$,", "PWAV", "APPR", "NN", "$,", "PPER", "VAFIN", "APPR", "NN", "ADJ