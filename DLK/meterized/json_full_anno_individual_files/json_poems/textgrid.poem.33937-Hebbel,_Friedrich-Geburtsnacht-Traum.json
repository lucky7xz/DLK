 ["Ging", "in", "den", "blei\u00b7chen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4r's ein neues Leben, auf", "tokens": ["Als", "w\u00e4r's", "ein", "neu\u00b7es", "Le\u00b7ben", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das innigste Vergn\u00fcgen.", "tokens": ["Das", "in\u00b7nigs\u00b7te", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.26": {"line.1": {"text": "Nun trat ein ernster Mann herzu,", "tokens": ["Nun", "trat", "ein", "erns\u00b7ter", "Mann", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich nicht mehr erkannte,", "tokens": ["Den", "ich", "nicht", "mehr", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sah ich, da\u00df er freundlich sich", "tokens": ["Doch", "sah", "ich", ",", "da\u00df", "er", "freund\u00b7lich", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Vater wandte.", "tokens": ["Zu", "mei\u00b7nem", "Va\u00b7ter", "wand\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Und immer gr\u00f6\u00dfer ward die Schaar", "tokens": ["Und", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", "ward", "die", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von M\u00e4nnern, welche kamen,", "tokens": ["Von", "M\u00e4n\u00b7nern", ",", "wel\u00b7che", "ka\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und stets durchzuckte mir's die Brust:", "tokens": ["Und", "stets", "durch\u00b7zuck\u00b7te", "mir's", "die", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist von ihrem Samen!", "tokens": ["Du", "bist", "von", "ih\u00b7rem", "Sa\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Auch zarter Frauen nahten viel", "tokens": ["Auch", "zar\u00b7ter", "Frau\u00b7en", "nah\u00b7ten", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Trachten, fremd und eigen;", "tokens": ["In", "Trach\u00b7ten", ",", "fremd", "und", "ei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein schlummerndes Jahrhundert schien", "tokens": ["Ein", "schlum\u00b7mern\u00b7des", "Jahr\u00b7hun\u00b7dert", "schien"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jeder aufzusteigen.", "tokens": ["Mit", "je\u00b7der", "auf\u00b7zu\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Die sanften Augen waren all'", "tokens": ["Die", "sanf\u00b7ten", "Au\u00b7gen", "wa\u00b7ren", "all'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So s\u00fc\u00df auf mich geheftet,", "tokens": ["So", "s\u00fc\u00df", "auf", "mich", "ge\u00b7hef\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch war der l\u00e4chelnd holde Mund,", "tokens": ["Doch", "war", "der", "l\u00e4\u00b7chelnd", "hol\u00b7de", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Rede zu entkr\u00e4ftet.", "tokens": ["Zur", "Re\u00b7de", "zu", "ent\u00b7kr\u00e4f\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Vom Thurme schlug es, dumpf und bang,", "tokens": ["Vom", "Thur\u00b7me", "schlug", "es", ",", "dumpf", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schieden mit Get\u00fcmmel;", "tokens": ["Sie", "schie\u00b7den", "mit", "Ge\u00b7t\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00e4nner deuteten auf's Grab,", "tokens": ["Die", "M\u00e4n\u00b7ner", "deu\u00b7te\u00b7ten", "auf's", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frauen auf den Himmel.", "tokens": ["Die", "Frau\u00b7en", "auf", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Das war die Stund', die mich gebar;", "tokens": ["Das", "war", "die", "Stund'", ",", "die", "mich", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun frag' ich mich mit Beben:", "tokens": ["Nun", "frag'", "ich", "mich", "mit", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob sich das Leben und der Tod", "tokens": ["Ob", "sich", "das", "Le\u00b7ben", "und", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grabe noch verweben?", "tokens": ["Im", "Gra\u00b7be", "noch", "ver\u00b7we\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Ob, die sich regt in meiner Brust,", "tokens": ["Ob", ",", "die", "sich", "regt", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ungest\u00fcme Flamme,", "tokens": ["Die", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Todten noch im Schlummer st\u00f6rt,", "tokens": ["Die", "Tod\u00b7ten", "noch", "im", "Schlum\u00b7mer", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus deren Blut ich stamme?", "tokens": ["Aus", "de\u00b7ren", "Blut", "ich", "stam\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Ob sie mir bla\u00df zur Seite geh'n,", "tokens": ["Ob", "sie", "mir", "bla\u00df", "zur", "Sei\u00b7te", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unm\u00e4chtig, zu erscheinen,", "tokens": ["Un\u00b7m\u00e4ch\u00b7tig", ",", "zu", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4cheln, wenn ich gl\u00fccklich bin,", "tokens": ["Und", "l\u00e4\u00b7cheln", ",", "wenn", "ich", "gl\u00fcck\u00b7lich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn ich's nicht bin, weinen?", "tokens": ["Und", "wenn", "ich's", "nicht", "bin", ",", "wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PTKNEG", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "Und ob ich selbst dereinst mein Kind,", "tokens": ["Und", "ob", "ich", "selbst", "de\u00b7reinst", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt ruhig auszuschlafen,", "tokens": ["Statt", "ru\u00b7hig", "aus\u00b7zu\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Nacht und Sturm begleiten mu\u00df", "tokens": ["Durch", "Nacht", "und", "Sturm", "be\u00b7glei\u00b7ten", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an den letzten Hafen?", "tokens": ["Bis", "an", "den", "letz\u00b7ten", "Ha\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}