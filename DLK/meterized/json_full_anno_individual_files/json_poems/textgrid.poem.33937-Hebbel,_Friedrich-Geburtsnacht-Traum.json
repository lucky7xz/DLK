{"textgrid.poem.33937": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Geburtsnacht-Traum", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich durfte \u00fcber Nacht im Traum", "tokens": ["Ich", "durf\u00b7te", "\u00fc\u00b7ber", "Nacht", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein seltsam Fest begehen,", "tokens": ["Ein", "selt\u00b7sam", "Fest", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe meine V\u00e4ter all'", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7ne", "V\u00e4\u00b7ter", "all'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um mich vereint gesehen.", "tokens": ["Um", "mich", "ver\u00b7eint", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVPP", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Vater f\u00fchrte stumm den Zug,", "tokens": ["Mein", "Va\u00b7ter", "f\u00fchr\u00b7te", "stumm", "den", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er l\u00e4chelte hin\u00fcber,", "tokens": ["Er", "l\u00e4\u00b7chel\u00b7te", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann aber wandte er sich ab,", "tokens": ["Dann", "a\u00b7ber", "wand\u00b7te", "er", "sich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm ward das Auge tr\u00fcber.", "tokens": ["Ihm", "ward", "das", "Au\u00b7ge", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war der Letzte, welcher starb,", "tokens": ["Es", "war", "der", "Letz\u00b7te", ",", "wel\u00b7cher", "starb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch hatt' er all' die Milde;", "tokens": ["Noch", "hatt'", "er", "all'", "die", "Mil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel hatte Nichts versch\u00f6nt", "tokens": ["Der", "Him\u00b7mel", "hat\u00b7te", "Nichts", "ver\u00b7sch\u00f6nt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An seinem theuren Bilde.", "tokens": ["An", "sei\u00b7nem", "theu\u00b7ren", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gro\u00dfvater nahte nun heran,", "tokens": ["Gro\u00df\u00b7va\u00b7ter", "nah\u00b7te", "nun", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mich zu wiegen pflegte,", "tokens": ["Der", "mich", "zu", "wie\u00b7gen", "pfleg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Eh', wie er mich, ihn selbst der Tod", "tokens": ["Eh'", ",", "wie", "er", "mich", ",", "ihn", "selbst", "der", "Tod"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "PRF", "$,", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In's stille Bette legte.", "tokens": ["In's", "stil\u00b7le", "Bet\u00b7te", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich habe ihn sogleich erkannt,", "tokens": ["Ich", "ha\u00b7be", "ihn", "sog\u00b7leich", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als h\u00e4tte, wie die Nische", "tokens": ["Als", "h\u00e4t\u00b7te", ",", "wie", "die", "Ni\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Heiligen, mein Herz sein Bild", "tokens": ["Den", "Hei\u00b7li\u00b7gen", ",", "mein", "Herz", "sein", "Bild"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bewahrt in voller Frische.", "tokens": ["Be\u00b7wahrt", "in", "vol\u00b7ler", "Fri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sein Auge weilte, wie erstaunt,", "tokens": ["Sein", "Au\u00b7ge", "weil\u00b7te", ",", "wie", "er\u00b7staunt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf mir und schien zu fragen:", "tokens": ["Auf", "mir", "und", "schien", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bist du dasselbe kleine Kind,", "tokens": ["Bist", "du", "das\u00b7sel\u00b7be", "klei\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das einst mein Arm getragen?", "tokens": ["Das", "einst", "mein", "Arm", "ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gro\u00dfmutter auch, sie nahte sich,", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "auch", ",", "sie", "nah\u00b7te", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mildeste der Frauen;", "tokens": ["Die", "mil\u00b7des\u00b7te", "der", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf meinen Vater schien sie bald", "tokens": ["Auf", "mei\u00b7nen", "Va\u00b7ter", "schien", "sie", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bald auf mich zu schauen.", "tokens": ["Und", "bald", "auf", "mich", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und als sie fand, da\u00df ich ihm glich,", "tokens": ["Und", "als", "sie", "fand", ",", "da\u00df", "ich", "ihm", "glich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ging in den bleichen Z\u00fcgen,", "tokens": ["Ging", "in", "den", "blei\u00b7chen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4r's ein neues Leben, auf", "tokens": ["Als", "w\u00e4r's", "ein", "neu\u00b7es", "Le\u00b7ben", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das innigste Vergn\u00fcgen.", "tokens": ["Das", "in\u00b7nigs\u00b7te", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Nun trat ein ernster Mann herzu,", "tokens": ["Nun", "trat", "ein", "erns\u00b7ter", "Mann", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ich nicht mehr erkannte,", "tokens": ["Den", "ich", "nicht", "mehr", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sah ich, da\u00df er freundlich sich", "tokens": ["Doch", "sah", "ich", ",", "da\u00df", "er", "freund\u00b7lich", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Vater wandte.", "tokens": ["Zu", "mei\u00b7nem", "Va\u00b7ter", "wand\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und immer gr\u00f6\u00dfer ward die Schaar", "tokens": ["Und", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", "ward", "die", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von M\u00e4nnern, welche kamen,", "tokens": ["Von", "M\u00e4n\u00b7nern", ",", "wel\u00b7che", "ka\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und stets durchzuckte mir's die Brust:", "tokens": ["Und", "stets", "durch\u00b7zuck\u00b7te", "mir's", "die", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist von ihrem Samen!", "tokens": ["Du", "bist", "von", "ih\u00b7rem", "Sa\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auch zarter Frauen nahten viel", "tokens": ["Auch", "zar\u00b7ter", "Frau\u00b7en", "nah\u00b7ten", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Trachten, fremd und eigen;", "tokens": ["In", "Trach\u00b7ten", ",", "fremd", "und", "ei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein schlummerndes Jahrhundert schien", "tokens": ["Ein", "schlum\u00b7mern\u00b7des", "Jahr\u00b7hun\u00b7dert", "schien"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jeder aufzusteigen.", "tokens": ["Mit", "je\u00b7der", "auf\u00b7zu\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die sanften Augen waren all'", "tokens": ["Die", "sanf\u00b7ten", "Au\u00b7gen", "wa\u00b7ren", "all'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So s\u00fc\u00df auf mich geheftet,", "tokens": ["So", "s\u00fc\u00df", "auf", "mich", "ge\u00b7hef\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch war der l\u00e4chelnd holde Mund,", "tokens": ["Doch", "war", "der", "l\u00e4\u00b7chelnd", "hol\u00b7de", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Rede zu entkr\u00e4ftet.", "tokens": ["Zur", "Re\u00b7de", "zu", "ent\u00b7kr\u00e4f\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Vom Thurme schlug es, dumpf und bang,", "tokens": ["Vom", "Thur\u00b7me", "schlug", "es", ",", "dumpf", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schieden mit Get\u00fcmmel;", "tokens": ["Sie", "schie\u00b7den", "mit", "Ge\u00b7t\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00e4nner deuteten auf's Grab,", "tokens": ["Die", "M\u00e4n\u00b7ner", "deu\u00b7te\u00b7ten", "auf's", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frauen auf den Himmel.", "tokens": ["Die", "Frau\u00b7en", "auf", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Das war die Stund', die mich gebar;", "tokens": ["Das", "war", "die", "Stund'", ",", "die", "mich", "ge\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun frag' ich mich mit Beben:", "tokens": ["Nun", "frag'", "ich", "mich", "mit", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob sich das Leben und der Tod", "tokens": ["Ob", "sich", "das", "Le\u00b7ben", "und", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grabe noch verweben?", "tokens": ["Im", "Gra\u00b7be", "noch", "ver\u00b7we\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ob, die sich regt in meiner Brust,", "tokens": ["Ob", ",", "die", "sich", "regt", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ungest\u00fcme Flamme,", "tokens": ["Die", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Todten noch im Schlummer st\u00f6rt,", "tokens": ["Di