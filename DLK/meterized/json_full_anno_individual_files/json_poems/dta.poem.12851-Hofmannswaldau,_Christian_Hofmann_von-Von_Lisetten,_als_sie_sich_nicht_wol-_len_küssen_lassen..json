{"dta.poem.12851": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von Lisetten, als sie sich nicht wol-  \n len k\u00fcssen lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein! warum wehret sich Lisette,", "tokens": ["Mein", "!", "wa\u00b7rum", "weh\u00b7ret", "sich", "Li\u00b7set\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PWAV", "VVFIN", "PRF", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man ihr \u00f6ffentlich ein k\u00fc\u00dfgen geben will?", "tokens": ["Wenn", "man", "ihr", "\u00f6f\u00b7fent\u00b7lich", "ein", "k\u00fc\u00df\u00b7gen", "ge\u00b7ben", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die antwort ist nicht schwer: Weil sie das s\u00fc\u00dfe spiel", "tokens": ["Die", "ant\u00b7wort", "ist", "nicht", "schwer", ":", "Weil", "sie", "das", "s\u00fc\u00b7\u00dfe", "spiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$.", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Viel lieber im verborgnen h\u00e4tte.", "tokens": ["Viel", "lie\u00b7ber", "im", "ver\u00b7borg\u00b7nen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}