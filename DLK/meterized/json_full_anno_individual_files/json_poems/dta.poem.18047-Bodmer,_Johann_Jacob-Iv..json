{"dta.poem.18047": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Weiher war von Fr\u00f6schen voll.", "tokens": ["Ein", "Wei\u00b7her", "war", "von", "Fr\u00f6\u00b7schen", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denselben war nach ihren Arten wohl.", "tokens": ["Den\u00b7sel\u00b7ben", "war", "nach", "ih\u00b7ren", "Ar\u00b7ten", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie hatten Wasser Land und Feld,", "tokens": ["Sie", "hat\u00b7ten", "Was\u00b7ser", "Land", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dessen gnug ohn alles Geld.", "tokens": ["Und", "des\u00b7sen", "gnug", "ohn", "al\u00b7les", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie waren unbeherrschet gar,", "tokens": ["Sie", "wa\u00b7ren", "un\u00b7be\u00b7herr\u00b7schet", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nahmen keines Herren wahr.", "tokens": ["Und", "nah\u00b7men", "kei\u00b7nes", "Her\u00b7ren", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Freyheit stuhnd ihr aller Muth,", "tokens": ["In", "Frey\u00b7heit", "stuhnd", "ihr", "al\u00b7ler", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr Leib, ihr Leben, und ihr Gut.", "tokens": ["Ihr", "Leib", ",", "ihr", "Le\u00b7ben", ",", "und", "ihr", "Gut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Freyheit mogten sie nicht mehr vertragen.", "tokens": ["Die", "Frey\u00b7heit", "mog\u00b7ten", "sie", "nicht", "mehr", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sie fiengen ernstlich an zu klagen,", "tokens": ["Sie", "fi\u00b7en\u00b7gen", "ernst\u00b7lich", "an", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sie m\u00f6gten nicht ohn\u2019 einen K\u00f6nig leben.", "tokens": ["Sie", "m\u00f6g\u00b7ten", "nicht", "ohn'", "ei\u00b7nen", "K\u00f6\u00b7nig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gott Jupiter sollt ihnen einen geben,", "tokens": ["Gott", "Ju\u00b7pi\u00b7ter", "sollt", "ih\u00b7nen", "ei\u00b7nen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der ihr Regent und K\u00f6nig w\u00e4r.", "tokens": ["Der", "ihr", "Re\u00b7gent", "und", "K\u00f6\u00b7nig", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "De\u00df lachete Gott Jupiter,", "tokens": ["De\u00df", "la\u00b7che\u00b7te", "Gott", "Ju\u00b7pi\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Und schwieg. Sie hielten wiederum", "tokens": ["Und", "schwieg", ".", "Sie", "hiel\u00b7ten", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Um einen Herrscher an. Kein Frosch blieb stumm.", "tokens": ["Um", "ei\u00b7nen", "Herr\u00b7scher", "an", ".", "Kein", "Frosch", "blieb", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKVZ", "$.", "PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Da lag ein Tr\u00e4mel, plump und gro\u00df;", "tokens": ["Da", "lag", "ein", "Tr\u00e4\u00b7mel", ",", "plump", "und", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den Jupiter itzt in den Weiher scho\u00df.", "tokens": ["Den", "Ju\u00b7pi\u00b7ter", "itzt", "in", "den", "Wei\u00b7her", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Der sollt ihr K\u00f6nig seyn. Dazu w\u00e4r er gebohren.", "tokens": ["Der", "sollt", "ihr", "K\u00f6\u00b7nig", "seyn", ".", "Da\u00b7zu", "w\u00e4r", "er", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$.", "PAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Sie hielten sich zuerst vor gantz verlohren,", "tokens": ["Sie", "hiel\u00b7ten", "sich", "zu\u00b7erst", "vor", "gantz", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sie waren schnell die Flucht zu wehlen,", "tokens": ["Sie", "wa\u00b7ren", "schnell", "die", "Flucht", "zu", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Verstummet waren ihre Kehlen.", "tokens": ["Ver\u00b7stum\u00b7met", "wa\u00b7ren", "ih\u00b7re", "Keh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So schrecklich war der erste Schlag,", "tokens": ["So", "schreck\u00b7lich", "war", "der", "ers\u00b7te", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nach welchem bald der Tr\u00e4mel stille lag.", "tokens": ["Nach", "wel\u00b7chem", "bald", "der", "Tr\u00e4\u00b7mel", "stil\u00b7le", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Er r\u00fchrte sich nicht um ein Haar.", "tokens": ["Er", "r\u00fchr\u00b7te", "sich", "nicht", "um", "ein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.26": {"text": "Die Fr\u00f6sche wurden das gewahr.", "tokens": ["Die", "Fr\u00f6\u00b7sche", "wur\u00b7den", "das", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Sie konnten sich im Spott nicht fassen,", "tokens": ["Sie", "konn\u00b7ten", "sich", "im", "Spott", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df sie nicht auf den K\u00f6nig sassen.", "tokens": ["Da\u00df", "sie", "nicht", "auf", "den", "K\u00f6\u00b7nig", "sas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Sie fiengen wieder an zu schrey\u2019n,", "tokens": ["Sie", "fi\u00b7en\u00b7gen", "wie\u00b7der", "an", "zu", "schrey'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Sie k\u00f6nnten nicht in Ruhe seyn,", "tokens": ["Sie", "k\u00f6nn\u00b7ten", "nicht", "in", "Ru\u00b7he", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "W\u00fcrd ihnen nicht ein F\u00fcrst gegeben,", "tokens": ["W\u00fcrd", "ih\u00b7nen", "nicht", "ein", "F\u00fcrst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Ein K\u00f6nig, unter dem sie m\u00f6gten leben.", "tokens": ["Ein", "K\u00f6\u00b7nig", ",", "un\u00b7ter", "dem", "sie", "m\u00f6g\u00b7ten", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Jhr Ungest\u00fcm verdro\u00df den Gott ein wenig.", "tokens": ["Ihr", "Un\u00b7ge\u00b7st\u00fcm", "ver\u00b7dro\u00df", "den", "Gott", "ein", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Jm Zorn ernennet er den Storch zu ihrem K\u00f6nig.", "tokens": ["Jm", "Zorn", "er\u00b7nen\u00b7net", "er", "den", "Storch", "zu", "ih\u00b7rem", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der kam mit grossem Staat, mit hohem Muth u. Wesen", "tokens": ["Der", "kam", "mit", "gros\u00b7sem", "Staat", ",", "mit", "ho\u00b7hem", "Muth", "u.", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Vor seinem Ernste mocht kein Frosch genesen.", "tokens": ["Vor", "sei\u00b7nem", "Erns\u00b7te", "mocht", "kein", "Frosch", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Sein Magen war sehr hei\u00df, weit waren Kropf u. Schlund,", "tokens": ["Sein", "Ma\u00b7gen", "war", "sehr", "hei\u00df", ",", "weit", "wa\u00b7ren", "Kropf", "u.", "Schlund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "Und immer offen stuhnd der Mund.", "tokens": ["Und", "im\u00b7mer", "of\u00b7fen", "stuhnd", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Was ihm bekam, verschlang er lebendig.", "tokens": ["Was", "ihm", "be\u00b7kam", ",", "ver\u00b7schlang", "er", "le\u00b7ben\u00b7dig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Das Fr\u00f6schen-Volck sah sich in letzter Noth,", "tokens": ["Das", "Fr\u00f6s\u00b7chen\u00b7Volck", "sah", "sich", "in", "letz\u00b7ter", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Sie schrien: hilf Jupiter, sonst sind wir alle todt.", "tokens": ["Sie", "schri\u00b7en", ":", "hilf", "Ju\u00b7pi\u00b7ter", ",", "sonst", "sind", "wir", "al\u00b7le", "todt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "NN", "$,", "ADV", "VAFIN", "PPER", "PIAT", "ADJD", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.42": {"text": "Er schonet weder gro\u00df noch klein.", "tokens": ["Er", "scho\u00b7net", "we\u00b7der", "gro\u00df", "noch", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Wir wollen gern ohn einen K\u00f6nig seyn.", "tokens": ["Wir", "wol\u00b7len", "gern", "ohn", "ei\u00b7nen", "K\u00f6\u00b7nig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Es mag nicht seyn, sprach Jupiter,", "tokens": ["Es", "mag", "nicht", "seyn", ",", "sprach", "Ju\u00b7pi\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VAINF", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Er ist und bleibet euer Herr;", "tokens": ["Er", "ist", "und", "blei\u00b7bet", "eu\u00b7er", "Herr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Ein Richter \u00fcber euer Leben.", "tokens": ["Ein", "Rich\u00b7ter", "\u00fc\u00b7ber", "eu\u00b7er", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Ich hab ihn euch auf euer Flehn gegeben,", "tokens": ["Ich", "hab", "ihn", "euch", "auf", "eu\u00b7er", "Flehn", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Dem m\u00fcsset ihr ietzt unterth\u00e4nig seyn.", "tokens": ["Dem", "m\u00fcs\u00b7set", "ihr", "ietzt", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Will er, so t\u00f6dtet er so gro\u00df, als klein.", "tokens": ["Will", "er", ",", "so", "t\u00f6d\u00b7tet", "er", "so", "gro\u00df", ",", "als", "klein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "Dadurch geschieht euch lauter Recht;", "tokens": ["Da\u00b7durch", "ge\u00b7schieht", "euch", "lau\u00b7ter", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Wer Herr mag seyn, der sey nicht Knecht.", "tokens": ["Wer", "Herr", "mag", "seyn", ",", "der", "sey", "nicht", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VAINF", "$,", "PRELS", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}