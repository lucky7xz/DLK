{"dta.poem.19955": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Ade .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was macht dir, Herzliebster!", "tokens": ["Was", "macht", "dir", ",", "Herz\u00b7liebs\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Wange so bla\u00df?", "tokens": ["Die", "Wan\u00b7ge", "so", "bla\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was macht dir das Auge", "tokens": ["Was", "macht", "dir", "das", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von Thr\u00e4nen so na\u00df?", "tokens": ["Von", "Thr\u00e4\u00b7nen", "so", "na\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "O Liebchen! Herzliebchen!", "tokens": ["O", "Lieb\u00b7chen", "!", "Herz\u00b7lieb\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wohl ist es mir weh;", "tokens": ["Wohl", "ist", "es", "mir", "weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Weit mu\u00df ich von hinnen,", "tokens": ["Weit", "mu\u00df", "ich", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Weit \u00fcber die See!", "tokens": ["Weit", "\u00fc\u00b7ber", "die", "See", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und mu\u00dft du von hinnen \u2014", "tokens": ["Und", "mu\u00dft", "du", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dort \u00fcber der See", "tokens": ["Dort", "\u00fc\u00b7ber", "der", "See"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Giebt's wohl noch ein Liebchen,", "tokens": ["Giebt's", "wohl", "noch", "ein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Herzliebster! ade!", "tokens": ["Herz\u00b7liebs\u00b7ter", "!", "a\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Es scheinen viel Sterne", "tokens": ["Es", "schei\u00b7nen", "viel", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am Himmelsgezelt;", "tokens": ["Am", "Him\u00b7mels\u00b7ge\u00b7zelt", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch keiner von allen,", "tokens": ["Doch", "kei\u00b7ner", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PIAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie Luna gef\u00e4llt.", "tokens": ["Wie", "Lu\u00b7na", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "So nimm nur die\u00df Ringlein", "tokens": ["So", "nimm", "nur", "die\u00df", "Rin\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "PDS", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von Golde so schwer,", "tokens": ["Von", "Gol\u00b7de", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und wird es zu eng dir,", "tokens": ["Und", "wird", "es", "zu", "eng", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKA", "ADJD", "PPER", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "So wirf's in das Meer!", "tokens": ["So", "wir\u00b7f's", "in", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "So steck' nur die\u00df Bl\u00fcmlein", "tokens": ["So", "steck'", "nur", "die\u00df", "Bl\u00fcm\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "NN"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "An's klopfende Herz!", "tokens": ["An's", "klop\u00b7fen\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und duftet's dir nimmer,", "tokens": ["Und", "duf\u00b7tet's", "dir", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Vergieng auch dein Schmerz.", "tokens": ["Ver\u00b7gieng", "auch", "dein", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}