{"dta.poem.15946": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "151.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr m\u00f6gt mich umganglos und ungesellig schelten!", "tokens": ["Ihr", "m\u00f6gt", "mich", "um\u00b7gang\u00b7los", "und", "un\u00b7ge\u00b7sel\u00b7lig", "schel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wen aber hab' ich denn, der mich als mich l\u00e4\u00dft gelten?", "tokens": ["Wen", "a\u00b7ber", "hab'", "ich", "denn", ",", "der", "mich", "als", "mich", "l\u00e4\u00dft", "gel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "KOUS", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+++-", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Wo ich mich selber mu\u00df verleugnen immerhin,", "tokens": ["Wo", "ich", "mich", "sel\u00b7ber", "mu\u00df", "ver\u00b7leug\u00b7nen", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da bin ich einsam, wo ich in Gesellschaft bin.", "tokens": ["Da", "bin", "ich", "ein\u00b7sam", ",", "wo", "ich", "in", "Ge\u00b7sell\u00b7schaft", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "APPR