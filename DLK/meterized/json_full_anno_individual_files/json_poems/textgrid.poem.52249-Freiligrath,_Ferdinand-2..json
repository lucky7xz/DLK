{"textgrid.poem.52249": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, dr\u00fcckt' auch uns nur landlos ein Johann!", "tokens": ["O", ",", "dr\u00fcckt'", "auch", "uns", "nur", "land\u00b7los", "ein", "Jo\u00b7hann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein gr\u00f6\u00dfer Heil, bei Gott, als solche Johne!", "tokens": ["Kein", "gr\u00f6\u00b7\u00dfer", "Heil", ",", "bei", "Gott", ",", "als", "sol\u00b7che", "Joh\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "APPR", "NN", "$,", "KOUS", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr wi\u00dft, wie K\u00fchnheit zorniger Barone", "tokens": ["Ihr", "wi\u00dft", ",", "wie", "K\u00fchn\u00b7heit", "zor\u00b7ni\u00b7ger", "Ba\u00b7ro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Freiheit Englands jenem abgewann!", "tokens": ["Die", "Frei\u00b7heit", "En\u00b7glands", "je\u00b7nem", "ab\u00b7ge\u00b7wann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein schlaffer K\u00f6nig und ein feiger Mann,", "tokens": ["Ein", "schlaf\u00b7fer", "K\u00f6\u00b7nig", "und", "ein", "fei\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schachvoll vom Papste hielt er Land und Krone;", "tokens": ["Schach\u00b7voll", "vom", "Paps\u00b7te", "hielt", "er", "Land", "und", "Kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trieb sich umher auf blut'gem Wanderthrone,", "tokens": ["Trieb", "sich", "um\u00b7her", "auf", "blut'\u00b7gem", "Wan\u00b7der\u00b7thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zu gleicher Zeit ein Schw\u00e4chling und Tyrann!", "tokens": ["Zu", "glei\u00b7cher", "Zeit", "ein", "Schw\u00e4ch\u00b7ling", "und", "Ty\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So schafft' er sich und seinem Volke Not,", "tokens": ["So", "schafft'", "er", "sich", "und", "sei\u00b7nem", "Vol\u00b7ke", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis jach ein Heer von seinem Zelte scharrte,", "tokens": ["Bis", "jach", "ein", "Heer", "von", "sei\u00b7nem", "Zel\u00b7te", "scharr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "AD