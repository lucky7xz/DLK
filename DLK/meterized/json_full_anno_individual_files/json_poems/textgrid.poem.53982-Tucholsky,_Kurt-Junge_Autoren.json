{"textgrid.poem.53982": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Junge Autoren", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sie nur wollen \u2013!", "tokens": ["Was", "sie", "nur", "wol\u00b7len", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "$(", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da schimpfen sie auf die Ollen,", "tokens": ["Da", "schimp\u00b7fen", "sie", "auf", "die", "Ol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und die sind stieke", "tokens": ["und", "die", "sind", "stie\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "und \u00fcberlassen die ganze Musike", "tokens": ["und", "\u00fc\u00b7ber\u00b7las\u00b7sen", "die", "gan\u00b7ze", "Mu\u00b7si\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "den Jungen.", "tokens": ["den", "Jun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und die machen ein Geschrei!", "tokens": ["Und", "die", "ma\u00b7chen", "ein", "Ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es sind alle dabei:", "tokens": ["Und", "es", "sind", "al\u00b7le", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da sieht man \u00e4ltere Knaben,", "tokens": ["Da", "sieht", "man", "\u00e4l\u00b7te\u00b7re", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die schon ihre f\u00fcnfzig auf dem Buckel haben,", "tokens": ["die", "schon", "ih\u00b7re", "f\u00fcnf\u00b7zig", "auf", "dem", "Bu\u00b7ckel", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "CARD", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "in kurzen Hosen umeinanderlaufen;", "tokens": ["in", "kur\u00b7zen", "Ho\u00b7sen", "um\u00b7ein\u00b7an\u00b7der\u00b7lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wenn sie schnell gehen, kriegen sie das Schnaufen \u2013", "tokens": ["wenn", "sie", "schnell", "ge\u00b7hen", ",", "krie\u00b7gen", "sie", "das", "Schnau\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "aber blo\u00df nicht hinten bleiben!", "tokens": ["a\u00b7ber", "blo\u00df", "nicht", "hin\u00b7ten", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Modern! modern m\u00fcssen Sie schreiben!", "tokens": ["Mo\u00b7dern", "!", "mo\u00b7dern", "m\u00fcs\u00b7sen", "Sie", "schrei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVINF", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Nur nicht sein Leben zu Ende leben \u2013", "tokens": ["Nur", "nicht", "sein", "Le\u00b7ben", "zu", "En\u00b7de", "le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "jung! jung mu\u00dft du dich geben!", "tokens": ["jung", "!", "jung", "mu\u00dft", "du", "dich", "ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Bei uns haben sie sonen Bart, der von alleine steht \u2013", "tokens": ["Bei", "uns", "ha\u00b7ben", "sie", "so\u00b7nen", "Bart", ",", "der", "von", "al\u00b7lei\u00b7ne", "steht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$("], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "oder sie kommen gar nicht raus aus der Pubert\u00e4t.", "tokens": ["o\u00b7der", "sie", "kom\u00b7men", "gar", "nicht", "raus", "aus", "der", "Pu\u00b7ber\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Was sie nur haben \u2013!", "tokens": ["Was", "sie", "nur", "ha\u00b7ben", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$(", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Hindert denn einer die jungen Knaben?", "tokens": ["Hin\u00b7dert", "denn", "ei\u00b7ner", "die", "jun\u00b7gen", "Kna\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "H\u00f6rt doch blo\u00df mal: Die junge Generation!", "tokens": ["H\u00f6rt", "doch", "blo\u00df", "mal", ":", "Die", "jun\u00b7ge", "Ge\u00b7ne\u00b7ra\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADV", "$.", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Na, da macht doch schon!", "tokens": ["Na", ",", "da", "macht", "doch", "schon", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Es h\u00e4lt euch ja keiner. Als ob uns das nicht frommt,", "tokens": ["Es", "h\u00e4lt", "euch", "ja", "kei\u00b7ner", ".", "Als", "ob", "uns", "das", "nicht", "frommt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "$.", "KOUS", "KOUS", "PPER", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "wenn ein neues Talent geloffen kommt.", "tokens": ["wenn", "ein", "neu\u00b7es", "Ta\u00b7lent", "ge\u00b7lof\u00b7fen", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Neunzehn Jahre! Was ist denn das schon?", "tokens": ["Neun\u00b7zehn", "Jah\u00b7re", "!", "Was", "ist", "denn", "das", "schon", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "PWS", "VAFIN", "ADV", "ART", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Das ist keine Qualifikation.", "tokens": ["Das", "ist", "kei\u00b7ne", "Qua\u00b7li\u00b7fi\u00b7ka\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Ludendorff war auch mal neunzehn Jahr.", "tokens": ["Lu\u00b7den\u00b7dorff", "war", "auch", "mal", "neun\u00b7zehn", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Jung sein ist gar nichts. Es fragt sich, wers war.", "tokens": ["Jung", "sein", "ist", "gar", "nichts", ".", "Es", "fragt", "sich", ",", "wers", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAINF", "VAFIN", "ADV", "PIS", "$.", "PPER", "VVFIN", "PRF", "$,", "PWS", "VAFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Es gibt alte Esel und junge Talente \u2013", "tokens": ["Es", "gibt", "al\u00b7te", "E\u00b7sel", "und", "jun\u00b7ge", "Ta\u00b7len\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+---+-+----", "measure": "dactylic.init"}, "line.12": {"text": "Geburtsscheine sind keine Argumente.", "tokens": ["Ge\u00b7burts\u00b7schei\u00b7ne", "sind", "kei\u00b7ne", "Ar\u00b7gu\u00b7men\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-++--+--+--", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und wenns nicht klappt: es liegt nicht am Pa\u00df.", "tokens": ["Und", "wenns", "nicht", "klappt", ":", "es", "liegt", "nicht", "am", "Pa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Dann liegts an euch. K\u00f6nnt ihr was \u2013?", "tokens": ["Dann", "liegts", "an", "euch", ".", "K\u00f6nnt", "ihr", "was", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$.", "VMFIN", "PPER", "PIS", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Noch nie hat man sich so um Jugend gerissen.", "tokens": ["Noch", "nie", "hat", "man", "sich", "so", "um", "Ju\u00b7gend", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "PRF", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Direktoren, Verleger warten servil . . .", "tokens": ["Di\u00b7rek\u00b7to\u00b7ren", ",", "Ver\u00b7le\u00b7ger", "war\u00b7ten", "ser\u00b7vil", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "VMFIN", "$.", "$.", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "jeder lauert auf einen fetten Bissen \u2013", "tokens": ["je\u00b7der", "lau\u00b7ert", "auf", "ei\u00b7nen", "fet\u00b7ten", "Bis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Speelt man god. Und schreit nicht so viel.", "tokens": ["Speelt", "man", "god", ".", "Und", "schreit", "nicht", "so", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "$.", "KON", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wer was kann, der sei willkommen.", "tokens": ["Wer", "was", "kann", ",", "der", "sei", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PWS", "VMFIN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Rest hat die Jugend zum Vorwand genommen;", "tokens": ["Der", "Rest", "hat", "die", "Ju\u00b7gend", "zum", "Vor\u00b7wand", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "das sind \u2013 wir wollen uns da nicht streiten \u2013", "tokens": ["das", "sind", "\u2013", "wir", "wol\u00b7len", "uns", "da", "nicht", "strei\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "verhinderte Talentlosigkeiten.", "tokens": ["ver\u00b7hin\u00b7der\u00b7te", "Ta\u00b7lent\u00b7lo\u00b7sig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Was sie nur wollen \u2013!", "tokens": ["Was", "sie", "nur", "wol\u00b7len", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "$(", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da schimpfen sie auf die Ollen,", "tokens": ["Da", "schimp\u00b7fen", "sie", "auf", "die", "Ol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und die sind stieke", "tokens": ["und", "die", "sind", "stie\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "und \u00fcberlassen die ganze Musike", "tokens": ["und", "\u00fc\u00b7ber\u00b7las\u00b7sen", "die", "gan\u00b7ze", "Mu\u00b7si\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "den Jungen.", "tokens": ["den", "Jun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Und die machen ein Geschrei!", "tokens": ["Und", "die", "ma\u00b7chen", "ein", "Ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es sind alle dabei:", "tokens": ["Und", "es", "sind", "al\u00b7le", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"],