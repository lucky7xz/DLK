{"dta.poem.12293": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der vortreffliche Stallbruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Sch\u00e4fer scheeren will,               ", "tokens": ["Wenn", "der", "Sch\u00e4\u00b7fer", "schee\u00b7ren", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellt er sich hinter die Hecke,", "tokens": ["Stellt", "er", "sich", "hin\u00b7ter", "die", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Scheert dem Schaaf die Wolle ab,", "tokens": ["Scheert", "dem", "Schaaf", "die", "Wol\u00b7le", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steckt sie in die S\u00e4cke.", "tokens": ["Steckt", "sie", "in", "die", "S\u00e4\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00e4ngt zu tanzen an, zu singen,", "tokens": ["F\u00e4ngt", "zu", "tan\u00b7zen", "an", ",", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00e4st auf seinem Dudeldu:", "tokens": ["Bl\u00e4st", "auf", "sei\u00b7nem", "Du\u00b7del\u00b7du", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201elieber Bruder dir ichs bringe,", "tokens": ["\u201e", "lie\u00b7ber", "Bru\u00b7der", "dir", "ichs", "brin\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201elieber Bruder trink dir's zu.", "tokens": ["\u201e", "lie\u00b7ber", "Bru\u00b7der", "trink", "dir's", "zu", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ewas ich trag auf H\u00e4nden mein,", "tokens": ["\u201e", "was", "ich", "trag", "auf", "H\u00e4n\u00b7den", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VVFIN", "APPR", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201eist ein Gl\u00e4\u00dflein k\u00fchlen Wein;", "tokens": ["\u201e", "ist", "ein", "Gl\u00e4\u00df\u00b7lein", "k\u00fch\u00b7len", "Wein", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201efl\u00f6g doch ein V\u00f6glein \u00fcbern Rhein,", "tokens": ["\u201e", "fl\u00f6g", "doch", "ein", "V\u00f6\u00b7glein", "\u00fc\u00b7bern", "Rhein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ebr\u00e4cht's dem lieben Stallbruder mein.", "tokens": ["\u201e", "br\u00e4cht's", "dem", "lie\u00b7ben", "Stall\u00b7bru\u00b7der", "mein", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "\u201estallbruder mein, du bist wohl werth,", "tokens": ["\u201e", "stall\u00b7bru\u00b7der", "mein", ",", "du", "bist", "wohl", "werth", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eda\u00df man dich auf'm Altar verehrt,", "tokens": ["\u201e", "da\u00df", "man", "dich", "auf'm", "Al\u00b7tar", "ver\u00b7ehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ehast ein Paar W\u00e4ngelein", "tokens": ["\u201e", "hast", "ein", "Paar", "W\u00e4n\u00b7ge\u00b7lein"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "ART", "NN", "NN"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "\u201ewie ein Rubin,", "tokens": ["\u201e", "wie", "ein", "Ru\u00b7bin", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "\u201eaugen wie Schwarzenstein,", "tokens": ["\u201e", "au\u00b7gen", "wie", "Schwar\u00b7zen\u00b7stein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KOKOM", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u201ez\u00e4hne wie Elfenbein,", "tokens": ["\u201e", "z\u00e4h\u00b7ne", "wie", "El\u00b7fen\u00b7bein", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u201ebist gar ein kluger Kerl,", "tokens": ["\u201e", "bist", "gar", "ein", "klu\u00b7ger", "Kerl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewie ich es bin.\u201c", "tokens": ["\u201e", "wie", "ich", "es", "bin", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PPER", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}