{"textgrid.poem.57882": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Hochschulenbau am Misburger Damm", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hochschulenbau am Misburger Damm", "tokens": ["Der", "Hoch\u00b7schu\u00b7len\u00b7bau", "am", "Mis\u00b7bur\u00b7ger", "Damm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Macht keinem Menschen Freude,", "tokens": ["Macht", "kei\u00b7nem", "Men\u00b7schen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn er ist alles andere eher", "tokens": ["Denn", "er", "ist", "al\u00b7les", "an\u00b7de\u00b7re", "e\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIS", "PIS", "ADV"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als eine Augenweide.", "tokens": ["Als", "ei\u00b7ne", "Au\u00b7gen\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Zimmererstreik ist neulich nun", "tokens": ["Ein", "Zim\u00b7me\u00b7rer\u00b7streik", "ist", "neu\u00b7lich", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An dem Neubau ausgebrochen,", "tokens": ["An", "dem", "Neu\u00b7bau", "aus\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Ursache sei eine Lohndifferenz,", "tokens": ["Die", "Ur\u00b7sa\u00b7che", "sei", "ei\u00b7ne", "Lohn\u00b7dif\u00b7fe\u00b7renz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So wird allgemein gesprochen.", "tokens": ["So", "wird", "all\u00b7ge\u00b7mein", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch das ist falsch, die Ursache sind", "tokens": ["Doch", "das", "ist", "falsch", ",", "die", "Ur\u00b7sa\u00b7che", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ideale Differenzen,", "tokens": ["I\u00b7dea\u00b7le", "Dif\u00b7fe\u00b7ren\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nur diese bewogen die Zimmerleut',", "tokens": ["Nur", "die\u00b7se", "be\u00b7wo\u00b7gen", "die", "Zim\u00b7mer\u00b7leut'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seit einer Woche zu schw\u00e4nzen.", "tokens": ["Seit", "ei\u00b7ner", "Wo\u00b7che", "zu", "schw\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die ostentative Geschmacklosigkeit", "