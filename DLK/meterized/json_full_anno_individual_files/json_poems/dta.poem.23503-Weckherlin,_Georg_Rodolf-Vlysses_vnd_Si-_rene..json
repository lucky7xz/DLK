{"dta.poem.23503": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Nymf/ wan man durch wollust", "tokens": ["Sch\u00f6\u00b7ne", "Nymf", "/", "wan", "man", "durch", "wol\u00b7lust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "PWAV", "PIS", "APPR", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein gro\u00dfen namen kont erlangen/", "tokens": ["Ein", "gro\u00b7\u00dfen", "na\u00b7men", "kont", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wolt ich deinen mund vnd brust", "tokens": ["So", "wolt", "ich", "dei\u00b7nen", "mund", "vnd", "brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "(so sch\u00f6n) gern k\u00fcssen vnd vmb fangen:", "tokens": ["(", "so", "sch\u00f6n", ")", "gern", "k\u00fcs\u00b7sen", "vnd", "vmb", "fan\u00b7gen", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$(", "ADV", "VVINF", "KON", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aber die m\u00fch vnd nicht die ruh", "tokens": ["A\u00b7ber", "die", "m\u00fch", "vnd", "nicht", "die", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "KON", "PTKNEG", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Kan recht eines mans leben zieren;", "tokens": ["Kan", "recht", "ei\u00b7nes", "mans", "le\u00b7ben", "zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "PIS", "VVINF", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Keinem wehrten man steht es zu", "tokens": ["Kei\u00b7nem", "wehr\u00b7ten", "man", "steht", "es", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "VVFIN", "PPER", "PTKZU"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Die zeit \u00fcppiglich zuverlieren.", "tokens": ["Die", "zeit", "\u00fcp\u00b7pig\u00b7lich", "zu\u00b7ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}