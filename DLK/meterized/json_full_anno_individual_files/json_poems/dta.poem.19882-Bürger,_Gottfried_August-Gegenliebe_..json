{"dta.poem.19882": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Gegenliebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcst' ich, w\u00fcst' ich, da\u00df du mich               ", "tokens": ["W\u00fcst'", "ich", ",", "w\u00fcst'", "ich", ",", "da\u00df", "du", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb und wehrt ein bischen hieltest,", "tokens": ["Lieb", "und", "wehrt", "ein", "bi\u00b7schen", "hiel\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von dem, was ich f\u00fcr dich,", "tokens": ["Und", "von", "dem", ",", "was", "ich", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "$,", "PWS", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein Hunderttheilchen f\u00fchltest;", "tokens": ["Nur", "ein", "Hun\u00b7dert\u00b7theil\u00b7chen", "f\u00fchl\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df dein Danken meinem Grus", "tokens": ["Da\u00df", "dein", "Dan\u00b7ken", "mei\u00b7nem", "Grus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halbes Wegs entgegen k\u00e4me,", "tokens": ["Hal\u00b7bes", "Wegs", "ent\u00b7ge\u00b7gen", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Mund den Wechselkus", "tokens": ["Und", "dein", "Mund", "den", "Wech\u00b7sel\u00b7kus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gerne g\u00e4b\u2019 und wiedern\u00e4me:", "tokens": ["Ger\u00b7ne", "g\u00e4b'", "und", "wie\u00b7der\u00b7n\u00e4\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann, o Himmel, ausser sich,", "tokens": ["Dann", ",", "o", "Him\u00b7mel", ",", "aus\u00b7ser", "sich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "APPR", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrde ganz mein Herz zerlodern!", "tokens": ["W\u00fcr\u00b7de", "ganz", "mein", "Herz", "zer\u00b7lo\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leib und Leben k\u00f6nt\u2019 ich dich", "tokens": ["Leib", "und", "Le\u00b7ben", "k\u00f6nt'", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht vergebens lassen fodern! \u2014", "tokens": ["Nicht", "ver\u00b7ge\u00b7bens", "las\u00b7sen", "fo\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gegengunst erh\u00f6het Gunst,", "tokens": ["Ge\u00b7gen\u00b7gunst", "er\u00b7h\u00f6\u00b7het", "Gunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe n\u00e4hret Gegenliebe,", "tokens": ["Lie\u00b7be", "n\u00e4h\u00b7ret", "Ge\u00b7gen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und entflamt zu Feuersbrunst,", "tokens": ["Und", "ent\u00b7flamt", "zu", "Feu\u00b7ers\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ein Aschenf\u00fcnkchen bliebe.", "tokens": ["Was", "ein", "A\u00b7schen\u00b7f\u00fcnk\u00b7chen", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}