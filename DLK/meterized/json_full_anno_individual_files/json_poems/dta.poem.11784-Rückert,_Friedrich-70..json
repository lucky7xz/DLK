{"dta.poem.11784": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Weisheitdurstenden hat nie so recht von Grund", "tokens": ["Dem", "Weis\u00b7heit\u00b7durs\u00b7ten\u00b7den", "hat", "nie", "so", "recht", "von", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Durst gestillt ein Buch, wie eines Lehrers Mund.", "tokens": ["Den", "Durst", "ge\u00b7stillt", "ein", "Buch", ",", "wie", "ei\u00b7nes", "Leh\u00b7rers", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "$,", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Lebendig ist der Trieb nur des gesprochnen Wortes,", "tokens": ["Le\u00b7ben\u00b7dig", "ist", "der", "Trieb", "nur", "des", "ge\u00b7sproch\u00b7nen", "Wor\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und das beschriebne Blatt vom Baum ist ein verdorrtes.", "tokens": ["Und", "das", "be\u00b7schrieb\u00b7ne", "Blatt", "vom", "Baum", "ist", "ein", "ver\u00b7dorr\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Selbst jenes Wort, das Erd' erschuf und Himmel dort,", "tokens": ["Selbst", "je\u00b7nes", "Wort", ",", "das", "Erd'", "er\u00b7schuf", "und", "Him\u00b7mel", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "ART", "NN", "VVFIN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War ein gesprochenes, nicht ein geschriebnes Wort.", "tokens": ["War", "ein", "ge\u00b7spro\u00b7che\u00b7nes", ",", "nicht", "ein", "ge\u00b7schrieb\u00b7nes", "Wort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und dem gesprochnen Wort verblieb der Lehrberuf,", "tokens": ["Und", "dem", "ge\u00b7sproch\u00b7nen", "Wort", "ver\u00b7blieb", "der", "Lehr\u00b7be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu schaffen immerfort, wie es zuerst erschuf.", "tokens": ["Zu", "schaf\u00b7fen", "im\u00b7mer\u00b7fort", ",", "wie", "es", "zu\u00b7erst", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und selber Gottes Schrift in Schrift und in Natur,", "tokens": ["Und", "sel\u00b7ber", "Got\u00b7tes", "Schrift", "in", "Schrift", "und", "in", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird immer neu belebt durch Schriftauslegung nur.", "tokens": ["Wird", "im\u00b7mer", "neu", "be\u00b7lebt", "durch", "Schrift\u00b7aus\u00b7le\u00b7gung", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Geschriebnes Wort, dem Buch vertraut, ist halb verlassen", "tokens": ["Ge\u00b7schrieb\u00b7nes", "Wort", ",", "dem", "Buch", "ver\u00b7traut", ",", "ist", "halb", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADJD", "$,", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Geist, und halb nur kann der Menschengeist es fassen.", "tokens": ["Vom", "Geist", ",", "und", "halb", "nur", "kann", "der", "Men\u00b7schen\u00b7geist", "es", "fas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "ADJD", "ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es geht von Hand zu Hand, es kommt von Land zu Land,", "tokens": ["Es", "geht", "von", "Hand", "zu", "Hand", ",", "es", "kommt", "von", "Land", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und findet, wie sichs trifft, Verstand und Misverstand.", "tokens": ["Und", "fin\u00b7det", ",", "wie", "sichs", "trifft", ",", "Ver\u00b7stand", "und", "Mis\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Gesprochnes gehet durch erw\u00e4hlter H\u00f6rer Runde,", "tokens": ["Ge\u00b7sproch\u00b7nes", "ge\u00b7het", "durch", "er\u00b7w\u00e4hl\u00b7ter", "H\u00f6\u00b7rer", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer neu belebt geht es von Mund zu Munde.", "tokens": ["Und", "im\u00b7mer", "neu", "be\u00b7lebt", "geht", "es", "von", "Mund", "zu", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch bildet es sich um, je weiter um es geht,", "tokens": ["Doch", "bil\u00b7det", "es", "sich", "um", ",", "je", "wei\u00b7ter", "um", "es", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwandelt sich und schwankt, nur das geschriebne steht.", "tokens": ["Ver\u00b7wan\u00b7delt", "sich", "und", "schwankt", ",", "nur", "das", "ge\u00b7schrieb\u00b7ne", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "$,", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ja, h\u00e4tte nicht die Schrift den Zauberkreis gezogen,", "tokens": ["Ja", ",", "h\u00e4t\u00b7te", "nicht", "die", "Schrift", "den", "Zau\u00b7ber\u00b7kreis", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Gold der Vorzeit w\u00e4r' im Wind wie Spreu verflogen.", "tokens": ["Viel", "Gold", "der", "Vor\u00b7zeit", "w\u00e4r'", "im", "Wind", "wie", "Spreu", "ver\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Nicht minder drum dem Mund lerndurst'ger Menschenkinder", "tokens": ["Nicht", "min\u00b7der", "drum", "dem", "Mund", "lern\u00b7dur\u00b7st'\u00b7ger", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als Spracherfinder sei geehrt der Schrifterfinder.", "tokens": ["Als", "Spra\u00b7cher\u00b7fin\u00b7der", "sei", "ge\u00b7ehrt", "der", "Schrif\u00b7ter\u00b7fin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Wer ists? Gott, dessen Stift an Erd- und Himmelstrift", "tokens": ["Wer", "ists", "?", "Gott", ",", "des\u00b7sen", "Stift", "an", "Erd", "und", "Him\u00b7mel\u00b7strift"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$.", "NN", "$,", "PRELAT", "NN", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschrieben seinen Ruhm in Blum- und Sternenschrift.", "tokens": ["Ge\u00b7schrie\u00b7ben", "sei\u00b7nen", "Ruhm", "in", "Blum", "und", "Ster\u00b7nen\u00b7schrift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Auf Tafeln von Lazur und auf smaragdner Flur,", "tokens": ["Auf", "Ta\u00b7feln", "von", "La\u00b7zur", "und", "auf", "sma\u00b7ragd\u00b7ner", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie im Rubin der Brust, lies seine Namen nur.", "tokens": ["Wie", "im", "Ru\u00b7bin", "der", "Brust", ",", "lies", "sei\u00b7ne", "Na\u00b7men", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NE", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}