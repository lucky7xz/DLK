{"textgrid.poem.57472": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "An die ber\u00fchmte K\u00f6nigl. Hofmalerinn Frau A.M. Wernerinn in Dresden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach so viel trefflichen Geschenken,", "tokens": ["Nach", "so", "viel", "treff\u00b7li\u00b7chen", "Ge\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Verwundre dich nur nicht, gepriesne Wernerinn,", "tokens": ["Ver\u00b7wund\u00b7re", "dich", "nur", "nicht", ",", "ge\u00b7pri\u00b7es\u00b7ne", "Wer\u00b7ne\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df, ob ich gleich entfernet bin,", "tokens": ["Da\u00df", ",", "ob", "ich", "gleich", "ent\u00b7fer\u00b7net", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Herz und Sinne sich noch eifrigst nach dir lenken.", "tokens": ["Doch", "Herz", "und", "Sin\u00b7ne", "sich", "noch", "eif\u00b7rigst", "nach", "dir", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PRF", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich kann mir, auch hier an der Plei\u00dfen,", "tokens": ["Ich", "kann", "mir", ",", "auch", "hier", "an", "der", "Plei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein witzerf\u00fcllt und g\u00fctig Haus", "tokens": ["Dein", "wit\u00b7zer\u00b7f\u00fcllt", "und", "g\u00fc\u00b7tig", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch nicht aus den Gedanken rei\u00dfen,", "tokens": ["Noch", "nicht", "aus", "den", "Ge\u00b7dan\u00b7ken", "rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dr\u00fccke meinen Dank durch diese Zeilen aus.", "tokens": ["Und", "dr\u00fc\u00b7cke", "mei\u00b7nen", "Dank", "durch", "die\u00b7se", "Zei\u00b7len", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du hast mich gar zu sehr verbunden;", "tokens": ["Du", "hast", "mich", "gar", "zu", "sehr", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn was bewog doch wohl die werthe Meisterhand,", "tokens": ["Denn", "was", "be\u00b7wog", "doch", "wohl", "die", "wert\u00b7he", "Meis\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sie so viel auf mich verwandt?", "tokens": ["Da\u00df", "sie", "so", "viel", "auf", "mich", "ver\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hab ich so viel Theil an deiner Huld gefunden?", "tokens": ["Wie", "hab", "ich", "so", "viel", "Theil", "an", "dei\u00b7ner", "Huld", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Pinsel soll nur K\u00f6nigskronen,", "tokens": ["Dein", "Pin\u00b7sel", "soll", "nur", "K\u00f6\u00b7nigs\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur hohen H\u00e4uptern heilig seyn;", "tokens": ["Nur", "ho\u00b7hen", "H\u00e4up\u00b7tern", "hei\u00b7lig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht Sch\u00e4fern, die in H\u00fctten wohnen:", "tokens": ["Nicht", "Sch\u00e4\u00b7fern", ",", "die", "in", "H\u00fct\u00b7ten", "woh\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie stimmt nun deine Kunst mit deiner Demuth ein?", "tokens": ["Wie", "stimmt", "nun", "dei\u00b7ne", "Kunst", "mit", "dei\u00b7ner", "De\u00b7muth", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich kann es wahrlich nicht ermessen:", "tokens": ["Ich", "kann", "es", "wahr\u00b7lich", "nicht", "er\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin von dir besch\u00e4mt, und steh in tiefster Schuld.", "tokens": ["Ich", "bin", "von", "dir", "be\u00b7sch\u00e4mt", ",", "und", "steh", "in", "tiefs\u00b7ter", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADJD", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch habe nur mit mir Geduld:", "tokens": ["Doch", "ha\u00b7be", "nur", "mit", "mir", "Ge\u00b7duld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz soll nichts von dem, was du gethan, vergessen.", "tokens": ["Mein", "Herz", "soll", "nichts", "von", "dem", ",", "was", "du", "ge\u00b7than", ",", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! kl\u00e4ngen meine Cyther Seyten", "tokens": ["Ach", "!", "kl\u00e4n\u00b7gen", "mei\u00b7ne", "Cy\u00b7ther", "Sey\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So lieblich, als dein Pinsel malt:", "tokens": ["So", "lieb\u00b7lich", ",", "als", "dein", "Pin\u00b7sel", "malt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So w\u00fcrde bis auf sp\u00e4te Zeiten", "tokens": ["So", "w\u00fcr\u00b7de", "bis", "auf", "sp\u00e4\u00b7te", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Dank durch manches Lob von deiner Kunst bezahlt.", "tokens": ["Mein", "Dank", "durch", "man\u00b7ches", "Lob", "von", "dei\u00b7ner", "Kunst", "be\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vieleicht verst\u00e4rken sich die Lieder,", "tokens": ["Vie\u00b7leicht", "ver\u00b7st\u00e4r\u00b7ken", "sich", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ein so w\u00fcrdig Lob den matten Kiel belebt;", "tokens": ["Wenn", "ein", "so", "w\u00fcr\u00b7dig", "Lob", "den", "mat\u00b7ten", "Kiel", "be\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vielleicht, wenn dich mein Reim erhebt,", "tokens": ["Viel\u00b7leicht", ",", "wenn", "dich", "mein", "Reim", "er\u00b7hebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schallt selbst der ", "tokens": ["Schallt", "selbst", "der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die Musen werdens leicht verg\u00f6nnen,", "tokens": ["Die", "Mu\u00b7sen", "wer\u00b7dens", "leicht", "ver\u00b7g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mich ein frischer Lorber kr\u00f6nt,", "tokens": ["Da\u00df", "mich", "ein", "fri\u00b7scher", "Lor\u00b7ber", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil sie nichts sch\u00f6nes fodern k\u00f6nnen,", "tokens": ["Weil", "sie", "nichts", "sch\u00f6\u00b7nes", "fo\u00b7dern", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als da\u00df ein Dichter sich an deinen Ruhm gew\u00f6hnt.", "tokens": ["Als", "da\u00df", "ein", "Dich\u00b7ter", "sich", "an", "dei\u00b7nen", "Ruhm", "ge\u00b7w\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dein redlich Herz, dein frommes Wesen,", "tokens": ["Dein", "red\u00b7lich", "Herz", ",", "dein", "from\u00b7mes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist so, wie deine Kunst, das ist ganz ungemein;", "tokens": ["Ist", "so", ",", "wie", "dei\u00b7ne", "Kunst", ",", "das", "ist", "ganz", "un\u00b7ge\u00b7mein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von jedem St\u00fccke ganz allein", "tokens": ["Von", "je\u00b7dem", "St\u00fc\u00b7cke", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll billig einst die Welt ganz eigne Lieder lesen.", "tokens": ["Soll", "bil\u00b7lig", "einst", "die", "Welt", "ganz", "eig\u00b7ne", "Lie\u00b7der", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch die, der ich ganz eigen lebe,", "tokens": ["Auch", "die", ",", "der", "ich", "ganz", "ei\u00b7gen", "le\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vereinigt ihren Kiel mit mir:", "tokens": ["Ver\u00b7ei\u00b7nigt", "ih\u00b7ren", "Kiel", "mit", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn ich dich nun nicht gnug erhebe,", "tokens": ["Wenn", "ich", "dich", "nun", "nicht", "gnug", "er\u00b7he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So hoffe doch das Lob, das dir geb\u00fchrt, von ihr.", "tokens": ["So", "hof\u00b7fe", "doch", "das", "Lob", ",", "das", "dir", "ge\u00b7b\u00fchrt", ",", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie schreibet, wie dein Pinsel malet,", "tokens": ["Sie", "schrei\u00b7bet", ",", "wie", "dein", "Pin\u00b7sel", "ma\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr beyde ziert zugleich die gro\u00dfe Weichselstadt;", "tokens": ["Ihr", "bey\u00b7de", "ziert", "zu\u00b7gleich", "die", "gro\u00b7\u00dfe", "Weich\u00b7sel\u00b7stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein Danzig, das zwo T\u00f6chter hat,", "tokens": ["Dein", "Dan\u00b7zig", ",", "das", "zwo", "T\u00f6ch\u00b7ter", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "PRELS", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit welchen es f\u00fcrwahr aus gutem Grunde pralet.", "tokens": ["Mit", "wel\u00b7chen", "es", "f\u00fcr\u00b7wahr", "aus", "gu\u00b7tem", "Grun\u00b7de", "pra\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wird sie nun durch der Musen K\u00fcnste", "tokens": ["Wird", "sie", "nun", "durch", "der", "Mu\u00b7sen", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dereinst der sp\u00e4ten Welt bekannt:", "tokens": ["De\u00b7reinst", "der", "sp\u00e4\u00b7ten", "Welt", "be\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So hast du Theil an dem Gewinnste,", "tokens": ["So", "hast", "du", "Theil", "an", "dem", "Ge\u00b7winns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Denn so verewigt sie auch deine Meisterhand.", "tokens": ["Denn", "so", "ve\u00b7re\u00b7wigt", "sie", "auch", "dei\u00b7ne", "Meis\u00b7ter\u00b7hand",