{"textgrid.poem.40851": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: In unsern weichen, seid'nen Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In unsern weichen, seid'nen Zeiten", "tokens": ["In", "un\u00b7sern", "wei\u00b7chen", ",", "sei\u00b7d'\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was soll der Mann in starrem Erz?", "tokens": ["Was", "soll", "der", "Mann", "in", "star\u00b7rem", "Erz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht, da\u00df sein Vor\u00fcberschreiten", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "sein", "Vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch heute r\u00fchrt manch deutsches Herz!", "tokens": ["Noch", "heu\u00b7te", "r\u00fchrt", "manch", "deut\u00b7sches", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht ihn am Schaft des nun zerfetzten", "tokens": ["Seht", "ihn", "am", "Schaft", "des", "nun", "zer\u00b7fetz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Germanenbanners mannlich stehn!", "tokens": ["Ger\u00b7ma\u00b7nen\u00b7ban\u00b7ners", "mann\u00b7lich", "stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sahn's als Silberhaar des letzten", "tokens": ["Wir", "sahn's", "als", "Sil\u00b7ber\u00b7haar", "des", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der deutschen Kaiser j\u00fcngst verwehn.", "tokens": ["Der", "deut\u00b7schen", "Kai\u00b7ser", "j\u00fcngst", "ver\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sein Weckruf dr\u00f6hnt, der ihm die Bahnen,", "tokens": ["Sein", "Weck\u00b7ruf", "dr\u00f6hnt", ",", "der", "ihm", "die", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein eherner Johannes, weiht;", "tokens": ["Ein", "e\u00b7her\u00b7ner", "Jo\u00b7han\u00b7nes", ",", "weiht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wer wecken wollte, mu\u00dft' es ahnen,", "tokens": ["Wer", "we\u00b7cken", "woll\u00b7te", ",", "mu\u00dft'", "es", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df zu erstehn es hohe Zeit!", "tokens": ["Da\u00df", "zu", "er\u00b7stehn", "es", "ho\u00b7he", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Taugt Winterabende zu k\u00fcrzen,", "tokens": ["Taugt", "Win\u00b7ter\u00b7a\u00b7ben\u00b7de", "zu", "k\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der J\u00e4ger Theuerdank auch nicht,", "tokens": ["Der", "J\u00e4\u00b7ger", "Theu\u00b7er\u00b7dank", "auch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann er uns doch die Lehren w\u00fcrzen,", "tokens": ["Kann", "er", "uns", "doch", "die", "Leh\u00b7ren", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich's mit wilden Bestien ficht;", "tokens": ["Wie", "sich's", "mit", "wil\u00b7den", "Be\u00b7sti\u00b7en", "ficht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Ungeth\u00fcme sind die alten,", "tokens": ["Die", "Un\u00b7ge\u00b7th\u00fc\u00b7me", "sind", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch heut' nicht lassend von der Art,", "tokens": ["Noch", "heut'", "nicht", "las\u00b7send", "von", "der", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Spiele der Natur entfalten", "tokens": ["Nur", "Spie\u00b7le", "der", "Na\u00b7tur", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich neu in Schnauzen, Tatzen, Bart.", "tokens": ["Sich", "neu", "in", "Schnau\u00b7zen", ",", "Tat\u00b7zen", ",", "Bart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Jugendlied hat nicht gepriesen", "tokens": ["Mein", "Ju\u00b7gend\u00b7lied", "hat", "nicht", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Helden, weil's ein ", "tokens": ["Den", "Hel\u00b7den", ",", "weil's", "ein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nur darum war's entflammt f\u00fcr diesen,", "tokens": ["Nur", "da\u00b7rum", "wa\u00b7r's", "ent\u00b7flammt", "f\u00fcr", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "VVPP", "APPR", "PDAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil er der ", "tokens": ["Weil", "er", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "So r\u00fchrt gewaltiger im Herzen,", "tokens": ["So", "r\u00fchrt", "ge\u00b7wal\u00b7ti\u00b7ger", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als weicher, \u00fcpp'ger, reicher Mai,", "tokens": ["Als", "wei\u00b7cher", ",", "\u00fcpp'\u00b7ger", ",", "rei\u00b7cher", "Mai", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vorfr\u00fchling mich im st\u00fcrm'schen M\u00e4rzen,", "tokens": ["Vor\u00b7fr\u00fch\u00b7ling", "mich", "im", "st\u00fcr\u00b7m'\u00b7schen", "M\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenns Kampfzeit noch, waghaft und frei!", "tokens": ["Wenns", "Kampf\u00b7zeit", "noch", ",", "wag\u00b7haft", "und", "frei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Wenn Halme, die zum Lichte treiben,", "tokens": ["Wenn", "Hal\u00b7me", ",", "die", "zum", "Lich\u00b7te", "trei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Speere starren kampfbereit,", "tokens": ["Als", "Spee\u00b7re", "star\u00b7ren", "kampf\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blumen Rottenf\u00e4hnlein bleiben,", "tokens": ["Die", "Blu\u00b7men", "Rot\u00b7ten\u00b7f\u00e4hn\u00b7lein", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und noch nicht Sybaritenkleid;", "tokens": ["Und", "noch", "nicht", "Sy\u00b7ba\u00b7ri\u00b7ten\u00b7kleid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn Pfeile noch die Sonnenblicke,", "tokens": ["Wenn", "Pfei\u00b7le", "noch", "die", "Son\u00b7nen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Singvogel als Prophete wirbt", "tokens": ["Sing\u00b7vo\u00b7gel", "als", "Pro\u00b7phe\u00b7te", "wirbt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, M\u00e4rtyrer der Frostest\u00fccke,", "tokens": ["Und", ",", "M\u00e4r\u00b7ty\u00b7rer", "der", "Fros\u00b7te\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr sch\u00f6nen Fr\u00fchlingsglauben stirbt;", "tokens": ["F\u00fcr", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7glau\u00b7ben", "stirbt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Strom noch nicht als M\u00fc\u00dfigg\u00e4nger", "tokens": ["Der", "Strom", "noch", "nicht", "als", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKNEG", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Berg und Wald behaglich streicht,", "tokens": ["Durch", "Berg", "und", "Wald", "be\u00b7hag\u00b7lich", "streicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, mit den Spolien seiner Dr\u00e4nger,", "tokens": ["Nein", ",", "mit", "den", "Spo\u00b7li\u00b7en", "sei\u00b7ner", "Dr\u00e4n\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dem Scholleneis, beladen keucht;", "tokens": ["Dem", "Schol\u00b7len\u00b7eis", ",", "be\u00b7la\u00b7den", "keucht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, da\u00df ein Herz von Fr\u00fchlingswonne", "tokens": ["Ach", ",", "da\u00df", "ein", "Herz", "von", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stets tr\u00e4umt, wenn ihrer es entbehrt!", "tokens": ["Stets", "tr\u00e4umt", ",", "wenn", "ih\u00b7rer", "es", "ent\u00b7behrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getrost! S\u00e4umt auch die Weltensonne,", "tokens": ["Ge\u00b7trost", "!", "S\u00e4umt", "auch", "die", "Wel\u00b7ten\u00b7son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcht traulich doch manch Nachbarherd.", "tokens": ["Gl\u00fcht", "trau\u00b7lich", "doch", "manch", "Nach\u00b7bar\u00b7herd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sei auch von einst'gen Lenzesreichen", "tokens": ["Sei", "auch", "von", "einst'\u00b7gen", "Len\u00b7zes\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns noch kein sch\u00f6n'res Pfand zu schaun,", "tokens": ["Uns", "noch", "kein", "sch\u00f6n'\u00b7res", "Pfand", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Scheint's doch kein \u00fcbles Fr\u00fchlingszeichen,", "tokens": ["Scheint's", "doch", "kein", "\u00fcb\u00b7les", "Fr\u00fch\u00b7lings\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df schon die alten Kater miaun!", "tokens": ["Da\u00df", "schon", "die", "al\u00b7ten", "Ka\u00b7ter", "mi\u00b7aun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Herbstschauer h\u00e4lt mich jetzt beklommen,", "tokens": ["Herbst\u00b7schau\u00b7er", "h\u00e4lt", "mich", "jetzt", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein d\u00fcrres Blatt spielt mir vorbei;", "tokens": ["Ein", "d\u00fcr\u00b7res", "Blatt", "spielt", "mir", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df welke Blatt auch soll mir frommen,", "tokens": ["Die\u00df", "wel\u00b7ke", "Blatt", "auch", "soll", "mir", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PWAT", "NN", "ADV", "VMFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob mein Jugendlied es sei.", "tokens": ["Als", "ob", "mein", "Ju\u00b7gend\u00b7lied", "es", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nimm es, o Windeshauch, du freier,", "tokens": ["Nimm", "es", ",", "o", "Win\u00b7des\u00b7hauch", ",", "du", "frei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "FM", "NN", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entf\u00fchr's aus liebem Schwabenland", "tokens": ["Ent\u00b7f\u00fchr's", "aus", "lie\u00b7bem", "Schwa\u00b7ben\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weithin zum sch\u00f6nen Lande Steyer,", "tokens": ["Weit\u00b7hin", "zum", "sch\u00f6\u00b7nen", "Lan\u00b7de", "Stey\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Freunde dort ein Liebespfand!", "tokens": ["Dem", "Freun\u00b7de", "dort", "ein", "Lie\u00b7be\u00b7spfand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Vielleicht bald bring' ich andre Gabe:", "tokens": ["Viel\u00b7leicht", "bald", "bring'", "ich", "and\u00b7re", "Ga\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wenn ich nichts mehr geben kann,", "tokens": ["Doch", "wenn", "ich", "nichts", "mehr", "ge\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrwahr, da\u00df man mein Grab mir grabe,", "tokens": ["F\u00fcr\u00b7wahr", ",", "da\u00df", "man", "mein", "Grab", "mir", "gra\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die beste, h\u00f6chste Zeit ist's dann.", "tokens": ["Die", "bes\u00b7te", ",", "h\u00f6chs\u00b7te", "Zeit", "ist's", "dann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "In unsern weichen, seid'nen Zeiten", "tokens": ["In", "un\u00b7sern", "wei\u00b7chen", ",", "sei\u00b7d'\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was soll der Mann in starrem Erz?", "tokens": ["Was", "soll", "der", "Mann", "in", "star\u00b7rem", "Erz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht, da\u00df sein Vor\u00fcberschreiten", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "sein", "Vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch heute r\u00fchrt manch deutsches Herz!", "tokens": ["Noch", "heu\u00b7te", "r\u00fchrt", "manch", "deut\u00b7sches", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Seht ihn am Schaft des nun zerfetzten", "tokens": ["Seht", "ihn", "am", "Schaft", "des", "nun", "zer\u00b7fetz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Germanenbanners mannlich stehn!", "tokens": ["Ger\u00b7ma\u00b7nen\u00b7ban\u00b7ners", "mann\u00b7lich", "stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sahn's als Silberhaar des letzten", "tokens": ["Wir", "sahn's", "als", "Sil\u00b7ber\u00b7haar", "des", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der deutschen Kaiser j\u00fcngst verwehn.", "tokens": ["Der", "deut\u00b7schen", "Kai\u00b7ser", "j\u00fcngst", "ver\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Sein Weckruf dr\u00f6hnt, der ihm die Bahnen,", "tokens": ["Sein", "Weck\u00b7ruf", "dr\u00f6hnt", ",", "der", "ihm", "die", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein eherner Johannes, weiht;", "tokens": ["Ein", "e\u00b7her\u00b7ner", "Jo\u00b7han\u00b7nes", ",", "weiht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wer wecken wollte, mu\u00dft' es ahnen,", "tokens": ["Wer", "we\u00b7cken", "woll\u00b7te", ",", "mu\u00dft'", "es", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df zu erstehn es hohe Zeit!", "tokens": ["Da\u00df", "zu", "er\u00b7stehn", "es", "ho\u00b7he", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Taugt Winterabende zu k\u00fcrzen,", "tokens": ["Taugt", "Win\u00b7ter\u00b7a\u00b7ben\u00b7de", "zu", "k\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der J\u00e4ger Theuerdank auch nicht,", "tokens": ["Der", "J\u00e4\u00b7ger", "Theu\u00b7er\u00b7dank", "auch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann er uns doch die Lehren w\u00fcrzen,", "tokens": ["Kann", "er", "uns", "doch", "die", "Leh\u00b7ren", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich's mit wilden Bestien ficht;", "tokens": ["Wie", "sich's", "mit", "wil\u00b7den", "Be\u00b7sti\u00b7en", "ficht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.20": {"line.1": {"text": "Die Ungeth\u00fcme sind die alten,", "tokens": ["Die", "Un\u00b7ge\u00b7th\u00fc\u00b7me", "sind", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch heut' nicht lassend von der Art,", "tokens": ["Noch", "heut'", "nicht", "las\u00b7send", "von", "der", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "w