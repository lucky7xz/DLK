auch", "die", "Wel\u00b7ten\u00b7son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcht traulich doch manch Nachbarherd.", "tokens": ["Gl\u00fcht", "trau\u00b7lich", "doch", "manch", "Nach\u00b7bar\u00b7herd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Sei auch von einst'gen Lenzesreichen", "tokens": ["Sei", "auch", "von", "einst'\u00b7gen", "Len\u00b7zes\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns noch kein sch\u00f6n'res Pfand zu schaun,", "tokens": ["Uns", "noch", "kein", "sch\u00f6n'\u00b7res", "Pfand", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Scheint's doch kein \u00fcbles Fr\u00fchlingszeichen,", "tokens": ["Scheint's", "doch", "kein", "\u00fcb\u00b7les", "Fr\u00fch\u00b7lings\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df schon die alten Kater miaun!", "tokens": ["Da\u00df", "schon", "die", "al\u00b7ten", "Ka\u00b7ter", "mi\u00b7aun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.28": {"line.1": {"text": "Herbstschauer h\u00e4lt mich jetzt beklommen,", "tokens": ["Herbst\u00b7schau\u00b7er", "h\u00e4lt", "mich", "jetzt", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein d\u00fcrres Blatt spielt mir vorbei;", "tokens": ["Ein", "d\u00fcr\u00b7res", "Blatt", "spielt", "mir", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df welke Blatt auch soll mir frommen,", "tokens": ["Die\u00df", "wel\u00b7ke", "Blatt", "auch", "soll", "mir", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PWAT", "NN", "ADV", "VMFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob mein Jugendlied es sei.", "tokens": ["Als", "ob", "mein", "Ju\u00b7gend\u00b7lied", "es", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.29": {"line.1": {"text": "Nimm es, o Windeshauch, du freier,", "tokens": ["Nimm", "es", ",", "o", "Win\u00b7des\u00b7hauch", ",", "du", "frei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "FM", "NN", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entf\u00fchr's aus liebem Schwabenland", "tokens": ["Ent\u00b7f\u00fchr's", "aus", "lie\u00b7bem", "Schwa\u00b7ben\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weithin zum sch\u00f6nen Lande Steyer,", "tokens": ["Weit\u00b7hin", "zum", "sch\u00f6\u00b7nen", "Lan\u00b7de", "Stey\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Freunde dort ein Liebespfand!", "tokens": ["Dem", "Freun\u00b7de", "dort", "ein", "Lie\u00b7be\u00b7spfand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.30": {"line.1": {"text": "Vielleicht bald bring' ich andre Gabe:", "tokens": ["Viel\u00b7leicht", "bald", "bring'", "ich", "and\u00b7re", "Ga\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wenn ich nichts mehr geben kann,", "tokens": ["Doch", "wenn", "ich", "nichts", "mehr", "ge\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrwahr, da\u00df man mein Grab mir grabe,", "tokens": ["F\u00fcr\u00b7wahr", ",", "da\u00df", "man", "mein", "Grab", "mir", "gra\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die beste, h\u00f6chste Zeit ist's dann.", "tokens": ["Die", "bes\u00b7te", ",", "h\u00f6chs\u00b7te", "Zeit", "ist's", "dann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}