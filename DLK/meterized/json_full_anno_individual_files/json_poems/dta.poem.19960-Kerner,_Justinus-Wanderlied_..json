{"dta.poem.19960": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf! noch getrunken", "tokens": ["Wohl\u00b7auf", "!", "noch", "ge\u00b7trun\u00b7ken"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$.", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den funkelnden Wein!", "tokens": ["Den", "fun\u00b7keln\u00b7den", "Wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ade nun, ihr Lieben!", "tokens": ["A\u00b7de", "nun", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geschieden mu\u00df seyn.", "tokens": ["Ge\u00b7schie\u00b7den", "mu\u00df", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VAINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ade nun, ihr Berge,", "tokens": ["A\u00b7de", "nun", ",", "ihr", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Du v\u00e4terlich Haus!", "tokens": ["Du", "v\u00e4\u00b7ter\u00b7lich", "Haus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es treibt in die Ferne", "tokens": ["Es", "treibt", "in", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mich m\u00e4chtig hinaus.", "tokens": ["Mich", "m\u00e4ch\u00b7tig", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Die Sonne, sie bleibet", "tokens": ["Die", "Son\u00b7ne", ",", "sie", "blei\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am Himmel nicht steh'n,", "tokens": ["Am", "Him\u00b7mel", "nicht", "steh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es treibt sie, durch L\u00e4nder", "tokens": ["Es", "treibt", "sie", ",", "durch", "L\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Meere zu geh'n.", "tokens": ["Und", "Mee\u00b7re", "zu", "geh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "NN", "PTKZU", "VVFIN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die Woge nicht haftet", "tokens": ["Die", "Wo\u00b7ge", "nicht", "haf\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Am einsamen Strand,", "tokens": ["Am", "ein\u00b7sa\u00b7men", "Strand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die St\u00fcrme, sie brausen", "tokens": ["Die", "St\u00fcr\u00b7me", ",", "sie", "brau\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit Macht durch das Land.", "tokens": ["Mit", "Macht", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Mit eilenden Wolken", "tokens": ["Mit", "ei\u00b7len\u00b7den", "Wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Vogel dort zieht,", "tokens": ["Der", "Vo\u00b7gel", "dort", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und singt in der Ferne", "tokens": ["Und", "singt", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein heimatlich Lied.", "tokens": ["Ein", "hei\u00b7mat\u00b7lich", "Lied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "So treibt es den Burschen", "tokens": ["So", "treibt", "es", "den", "Bur\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durch W\u00e4lder und Feld,", "tokens": ["Durch", "W\u00e4l\u00b7der", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Zu gleichen der Mutter,", "tokens": ["Zu", "glei\u00b7chen", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der wandernden Welt.", "tokens": ["Der", "wan\u00b7dern\u00b7den", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Da gr\u00fc\u00dfen ihn V\u00f6gel", "tokens": ["Da", "gr\u00fc\u00b7\u00dfen", "ihn", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bekannt \u00fcber'm Meer,", "tokens": ["Be\u00b7kannt", "\u00fc\u00b7ber'm", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie flogen von Fluren", "tokens": ["Sie", "flo\u00b7gen", "von", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Heimat hieher,", "tokens": ["Der", "Hei\u00b7mat", "hie\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da duften die Blumen", "tokens": ["Da", "duf\u00b7ten", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vertraulich um ihn,", "tokens": ["Ver\u00b7trau\u00b7lich", "um", "ihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie trieben vom Lande", "tokens": ["Sie", "trie\u00b7ben", "vom", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die L\u00fcfte dahin.", "tokens": ["Die", "L\u00fcf\u00b7te", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Die V\u00f6gel die kennen", "tokens": ["Die", "V\u00f6\u00b7gel", "die", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sein v\u00e4terlich Haus.", "tokens": ["Sein", "v\u00e4\u00b7ter\u00b7lich", "Haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Blumen einst pflanzt er", "tokens": ["Die", "Blu\u00b7men", "einst", "pflanzt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Liebe zum Strau\u00df,", "tokens": ["Der", "Lie\u00b7be", "zum", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und Liebe die folgt ihm,", "tokens": ["Und", "Lie\u00b7be", "die", "folgt", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie geht ihm zur Hand;", "tokens": ["Sie", "geht", "ihm", "zur", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So wird ihm zur Heimat", "tokens": ["So", "wird", "ihm", "zur", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das ferneste Land.", "tokens": ["Das", "fer\u00b7nes\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}