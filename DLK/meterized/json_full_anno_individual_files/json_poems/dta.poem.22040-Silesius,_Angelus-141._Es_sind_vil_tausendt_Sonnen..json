{"dta.poem.22040": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "141. Es sind vil tausendt Sonnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst im Firmament sey eine Sonn\u2019 allein", "tokens": ["Du", "sprichst", "im", "Fir\u00b7ma\u00b7ment", "sey", "ei\u00b7ne", "Sonn'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aber sage/ da\u00df vil tausendt Sonnen seyn.", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "/", "da\u00df", "vil", "tau\u00b7sendt", "Son\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$(", "KOUS", "ADV", "CARD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}