{"textgrid.poem.26130": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Versonnen wie die Augenblicke, von denen keine Tafeln schreiben", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Abendberge, wie eine eiserne Schrift,", "tokens": ["Durch", "die", "A\u00b7bend\u00b7ber\u00b7ge", ",", "wie", "ei\u00b7ne", "ei\u00b7ser\u00b7ne", "Schrift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der gewundene Flu\u00df im Tal hinschreibt.", "tokens": ["Der", "ge\u00b7wun\u00b7de\u00b7ne", "Flu\u00df", "im", "Tal", "hin\u00b7schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie auf graue Tafeln mit eisernem Stift,", "tokens": ["Wie", "auf", "grau\u00b7e", "Ta\u00b7feln", "mit", "ei\u00b7ser\u00b7nem", "Stift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wie ein ewig Wort er eingegraben bleibt.", "tokens": ["Und", "wie", "ein", "e\u00b7wig", "Wort", "er", "ein\u00b7ge\u00b7gra\u00b7ben", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJD", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch fl\u00fcchtig nur stehen an steiniger Stell'", "tokens": ["Doch", "fl\u00fcch\u00b7tig", "nur", "ste\u00b7hen", "an", "stei\u00b7ni\u00b7ger", "Stell'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Am Berg bei mir oben die Anemonen,", "tokens": ["Am", "Berg", "bei", "mir", "o\u00b7ben", "die", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die wie die Taubenschaar wei\u00df auf H\u00f6hen wohnen", "tokens": ["Die", "wie", "die", "Tau\u00b7ben\u00b7schaar", "wei\u00df", "auf", "H\u00f6\u00b7hen", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "NN", "VVFIN", "APPR", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und im Abend noch lange helleuchtend bleiben,", "tokens": ["Und", "im", "A\u00b7bend", "noch", "lan\u00b7ge", "hel\u00b7leuch\u00b7tend", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Versonnen wie die Augenblicke, von denen keine Tafeln schreiben.", "tokens": ["Ver\u00b7son\u00b7nen", "wie", "die", "Au\u00b7gen\u00b7bli\u00b7cke", ",", "von", "de\u00b7nen", "kei\u00b7ne", "Ta\u00b7feln", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "APPR", "PRELS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Durch die Abendberge,