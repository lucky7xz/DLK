{"textgrid.poem.59298": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Stille", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wei\u00df und r\u00e4t es doch keiner,", "tokens": ["Es", "wei\u00df", "und", "r\u00e4t", "es", "doch", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie mir so wohl ist, so wohl!", "tokens": ["Wie", "mir", "so", "wohl", "ist", ",", "so", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VAFIN", "$,", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, w\u00fc\u00dft es nur Einer, nur Einer,", "tokens": ["Ach", ",", "w\u00fc\u00dft", "es", "nur", "Ei\u00b7ner", ",", "nur", "Ei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "PIS", "$,", "ADV", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Kein Mensch es sonst wissen soll!", "tokens": ["Kein", "Mensch", "es", "sonst", "wis\u00b7sen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So still ist's nicht drau\u00dfen im Schnee,", "tokens": ["So", "still", "ist's", "nicht", "drau\u00b7\u00dfen", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So stumm und verschwiegen sind", "tokens": ["So", "stumm", "und", "ver\u00b7schwie\u00b7gen", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVPP", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Sterne nicht in der H\u00f6he,", "tokens": ["Die", "Ster\u00b7ne", "nicht", "in", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als meine Gedanken sind.", "tokens": ["Als", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich w\u00fcnscht, es w\u00e4re schon Morgen,", "tokens": ["Ich", "w\u00fcnscht", ",", "es", "w\u00e4\u00b7re", "schon", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da fliegen zwei Lerchen auf,", "tokens": ["Da", "flie\u00b7gen", "zwei", "Ler\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die \u00fcberfliegen einander,", "tokens": ["Die", "\u00fc\u00b7berf\u00b7lie\u00b7gen", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Herze folgt ihrem Lauf.", "tokens": ["Mein", "Her\u00b7ze", 