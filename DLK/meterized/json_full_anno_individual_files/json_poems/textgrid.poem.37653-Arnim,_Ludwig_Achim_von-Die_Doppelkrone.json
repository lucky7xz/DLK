ft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mag dich darum grauen,", "tokens": ["So", "mag", "dich", "da\u00b7rum", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch siehst du doppelt schon die Kron,", "tokens": ["Doch", "siehst", "du", "dop\u00b7pelt", "schon", "die", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann st\u00fcrz' ich mich mit dir vom Thron,", "tokens": ["Dann", "st\u00fcrz'", "ich", "mich", "mit", "dir", "vom", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Teufel m\u00f6cht' dir trauen,", "tokens": ["Der", "Teu\u00b7fel", "m\u00f6cht'", "dir", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du k\u00f6nntest lernen schauen.", "tokens": ["Du", "k\u00f6nn\u00b7test", "ler\u00b7nen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}