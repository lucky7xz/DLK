{"textgrid.poem.62900": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Bauer kam berauscht von einem Hochzeitschmaus", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bauer kam berauscht von einem Hochzeitschmaus", "tokens": ["Ein", "Bau\u00b7er", "kam", "be\u00b7rauscht", "von", "ei\u00b7nem", "Hoch\u00b7zeit\u00b7schmaus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schl\u00e4ngelte, gleich dem Meanderflusse,", "tokens": ["Und", "schl\u00e4n\u00b7gel\u00b7te", ",", "gleich", "dem", "Me\u00b7an\u00b7der\u00b7flus\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sich mit gesenktem Kopf nach Haus.", "tokens": ["Sich", "mit", "ge\u00b7senk\u00b7tem", "Kopf", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einmal lag sein Hut ihm vor dem Fusse;", "tokens": ["Auf", "ein\u00b7mal", "lag", "sein", "Hut", "ihm", "vor", "dem", "Fus\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er raft ihn auf; mit freundlichem Gesicht", "tokens": ["Er", "raft", "ihn", "auf", ";", "mit", "freund\u00b7li\u00b7chem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "H\u00e4lt er ihn hoch empor: wer hat den Hut verloren?", "tokens": ["H\u00e4lt", "er", "ihn", "hoch", "em\u00b7por", ":", "wer", "hat", "den", "Hut", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$.", "PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ruft er aus vollem Hals. Ein jeder lacht des Thoren;", "tokens": ["Ruft", "er", "aus", "vol\u00b7lem", "Hals", ".", "Ein", "je\u00b7der", "lacht", "des", "Tho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur einer sprach: Freund, merket ihr denn nicht,", "tokens": ["Nur", "ei\u00b7ner", "sprach", ":", "Freund", ",", "mer\u00b7ket", "ihr", "denn", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$.", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Da\u00df ihr mit blosem Sch\u00e4del wandelt?", "tokens": ["Da\u00df", "ihr", "mit", "blo\u00b7sem", "Sch\u00e4\u00b7del", "wan\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er setzt den Deckel auf und schleicht nach seinem Hof,", "tokens":