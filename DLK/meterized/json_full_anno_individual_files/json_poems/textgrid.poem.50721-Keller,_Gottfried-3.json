{"textgrid.poem.50721": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht wiegt sich mit sternbes\u00e4ten Schwingen", "tokens": ["Die", "Nacht", "wiegt", "sich", "mit", "stern\u00b7be\u00b7s\u00e4\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf stillen Weltmeers blauen Wasserg\u00e4rten;", "tokens": ["Auf", "stil\u00b7len", "Welt\u00b7meers", "blau\u00b7en", "Was\u00b7ser\u00b7g\u00e4r\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daraus zur\u00fcck, wie Silbergl\u00f6cklein, klingen", "tokens": ["Da\u00b7raus", "zu\u00b7r\u00fcck", ",", "wie", "Sil\u00b7ber\u00b7gl\u00f6c\u00b7klein", ",", "klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "PTKVZ", "$,", "PWAV", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sterne, die in feuchter Flut verkl\u00e4rten.", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "in", "feuch\u00b7ter", "Flut", "ver\u00b7kl\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ein entschlummert Kind an Mutterbr\u00fcsten", "tokens": ["Wie", "ein", "ent\u00b7schlum\u00b7mert", "Kind", "an", "Mut\u00b7ter\u00b7br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruht eine Insel selig in den Wogen:", "tokens": ["Ruht", "ei\u00b7ne", "In\u00b7sel", "se\u00b7lig", "in", "den", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So weich und wei\u00df ist um die gr\u00fcnen K\u00fcsten,", "tokens": ["So", "weich", "und", "wei\u00df", "ist", "um", "die", "gr\u00fc\u00b7nen", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mutterarm, die Brandung rings gezogen.", "tokens": ["Der", "Mut\u00b7ter\u00b7arm", ",", "die", "Bran\u00b7dung", "rings", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Insel schl\u00e4ft, doch Tr\u00e4ume auf ihr gaukeln.", "tokens": ["Die", "In\u00b7sel", "schl\u00e4ft", ",", "doch", "Tr\u00e4u\u00b7me", "auf", "ihr", "gau\u00b7keln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie bl\u00fcht, wie leuchtet, fl\u00fcstert es so minnig!", "tokens": ["Wie", "bl\u00fcht", ",", "wie", "leuch\u00b7tet", ",", "fl\u00fcs\u00b7tert", "es", "so", "min\u00b7nig", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PWAV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie lustig die Lianenkr\u00e4nze schaukeln!", "tokens": ["Wie", "lus\u00b7tig", "die", "Li\u00b7a\u00b7nen\u00b7kr\u00e4n\u00b7ze", "schau\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie duftet der Orangenhain so innig!", "tokens": ["Wie", "duf\u00b7tet", "der", "O\u00b7ran\u00b7gen\u00b7hain", "so", "in\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich wollt, mein Herze w\u00e4r so dicht umflossen", "tokens": ["Ich", "wollt", ",", "mein", "Her\u00b7ze", "w\u00e4r", "so", "dicht", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "VVFIN", "VAFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einem Meer der Ruhe und der Klarheit", "tokens": ["Von", "ei\u00b7nem", "Meer", "der", "Ru\u00b7he", "und", "der", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dr\u00fcberhin ein Himmel ausgegossen,", "tokens": ["Und", "dr\u00fc\u00b7berh\u00b7in", "ein", "Him\u00b7mel", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des einz'ge Sonne nur das Licht der Wahrheit!", "tokens": ["Des", "einz'\u00b7ge", "Son\u00b7ne", "nur", "das", "Licht", "der", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was mag denn wohl der alte Urwald tr\u00e4umen?", "tokens": ["Was", "mag", "denn", "wohl", "der", "al\u00b7te", "Ur\u00b7wald", "tr\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er ist ja selbst ein \u00fcpp'ger Traum der Zeiten,", "tokens": ["Er", "ist", "ja", "selbst", "ein", "\u00fcpp'\u00b7ger", "Traum", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn, gr\u00fc\u00dfend nach des Meeres Silbersch\u00e4umen,", "tokens": ["Wenn", ",", "gr\u00fc\u00b7\u00dfend", "nach", "des", "Mee\u00b7res", "Sil\u00b7ber\u00b7sch\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hoch auf ihm hin sich Blumeng\u00e4rten breiten.", "tokens": ["Hoch", "auf", "ihm", "hin", "sich", "Blu\u00b7men\u00b7g\u00e4r\u00b7ten", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das ist das Paradies im Mondenschimmer,", "tokens": ["Das", "ist", "das", "Pa\u00b7ra\u00b7dies", "im", "Mon\u00b7den\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Geisterabglanz des so schn\u00f6d verlornen,", "tokens": ["Ein", "Geis\u00b7ter\u00b7ab\u00b7glanz", "des", "so", "schn\u00f6d", "ver\u00b7lor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Erdengl\u00fcck ein abgesprungner Flimmer,", "tokens": ["Vom", "Er\u00b7den\u00b7gl\u00fcck", "ein", "ab\u00b7ge\u00b7sprung\u00b7ner", "Flim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Bl\u00fctenblatt vom Sarg des totgebornen!", "tokens": ["Ein", "Bl\u00fc\u00b7ten\u00b7blatt", "vom", "Sarg", "des", "tot\u00b7ge\u00b7bor\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und sch\u00f6ne Menschen schlafen in den B\u00fcschen,", "tokens": ["Und", "sch\u00f6\u00b7ne", "Men\u00b7schen", "schla\u00b7fen", "in", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Bildwerk in ein Blumentuch gewoben,", "tokens": ["Wie", "Bild\u00b7werk", "in", "ein", "Blu\u00b7men\u00b7tuch", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Engel in der Waldkapelle Nischen,", "tokens": ["Wie", "En\u00b7gel", "in", "der", "Wald\u00b7ka\u00b7pel\u00b7le", "Ni\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie G\u00f6tterspielzeug sorglich aufgehoben.", "tokens": ["Wie", "G\u00f6t\u00b7ter\u00b7spiel\u00b7zeug", "sorg\u00b7lich", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und \u00fcber allem wallt ein frohes Ahnen,", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7lem", "wallt", "ein", "fro\u00b7hes", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich unbewu\u00dft, und doch so alldurchdrungen,", "tokens": ["Sich", "un\u00b7be\u00b7wu\u00dft", ",", "und", "doch", "so", "all\u00b7durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Blutlauf, der in unsichtbaren Bahnen", "tokens": ["Der", "Blut\u00b7lauf", ",", "der", "in", "un\u00b7sicht\u00b7ba\u00b7ren", "Bah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies reine Leben in den Gang geschwungen.", "tokens": ["Dies", "rei\u00b7ne", "Le\u00b7ben", "in", "den", "Gang", "ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u2013 Ein Blitz, ein Schlag! \u2013 die Meeresfl\u00e4che zittert;", "tokens": ["\u2013", "Ein", "Blitz", ",", "ein", "Schlag", "!", "\u2013", "die", "Mee\u00b7res\u00b7fl\u00e4\u00b7che", "zit\u00b7tert", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Braun w\u00e4lzt der Rauch sich auf dem krausen Spiegel:", "tokens": ["Braun", "w\u00e4lzt", "der", "Rauch", "sich", "auf", "dem", "krau\u00b7sen", "Spie\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Als h\u00e4tt ein Drache seinen Raub gewittert,", "tokens": ["Als", "h\u00e4tt", "ein", "Dra\u00b7che", "sei\u00b7nen", "Raub", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Naht es sich pfeilschnell mit gespreiztem Fl\u00fcgel.", "tokens": ["Naht", "es", "sich", "pfeil\u00b7schnell", "mit", "ge\u00b7spreiz\u00b7tem", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Die Anker rasseln und die Segel sinken", "tokens": ["Die", "An\u00b7ker", "ras\u00b7seln", "und", "die", "Se\u00b7gel", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Wohl fallen auch des Himmels sch\u00f6ne Sterne \u2013", "tokens": ["\u2013", "Wohl", "fal\u00b7len", "auch", "des", "Him\u00b7mels", "sch\u00f6\u00b7ne", "Ster\u00b7ne", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hundert Bleigesichter l\u00fcstern blinken", "tokens": ["Und", "hun\u00b7dert", "Blei\u00b7ge\u00b7sich\u00b7ter", "l\u00fcs\u00b7tern", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im fahlen Schein der tr\u00fcben Schiffslaterne.", "tokens": ["Im", "fah\u00b7len", "Schein", "der", "tr\u00fc\u00b7ben", "Schiffs\u00b7la\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Zuvorderst, auf des Schiffes schwarzen W\u00e4nden,", "tokens": ["Zu\u00b7vor\u00b7derst", ",", "auf", "des", "Schif\u00b7fes", "schwar\u00b7zen", "W\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ragt schw\u00e4rzlich aus der giererf\u00fcllten Rotte", "tokens": ["Ragt", "schw\u00e4rz\u00b7lich", "aus", "der", "gie\u00b7rer\u00b7f\u00fcll\u00b7ten", "Rot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Christenpfaffe, schwingend in den H\u00e4nden", "tokens": ["Der", "Chris\u00b7ten\u00b7pfaf\u00b7fe", ",", "schwin\u00b7gend", "in", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das blut'ge Kreuz mit dem gequ\u00e4lten Gotte!", "tokens": ["Das", "blut'\u00b7ge", "Kreuz", "mit", "dem", "ge\u00b7qu\u00e4l\u00b7ten", "Got\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die Nacht wiegt sich mit sternbes\u00e4ten Schwingen", "tokens": ["Die", "Nacht", "wiegt", "sich", "mit", "stern\u00b7be\u00b7s\u00e4\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf stillen Weltmeers blauen Wasserg\u00e4rten;", "tokens": ["Auf", "stil\u00b7len", "Welt\u00b7meers", "blau\u00b7en", "Was\u00b7ser\u00b7g\u00e4r\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daraus zur\u00fcck, wie Silbergl\u00f6cklein, klingen", "tokens": ["Da\u00b7raus", "zu\u00b7r\u00fcck", ",", "wie", "Sil\u00b7ber\u00b7gl\u00