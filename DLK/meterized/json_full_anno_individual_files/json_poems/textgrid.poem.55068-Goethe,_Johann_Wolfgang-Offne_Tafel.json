{"textgrid.poem.55068": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Offne Tafel", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viele G\u00e4ste w\u00fcnsch ich heut", "tokens": ["Vie\u00b7le", "G\u00e4s\u00b7te", "w\u00fcnsch", "ich", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu meinem Tische!", "tokens": ["Mir", "zu", "mei\u00b7nem", "Ti\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Speisen sind genug bereit,", "tokens": ["Spei\u00b7sen", "sind", "ge\u00b7nug", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "V\u00f6gel, Wild und Fische.", "tokens": ["V\u00f6\u00b7gel", ",", "Wild", "und", "Fi\u00b7sche", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eingeladen sind sie ja,", "tokens": ["Ein\u00b7ge\u00b7la\u00b7den", "sind", "sie", "ja", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6ne Kinder hoff ich nun,", "tokens": ["Sch\u00f6\u00b7ne", "Kin\u00b7der", "hoff", "ich", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die von gar nichts wissen,", "tokens": ["Die", "von", "gar", "nichts", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht, da\u00df es was H\u00fcbsches sei,", "tokens": ["Nicht", ",", "da\u00df", "es", "was", "H\u00fcb\u00b7sches", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PIS", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Freund zu k\u00fcssen.", "tokens": ["Ei\u00b7nen", "Freund", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eingeladen sind sie all,", "tokens": ["Ein\u00b7ge\u00b7la\u00b7den", "sind", "sie", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Frauen denk ich auch zu sehn,", "tokens": ["Frau\u00b7en", "denk", "ich", "auch", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Ehegatten,", "tokens": ["Die", "den", "E\u00b7he\u00b7gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ward er immer brummiger,", "tokens": ["Ward", "er", "im\u00b7mer", "brum\u00b7mi\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer lieber hatten.", "tokens": ["Im\u00b7mer", "lie\u00b7ber", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eingeladen wurden sie,", "tokens": ["Ein\u00b7ge\u00b7la\u00b7den", "wur\u00b7den", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Junge Herrn berief ich auch,", "tokens": ["Jun\u00b7ge", "Herrn", "be\u00b7rief", "ich", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht im mindsten eitel,", "tokens": ["Nicht", "im", "minds\u00b7ten", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die sogar bescheiden sind", "tokens": ["Die", "so\u00b7gar", "be\u00b7schei\u00b7den", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gef\u00fclltem Beutel;", "tokens": ["Mit", "ge\u00b7f\u00fcll\u00b7tem", "Beu\u00b7tel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Diese bat ich sonderlich,", "tokens": ["Die\u00b7se", "bat", "ich", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "M\u00e4nner lud ich mit Respekt,", "tokens": ["M\u00e4n\u00b7ner", "lud", "ich", "mit", "Res\u00b7pekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auf ihre Frauen", "tokens": ["Die", "auf", "ih\u00b7re", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ganz allein, nicht nebenaus", "tokens": ["Ganz", "al\u00b7lein", ",", "nicht", "ne\u00b7be\u00b7naus"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "PTKNEG", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Auf die sch\u00f6nste schauen.", "tokens": ["Auf", "die", "sch\u00f6ns\u00b7te", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie erwiderten den Gru\u00df,", "tokens": ["Sie", "er\u00b7wi\u00b7der\u00b7ten", "den", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dichter lud ich auch herbei,", "tokens": ["Dich\u00b7ter", "lud", "ich", "auch", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Lust zu mehren,", "tokens": ["Uns\u00b7re", "Lust", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die weit lieber ein fremdes Lied", "tokens": ["Die", "weit", "lie\u00b7ber", "ein", "frem\u00b7des", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ART", "ADJA", "NN"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als ihr eignes h\u00f6ren.", "tokens": ["Als", "ihr", "eig\u00b7nes", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Alle diese stimmten ein,", "tokens": ["Al\u00b7le", "die\u00b7se", "stimm\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Doch ich sehe niemand gehn,", "tokens": ["Doch", "ich", "se\u00b7he", "nie\u00b7mand", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehe niemand rennen!", "tokens": ["Se\u00b7he", "nie\u00b7mand", "ren\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Suppe kocht und siedet ein,", "tokens": ["Sup\u00b7pe", "kocht", "und", "sie\u00b7det", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Braten will verbrennen.", "tokens": ["Bra\u00b7ten", "will", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ach, wir haben's, f\u00fcrcht ich nun,", "tokens": ["Ach", ",", "wir", "ha\u00b7ben's", ",", "f\u00fcrcht", "ich", "nun", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu genau genommen!", "tokens": ["Zu", "ge\u00b7nau", "ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, sag, was meinst du wohl?", "tokens": ["H\u00e4n\u00b7schen", ",", "sag", ",", "was", "meinst", "du", "wohl", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Es wird niemand kommen.", "tokens": ["Es", "wird", "nie\u00b7mand", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "H\u00e4nschen, lauf und s\u00e4ume nicht,", "tokens": ["H\u00e4n\u00b7schen", ",", "lauf", "und", "s\u00e4u\u00b7me", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruf mir neue G\u00e4ste!", "tokens": ["Ruf", "mir", "neu\u00b7e", "G\u00e4s\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jeder komme, wie er ist,", "tokens": ["Je\u00b7der", "kom\u00b7me", ",", "wie", "er", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist wohl das beste!", "tokens": ["Das", "ist", "wohl", "das", "bes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schon ist's in der Stadt bekannt,", "tokens": ["Schon", "ist's", "in", "der", "Stadt", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wohl ist's aufgenommen.", "tokens": ["Wohl", "ist's", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, mach die T\u00fcren auf:", "tokens": ["H\u00e4n\u00b7schen", ",", "mach", "die", "T\u00fc\u00b7ren", "auf", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh nur, wie sie kommen!", "tokens": ["Sieh", "nur", ",", "wie", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Viele G\u00e4ste w\u00fcnsch ich heut", "tokens": ["Vie\u00b7le", "G\u00e4s\u00b7te", "w\u00fcnsch", "ich", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zu meinem Tische!", "tokens": ["Mir", "zu", "mei\u00b7nem", "Ti\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Speisen sind genug bereit,", "tokens": ["Spei\u00b7sen", "sind", "ge\u00b7nug", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "V\u00f6gel, Wild und Fische.", "tokens": ["V\u00f6\u00b7gel", ",", "Wild", "und", "Fi\u00b7sche", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eingeladen sind sie ja,", "tokens": ["Ein\u00b7ge\u00b7la\u00b7den", "sind", "sie", "ja", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Sch\u00f6ne Kinder hoff ich nun,", "tokens": ["Sch\u00f6\u00b7ne", "Kin\u00b7der", "hoff", "ich", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die von gar nichts wissen,", "tokens": ["Die", "von", "gar", "nichts", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht, da\u00df es was H\u00fcbsches sei,", "tokens": ["Nicht", ",", "da\u00df", "es", "was", "H\u00fcb\u00b7sches", "sei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PIS", "ADJA", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Freund zu k\u00fcssen.", "tokens": ["Ei\u00b7nen", "Freund", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eingeladen sind sie all