 "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ganz allein, nicht nebenaus", "tokens": ["Ganz", "al\u00b7lein", ",", "nicht", "ne\u00b7be\u00b7naus"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "PTKNEG", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Auf die sch\u00f6nste schauen.", "tokens": ["Auf", "die", "sch\u00f6ns\u00b7te", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie erwiderten den Gru\u00df,", "tokens": ["Sie", "er\u00b7wi\u00b7der\u00b7ten", "den", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Haben's angenommen.", "tokens": ["Ha\u00b7ben's", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "H\u00e4nschen, geh und sieh dich um!", "tokens": ["H\u00e4n\u00b7schen", ",", "geh", "und", "sieh", "dich", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieh mir, ob sie kommen!", "tokens": ["Sieh", "mir", ",", "ob", "sie", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Dichter lud ich auch herbei,", "tokens": ["Dich\u00b7ter", "lud", "ich", "auch", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": 