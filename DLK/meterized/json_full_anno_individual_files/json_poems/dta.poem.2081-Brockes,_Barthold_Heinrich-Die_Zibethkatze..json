{"dta.poem.2081": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Zibethkatze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Thiere werden zwar bey uns nicht leichtlich le-", "tokens": ["Die", "Thie\u00b7re", "wer\u00b7den", "zwar", "bey", "uns", "nicht", "leicht\u00b7lich", "le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "PTKNEG", "ADJD", "TRUNC"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "bendig gefunden,", "tokens": ["ben\u00b7dig", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch wird was angenehm an ihnen von uns nicht weni-", "tokens": ["Doch", "wird", "was", "an\u00b7ge\u00b7nehm", "an", "ih\u00b7nen", "von", "uns", "nicht", "we\u00b7ni"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "ADJD", "APPR", "PPER", "APPR", "PPER", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "ger empfunden,", "tokens": ["ger", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Wenn man die B\u00e4lge zu uns bringt, nebst ihrem Aus-", "tokens": ["Wenn", "man", "die", "B\u00e4l\u00b7ge", "zu", "uns", "bringt", ",", "nebst", "ih\u00b7rem", "Aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wurf, dessen Duft", "tokens": ["wurf", ",", "des\u00b7sen", "Duft"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PRELAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Mit solcher s\u00fc\u00dfen Lieblichkeit und holden D\u00fcnsten in die", "tokens": ["Mit", "sol\u00b7cher", "s\u00fc\u00b7\u00dfen", "Lieb\u00b7lich\u00b7keit", "und", "hol\u00b7den", "D\u00fcns\u00b7ten", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-++", "measure": "unknown.measure.octa.plus"}, "line.8": {"text": "Luft,", "tokens": ["Luft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Die sie umgiebt, best\u00e4ndig quillet,", "tokens": ["Die", "sie", "um\u00b7giebt", ",", "be\u00b7st\u00e4n\u00b7dig", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df ein empfindliches Vergn\u00fcgen durch den Geruch das", "tokens": ["Da\u00df", "ein", "emp\u00b7find\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", "durch", "den", "Ge\u00b7ruch", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Hirn erf\u00fcllet", "tokens": ["Hirn", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Und uns recht inniglich vergn\u00fcgt. Wer von uns Men-", "tokens": ["Und", "uns", "recht", "in\u00b7nig\u00b7lich", "ver\u00b7gn\u00fcgt", ".", "Wer", "von", "uns", "Men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVPP", "$.", "PWS", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "schen kann begreifen,", "tokens": ["schen", "kann", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Auf welche Weise sich die Theilchen, die den Geruch ver-", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sich", "die", "Theil\u00b7chen", ",", "die", "den", "Ge\u00b7ruch", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PRF", "ART", "NN", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "gn\u00fcgen, h\u00e4ufen,", "tokens": ["gn\u00fc\u00b7gen", ",", "h\u00e4u\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Entstehen, und so lange dauren? Da Dinge, die bey", "tokens": ["Ent\u00b7ste\u00b7hen", ",", "und", "so", "lan\u00b7ge", "dau\u00b7ren", "?", "Da", "Din\u00b7ge", ",", "die", "bey"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "ADV", "VVINF", "$.", "KOUS", "NN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "ihnen liegen,", "tokens": ["ih\u00b7nen", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Von ihnen gleichsam eingebiesamt, so stark uns, wie sie", "tokens": ["Von", "ih\u00b7nen", "gleich\u00b7sam", "ein\u00b7ge\u00b7bie\u00b7samt", ",", "so", "stark", "uns", ",", "wie", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ADJD", "VVPP", "$,", "ADV", "ADJD", "PPER", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "selbst, vergn\u00fcgen,", "tokens": ["selbst", ",", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Ohn etwas ihnen zu benehmen. Die\u00df Thier ist grau", "tokens": ["Ohn", "et\u00b7was", "ih\u00b7nen", "zu", "be\u00b7neh\u00b7men", ".", "Die\u00df", "Thier", "ist", "grau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PPER", "PTKZU", "VVINF", "$.", "PDS", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "mit schwarzen Flecken.", "tokens": ["mit", "schwar\u00b7zen", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Am Bauch, in einem kleinen Beutel, soll der Zibeth be-", "tokens": ["Am", "Bauch", ",", "in", "ei\u00b7nem", "klei\u00b7nen", "Beu\u00b7tel", ",", "soll", "der", "Zi\u00b7beth", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VMFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "sonders stecken,", "tokens": ["son\u00b7ders", "ste\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Der gegen Schmerzen der Kolik sehr heilsam und besonders", "tokens": ["Der", "ge\u00b7gen", "Schmer\u00b7zen", "der", "Ko\u00b7lik", "sehr", "heil\u00b7sam", "und", "be\u00b7son\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ART", "NN", "ADV", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.25": {"text": "gut,", "tokens": ["gut", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Und gegen die Apoplexie nicht minder gro\u00dfe Wirkung thut.", "tokens": ["Und", "ge\u00b7gen", "die", "A\u00b7pop\u00b7le\u00b7xie", "nicht", "min\u00b7der", "gro\u00b7\u00dfe", "Wir\u00b7kung", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.27": {"text": "Da\u00df freylich auch in diesem Thier, wenn man so Nutz", "tokens": ["Da\u00df", "frey\u00b7lich", "auch", "in", "die\u00b7sem", "Thier", ",", "wenn", "man", "so", "Nutz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PDAT", "NN", "$,", "KOUS", "PIS", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "als Lust verbindet,", "tokens": ["als", "Lust", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Man einen wundernsw\u00fcrd\u2019gen Vorwurf Gott \u00fcberzeugt", "tokens": ["Man", "ei\u00b7nen", "wun\u00b7derns\u00b7w\u00fcrd'\u00b7gen", "Vor\u00b7wurf", "Gott", "\u00fc\u00b7berz\u00b7eugt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "NN", "VVPP"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "zu danken findet.", "tokens": ["zu", "dan\u00b7ken", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}