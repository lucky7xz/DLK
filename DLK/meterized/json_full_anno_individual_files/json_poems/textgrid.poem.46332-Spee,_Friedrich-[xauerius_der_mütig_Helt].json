{"textgrid.poem.46332": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[xauerius der m\u00fctig Helt]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Xauerius der m\u00fctig Helt,", "tokens": ["Xau\u00b7e\u00b7rius", "der", "m\u00fc\u00b7tig", "Helt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt eyffer dergestalten,", "tokens": ["Hatt", "eyf\u00b7fer", "der\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wan er gedacht der newen welt,", "tokens": ["Wan", "er", "ge\u00b7dacht", "der", "ne\u00b7wen", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+--+--++", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sein hertz wolt sich zerspalten,", "tokens": ["Sein", "hertz", "wolt", "sich", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd rieff dan laut gantz vnuerhelt,", "tokens": ["Vnd", "rieff", "dan", "laut", "gantz", "vnu\u00b7er\u00b7helt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Gott, kan mich nicht halten!", "tokens": ["O", "Gott", ",", "kan", "mich", "nicht", "hal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "H\u00f6r auff, h\u00f6r auff, felt mir zu schwer,", "tokens": ["H\u00f6r", "auff", ",", "h\u00f6r", "auff", ",", "felt", "mir", "zu", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gnad ist mir zu m\u00e4chtig.", "tokens": ["Die", "gnad", "ist", "mir", "zu", "m\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Seelen eyffer w\u00fctet sehr,", "tokens": ["Der", "See\u00b7len", "eyf\u00b7fer", "w\u00fc\u00b7tet", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd brennet mich so kr\u00e4fftig,", "tokens": ["Vnd", "bren\u00b7net", "mich", "so", "kr\u00e4ff\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df kaum vor hitz kan bleiben mehr:", "tokens": ["Da\u00df", "kaum", "vor", "hitz", "kan", "blei\u00b7ben", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VMFIN", "VVINF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Gott die brunst ist hefftig!", "tokens": ["O", "Gott", "die", "brunst", "ist", "heff\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drumb wird noch bleich- noch purper-tod", "tokens": ["Drumb", "wird", "noch", "bleich", "noch", "pur\u00b7per\u00b7tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur forcht bey mir erklecken:", "tokens": ["Zur", "forcht", "bey", "mir", "er\u00b7kle\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja wan mitt di\u00df- vnd iener noth,", "tokens": ["Ja", "wan", "mitt", "di\u00df", "vnd", "ie\u00b7ner", "noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "VVFIN", "TRUNC", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man d\u00e4chte mich zu schrecken", "tokens": ["Man", "d\u00e4ch\u00b7te", "mich", "zu", "schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da w\u00fcrd sich erst in wangen roth", "tokens": ["Da", "w\u00fcrd", "sich", "erst", "in", "wan\u00b7gen", "roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "All blutt vnd muth erwecken.", "tokens": ["All", "blutt", "vnd", "muth", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wan ich so gar auch vber Meer", "tokens": ["Wan", "ich", "so", "gar", "auch", "vber", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "APPR", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ein Seel w\u00fcst abzulangen;", "tokens": ["Ein", "Seel", "w\u00fcst", "ab\u00b7zu\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wolt gern durch lauter spie\u00df vnd speer,", "tokens": ["Wolt", "gern", "durch", "lau\u00b7ter", "spie\u00df", "vnd", "speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PIAT", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch pfeil vnd spitzig stangen", "tokens": ["Durch", "pfeil", "vnd", "spit\u00b7zig", "stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durchlauffen, wie der wilde Beer,", "tokens": ["Durch\u00b7lauf\u00b7fen", ",", "wie", "der", "wil\u00b7de", "Beer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df nur die Seel m\u00f6cht fangen.", "tokens": ["Da\u00df", "nur", "die", "Seel", "m\u00f6cht", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, ach, wie bringt mirs grosse pein,", "tokens": ["Ach", ",", "ach", ",", "wie", "bringt", "mirs", "gros\u00b7se", "pein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "PWAV", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie springt mir mein gebl\u00fcte,", "tokens": ["Wie", "springt", "mir", "mein", "ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nitt all heyden Christen sein!", "tokens": ["Da\u00df", "nitt", "all", "hey\u00b7den", "Chris\u00b7ten", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PIAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drumb Gott mich noch beh\u00fcte,", "tokens": ["Drumb", "Gott", "mich", "noch", "be\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df mich zur newen welt hinein,", "tokens": ["La\u00df", "mich", "zur", "ne\u00b7wen", "welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Darnach steht mein gem\u00fcthe.", "tokens": ["Dar\u00b7nach", "steht", "mein", "ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zu lang ist mir die zeit, vnd stund,", "tokens": ["Zu", "lang", "ist", "mir", "die", "zeit", ",", "vnd", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein hertz will mir zerbrechen:", "tokens": ["Mein", "hertz", "will", "mir", "zer\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Begird vnd eyffer mich verwund,", "tokens": ["Be\u00b7gird", "vnd", "eyf\u00b7fer", "mich", "ver\u00b7wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mitt warheit ich mach sprechen,", "tokens": ["Mitt", "war\u00b7heit", "ich", "mach", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Gott werd ich nicht bald gesund", "tokens": ["O", "Gott", "werd", "ich", "nicht", "bald", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So magst mich gar erstechen.", "tokens": ["So", "magst", "mich", "gar", "er\u00b7ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Lieb nim hin all ingeweid", "tokens": ["O", "Lieb", "nim", "hin", "all", "in\u00b7ge\u00b7weid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Au\u00df meinem leib zurstunde", "tokens": ["Au\u00df", "mei\u00b7nem", "leib", "zur\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Werffs vber Meer auff iene seit", "tokens": ["Werffs", "vber", "Meer", "auff", "ie\u00b7ne", "seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es dient zum newen funde:", "tokens": ["Es", "dient", "zum", "ne\u00b7wen", "fun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Mein hertz doch k\u00e4m in Jappon weit,", "tokens": ["Mein", "hertz", "doch", "k\u00e4m", "in", "Jap\u00b7pon", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wan ich schon gieng zu grunde.", "tokens": ["Wan", "ich", "schon", "gieng", "zu", "grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Xauerius der m\u00fctig Helt,", "tokens": ["Xau\u00b7e\u00b7rius", "der", "m\u00fc\u00b7tig", "Helt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt eyffer dergestalten,", "tokens": ["Hatt", "eyf\u00b7fer", "der\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text