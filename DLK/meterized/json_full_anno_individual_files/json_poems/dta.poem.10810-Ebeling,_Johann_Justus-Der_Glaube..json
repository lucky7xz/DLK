{"dta.poem.10810": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Glaube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Augen helles Glas ist finster ohne\nLicht;", "tokens": ["Der", "Au\u00b7gen", "hel\u00b7les", "Glas", "ist", "fins\u00b7ter", "oh\u00b7ne", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So siehet der Verstand ohn das Erkennt-", "tokens": ["So", "sie\u00b7het", "der", "Ver\u00b7stand", "ohn", "das", "Er\u00b7kennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das uns theils die Vernunfft, theils", "tokens": ["Das", "uns", "theils", "die", "Ver\u00b7nunfft", ",", "theils"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "PPER", "ADV", "ART", "NN", "$,", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenn man die Warheit sucht, und ihre Gr\u00fcnde", "tokens": ["Wenn", "man", "die", "War\u00b7heit", "sucht", ",", "und", "ih\u00b7re", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer einem Zeugnis traut, dem keine Warheit", "tokens": ["Wer", "ei\u00b7nem", "Zeug\u00b7nis", "traut", ",", "dem", "kei\u00b7ne", "War\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dadurch den Zweiffel hebt, der uns mit Unruh", "tokens": ["Da\u00b7durch", "den", "Zweif\u00b7fel", "hebt", ",", "der", "uns", "mit", "Un\u00b7ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Der hat den Glauben fest auf einem Fels gebauet,", "tokens": ["Der", "hat", "den", "Glau\u00b7ben", "fest", "auf", "ei\u00b7nem", "Fels", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der unersch\u00fcttert bleibt, wenn gleich ein Sturm", "tokens": ["Der", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "bleibt", ",", "wenn", "gleich", "ein", "Sturm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der die gesch\u00e4umte Flut daran mit Toben schl\u00e4gt;", "tokens": ["Der", "die", "ge\u00b7sch\u00e4um\u00b7te", "Flut", "da\u00b7ran", "mit", "To\u00b7ben", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die aber alsobald mu\u00df wieder r\u00fckwerts springen,", "tokens": ["Die", "a\u00b7ber", "al\u00b7so\u00b7bald", "mu\u00df", "wie\u00b7der", "r\u00fck\u00b7werts", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn sie mit Sprudeln wil den steilen Fels ver-", "tokens": ["Wenn", "sie", "mit", "Spru\u00b7deln", "wil", "den", "stei\u00b7len", "Fels", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So unverlezzet bleibt ein Mensch der GOtt ver-", "tokens": ["So", "un\u00b7ver\u00b7lez\u00b7zet", "bleibt", "ein", "Mensch", "der", "Gott", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Er hat sein sichres Wohl auf einem Fels gebaut.", "tokens": ["Er", "hat", "sein", "sich\u00b7res", "Wohl", "auf", "ei\u00b7nem", "Fels", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Glaube nur allein kan unsern ", "tokens": ["Der", "Glau\u00b7be", "nur", "al\u00b7lein", "kan", "un\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So lange wir allhie als Erden Pilgrim wallen.", "tokens": ["So", "lan\u00b7ge", "wir", "all\u00b7hie", "als", "Er\u00b7den", "Pil\u00b7grim", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Daraus erhellet schon da\u00df der gl\u00fckselig sey,", "tokens": ["Da\u00b7raus", "er\u00b7hel\u00b7let", "schon", "da\u00df", "der", "gl\u00fck\u00b7se\u00b7lig", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "KOUS", "ART", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Wer sich ins Herze dr\u00fckt, da\u00df unser ", "tokens": ["Wer", "sich", "ins", "Her\u00b7ze", "dr\u00fckt", ",", "da\u00df", "un\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und da\u00df sein Wort gewis; und wer sich wird be-", "tokens": ["Und", "da\u00df", "sein", "Wort", "ge\u00b7wis", ";", "und", "wer", "sich", "wird", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NE", "$.", "KON", "PWS", "PRF", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Was er uns hat gesagt, als Warheit anzuneh-", "tokens": ["Was", "er", "uns", "hat", "ge\u00b7sagt", ",", "als", "War\u00b7heit", "an\u00b7zu\u00b7neh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,", "KOUS", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie gl\u00fcklich war der Mensch in seinem Para-", "tokens": ["Wie", "gl\u00fck\u00b7lich", "war", "der", "Mensch", "in", "sei\u00b7nem", "Pa\u00b7ra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "So lang er das annahm, was ihn sein ", "tokens": ["So", "lang", "er", "das", "an\u00b7nahm", ",", "was", "ihn", "sein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PDS", "VVFIN", "$,", "PRELS", "PPER", "PPOSAT"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "So bald der Glaube war aus seiner Seel verlohren;", "tokens": ["So", "bald", "der", "Glau\u00b7be", "war", "aus", "sei\u00b7ner", "Seel", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So bald war Unruh, Schmerz in seinem Geist", "tokens": ["So", "bald", "war", "Un\u00b7ruh", ",", "Schmerz", "in", "sei\u00b7nem", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "NN", "$,", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Er ward ein Ungl\u00fckskind, er rie\u00df das Band ent-", "tokens": ["Er", "ward", "ein", "Un\u00b7gl\u00fcks\u00b7kind", ",", "er", "rie\u00df", "das", "Band", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Das ihn mit ", "tokens": ["Das", "ihn", "mit"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Der ihn das Gl\u00fcklichseyn so fest ins Herz gepr\u00e4get,", "tokens": ["Der", "ihn", "das", "Gl\u00fck\u00b7lich\u00b7seyn", "so", "fest", "ins", "Herz", "ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Mittel zu dem Woll, so deutlich vorgeleget.", "tokens": ["Die", "Mit\u00b7tel", "zu", "dem", "Woll", ",", "so", "deut\u00b7lich", "vor\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jhr die ihr dies erkennt, das H\u00f6chste Gut verehrt,", "tokens": ["Ihr", "die", "ihr", "dies", "er\u00b7kennt", ",", "das", "H\u00f6chs\u00b7te", "Gut", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Jhr sprecht von Glauben oft, da euch ein Wahn", "tokens": ["Ihr", "sprecht", "von", "Glau\u00b7ben", "oft", ",", "da", "euch", "ein", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Es ist ein Unterscheid den Allerh\u00f6chsten kennen,", "tokens": ["Es", "ist", "ein", "Un\u00b7ter\u00b7scheid", "den", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und ihn mit Zuversicht auch seinen Vater nennen.", "tokens": ["Und", "ihn", "mit", "Zu\u00b7ver\u00b7sicht", "auch", "sei\u00b7nen", "Va\u00b7ter", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der eine saget oft: Ich glaube da\u00df ein ", "tokens": ["Der", "ei\u00b7ne", "sa\u00b7get", "oft", ":", "Ich", "glau\u00b7be", "da\u00df", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "KOUS", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.34": {"text": "Er lacht die Thoren aus, die sich mit Schimpf und", "tokens": ["Er", "lacht", "die", "Tho\u00b7ren", "aus", ",", "die", "sich", "mit", "Schimpf", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Mit blinder Unvernunfft an ihren Sch\u00f6pfer wagen,", "tokens": ["Mit", "blin\u00b7der", "Un\u00b7ver\u00b7nunfft", "an", "ih\u00b7ren", "Sch\u00f6p\u00b7fer", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und ihr Gewissen nur mit falschen L\u00fcgen plagen.", "tokens": ["Und", "ihr", "Ge\u00b7wis\u00b7sen", "nur", "mit", "fal\u00b7schen", "L\u00fc\u00b7gen", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der andre br\u00fcstet sich mit seiner Wissenschafft", "tokens": ["Der", "and\u00b7re", "br\u00fcs\u00b7tet", "sich", "mit", "sei\u00b7ner", "Wis\u00b7sen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und meint das Wissen sey, das im Ged\u00e4chtnis hafft,", "tokens": ["Und", "meint", "das", "Wis\u00b7sen", "sey", ",", "das", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "hafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VAFIN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein sichrer Grund vor ihm, da\u00df er dem Glauben habe", "tokens": ["Ein", "sich\u00b7rer", "Grund", "vor", "ihm", ",", "da\u00df", "er", "dem", "Glau\u00b7ben", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der doch des Geistes Gut, des Himmels Gnaden-", "tokens": ["Der", "doch", "des", "Geis\u00b7tes", "Gut", ",", "des", "Him\u00b7mels", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Das ist noch lange nicht, was man den Glauben", "tokens": ["Das", "ist", "noch", "lan\u00b7ge", "nicht", ",", "was", "man", "den", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PTKNEG", "$,", "PRELS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Der als ein starkes Feur in denen Herzen brennt,", "tokens": ["Der", "als", "ein", "star\u00b7kes", "Feur", "in", "de\u00b7nen", "Her\u00b7zen", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "ADJA", "NN", "APPR", "PRELS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der gl\u00e4ubt wer im Gem\u00fct der Warheit Lehren he-", "tokens": ["Der", "gl\u00e4ubt", "wer", "im", "Ge\u00b7m\u00fct", "der", "War\u00b7heit", "Leh\u00b7ren", "he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PWS", "APPRART", "NN", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die Gr\u00fcnde deutlich sieht, wodurch das Herz be-", "tokens": ["Die", "Gr\u00fcn\u00b7de", "deut\u00b7lich", "sieht", ",", "wo\u00b7durch", "das", "Herz", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "PWAV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Dem Beifal nicht versagt, dem was er in der", "tokens": ["Dem", "Bei\u00b7fal", "nicht", "ver\u00b7sagt", ",", "dem", "was", "er", "in", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "PRELS", "PWS", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "li