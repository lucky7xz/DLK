{"textgrid.poem.52971": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon gl\u00e4nzt dort hoch der Abendstern;", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon gl\u00e4nzt dort hoch der Abendstern;", "tokens": ["Schon", "gl\u00e4nzt", "dort", "hoch", "der", "A\u00b7bends\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lob' ihn, mein Geist, lob' ihn, den Herrn!", "tokens": ["Lob'", "ihn", ",", "mein", "Geist", ",", "lob'", "ihn", ",", "den", "Herrn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sank der Sonne goldnes Licht,", "tokens": ["Es", "sank", "der", "Son\u00b7ne", "gold\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch seine G\u00fcte sinket nicht.", "tokens": ["Doch", "sei\u00b7ne", "G\u00fc\u00b7te", "sin\u00b7ket", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hat von meiner Jugend auf", "tokens": ["Er", "hat", "von", "mei\u00b7ner", "Ju\u00b7gend", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geleitet meines Lebens Lauf;", "tokens": ["Ge\u00b7lei\u00b7tet", "mei\u00b7nes", "Le\u00b7bens", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er stand mir bey, wenn von Gefahr", "tokens": ["Er", "stand", "mir", "bey", ",", "wenn", "von", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich rund umher umgeben war.", "tokens": ["Ich", "rund", "um\u00b7her", "um\u00b7ge\u00b7ben", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er war mein Trost, wenn Kummer sich", "tokens": ["Er", "war", "mein", "Trost", ",", "wenn", "Kum\u00b7mer", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um mein bethr\u00e4ntes Lager schlich;", "tokens": ["Um", "mein", "be\u00b7thr\u00e4n\u00b7tes", "La\u00b7ger", "schlich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00f6rte, wenn ich schwer und tief", "tokens": ["Er", "h\u00f6r\u00b7te", ",", "wenn", "ich", "schwer", "und", "tief"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meiner Angst um Rettung rief.", "tokens": ["Aus", "mei\u00b7ner", "Angst", "um", "Ret\u00b7tung", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun sing' ich noch mit jeder Nacht,", "tokens": ["Nun", "sing'", "ich", "noch", "mit", "je\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herr hat alles wohl gemacht:", "tokens": ["Der", "Herr", "hat", "al\u00b7les", "wohl", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schickt uns nur zu unsrer Ruh", "tokens": ["Er", "schickt", "uns", "nur", "zu", "uns\u00b7rer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den bittern Kelch der Leiden zu.", "tokens": ["Den", "bit\u00b7tern", "Kelch", "der", "Lei\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich habe lang' und viel gelebt,", "tokens": ["Ich", "ha\u00b7be", "lang'", "und", "viel", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und manche tr\u00fcbe Stunde schwebt", "tokens": ["Und", "man\u00b7che", "tr\u00fc\u00b7be", "Stun\u00b7de", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einsam jetzt vor meinem Blick;", "tokens": ["Noch", "ein\u00b7sam", "jetzt", "vor", "mei\u00b7nem", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch dankbar denk' ich nur zur\u00fcck.", "tokens": ["Doch", "dank\u00b7bar", "denk'", "ich", "nur", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott, sey mein Vater; steh mir bey,", "tokens": ["Gott", ",", "sey", "mein", "Va\u00b7ter", ";", "steh", "mir", "bey", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich des Lebens Abend frey,", "tokens": ["Da\u00df", "ich", "des", "Le\u00b7bens", "A\u00b7bend", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ich ihn nunmehr vor mir seh,", "tokens": ["Wie", "ich", "ihn", "nun\u00b7mehr", "vor", "mir", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Still, sanft und froh hinunter geh.", "tokens": ["Still", ",", "sanft", "und", "froh", "hin\u00b7un\u00b7ter", "geh."], "token_info": ["word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df fromm mich und von Tadel rein", "tokens": ["La\u00df", "fromm", "mich", "und", "von", "Ta\u00b7del", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPER", "KON", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dir und vor den Menschen seyn,", "tokens": ["Vor", "dir", "und", "vor", "den", "Men\u00b7schen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df man, wenn mein Gebein einst ruht,", "tokens": ["Da\u00df", "man", ",", "wenn", "mein", "Ge\u00b7bein", "einst", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch herzlich sage, er war gut.", "tokens": ["Noch", "herz\u00b7lich", "sa\u00b7ge", ",", "er", "war", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df meine Kinder, meiner werth,", "tokens": ["La\u00df", "mei\u00b7ne", "Kin\u00b7der", ",", "mei\u00b7ner", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur bleiben, wie ich sie gelehrt,", "tokens": ["Nur", "blei\u00b7ben", ",", "wie", "ich", "sie", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich deiner und der Tugend freun;", "tokens": ["Sich", "dei\u00b7ner", "und", "der", "Tu\u00b7gend", "freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist ihr Erbtheil nicht mehr klein.", "tokens": ["So", "ist", "ihr", "E\u00b7rbtheil", "nicht", "mehr", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So wall' ich ruhig, wie ich bin,", "tokens": ["So", "wall'", "ich", "ru\u00b7hig", ",", "wie", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum stillen gro\u00dfen Schlafe hin,", "tokens": ["Zum", "stil\u00b7len", "gro\u00b7\u00dfen", "Schla\u00b7fe", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo schlummerschwer mein Auge sinkt,", "tokens": ["Wo", "schlum\u00b7mer\u00b7schwer", "mein", "Au\u00b7ge", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn mir der Tod, dein Bothe, winkt.",