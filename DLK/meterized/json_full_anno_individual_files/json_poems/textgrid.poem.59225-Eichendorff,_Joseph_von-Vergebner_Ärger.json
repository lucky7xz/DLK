{"textgrid.poem.59225": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vergebner \u00c4rger", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im alten Hause steh ich in Gedanken;", "tokens": ["Im", "al\u00b7ten", "Hau\u00b7se", "steh", "ich", "in", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist das Haus nicht mehr, der Wind mit Schauern", "tokens": ["Es", "ist", "das", "Haus", "nicht", "mehr", ",", "der", "Wind", "mit", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geht durch das Gras im Hof, und Eulen lauern", "tokens": ["Geht", "durch", "das", "Gras", "im", "Hof", ",", "und", "Eu\u00b7len", "lau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In leeren Fenstern, die schon halb versanken.", "tokens": ["In", "lee\u00b7ren", "Fens\u00b7tern", ",", "die", "schon", "halb", "ver\u00b7san\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mich \u00e4rgern nur die jungen, kecken Ranken,", "tokens": ["Mich", "\u00e4r\u00b7gern", "nur", "die", "jun\u00b7gen", ",", "ke\u00b7cken", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wie zum Spott noch schm\u00fccken Tor und Mauern,", "tokens": ["Die", "wie", "zum", "Spott", "noch", "schm\u00fc\u00b7cken", "Tor", "und", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "APPRART", "NN", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die gr\u00fcnen Birken, die mit falschem Trauern", "tokens": ["Die", "gr\u00fc\u00b7nen", "Bir\u00b7ken", ",", "die", "mit", "fal\u00b7schem", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leicht \u00fcberm Grabe meiner Lieben schwanken.", "tokens": ["Leicht", "\u00fc\u00b7berm", "Gra\u00b7be", "mei\u00b7ner", "Lie\u00b7ben", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So, Nachteul selber, auf dem \u00f6den Gipfel", "tokens": ["So", ",", "Nach\u00b7teul", "sel\u00b7ber", ",", "auf", "dem", "\u00f6\u00b7den", "Gip\u00b7fel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sa\u00df ich in meines Jugendgl\u00fccks Ruinen,", "tokens": ["Sa\u00df", "ich", "in", "mei\u00b7nes", "Ju\