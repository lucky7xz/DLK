{"textgrid.poem.46857": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie ich den Brief gesiegelt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich den Brief gesiegelt,", "tokens": ["Wie", "ich", "den", "Brief", "ge\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und nun ausblase das Licht,", "tokens": ["Und", "nun", "aus\u00b7bla\u00b7se", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und es lischt ohne Rauch,", "tokens": ["Und", "es", "lischt", "oh\u00b7ne", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Seh' ich dich wieder gespiegelt", "tokens": ["Seh'", "ich", "dich", "wie\u00b7der", "ge\u00b7spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Vor meinem Angesicht,", "tokens": ["Vor", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "O die ich vergesse nicht!", "tokens": ["O", "die", "ich", "ver\u00b7ges\u00b7se", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRELS", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So hat ein h\u00f6herer Hauch,", "tokens": ["So", "hat", "ein", "h\u00f6\u00b7he\u00b7rer", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Der in dir sich verk\u00fcndet,", "tokens": ["Der", "in", "dir", "sich", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Dich ausgel\u00f6scht nun auch,", "tokens": ["Dich", "aus\u00b7ge\u00b7l\u00f6scht", "nun", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "O meiner Freuden Licht,", "tokens": ["O", "mei\u00b7ner", "Freu\u00b7den", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und mir ist's unergr\u00fcndet,", "tokens": ["Und", "mir", "ist's", "un\u00b7er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wo zu neuem Gebrauch", "tokens": ["Wo", "zu", "neu\u00b7em", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Er wieder dich anz\u00fcndet.", "tokens": ["Er", "wie\u00b7der", "dich", "an\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ich den Brief gesiegelt,", "tokens": ["Wie", "ich", "den"