{"textgrid.poem.26207": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Bald ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebste, bald unterm Giebel, der viel Sterne n\u00e4chtlich tr\u00e4gt,", "tokens": ["Liebs\u00b7te", ",", "bald", "un\u00b7term", "Gie\u00b7bel", ",", "der", "viel", "Ster\u00b7ne", "n\u00e4cht\u00b7lich", "tr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPRART", "NN", "$,", "PRELS", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Hast, Geliebte, du, deine Br\u00fcste dicht an meine Brust gelegt,", "tokens": ["Hast", ",", "Ge\u00b7lieb\u00b7te", ",", "du", ",", "dei\u00b7ne", "Br\u00fcs\u00b7te", "dicht", "an", "mei\u00b7ne", "Brust", "ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Und der Mond steigt auf mit Lust \u00fcber \u00c4cker hin und Hald', er der still Begr\u00fc\u00dfte. \u2013", "tokens": ["Und", "der", "Mond", "steigt", "auf", "mit", "Lust", "\u00fc\u00b7ber", "\u00c4\u00b7cker", "hin", "und", "Hald'", ",", "er", "der", "still", "Be\u00b7gr\u00fc\u00df\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "APPR", "NN", "APPR", "NN", "PTKVZ", "KON", "NN", "$,", "PPER", "ART", "ADJA", 