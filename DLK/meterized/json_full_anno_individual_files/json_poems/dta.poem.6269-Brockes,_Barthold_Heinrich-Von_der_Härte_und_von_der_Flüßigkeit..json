{"dta.poem.6269": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat jedoch an Krafft sonst seines gleichen nicht.", "tokens": ["Es", "hat", "je\u00b7doch", "an", "Krafft", "sonst", "sei\u00b7nes", "glei\u00b7chen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "ADV", "PPOSAT", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch seine H\u00fclffe wird die Arbeit leicht gemacht.", "tokens": ["Durch", "sei\u00b7ne", "H\u00fclf\u00b7fe", "wird", "die", "Ar\u00b7beit", "leicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn durch das Werck, das uns Vulcanus zugericht,", "tokens": ["Denn", "durch", "das", "Werck", ",", "das", "uns", "Vul\u00b7ca\u00b7nus", "zu\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird von dem Handwercks-Man\u0303, was er nur will, vollbracht.", "tokens": ["Wird", "von", "dem", "Handwercks\u00b7Ma\u00f1", ",", "was", "er", "nur", "will", ",", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART