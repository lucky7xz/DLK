 "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sch\u00f6n war sie wie die S\u00fcnde.", "tokens": ["Sch\u00f6n", "war", "sie", "wie", "die", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Haupt war blo\u00df und nackt ihr Fu\u00df,", "tokens": ["Ihr", "Haupt", "war", "blo\u00df", "und", "nackt", "ihr", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Haar hing wild hernieder,", "tokens": ["Ihr", "Haar", "hing", "wild", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frechschelmisch klang ihr Bettelgru\u00df", "tokens": ["Frech\u00b7schel\u00b7misch", "klang", "ihr", "Bet\u00b7tel\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lumpig war das Mieder,", "tokens": ["Und", "lum\u00b7pig", "war", "das", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der d\u00fcnne Rock ging kaum zum Knie,", "tokens": ["Der", "d\u00fcn\u00b7ne", "Rock", "ging", "kaum", "zum", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sch