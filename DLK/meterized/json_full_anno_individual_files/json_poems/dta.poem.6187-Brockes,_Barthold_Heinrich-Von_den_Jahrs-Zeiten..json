{"dta.poem.6187": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, die im Osten aufzugehn,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "im", "Os\u00b7ten", "auf\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wieder Westwerts, wie man meinet,", "tokens": ["Und", "wie\u00b7der", "West\u00b7werts", ",", "wie", "man", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Meer zu sincken scheinet;", "tokens": ["Ins", "Meer", "zu", "sin\u00b7cken", "schei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und die wir wiederum des Morgens sehn", "tokens": ["Und", "die", "wir", "wie\u00b7de\u00b7rum", "des", "Mor\u00b7gens", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "ART", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den krummen Weg aufs neu durchrennen,", "tokens": ["Den", "krum\u00b7men", "Weg", "aufs", "neu", "durch\u00b7ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ob schon sein Flammen-reicher Creis", "tokens": ["Ob", "schon", "sein", "Flam\u00b7men\u00b7rei\u00b7cher", "Creis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jm Mittelpunet der Welt nichts von Bewegung wei\u00df;", "tokens": ["Jm", "Mit\u00b7tel\u00b7pu\u00b7net", "der", "Welt", "nichts", "von", "Be\u00b7we\u00b7gung", "wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Zeigt unver\u00e4nderlich in seinem Schein-Lauff an", "tokens": ["Zeigt", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "in", "sei\u00b7nem", "Schein\u00b7Lauff", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Wodurch verschiedentlich die Felder dann", "tokens": ["Wo\u00b7durch", "ver\u00b7schie\u00b7dent\u00b7lich", "die", "Fel\u00b7der", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Jm holden Gr\u00fcnen bald, und bald im Eise gl\u00e4ntzen.", "tokens": ["Jm", "hol\u00b7den", "Gr\u00fc\u00b7nen", "bald", ",", "und", "bald", "im", "Ei\u00b7se", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}