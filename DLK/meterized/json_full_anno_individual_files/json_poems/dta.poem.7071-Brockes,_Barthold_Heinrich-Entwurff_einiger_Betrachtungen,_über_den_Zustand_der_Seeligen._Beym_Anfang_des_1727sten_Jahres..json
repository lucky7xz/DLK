{"dta.poem.7071": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was vor ein Jubel-Ton wird nicht sodann erklingen!", "tokens": ["Was", "vor", "ein", "Ju\u00b7bel\u00b7Ton", "wird", "nicht", "so\u00b7dann", "er\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie werden sie zum Ruhm des Grossen Sch\u00f6pffers singen:", "tokens": ["Wie", "wer\u00b7den", "sie", "zum", "Ruhm", "des", "Gros\u00b7sen", "Sch\u00f6pf\u00b7fers", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O welche Tieffen! welche H\u00f6hen!", "tokens": ["O", "wel\u00b7che", "Tief\u00b7fen", "!", "wel\u00b7che", "H\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Reichthums Deiner Wunder-G\u00fcte,", "tokens": ["Des", "Reicht\u00b7hums", "Dei\u00b7ner", "Wun\u00b7der\u00b7G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die wir mit br\u00fcnstigem Gem\u00fcthe", "tokens": ["Die", "wir", "mit", "br\u00fcns\u00b7ti\u00b7gem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ewig seel\u2019ger Klarheit sehen!", "tokens": ["In", "e\u00b7wig", "seel'\u00b7ger", "Klar\u00b7heit", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie hat Dein V\u00e4terlichs Erbarmen,", "tokens": ["Wie", "hat", "Dein", "V\u00e4\u00b7ter\u00b7lichs", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn wir gar offt in S\u00fcnden-Strudel kommen,", "tokens": ["Wenn", "wir", "gar", "offt", "in", "S\u00fcn\u00b7den\u00b7Stru\u00b7del", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auf Erden dort sich unser Armen", "tokens": ["Auf", "Er\u00b7den", "dort", "sich", "un\u00b7ser", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So huld-und liebreich angenommen!", "tokens": ["So", "huld\u00b7\u00b7und", "lieb\u00b7reich", "an\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie unbeschreiblich sind die Triebe,", "tokens": ["Wie", "un\u00b7be\u00b7schreib\u00b7lich", "sind", "die", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "O Ew\u2019ge GOttheit, Deiner Liebe!", "tokens": ["O", "Ew'\u00b7ge", "Got\u00b7theit", ",", "Dei\u00b7ner", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Du, damit wir ewig m\u00f6chten leben,", "tokens": ["Der", "Du", ",", "da\u00b7mit", "wir", "e\u00b7wig", "m\u00f6ch\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So gar den Grossen SOHN f\u00fcr uns dahin gegeben,", "tokens": ["So", "gar", "den", "Gros\u00b7sen", "SoHN", "f\u00fcr", "uns", "da\u00b7hin", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So, da\u00df wir nun, da sich die Noth geendet,", "tokens": ["So", ",", "da\u00df", "wir", "nun", ",", "da", "sich", "die", "Noth", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "$,", "KOUS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Jm Port des Friedens angel\u00e4ndet,", "tokens": ["Jm", "Port", "des", "Frie\u00b7dens", "an\u00b7ge\u00b7l\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wo wir in stoltzer Ruh und ungest\u00f6rten Freuden", "tokens": ["Wo", "wir", "in", "stolt\u00b7zer", "Ruh", "und", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "An Deinem Seel\u2019gen Licht uns ewig, ewig, weiden.", "tokens": ["An", "Dei\u00b7nem", "Seel'\u00b7gen", "Licht", "uns", "e\u00b7wig", ",", "e\u00b7wig", ",", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "ADJD", "$,", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Amen!", "tokens": ["A\u00b7men", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Lob, Weisheit, Ehr und Danck und Preis und Krafft", "tokens": ["Lob", ",", "Weis\u00b7heit", ",", "Ehr", "und", "Danck", "und", "Preis", "und", "Krafft"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "und St\u00e4rcke", "tokens": ["und", "St\u00e4r\u00b7cke"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Sey Dir, o ewigs ALL, Quell der Vollkommenheit,", "tokens": ["Sey", "Dir", ",", "o", "e\u00b7wigs", "AlL", ",", "Quell", "der", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "FM", "FM", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Und Deinem wunderbaren Nahmen,", "tokens": ["Und", "Dei\u00b7nem", "wun\u00b7der\u00b7ba\u00b7ren", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Von Ewigkeit zu Ewigkeit,", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Amen!", "tokens": ["A\u00b7men", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}