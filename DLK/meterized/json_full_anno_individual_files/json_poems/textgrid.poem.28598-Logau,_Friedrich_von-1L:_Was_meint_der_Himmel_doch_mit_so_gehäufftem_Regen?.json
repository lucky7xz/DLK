{"textgrid.poem.28598": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was meint der Himmel doch mit so geh\u00e4ufftem Regen?", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was meint der Himmel doch mit so geh\u00e4ufftem Regen?", "tokens": ["Was", "meint", "der", "Him\u00b7mel", "doch", "mit", "so", "ge\u00b7h\u00e4uff\u00b7tem", "Re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wil von de\u00df Krieges Schmutz befleckte Welt er fegen?", "tokens": ["Wil", "von", "de\u00df", "Krie\u00b7ges", "Schmutz", "be\u00b7fleck\u00b7te", "Welt", "er", "fe\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "VVFIN", "NN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Bedeut es wol hinf\u00fcr viel Heil und reichen Segen?", "tokens": ["Be\u00b7deut", "es", "wol", "hin\u00b7f\u00fcr", "viel", "Heil", "und", "rei\u00b7chen", "Se\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich d\u00fcnckt, er traure so und giesse milde Zehren", "tokens": ["Mich", "d\u00fcnckt", ",", "er", "trau\u00b7re", "so", "und", "gies\u00b7se", "mil\u00b7de", "Zeh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER