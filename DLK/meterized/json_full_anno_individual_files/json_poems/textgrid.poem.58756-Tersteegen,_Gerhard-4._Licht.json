{"textgrid.poem.58756": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "4. Licht", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Licht, du Licht- und Lebensbrunn,", "tokens": ["O", "Licht", ",", "du", "Licht", "und", "Le\u00b7bens\u00b7brunn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freundlichkeit, der Freuden Sonn'!", "tokens": ["Der", "Freund\u00b7lich\u00b7keit", ",", "der", "Freu\u00b7den", "Sonn'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man schaut's verh\u00fcllt, geb\u00fcckt man's ehret.", "tokens": ["Man", "schaut's", "ver\u00b7h\u00fcllt", ",", "ge\u00b7b\u00fcckt", "man's", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVPP", "$,", "VVFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Jesus strahlt dein Widerschein", "tokens": ["Von", "Je\u00b7sus", "strahlt", "dein", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lieblichsanft ins Herz hinein;", "tokens": ["So", "lieb\u00b7lichs\u00b7anft", "ins", "Herz", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bleib' ich nun stille zugekehret,", "tokens": ["Bleib'", "ich", "nun", "stil\u00b7le", "zu\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So macht dies Licht dem Licht mich gleich", "tokens": ["So", "macht", "dies", "Licht", "dem", "Licht", "mich", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "NN", "ART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und gibt des Lichtes Freudenreich,", "tokens": ["Und", "gibt", "des", "Lich\u00b7tes", "Freu\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "O Licht, du Licht- und Lebensbrunn,", "tokens": ["O", "Licht", ",", "du", "Licht", "und", "Le\u00b7bens\u00b7brunn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freundlichkeit, der Freuden Sonn'!", "tokens": ["Der", "Freund\u00b7lich\u00b7keit", ",", "der", "Freu\u00b7den", "Sonn'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man schaut's verh\u00fcllt, geb\u00fcckt man's ehret.", "tokens": ["Man", "schaut's", "ver\u00b7h\u00fcllt", ",", "ge\u00b7b\u00fcck