{"dta.poem.955": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn tr&#252;be mahnung noch einmal uns peinigt               ", "tokens": ["Wenn", "tr", "&#252;", "be", "mah\u00b7nung", "noch", "ein\u00b7mal", "uns", "pei\u00b7nigt"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "$(", "ADJA", "NN", "ADV", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schreck in unsre goldnen lande streut               ", "tokens": ["Und", "schreck", "in", "uns\u00b7re", "gold\u00b7nen", "lan\u00b7de", "streut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du sprichst in zuversicht: mit mir vereinigt               ", "tokens": ["Du", "sprichst", "in", "zu\u00b7ver\u00b7sicht", ":", "mit", "mir", "ver\u00b7ei\u00b7nigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$.", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bef&#252;rchte nicht was fl&#252;chtig sich erneut             ", "tokens": ["Bef", "&#252;", "rch\u00b7te", "nicht", "was", "fl", "&#252;", "ch\u00b7tig", "sich", "er\u00b7neut"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "PTKNEG", "PWS", "NE", "$(", "ADJD", "PRF", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Nur dass du meinem schutz dich nicht entfernst               ", "tokens": ["Nur", "dass", "du", "mei\u00b7nem", "schutz", "dich", "nicht", "ent\u00b7fernst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bevor das scharfe licht ersterbend loht               ", "tokens": ["Be\u00b7vor", "das", "schar\u00b7fe", "licht", "ers\u00b7ter\u00b7bend", "loht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und dir der gartenwald vers\u00f6hnlich ernst               ", "tokens": ["Und", "dir", "der", "gar\u00b7ten\u00b7wald", "ver\u00b7s\u00f6hn\u00b7lich", "ernst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinen schatten wieder abend bot.             ", "tokens": ["Mit", "sei\u00b7nen", "schat\u00b7ten", "wie\u00b7der", "a\u00b7bend", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}