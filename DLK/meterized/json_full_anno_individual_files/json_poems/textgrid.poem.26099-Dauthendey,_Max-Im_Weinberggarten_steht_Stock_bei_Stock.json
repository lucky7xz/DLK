{"textgrid.poem.26099": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Im Weinberggarten steht Stock bei Stock", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Weinberggarten steht Stock bei Stock,", "tokens": ["Im", "Wein\u00b7berg\u00b7gar\u00b7ten", "steht", "Stock", "bei", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um jeden Pflock eine Rebe gewunden,", "tokens": ["Um", "je\u00b7den", "Pflock", "ei\u00b7ne", "Re\u00b7be", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit Strohfaden aufrecht angebunden.", "tokens": ["Mit", "Stroh\u00b7fa\u00b7den", "auf\u00b7recht", "an\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Noch ist kein Blatt an den braunen Ranken,", "tokens": ["Noch", "ist", "kein", "Blatt", "an", "den", "brau\u00b7nen", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie stehen noch alle wie d\u00fcrr in Gedanken,", "tokens": ["Sie", "ste\u00b7hen", "noch", "al\u00b7le", "wie", "d\u00fcrr", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "p