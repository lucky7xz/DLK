{"textgrid.poem.58596": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein arm', elender Geist w\u00e4r' bald vor Durst vergangen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein arm', elender Geist w\u00e4r' bald vor Durst vergangen,", "tokens": ["Mein", "arm'", ",", "e\u00b7len\u00b7der", "Geist", "w\u00e4r'", "bald", "vor", "Durst", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ich suchte au\u00dfer mir das Wasser hier und dar", "tokens": ["Ich", "such\u00b7te", "au\u00b7\u00dfer", "mir", "das", "Was\u00b7ser", "hier", "und", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und fand doch gar nichts da, was stillet mein Verlangen;", "tokens": ["Und", "fand", "doch", "gar", "nichts", "da", ",", "was", "stil\u00b7let", "mein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PIS", "ADV", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies eine fand ich, da\u00df ich arm und elend war.", "tokens": ["Dies", "ei\u00b7ne", "fand", "ich", ",", "da\u00df", "ich", "arm", "und", "e\u00b7lend", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun will ich gr\u00fcndlich mich von allem einw\u00e4rts kehren", "tokens": ["Nun", "will", "ich", "gr\u00fcnd\u00b7lich", "mich", "von", "al\u00b7lem", "ein\u00b7w\u00e4rts", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PRF", "APPR", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und suchen nur in Gott, was meinem Geist gebricht.", "tokens": ["Und", "su\u00b7chen", "nur", "in", "Gott", ",", "was", "mei\u00b7nem", "Geist", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Herr, sprich denn auch zu mir: \u00bbIch, ich will dich erh\u00f6ren,", "tokens": ["Herr", ",", "sprich", "denn", "auch", "zu", "mir", ":", "\u00bb", "Ich", ",", "ich", "will", "dich", "er\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "ADV", "APPR", "PPER", "$.", "$(", "PPER", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich, als dein Bundesgott, will dich verlassen nicht!\u00ab", "tokens": ["Ich", ",", "als", "dein", "Bun\u00b7des\u00b7gott", ",", "will", "dich", "ver\u00b7las\u00b7sen", "nicht", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "KOUS