{"dta.poem.21399": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die alte Gudel fand ich geschminkt", "tokens": ["Die", "al\u00b7te", "Gu\u00b7del", "fand", "ich", "ge\u00b7schminkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und geputzt wie eine Syrene;", "tokens": ["Und", "ge\u00b7putzt", "wie", "ei\u00b7ne", "Sy\u00b7re\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.3": {"text": "Hat schwarze Locken sich angeschafft", "tokens": ["Hat", "schwar\u00b7ze", "Lo\u00b7cken", "sich", "an\u00b7ge\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "PRF", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und blendend wei\u00dfe Z\u00e4hne.", "tokens": ["Und", "blen\u00b7dend", "wei\u00b7\u00dfe", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am besten hat sich konservirt", "tokens": ["Am", "bes\u00b7ten", "hat", "sich", "kon\u00b7ser\u00b7virt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Freund der Papierverk\u00e4ufer;", "tokens": ["Mein", "Freund", "der", "Pa\u00b7pier\u00b7ver\u00b7k\u00e4u\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sein Haar ward gelb und umwallt sein Haupt,", "tokens": ["Sein", "Haar", "ward", "gelb", "und", "um\u00b7wallt", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sieht aus wie Johannes der T\u00e4ufer.", "tokens": ["Sieht", "aus", "wie", "Jo\u00b7han\u00b7nes", "der", "T\u00e4u\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "NE", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Den **** den sah ich nur von fern,", "tokens": ["Den", "*", "*", "*", "*", "den", "sah", "ich", "nur", "von", "fern", ","], "token_info": ["word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "XY", "XY", "XY", "XY", "ART", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er huschte mir rasch vor\u00fcber;", "tokens": ["Er", "huschte", "mir", "rasch", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6re, sein Geist ist abgebrannt", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "sein", "Geist", "ist", "ab\u00b7ge\u00b7brannt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und war versichert bey Biber.", "tokens": ["Und", "war", "ver\u00b7si\u00b7chert", "bey", "Bi\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+----+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Auch meinen alten Censor sah", "tokens": ["Auch", "mei\u00b7nen", "al\u00b7ten", "Cen\u00b7sor", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wieder. Im Nebel, geb\u00fccket,", "tokens": ["Ich", "wie\u00b7der", ".", "Im", "Ne\u00b7bel", ",", "ge\u00b7b\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$.", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Begegnet\u2019 er mir auf dem G\u00e4nsemarkt,", "tokens": ["Be\u00b7geg\u00b7net'", "er", "mir", "auf", "dem", "G\u00e4n\u00b7se\u00b7markt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schien sehr darnieder gedr\u00fccket.", "tokens": ["Schien", "sehr", "dar\u00b7nie\u00b7der", "ge\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}