{"textgrid.poem.67776": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Versuchung Jesu", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gottessohn vom Jordan kam,", "tokens": ["Der", "Got\u00b7tes\u00b7sohn", "vom", "Jor\u00b7dan", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch schallte Vaterswort:", "tokens": ["Noch", "schall\u00b7te", "Va\u00b7ters\u00b7wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Eingeliebter, der bist Du!\u00ab", "tokens": ["\u00bb", "mein", "Ein\u00b7ge\u00b7lieb\u00b7ter", ",", "der", "bist", "Du", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm in der Seele fort.", "tokens": ["Ihm", "in", "der", "See\u00b7le", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch schwebt' mit zartem Flug auf ihm", "tokens": ["Noch", "schwebt'", "mit", "zar\u00b7tem", "Flug", "auf", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Taub' in Blick und Sinn,", "tokens": ["Die", "Taub'", "in", "Blick", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Vatershuld und Reinigkeit", "tokens": ["Wo", "Va\u00b7ter\u00b7shuld", "und", "Rei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gottheit wohnte drin.", "tokens": ["Und", "Got\u00b7theit", "wohn\u00b7te", "drin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohin? wo wendest Du den Gang,", "tokens": ["Wo\u00b7hin", "?", "wo", "wen\u00b7dest", "Du", "den", "Gang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gepriesner Gottessohn?", "tokens": ["Ge\u00b7pri\u00b7es\u00b7ner", "Got\u00b7tes\u00b7sohn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gehst einsam, Menschen fern, und tief", "tokens": ["Gehst", "ein\u00b7sam", ",", "Men\u00b7schen", "fern", ",", "und", "tief"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "NN", "PTKVZ", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In W\u00fcsteneien schon.", "tokens": ["In", "W\u00fcs\u00b7te\u00b7nei\u00b7en", "schon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er geht und klimmt zu Gott empor,", "tokens": ["Er", "geht", "und", "klimmt", "zu", "Gott", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergisset Speis' und Trank", "tokens": ["Ver\u00b7gis\u00b7set", "Speis'", "und", "Trank"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ringt und betet, vierzig Tag'", "tokens": ["Und", "ringt", "und", "be\u00b7tet", ",", "vier\u00b7zig", "Tag'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und vierzig N\u00e4chte lang.", "tokens": ["Und", "vier\u00b7zig", "N\u00e4ch\u00b7te", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und kommt zur\u00fcck, und w\u00fcthig f\u00e4llt", "tokens": ["Und", "kommt", "zu\u00b7r\u00fcck", ",", "und", "w\u00fct\u00b7hig", "f\u00e4llt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun auf ihn Hungersnoth;", "tokens": ["Nun", "auf", "ihn", "Hun\u00b7gers\u00b7noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um ihn die weite W\u00fcstenei", "tokens": ["Um", "ihn", "die", "wei\u00b7te", "W\u00fcs\u00b7te\u00b7nei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Thiergeheul und Tod.", "tokens": ["Und", "Thier\u00b7ge\u00b7heul", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbbist Du nun, bist Du Gottes Sohn", "tokens": ["\u00bb", "bist", "Du", "nun", ",", "bist", "Du", "Got\u00b7tes", "Sohn"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "ADV", "$,", "VAFIN", "PPER", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Deiner Hungersnoth?", "tokens": ["In", "Dei\u00b7ner", "Hun\u00b7gers\u00b7noth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieh diesen Stein (der Satan sprach's)", "tokens": ["Sieh", "die\u00b7sen", "Stein", "(", "der", "Sa\u00b7tan", "sprach's", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprich den Stein Dir \u2013 Brod!\u00ab", "tokens": ["Und", "sprich", "den", "Stein", "Dir", "\u2013", "Brod", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$(", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und neu mit L\u00f6wenklauen f\u00e4llt", "tokens": ["Und", "neu", "mit", "L\u00f6\u00b7wenk\u00b7lau\u00b7en", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hunger an sein Herz;", "tokens": ["Der", "Hun\u00b7ger", "an", "sein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um ihn die weite W\u00fcstenei", "tokens": ["Um", "ihn", "die", "wei\u00b7te", "W\u00fcs\u00b7te\u00b7nei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in ihm Todesschmerz.", "tokens": ["Und", "in", "ihm", "To\u00b7des\u00b7schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbversucher,\u00ab spricht er, \u00bbdas ist ", "tokens": ["\u00bb", "ver\u00b7su\u00b7cher", ",", "\u00ab", "spricht", "er", ",", "\u00bb", "das", "ist"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PDS", "VAFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und was ist ", "tokens": ["Und", "was", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nicht Brod allein, auch Gottes Hauch", "tokens": ["Nicht", "Brod", "al\u00b7lein", ",", "auch", "Got\u00b7tes", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "$,", "ADV", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Webt unser Leben fort.", "tokens": ["Webt", "un\u00b7ser", "Le\u00b7ben", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das Wort aus Gottes Mund, es ist", "tokens": ["Das", "Wort", "aus", "Got\u00b7tes", "Mund", ",", "es", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Armen Himmelsthau.\u00ab", "tokens": ["Dem", "Ar\u00b7men", "Him\u00b7mel\u00b7sthau", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach's; die W\u00fcste h\u00f6rt' das Wort", "tokens": ["Er", "sprach's", ";", "die", "W\u00fcs\u00b7te", "h\u00f6rt'", "das", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ward zur frischen Au'.", "tokens": ["Und", "ward", "zur", "fri\u00b7schen", "Au'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und schnell die frische Au' ist hin,", "tokens": ["Und", "schnell", "die", "fri\u00b7sche", "Au'", "ist", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er steht auf Tempelsh\u00f6hn.", "tokens": ["Er", "steht", "auf", "Tem\u00b7pels\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbschau nieder! wer kann schwindellos", "tokens": ["\u00bb", "schau", "nie\u00b7der", "!", "wer", "kann", "schwin\u00b7del\u00b7los"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "PTKVZ", "$.", "PWS", "VMFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ab in die Tiefen sehn?", "tokens": ["Ab", "in", "die", "Tie\u00b7fen", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Und Du, Du kannst, ein Gottessohn,", "tokens": ["Und", "Du", ",", "Du", "kannst", ",", "ein", "Got\u00b7tes\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab Dich senken. Fort", "tokens": ["Hin\u00b7ab", "Dich", "sen\u00b7ken", ".", "Fort"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VVINF", "$.", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Tr\u00e4gt Dich aus Gottes Mund ein Hauch", "tokens": ["Tr\u00e4gt", "Dich", "aus", "Got\u00b7tes", "Mund", "ein", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "(auch ich wei\u00df Gottes Wort);", "tokens": ["(", "auch", "ich", "wei\u00df", "Got\u00b7tes", "Wort", ")", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "NN", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Engel Schaar um Dich, sie wird", "tokens": ["Der", "En\u00b7gel", "Schaar", "um", "Dich", ",", "sie", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir ihren Fittig leihn,", "tokens": ["Dir", "ih\u00b7ren", "Fi