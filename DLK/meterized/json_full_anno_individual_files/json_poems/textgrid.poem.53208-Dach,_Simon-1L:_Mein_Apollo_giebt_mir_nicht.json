{"textgrid.poem.53208": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Apollo giebt mir nicht", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Apollo giebt mir nicht", "tokens": ["Mein", "A\u00b7pol\u00b7lo", "giebt", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Allzeit gleiche Lust zu singen:", "tokens": ["All\u00b7zeit", "glei\u00b7che", "Lust", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Offtmals fleuget mein Geticht,", "tokens": ["Offt\u00b7mals", "fleu\u00b7get", "mein", "Ge\u00b7ticht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Ader mir wil springen,", "tokens": ["Da\u00df", "die", "A\u00b7der", "mir", "wil", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "--+--++-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Offtmals f\u00fchl' ich gar kein Blut", "tokens": ["Offt\u00b7mals", "f\u00fchl'", "ich", "gar", "kein", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd zu reimen keinen Muht.", "tokens": ["Vnd", "zu", "rei\u00b7men", "kei\u00b7nen", "Muht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie auch jetzund mir geschieht", "tokens": ["Wie", "auch", "je\u00b7tzund", "mir", "ge\u00b7schieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da mir Geist und Sinnen liegen,", "tokens": ["Da", "mir", "Geist", "und", "Sin\u00b7nen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du auch kein Hochzeit-Lied", "tokens": ["Da\u00df", "du", "auch", "kein", "Hoch\u00b7zeit\u00b7Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schier, Herr Br\u00e4utgam, soltest kriegen,", "tokens": ["Schier", ",", "Herr", "Br\u00e4ut\u00b7gam", ",", "sol\u00b7test", "krie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NE", "$,", "VMFIN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Dem ich doch mein S\u00e4iten-Spiel", "tokens": ["Dem", "ich", "doch", "mein", "S\u00e4i\u00b7ten\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schuldig halten mu\u00df und wil.", "tokens": ["Schul\u00b7dig", "hal\u00b7ten", "mu\u00df", "und", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vnd was l\u00e4g' auch wol daran,", "tokens": ["Vnd", "was", "l\u00e4g'", "auch", "wol", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00df' ich gleich mich itzt nicht h\u00f6ren,", "tokens": ["Lie\u00df'", "ich", "gleich", "mich", "itzt", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil zu deiner Hochzeit Pan", "tokens": ["Weil", "zu", "dei\u00b7ner", "Hoch\u00b7zeit", "Pan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber k\u00f6mpt mit seinen R\u00f6hren,", "tokens": ["Sel\u00b7ber", "k\u00f6mpt", "mit", "sei\u00b7nen", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd Naturen gantzer Pracht", "tokens": ["Vnd", "Na\u00b7tu\u00b7ren", "gant\u00b7zer", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir ein sch\u00f6nes Braut-Lied macht.", "tokens": ["Dir", "ein", "sch\u00f6\u00b7nes", "Braut\u00b7Lied", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie bedient das Taglicht dich?", "tokens": ["Wie", "be\u00b7dient", "das", "Tag\u00b7licht", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lufft und Himmel m\u00fcssen glentzen,", "tokens": ["Lufft", "und", "Him\u00b7mel", "m\u00fcs\u00b7sen", "glent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd die Sonne mahlet sich", "tokens": ["Vnd", "die", "Son\u00b7ne", "mah\u00b7let", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch nicht sch\u00f6ner in dem Lentzen,", "tokens": ["Auch", "nicht", "sch\u00f6\u00b7ner", "in", "dem", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mehr die Nacht schenckt ihre Zier", "tokens": ["Mehr", "die", "Nacht", "schenckt", "ih\u00b7re", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit erg\u00e4ntztem Monde dir.", "tokens": ["Mit", "er\u00b7g\u00e4ntz\u00b7tem", "Mon\u00b7de", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber schl\u00e4fft der Herbst hierbey?", "tokens": ["A\u00b7ber", "schl\u00e4fft", "der", "Herbst", "hier\u00b7bey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der wil gleichfals dir erweisen", "tokens": ["Der", "wil", "gleich\u00b7fals", "dir", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vnd f\u00fcr andern seine Trew", "tokens": ["Vnd", "f\u00fcr", "an\u00b7dern", "sei\u00b7ne", "Trew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den reiff-gewachsnen Speisen,", "tokens": ["Mit", "den", "reiff\u00b7ge\u00b7wachs\u00b7nen", "Spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denen Schmack und Anmuht schafft", "tokens": ["De\u00b7nen", "Schmack", "und", "An\u00b7muht", "schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ceres und Lyeen Safft.", "tokens": ["Ce\u00b7res", "und", "Ly\u00b7een", "Safft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Werden di\u00df nicht Zeichen seyn,", "tokens": ["Wer\u00b7den", "di\u00df", "nicht", "Zei\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "VAINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Da\u00df euch kr\u00f6nen wird der Segen?", "tokens": ["Da\u00df", "euch", "kr\u00f6\u00b7nen", "wird", "der", "Se\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd da\u00df keines Mangels Pein", "tokens": ["Vnd", "da\u00df", "kei\u00b7nes", "Man\u00b7gels", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich umb ewer Hau\u00df wird regen,", "tokens": ["Sich", "umb", "e\u00b7wer", "Hau\u00df", "wird", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VAFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sondern Gn\u00fcg' und Fruchtbarkeit", "tokens": ["Son\u00b7dern", "Gn\u00fcg'", "und", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch umbfassen allezeit?", "tokens": ["Euch", "umb\u00b7fas\u00b7sen", "al\u00b7le\u00b7zeit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Solches schreibst du billich zu", "tokens": ["Sol\u00b7ches", "schreibst", "du", "bil\u00b7lich", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Vatters sch\u00f6nem Leben,", "tokens": ["Dei\u00b7nes", "Vat\u00b7ters", "sch\u00f6\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher war der Musen Ruh,", "tokens": ["Wel\u00b7cher", "war", "der", "Mu\u00b7sen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die noch jetzund ihn erheben", "tokens": ["Die", "noch", "je\u00b7tzund", "ihn", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd gestehn, da\u00df seinem Flei\u00df", "tokens": ["Vnd", "ge\u00b7stehn", ",", "da\u00df", "sei\u00b7nem", "Flei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mancher alles schuldig weis.", "tokens": ["Man\u00b7cher", "al\u00b7les", "schul\u00b7dig", "weis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hiezu k\u00f6mpt auch dein Verstand", "tokens": ["Hie\u00b7zu", "k\u00f6mpt", "auch", "dein", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd die Vnschuld deiner Jugend,", "tokens": ["Vnd", "die", "Vn\u00b7schuld", "dei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die du einig hast gewandt", "tokens": ["Die", "du", "ei\u00b7nig", "hast", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff die Wissenschaft und Tugend,", "tokens": ["Auff", "die", "Wis\u00b7sen\u00b7schaft", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die\u00df gestehen willig dir", "tokens": ["Die\u00df", "ge\u00b7ste\u00b7hen", "wil\u00b7lig", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVINF", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich wie Deutschland also wir.", "tokens": ["Gleich", "wie", "Deutschland", "al\u00b7so", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "ADV", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Vnd wo bleibt die wehrte Braut?", "tokens": ["Vnd", "wo", "bleibt", "die", "wehr\u00b7te", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der sich Zucht sampt andern Gaben", "tokens": ["Der", "sich", "Zucht", "sampt", "an\u00b7dern", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also reichlich anvertrawt", "tokens": ["Al\u00b7so", "reich\u00b7lich", "an\u00b7ver\u00b7trawt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als sie Frawen k\u00f6nnen haben,", "tokens": ["Als", "sie", "Fra\u00b7wen", "k\u00f6n\u00b7nen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das von ihr der L\u00f6benicht", "tokens": ["Das", "von", "ihr", "der", "L\u00f6\u00b7be\u00b7nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein wahres Zeugni\u00df spricht?", "tokens": ["Als", "ein", "wah\u00b7res", "Zeug\u00b7ni\u00df", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "O der angenehmen Glut,", "tokens": ["O", "der", "an\u00b7ge\u00b7neh\u00b7men", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die entbrennt in ewren Hertzen", "tokens": ["Die", "ent\u00b7brennt", "in", "ew\u00b7ren", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd euch r\u00fchrt das frische Blut!", "tokens": ["Vnd", "euch", "r\u00fchrt", "das", "fri\u00b7sche", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt, empfindet nirgends Schmertzen,", "tokens": ["Lebt", ",", "emp\u00b7fin\u00b7det", "nir\u00b7gends", "Schmert\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch die Kriegs-Noht halte Ruh", "tokens": ["Auch", "die", "Kriegs\u00b7Noht", "hal\u00b7te", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd sag' ewre Liebe zu.", "tokens": ["Vnd", "sag'", "ew\u00b7re", "Lie\u00b7be", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVF