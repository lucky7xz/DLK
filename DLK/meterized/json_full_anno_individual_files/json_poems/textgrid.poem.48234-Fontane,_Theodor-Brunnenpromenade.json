{"textgrid.poem.48234": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Brunnenpromenade", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich ankam, Johannistag war grade,", "tokens": ["Als", "ich", "an\u00b7kam", ",", "Jo\u00b7han\u00b7nis\u00b7tag", "war", "gra\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich ging ich auf die Brunnenpromnade.", "tokens": ["Gleich", "ging", "ich", "auf", "die", "Brun\u00b7nen\u00b7prom\u00b7na\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kaum wollt' ich meinen Augen traun,", "tokens": ["Kaum", "wollt'", "ich", "mei\u00b7nen", "Au\u00b7gen", "traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So viel des Herrlichen war da zu schaun,", "tokens": ["So", "viel", "des", "Herr\u00b7li\u00b7chen", "war", "da", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.5": {"text": "Eine lange Reihe der sch\u00f6nsten Damen,", "tokens": ["Ei\u00b7ne", "lan\u00b7ge", "Rei\u00b7he", "der", "sch\u00f6ns\u00b7ten", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Wer z\u00e4hlt die V\u00f6lker, wer nennt die Namen!", "tokens": ["Wer", "z\u00e4hlt", "die", "V\u00f6l\u00b7ker", ",", "wer", "nennt", "die", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Eine ganz Teint und Taille war,", "tokens": ["Ei\u00b7ne", "ganz", "Teint", "und", "Tail\u00b7le", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aschblond das schlicht gescheitelte Haar,", "tokens": ["Aschblond", "das", "schlicht", "ge\u00b7schei\u00b7tel\u00b7te", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Blendende Z\u00e4hne, feines Kinn,", "tokens": ["Blen\u00b7den\u00b7de", "Z\u00e4h\u00b7ne", ",", "fei\u00b7nes", "Kinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Typus einer Engl\u00e4nderin,", "tokens": ["Ty\u00b7pus", "ei\u00b7ner", "En\u00b7gl\u00e4n\u00b7de\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Aber solcher, die palankin-\u00fcberdacht", "tokens": ["A\u00b7ber", "sol\u00b7cher", ",", "die", "pa\u00b7lan\u00b7kin\u00b7\u00fcber\u00b7dacht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Weit drau\u00dfen ihre Tage verbracht,", "tokens": ["Weit", "drau\u00b7\u00dfen", "ih\u00b7re", "Ta\u00b7ge", "ver\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "In Hongkong oder Singapor", "tokens": ["In", "Hong\u00b7kong", "o\u00b7der", "Sin\u00b7ga\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "(ihr Diener Malaie halb, halb Mohr),", "tokens": ["(", "ihr", "Die\u00b7ner", "Ma\u00b7lai\u00b7e", "halb", ",", "halb", "Mohr", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "NE", "ADJD", "$,", "ADJD", "NN", "$(", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und neben ihr plaudert ein junger Lord", "tokens": ["Und", "ne\u00b7ben", "ihr", "plau\u00b7dert", "ein", "jun\u00b7ger", "Lord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Von Lachsfang im Stavanger-Fjord,", "tokens": ["Von", "Lachs\u00b7fang", "im", "Stavan\u00b7ger\u00b7\u00b7F\u00b7jord", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Alles albionm\u00e4\u00dfig abgestempelt,", "tokens": ["Al\u00b7les", "al\u00b7bion\u00b7m\u00e4\u00b7\u00dfig", "ab\u00b7ges\u00b7tem\u00b7pelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Die Beinkleider unten umgekrempelt.", "tokens": ["Die", "Bein\u00b7klei\u00b7der", "un\u00b7ten", "um\u00b7ge\u00b7krem\u00b7pelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es pl\u00e4tschert der Springbrunn, es duften die Blumen,", "tokens": ["Es", "pl\u00e4t\u00b7schert", "der", "Spring\u00b7brunn", ",", "es", "duf\u00b7ten", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Fremd blicken die Bonnen und Kindermuhmen,", "tokens": ["Fremd", "bli\u00b7cken", "die", "Bon\u00b7nen", "und", "Kin\u00b7der\u00b7muh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Noch fremder die Ammen; die Badekapelle", "tokens": ["Noch", "frem\u00b7der", "die", "Am\u00b7men", ";", "die", "Ba\u00b7de\u00b7ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "ART", "NN", "$.", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Spielt eben eine Wagnerstelle,", "tokens": ["Spielt", "e\u00b7ben", "ei\u00b7ne", "Wag\u00b7ner\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lohengrin-Arie, jetzt laut, jetzt leis,", "tokens": ["Lo\u00b7hen\u00b7grin\u00b7\u00b7A\u00b7rie", ",", "jetzt", "laut", ",", "jetzt", "leis", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Die Damen schlie\u00dfen einen Kreis,", "tokens": ["Die", "Da\u00b7men", "schlie\u00b7\u00dfen", "ei\u00b7nen", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und in den Kreis, auf den Schlag des Gong,", "tokens": ["Und", "in", "den", "Kreis", ",", "auf", "den", "Schlag", "des", "Gong", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Tritt jetzt die Sch\u00f6nheit der Saison.", "tokens": ["Tritt", "jetzt", "die", "Sch\u00f6n\u00b7heit", "der", "Sai\u00b7son", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Ihr Aug' ist wie getaucht in Glut,", "tokens": ["Ihr", "Aug'", "ist", "wie", "ge\u00b7taucht", "in", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Rot ist ihr Kleid und rot ihr Hut,", "tokens": ["Rot", "ist", "ihr", "Kleid", "und", "rot", "ihr", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Ein Hut, wie die Kirchenf\u00fcrsten ihn tragen,", "tokens": ["Ein", "Hut", ",", "wie", "die", "Kir\u00b7chen\u00b7f\u00fcrs\u00b7ten", "ihn", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Breitkrempig, ein Schleier umgeschlagen,", "tokens": ["Breit\u00b7krem\u00b7pig", ",", "ein", "Schlei\u00b7er", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Der Schleier ", "tokens": ["Der", "Schlei\u00b7er"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Rot alles, worauf die Blicke fall'n,", "tokens": ["Rot", "al\u00b7les", ",", "wo\u00b7rauf", "die", "Bli\u00b7cke", "fall'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.15": {"text": "Eine R\u00f6merin (fl\u00fcstert man) soll es sein,", "tokens": ["Ei\u00b7ne", "R\u00f6\u00b7me\u00b7rin", "(", "fl\u00fcs\u00b7tert", "man", ")", "soll", "es", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PIS", "$(", "VMFIN", "PPER", "VAINF", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Andre sagen: aus Frankfurt am Main.", "tokens": ["And\u00b7re", "sa\u00b7gen", ":", "aus", "Frank\u00b7furt", "am", "Main", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$.", "APPR", "NE", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und herw\u00e4rts wogt es und wieder zur\u00fcck,", "tokens": ["Und", "her\u00b7w\u00e4rts", "wogt", "es", "und", "wie\u00b7der", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Wagner folgt ein ungrisch St\u00fcck,", "tokens": ["Auf", "Wag\u00b7ner", "folgt", "ein", "un\u00b7grisch", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Czardas, und auf dem bew\u00e4sserten Rasen", "tokens": ["Ein", "Czar\u00b7das", ",", "und", "auf", "dem", "be\u00b7w\u00e4s\u00b7ser\u00b7ten", "Ra\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Blitzt es wie von Goldtopasen;", "tokens": ["Blitzt", "es", "wie", "von", "Gold\u00b7to\u00b7pa\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcberirdisch, ein paradiesisch Revier,", "tokens": ["\u00dc\u00b7be\u00b7rir\u00b7disch", ",", "ein", "pa\u00b7ra\u00b7die\u00b7sisch", "Re\u00b7vier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Und die Frage kommt mir: \u00bbWas willst ", "tokens": ["Und", "die", "Fra\u00b7ge", "kommt", "mir", ":", "\u00bb", "Was", "willst"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$.", "$(", "PWS", "VMFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Eine Freiin gr\u00fc\u00dft mich ... doch, wer sie nicht kennte,", "tokens": ["Ei\u00b7ne", "Frei\u00b7in", "gr\u00fc\u00dft", "mich", "...", "doch", ",", "wer", "sie", "nicht", "kenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Die Macht der h\u00f6heren Elemente!", "tokens": ["Die", "Macht", "der", "h\u00f6\u00b7he\u00b7ren", "E\u00b7le\u00b7men\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Nun ist die erste Woche dahin,", "tokens": ["Nun", "ist", "die", "ers\u00b7te", "Wo\u00b7che", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ver\u00e4ndert schon f\u00fchl' ich Herz und Sinn,", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "schon", "f\u00fchl'", "ich", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und eh' eine zweite Woche vergangen,", "tokens": ["Und", "eh'", "ei\u00b7ne", "zwei\u00b7te", "Wo\u00b7che", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist es nahzu vorbei mit meinem Bangen;", "tokens": ["Ist", "es", "nah\u00b7zu", "vor\u00b7bei", "mit", "mei\u00b7nem", "Ban\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mummenschanz alles und Fastnachtsorden,", "tokens": ["Mum\u00b7men\u00b7schanz", "al\u00b7les", "und", "Fast\u00b7nach\u00b7tsor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Selbst der rote Hut ist mir komisch geworden,", "tokens": ["Selbst", "der", "ro\u00b7te", "Hut", "ist", "mir", "ko\u00b7misch", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ob aus Rom oder Frankfurt \u2013 ich seh' in Ruh'", "tokens": ["Ob", "aus", "Rom", "o\u00b7der", "Frank\u00b7furt", "\u2013", "ich", "seh'", "in", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "KON", "NE", "$(", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Jetzt lieber dem Paukenschl\u00e4ger zu,", "tokens": ["Jetzt", "lie\u00b7ber", "dem", "Pau\u00b7ken\u00b7schl\u00e4\u00b7ger", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der kr\u00e4nklich und m\u00fcrrisch und doch begeistert", "tokens": ["Der", "kr\u00e4nk\u00b7lich", "und", "m\u00fcr\u00b7risch", "und", "