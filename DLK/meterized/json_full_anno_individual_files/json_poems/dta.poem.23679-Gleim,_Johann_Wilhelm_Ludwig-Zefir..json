{"dta.poem.23679": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Zefir.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rosen bl&#252;hn auf schwarzen St\u00f6kken.", "tokens": ["Ro\u00b7sen", "bl", "&#252;", "hn", "auf", "schwar\u00b7zen", "St\u00f6k\u00b7ken", "."], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Seht, wie sich die Farben mischen!", "tokens": ["Seht", ",", "wie", "sich", "die", "Far\u00b7ben", "mi\u00b7schen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lilien stehn, wie weisse Kronen,", "tokens": ["Li\u00b7li\u00b7en", "stehn", ",", "wie", "weis\u00b7se", "Kro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Stolz auf gr\u00fcnen Heroldsst\u00e4ben.", "tokens": ["Stolz", "auf", "gr\u00fc\u00b7nen", "He\u00b7rolds\u00b7st\u00e4\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nelken stehn, wie bunte Kr\u00e4nze,", "tokens": ["Nel\u00b7ken", "stehn", ",", "wie", "bun\u00b7te", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf gef\u00e4rbten Schwanenh\u00e4lsen.", "tokens": ["Auf", "ge\u00b7f\u00e4rb\u00b7ten", "Schwa\u00b7nen\u00b7h\u00e4l\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber seht, sie stehn so stille!", "tokens": ["A\u00b7ber", "seht", ",", "sie", "stehn", "so", "stil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4\u00dft sie Zefir so zufrieden?", "tokens": ["L\u00e4\u00dft", "sie", "Ze\u00b7fir", "so", "zu\u00b7frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zefir, bist du denn so m\u00fc\u00dfig,", "tokens": ["Ze\u00b7fir", ",", "bist", "du", "denn", "so", "m\u00fc\u00b7\u00dfig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Oder bist du weggeschw\u00e4rmet?", "tokens": ["O\u00b7der", "bist", "du", "weg\u00b7ge\u00b7schw\u00e4r\u00b7met", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Kannst du diese Flur verlassen?", "tokens": ["Kannst", "du", "die\u00b7se", "Flur", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wohnst du nicht in diesem Garten?", "tokens": ["Wohnst", "du", "nicht", "in", "die\u00b7sem", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schw\u00e4rmst du nicht in diesen B\u00fcschen,", "tokens": ["Schw\u00e4rmst", "du", "nicht", "in", "die\u00b7sen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die mein Prinz f\u00fcr dich gepflanzet?", "tokens": ["Die", "mein", "Prinz", "f\u00fcr", "dich", "ge\u00b7pflan\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Komm, es warten tausend Nelken,", "tokens": ["Komm", ",", "es", "war\u00b7ten", "tau\u00b7send", "Nel\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Komm, und sch\u00fcttle sie zusammen,", "tokens": ["Komm", ",", "und", "sch\u00fctt\u00b7le", "sie", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df es l\u00e4\u00dft, als wenn sie k\u00fc\u00dften!", "tokens": ["Da\u00df", "es", "l\u00e4\u00dft", ",", "als", "wenn", "sie", "k\u00fc\u00df\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Schw\u00e4rme doch um tausend Rosen!", "tokens": ["Schw\u00e4r\u00b7me", "doch", "um", "tau\u00b7send", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "La\u00df mich sehn, ob sich am liebsten", "tokens": ["La\u00df", "mich", "sehn", ",", "ob", "sich", "am", "liebs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "KOUS", "PRF", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Rosen oder Nelken k\u00fcssen!", "tokens": ["Ro\u00b7sen", "o\u00b7der", "Nel\u00b7ken", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Zefir kannst du nicht mehr schw\u00e4rmen?", "tokens": ["Ze\u00b7fir", "kannst", "du", "nicht", "mehr", "schw\u00e4r\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Oder bist du weggeschw\u00e4rmet?", "tokens": ["O\u00b7der", "bist", "du", "weg\u00b7ge\u00b7schw\u00e4r\u00b7met", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Sucht ihn doch, ihr muntern Knaben,", "tokens": ["Sucht", "ihn", "doch", ",", "ihr", "mun\u00b7tern", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Sucht ihn doch, den M\u00fc\u00dfigg\u00e4nger!", "tokens": ["Sucht", "ihn", "doch", ",", "den", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Kommt, dort wollen wir ihn suchen,", "tokens": ["Kommt", ",", "dort", "wol\u00b7len", "wir", "ihn", "su\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Dort bewegen sich die Lilien.", "tokens": ["Dort", "be\u00b7we\u00b7gen", "sich", "die", "Li\u00b7li\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Seid nur still, ich h\u00f6r ihn lachen,", "tokens": ["Seid", "nur", "still", ",", "ich", "h\u00f6r", "ihn", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "H\u00f6rt nur zu, er lacht recht laute!", "tokens": ["H\u00f6rt", "nur", "zu", ",", "er", "lacht", "recht", "lau\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Seht, dort schw\u00e4rmt er um das M\u00e4dchen!", "tokens": ["Seht", ",", "dort", "schw\u00e4rmt", "er", "um", "das", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Seht, der Zefir iagt das M\u00e4dchen!", "tokens": ["Seht", ",", "der", "Ze\u00b7fir", "i\u00b7agt", "das", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Seht, ietzt schw\u00e4rmt er um den Busen!", "tokens": ["Seht", ",", "ietzt", "schw\u00e4rmt", "er", "um", "den", "Bu\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Seht, ietzt weicht die leichte Seide!", "tokens": ["Seht", ",", "ietzt", "weicht", "die", "leich\u00b7te", "Sei\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Seht, ietzt zeigt er uns den Busen.", "tokens": ["Seht", ",", "ietzt", "zeigt", "er", "uns", "den", "Bu\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Kommt, wir wollen n\u00e4her laufen,", "tokens": ["Kommt", ",", "wir", "wol\u00b7len", "n\u00e4\u00b7her", "lau\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Denn er soll uns noch was zeigen!", "tokens": ["Denn", "er", "soll", "uns", "noch", "was", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}