{"dta.poem.1438": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Fidei-Commissari scher lezter Wille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater/ liebster GOtt/ die Zeit kommt bald herbey/", "tokens": ["Mein", "Va\u00b7ter", "/", "liebs\u00b7ter", "Gott", "/", "die", "Zeit", "kommt", "bald", "her\u00b7bey", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(und ist vielleicht nicht weit/) di\u00df Leben zu verlassen/", "tokens": ["(", "und", "ist", "viel\u00b7leicht", "nicht", "weit", "/", ")", "di\u00df", "Le\u00b7ben", "zu", "ver\u00b7las\u00b7sen", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ADV", "PTKNEG", "ADJD", "$(", "$(", "PDS", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit kein Irrthum nun nach meinem Tode sey/", "tokens": ["Da\u00b7mit", "kein", "Irr\u00b7thum", "nun", "nach", "mei\u00b7nem", "To\u00b7de", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Will ich den lezten Schlu\u00df in diese Zeilen fassen.", "tokens": ["Will", "ich", "den", "lez\u00b7ten", "Schlu\u00df", "in", "die\u00b7se", "Zei\u00b7len", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Halt dr\u00fcber feste Hand/ da\u00df ich ihn selbst nicht breche/", "tokens": ["Halt", "dr\u00fc\u00b7ber", "fes\u00b7te", "Hand", "/", "da\u00df", "ich", "ihn", "selbst", "nicht", "bre\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PAV", "ADJA", "NN", "$(", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch Zanck/ Gewalt und List nach meinem Tode schw\u00e4che.", "tokens": ["Noch", "Zanck", "/", "Ge\u00b7walt", "und", "List", "nach", "mei\u00b7nem", "To\u00b7de", "schw\u00e4\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Seele/ welche du mir eingeblasen hast/", "tokens": ["Die", "See\u00b7le", "/", "wel\u00b7che", "du", "mir", "ein\u00b7ge\u00b7bla\u00b7sen", "hast", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Will ich dir wiederum zu treuen H\u00e4nden geben.", "tokens": ["Will", "ich", "dir", "wie\u00b7de\u00b7rum", "zu", "treu\u00b7en", "H\u00e4n\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df sie/ durch Christi Blutt befreyt der S\u00fcnden Last/", "tokens": ["La\u00df", "sie", "/", "durch", "Chris\u00b7ti", "Blutt", "be\u00b7freyt", "der", "S\u00fcn\u00b7den", "Last", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "APPR", "NE", "ADJD", "VVFIN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von Schulden rein gemacht/ vor deinem Throne schweben/", "tokens": ["Von", "Schul\u00b7den", "rein", "ge\u00b7macht", "/", "vor", "dei\u00b7nem", "Thro\u00b7ne", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Grab ohn alle Pracht bedecke die Gebeine/", "tokens": ["Ein", "Grab", "ohn", "al\u00b7le", "Pracht", "be\u00b7de\u00b7cke", "die", "Ge\u00b7bei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Erd ist \u00fcberall durch ihn geweyht und deine.", "tokens": ["Die", "Erd", "ist", "\u00fc\u00b7be\u00b7rall", "durch", "ihn", "ge\u00b7weyht", "und", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "KON", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn dein Gerichts-Tag k\u00f6mmt/ und die Posaune rufft/", "tokens": ["Wenn", "dein", "Ge\u00b7richts\u00b7Tag", "k\u00f6mmt", "/", "und", "die", "Po\u00b7sau\u00b7ne", "rufft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die alle Todten soll vom langen Schlaffe wecken/", "tokens": ["Die", "al\u00b7le", "Tod\u00b7ten", "soll", "vom", "lan\u00b7gen", "Schlaf\u00b7fe", "we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So fordre meinen Staub hinwieder aus der Grufft/", "tokens": ["So", "ford\u00b7re", "mei\u00b7nen", "Staub", "hin\u00b7wie\u00b7der", "aus", "der", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und la\u00df mich f\u00fcr dem Spruch des Richters nicht erschre-", "tokens": ["Und", "la\u00df", "mich", "f\u00fcr", "dem", "Spruch", "des", "Rich\u00b7ters", "nicht", "er\u00b7schre"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "cken.", "tokens": ["cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.18": {"text": "La\u00df Seel und Leib vereint sich in dem Himmel schwingen/", "tokens": ["La\u00df", "Seel", "und", "Leib", "ver\u00b7eint", "sich", "in", "dem", "Him\u00b7mel", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und ewig Prei\u00df sey GOtt! mit heller Stimme singen.", "tokens": ["Und", "e\u00b7wig", "Prei\u00df", "sey", "Gott", "!", "mit", "hel\u00b7ler", "Stim\u00b7me", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VAFIN", "NN", "$.", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "La\u00df ich von G\u00fcttern was/ wie viel es m\u00f6ge seyn/", "tokens": ["La\u00df", "ich", "von", "G\u00fct\u00b7tern", "was", "/", "wie", "viel", "es", "m\u00f6\u00b7ge", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PWS", "$(", "PWAV", "PIS", "PPER", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So wei\u00df ich/ da\u00df ich nichts/ ohn deine Gunst kan haben/", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "ich", "nichts", "/", "ohn", "dei\u00b7ne", "Gunst", "kan", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PIS", "$(", "APPR", "PPOSAT", "NN", "VMFIN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Drum setz ich dich hiermit zum Erben wieder ein/", "tokens": ["Drum", "setz", "ich", "dich", "hier\u00b7mit", "zum", "Er\u00b7ben", "wie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PAV", "APPRART", "NN", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Inzwischen la\u00df mich recht gebrauchen deiner Gaben;", "tokens": ["I\u00b7nzwi\u00b7schen", "la\u00df", "mich", "recht", "ge\u00b7brau\u00b7chen", "dei\u00b7ner", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du hast allein die Macht zu mehren und zu mindern/", "tokens": ["Du", "hast", "al\u00b7lein", "die", "Macht", "zu", "meh\u00b7ren", "und", "zu", "min\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gieb/ was du wilt davon/ wies selig meinen Kindern.", "tokens": ["Gieb", "/", "was", "du", "wilt", "da\u00b7von", "/", "wies", "se\u00b7lig", "mei\u00b7nen", "Kin\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PWS", "PPER", "VMFIN", "PAV", "$(", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wer traurig hinterbleibt dem schencke Trost und Schutz/", "tokens": ["Wer", "trau\u00b7rig", "hin\u00b7ter\u00b7bleibt", "dem", "schen\u00b7cke", "Trost", "und", "Schutz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sey Vormund/ Vater/ Mann/ zeuch was noch unerzogen", "tokens": ["Sey", "Vor\u00b7mund", "/", "Va\u00b7ter", "/", "Mann", "/", "zeuch", "was", "noch", "un\u00b7er\u00b7zo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$(", "NN", "$(", "NN", "$(", "VVIMP", "PWS", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zu deinen Ehren auff/ beut allen Feinden Trutz/", "tokens": ["Zu", "dei\u00b7nen", "Eh\u00b7ren", "auff", "/", "beut", "al\u00b7len", "Fein\u00b7den", "Trutz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "$(", "VVFIN", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Erhalte sie von Welt und S\u00fcnden unbetrogen.", "tokens": ["Er\u00b7hal\u00b7te", "sie", "von", "Welt", "und", "S\u00fcn\u00b7den", "un\u00b7be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "La\u00df/ die du mir geschenckt/ und die ich dir geb eigen/", "tokens": ["La\u00df", "/", "die", "du", "mir", "ge\u00b7schenckt", "/", "und", "die", "ich", "dir", "geb", "ei\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PRELS", "PPER", "PPER", "VVPP", "$(", "KON", "PRELS", "PPER", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Auch deinem Creutze nach/ wie mich/ in Himmel steigen.", "tokens": ["Auch", "dei\u00b7nem", "Creut\u00b7ze", "nach", "/", "wie", "mich", "/", "in", "Him\u00b7mel", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "$(", "PWAV", "PPER", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Di\u00df ist mein Testament: Solt eine Zierligkeit/", "tokens": ["Di\u00df", "ist", "mein", "Tes\u00b7ta\u00b7ment", ":", "Solt", "ei\u00b7ne", "Zier\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "VMFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Solt etwas/ welches noth bey lezten Rechts-Gesch\u00e4fften/", "tokens": ["Solt", "et\u00b7was", "/", "wel\u00b7ches", "noth", "bey", "lez\u00b7ten", "Rechts\u00b7Ge\u00b7sch\u00e4ff\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$(", "PWAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Allhier vergessen seyn/ so bleib es doch zur Zeit", "tokens": ["All\u00b7hier", "ver\u00b7ges\u00b7sen", "seyn", "/", "so", "bleib", "es", "doch", "zur", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAINF", "$(", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Als lezter Vater-Will/ als Todes-Gab\u2019 in Kr\u00e4fften.", "tokens": ["Als", "lez\u00b7ter", "Va\u00b7ter\u00b7Will", "/", "als", "To\u00b7des\u00b7Gab'", "in", "Kr\u00e4ff\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$(", "KOUS", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dein Geist/ dein JEsus zeugt mit Siegel/ Hand und Nah-", "tokens": ["Dein", "Geist", "/", "dein", "Je\u00b7sus", "zeugt", "mit", "Sie\u00b7gel", "/", "Hand", "und", "Nah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NE", "VVFIN", "APPR", "NN", "$(", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "men/", "tokens": ["men", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.38": {"text": "So schl\u00fc\u00df ich nun getrost: hilff Helffer selig Amen!", "tokens": ["So", "schl\u00fc\u00df", "ich", "nun", "ge\u00b7trost", ":", "hilff", "Helf\u00b7fer", "se\u00b7lig", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$.", "NN", "NE", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}