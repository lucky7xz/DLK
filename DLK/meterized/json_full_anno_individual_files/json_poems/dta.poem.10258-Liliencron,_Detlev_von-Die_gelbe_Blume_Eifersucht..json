{"dta.poem.10258": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Die gelbe Blume Eifersucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Was war das, dr&#252;ckt er ihr leise die Hand,             ", "tokens": ["Was", "war", "das", ",", "dr", "&#252;", "ckt", "er", "ihr", "lei\u00b7se", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$,", "XY", "$(", "VVFIN", "PPER", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Als gestern Abend er neben ihr stand,", "tokens": ["Als", "ge\u00b7stern", "A\u00b7bend", "er", "ne\u00b7ben", "ihr", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heut sah sie den ganzen Tag hinaus:", "tokens": ["Heut", "sah", "sie", "den", "gan\u00b7zen", "Tag", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wann wird er kommen.", "tokens": ["Wann", "wird", "er", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und als er um die Ecke bog,", "tokens": ["Und", "als", "er", "um", "die", "E\u00b7cke", "bog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Rot ihr in die Schl\u00e4fen flog.", "tokens": ["Das", "Rot", "ihr", "in", "die", "Schl\u00e4\u00b7fen", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das soll dir nicht frommen,", "tokens": ["Das", "soll", "dir", "nicht", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Heut Abend, ich lauschte, in heimlicher Stund\u2019", "tokens": ["Heut", "A\u00b7bend", ",", "ich", "lauschte", ",", "in", "heim\u00b7li\u00b7cher", "Stund'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er k\u00fc\u00dfte sie z\u00e4rtlich auf Augen und Mund,", "tokens": ["Er", "k\u00fc\u00df\u00b7te", "sie", "z\u00e4rt\u00b7lich", "auf", "Au\u00b7gen", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nun lauer\u2019 und schleich ich im S\u00e4ulengang", "tokens": ["Nun", "lau\u00b7e\u00b7r'", "und", "schleich", "ich", "im", "S\u00e4u\u00b7len\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf Katzenpfoten.", "tokens": ["Auf", "Kat\u00b7zen\u00b7pfo\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Meinen Dolch betast\u2019 ich wohl hundertmal,", "tokens": ["Mei\u00b7nen", "Dolch", "be\u00b7tast'", "ich", "wohl", "hun\u00b7dert\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "In die Brust ihn dir brech\u2019 ich f\u00fcr alle die Qual,", "tokens": ["In", "die", "Brust", "ihn", "dir", "brech'", "ich", "f\u00fcr", "al\u00b7le", "die", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PPER", "VVFIN", "PPER", "APPR", "PIS", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Als Liebesboten,", "tokens": ["Als", "Lie\u00b7bes\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}}}}