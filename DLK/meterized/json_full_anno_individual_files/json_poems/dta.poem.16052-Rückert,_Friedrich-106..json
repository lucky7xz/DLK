{"dta.poem.16052": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich finde dich, wo ich, o H\u00f6chster, hin mich wende;", "tokens": ["Ich", "fin\u00b7de", "dich", ",", "wo", "ich", ",", "o", "H\u00f6chs\u00b7ter", ",", "hin", "mich", "wen\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "$,", "FM", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Anfang find' ich dich, und finde dich am Ende.", "tokens": ["Am", "An\u00b7fang", "find'", "ich", "dich", ",", "und", "fin\u00b7de", "dich", "am", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Anfang geh' ich nach, in dir verliert er sich;", "tokens": ["Dem", "An\u00b7fang", "geh'", "ich", "nach", ",", "in", "dir", "ver\u00b7liert", "er", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Abschlu\u00df sp\u00e4h' ich nach, aus dir gebiert er sich.", "tokens": ["Dem", "Ab\u00b7schlu\u00df", "sp\u00e4h'", "ich", "nach", ",", "aus", "dir", "ge\u00b7biert", "er", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPER", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist der Anfang, der sich aus sich selbst vollendet,", "tokens": ["Du", "bist", "der", "An\u00b7fang", ",", "der", "sich", "aus", "sich", "selbst", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ende, das zur\u00fcck sich in den Anfang wendet.", "tokens": ["Das", "En\u00b7de", ",", "das", "zu\u00b7r\u00fcck", "sich", "in", "den", "An\u00b7fang", "wen\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und in der Mitte bist du selber das was ist;", "tokens": ["Und", "in", "der", "Mit\u00b7te", "bist", "du", "sel\u00b7ber", "das", "was", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "ART", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ich bin ich, weil du in mir die Mitte bist.", "tokens": ["Und", "ich", "bin", "ich", ",", "weil", "du", "in", "mir", "die", "Mit\u00b7te", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}}}}