{"dta.poem.4182": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Sch\u00f6nheit eines stillen Meers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ist, wie schrecklich erst das Meer, wie f\u00fcrchterlich", "tokens": ["Da", "ist", ",", "wie", "schreck\u00b7lich", "erst", "das", "Meer", ",", "wie", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "ADJD", "ADV", "ART", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Wasser-Wogen,", "tokens": ["die", "Was\u00b7ser\u00b7Wo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie stark der schweren Wellen Last, und wie entsetzlich sie;", "tokens": ["Wie", "stark", "der", "schwe\u00b7ren", "Wel\u00b7len", "Last", ",", "und", "wie", "ent\u00b7setz\u00b7lich", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "NE", "$,", "KON", "PWAV", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "so sch\u00f6n,", "tokens": ["so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "So lieblich, sanft, ergetzend, pr\u00e4chtig, sogleich, wenn sich der", "tokens": ["So", "lieb\u00b7lich", ",", "sanft", ",", "er\u00b7get\u00b7zend", ",", "pr\u00e4ch\u00b7tig", ",", "sog\u00b7leich", ",", "wenn", "sich", "der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "VVPP", "$,", "ADJD", "$,", "ADV", "$,", "KOUS", "PRF", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sturm verzogen;", "tokens": ["Sturm", "ver\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die ungeme\u00dfne Wasser-Welt, das Reich der Fluhten, anzu-", "tokens": ["Die", "un\u00b7ge\u00b7me\u00df\u00b7ne", "Was\u00b7ser\u00b7Welt", ",", "das", "Reich", "der", "Fluh\u00b7ten", ",", "an\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "sehn.", "tokens": ["sehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Jtzt schwebt mein Blick, auf stillem Meer, in einem neuen", "tokens": ["Jtzt", "schwebt", "mein", "Blick", ",", "auf", "stil\u00b7lem", "Meer", ",", "in", "ei\u00b7nem", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Meer von Strahlen,", "tokens": ["Meer", "von", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die, durch die Luft, der Fluht Sapphir, mit g\u00fcldnem Glanz", "tokens": ["Die", ",", "durch", "die", "Luft", ",", "der", "Fluht", "Sap\u00b7phir", ",", "mit", "g\u00fcld\u00b7nem", "Glanz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und Schimmer, mahlen.", "tokens": ["und", "Schim\u00b7mer", ",", "mah\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mein GOtt! wie ist, auf glattem Wasser, der heitern Son-", "tokens": ["Mein", "Gott", "!", "wie", "ist", ",", "auf", "glat\u00b7tem", "Was\u00b7ser", ",", "der", "hei\u00b7tern", "Son"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "nen Licht und Schein", "tokens": ["nen", "Licht", "und", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So herrlich, so belebend pr\u00e4chtig, so voller Glanz, so hell, so", "tokens": ["So", "herr\u00b7lich", ",", "so", "be\u00b7le\u00b7bend", "pr\u00e4ch\u00b7tig", ",", "so", "vol\u00b7ler", "Glanz", ",", "so", "hell", ",", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,", "ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "rein!", "tokens": ["rein", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Die ebne Fl\u00e4che scheint aus einem nicht abzusehenden", "tokens": ["Die", "eb\u00b7ne", "Fl\u00e4\u00b7che", "scheint", "aus", "ei\u00b7nem", "nicht", "ab\u00b7zu\u00b7se\u00b7hen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "PTKNEG", "VVIZU"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Sapphir", "tokens": ["Sap\u00b7phir"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Ein glatt-formirter Himmels-Spiegel. Es scheint, so weit", "tokens": ["Ein", "glat\u00b7tfor\u00b7mir\u00b7ter", "Him\u00b7mels\u00b7Spie\u00b7gel", ".", "Es", "scheint", ",", "so", "weit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "die sch\u00e4rfsten Augen,", "tokens": ["die", "sch\u00e4rfs\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Auf dieses blaue Fluhten-Feld, den schnellen Blick zu lenken", "tokens": ["Auf", "die\u00b7ses", "blau\u00b7e", "Fluh\u00b7ten\u00b7Feld", ",", "den", "schnel\u00b7len", "Blick", "zu", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "taugen,", "tokens": ["tau\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Als wenn das Meer sich in dem Himmel, der Himmel sich im", "tokens": ["Als", "wenn", "das", "Meer", "sich", "in", "dem", "Him\u00b7mel", ",", "der", "Him\u00b7mel", "sich", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,", "ART", "NN", "PRF", "APPRART"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Meer verliehr.", "tokens": ["Meer", "ver\u00b7liehr", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "So, wie die Luft sich f\u00e4rbt, so f\u00e4rbt sich auch das weite", "tokens": ["So", ",", "wie", "die", "Luft", "sich", "f\u00e4rbt", ",", "so", "f\u00e4rbt", "sich", "auch", "das", "wei\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wasser-Reich.", "tokens": ["Was\u00b7ser\u00b7Reich", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}