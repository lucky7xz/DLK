{"textgrid.poem.33113": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lorenzo folgt dem allgemeinen Drange,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lorenzo folgt dem allgemeinen Drange,", "tokens": ["Lo\u00b7ren\u00b7zo", "folgt", "dem", "all\u00b7ge\u00b7mei\u00b7nen", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Er tritt herein, er schaut die Engelreine,", "tokens": ["Er", "tritt", "her\u00b7ein", ",", "er", "schaut", "die", "En\u00b7gel\u00b7rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rings um sie her kein Auge, das nicht weine \u2013", "tokens": ["Rings", "um", "sie", "her", "kein", "Au\u00b7ge", ",", "das", "nicht", "wei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da schl\u00e4gt das Herz im Busen ihm so bange,", "tokens": ["Da", "schl\u00e4gt", "das", "Herz", "im", "Bu\u00b7sen", "ihm", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihm wird klar, was er gesucht so lange!", "tokens": ["Und", "ihm", "wird", "klar", ",", "was", "er", "ge\u00b7sucht", "so", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "PWS", "PPER", "VVPP", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vor dieser todten Sch\u00f6nheit heil'gem Schreine,", "tokens": ["Vor", "die\u00b7ser", "tod\u00b7ten", "Sch\u00f6n\u00b7heit", "heil'\u00b7gem", "Schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei diesem Weh trifft's ihn mit Blitzesscheine,", "tokens": ["Bei", "die\u00b7sem", "Weh", "trifft's", "ihn", "mit", "Blit\u00b7zes\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und seine Seele l\u00f6st sich im Gesange.", "tokens": ["Und", "sei\u00b7ne", "See\u00b7le", "l\u00f6st", "sich", "im", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie jach ein Stern hervorbricht aus der Nacht,", "tokens": ["Wie", "jach", "ein", "Stern", "her\u00b7vor\u00b7bricht", "aus", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wird Poesie sein unbestimmtes Tr\u00e4umen \u2013", "tokens": ["Wird", "Poe\u00b7sie", "sein", "un\u00b7be\u00b7stimm\u00b7tes", "Tr\u00e4u\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Dichter zieht er fort aus diesen R\u00e4umen.", "tokens": ["Ein", "Dich\u00b7ter", "zieht", "er", "fort", "aus", "die\u00b7sen", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So unverg\u00e4nglich ist des Sch\u00f6nen Macht,", "tokens": ["So", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "ist", "des", "Sch\u00f6\u00b7nen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So mu\u00df sein Anblick selber noch im Sterben", "tokens": ["So", "mu\u00df", "sein", "An\u00b7blick", "sel\u00b7ber", "noch", "im", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu neuem Leben neue Sch\u00f6nheit werben!", "tokens": ["Zu", "neu\u00b7em", "Le\u00b7ben", "neu\u00b7e", "Sch\u00f6n\u00b7heit", "wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Lorenzo folgt dem allgemeinen Drange,", "tokens": ["Lo\u00b7ren\u00b7zo", "folgt", "dem", "all\u00b7ge\u00b7mei\u00b7nen", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punc