{"dta.poem.6805": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Zauberey, die gantz unschuldig, zu geschweigen,", "tokens": ["Der", "Zau\u00b7be\u00b7rey", ",", "die", "gantz", "un\u00b7schul\u00b7dig", ",", "zu", "ge\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die so zarte Z\u00fcg\u2019 uns manche Sch\u00f6nheit zeigen,", "tokens": ["Durch", "die", "so", "zar\u00b7te", "Z\u00fcg'", "uns", "man\u00b7che", "Sch\u00f6n\u00b7heit", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie sehn wir nicht bereits so hoch die K\u00fcnste steigen!", "tokens": ["Wie", "sehn", "wir", "nicht", "be\u00b7reits", "so", "hoch", "die", "K\u00fcns\u00b7te", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was zeigt uns euer edles Licht,", "tokens": ["Was", "zeigt", "uns", "eu\u00b7er", "ed\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Stern-Kunst und Phisie, f\u00fcr Wahrheit nicht! ", "tokens": ["O", "Stern\u00b7Kunst", "und", "Phi\u00b7sie", ",", "f\u00fcr", "Wahr\u00b7heit", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "APPR", "NN", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Kennt doch die Welt durch die Vortrefflichkeiten,", "tokens": ["Kennt", "doch", "die", "Welt", "durch", "die", "Vor\u00b7treff\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Wozu uns Stern-und Gr\u00f6ssrungs-Gl\u00e4ser leiten,", "tokens": ["Wo\u00b7zu", "uns", "Stern\u00b7\u00b7und", "Gr\u00f6ss\u00b7rungs\u00b7Gl\u00e4\u00b7ser", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fast ferner keine Dunckelheiten.", "tokens": ["Fast", "fer\u00b7ner", "kei\u00b7ne", "Dun\u00b7ckel\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}