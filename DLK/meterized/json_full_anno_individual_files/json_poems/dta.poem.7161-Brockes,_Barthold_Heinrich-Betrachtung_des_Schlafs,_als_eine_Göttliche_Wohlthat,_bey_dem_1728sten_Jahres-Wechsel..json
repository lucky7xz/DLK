{"dta.poem.7161": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat mir GOTT auch wollen g\u00f6nnen,", "tokens": ["Es", "hat", "mir", "GoTT", "auch", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NE", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df der Gesundheit abzuwarten,", "tokens": ["Da\u00df", "der", "Ge\u00b7sund\u00b7heit", "ab\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nebst meiner ", "tokens": ["Nebst", "mei\u00b7ner"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich bey recht angenehm-und sch\u00f6ner Witterung,", "tokens": ["Ich", "bey", "recht", "an\u00b7ge\u00b7nehm\u00b7\u00b7und", "sch\u00f6\u00b7ner", "Wit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "Die Wasser, aus Pyrmont, frey von Bel\u00e4stigung,", "tokens": ["Die", "Was\u00b7ser", ",", "aus", "Pyr\u00b7mont", ",", "frey", "von", "Be\u00b7l\u00e4s\u00b7ti\u00b7gung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NE", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mit guter Wirckung trincken k\u00f6nnen.", "tokens": ["Mit", "gu\u00b7ter", "Wir\u00b7ckung", "trin\u00b7cken", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn dieses Wasser nun so edlen Balsam heget,", "tokens": ["Wenn", "die\u00b7ses", "Was\u00b7ser", "nun", "so", "ed\u00b7len", "Bal\u00b7sam", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So danck ich GOTT, da\u00df Er die Krafft darein geleget,", "tokens": ["So", "danck", "ich", "GoTT", ",", "da\u00df", "Er", "die", "Krafft", "da\u00b7rein", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "NE", "$,", "KOUS", "PPER", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zumal da offenbar,", "tokens": ["Zu\u00b7mal", "da", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wovor absonderlich ich meinem Sch\u00f6pffer preise,", "tokens": ["Wo\u00b7vor", "ab\u00b7son\u00b7der\u00b7lich", "ich", "mei\u00b7nem", "Sch\u00f6pf\u00b7fer", "prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df mir der C\u00f6rper, auf die Weise,", "tokens": ["Da\u00df", "mir", "der", "C\u00f6r\u00b7per", ",", "auf", "die", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zu der mich bald darauf betroffenen Gefahr", "tokens": ["Zu", "der", "mich", "bald", "da\u00b7rauf", "be\u00b7trof\u00b7fe\u00b7nen", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPER", "ADV", "PAV", "ADJA", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.13": {"text": "Des Armbruchs, zugericht\u2019t, gereinigt war.", "tokens": ["Des", "Arm\u00b7bruchs", ",", "zu\u00b7ge\u00b7richt't", ",", "ge\u00b7rei\u00b7nigt", "war", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}