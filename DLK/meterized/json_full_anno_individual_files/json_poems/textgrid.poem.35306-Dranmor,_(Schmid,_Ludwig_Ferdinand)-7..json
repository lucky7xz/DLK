{"textgrid.poem.35306": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein L\u00e4rmen \u00fcber mir. \u2013 Aufs Deck gesprungen,", "tokens": ["Ein", "L\u00e4r\u00b7men", "\u00fc\u00b7ber", "mir", ".", "\u2013", "Aufs", "Deck", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "$(", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kapt\u00e4n, ich lag im sch\u00f6nsten Mittagsschlaf.", "tokens": ["Kap\u00b7t\u00e4n", ",", "ich", "lag", "im", "sch\u00f6ns\u00b7ten", "Mit\u00b7tags\u00b7schlaf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was soll's? \u2013 Ein Schiff, ein Yankee, segelt brav \u2013", "tokens": ["Was", "soll's", "?", "\u2013", "Ein", "Schiff", ",", "ein", "Y\u00b7an\u00b7kee", ",", "se\u00b7gelt", "brav", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$.", "$(", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da dreht er bei \u2013 Hurrah! das ist gelungen!", "tokens": ["Da", "dreht", "er", "bei", "\u2013", "Hur\u00b7rah", "!", "das", "ist", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "ITJ", "$.", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Woher, wohin? \u2013 ", "tokens": ["Wo\u00b7her", ",", "wo\u00b7hin", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["PWAV", "$,", "PWAV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Backbord gesteuert! \u2013 Dichterherz, erwache!", "tokens": ["Back\u00b7bord", "ge\u00b7steu\u00b7ert", "!", "\u2013", "Dich\u00b7ter\u00b7herz", ",", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVPP", "$.", "$(", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frisch bl\u00e4st der Wind, die Segel sind gespannt \u2013", "tokens": ["Frisch", "bl\u00e4st", "der", "Wind", ",", "die", "Se\u00b7gel", "sind", "ge\u00b7spannt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch eine Frage: Braucht ihr Proviant? \u2013", "tokens": ["Noch", "ei\u00b7ne", "Fra\u00b7ge", ":", "Braucht", "ihr", "Pro\u00b7vi\u00b7ant", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$.", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein! \u2013 Vorw\u00e4rts denn, der Rest ist Nebensache.", "tokens": ["Nein", "!", "\u2013", "Vor\u00b7w\u00e4rts", "denn", ",", "der", "Rest", "ist", "Ne\u00b7ben\u00b7sa\u00b7che", "."], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "ADV", "ADV", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambi