{"textgrid.poem.48228": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Publikum", "genre": "verse", "period": "N.A.", "pub_year": 1883, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Publikum ist eine einfache Frau,", "tokens": ["Das", "Pub\u00b7li\u00b7kum", "ist", "ei\u00b7ne", "ein\u00b7fa\u00b7che", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bourgeoishaft, eitel und wichtig,", "tokens": ["Bour\u00b7ge\u00b7o\u00b7is\u00b7haft", ",", "ei\u00b7tel", "und", "wich\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und folgt man, wenn sie spricht, genau,", "tokens": ["Und", "folgt", "man", ",", "wenn", "sie", "spricht", ",", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So spricht sie nicht mal richtig.", "tokens": ["So", "spricht", "sie", "nicht", "mal", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Eine einfache Frau, doch rosig und frisch,", "tokens": ["Ei\u00b7ne", "ein\u00b7fa\u00b7che", "Frau", ",", "doch", "ro\u00b7sig", "und", "frisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und ihre Juwelen blitzen,", "tokens": ["Und", "ih\u00b7re", "Ju\u00b7we\u00b7len", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sie lacht und f\u00fchrt einen guten Tisch,", "tokens": ["Und", "sie", "lacht", "und", "f\u00fchrt", "ei\u00b7nen", "gu\u00b7ten", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und es m\u00f6chte sie jeder besitzen.", "tokens": ["Und", "es", "m\u00f6ch\u00b7te", "sie", "je\u00b7der", "be\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Das Publikum ist eine einfache Frau,", "tokens": ["Das", "Pub\u00b7li\u00b7kum", "ist", "ei\u00b7ne", "ein\u00b7fa\u00b7che", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bourgeoishaft, eitel und wichtig,", "tokens": ["Bour\u00b7ge\u00b7o\u00b7is\u00b7haft", ",", "ei\u00b7tel", "und", "wich\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und folgt man, wenn sie spricht, genau,", "tokens": ["Und", "folgt", "man", ",", "wenn", "sie", "spricht", ",", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "punct