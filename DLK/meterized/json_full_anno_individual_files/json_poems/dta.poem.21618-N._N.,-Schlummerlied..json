{"dta.poem.21618": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Schlummerlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Langsam, ihr funkelnden Sterne der Nacht,", "tokens": ["Lang\u00b7sam", ",", "ihr", "fun\u00b7keln\u00b7den", "Ster\u00b7ne", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Schreitet dahin im Reigen.", "tokens": ["Schrei\u00b7tet", "da\u00b7hin", "im", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Rauschender Wind, nun wehe sacht,", "tokens": ["Rau\u00b7schen\u00b7der", "Wind", ",", "nun", "we\u00b7he", "sacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wiege dich sanft in den Zweigen.", "tokens": ["Wie\u00b7ge", "dich", "sanft", "in", "den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Denn die Liebste hat kosensm\u00fcd", "tokens": ["Denn", "die", "Liebs\u00b7te", "hat", "ko\u00b7sens\u00b7m\u00fcd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Schlummernde Lider geschlossen.", "tokens": ["Schlum\u00b7mern\u00b7de", "Li\u00b7der", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Rosenfarbe, heimlich ergl\u00fcht,", "tokens": ["Ro\u00b7sen\u00b7far\u00b7be", ",", "heim\u00b7lich", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Ist auf ihr Antlitz gegossen.", "tokens": ["Ist", "auf", "ihr", "Ant\u00b7litz", "ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr zu F\u00fc\u00dfen mein Leben ruht,", "tokens": ["Ihr", "zu", "F\u00fc\u00b7\u00dfen", "mein", "Le\u00b7ben", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wonniges Lauschen und Sinnen!", "tokens": ["Won\u00b7ni\u00b7ges", "Lau\u00b7schen", "und", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ferne h\u00f6r\u2019 ich die heilige Fluth", "tokens": ["Fer\u00b7ne", "h\u00f6r'", "ich", "die", "hei\u00b7li\u00b7ge", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dieses Daseins verrinnen.", "tokens": ["Die\u00b7ses", "Da\u00b7seins", "ver\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wunderseligen Wiederhall", "tokens": ["Wun\u00b7der\u00b7se\u00b7li\u00b7gen", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Weckt mir das ewige: Werde!", "tokens": ["Weckt", "mir", "das", "e\u00b7wi\u00b7ge", ":", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$.", "VAFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Und ich segne mein Heim das All,", "tokens": ["Und", "ich", "seg\u00b7ne", "mein", "Heim", "das", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und den Staub dieser Erde. \u2014", "tokens": ["Und", "den", "Staub", "die\u00b7ser", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "NN", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}