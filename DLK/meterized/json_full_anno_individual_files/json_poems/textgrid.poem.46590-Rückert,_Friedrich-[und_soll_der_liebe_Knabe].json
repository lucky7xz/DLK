, "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Mit gutem Zeugni\u00df deiner Klasse,", "tokens": ["Mit", "gu\u00b7tem", "Zeug\u00b7ni\u00df", "dei\u00b7ner", "Klas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die ihm nicht mehr mag frommen,", "tokens": ["Die", "ihm", "nicht", "mehr", "mag", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Er ist zur h\u00f6hern aufgenommen.", "tokens": ["Er", "ist", "zur", "h\u00f6\u00b7hern", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Doch trennst du dich vom Z\u00f6gling", "tokens": ["Doch", "trennst", "du", "dich", "vom", "Z\u00f6g\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Schwer wie das Nest vom jungen V\u00f6gling,", "tokens": ["Schwer", "wie", "das", "Nest", "vom", "jun\u00b7gen", "V\u00f6g\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Der, wie er f\u00fchlt sich fl\u00fccke,", "tokens": ["Der", ",", "wie", "er", "f\u00fchlt", "sich", "fl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Auffliegt und nie mehr kehrt zur\u00fccke.", "tokens": ["Auf\u00b7fliegt", "und", "nie", "mehr", "kehrt", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}