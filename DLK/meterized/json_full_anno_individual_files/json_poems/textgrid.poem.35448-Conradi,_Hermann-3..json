{"textgrid.poem.35448": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So st\u00fcrze, Moloch der Vernichtung,", "tokens": ["So", "st\u00fcr\u00b7ze", ",", "Mo\u00b7loch", "der", "Ver\u00b7nich\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In meine Brust und morde sie,", "tokens": ["In", "mei\u00b7ne", "Brust", "und", "mor\u00b7de", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die stolze Flamme meiner Dichtung,", "tokens": ["Die", "stol\u00b7ze", "Flam\u00b7me", "mei\u00b7ner", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leuchte meiner Poesie!", "tokens": ["Die", "Leuch\u00b7te", "mei\u00b7ner", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So wirf mit deiner Keule nieder", "tokens": ["So", "wirf", "mit", "dei\u00b7ner", "Keu\u00b7le", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Bau, den ich so hoch erricht't!", "tokens": ["Den", "Bau", ",", "den", "ich", "so", "hoch", "er\u00b7richt't", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ersticke die Emp\u00f6rungslieder!", "tokens": ["Er\u00b7sti\u00b7cke", "die", "Em\u00b7p\u00f6\u00b7rungs\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zertritt mein wildes Sturmgedicht!", "tokens": ["Zer\u00b7tritt", "mein", "wil\u00b7des", "Sturm\u00b7ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur zu! Zerfetz mit deinen Krallen", "tokens": ["Nur", "zu", "!", "Zer\u00b7fetz", "mit", "dei\u00b7nen", "Kral\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Dokument des freien Geists!", "tokens": ["Das", "Do\u00b7ku\u00b7ment", "des", "frei\u00b7en", "Geists", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin nun einmal dir verfallen!", "tokens": ["Ich", "bin", "nun", "ein\u00b7mal", "dir", "ver\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zermalmungsm\u00e4chte, nun zerrei\u00dft's!", "tokens": ["Zer\u00b7mal\u00b7mungs\u00b7m\u00e4ch\u00b7te", ",", "nun", "zer\u00b7rei\u00dft's", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV