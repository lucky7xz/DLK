{"textgrid.poem.63362": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod im Bridge", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es spielen dreie mit verdeckten Karten.", "tokens": ["Es", "spie\u00b7len", "drei\u00b7e", "mit", "ver\u00b7deck\u00b7ten", "Kar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein dummer Vierter findet sich zumeist,", "tokens": ["Ein", "dum\u00b7mer", "Vier\u00b7ter", "fin\u00b7det", "sich", "zu\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ihre Heuchelei als Tugend preist", "tokens": ["Der", "ih\u00b7re", "Heu\u00b7che\u00b7lei", "als", "Tu\u00b7gend", "preist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und den sie mit erhab'nen Reden narrten.", "tokens": ["Und", "den", "sie", "mit", "er\u00b7hab'\u00b7nen", "Re\u00b7den", "narr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dieweil er sinnend in den H\u00f6hen reist,", "tokens": ["Die\u00b7weil", "er", "sin\u00b7nend", "in", "den", "H\u00f6\u00b7hen", "reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Sinne der Erf\u00fcllung harrten,", "tokens": ["Und", "sei\u00b7ne", "Sin\u00b7ne", "der", "Er\u00b7f\u00fcl\u00b7lung", "harr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4cheln die andern h\u00f6hnisch, und sie karrten", "tokens": ["L\u00e4\u00b7cheln", "die", "an\u00b7dern", "h\u00f6h\u00b7nisch", ",", "und", "sie", "karr\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJD", "$,", "KON", "PPER", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schutt auf sein Veilchenbeet, das Wehmut hei\u00dft.", "tokens": ["Schutt", "auf", "sein", "Veil\u00b7chen\u00b7beet", ",", "das", "Weh\u00b7mut", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NE", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er nennt die Wahrheit Spiegel, Spiel und Pflicht.", "tokens": ["Er", "nennt", "die", "Wahr\u00b7heit", "Spie\u00b7gel", ",", "Spiel", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und offen will er seine Pfeile senden.", "tokens": ["Und", "of\u00b7fen", "will", "er", "sei\u00b7ne", "Pfei\u00b7le", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line