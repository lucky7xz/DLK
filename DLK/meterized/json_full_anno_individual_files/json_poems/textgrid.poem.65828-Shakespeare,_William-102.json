{"textgrid.poem.65828": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "102", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieben, scheinbar schw\u00e4cher, ist vermehrt;", "tokens": ["Mein", "Lie\u00b7ben", ",", "schein\u00b7bar", "schw\u00e4\u00b7cher", ",", "ist", "ver\u00b7mehrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "ADJD", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht lieb' ich minder, weil sich's mehr verhehlt;", "tokens": ["Nicht", "lieb'", "ich", "min\u00b7der", ",", "weil", "sich's", "mehr", "ver\u00b7hehlt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Eigners Zunge aller Welt erz\u00e4hlt.", "tokens": ["Des", "Eig\u00b7ners", "Zun\u00b7ge", "al\u00b7ler", "Welt", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Lenz war unsre Liebe neu; und helle", "tokens": ["Im", "Lenz", "war", "uns\u00b7re", "Lie\u00b7be", "neu", ";", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$.", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hab' ich sie da mit meinem Lied begr\u00fc\u00dft,", "tokens": ["Hab'", "ich", "sie", "da", "mit", "mei\u00b7nem", "Lied", "be\u00b7gr\u00f