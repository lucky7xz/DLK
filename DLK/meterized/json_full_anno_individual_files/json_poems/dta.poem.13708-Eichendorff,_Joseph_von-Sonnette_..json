{"dta.poem.13708": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Sind alle Eure Schiffe denn zerschlagen:", "tokens": ["Sind", "al\u00b7le", "Eu\u00b7re", "Schif\u00b7fe", "denn", "zer\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht man die heil'ge Flagge ", "tokens": ["Sieht", "man", "die", "heil'\u00b7ge", "Flag\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Liebessturm, der jene mu\u00dft' vernichten,", "tokens": ["Vom", "Lie\u00b7bes\u00b7sturm", ",", "der", "je\u00b7ne", "mu\u00dft'", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein junges Schiff siegreich hinweggetragen.", "tokens": ["Dein", "jun\u00b7ges", "Schiff", "sieg\u00b7reich", "hin\u00b7weg\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}