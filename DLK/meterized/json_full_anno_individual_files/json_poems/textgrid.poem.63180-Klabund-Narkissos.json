ie", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Der Fechter l\u00e4sst den Degen sinken,", "tokens": ["Der", "Fech\u00b7ter", "l\u00e4sst", "den", "De\u00b7gen", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Der Ringer Blick und Arm,", "tokens": ["Der", "Rin\u00b7ger", "Blick", "und", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Und selbst die Greise und die Kinder", "tokens": ["Und", "selbst", "die", "Grei\u00b7se", "und", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Erschrecken s\u00fcss vor deinem Angesicht.", "tokens": ["Er\u00b7schre\u00b7cken", "s\u00fcss", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}