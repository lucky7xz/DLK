{"dta.poem.11838": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Pyramiden, die Jahrtausenden getrotzt,", "tokens": ["Nicht", "Py\u00b7ra\u00b7mi\u00b7den", ",", "die", "Jahr\u00b7tau\u00b7sen\u00b7den", "ge\u00b7trotzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daran die Gegenwart wie Moos am Stamm schmarotzt;", "tokens": ["Da\u00b7ran", "die", "Ge\u00b7gen\u00b7wart", "wie", "Moos", "am", "Stamm", "schma\u00b7rotzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von Elefante nicht die Wunder noch Ellore,", "tokens": ["Von", "E\u00b7lef\u00b7an\u00b7te", "nicht", "die", "Wun\u00b7der", "noch", "El\u00b7lo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ART", "NN", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht am Kaukasus Alanen-Hunnenthore;", "tokens": ["Und", "nicht", "am", "Kau\u00b7ka\u00b7sus", "A\u00b7la\u00b7nen\u00b7Hunn\u00b7en\u00b7tho\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Noch eine Mauer, die ein Weltreich weit umzirkte,", "tokens": ["Noch", "ei\u00b7ne", "Mau\u00b7er", ",", "die", "ein", "Welt\u00b7reich", "weit", "um\u00b7zirk\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spricht so vom Riesengeist, womit die Urzeit wirkte,", "tokens": ["Spricht", "so", "vom", "Rie\u00b7sen\u00b7geist", ",", "wo\u00b7mit", "die", "Ur\u00b7zeit", "wirk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Alswie die Sprache, die auf einmal sie ersann,", "tokens": ["Als\u00b7wie", "die", "Spra\u00b7che", ",", "die", "auf", "ein\u00b7mal", "sie", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nicht ein neues Wort der Geist zusetzen kann:", "tokens": ["Der", "nicht", "ein", "neu\u00b7es", "Wort", "der", "Geist", "zu\u00b7set\u00b7zen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Thurm von Babel, den zum Himmel sie gebaut,", "tokens": ["Der", "Thurm", "von", "Ba\u00b7bel", ",", "den", "zum", "Him\u00b7mel", "sie", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "PRELS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dessen Zinnen sie vernahmen G\u00f6tterlaut.", "tokens": ["Von", "des\u00b7sen", "Zin\u00b7nen", "sie", "ver\u00b7nah\u00b7men", "G\u00f6t\u00b7ter\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Thurm ist umgest\u00fcrzt, der Himmel unerstiegen,", "tokens": ["Der", "Thurm", "ist", "um\u00b7ge\u00b7st\u00fcrzt", ",", "der", "Him\u00b7mel", "un\u00b7er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Davon die Sprachen nun als Tr\u00fcmmerhaufen liegen,", "tokens": ["Da\u00b7von", "die", "Spra\u00b7chen", "nun", "als", "Tr\u00fcm\u00b7mer\u00b7hau\u00b7fen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wovon mit einem je sich je ein Volk begn\u00fcgt,", "tokens": ["Wo\u00b7von", "mit", "ei\u00b7nem", "je", "sich", "je", "ein", "Volk", "be\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADV", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seinen Geistesbau daraus nothd\u00fcrftig f\u00fcgt.", "tokens": ["Und", "sei\u00b7nen", "Geis\u00b7tes\u00b7bau", "da\u00b7raus", "noth\u00b7d\u00fcrf\u00b7tig", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der umgebaute Schutt, verwitternd Jahr um Jahr,", "tokens": ["Der", "um\u00b7ge\u00b7bau\u00b7te", "Schutt", ",", "ver\u00b7wit\u00b7ternd", "Jahr", "um", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeugt im Verfall noch klar, wie stark der Urbau war.", "tokens": ["Zeugt", "im", "Ver\u00b7fall", "noch", "klar", ",", "wie", "stark", "der", "Ur\u00b7bau", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.9": {"line.1": {"text": "Nun sucht die Wissenschaft mit k\u00fcnstlicher Verk\u00fcttung", "tokens": ["Nun", "sucht", "die", "Wis\u00b7sen\u00b7schaft", "mit", "k\u00fcnst\u00b7li\u00b7cher", "Ver\u00b7k\u00fct\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Reste m\u00fchsam herzustellen die Zerr\u00fcttung.", "tokens": ["Der", "Res\u00b7te", "m\u00fch\u00b7sam", "her\u00b7zu\u00b7stel\u00b7len", "die", "Zer\u00b7r\u00fct\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Doch nur die Kunst besiegt die Stoffverk\u00fcmmerung,", "tokens": ["Doch", "nur", "die", "Kunst", "be\u00b7siegt", "die", "Stoff\u00b7ver\u00b7k\u00fcm\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Uranf\u00e4nglichs schafft auch aus Zertr\u00fcmmerung.", "tokens": ["Die", "U\u00b7ran\u00b7f\u00e4ng\u00b7lichs", "schafft", "auch", "aus", "Zer\u00b7tr\u00fcm\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}