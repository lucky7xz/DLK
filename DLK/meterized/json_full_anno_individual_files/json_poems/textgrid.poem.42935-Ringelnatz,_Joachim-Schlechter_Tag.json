{"textgrid.poem.42935": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Schlechter Tag", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcde streichen meine Finger", "tokens": ["M\u00fc\u00b7de", "strei\u00b7chen", "mei\u00b7ne", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Runzeln, \u00fcber Narben,", "tokens": ["\u00dc\u00b7ber", "Run\u00b7zeln", ",", "\u00fc\u00b7ber", "Nar\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber graue Haare.", "tokens": ["\u00dc\u00b7ber", "grau\u00b7e", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Prost, ihr Freunde, die in diesem Jahre", "tokens": ["Prost", ",", "ihr", "Freun\u00b7de", ",", "die", "in", "die\u00b7sem", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir entstarben! \u2013 Bums!!", "tokens": ["Mir", "ent\u00b7star\u00b7ben", "!", "\u2013", "Bums", "!!"], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "$(", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bums und klirr!! \u2013 Nun hab ich sozusagen", "tokens": ["Bums", "und", "klirr", "!!", "\u2013", "Nun", "hab", "ich", "so\u00b7zu\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "$.", "$(", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Instinktiv", "tokens": ["Ins\u00b7tink\u00b7tiv"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Eine Fliege totgeschlagen.", "tokens": ["Ei\u00b7ne", "Flie\u00b7ge", "tot\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "War es nicht, als ob sie Hilfe rief?!", "tokens": ["War", "es", "nicht", ",", "als", "ob", "sie", "Hil\u00b7fe", "rief", "?!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Glas kaputt. So! Und jetzt l\u00f6st mein vierter,", "tokens": ["Glas", "ka\u00b7putt", ".", "So", "!", "Und", "jetzt", "l\u00f6st", "mein", "vier\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "ADV", "$.", "KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Letzter Knopf sich scheu von Hose und Faden.", "tokens": ["Letz\u00b7ter", "Knopf", "sich", "scheu", "von", "Ho\u00b7se", "und", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "ADJD", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mu\u00df ich alles, alles ausbaden!?", "tokens": ["Mu\u00df", "ich", "al\u00b7les", ",", "al\u00b7les", "aus\u00b7ba\u00b7den", "!?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "$,", "PIS", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ach, ich werde immer deprimierter.", "tokens": ["Ach", ",", "ich", "wer\u00b7de", "im\u00b7mer", "de\u00b7pri\u00b7mier\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn doch eine Motte jetzt geflogen k\u00e4me.", "tokens": ["Wenn", "doch", "ei\u00b7ne", "Mot\u00b7te", "jetzt", "ge\u00b7flo\u00b7gen", "k\u00e4\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVPP", "VVFIN", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ach, ich w\u00fcrde sie zu Pl\u00fcschsesseln einladen.", "tokens": ["Ach", ",", "ich", "w\u00fcr\u00b7de", "sie", "zu", "Pl\u00fcschses\u00b7seln", "ein\u00b7la\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und noch Samt ihr hinlegen,", "tokens": ["Und", "noch", "Samt", "ihr", "hin\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "VVINF", "$,"], "meter": "--+--++", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weil ich mich doch wegen", "tokens": ["Weil", "ich", "mich", "doch", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der Fliege so sch\u00e4me.", "tokens": ["Der", "Flie\u