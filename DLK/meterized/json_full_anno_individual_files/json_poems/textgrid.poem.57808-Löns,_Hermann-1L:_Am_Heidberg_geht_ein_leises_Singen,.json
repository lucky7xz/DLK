{"textgrid.poem.57808": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Heidberg geht ein leises Singen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Heidberg geht ein leises Singen,", "tokens": ["Am", "Heid\u00b7berg", "geht", "ein", "lei\u00b7ses", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leises Singen her und hin,", "tokens": ["Ein", "lei\u00b7ses", "Sin\u00b7gen", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sitzt und wiegt die goldne Wiege", "tokens": ["Da", "sitzt", "und", "wiegt", "die", "gold\u00b7ne", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die tote Zwergenk\u00f6nigin.", "tokens": ["Die", "to\u00b7te", "Zwer\u00b7gen\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Frau K\u00f6nigin, Euch will ich's klagen,", "tokens": ["Frau", "K\u00f6\u00b7ni\u00b7gin", ",", "Euch", "will", "ich's", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will klagen Euch mein Herzeleid,", "tokens": ["W