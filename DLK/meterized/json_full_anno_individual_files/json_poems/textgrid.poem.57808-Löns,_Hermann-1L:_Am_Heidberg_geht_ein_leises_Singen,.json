": {"text": "Bei meinem toten Kindelein.", "tokens": ["Bei", "mei\u00b7nem", "to\u00b7ten", "Kin\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wiegt Ihr es auch sieben Jahre", "tokens": ["Und", "wiegt", "Ihr", "es", "auch", "sie\u00b7ben", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wiegt Ihr es auch immerzu,", "tokens": ["Und", "wiegt", "Ihr", "es", "auch", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es h\u00f6rt und h\u00f6rt nicht auf zu weinen,", "tokens": ["Es", "h\u00f6rt", "und", "h\u00f6rt", "nicht", "auf", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es l\u00e4\u00dft und l\u00e4\u00dft mir keine Ruh'.", "tokens": ["Es", "l\u00e4\u00dft", "und", "l\u00e4\u00dft", "mir", "kei\u00b7ne", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df ein tiefes Wasser rauschen,", "tokens": ["Ich", "wei\u00df", "ein", "tie\u00b7fes", "Was\u00b7ser", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-