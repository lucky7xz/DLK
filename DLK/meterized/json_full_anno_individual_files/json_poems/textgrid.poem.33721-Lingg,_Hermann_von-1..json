{"textgrid.poem.33721": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am lang verschleierten Gem\u00e4lde bleichen", "tokens": ["Am", "lang", "ver\u00b7schlei\u00b7er\u00b7ten", "Ge\u00b7m\u00e4l\u00b7de", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Farben endlich ab, welk wird die Bl\u00fcte,", "tokens": ["Die", "Far\u00b7ben", "end\u00b7lich", "ab", ",", "welk", "wird", "die", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich umsonst nach Licht und Sonne m\u00fchte,", "tokens": ["Die", "sich", "um\u00b7sonst", "nach", "Licht", "und", "Son\u00b7ne", "m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Kraft versiegt, kann sie nicht Ruhm erreichen.", "tokens": ["Die", "Kraft", "ver\u00b7siegt", ",", "kann", "sie", "nicht", "Ruhm", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Trug waren die verh\u00e4ngnisvollen Zeichen!", "tokens": ["Trug", "wa\u00b7ren", "die", "ver\u00b7h\u00e4ng\u00b7nis\u00b7vol\u00b7len", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verzehrt vom Feuer, das mich einst durchgl\u00fchte,", "tokens": ["Ver\u00b7zehrt", "vom", "Feu\u00b7er", ",", "das", "mich", "einst", "durch\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vom Grab der Hoffnung, das ich tatlos h\u00fcte,", "tokens": ["Vom", "Grab", "der", "Hoff\u00b7nung", ",", "das", "ich", "tat\u00b7los", "h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Holt bald der Tod mich weg wie andre Leichen.", "tokens": ["Holt", "bald", "der", "Tod", "mich", "weg", "wie", "and\u00b7re", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PPER", "ADV", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oft Nachts, wenn alle Pulse hei\u00dfer kochen,", "tokens": ["Oft", "Nachts", ",", "wenn", "al\u00b7le", "Pul\u00b7se", "hei\u00b7\u00dfer", "ko\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIAT", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Naht mir ein Geist und fl\u00fcstert voll Verh\u00f6hnung:", "tokens": ["Naht", "mir", "ein", "Geist", "und", "fl\u00fcs\u00b7tert", "voll", "Ver\u00b7h\u00f6h\u00b7nung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Titanen nur sind nicht zu unterjochen.", "tokens": ["Ti\u00b7ta\u00b7nen", "nur", "sind", "nicht", "zu", "un\u00b7ter\u00b7jo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du hast die Wahl: ergib dich in Vers\u00f6hnung", "tokens": ["Du", "hast", "die", "Wahl", ":", "er\u00b7gib", "dich", "in", "Ver\u00b7s\u00f6h\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "VVIMP", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Allgemeinlos, oder ungebrochen", "tokens": ["Dem", "All\u00b7ge\u00b7mein\u00b7los", ",", "o\u00b7der", "un\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhebe selbst die Hand zu deiner Kr\u00f6nung!", "tokens": ["Er\u00b7he\u00b7be", "selbst", "die", "Hand", "zu", "dei\u00b7ner", "Kr\u00f6\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Am lang verschleierten Gem\u00e4lde bleichen", "tokens": ["Am", "lang", "ver\u00b7schlei\u00b7er\u00b7ten", "Ge\u00b7m\u00e4l\u00b7de", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "VVI