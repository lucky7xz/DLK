{"dta.poem.19613": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Seitenwunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber ihre Thore statt der Muse", "tokens": ["Ue\u00b7ber", "ih\u00b7re", "Tho\u00b7re", "statt", "der", "Mu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mei\u00dfeln die Baglioni die Meduse", "tokens": ["Mei\u00b7\u00dfeln", "die", "Bag\u00b7li\u00b7o\u00b7ni", "die", "Me\u00b7du\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und an ihren grausen Hochzeitsfesten", "tokens": ["Und", "an", "ih\u00b7ren", "grau\u00b7sen", "Hoch\u00b7zeits\u00b7fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00e4mpft der Br\u00e4utigam mit seinen G\u00e4sten.", "tokens": ["K\u00e4mpft", "der", "Br\u00e4u\u00b7ti\u00b7gam", "mit", "sei\u00b7nen", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Heute liegen wieder sie wie Garben:", "tokens": ["Heu\u00b7te", "lie\u00b7gen", "wie\u00b7der", "sie", "wie", "Gar\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blutsgenossen, die sich w\u00fcrgend starben!", "tokens": ["Bluts\u00b7ge\u00b7nos\u00b7sen", ",", "die", "sich", "w\u00fcr\u00b7gend", "star\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo des Bruderhasses Fackel brannte,", "tokens": ["Wo", "des", "Bru\u00b7der\u00b7has\u00b7ses", "Fa\u00b7ckel", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sucht das Kind und findet's Atalante.", "tokens": ["Sucht", "das", "Kind", "und", "fin\u00b7det's", "A\u00b7tal\u00b7an\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Niederstarrend, auf das Knie gesunken,", "tokens": ["Nie\u00b7ders\u00b7tar\u00b7rend", ",", "auf", "das", "Knie", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hebt des Sohnes Haupt sie jammertrunken,", "tokens": ["Hebt", "des", "Soh\u00b7nes", "Haupt", "sie", "jam\u00b7mer\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dr\u00fcber hebt sie die geballte Rechte,", "tokens": ["Dr\u00fc\u00b7ber", "hebt", "sie", "die", "ge\u00b7ball\u00b7te", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df sie fluche diesem Mordgeschlechte ...", "tokens": ["Da\u00df", "sie", "flu\u00b7che", "die\u00b7sem", "Mord\u00b7ge\u00b7schlech\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ihrem Knaben steht die Seite offen,", "tokens": ["Ih\u00b7rem", "Kna\u00b7ben", "steht", "die", "Sei\u00b7te", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Speer Longin's den Herrn getroffen,", "tokens": ["Wo", "der", "Speer", "Lon\u00b7gin's", "den", "Herrn", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihres Knaben Haupt, ein blondes ist es,", "tokens": ["Ih\u00b7res", "Kna\u00b7ben", "Haupt", ",", "ein", "blon\u00b7des", "ist", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "ART", "ADJA", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie das dorngekr\u00f6nte Haupt des Christes ...", "tokens": ["Wie", "das", "dorn\u00b7ge\u00b7kr\u00f6n\u00b7te", "Haupt", "des", "Chris\u00b7tes", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wie des Christes Haupt ist's ein erbleichtes,", "tokens": ["Wie", "des", "Chris\u00b7tes", "Haupt", "ist's", "ein", "er\u00b7bleich\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf die Schulter friedevoll geneigtes,", "tokens": ["Auf", "die", "Schul\u00b7ter", "frie\u00b7de\u00b7voll", "ge\u00b7neig\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ha\u00df und Fluch erlischt auf ihrem Munde,", "tokens": ["Ha\u00df", "und", "Fluch", "er\u00b7lischt", "auf", "ih\u00b7rem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie verehrt die heil'ge Seitenwunde ...", "tokens": ["Sie", "ver\u00b7ehrt", "die", "heil'\u00b7ge", "Sei\u00b7ten\u00b7wun\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}