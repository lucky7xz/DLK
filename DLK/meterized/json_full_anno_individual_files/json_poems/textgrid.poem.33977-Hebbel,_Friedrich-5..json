{"textgrid.poem.33977": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott wei\u00df, wie tief der Meeresgrund,", "tokens": ["Gott", "wei\u00df", ",", "wie", "tief", "der", "Mee\u00b7res\u00b7grund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott wei\u00df, wie tief die Wunde ist!", "tokens": ["Gott", "wei\u00df", ",", "wie", "tief", "die", "Wun\u00b7de", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf ewig schlie\u00df' ich drum den Mund,", "tokens": ["Auf", "e\u00b7wig", "schlie\u00df'", "ich", "drum", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich werde dadurch nicht gesund,", "tokens": ["Ich", "wer\u00b7de", "da\u00b7durch", "nicht", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, die sie schlug, sie auch ermi\u00dft.", "tokens": ["Da\u00df", ",", "die", "sie", "schlug", ",", "sie", "auch", "er\u00b7mi\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "$."], 