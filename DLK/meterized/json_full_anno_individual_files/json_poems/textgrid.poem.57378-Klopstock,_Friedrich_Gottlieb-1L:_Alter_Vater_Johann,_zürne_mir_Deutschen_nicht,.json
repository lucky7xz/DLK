": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Nun du hast es geh\u00f6rt, wie, dich zu preisen, mir", "tokens": ["Nun", "du", "hast", "es", "ge\u00b7h\u00f6rt", ",", "wie", ",", "dich", "zu", "prei\u00b7sen", ",", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVFIN", "$,", "PWAV", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Meine schl\u00fcrfende Lippe trof!", "tokens": ["Mei\u00b7ne", "schl\u00fcr\u00b7fen\u00b7de", "Lip\u00b7pe", "trof", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Hast verziehen. Allein Wahrheit ist wahr, und bleibt's!", "tokens": ["Hast", "ver\u00b7zie\u00b7hen", ".", "Al\u00b7lein", "Wahr\u00b7heit", "ist", "wahr", ",", "und", "bleibt's", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$.", "ADV", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Deine Tochter Konstanzia", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", "Kons\u00b7tan\u00b7zia"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Blinkt einladend, wenn sie Farbe des Goldes schm\u00fcckt;", "tokens": ["Blinkt", "ein\u00b7la\u00b7dend", ",", "wenn", "sie", "Far\u00b7be", "des", "Gol\u00b7des", "schm\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$,", "KOUS", "PPER", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"t