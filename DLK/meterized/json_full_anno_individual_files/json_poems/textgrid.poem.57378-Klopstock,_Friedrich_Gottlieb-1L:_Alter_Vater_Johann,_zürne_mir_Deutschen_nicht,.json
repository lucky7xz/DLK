{"textgrid.poem.57378": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alter Vater Johann, z\u00fcrne mir Deutschen nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alter Vater Johann, z\u00fcrne mir Deutschen nicht,", "tokens": ["Al\u00b7ter", "Va\u00b7ter", "Jo\u00b7hann", ",", "z\u00fcr\u00b7ne", "mir", "Deut\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$,", "VVFIN", "PPER", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dass ich die Tochter Konstanzia", "tokens": ["Dass", "ich", "die", "Toch\u00b7ter", "Kons\u00b7tan\u00b7zia"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lieber (darf ich es auch, darf ich das trunkne Wort", "tokens": ["Lie\u00b7ber", "(", "darf", "ich", "es", "auch", ",", "darf", "ich", "das", "trunk\u00b7ne", "Wort"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "VMFIN", "PPER", "PPER", "ADV", "$,", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Wagen?) lieber sie trink' als dich.", "tokens": ["Wa\u00b7gen", "?", ")", "lie\u00b7ber", "sie", "trink'", "als", "dich", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ADV", "PPER", "VVFIN", "KOUS", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Du verzeihest vielleicht; doch die Kanoniker,", "tokens": ["Du", "ver\u00b7zei\u00b7hest", "viel\u00b7leicht", ";", "doch", "die", "Ka\u00b7no\u00b7ni\u00b7ker", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Deine S\u00e4uglinge, diese nicht!", "tokens": ["Dei\u00b7ne", "S\u00e4ug\u00b7lin\u00b7ge", ",", "die\u00b7se", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ohne Schimmer, (du liebst gl\u00e4nzende Eitelkeit,", "tokens": ["Oh\u00b7ne", "Schim\u00b7mer", ",", "(", "du", "liebst", "gl\u00e4n\u00b7zen\u00b7de", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "$(", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Liebest Blendung des Auges nicht)", "tokens": ["Lie\u00b7best", "Blen\u00b7dung", "des", "Au\u00b7ges", "nicht", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "PTKNEG", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Ruhest du in dem Kristall. Deine Ger\u00fcche sind", "tokens": ["Ru\u00b7hest", "du", "in", "dem", "Kris\u00b7tall", ".", "Dei\u00b7ne", "Ge\u00b7r\u00fc\u00b7che", "sind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Stiller St\u00e4rke Verk\u00fcndiger.", "tokens": ["Stil\u00b7ler", "St\u00e4r\u00b7ke", "Ver\u00b7k\u00fcn\u00b7di\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Guter, alter Johann, froheres Leben dringt", "tokens": ["Gu\u00b7ter", ",", "al\u00b7ter", "Jo\u00b7hann", ",", "fro\u00b7he\u00b7res", "Le\u00b7ben", "dringt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NE", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Mit dir Greisen durch Mark und Bein!", "tokens": ["Mit", "dir", "Grei\u00b7sen", "durch", "Mark", "und", "Bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Bald ist ihnen nicht mehr Kr\u00fccke der Rebenstab;", "tokens": ["Bald", "ist", "ih\u00b7nen", "nicht", "mehr", "Kr\u00fc\u00b7cke", "der", "Re\u00b7bens\u00b7tab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "PIAT", "NN", "ART", "NN", "$."], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Bald versuchen sie seinen Schwung.", "tokens": ["Bald", "ver\u00b7su\u00b7chen", "sie", "sei\u00b7nen", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Nun du hast es geh\u00f6rt, wie, dich zu preisen, mir", "tokens": ["Nun", "du", "hast", "es", "ge\u00b7h\u00f6rt", ",", "wie", ",", "dich", "zu", "prei\u00b7sen", ",", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVFIN", "$,", "PWAV", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Meine schl\u00fcrfende Lippe trof!", "tokens": ["Mei\u00b7ne", "schl\u00fcr\u00b7fen\u00b7de", "Lip\u00b7pe", "trof", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Hast verziehen. Allein Wahrheit ist wahr, und bleibt's!", "tokens": ["Hast", "ver\u00b7zie\u00b7hen", ".", "Al\u00b7lein", "Wahr\u00b7heit", "ist", "wahr", ",", "und", "bleibt's", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$.", "ADV", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Deine Tochter Konstanzia", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", "Kons\u00b7tan\u00b7zia"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Blinkt einladend, wenn sie Farbe des Goldes schm\u00fcckt;", "tokens": ["Blinkt", "ein\u00b7la\u00b7dend", ",", "wenn", "sie", "Far\u00b7be", "des", "Gol\u00b7des", "schm\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$,", "KOUS", "PPER", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Doch wenn die des erwachten Tags,", "tokens": ["Doch", "wenn", "die", "des", "er\u00b7wach\u00b7ten", "Tags", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Blinkt sie lockender, gl\u00fcht, gl\u00fcht wie die Braut, die sich", "tokens": ["Blinkt", "sie", "lo\u00b7cken\u00b7der", ",", "gl\u00fcht", ",", "gl\u00fcht", "wie", "die", "Braut", ",", "die", "sich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Nun doch auch zu gewaltig sch\u00e4mt.", "tokens": ["Nun", "doch", "auch", "zu", "ge\u00b7wal\u00b7tig", "sch\u00e4mt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Deiner Konstanzia Duft gleichet des Rbsen\u00f6hls,", "tokens": ["Dei\u00b7ner", "Kons\u00b7tan\u00b7zia", "Duft", "glei\u00b7chet", "des", "Rb\u00b7se\u00b7n\u00f6hls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.24": {"text": "Nein, gleicht dem der durchw\u00fcrzten Luft,", "tokens": ["Nein", ",", "gleicht", "dem", "der", "durch\u00b7w\u00fcrz\u00b7ten", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Welche trinkt der Pilot, wenn ihm der Wimpel wehr", "tokens": ["Wel\u00b7che", "trinkt", "der", "Pi\u00b7lot", ",", "wenn", "ihm", "der", "Wim\u00b7pel", "wehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Nach den Inseln der Seligen.", "tokens": ["Nach", "den", "In\u00b7seln", "der", "Se\u00b7li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.27": {"text": "Dicht mit Eichen bekr\u00e4nzt, wandte sich Tmolus einst,", "tokens": ["Dicht", "mit", "Ei\u00b7chen", "be\u00b7kr\u00e4nzt", ",", "wand\u00b7te", "sich", "Tmo\u00b7lus", "einst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,", "VVFIN", "PRF", "NE", "ADV", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.28": {"text": "Mit dem Gotte der Wald, dass hin", "tokens": ["Mit", "dem", "Got\u00b7te", "der", "Wald", ",", "dass", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Bis ins tiefere Thal rauschten die Bl\u00e4tter, bis", "tokens": ["Bis", "ins", "tie\u00b7fe\u00b7re", "Thal", "rauschten", "die", "Bl\u00e4t\u00b7ter", ",", "bis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KOUS"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Zu dem fliehenden Reh; so hast,", "tokens": ["Zu", "dem", "flie\u00b7hen\u00b7den", "Reh", ";", "so", "hast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.31": {"text": "Alter Vater Johann, du dich nach mir gewandt,", "tokens": ["Al\u00b7ter", "Va\u00b7ter", "Jo\u00b7hann", ",", "du", "dich", "nach", "mir", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$,", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.32": {"text": "Und zurauschend Konstanzia's", "tokens": ["Und", "zu\u00b7rau\u00b7schend", "Kon\u00b7stan\u00b7zia's"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVPP", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Preis vernommen. Sie hat br\u00e4utliche R\u00f6the! sie", "tokens": ["Preis", "ver\u00b7nom\u00b7men", ".", "Sie", "hat", "br\u00e4ut\u00b7li\u00b7che", "R\u00f6\u00b7the", "!", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVPP", "$.", "PPER", "VAFIN", "ADJA", "NN", "$.", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Duftet, wie Inseln der Seligen!", "tokens": ["Duf\u00b7tet", ",", "wie", "In\u00b7seln", "der", "Se\u00b7li\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Und die S\u00fcsse, mit der sie auf die Zunge rint,", "tokens": ["Und", "die", "S\u00fcs\u00b7se", ",", "mit", "der", "sie", "auf", "die", "Zun\u00b7ge", "rint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Rann aus dem Nektarpokale nicht.", "tokens": ["Rann", "aus", "dem", "Nek\u00b7tar\u00b7po\u00b7ka\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Aber wer sitzet dir denn in dem beeichelten", "tokens": ["A\u00b7ber", "wer", "sit\u00b7zet", "dir", "denn", "in", "dem", "be\u00b7ei\u00b7chel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.38": {"text": "Kranze, zechend mit wildem Schrey,", "tokens": ["Kran\u00b7ze", ",", "ze\u00b7chend", "mit", "wil\u00b7dem", "Schrey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.39": {"text": "Dass dein Laub dir erbebt? Ha die Kanoniker", "tokens": ["Dass", "dein", "Laub", "dir", "er\u00b7bebt", "?", "Ha", "die", "Ka\u00b7no\u00b7ni\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "NE", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.40": {"text": "Sind es! Wehe mir! sind's, und ich", "tokens": ["Sind", "es", "!", "We\u00b7he", "mir", "!", "sin\u00b7d's", ",", "und", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "$.", "NN", "PPER", "$.", "VAFIN", "$,", "KON", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "Bin verloren! Das Lob deiner Konstanzia", "tokens": ["Bin", "ver\u00b7lo\u00b7ren", "!", "Das", "Lob", "dei\u00b7ner", "Kons\u00b7tan\u00b7zia"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.42": {"text": "Hat zur Rache sie gegen mich", "tokens": ["Hat", "zur", "Ra\u00b7che", "sie", "ge\u00b7gen", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "APPR", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.43": {"text": "Angeflammet! Schon schliefst tobend ihr Kreis mich ein!", "tokens": ["An\u00b7ge\u00b7flam\u00b7met", "!", "Schon", "schliefst", "to\u00b7bend", "ihr", "Kreis", "mich", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.44": {"text": "Schon zerschmettern sie, st\u00fcmmeln mich,", "tokens": ["Schon", "zer\u00b7schmet\u00b7tern", "sie", ",", "st\u00fcm\u00b7meln", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.45": {"text": "Und wie vor Alters sein Haupt Orpheus im Hebrus floss,", "tokens": ["Und", "wie", "vor", "Al\u00b7ters", "sein", "Haupt", "Or\u00b7pheus", "im", "Heb\u00b7rus", "floss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "PPOSAT", "NN", "NE", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "Fliesset mir in dem Rhein das Haupt!", "tokens": ["Flies\u00b7set", "mir", "in", "dem", "Rhein", "das", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Aber die Stimm' ist auch mir todt nicht, Konstanzia", "tokens": ["A\u00b7ber", "die", "Stimm'", "ist", "auch", "mir", "todt", "nicht", ",", "Kons\u00b7tan\u00b7zia"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PPER", "ADJD", "PTKNEG", "$,", "NE"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.48": {"text": "Ruft sie, die starrende Zunge ruft", "tokens": ["Ruft", "sie", ",", "die", "star\u