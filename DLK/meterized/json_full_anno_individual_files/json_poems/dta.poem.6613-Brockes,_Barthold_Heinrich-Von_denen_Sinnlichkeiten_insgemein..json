{"dta.poem.6613": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bleibt immer der Verband der Dinge, welche sich", "tokens": ["Bleibt", "im\u00b7mer", "der", "Ver\u00b7band", "der", "Din\u00b7ge", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ver\u00e4ndern, unver\u00e4nderlich.", "tokens": ["Ver\u00b7\u00e4n\u00b7dern", ",", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt erhielte das, vom ersten Tage an,", "tokens": ["Die", "Welt", "er\u00b7hiel\u00b7te", "das", ",", "vom", "ers\u00b7ten", "Ta\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PDS", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wodurch sie sich verneu\u2019n und stets ver\u00e4ndern kan.", "tokens": ["Wo\u00b7durch", "sie", "sich", "ver\u00b7neu'n", "und", "stets", "ver\u00b7\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "KON", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}