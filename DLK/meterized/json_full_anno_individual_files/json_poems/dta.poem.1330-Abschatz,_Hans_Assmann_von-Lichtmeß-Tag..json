{"dta.poem.1330": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Lichtme\u00df-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du must bey diesem Tag/ o Hertze/ nicht vergessen/", "tokens": ["Du", "must", "bey", "die\u00b7sem", "Tag", "/", "o", "Hert\u00b7ze", "/", "nicht", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "$(", "FM", "VVFIN", "$(", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dir vorl\u00e4ngst das Licht des Lebens abgemessen/", "tokens": ["Da\u00df", "dir", "vor\u00b7l\u00e4ngst", "das", "Licht", "des", "Le\u00b7bens", "ab\u00b7ge\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mi\u00df deine Zeit so ab/ da\u00df du im Lichte gehst/", "tokens": ["Mi\u00df", "dei\u00b7ne", "Zeit", "so", "ab", "/", "da\u00df", "du", "im", "Lich\u00b7te", "gehst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "PTKVZ", "$(", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und/ wenn denn alles k\u00f6mmt ans Licht/ f\u00fcr GOtt bestehst.", "tokens": ["Und", "/", "wenn", "denn", "al\u00b7les", "k\u00f6mmt", "ans", "Licht", "/", "f\u00fcr", "Gott", "be\u00b7stehst",