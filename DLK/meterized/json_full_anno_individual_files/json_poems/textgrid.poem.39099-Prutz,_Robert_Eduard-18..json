{"textgrid.poem.39099": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ihr teuren Kirchenlichter,", "tokens": ["Ach", "ihr", "teu\u00b7ren", "Kir\u00b7chen\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die ihr einst so hell gestrahlt,", "tokens": ["die", "ihr", "einst", "so", "hell", "ge\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die ihr in die Angesichter", "tokens": ["die", "ihr", "in", "die", "An\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "so viel Falten euch gemalt:", "tokens": ["so", "viel", "Fal\u00b7ten", "euch", "ge\u00b7malt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach wie jetzt so anders steht ihr", "tokens": ["Ach", "wie", "jetzt", "so", "an\u00b7ders", "steht", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "ADV", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in der Kirche zu Sankt Paul!", "tokens": ["in", "der", "Kir\u00b7che", "zu", "Sankt", "Paul", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTK