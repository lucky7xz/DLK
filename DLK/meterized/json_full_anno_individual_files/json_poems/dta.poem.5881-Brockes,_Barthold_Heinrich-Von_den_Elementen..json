{"dta.poem.5881": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Moschus, (er verdient, da\u00df sein Ruhm ewig sey.)", "tokens": ["Ein", "Mo\u00b7schus", ",", "(", "er", "ver\u00b7dient", ",", "da\u00df", "sein", "Ruhm", "e\u00b7wig", "sey", ".", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NE", "$,", "$(", "PPER", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entwarff zuerst dies sch\u00f6ne Lehr-Geb\u00e4u.", "tokens": ["Ent\u00b7warff", "zu\u00b7erst", "dies", "sch\u00f6\u00b7ne", "Lehr\u00b7Ge\u00b7b\u00e4u", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn dem Phoenicier allein", "tokens": ["Denn", "dem", "Phoe\u00b7ni\u00b7cier", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Philosophie den ersten neuen Schein", "tokens": ["Wird", "die", "Phi\u00b7lo\u00b7so\u00b7phie", "den", "ers\u00b7ten", "neu\u00b7en", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In ihrer Wieg, unstreitig schuldig seyn.", "tokens": ["In", "ih\u00b7rer", "Wieg", ",", "un\u00b7strei\u00b7tig", "schul\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es werden ihm mit gr\u00f6ssern Rechte", "tokens": ["Es", "wer\u00b7den", "ihm", "mit", "gr\u00f6s\u00b7sern", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sieges-Palmen zugeh\u00f6ren,", "tokens": ["Die", "Sie\u00b7ges\u00b7Pal\u00b7men", "zu\u00b7ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als denen, die die Kunst dem menschlichen Geschlechte", "tokens": ["Als", "de\u00b7nen", ",", "die", "die", "Kunst", "dem", "menschli\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Er\u00f6ffneten, das Meer mit Schiffen zu beschweren.", "tokens": ["Er\u00b7\u00f6ff\u00b7ne\u00b7ten", ",", "das", "Meer", "mit", "Schif\u00b7fen", "zu", "be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als denen ebenfalls, die durch die Kunst zu schreiben,", "tokens": ["Als", "de\u00b7nen", "e\u00b7ben\u00b7falls", ",", "die", "durch", "die", "Kunst", "zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die W\u00f6rter unserm Blick vermochten vorzustellen.", "tokens": ["Die", "W\u00f6r\u00b7ter", "un\u00b7serm", "Blick", "ver\u00b7moch\u00b7ten", "vor\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn jene sich bestrebt, als Herr\u2019n von Wind und Wellen,", "tokens": ["Wenn", "je\u00b7ne", "sich", "be\u00b7strebt", ",", "als", "Herr'n", "von", "Wind", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PRF", "VVFIN", "$,", "KOUS", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "(den steiffen Blick zum Pol-Stern hingekehrt,", "tokens": ["(", "den", "steif\u00b7fen", "Blick", "zum", "Pol\u00b7S\u00b7tern", "hin\u00b7ge\u00b7kehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der durch ein stetes Licht uns sichre Wege lehrt)", "tokens": ["Der", "durch", "ein", "ste\u00b7tes", "Licht", "uns", "sich\u00b7re", "We\u00b7ge", "lehrt", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jhr Schiff an ein entlegnes Land zu treiben:", "tokens": ["Ihr", "Schiff", "an", "ein", "ent\u00b7leg\u00b7nes", "Land", "zu", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Erfunden die die Kunst, die wol unsterblich heist,", "tokens": ["Er\u00b7fun\u00b7den", "die", "die", "Kunst", ",", "die", "wol", "uns\u00b7terb\u00b7lich", "heist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wodurch sich der Verg\u00e4nglichkeit", "tokens": ["Wo\u00b7durch", "sich", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So manche Helden-That entreisst,", "tokens": ["So", "man\u00b7che", "Hel\u00b7den\u00b7That", "en\u00b7treisst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wodurch zugleich, ob sie gleich noch so weit", "tokens": ["Wo\u00b7durch", "zu\u00b7gleich", ",", "ob", "sie", "gleich", "noch", "so", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sich von einander trennen,", "tokens": ["Sich", "von", "ein\u00b7an\u00b7der", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Die Handlung, Freundschafft, Treu und Pflicht,", "tokens": ["Die", "Hand\u00b7lung", ",", "Freund\u00b7schafft", ",", "Treu", "und", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Von allen Orten her sich unterhalten k\u00f6nnen;", "tokens": ["Von", "al\u00b7len", "Or\u00b7ten", "her", "sich", "un\u00b7ter\u00b7hal\u00b7ten", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APZR", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hat Dieser, da sein Geist die ungemessne Tieffe,", "tokens": ["Hat", "Die\u00b7ser", ",", "da", "sein", "Geist", "die", "un\u00b7ge\u00b7mess\u00b7ne", "Tief\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Worinn der Zeug der Welt f\u00fcr uns verstecket,", "tokens": ["Wo\u00b7rinn", "der", "Zeug", "der", "Welt", "f\u00fcr", "uns", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Mit einem scharffen Blick durchlieffe,", "tokens": ["Mit", "ei\u00b7nem", "scharf\u00b7fen", "Blick", "durch\u00b7lief\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der C\u00f6rper ungez\u00e4hlte Zahl", "tokens": ["Der", "C\u00f6r\u00b7per", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Zuallererst entdecket,", "tokens": ["Zu\u00b7al\u00b7le\u00b7rerst", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Die so verschiedentlich geformt, sich seltsam f\u00fcgen,", "tokens": ["Die", "so", "ver\u00b7schie\u00b7dent\u00b7lich", "ge\u00b7formt", ",", "sich", "selt\u00b7sam", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$,", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die ein geheimer Trieb beweget allemahl.", "tokens": ["Die", "ein", "ge\u00b7hei\u00b7mer", "Trieb", "be\u00b7we\u00b7get", "al\u00b7le\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}