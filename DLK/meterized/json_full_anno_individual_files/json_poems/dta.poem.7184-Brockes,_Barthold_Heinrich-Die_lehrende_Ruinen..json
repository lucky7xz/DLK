{"dta.poem.7184": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Ruinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ber\u00fchmter Obrist Lieutenant,", "tokens": ["Be\u00b7r\u00fchm\u00b7ter", "O\u00b7brist", "Lie\u00b7u\u00b7ten\u00b7ant", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Versuchter ", "tokens": ["Ver\u00b7such\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "An Tapferkeit, Erfahrung und Verstand,", "tokens": ["An", "Tap\u00b7fer\u00b7keit", ",", "Er\u00b7fah\u00b7rung", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zumal im Festungs-Bau, der Erd-me\u00df-Zeichnungs-Kunst,", "tokens": ["Zu\u00b7mal", "im", "Fes\u00b7tungs\u00b7Bau", ",", "der", "Erd\u00b7me\u00df\u00b7Zeich\u00b7nungs\u00b7Kunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Absonderlich an weit entfernten Reisen,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "an", "weit", "ent\u00b7fern\u00b7ten", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Du durch Ottomans und Deines K\u00f6nigs Gunst,", "tokens": ["Die", "Du", "durch", "Ot\u00b7to\u00b7mans", "und", "Dei\u00b7nes", "K\u00f6\u00b7nigs", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm Orient gethan, wie Deine Risse weisen,", "tokens": ["Jm", "O\u00b7rient", "ge\u00b7than", ",", "wie", "Dei\u00b7ne", "Ris\u00b7se", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Nicht Deines gleichen kennst; was hast Du uns gezeigt!", "tokens": ["Nicht", "Dei\u00b7nes", "glei\u00b7chen", "kennst", ";", "was", "hast", "Du", "uns", "ge\u00b7zeigt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir haben was gesehn, so alles \u00fcbersteigt", "tokens": ["Wir", "ha\u00b7ben", "was", "ge\u00b7sehn", ",", "so", "al\u00b7les", "\u00fc\u00b7bers\u00b7teigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "VVPP", "$,", "ADV", "PIS", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was Hamburg sonst erblickt.", "tokens": ["Was", "Ham\u00b7burg", "sonst", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}