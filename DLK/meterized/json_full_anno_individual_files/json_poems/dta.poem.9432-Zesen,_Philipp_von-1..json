{"dta.poem.9432": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich suchte des nachtes im bette mein Leben/", "tokens": ["Ich", "such\u00b7te", "des", "nach\u00b7tes", "im", "bet\u00b7te", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPRART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den Liebsten ich suchte doch fand ich Jhn nicht;", "tokens": ["Den", "Liebs\u00b7ten", "ich", "such\u00b7te", "doch", "fand", "ich", "Jhn", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich konte mich nirgend zu frieden ergeben/", "tokens": ["Ich", "kon\u00b7te", "mich", "nir\u00b7gend", "zu", "frie\u00b7den", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich weinte von hertzen und suchte mein Licht.", "tokens": ["Ich", "wein\u00b7te", "von", "hert\u00b7zen", "und", "such\u00b7te", "mein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Liebes-brunst zwang mich das bette zula\u00dfen", "tokens": ["Die", "Lie\u00b7bes\u00b7brunst", "zwang", "mich", "das", "bet\u00b7te", "zu\u00b7la\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDS", "VVFIN", "VVINF"], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ich eylte mit macht", "tokens": ["Ich", "eyl\u00b7te", "mit", "macht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Durch schatten und nacht/", "tokens": ["Durch", "schat\u00b7ten", "und", "nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Lief traurig ernieder und fehlte der stra\u00dfen/", "tokens": ["Lief", "trau\u00b7rig", "er\u00b7nie\u00b7der", "und", "fehl\u00b7te", "der", "stra\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "KON", "VVFIN", "ART", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Ich fragte die wacht", "tokens": ["Ich", "frag\u00b7te", "die", "wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Objemand den Liebsten genommen in acht?", "tokens": ["Ob\u00b7je\u00b7mand", "den", "Liebs\u00b7ten", "ge\u00b7nom\u00b7men", "in", "acht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "APPR", "CARD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Doch fand ich jhn nicht", "tokens": ["Doch", "fand", "ich", "jhn", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Mein Leben und Licht/", "tokens": ["Mein", "Le\u00b7ben", "und", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Ich ginge fort/ sihe! da kam Er gegangen/", "tokens": ["Ich", "gin\u00b7ge", "fort", "/", "si\u00b7he", "!", "da", "kam", "Er", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "VVIMP", "$.", "ADV", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Da fand ich mein Licht./", "tokens": ["Da", "fand", "ich", "mein", "Licht", ".", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "O Liebster mein Leben mein bestes Verlangen/", "tokens": ["O", "Liebs\u00b7ter", "mein", "Le\u00b7ben", "mein", "bes\u00b7tes", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.16": {"text": "Ich halte Mich an Dich und la\u00dfe dich nicht.", "tokens": ["Ich", "hal\u00b7te", "Mich", "an", "Dich", "und", "la\u00b7\u00dfe", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Ich f\u00fchre dich Liebster/ bin immer bem\u00fchet/", "tokens": ["Ich", "f\u00fch\u00b7re", "dich", "Liebs\u00b7ter", "/", "bin", "im\u00b7mer", "be\u00b7m\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Da\u00df meine geliebete Mutter dich sihet/", "tokens": ["Da\u00df", "mei\u00b7ne", "ge\u00b7lie\u00b7be\u00b7te", "Mut\u00b7ter", "dich", "si\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.19": {"text": "Nun folge mein Licht/", "tokens": ["Nun", "fol\u00b7ge", "mein", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Ich la\u00dfe dich nicht.", "tokens": ["Ich", "la\u00b7\u00dfe", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}