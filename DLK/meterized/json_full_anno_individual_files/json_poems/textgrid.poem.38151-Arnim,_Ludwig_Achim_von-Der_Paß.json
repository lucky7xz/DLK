{"textgrid.poem.38151": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pa\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ns Salzburger M\u00e4dl,", "tokens": ["Sch\u00f6ns", "Salz\u00b7bur\u00b7ger", "M\u00e4dl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit dem krausen H\u00e4rl,", "tokens": ["Mit", "dem", "krau\u00b7sen", "H\u00e4rl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Thust mir \u00fcberaus gefallen,", "tokens": ["Thust", "mir", "\u00fc\u00b7be\u00b7raus", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann ich dich seh gehen,", "tokens": ["Wann", "ich", "dich", "seh", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bleib ich allzeit stehen,", "tokens": ["Bleib", "ich", "all\u00b7zeit", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und betrachte dich vor andern allen,", "tokens": ["Und", "be\u00b7trach\u00b7te", "dich", "vor", "an\u00b7dern", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIS", "PIAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Deine schwarze Hauben", "tokens": ["Dei\u00b7ne", "schwar\u00b7ze", "Hau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sticht mir in die Augen,", "tokens": ["Sticht", "mir", "in", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Mit deinen g\u00fcldnen Borten;", "tokens": ["Mit", "dei\u00b7nen", "g\u00fcld\u00b7nen", "Bor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bin ich z'Haus allein,", "tokens": ["Bin", "ich", "z'\u00b7Haus", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Fallt mirs wieder ein,", "tokens": ["Fallt", "mirs", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Dirnerl w\u00e4r ich bey dir dorten.", "tokens": ["Dir\u00b7nerl", "w\u00e4r", "ich", "bey", "dir", "dor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich fragen darf,", "tokens": ["Wenn", "ich", "fra\u00b7gen", "darf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mein, wer ist der Herr,", "tokens": ["Mein", ",", "wer", "ist", "der", "Herr", ","], "token_info": ["word", "punct"