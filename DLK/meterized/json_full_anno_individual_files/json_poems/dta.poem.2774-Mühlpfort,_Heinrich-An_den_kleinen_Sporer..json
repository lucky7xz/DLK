{"dta.poem.2774": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An den kleinen Sporer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sporer Stern und Licht/ Herr O\u00dfwald seyd gegr\u00fc\u00dft/", "tokens": ["Der", "Spo\u00b7rer", "Stern", "und", "Licht", "/", "Herr", "O\u00df\u00b7wald", "seyd", "ge\u00b7gr\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "NN", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil heut euer Namens-Tag/ das Leben euch vers\u00fc\u00dft/", "tokens": ["Weil", "heut", "eu\u00b7er", "Na\u00b7mens\u00b7Tag", "/", "das", "Le\u00b7ben", "euch", "ver\u00b7s\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$(", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Und ihr sechs Schillge Jahr samt zweyen habt erlebet/", "tokens": ["Und", "ihr", "sechs", "Schill\u00b7ge", "Jahr", "samt", "zwe\u00b7yen", "habt", "er\u00b7le\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "CARD", "NN", "NN", "ADJD", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch stets nach Wissenschafft der alten Welt gestrebet/", "tokens": ["Auch", "stets", "nach", "Wis\u00b7sen\u00b7schafft", "der", "al\u00b7ten", "Welt", "ge\u00b7stre\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel Dinge selbst erfahren/ ob sich die Schwachheit gleich/", "tokens": ["Viel", "Din\u00b7ge", "selbst", "er\u00b7fah\u00b7ren", "/", "ob", "sich", "die", "Schwach\u00b7heit", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$(", "KOUS", "PRF", "ART", "NN", "ADV", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Auch h\u00e4uffig an euch weist/ so ist euer Brunn doch reich", "tokens": ["Auch", "h\u00e4uf\u00b7fig", "an", "euch", "weist", "/", "so", "ist", "eu\u00b7er", "Brunn", "doch", "reich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "An frischem Wasserquell/ und kan es schon ersetzen/", "tokens": ["An", "fri\u00b7schem", "Was\u00b7ser\u00b7quell", "/", "und", "kan", "es", "schon", "er\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was euch ja sonsten fehlt/ der Bauren ihr Ergetzen/", "tokens": ["Was", "euch", "ja", "sons\u00b7ten", "fehlt", "/", "der", "Bau\u00b7ren", "ihr", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Beruht auf diesem Trunck/ wenn ihr den Stengel mahlt/", "tokens": ["Be\u00b7ruht", "auf", "die\u00b7sem", "Trunck", "/", "wenn", "ihr", "den", "Sten\u00b7gel", "mahlt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df hernach ein Gesicht gleich schwartzen Mohren strahlt", "tokens": ["Da\u00df", "her\u00b7nach", "ein", "Ge\u00b7sicht", "gleich", "schwart\u00b7zen", "Moh\u00b7ren", "strahlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es w\u00fcnschet Stadt und Dorff euch noch ein l\u00e4nger Leben/", "tokens": ["Es", "w\u00fcn\u00b7schet", "Stadt", "und", "Dorff", "euch", "noch", "ein", "l\u00e4n\u00b7ger", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Wasser ihr der Welt zum besten k\u00f6nnet geben.", "tokens": ["Da\u00df", "Was\u00b7ser", "ihr", "der", "Welt", "zum", "bes\u00b7ten", "k\u00f6n\u00b7net", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "APPRART", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Heut aber diesen Tag da \u00fcbersend ich Wein/", "tokens": ["Heut", "a\u00b7ber", "die\u00b7sen", "Tag", "da", "\u00fc\u00b7ber\u00b7send", "ich", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "NN", "ADV", "VVPP", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den schl\u00fcrffet durch den Zahn fein sacht in Hals hinein.", "tokens": ["Den", "schl\u00fcrf\u00b7fet", "durch", "den", "Zahn", "fein", "sacht", "in", "Hals", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "ADJD", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "La\u00dft eure Weisheit h\u00f6ren/ und lehret das die Schwachen/", "tokens": ["La\u00dft", "eu\u00b7re", "Weis\u00b7heit", "h\u00f6\u00b7ren", "/", "und", "leh\u00b7ret", "das", "die", "Schwa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$(", "KON", "VVFIN", "ART", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Am allerk\u00fcnstlichsten nachdencken ihren Sachen.", "tokens": ["Am", "al\u00b7ler\u00b7k\u00fcnst\u00b7lichs\u00b7ten", "nach\u00b7den\u00b7cken", "ih\u00b7ren", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.17": {"text": "Wenn eures Hauptes Schnee zergeht/ wirds kotig seyn/", "tokens": ["Wenn", "eu\u00b7res", "Haup\u00b7tes", "Schnee", "zer\u00b7geht", "/", "wirds", "ko\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$(", "VAFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Brunn vertrocknet gar. Und weil ihr Heuchelschein", "tokens": ["Der", "Brunn", "ver\u00b7trock\u00b7net", "gar", ".", "Und", "weil", "ihr", "Heu\u00b7chel\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "KON", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und falsche Reden ha\u00dft/ mein Teutsch-gesinnter Alter/", "tokens": ["Und", "fal\u00b7sche", "Re\u00b7den", "ha\u00dft", "/", "mein", "Teut\u00b7schge\u00b7sinn\u00b7ter", "Al\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Meister Kron allhier/ und w\u00fcrdig Brunn/ Verwalter/", "tokens": ["Der", "Meis\u00b7ter", "Kron", "all\u00b7hier", "/", "und", "w\u00fcr\u00b7dig", "Brunn", "/", "Ver\u00b7wal\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "$(", "KON", "ADJD", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So thut auff Redlichkeit und Teutsch fein einen Trunck;", "tokens": ["So", "thut", "auff", "Red\u00b7lich\u00b7keit", "und", "Teutsch", "fein", "ei\u00b7nen", "Trunck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn diesen heut\u2019gen Tag seht ihr nicht mehr so jung.", "tokens": ["Denn", "die\u00b7sen", "heut'\u00b7gen", "Tag", "seht", "ihr", "nicht", "mehr", "so", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}