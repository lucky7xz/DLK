{"dta.poem.12667": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die falsche liebe.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr gem\u00fcther!", "tokens": ["Ihr", "ge\u00b7m\u00fc\u00b7ther", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Zieht der falschen liebe splitter", "tokens": ["Zieht", "der", "fal\u00b7schen", "lie\u00b7be", "split\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch aus eurer brust;", "tokens": ["Doch", "aus", "eu\u00b7rer", "brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenig lust", "tokens": ["We\u00b7nig", "lust"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Bringen ihre sch\u00e4tze.", "tokens": ["Brin\u00b7gen", "ih\u00b7re", "sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Meine seele", "tokens": ["Mei\u00b7ne", "see\u00b7le"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Kennet schon die dunckle h\u00f6hle,", "tokens": ["Ken\u00b7net", "schon", "die", "dunck\u00b7le", "h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die unlust liegt;", "tokens": ["Wo", "die", "un\u00b7lust", "liegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Unvergn\u00fcgt", "tokens": ["Un\u00b7ver\u00b7gn\u00fcgt"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ist die liebes-grotte.", "tokens": ["Ist", "die", "lie\u00b7bes\u00b7\u00b7grot\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Noth und jammer", "tokens": ["Noth", "und", "jam\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Haben da die n\u00e4chste kammer,", "tokens": ["Ha\u00b7ben", "da", "die", "n\u00e4chs\u00b7te", "kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die liebe wohnt;", "tokens": ["Wo", "die", "lie\u00b7be", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sie verschont", "tokens": ["Sie", "ver\u00b7schont"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nicht die sch\u00f6nsten hertzen.", "tokens": ["Nicht", "die", "sch\u00f6ns\u00b7ten", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Drum zerreisset,", "tokens": ["Drum", "zer\u00b7reis\u00b7set", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Was nach dieser liebe heisset.", "tokens": ["Was", "nach", "die\u00b7ser", "lie\u00b7be", "heis\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werfft die sehnsucht hin;", "tokens": ["Werfft", "die", "sehn\u00b7sucht", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Der gewinn", "tokens": ["Der", "ge\u00b7winn"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Riecht nach bittren myrrhen.", "tokens": ["Riecht", "nach", "bit\u00b7tren", "myr\u00b7rhen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du, mein hertze!", "tokens": ["Du", ",", "mein", "hert\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Fleuch der liebe falsche schertze,", "tokens": ["Fleuch", "der", "lie\u00b7be", "fal\u00b7sche", "schert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr dampff-altar;", "tokens": ["Und", "ihr", "dampf\u00b7fal\u00b7tar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eine bahr", "tokens": ["Ei\u00b7ne", "bahr"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ist ihr wohllust-bette.", "tokens": ["Ist", "ihr", "wohl\u00b7lust\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}