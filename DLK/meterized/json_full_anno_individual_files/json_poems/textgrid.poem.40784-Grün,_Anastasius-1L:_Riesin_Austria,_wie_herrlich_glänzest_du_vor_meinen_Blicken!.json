{"textgrid.poem.40784": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Riesin Austria, wie herrlich gl\u00e4nzest du vor meinen Blicken!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Riesin Austria, wie herrlich gl\u00e4nzest du vor meinen Blicken!", "tokens": ["Rie\u00b7sin", "Aust\u00b7ria", ",", "wie", "herr\u00b7lich", "gl\u00e4n\u00b7zest", "du", "vor", "mei\u00b7nen", "Bli\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Eine blanke Mauerkrone seh' ich stolz das Haupt dir schm\u00fccken,", "tokens": ["Ei\u00b7ne", "blan\u00b7ke", "Mau\u00b7er\u00b7kro\u00b7ne", "seh'", "ich", "stolz", "das", "Haupt", "dir", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Weicher Locken \u00fcpp'ge F\u00fclle reich auf deine Schultern fallen", "tokens": ["Wei\u00b7cher", "Lo\u00b7cken", "\u00fcpp'\u00b7ge", "F\u00fcl\u00b7le", "reich", "auf", "dei\u00b7ne", "Schul\u00b7tern", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Blonden Golds, wie deine Saaten, die im Winde fr\u00f6hlich wallen.", "tokens": ["Blon\u00b7den", "Golds", ",", "wie", "dei\u00b7ne", "Saa\u00b7ten", ",", "die", "im", "Win\u00b7de", "fr\u00f6h\u00b7lich", "wal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Festlich prangt dein Leib, der wonn'ge, in dem gr\u00fcnen Sammtgewande,", "tokens": ["Fest\u00b7lich", "prangt", "dein", "Leib", ",", "der", "wonn'\u00b7ge", ",", "in", "dem", "gr\u00fc\u00b7nen", "Sammt\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dran als Silbergurt die Donau, und die Rebe als Guirlande;", "tokens": ["Dran", "als", "Sil\u00b7ber\u00b7gurt", "die", "Do\u00b7nau", ",", "und", "die", "Re\u00b7be", "als", "Gu\u00b7ir\u00b7lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "NN", "ART", "NE", "$,", "KON", "ART", "NN", "KOUS", "NN", "$."], "meter": "+-+-+--+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Leuchtend flammt dein Schild, der blanke, welchem Lerch' und Aar entsteigen,", "tokens": ["Leuch\u00b7tend", "flammt", "dein", "Schild", ",", "der", "blan\u00b7ke", ",", "wel\u00b7chem", "Ler\u00b7ch'", "und", "Aar", "ent\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "PWAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Aller Welt von deinem B\u00fcndni\u00df mit dem Tag und Licht zu zeigen!", "tokens": ["Al\u00b7ler", "Welt", "von", "dei\u00b7nem", "B\u00fcn\u00b7dni\u00df", "mit", "dem", "Tag", "und", "Licht", "zu", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Farbig ist ein Blumenestrich dir zu F\u00fc\u00dfen aufgegangen,", "tokens": ["Far\u00b7big", "ist", "ein", "Blu\u00b7me\u00b7ne\u00b7strich", "dir", "zu", "F\u00fc\u00b7\u00dfen", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Eine Garde stolzer Eichen seh' ich im Gefolg dir prangen,", "tokens": ["Ei\u00b7ne", "Gar\u00b7de", "stol\u00b7zer", "Ei\u00b7chen", "seh'", "ich", "im", "Ge\u00b7folg", "dir", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "K\u00f6n'gen gleich in Purpurm\u00e4nteln deine hohen Berge ragen,", "tokens": ["K\u00f6n'\u00b7gen", "gleich", "in", "Pur\u00b7pur\u00b7m\u00e4n\u00b7teln", "dei\u00b7ne", "ho\u00b7hen", "Ber\u00b7ge", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Die als Kronen schmucke Burgen hell im Morgenrothe tragen.", "tokens": ["Die", "als", "Kro\u00b7nen", "schmu\u00b7cke", "Bur\u00b7gen", "hell", "im", "Mor\u00b7gen\u00b7ro\u00b7the", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "VVFIN", "NN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Hier bist du die Braut, die heitre, unter Bl\u00fcthen an der Quelle,", "tokens": ["Hier", "bist", "du", "die", "Braut", ",", "die", "heit\u00b7re", ",", "un\u00b7ter", "Bl\u00fc\u00b7then", "an", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Kr\u00e4nzend sich mit Perl' und Rose, spiegelnd sich in klarer Welle!", "tokens": ["Kr\u00e4n\u00b7zend", "sich", "mit", "Perl'", "und", "Ro\u00b7se", ",", "spie\u00b7gelnd", "sich", "in", "kla\u00b7rer", "Wel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "KON", "NE", "$,", "ADJD", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dort gleich muth'ger Amazone nach ersiegter Schlacht zu schauen,", "tokens": ["Dort", "gleich", "muth'\u00b7ger", "A\u00b7ma\u00b7zo\u00b7ne", "nach", "er\u00b7sieg\u00b7ter", "Schlacht", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Erzumpanzert und gewaltig, doch voll Sch\u00f6nheit selbst das Grauen!", "tokens": ["Er\u00b7zum\u00b7pan\u00b7zert", "und", "ge\u00b7wal\u00b7tig", ",", "doch", "voll", "Sch\u00f6n\u00b7heit", "selbst", "das", "Grau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "ADV", "ADJD", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wie im hohen G\u00f6ttertempel glorreich einst Pallas-Athene,", "tokens": ["Wie", "im", "ho\u00b7hen", "G\u00f6t\u00b7ter\u00b7tem\u00b7pel", "glor\u00b7reich", "einst", "Pal\u00b7las\u00b7A\u00b7the\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ADJD", "ADV", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Stehst du da in stiller Weisheit, heil'ger Kraft und milder Sch\u00f6ne!", "tokens": ["Stehst", "du", "da", "in", "stil\u00b7ler", "Weis\u00b7heit", ",", "heil'\u00b7ger", "Kraft", "und", "mil\u00b7der", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aus den lieben s\u00fc\u00dfen Augen mu\u00df ein hoher Geist auch spr\u00fchen,", "tokens": ["Aus", "den", "lie\u00b7ben", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen", "mu\u00df", "ein", "ho\u00b7her", "Geist", "auch", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Unterm \u00fcpp'gen sch\u00f6nen Busen dir ein edles Herz auch gl\u00fchen.", "tokens": ["Un\u00b7term", "\u00fcpp'\u00b7gen", "sch\u00f6\u00b7nen", "Bu\u00b7sen", "dir", "ein", "ed\u00b7les", "Herz", "auch", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "In der Hand des Wissens B\u00fccher h\u00e4ltst du siegreich aufgeschlagen,", "tokens": ["In", "der", "Hand", "des", "Wis\u00b7sens", "B\u00fc\u00b7cher", "h\u00e4ltst", "du", "sieg\u00b7reich", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wissend, da\u00df, wie deine Saaten, sie manch goldnes K\u00f6rnlein tragen,", "tokens": ["Wis\u00b7send", ",", "da\u00df", ",", "wie", "dei\u00b7ne", "Saa\u00b7ten", ",", "sie", "manch", "gold\u00b7nes", "K\u00f6rn\u00b7lein", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "$,", "PWAV", "PPOSAT", "NN", "$,", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df, wer hat gesunde Augen, Tageslicht vertragen lerne,", "tokens": ["Da\u00df", ",", "wer", "hat", "ge\u00b7sun\u00b7de", "Au\u00b7gen", ",", "Ta\u00b7ges\u00b7licht", "ver\u00b7tra\u00b7gen", "ler\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "VAFIN", "ADJA", "NN", "$,", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und noch keine H\u00fctt' in Flammen ward gesteckt durchs Licht der Sterne.", "tokens": ["Und", "noch", "kei\u00b7ne", "H\u00fctt'", "in", "Flam\u00b7men", "ward", "ge\u00b7steckt", "durchs", "Licht", "der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Erz ber\u00fchrt und Stein und Leinwand deine Zauberhand nur sachte,", "tokens": ["Erz", "be\u00b7r\u00fchrt", "und", "Stein", "und", "Lein\u00b7wand", "dei\u00b7ne", "Zau\u00b7ber\u00b7hand", "nur", "sach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "KON", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sieh, da als ein Gott lebendig springt der Marmor aus dem Schachte,", "tokens": ["Sieh", ",", "da", "als", "ein", "Gott", "le\u00b7ben\u00b7dig", "springt", "der", "Mar\u00b7mor", "aus", "dem", "Schach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "KOKOM", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sieh, da lebt und spricht die Leinwand, fr\u00f6hlich klingen die Metalle,", "tokens": ["Sieh", ",", "da", "lebt", "und", "spricht", "die", "Lein\u00b7wand", ",", "fr\u00f6h\u00b7lich", "klin\u00b7gen", "die", "Me\u00b7tal\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und der Kunst geweihte Dome ragen hoch zur Sternenhalle!", "tokens": ["Und", "der", "Kunst", "ge\u00b7weih\u00b7te", "Do\u00b7me", "ra\u00b7gen", "hoch", "zur", "Ster\u00b7nen\u00b7hal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Freiheit prangt als heil'ge Losung \u00fcber deinen Friedensh\u00fctten,", "tokens": ["Frei\u00b7heit", "prangt", "als", "heil'\u00b7ge", "Lo\u00b7sung", "\u00fc\u00b7ber", "dei\u00b7nen", "Frie\u00b7dens\u00b7h\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Freiheit gl\u00e4nzt auf allen Bannern, drunter je dein Volk gestritten;", "tokens": ["Frei\u00b7heit", "gl\u00e4nzt", "auf", "al\u00b7len", "Ban\u00b7nern", ",", "drun\u00b7ter", "je", "dein", "Volk", "ge\u00b7strit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,", "PAV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Besser als die H\u00e4nd' in Fesseln taugen dir die fessellosen,", "tokens": ["Bes\u00b7ser", "als", "die", "H\u00e4nd'", "in", "Fes\u00b7seln", "tau\u00b7gen", "dir", "die", "fes\u00b7sel\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sei's das Schwert der Schlacht zu schwingen, sei's zu pfl\u00fccken Friedensrosen.", "tokens": ["Sei's", "das", "Schwert", "der", "Schlacht", "zu", "schwin\u00b7gen", ",", "sei's", "zu", "pfl\u00fc\u00b7cken", "Frie\u00b7dens\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "PIS", "PTKZU", "VVINF", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Doch: Vertrauen! hei\u00dft die Fessel, die dir gilt, dein Volk zu binden,", "tokens": ["Doch", ":", "Ver\u00b7trau\u00b7en", "!", "hei\u00dft", "die", "Fes\u00b7sel", ",", "die", "dir", "gilt", ",", "dein", "Volk", "zu", "bin\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "NN", "$.", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und um Br\u00fcder sie und Br\u00fcder und um F\u00fcrst und Volk zu winden;", "tokens": ["Und", "um", "Br\u00fc\u00b7der", "sie", "und", "Br\u00fc\u00b7der", "und", "um", "F\u00fcrst", "und", "Volk", "zu", "win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "KON", "NN", "KON", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "m