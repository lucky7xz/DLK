{"dta.poem.2839": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Kindchen, das zuerst auf seinen F\u00fc\u00dfchen steht,", "tokens": ["Ein", "Kind\u00b7chen", ",", "das", "zu\u00b7erst", "auf", "sei\u00b7nen", "F\u00fc\u00df\u00b7chen", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst zagend einen Schritt, dann wagend einen geht,", "tokens": ["Erst", "za\u00b7gend", "ei\u00b7nen", "Schritt", ",", "dann", "wa\u00b7gend", "ei\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie hat es mich gefreut, wie hat es mich ger\u00fchrt,", "tokens": ["Wie", "hat", "es", "mich", "ge\u00b7freut", ",", "wie", "hat", "es", "mich", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Vorstellungen mir weit hinaus gef\u00fchrt,", "tokens": ["Und", "die", "Vor\u00b7stel\u00b7lun\u00b7gen", "mir", "weit", "hin\u00b7aus", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ADJD", "APZR", "VVPP", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "In seine Zukunft, wann der Mann die Kraft gewann,", "tokens": ["In", "sei\u00b7ne", "Zu\u00b7kunft", ",", "wann", "der", "Mann", "die", "Kraft", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die geistig stehn und gehn auf eignen F\u00fc\u00dfen kann.", "tokens": ["Die", "geis\u00b7tig", "stehn", "und", "gehn", "auf", "eig\u00b7nen", "F\u00fc\u00b7\u00dfen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "KON", "VVFIN", "APPR", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}