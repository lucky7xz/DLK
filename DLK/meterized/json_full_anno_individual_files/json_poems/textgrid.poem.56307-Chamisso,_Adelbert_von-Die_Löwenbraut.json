{"textgrid.poem.56307": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die L\u00f6wenbraut", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der Myrte geschm\u00fcckt und dem Brautgeschmeid,", "tokens": ["Mit", "der", "Myr\u00b7te", "ge\u00b7schm\u00fcckt", "und", "dem", "Braut\u00b7ge\u00b7schmeid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Des W\u00e4rters Tochter, die rosige Maid,", "tokens": ["Des", "W\u00e4r\u00b7ters", "Toch\u00b7ter", ",", "die", "ro\u00b7si\u00b7ge", "Maid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tritt ein in den Zwinger des L\u00f6wen; er liegt", "tokens": ["Tritt", "ein", "in", "den", "Zwin\u00b7ger", "des", "L\u00f6\u00b7wen", ";", "er", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Herrin zu F\u00fc\u00dfen, vor der er sich schmiegt.", "tokens": ["Der", "Her\u00b7rin", "zu", "F\u00fc\u00b7\u00dfen", ",", "vor", "der", "er", "sich", "schmiegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Gewaltige, wild und unb\u00e4ndig zuvor,", "tokens": ["Der", "Ge\u00b7wal\u00b7ti\u00b7ge", ",", "wild", "und", "un\u00b7b\u00e4n\u00b7dig", "zu\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "--+--+-+---+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schaut fromm und verst\u00e4ndig zur Herrin empor;", "tokens": ["Schaut", "fromm", "und", "ver\u00b7st\u00e4n\u00b7dig", "zur", "Her\u00b7rin", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Jungfrau, zart und wonnereich,", "tokens": ["Die", "Jung\u00b7frau", ",", "zart", "und", "won\u00b7ne\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liebstreichelt ihn sanft und weinet zugleich:", "tokens": ["Liebs\u00b7trei\u00b7chelt", "ihn", "sanft", "und", "wei\u00b7net", "zu\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbwir waren in Tagen, die nicht mehr sind,", "tokens": ["\u00bb", "wir", "wa\u00b7ren", "in", "Ta\u00b7gen", ",", "die", "nicht", "mehr", "sind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gar treue Gespielen wie Kind und Kind,", "tokens": ["Gar", "treu\u00b7e", "Ge\u00b7spie\u00b7len", "wie", "Kind", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hatten uns lieb, und hatten uns gern;", "tokens": ["Und", "hat\u00b7ten", "uns", "lieb", ",", "und", "hat\u00b7ten", "uns", "gern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "VAFIN", "PPER", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Tage der Kindheit, sie liegen uns fern.", "tokens": ["Die", "Ta\u00b7ge", "der", "Kind\u00b7heit", ",", "sie", "lie\u00b7gen", "uns", "fern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Du sch\u00fctteltest machtvoll, eh wir's geglaubt,", "tokens": ["Du", "sch\u00fct\u00b7tel\u00b7test", "macht\u00b7voll", ",", "eh", "wir's", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dein m\u00e4hnen-umwogtes, k\u00f6niglich Haupt;", "tokens": ["Dein", "m\u00e4h\u00b7nen\u00b7um\u00b7wog\u00b7tes", ",", "k\u00f6\u00b7nig\u00b7lich", "Haupt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wuchs heran, du siehst es, ich bin", "tokens": ["Ich", "wuchs", "he\u00b7ran", ",", "du", "siehst", "es", ",", "ich", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das Kind nicht mehr mit kindischem Sinn.", "tokens": ["Das", "Kind", "nicht", "mehr", "mit", "kin\u00b7di\u00b7schem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "O w\u00e4r ich das Kind noch und bliebe bei dir,", "tokens": ["O", "w\u00e4r", "ich", "das", "Kind", "noch", "und", "blie\u00b7be", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "ADV", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mein starkes, getreues, mein redliches Tier;", "tokens": ["Mein", "star\u00b7kes", ",", "ge\u00b7treu\u00b7es", ",", "mein", "red\u00b7li\u00b7ches", "Tier", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich aber mu\u00df folgen, sie taten's mir an,", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "fol\u00b7gen", ",", "sie", "ta\u00b7ten's", "mir", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hinaus in die Fremde dem fremden Mann.", "tokens": ["Hin\u00b7aus", "in", "die", "Frem\u00b7de", "dem", "frem\u00b7den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Es fiel ihm ein, da\u00df sch\u00f6n ich sei,", "tokens": ["Es", "fiel", "ihm", "ein", ",", "da\u00df", "sch\u00f6n", "ich", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wurde gefreiet, es ist nun vorbei; \u2013", "tokens": ["Ich", "wur\u00b7de", "ge\u00b7frei\u00b7et", ",", "es", "ist", "nun", "vor\u00b7bei", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Kranz im Haare, mein guter Gesell,", "tokens": ["Der", "Kranz", "im", "Haa\u00b7re", ",", "mein", "gu\u00b7ter", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nicht vor Tr\u00e4nen die Blicke mehr hell.", "tokens": ["Und", "nicht", "vor", "Tr\u00e4\u00b7nen", "die", "Bli\u00b7cke", "mehr", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Verstehst du mich ganz? schaust grimmig dazu;", "tokens": ["Ver\u00b7stehst", "du", "mich", "ganz", "?", "schaust", "grim\u00b7mig", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin ja gefa\u00dft, sei ruhig auch du;", "tokens": ["Ich", "bin", "ja", "ge\u00b7fa\u00dft", ",", "sei", "ru\u00b7hig", "auch", "du", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "VAFIN", "ADJD", "ADV", "PPER", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort seh ich ihn kommen, dem folgen ich mu\u00df,", "tokens": ["Dort", "seh", "ich", "ihn", "kom\u00b7men", ",", "dem", "fol\u00b7gen", "ich", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVINF", "$,", "PRELS", "VVFIN", "PPER", "VMFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So geb ich denn, Freund, dir den letzten Ku\u00df!\u00ab", "tokens": ["So", "geb", "ich", "denn", ",", "Freund", ",", "dir", "den", "letz\u00b7ten", "Ku\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Und wie ihn die Lippe des M\u00e4dchens ber\u00fchrt,", "tokens": ["Und", "wie", "ihn", "die", "Lip\u00b7pe", "des", "M\u00e4d\u00b7chens", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da hat man den Zwinger erzittern gesp\u00fcrt;", "tokens": ["Da", "hat", "man", "den", "Zwin\u00b7ger", "er\u00b7zit\u00b7tern", "ge\u00b7sp\u00fcrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wie er am Gitter den J\u00fcngling erschaut,", "tokens": ["Und", "wie", "er", "am", "Git\u00b7ter", "den", "J\u00fcng\u00b7ling", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Erfa\u00dft Entsetzen die bangende Braut.", "tokens": ["Er\u00b7fa\u00dft", "Ent\u00b7set\u00b7zen", "die", "ban\u00b7gen\u00b7de", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Er stellt an die T\u00fcr sich des Zwingers zur Wacht,", "tokens": ["Er", "stellt", "an", "die", "T\u00fcr", "sich", "des", "Zwin\u00b7gers", "zur", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er schwinget den Schweif, er br\u00fcllet mit Macht;", "tokens": ["Er", "schwin\u00b7get", "den", "Schweif", ",", "er", "br\u00fcl\u00b7let", "mit", "Macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie flehend, gebietend und drohend begehrt", "tokens": ["Sie", "fle\u00b7hend", ",", "ge\u00b7bie\u00b7tend", "und", "dro\u00b7hend", "be\u00b7gehrt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "VVPP", "KON", "ADJD", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hinaus; er im Zorn den Ausgang wehrt.", "tokens": ["Hin\u00b7aus", ";", "er", "im", "Zorn", "den", "Aus\u00b7gang", "wehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Und drau\u00dfen erhebt sich verworren Geschrei,", "tokens": ["Und", "drau\u00b7\u00dfen", "er\u00b7hebt", "sich", "ver\u00b7wor\u00b7ren", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der J\u00fcngling ruft: \u00bbBringt Waffen herbei;", "tokens": ["Der", "J\u00fcng\u00b7ling", "ruft", ":", "\u00bb", "Bringt", "Waf\u00b7fen", "her\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich schie\u00df ihn nieder, ich treff ihn gut!\u00ab", "tokens": ["Ich", "schie\u00df", "ihn", "nie\u00b7der", ",", "ich", "treff", "ihn", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf br\u00fcllt der Gereizte, sch\u00e4umend vor Wut.", "tokens": ["Auf", "br\u00fcllt", "der", "Ge\u00b7reiz\u00b7te", ",", "sch\u00e4u\u00b7mend", "vor", "Wut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Die Unselige wagt's, sich der T\u00fcre zu nahn,", "tokens": ["Die", "Un\u00b7se\u00b7li\u00b7ge", "wagt's", ",", "sich", "der", "T\u00fc\u00b7re", "zu", "nahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRF", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da f\u00e4llt er verwandelt die Herrin an;", "tokens": ["Da", "f\u00e4llt", "er", "ver\u00b7wan\u00b7delt", "die", "Her\u00b7rin", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die sch\u00f6ne Gestalt, ein gr\u00e4\u00dflicher Raub,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ge\u00b7stalt", ",", "ein", "gr\u00e4\u00df\u00b7li\u00b7cher", "Raub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Liegt blutig, zerrissen, entstellt in dem Staub.", "tokens": ["Liegt", "blu\u00b7tig", ",", "zer\u00b7ris\u00b7sen", ",", "ent\u00b7stellt", "in", "dem", "Staub", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Und wie er vergossen das teure Blut,", "tokens": ["Und", "wie", "er", "ver\u00b7gos\u00b7sen", "das", "teu\u00b7re", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er legt sich zur Leiche mit finsterem Mut,", "tokens": ["Er", "legt", "sich", "zur", "Lei\u00b7che", "mit", "fins\u00b7te\u00b7rem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er liegt so versunken in Trauer und Schmerz,", "tokens": ["Er", "liegt", "so", "ver\u00b7sun\u00b7ken", "in", "Trau\u00b7er", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bis t\u00f6dtlich die Kugel ihn trifft in das Herz.", "tokens": ["Bis", "t\u00f6dt\u00b7lich", "die", "Ku\u00b7gel", "ihn", "trifft", "in", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.13": {"line.1": {"text": "Mit der Myrte geschm\u00fcckt und dem Brautgeschmeid,", "tokens": ["Mit", "der", "Myr\u00b7te", "ge\u00b7schm\u00fcckt", "und", "dem", "Braut\u00b7ge\u00b7schmeid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Des W\u00e4rters Tochter, die rosige Maid,", "tokens": ["Des", "W\u00e4r\u00b7ters", "Toch\u00b7ter", ",", "die", "ro\u00b7si\u00b7ge", "Maid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tritt ein in den Zwinger des L\u00f6wen; er liegt", "tokens": ["Tritt", "ein", "in", "den", "Zwin\u00b7ger", "des", "L\u00f6\u00b7wen", ";", "er", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Herrin zu F\u00fc\u00dfen, vor der er sich schmiegt.", "tokens": ["Der", "Her\u00b7rin", "zu", "F\u00fc\u00b7\u00dfen", ",", "vor", "der", "er", "sich", "schmiegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Der Gewaltige, wild und unb\u00e4ndig zuvor,", "tokens": ["Der", "Ge\u00b7wal\u00b7ti\u00b7ge", ",", "wild", "und", "un\u00b7b\u00e4n\u00b7dig", "zu\u00b7vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "--+--+-+---+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schaut fromm und verst\u00e4ndig zur Herrin empor;", "tokens": ["Schaut", "fromm", "und", "ver\u00b7st\u00e4n\u00b7dig", "zur", "Her\u00b7rin", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Jungfrau, zart und wonnereich,", "tokens": ["Die", "Jung\u00b7frau", ",", "zart", "und", "won\u00b7ne\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liebstreichelt ihn sanft und weinet zugleich:", "tokens": ["Liebs\u00b7trei\u00b7chelt", "ihn", "sanft", "und", "wei\u00b7net", "zu\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "\u00bbwir waren in Tagen, die nicht mehr sind,", "tokens": ["\u00bb", "wir", "wa\u00b7ren", "in", "Ta\u00b7gen", ",", "die", "nicht", "mehr", "sind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gar treue Gespielen wie Kind und Kind,", "tokens": ["Gar", "treu\u00b7e", "Ge\u00b7spie\u00b7len", "wie", "Kind", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und hatten uns lieb, und hatten uns gern;", "tokens": ["Und", "hat\u00b7ten", "uns", "lieb", ",", "und", "hat\u00b7ten", "uns", "gern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "VAFIN", "PPER", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Tage der Kindheit, sie liegen uns fern.", "tokens": ["Die", "Ta\u00b7ge", "der", "Kind\u00b7heit", ",", "sie", "lie\u00b7gen", "uns", "fern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.16": {"line.1": {"text": "Du sch\u00fctteltest machtvoll, eh wir's geglaubt,", "tokens": ["Du", "sch\u00fct\u00b7tel\u00b7test", "macht\u00b7voll", ",", "eh", "wir's", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dein m\u00e4hnen-umwogtes, k\u00f6niglich Haupt;", "tokens": ["Dein", "m\u00e4h\u00b7nen\u00b7um\u00b7wog\u00b7tes", ",", "k\u00f6\u00b7nig\u00b7lich", "Haupt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wuchs heran, du siehst es, ich bin", "tokens": ["Ich", "wuchs", "he\u00b7ran", ",", "du", "siehst", "es", ",", "ich", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das Kind nicht mehr mit kindischem Sinn.", "tokens": ["Das", "Kind", "nicht", "mehr", "mit", "kin\u00b7di\u00b7schem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.17": {"line.1": {"text": "O w\u00e4r ich das Kind noch und bliebe bei dir,", "tokens": ["O", "w\u00e4r", "ich", "das", "Kind", "noch", "und", "blie\u00b7be", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "ADV", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mein starkes, getreues, mein redliches Tier;", "tokens": ["Mein", "star\u00b7kes", ",", "ge\u00b7treu\u00b7es", ",", "mein", "red\u00b7li\u00b7ches", "Tier", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich aber mu\u00df folgen, sie taten's mir an,", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "fol\u00b7gen", ",", "sie", "ta\u00b7ten's", "mir", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hinaus in die Fremde dem fremden Mann.", "tokens": ["Hin\u00b7aus", "in", "die", "Frem\u00b7de", "dem", "frem\u00b7den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.18": {"line.1": {"text": "Es fiel ihm ein, da\u00df sch\u00f6n ich sei,", "tokens": ["Es", "fiel", "ihm", "ein", ",", "da\u00df", "sch\u00f6n", "ich", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wurde gefreiet, es ist nun vorbei; \u2013", "tokens": ["Ich", "wur\u00b7de", "ge\u00b7frei\u00b7et", ",", "es", "ist", "nun", "vor\u00b7bei", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Kranz im Haare, mein guter Gesell,", "tokens": ["Der", "Kranz", "im", "Haa\u00b7re", ",", "mein", "gu\u00b7ter", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nicht vor Tr\u00e4nen die Blicke mehr hell.", "tokens": ["Und", "nicht", "vor", "Tr\u00e4\u00b7nen", "die", "Bli\u00b7cke", "mehr", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Verstehst du mich ganz? schaust grimmig dazu;", "tokens": ["Ver\u00b7stehst", "du", "mich", "ganz", "?", "schaust", "grim\u00b7mig", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin ja gefa\u00dft, sei ruhig auch du;", "tokens": ["Ich", "bin", "ja", "ge\u00b7fa\u00dft", ",", "sei", "ru\u00b7hig", "auch", "du", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "VAFIN", "ADJD", "ADV", "PPER", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort seh ich ihn kommen, dem folgen ich mu\u00df,", "tokens": ["Dort", "seh", "ich", "ihn", "kom\u00b7men", ",", "dem", "fol\u00b7gen", "ich", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVINF", "$,", "PRELS", "VVFIN", "PPER", "VMFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So geb ich denn, Freund, dir den letzten Ku\u00df!\u00ab", "tokens": ["So", "geb", "ich", "denn", ",", "Freund", ",", "dir", "den", "letz\u00b7ten", "Ku\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.20": {"line.1": {"text": "Und wie ihn die Lippe des M\u00e4dchens ber\u00fchrt,", "tokens": ["Und", "wie", "ihn", "die", "Lip\u00b7pe", "des", "M\u00e4d\u00b7chens", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da hat man den Zwinger erzittern gesp\u00fcrt;", "tokens": ["Da", "hat", "man", "den", "Zwin\u00b7ger", "er\u00b7zit\u00b7tern", "ge\u00b7sp\u00fcrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wie er am Gitter den J\u00fcngling erschaut,", "tokens": ["Und", "wie", "er", "am", "Git\u00b7ter", "den", "J\u00fcng\u00b7ling", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Erfa\u