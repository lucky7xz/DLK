{"dta.poem.6149": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Subtilen Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie leicht er aber auch; dennoch verlieret", "tokens": ["Wie", "leicht", "er", "a\u00b7ber", "auch", ";", "den\u00b7noch", "ver\u00b7lie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "ADV", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er seine Kr\u00e4ffte nicht. Er kan von Klingen,", "tokens": ["Er", "sei\u00b7ne", "Kr\u00e4ff\u00b7te", "nicht", ".", "Er", "kan", "von", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "$.", "PPER", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So Lemnos selbst polirt,", "tokens": ["So", "Lem\u00b7nos", "selbst", "po\u00b7lirt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die H\u00e4rtigkeit des Stahls entfernen und bezwingen.", "tokens": ["Die", "H\u00e4r\u00b7tig\u00b7keit", "des", "Stahls", "ent\u00b7fer\u00b7nen", "und", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will offtermahlen Mars sein Schwerdt zur Probe biegen,", "tokens": ["Will", "off\u00b7ter\u00b7mah\u00b7len", "Mars", "sein", "Schwerdt", "zur", "Pro\u00b7be", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da\u00df die kleinen C\u00f6rperlein,", "tokens": ["Und", "da\u00df", "die", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So durch die Fluht geh\u00e4rtet seyn,", "tokens": ["So", "durch", "die", "Fluht", "ge\u00b7h\u00e4r\u00b7tet", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Indem sie krumm, die kleinen L\u00f6cher f\u00fcgen;", "tokens": ["In\u00b7dem", "sie", "krumm", ",", "die", "klei\u00b7nen", "L\u00f6\u00b7cher", "f\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So bahnt er sich den Weg, dringt in die Kling\u2019, und zwinget", "tokens": ["So", "bahnt", "er", "sich", "den", "Weg", ",", "dringt", "in", "die", "Kling'", ",", "und", "zwin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dieselbige, da\u00df sie sogleich gerade springet.", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", ",", "da\u00df", "sie", "sog\u00b7leich", "ge\u00b7ra\u00b7de", "sprin\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}