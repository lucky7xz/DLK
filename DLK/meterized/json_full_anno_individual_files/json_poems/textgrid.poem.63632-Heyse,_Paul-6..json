{"textgrid.poem.63632": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Augen weg, die ernsten Kinderaugen,", "tokens": ["Die", "Au\u00b7gen", "weg", ",", "die", "erns\u00b7ten", "Kin\u00b7der\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die unverr\u00fcckt mir \u00fcberm Bette strahlen,", "tokens": ["Die", "un\u00b7ver\u00b7r\u00fcckt", "mir", "\u00fc\u00b7berm", "Bet\u00b7te", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir Freud' und Frieden aus der Seele saugen", "tokens": ["Mir", "Freud'", "und", "Frie\u00b7den", "aus", "der", "See\u00b7le", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich zu Asche gl\u00fchn in Sehnsuchtsqualen!", "tokens": ["Und", "mich", "zu", "A\u00b7sche", "gl\u00fchn", "in", "Sehn\u00b7suchts\u00b7qua\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie fragen: mu\u00dften wir denn untergehn,", "tokens": ["Sie", "fra\u00b7gen", ":", "mu\u00df\u00b7ten", "wir", "denn", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "wor