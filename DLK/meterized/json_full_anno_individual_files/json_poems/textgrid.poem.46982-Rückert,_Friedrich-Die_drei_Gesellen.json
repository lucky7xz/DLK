{"textgrid.poem.46982": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Gesellen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren drei Gesellen,", "tokens": ["Es", "wa\u00b7ren", "drei", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die stritten wider'n Feind,", "tokens": ["Die", "strit\u00b7ten", "wi\u00b7der'n", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und th\u00e4ten stets sich stellen", "tokens": ["Und", "th\u00e4\u00b7ten", "stets", "sich", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In jedem Kampf vereint.", "tokens": ["In", "je\u00b7dem", "Kampf", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ein' ein \u00d6sterreicher,", "tokens": ["Der", "ein'", "ein", "\u00d6s\u00b7ter\u00b7rei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der andr' ein Preu\u00dfe hie\u00df,", "tokens": ["Der", "andr'", "ein", "Preu\u00b7\u00dfe", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Davon sein Land mit gleicher", "tokens": ["Da\u00b7von", "sein", "Land", "mit", "glei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gewalt ein jeder pries.", "tokens": ["Ge\u00b7walt", "ein", "je\u00b7der", "pries", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Woher war denn der dritte?", "tokens": ["Wo\u00b7her", "war", "denn", "der", "drit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nicht her von \u00d6streichs Flur,", "tokens": ["Nicht", "her", "von", "\u00d6s\u00b7treichs", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Auch nicht von Preu\u00dfens Sitte,", "tokens": ["Auch", "nicht", "von", "Preu\u00b7\u00dfens", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von Deutschland war er nur.", "tokens": ["Von", "Deutschland", "war", "er", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und als die drei einst wieder", "tokens": ["Und", "als", "die", "drei", "einst", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "CARD", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Standen im Kampf vereint,", "tokens": ["Stan\u00b7den", "im", "Kampf", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da warf in ihre Glieder", "tokens": ["Da", "warf", "in", "ih\u00b7re", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kart\u00e4tschensaat der Feind.", "tokens": ["Kar\u00b7t\u00e4t\u00b7schen\u00b7saat", "der", "Feind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da fielen alle dreie", "tokens": ["Da", "fie\u00b7len", "al\u00b7le", "drei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "CARD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf einen Schlag zugleich;", "tokens": ["Auf", "ei\u00b7nen", "Schlag", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der eine rief mit Schreie:", "tokens": ["Der", "ei\u00b7ne", "rief", "mit", "Schrei\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hoch lebe \u00d6sterreich!", "tokens": ["Hoch", "le\u00b7be", "\u00d6s\u00b7ter\u00b7reich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Der andre, sich entf\u00e4rbend,", "tokens": ["Der", "and\u00b7re", ",", "sich", "ent\u00b7f\u00e4r\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Rief: Preu\u00dfen lebe hoch!", "tokens": ["Rief", ":", "Preu\u00b7\u00dfen", "le\u00b7be", "hoch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der dritte, ruhig sterbend,", "tokens": ["Der", "drit\u00b7te", ",", "ru\u00b7hig", "ster\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Was rief der dritte doch?", "tokens": ["Was", "rief", "der", "drit\u00b7te", "doch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er rief: Deutschland soll leben!", "tokens": ["Er", "rief", ":", "Deutschland", "soll", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "VMFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da h\u00f6rten es die zwei,", "tokens": ["Da", "h\u00f6r\u00b7ten", "es", "die", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie rechts und links daneben", "tokens": ["Wie", "rechts", "und", "links", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "KON", "ADV", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sanken nah' dabei;", "tokens": ["Sie", "san\u00b7ken", "nah'", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da richteten im Sinken", "tokens": ["Da", "rich\u00b7te\u00b7ten", "im", "Sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich beide nach ihm hin,", "tokens": ["Sich", "bei\u00b7de", "nach", "ihm", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zur Rechten und zur Linken,", "tokens": ["Zur", "Rech\u00b7ten", "und", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und lehnten sich an ihn.", "tokens": ["Und", "lehn\u00b7ten", "sich", "an", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Da rief der in der Mitten", "tokens": ["Da", "rief", "der", "in", "der", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Noch einmal: Deutschland hoch!", "tokens": ["Noch", "ein\u00b7mal", ":", "Deutschland", "hoch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NE", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und beide mit dem dritten", "tokens": ["Und", "bei\u00b7de", "mit", "dem", "drit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Riefen's, und lauter noch.", "tokens": ["Rie\u00b7fen's", ",", "und", "lau\u00b7ter", "noch", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da ging ein Todesengel", "tokens": ["Da", "ging", "ein", "To\u00b7de\u00b7sen\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Kampfgew\u00fchl vorbei,", "tokens": ["Im", "Kampf\u00b7ge\u00b7w\u00fchl", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit einem Palmenstengel,", "tokens": ["Mit", "ei\u00b7nem", "Pal\u00b7mens\u00b7ten\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und liegen sah die drei.", "tokens": ["Und", "lie\u00b7gen", "sah", "die", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sah auf ihrem Munde", "tokens": ["Er", "sah", "auf", "ih\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Spur des Wortes noch,", "tokens": ["Die", "Spur", "des", "Wor\u00b7tes", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie sie im Todesbunde", "tokens": ["Wie", "sie", "im", "To\u00b7des\u00b7bun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gerufen: Deutschland hoch!", "tokens": ["Ge\u00b7ru\u00b7fen", ":", "Deutschland", "hoch", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "NE", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Da schlug er seine Fl\u00fcgel", "tokens": ["Da", "schlug", "er", "sei\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Um alle drei zugleich", "tokens": ["Um", "al\u00b7le", "drei", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "CARD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und trug zum h\u00f6chsten H\u00fcgel", "tokens": ["Und", "trug", "zum", "h\u00f6chs\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sie auf in Gottes Reich.", "tokens": ["Sie", "auf", "in", "Got\u00b7tes", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es waren drei Gesellen,", "tokens": ["Es", "wa\u00b7ren", "drei", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die stritten wider'n Feind,", "tokens": ["Die", "strit\u00b7ten", "wi\u00b7der'n", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und th\u00e4ten stets sich stellen", "tokens": ["Und", "th\u00e4\u0