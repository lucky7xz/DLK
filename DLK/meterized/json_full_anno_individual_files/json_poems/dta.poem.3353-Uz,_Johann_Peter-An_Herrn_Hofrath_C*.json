{"dta.poem.3353": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath C*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, wann der schwarzumw\u00f6lkte S\u00fcd,", "tokens": ["Wie", ",", "wann", "der", "schwar\u00b7zum\u00b7w\u00f6lk\u00b7te", "S\u00fcd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dessen finstrer Stirn ein w\u00fcthend Feuer gl\u00fcht,", "tokens": ["Auf", "des\u00b7sen", "finst\u00b7rer", "Stirn", "ein", "w\u00fct\u00b7hend", "Feu\u00b7er", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Am regenvollen Himmel br\u00fcllet,", "tokens": ["Am", "re\u00b7gen\u00b7vol\u00b7len", "Him\u00b7mel", "br\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihm aus Scythien, in schauernd Eis verh\u00fcllet,", "tokens": ["Und", "ihm", "aus", "Scyt\u00b7hien", ",", "in", "schau\u00b7ernd", "Eis", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NE", "$,", "APPR", "ADJD", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Der kalte Nord entgegen zieht;", "tokens": ["Der", "kal\u00b7te", "Nord", "ent\u00b7ge\u00b7gen", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von ihrem Kampf die Luft erzittert,", "tokens": ["Von", "ih\u00b7rem", "Kampf", "die", "Luft", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Erden Veste bebt, und im erschrocknen Hayn", "tokens": ["Der", "Er\u00b7den", "Ves\u00b7te", "bebt", ",", "und", "im", "er\u00b7schrock\u00b7nen", "Hayn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was sich nicht beuget, kracht und splittert,", "tokens": ["Was", "sich", "nicht", "beu\u00b7get", ",", "kracht", "und", "split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und alles taumelnd seufzt, vom furchtbarn Sturm", "tokens": ["Und", "al\u00b7les", "tau\u00b7melnd", "seufzt", ",", "vom", "furcht\u00b7barn", "Sturm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ersch\u00fcttert:", "tokens": ["er\u00b7sch\u00fct\u00b7tert", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "So nahm Get\u00f6s und L\u00e4rm den ganzen Tempel ein:", "tokens": ["So", "nahm", "Ge\u00b7t\u00f6s", "und", "L\u00e4rm", "den", "gan\u00b7zen", "Tem\u00b7pel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}