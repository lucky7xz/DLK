{"textgrid.poem.55108": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du siehst so ernst, Geliebter! Deinem Bilde", "tokens": ["Du", "siehst", "so", "ernst", ",", "Ge\u00b7lieb\u00b7ter", "!", "Dei\u00b7nem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Marmor hier m\u00f6cht ich dich wohl vergleichen;", "tokens": ["Von", "Mar\u00b7mor", "hier", "m\u00f6cht", "ich", "dich", "wohl", "ver\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie dieses gibst du mir kein Lebenszeichen", "tokens": ["Wie", "die\u00b7ses", "gibst", "du", "mir", "kein", "Le\u00b7bens\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "VVFIN", "PPER", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit dir verglichen, zeigt der Stein sich milde.", "tokens": ["Mit", "dir", "ver\u00b7gli\u00b7chen", ",", "zeigt", "der", "Stein", "sich", "mil\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Feind verbirgt sich hinter seinem Schilde,", "tokens": ["Der", "Feind", "ver\u00b7birgt", "sich", "hin\u00b7