{"dta.poem.8862": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Er ist's, f\u00fcr den die Erde der H\u00f6lle Rachen ist:", "tokens": ["Er", "ist's", ",", "f\u00fcr", "den", "die", "Er\u00b7de", "der", "H\u00f6l\u00b7le", "Ra\u00b7chen", "ist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ART", "ART", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der ew'gen Sch\u00f6nheit Athem beseelt den Leib der Zeit,", "tokens": ["Der", "ew'\u00b7gen", "Sch\u00f6n\u00b7heit", "A\u00b7them", "be\u00b7seelt", "den", "Leib", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der ohne sie ein Haufen von todten Sachen ist!", "tokens": ["Der", "oh\u00b7ne", "sie", "ein", "Hau\u00b7fen", "von", "tod\u00b7ten", "Sa\u00b7chen", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wer, ohne sie, noch m\u00f6chte bestehn in einer Welt,", "tokens": ["Wer", ",", "oh\u00b7ne", "sie", ",", "noch", "m\u00f6ch\u00b7te", "be\u00b7stehn", "in", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUI", "PPER", "$,", "ADV", "VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die, wenn auch reich an Sch\u00e4tzen, es auch an Drachen ist.", "tokens": ["Die", ",", "wenn", "auch", "reich", "an", "Sch\u00e4t\u00b7zen", ",", "es", "auch", "an", "Dra\u00b7chen", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ADV", "ADJD", "APPR", "NN", "$,", "PPER", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "O selig, wer im Herzen ein sch\u00f6nes Bild erkor,", "tokens": ["O", "se\u00b7lig", ",", "wer", "im", "Her\u00b7zen", "ein", "sch\u00f6\u00b7nes", "Bild", "er\u00b7kor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Bey dem es s\u00fc\u00df zu schlummern, und s\u00fc\u00df zu wachen ist!", "tokens": ["Bey", "dem", "es", "s\u00fc\u00df", "zu", "schlum\u00b7mern", ",", "und", "s\u00fc\u00df", "zu", "wa\u00b7chen", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PTKZU", "VVINF", "$,", "KON", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "In dessen Augen Seele, in dessen Gliedern Ma\u00df,", "tokens": ["In", "des\u00b7sen", "Au\u00b7gen", "See\u00b7le", ",", "in", "des\u00b7sen", "Glie\u00b7dern", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "$,", "APPR", "PRELAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und dessen Thr\u00e4ne lieblich wie dessen Lachen ist!", "tokens": ["Und", "des\u00b7sen", "Thr\u00e4\u00b7ne", "lieb\u00b7lich", "wie", "des\u00b7sen", "La\u00b7chen", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADJD", "KOKOM", "PDS", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Mir bleibt das Sch\u00f6ne ferne, der ich es stets besang:", "tokens": ["Mir", "bleibt", "das", "Sch\u00f6\u00b7ne", "fer\u00b7ne", ",", "der", "ich", "es", "stets", "be\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "$,", "PRELS", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Sprich, Weiser, was in F\u00e4llen, wie der, zu machen ist?", "tokens": ["Sprich", ",", "Wei\u00b7ser", ",", "was", "in", "F\u00e4l\u00b7len", ",", "wie", "der", ",", "zu", "ma\u00b7chen", "ist", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PRELS", "APPR", "NN", "$,", "PWAV", "ART", "$,", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Es steuert nach dem Hafen des Gl\u00fccks mein Herz um-", "tokens": ["Es", "steu\u00b7ert", "nach", "dem", "Ha\u00b7fen", "des", "Gl\u00fccks", "mein", "Herz", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Das auf dem Meer der Liebe der kleinste Nachen ist!", "tokens": ["Das", "auf", "dem", "Meer", "der", "Lie\u00b7be", "der", "kleins\u00b7te", "Na\u00b7chen", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}