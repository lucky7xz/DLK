{"textgrid.poem.26539": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das aber nehmt euch einmal zu Verstande:", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das aber nehmt euch einmal zu Verstande:", "tokens": ["Das", "a\u00b7ber", "nehmt", "euch", "ein\u00b7mal", "zu", "Ver\u00b7stan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Einer nie sein H\u00f6chstes kann vollbringen,", "tokens": ["Da\u00df", "Ei\u00b7ner", "nie", "sein", "H\u00f6chs\u00b7tes", "kann", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPOSAT", "ADJA", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nicht ein Gott ihm gn\u00e4dig l\u00f6s't die Schwingen,", "tokens": ["Wenn", "nicht", "ein", "Gott", "ihm", "gn\u00e4\u00b7dig", "l\u00f6s't", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht ein g\u00fcnst'ger Wind ihn treibt vom Strande.", "tokens": ["Und", "nicht", "ein", "g\u00fcnst'\u00b7ger", "Wind", "ihn", "treibt", "vom", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn nie gede