{"textgrid.poem.49541": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Fragment", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4gt ein Gef\u00e4\u00df, in dem manch blasse Lilge steht,", "tokens": ["Tr\u00e4gt", "ein", "Ge\u00b7f\u00e4\u00df", ",", "in", "dem", "manch", "blas\u00b7se", "Lil\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihrer rechten Hand, geu\u00dft aus mit ihrer Linken,", "tokens": ["In", "ih\u00b7rer", "rech\u00b7ten", "Hand", ",", "geu\u00dft", "aus", "mit", "ih\u00b7rer", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVPP", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Krug, von Z\u00e4hren voll, die Gras und Blumen trinken.", "tokens": ["Den", "Krug", ",", "von", "Z\u00e4h\u00b7ren", "voll", ",", "die", "Gras", "und", "Blu\u00b7men", "trin\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der West, der ihren Schlei'r, aus grauem Duft gewebt,", "tokens": ["Der", "West", ",", "der", "ih\u00b7ren", "Schlei'r", ",", "aus", "grau\u00b7em", "Duft", "ge\u00b7webt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vorwitzig, \u00fcber halb, von ihrer Stirne hebt,", "tokens": ["Vor\u00b7wit\u00b7zig", ",", "\u00fc\u00b7ber", "halb", ",", "von", "ih\u00b7rer", "Stir\u00b7ne", "hebt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJD", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zeigt ihr sonst l\u00e4chelnd Aug', und rosenreiche Wangen,", "tokens": ["Zeigt", "ihr", "sonst", "l\u00e4\u00b7chelnd", "Aug'", ",", "und", "ro\u00b7sen\u00b7rei\u00b7che", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Uns mit Verzweiflung itzt, und Traurigkeit umfangen.", "tokens": ["Uns", "mit", "Ver\u00b7zwei\u00b7flung", "itzt", ",", "und", "Trau\u00b7rig\u00b7keit", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "$,", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Tr\u00e4gt ein Gef\u00e4\u00df, in dem manch blasse Lilge steht,", "tokens": ["Tr\u00e4gt", "ein", "Ge\u00b7f\u00e4\u00df", ",", "in", "dem", "manch", "blas\u00b7se", "Lil\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure