{"dta.poem.8106": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ert\u00f6net sein Lob, Erden, t\u00f6nt\u2019s, Sonnen, Gestirn!", "tokens": ["Er\u00b7t\u00f6\u00b7net", "sein", "Lob", ",", "Er\u00b7den", ",", "t\u00f6nt's", ",", "Son\u00b7nen", ",", "Ge\u00b7stirn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Jhr Gestirn\u2019 hier in der Strasse des Lichts, hallt\u2019s seyrend", "tokens": ["Ihr", "Ge\u00b7stirn'", "hier", "in", "der", "Stras\u00b7se", "des", "Lichts", ",", "hallt's", "sey\u00b7rend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "VAFIN", "VVPP"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Des Erl\u00f6senden Lob, siehe des Herrlichen!", "tokens": ["Des", "Er\u00b7l\u00f6\u00b7sen\u00b7den", "Lob", ",", "sie\u00b7he", "des", "Herr\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unerreichten von dem Danklied der Natur!", "tokens": ["Un\u00b7er\u00b7reich\u00b7ten", "von", "dem", "Dank\u00b7lied", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+---+", "measure": "unknown.measure.penta"}}}}}