"meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wirst du mich auch verlassen nicht,", "tokens": ["Wirst", "du", "mich", "auch", "ver\u00b7las\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der dich verehrt und herzlich spricht:", "tokens": ["Der", "dich", "ver\u00b7ehrt", "und", "herz\u00b7lich", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gegr\u00fc\u00dft seist du Maria!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "seist", "du", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So will ich, liebste Mutter rein,", "tokens": ["So", "will", "ich", ",", "liebs\u00b7te", "Mut\u00b7ter", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich gr\u00fc\u00dfen mit Vertrauen,", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfen", "mit", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du mich allen Feinden mein", "tokens": ["Da\u00df", "du", "mich", "al\u00b7len", "Fein\u00b7den", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PIAT", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6gst rei\u00dfen aus den Klauen.", "tokens":