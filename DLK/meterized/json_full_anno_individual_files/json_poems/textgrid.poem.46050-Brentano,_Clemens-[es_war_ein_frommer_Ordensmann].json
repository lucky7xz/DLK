{"textgrid.poem.46050": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[es war ein frommer Ordensmann]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein frommer Ordensmann", "tokens": ["Es", "war", "ein", "from\u00b7mer", "Or\u00b7dens\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar treu in allen Dingen", "tokens": ["Gar", "treu", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mutter Gottes zugetan,", "tokens": ["Der", "Mut\u00b7ter", "Got\u00b7tes", "zu\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Beten und im Singen", "tokens": ["Im", "Be\u00b7ten", "und", "im", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In aller Rede fort und fort,", "tokens": ["In", "al\u00b7ler", "Re\u00b7de", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "War stets sein erst und letztes Wort:", "tokens": ["War", "stets", "sein", "erst", "und", "letz\u00b7tes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADV", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gegr\u00fc\u00dft seist du Maria!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "seist", "du", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gar lieb war ihm ein V\u00f6gelein,", "tokens": ["Gar", "lieb", "war", "ihm", "ein", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das jung ihm zugeflogen,", "tokens": ["Das", "jung", "ihm", "zu\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er im kleinen K\u00f6rbelein", "tokens": ["Und", "er", "im", "klei\u00b7nen", "K\u00f6r\u00b7be\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelehrt und aufgezogen", "tokens": ["Ge\u00b7lehrt", "und", "auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und lieblich sang es fr\u00fch und spat", "tokens": ["Und", "lieb\u00b7lich", "sang", "es", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie es von ihm geh\u00f6ret hat:", "tokens": ["Wie", "es", "von", "ihm", "ge\u00b7h\u00f6\u00b7ret", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gegr\u00fc\u00dft seist du Maria!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "seist", "du", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun war das kleine K\u00f6rbelein", "tokens": ["Nun", "war", "das", "klei\u00b7ne", "K\u00f6r\u00b7be\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bauf\u00e4llig und zerbrochen,", "tokens": ["Bau\u00b7f\u00e4l\u00b7lig", "und", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist das kluge V\u00f6gelein", "tokens": ["Da", "ist", "das", "klu\u00b7ge", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt herausgekrochen,", "tokens": ["Zu\u00b7letzt", "her\u00b7aus\u00b7ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und als es in die Freiheit kam,", "tokens": ["Und", "als", "es", "in", "die", "Frei\u00b7heit", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fieng fr\u00f6hlich es zu singen an:", "tokens": ["Fi\u00b7eng", "fr\u00f6h\u00b7lich", "es", "zu", "sin\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Gegr\u00fc\u00dft seist du Maria!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "seist", "du", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der fromme Mann dem V\u00f6gelein", "tokens": ["Der", "from\u00b7me", "Mann", "dem", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist lange nachgegangen", "tokens": ["Ist", "lan\u00b7ge", "nach\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hielt ihm dar das K\u00f6rbelein,", "tokens": ["Und", "hielt", "ihm", "dar", "das", "K\u00f6r\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wieder einzufangen.", "tokens": ["Es", "wie\u00b7der", "ein\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch dies von Baum zu Baum sich schwang", "tokens": ["Doch", "dies", "von", "Baum", "zu", "Baum", "sich", "schwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "NE", "APPR", "NE", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und immerfort sein Liedlein sang:", "tokens": ["Und", "im\u00b7mer\u00b7fort", "sein", "Lied\u00b7lein", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gegr\u00fc\u00dft seist du Maria!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "seist", "du", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das V\u00f6glein einst auf d\u00fcrrem Zweig", "tokens": ["Das", "V\u00f6\u00b7glein", "einst", "auf", "d\u00fcr\u00b7rem", "Zweig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wollt' sein Nestchen bauen", "tokens": ["Sich", "wollt'", "sein", "Nest\u00b7chen", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da st\u00fcrzt' auf es ein Geier gleich,", "tokens": ["Da", "st\u00fcrzt'", "auf", "es", "ein", "Gei\u00b7er", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trug's fort in seinen Klauen,", "tokens": ["Trug's", "fort", "in", "sei\u00b7nen", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da schrie das kleine V\u00f6gelein", "tokens": ["Da", "schrie", "das", "klei\u00b7ne", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl in den h\u00f6chsten N\u00f6ten sein:", "tokens": ["Wohl", "in", "den", "h\u00f6chs\u00b7ten", "N\u00f6\u00b7ten", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gegr\u00fc\u00dft seist du Maria!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "seist", "du", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da kam ein Blitz in h\u00f6chster Not", "tokens": ["Da", "kam", "ein", "Blitz", "in", "h\u00f6chs\u00b7ter", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus hellem Himmel nieder,", "tokens": ["Aus", "hel\u00b7lem", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schlug den b\u00f6sen Geier tot,", "tokens": ["Und", "schlug", "den", "b\u00f6\u00b7sen", "Gei\u00b7er", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frei flog das V\u00f6glein wieder,", "tokens": ["Frei", "flog", "das", "V\u00f6\u00b7glein", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und zu Mari\u00e4 Ehren sang", "tokens": ["Und", "zu", "Ma\u00b7ri\u00e4", "Eh\u00b7ren", "sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das V\u00f6glein mit noch hellerm Klang:", "tokens": ["Das", "V\u00f6\u00b7glein", "mit", "noch", "hel\u00b7lerm", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gegr\u00fc\u00dft seist du Maria!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "seist", "du", "Ma\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der fromme Mann im Garten stand,", "tokens": ["Der", "from\u00b7me", "Mann", "im", "Gar\u00b7ten", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah zu mit Angst und Bangen,", "tokens": ["Sah", "zu", "mit", "Angst", "und", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frisch und gesund ihm auf die Hand", "tokens": ["Frisch", "und", "ge\u00b7sund", "ihm", "auf", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Flog's V\u00f6glein, lie\u00df sich fangen,", "tokens": ["Flo\u00b7g's", "V\u00f6\u00b7glein", ",", "lie\u00df", "sich", "fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heim trug er's in dem K\u00