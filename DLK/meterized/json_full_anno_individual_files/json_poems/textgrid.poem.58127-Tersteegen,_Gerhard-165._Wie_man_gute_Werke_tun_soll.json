{"textgrid.poem.58127": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "165. Wie man gute Werke tun soll", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gutes Werk zu tun, ist n\u00f6tig, n\u00fctz und gut,", "tokens": ["Ein", "gu\u00b7tes", "Werk", "zu", "tun", ",", "ist", "n\u00f6\u00b7tig", ",", "n\u00fctz", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wird es b\u00f6s', wenn man's nach eignem Willen tut;", "tokens": ["Doch", "wird", "es", "b\u00f6s'", ",", "wenn", "man's", "nach", "eig\u00b7nem", "Wil\u00b7len", "tut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tu alles, wann und wie und wo es Gott will haben,", "tokens": ["Tu", "al\u00b7les", ",", "wann", "und", "wie", "und", "wo", "es", "Gott", "will", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "PWAV", "KON", "PWAV", "KON", "PWAV", "PPER", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebeugt, gehorsam sein, ist mehr als Opfergaben!", "tokens": ["Ge\u00b7beugt", ",", "ge\u00b7hor\u00b7sam", "sein", ",", "ist", "mehr", "als", "Op\u00b7fer\u00b7ga\u00b7ben", "!"], "token_info": ["word", "punct", "