{"dta.poem.4452": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Auf \u00fcberlebtes Elend blick ich nieder,", "tokens": ["Auf", "\u00fc\u00b7ber\u00b7leb\u00b7tes", "E\u00b7lend", "blick", "ich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nenne Deinen Nahmen laut, vor einer Welt,", "tokens": ["Und", "nen\u00b7ne", "Dei\u00b7nen", "Nah\u00b7men", "laut", ",", "vor", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der dieses, Dir geweyhte, Opfer meiner Lieder", "tokens": ["Der", "die\u00b7ses", ",", "Dir", "ge\u00b7weyh\u00b7te", ",", "Op\u00b7fer", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PDAT", "$,", "PPER", "VVFIN", "$,", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie Deine sch\u00f6ne That gef\u00e4llt.", "tokens": ["Wie", "Dei\u00b7ne", "sch\u00f6\u00b7ne", "That", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}