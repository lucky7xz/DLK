{"dta.poem.8212": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ostern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lie\u00df den Blick am Horizonte gleiten,", "tokens": ["Ich", "lie\u00df", "den", "Blick", "am", "Ho\u00b7ri\u00b7zon\u00b7te", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu mir her\u00fcber scholl verhei\u00dfungsreich", "tokens": ["Zu", "mir", "her\u00b7\u00fc\u00b7ber", "scholl", "ver\u00b7hei\u00b7\u00dfungs\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit vollem Klang das Osterglockenl\u00e4uten.", "tokens": ["Mit", "vol\u00b7lem", "Klang", "das", "Os\u00b7ter\u00b7glo\u00b7cken\u00b7l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie brennend Silber funkelte das Meer,", "tokens": ["Wie", "bren\u00b7nend", "Sil\u00b7ber", "fun\u00b7kel\u00b7te", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Inseln schwammen auf dem hohen Spiegel,", "tokens": ["Die", "In\u00b7seln", "schwam\u00b7men", "auf", "dem", "ho\u00b7hen", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00f6ven schossen blendend hin und her,", "tokens": ["Die", "M\u00f6\u00b7ven", "schos\u00b7sen", "blen\u00b7dend", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eintauchend in die Fluth die wei\u00dfen Fl\u00fcgel.", "tokens": ["Ein\u00b7tau\u00b7chend", "in", "die", "Fluth", "die", "wei\u00b7\u00dfen", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im tiefen Kooge bis zum Deichesrand", "tokens": ["Im", "tie\u00b7fen", "Koo\u00b7ge", "bis", "zum", "Dei\u00b7ches\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War sammetgr\u00fcn die Wiese aufgegangen;", "tokens": ["War", "sam\u00b7met\u00b7gr\u00fcn", "die", "Wie\u00b7se", "auf\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Fr\u00fchling zog prophetisch \u00fcber Land,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "zog", "pro\u00b7phe\u00b7tisch", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Lerchen jauchzten und die Knospen sprangen. \u2014", "tokens": ["Die", "Ler\u00b7chen", "jauchz\u00b7ten", "und", "die", "Knos\u00b7pen", "spran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Entfesselt ist die urgewaltge Kraft,", "tokens": ["Ent\u00b7fes\u00b7selt", "ist", "die", "ur\u00b7ge\u00b7walt\u00b7ge", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde quillt, die jungen S\u00e4fte tropfen,", "tokens": ["Die", "Er\u00b7de", "quillt", ",", "die", "jun\u00b7gen", "S\u00e4f\u00b7te", "trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Alles treibt, und Alles webt und schafft,", "tokens": ["Und", "Al\u00b7les", "treibt", ",", "und", "Al\u00b7les", "webt", "und", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens vollste Pulse h\u00f6r' ich klopfen.", "tokens": ["Des", "Le\u00b7bens", "volls\u00b7te", "Pul\u00b7se", "h\u00f6r'", "ich", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Fluth entsteigt der frische Meeresduft,", "tokens": ["Der", "Fluth", "ent\u00b7steigt", "der", "fri\u00b7sche", "Mee\u00b7res\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Himmel str\u00f6mt die goldne Sonnenf\u00fclle;", "tokens": ["Vom", "Him\u00b7mel", "str\u00f6mt", "die", "gold\u00b7ne", "Son\u00b7nen\u00b7f\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Fr\u00fchlingswind geht klingend durch die Luft", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7wind", "geht", "klin\u00b7gend", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sprengt im Flug des Schlummers letzte H\u00fclle.", "tokens": ["Und", "sprengt", "im", "Flug", "des", "Schlum\u00b7mers", "letz\u00b7te", "H\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O wehe fort, bis jede Knospe bricht,", "tokens": ["O", "we\u00b7he", "fort", ",", "bis", "je\u00b7de", "Knos\u00b7pe", "bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df endlich uns ein ganzer Sommer werde;", "tokens": ["Da\u00df", "end\u00b7lich", "uns", "ein", "gan\u00b7zer", "Som\u00b7mer", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entfalte dich, du gottgebornes Licht,", "tokens": ["Ent\u00b7fal\u00b7te", "dich", ",", "du", "gott\u00b7ge\u00b7bor\u00b7nes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wanke nicht, du feste Heimatherde! \u2014", "tokens": ["Und", "wan\u00b7ke", "nicht", ",", "du", "fes\u00b7te", "Hei\u00b7ma\u00b7ther\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Hier stand ich oft, wenn in Novembernacht", "tokens": ["Hier", "stand", "ich", "oft", ",", "wenn", "in", "No\u00b7vem\u00b7ber\u00b7nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aufgohr das Meer zu gischtbest\u00e4ubten H\u00fcgeln,", "tokens": ["Auf\u00b7gohr", "das", "Meer", "zu", "gischt\u00b7be\u00b7st\u00e4ub\u00b7ten", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn in den L\u00fcften war der Sturm erwacht,", "tokens": ["Wenn", "in", "den", "L\u00fcf\u00b7ten", "war", "der", "Sturm", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Kappe peitschend mit den Geierfl\u00fcgeln.", "tokens": ["Die", "Kap\u00b7pe", "peit\u00b7schend", "mit", "den", "Gei\u00b7er\u00b7fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und jauchzend lie\u00df ich an der festen Wehr", "tokens": ["Und", "jauch\u00b7zend", "lie\u00df", "ich", "an", "der", "fes\u00b7ten", "Wehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Wellenschlag die grimmen Z\u00e4hne reiben;", "tokens": ["Den", "Wel\u00b7len\u00b7schlag", "die", "grim\u00b7men", "Z\u00e4h\u00b7ne", "rei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn machtlos, zischend scho\u00df zur\u00fcck das Meer \u2014", "tokens": ["Denn", "macht\u00b7los", ",", "zi\u00b7schend", "scho\u00df", "zu\u00b7r\u00fcck", "das", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVPP", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Land ist unser, unser soll es bleiben!", "tokens": ["Das", "Land", "ist", "un\u00b7ser", ",", "un\u00b7ser", "soll", "es", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$,", "PPOSAT", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}