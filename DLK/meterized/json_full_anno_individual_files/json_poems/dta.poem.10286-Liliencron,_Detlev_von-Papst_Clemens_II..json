 ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ganz leise dr\u00f6hnt das Tamtam her,", "tokens": ["Ganz", "lei\u00b7se", "dr\u00f6hnt", "das", "Tam\u00b7tam", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schlo\u00dfhof flackern Opferfeuer", "tokens": ["Im", "Schlo\u00df\u00b7hof", "fla\u00b7ckern", "Op\u00b7fer\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Grell um das G\u00f6tzenungeheuer \u2014", "tokens": ["Grell", "um", "das", "G\u00f6t\u00b7ze\u00b7nun\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch heimlich rauscht das Gipfelmeer.", "tokens": ["Doch", "heim\u00b7lich", "rauscht", "das", "Gip\u00b7fel\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie Jedem, schlie\u00dft die letzte Stunde", "tokens": ["Wie", "Je\u00b7dem", ",", "schlie\u00dft", "die", "letz\u00b7te", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liebreich auch ihm die letzte Wunde.", "tokens": ["Lieb\u00b7reich", "auch", "ihm", "die", "letz\u00b7te", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+