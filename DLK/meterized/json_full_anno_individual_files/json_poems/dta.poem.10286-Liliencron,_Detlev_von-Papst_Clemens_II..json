{"dta.poem.10286": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Papst Clemens  II.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "In MeinstorfMeinstorf bei Pl\u00f6n in Holstein. reiten aus dem Turm             ", "tokens": ["In", "Meins\u00b7torf", "Meins\u00b7torf", "bei", "Pl\u00f6n", "in", "Hol\u00b7stein", ".", "rei\u00b7ten", "aus", "dem", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "APPR", "NE", "APPR", "NN", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Zwei J\u00e4ger frisch wie Fr\u00fchlingssturm.", "tokens": ["Zwei", "J\u00e4\u00b7ger", "frisch", "wie", "Fr\u00fch\u00b7lings\u00b7sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Juchen der Piqueure schallt,", "tokens": ["Kein", "Ju\u00b7chen", "der", "Pi\u00b7que\u00b7u\u00b7re", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und keine Doppelb\u00fcchse knallt.", "tokens": ["Und", "kei\u00b7ne", "Dop\u00b7pel\u00b7b\u00fcch\u00b7se", "knallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es jagt kein Feld von roten R\u00f6cken,", "tokens": ["Es", "jagt", "kein", "Feld", "von", "ro\u00b7ten", "R\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Treiber l\u00e4rmt mit Ruf und St\u00f6cken,", "tokens": ["Kein", "Trei\u00b7ber", "l\u00e4rmt", "mit", "Ruf", "und", "St\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hell nur im Wald giebt Hals die Meute,", "tokens": ["Hell", "nur", "im", "Wald", "giebt", "Hals", "die", "Meu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bricht durch Dickicht und Gereute.", "tokens": ["Und", "bricht", "durch", "Di\u00b7ckicht", "und", "Ge\u00b7reu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Und hinterher in scharfer Pace,", "tokens": ["Und", "hin\u00b7ter\u00b7her", "in", "schar\u00b7fer", "Pa\u00b7ce", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Den Z\u00fcgel fest, fest im Ges\u00e4\u00df,", "tokens": ["Den", "Z\u00fc\u00b7gel", "fest", ",", "fest", "im", "Ge\u00b7s\u00e4\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die beiden blonden Sachsenknaben.", "tokens": ["Die", "bei\u00b7den", "blon\u00b7den", "Sach\u00b7sen\u00b7kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hep Hussah \u00fcber Zaun und Graben,", "tokens": ["Hep", "Hus\u00b7sah", "\u00fc\u00b7ber", "Zaun", "und", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Brombeerstrauch und Dorngeflecht,", "tokens": ["Durch", "Brom\u00b7beer\u00b7strauch", "und", "Dorn\u00b7ge\u00b7flecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Edelinge und sein Knecht.", "tokens": ["Der", "E\u00b7de\u00b7lin\u00b7ge", "und", "sein", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo blieb der Keiler?", "tokens": ["Wo", "blieb", "der", "Kei\u00b7ler", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hat ihn gedeckt die Meute schon?", "tokens": ["Hat", "ihn", "ge\u00b7deckt", "die", "Meu\u00b7te", "schon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Neun Packer hat er abgeschlagen,", "tokens": ["Neun", "Pa\u00b7cker", "hat", "er", "ab\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer weiter geht das Jagen.", "tokens": ["Und", "im\u00b7mer", "wei\u00b7ter", "geht", "das", "Ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt verliert sich das Gel\u00e4ut", "tokens": ["Zu\u00b7letzt", "ver\u00b7liert", "sich", "das", "Ge\u00b7l\u00e4ut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Bruch und Moor und Schilfgest\u00e4ud.", "tokens": ["In", "Bruch", "und", "Moor", "und", "Schilf\u00b7ge\u00b7st\u00e4ud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Keiler nahm das Wasser an,", "tokens": ["Der", "Kei\u00b7ler", "nahm", "das", "Was\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Svidger und Burvin sind heran.", "tokens": ["Svid\u00b7ger", "und", "Bur\u00b7vin", "sind", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und nun ein k\u00f6stlich Bild zum Malen:", "tokens": ["Und", "nun", "ein", "k\u00f6st\u00b7lich", "Bild", "zum", "Ma\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Voran der Keiler, hinterher", "tokens": ["Vo\u00b7ran", "der", "Kei\u00b7ler", ",", "hin\u00b7ter\u00b7her"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die R\u00fcdenhunde, dann mit Speer", "tokens": ["Die", "R\u00fc\u00b7den\u00b7hun\u00b7de", ",", "dann", "mit", "Speer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und Pfeilen Burvin, Svidiger,", "tokens": ["Und", "Pfei\u00b7len", "Bur\u00b7vin", ",", "Svi\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das Alles kreuzt die stille Flut", "tokens": ["Das", "Al\u00b7les", "kreuzt", "die", "stil\u00b7le", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zur Mittagstund in Sonnenglut.", "tokens": ["Zur", "Mit\u00b7tags\u00b7tund", "in", "Son\u00b7nen\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und voll Entsetzen schwimmt der Keiler,", "tokens": ["Und", "voll", "Ent\u00b7set\u00b7zen", "schwimmt", "der", "Kei\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ein pr\u00e4chtig sch\u00f6ner Wellenteiler,", "tokens": ["Ein", "pr\u00e4ch\u00b7tig", "sch\u00f6\u00b7ner", "Wel\u00b7len\u00b7tei\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Voll Gier und Mordsucht dann die R\u00fcden,", "tokens": ["Voll", "Gier", "und", "Mord\u00b7sucht", "dann", "die", "R\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Hengste dann, die schon erm\u00fcden.", "tokens": ["Die", "Hengs\u00b7te", "dann", ",", "die", "schon", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "So schaufelt emsig fort die Hetze,", "tokens": ["So", "schau\u00b7felt", "em\u00b7sig", "fort", "die", "Het\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Es jauchzen Svidger und Burvin,", "tokens": ["Es", "jauch\u00b7zen", "Svid\u00b7ger", "und", "Bur\u00b7vin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Bis endlich unsichtbare Netze", "tokens": ["Bis", "end\u00b7lich", "un\u00b7sicht\u00b7ba\u00b7re", "Net\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die Pferde in die Tiefe ziehn.", "tokens": ["Die", "Pfer\u00b7de", "in", "die", "Tie\u00b7fe", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nun schwimmen selbst die Jagdgenossen,", "tokens": ["Nun", "schwim\u00b7men", "selbst", "die", "Jagd\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die gelben Locken seeumflossen.", "tokens": ["Die", "gel\u00b7ben", "Lo\u00b7cken", "see\u00b7um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Doch auch die st\u00e4rkste Reckenkraft", "tokens": ["Doch", "auch", "die", "st\u00e4rks\u00b7te", "Re\u00b7cken\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Erlahmt am Ende und erschlafft,", "tokens": ["Er\u00b7lahmt", "am", "En\u00b7de", "und", "er\u00b7schlafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und gerade war es Zeit zum Landen,", "tokens": ["Und", "ge\u00b7ra\u00b7de", "war", "es", "Zeit", "zum", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Eh\u2019 Sinn und Armkraft ihnen schwanden.", "tokens": ["Eh'", "Sinn", "und", "Arm\u00b7kraft", "ih\u00b7nen", "schwan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Nun ruhn sie matt auf wei\u00dfem Sand", "tokens": ["Nun", "ruhn", "sie", "matt", "auf", "wei\u00b7\u00dfem", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "In K\u00f6nig Buthus Heidenland,", "tokens": ["In", "K\u00f6\u00b7nig", "But\u00b7hus", "Hei\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wo unbarmherzig jeder Christ", "tokens": ["Wo", "un\u00b7barm\u00b7her\u00b7zig", "je\u00b7der", "Christ"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Dem G\u00f6tzengott verfallen ist.", "tokens": ["Dem", "G\u00f6t\u00b7zen\u00b7gott", "ver\u00b7fal\u00b7len", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Priester steht am Steinaltar,", "tokens": ["Der", "Pries\u00b7ter", "steht", "am", "Stei\u00b7nal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Tamtam dr\u00f6hnt, die Menge schreit,", "tokens": ["Das", "Tam\u00b7tam", "dr\u00f6hnt", ",", "die", "Men\u00b7ge", "schreit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den beiden Christen f\u00e4llt das Haar,", "tokens": ["Den", "bei\u00b7den", "Chris\u00b7ten", "f\u00e4llt", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Opfermesser ist bereit.", "tokens": ["Das", "Op\u00b7fer\u00b7mes\u00b7ser", "ist", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf scharlachrotem Thron schaut zu", "tokens": ["Auf", "schar\u00b7la\u00b7ch\u00b7ro\u00b7tem", "Thron", "schaut", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Die sch\u00f6ne Tochter von Buthu.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Toch\u00b7ter", "von", "Bu\u00b7thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die braunen Augen sehen schmerzlich", "tokens": ["Die", "brau\u00b7nen", "Au\u00b7gen", "se\u00b7hen", "schmerz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Svidiger, den blonden Sachsen,", "tokens": ["Auf", "Svi\u00b7di\u00b7ger", ",", "den", "blon\u00b7den", "Sach\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und Siva liebt ihn, liebt ihn herzlich,", "tokens": ["Und", "Si\u00b7va", "liebt", "ihn", ",", "liebt", "ihn", "herz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure