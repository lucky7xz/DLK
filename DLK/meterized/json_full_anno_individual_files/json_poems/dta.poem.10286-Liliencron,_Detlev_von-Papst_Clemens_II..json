, "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als Kaiser Heinrich ihn berief", "tokens": ["Als", "Kai\u00b7ser", "Hein\u00b7rich", "ihn", "be\u00b7rief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu seinem Kanzler, seinem Rat,", "tokens": ["Zu", "sei\u00b7nem", "Kanz\u00b7ler", ",", "sei\u00b7nem", "Rat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zum Herzog gut, zu mancher That.", "tokens": ["Zum", "Her\u00b7zog", "gut", ",", "zu", "man\u00b7cher", "That", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zum Bischof macht der Kaiser ihn", "tokens": ["Zum", "Bi\u00b7schof", "macht", "der", "Kai\u00b7ser", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von Bamberg, mit ihm zog Burvin,", "tokens": ["Von", "Bam\u00b7berg", ",", "mit", "ihm", "zog", "Bur\u00b7vin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der immer brav an seiner Seite", "tokens": ["Der", "im\u00b7mer", "brav", "an", "sei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Im Leben gab ihm das Geleite.", "tokens": ["Im", "Le\u00b7ben", "gab", "ihm", "das", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und endlich ist er Papst geworden,", "tokens": ["Und", "end\u00b7lich", "ist", "er", "Papst", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der Sachse aus dem Nebelnorden.", "tokens": ["Der", "Sach\u00b7se", "aus", "dem", "Ne\u00b7bel\u00b7nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Doch liebten ihn die Welschen nicht,", "tokens": ["Doch", "lieb\u00b7ten", "ihn", "die", "Wel\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Zu deutsch und ernst war sein Gesicht.", "tokens": ["Zu", "deutsch", "und", "ernst", "war", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sie ha\u00dften ihn, sein blondes Haar,", "tokens": ["Sie", "ha\u00df\u00b7ten", "ihn", ",", "sein", "blon\u00b7des", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sein treues, blaues Augenpaar.", "tokens": ["Sein", "treu\u00b7e