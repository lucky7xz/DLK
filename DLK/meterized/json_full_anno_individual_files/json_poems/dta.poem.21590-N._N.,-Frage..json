{"dta.poem.21590": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Frage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Das erste Veilchen", "tokens": ["Das", "ers\u00b7te", "Veil\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wo werd\u2019 ich\u2019s pfl\u00fccken?", "tokens": ["Wo", "werd'", "ich's", "pfl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wen \u2014 o Frage! \u2014", "tokens": ["Und", "wen", "o", "Fra\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "$(", "FM", "NN", "$.", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wen wird es schm\u00fccken?", "tokens": ["Wen", "wird", "es", "schm\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und \u2014 Gott, o sage! \u2014", "tokens": ["Und", "Gott", ",", "o", "sa\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "NN", "$,", "FM", "FM", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wird sie\u2019s begl\u00fccken?", "tokens": ["Wird", "sie's", "be\u00b7gl\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}