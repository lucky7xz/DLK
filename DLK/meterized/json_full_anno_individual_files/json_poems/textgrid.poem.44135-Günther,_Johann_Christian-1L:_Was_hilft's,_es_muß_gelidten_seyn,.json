 "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch zwingt er allzeit Fleisch und Blut,", "tokens": ["Doch", "zwingt", "er", "all\u00b7zeit", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sucht in sich selber Trost und Muth", "tokens": ["Sucht", "in", "sich", "sel\u00b7ber", "Trost", "und", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und lernt die Eitelkeit verlachen.", "tokens": ["Und", "lernt", "die", "Ei\u00b7tel\u00b7keit", "ver\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dein edles Herz, betr\u00fcbtes Haupt,", "tokens": ["Dein", "ed\u00b7les", "Herz", ",", "be\u00b7tr\u00fcb\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchlt auch ja mehr als einen Kummer,", "tokens": ["F\u00fchlt", "auch", "ja", "mehr", "als", "ei\u00b7nen", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst verschw\u00e4rzt, gedr\u00e4ngt, beraubt", "tokens": ["Du", "wirst", "ver\u00b7schw\u00e4rzt", ",", "ge\u00b7dr\u00e4ngt", ",", "be\u00b7raubt"], "token_info": ["word", "word",