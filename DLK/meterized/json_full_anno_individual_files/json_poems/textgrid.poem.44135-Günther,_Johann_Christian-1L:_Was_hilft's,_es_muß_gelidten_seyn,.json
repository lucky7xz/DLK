{"textgrid.poem.44135": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was hilft's, es mu\u00df gelidten seyn,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilft's, es mu\u00df gelidten seyn,", "tokens": ["Was", "hilft's", ",", "es", "mu\u00df", "ge\u00b7lid\u00b7ten", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lang uns Erd und Luft noch tragen;", "tokens": ["So", "lang", "uns", "Erd", "und", "Luft", "noch", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder Mensch hat seine Pein,", "tokens": ["Ein", "je\u00b7der", "Mensch", "hat", "sei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der zehnte darf sie niemand klagen.", "tokens": ["Der", "zehn\u00b7te", "darf", "sie", "nie\u00b7mand", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wandern alle durch die Zeit", "tokens": ["Wir", "wan\u00b7dern", "al\u00b7le", "durch", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Vaterland der Ewigkeit", "tokens": ["Ins", "Va\u00b7ter\u00b7land", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und suchen eine be\u00dfre St\u00e4tte;", "tokens": ["Und", "su\u00b7chen", "ei\u00b7ne", "be\u00df\u00b7re", "St\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da nimmt nun auch ein jeder wahr,", "tokens": ["Da", "nimmt", "nun", "auch", "ein", "je\u00b7der", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wieviel Verwirrung und Gefahr", "tokens": ["Wie\u00b7viel", "Ver\u00b7wir\u00b7rung", "und", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Pilgern Gl\u00fcck und Weg vertrete.", "tokens": ["Den", "Pil\u00b7gern", "Gl\u00fcck", "und", "Weg", "ver\u00b7tre\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Inzwischen nimmt ein kluger Geist", "tokens": ["I\u00b7nzwi\u00b7schen", "nimmt", "ein", "klu\u00b7ger", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedult und Hofnung zu Gefehrten,", "tokens": ["Ge\u00b7dult", "und", "Hof\u00b7nung", "zu", "Ge\u00b7fehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er folgt, wohin die Schickung weist,", "tokens": ["Er", "folgt", ",", "wo\u00b7hin", "die", "Schi\u00b7ckung", "weist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sieht nicht stets nach Roseng\u00e4rthen.", "tokens": ["Und", "sieht", "nicht", "stets", "nach", "Ro\u00b7sen\u00b7g\u00e4r\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bisweilen geht es ihm zwar nah,", "tokens": ["Bis\u00b7wei\u00b7len", "geht", "es", "ihm", "zwar", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Misgunst hier und Freunde da", "tokens": ["Wenn", "Mis\u00b7gunst", "hier", "und", "Freun\u00b7de", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den sauren Weg noch schwerer machen;", "tokens": ["Den", "sau\u00b7ren", "Weg", "noch", "schwe\u00b7rer", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch zwingt er allzeit Fleisch und Blut,", "tokens": ["Doch", "zwingt", "er", "all\u00b7zeit", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sucht in sich selber Trost und Muth", "tokens": ["Sucht", "in", "sich", "sel\u00b7ber", "Trost", "und", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und lernt die Eitelkeit verlachen.", "tokens": ["Und", "lernt", "die", "Ei\u00b7tel\u00b7keit", "ver\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein edles Herz, betr\u00fcbtes Haupt,", "tokens": ["Dein", "ed\u00b7les", "Herz", ",", "be\u00b7tr\u00fcb\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchlt auch ja mehr als einen Kummer,", "tokens": ["F\u00fchlt", "auch", "ja", "mehr", "als", "ei\u00b7nen", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst verschw\u00e4rzt, gedr\u00e4ngt, beraubt", "tokens": ["Du", "wirst", "ver\u00b7schw\u00e4rzt", ",", "ge\u00b7dr\u00e4ngt", ",", "be\u00b7raubt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hast mehr \u00c4rgern\u00fc\u00df als Schlummer.", "tokens": ["Und", "hast", "mehr", "\u00c4r\u00b7ger\u00b7n\u00fc\u00df", "als", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie manchmal mag der Thr\u00e4nenwein", "tokens": ["Wie", "manch\u00b7mal", "mag", "der", "Thr\u00e4\u00b7nen\u00b7wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Einsamkeit dein Labsahl seyn,", "tokens": ["In", "Ein\u00b7sam\u00b7keit", "dein", "Lab\u00b7sahl", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie vielmahl magstu r\u00fcckw\u00e4rts dencken", "tokens": ["Wie", "viel\u00b7mahl", "mags\u00b7tu", "r\u00fcck\u00b7w\u00e4rts", "den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und jener schon verstrichnen Zeit,", "tokens": ["Und", "je\u00b7ner", "schon", "ver\u00b7strich\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die unsre Kinderjahr erfreut,", "tokens": ["Die", "uns\u00b7re", "Kin\u00b7der\u00b7jahr", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein sehnsuchtsvolles Auge schencken.", "tokens": ["Ein", "sehn\u00b7suchts\u00b7vol\u00b7les", "Au\u00b7ge", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Feld sieht jezo traurig aus", "tokens": ["Das", "Feld", "sieht", "je\u00b7zo", "trau\u00b7rig", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und giebt uns schlechten Trost und Freude;", "tokens": ["Und", "giebt", "uns", "schlech\u00b7ten", "Trost", "und", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie weit betr\u00fcbter l\u00e4st dein Haus!", "tokens": ["Wie", "weit", "be\u00b7tr\u00fcb\u00b7ter", "l\u00e4st", "dein", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man weint, man klagt, man geht im Leide.", "tokens": ["Man", "weint", ",", "man", "klagt", ",", "man", "geht", "im", "Lei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Misgunst wehret deiner Brust", "tokens": ["Die", "Mis\u00b7gunst", "weh\u00b7ret", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Unschuld der gelehrten Lust,", "tokens": ["Die", "Un\u00b7schuld", "der", "ge\u00b7lehr\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man will, dein Feuer soll erkalten.", "tokens": ["Man", "will", ",", "dein", "Feu\u00b7er", "soll", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Himmel, ist ein Mensch genug,", "tokens": ["O", "Him\u00b7mel", ",", "ist", "ein", "Mensch", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er sey auch noch so starck und klug,", "tokens": ["Er", "sey", "auch", "noch", "so", "starck", "und", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So vielen Streichen auszuhalten?", "tokens": ["So", "vie\u00b7len", "Strei\u00b7chen", "aus\u00b7zu\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Sturm beweist des Schifers Kunst,", "tokens": ["Der", "Sturm", "be\u00b7weist", "des", "Schi\u00b7fers", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefahr mu\u00df Held und St\u00e4rcke zeigen,", "tokens": ["Ge\u00b7fahr", "mu\u00df", "Held", "und", "St\u00e4r\u00b7cke", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ist des Himmels Zorn oft Gunst", "tokens": ["So", "ist", "des", "Him\u00b7mels", "Zorn", "oft", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Herzen, die wie Palmen steigen;", "tokens": ["Bey", "Her\u00b7zen", ",", "die", "wie", "Pal\u00b7men", "stei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Vorsicht liebet, wen sie schl\u00e4gt,", "tokens": ["Die", "Vor\u00b7sicht", "lie\u00b7bet", ",", "wen", "sie", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erh\u00f6ht die, so sie niederlegt,", "tokens": ["Er\u00b7h\u00f6ht", "die", ",", "so", "sie", "nie\u00b7der\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie mi\u00dft die Last nach unsrer St\u00e4rcke.", "tokens": ["Sie", "mi\u00dft", "die", "Last", "nach", "uns\u00b7rer", "St\u00e4r\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Last ist deinem Geiste gleich:", "tokens": ["Die", "Last", "ist", "dei\u00b7nem", "Geis\u00b7te", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Leid, Mariane, Sto\u00df und Streich,", "tokens": ["Leid", ",", "Ma\u00b7ri\u00b7a\u00b7ne", ",", "Sto\u00df", "und", "Streich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Damit man deine Gr\u00f6\u00dfe mercke.", "tokens": ["Da\u00b7mit", "man", "dei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "mer\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was hilft's, es mu\u00df gelidten seyn,", "tokens": ["Was", "hilft's", ",", "es", "mu\u00df", "ge\u00b7lid\u00b7ten", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lang uns Erd und Luft noch tragen;", "tokens": ["So", "lang", "uns", "Erd", "und", "Luft", "noch", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder Mensch hat seine Pein,", "tokens": ["Ein", "je\u00b7der", "Mensch", "hat", "sei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der zehnte darf sie niemand klagen.", "tokens": ["Der", "zehn\u00b7te", "darf", "sie", "nie\u00b7mand", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wandern alle durch die Zeit", "tokens": ["Wir", "wan\u00b7dern", "al\u00b7le", "durch", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Vaterland der Ewigkeit", "tokens": ["Ins", "Va\u00b7ter\u00b7land", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und suchen eine be\u00dfre St\u00e4tte;", "tokens": ["Und", "su\u00b7chen", "ei\u00b7ne", "be\u00df\u00b7re", "St\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da nimmt nun auch ein jeder wahr,", "tokens": ["Da", "nimmt", "nun", "auch", "ein", "je\u00b7der", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wieviel Verwirrung und Gefahr", "tokens": ["Wie\u00b7viel", "Ver\u00b7wir\u00b7rung", "und", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Pilgern Gl\u00fcck und Weg vertrete.", "tokens": ["Den", "Pil\u00b7gern", "Gl\u00fcck", "und", "Weg", "ver\u00b7tre\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Inzwischen nimmt ein kluger Geist", "tokens": ["I\u00b7nzwi\u00b7schen", "nimmt", "ein", 