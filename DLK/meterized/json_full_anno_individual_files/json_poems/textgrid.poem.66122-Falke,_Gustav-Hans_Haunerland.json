{"textgrid.poem.66122": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Hans Haunerland", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hans Haunerland, ja, das war ein Mann!", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", ",", "ja", ",", "das", "war", "ein", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PTKANT", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der griff sein Leben fr\u00f6hlich an.", "tokens": ["Der", "griff", "sein", "Le\u00b7ben", "fr\u00f6h\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kehrte man bei Hans Haunerland ein,", "tokens": ["Kehr\u00b7te", "man", "bei", "Hans", "Hau\u00b7ner\u00b7land", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NE", "NE", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hans Haunerland schenkte den besten Wein.", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", "schenk\u00b7te", "den", "bes\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der reichste Bauer weit herum,", "tokens": ["Der", "reichs\u00b7te", "Bau\u00b7er", "weit", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hans Haunerland fand das Darben dumm.", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", "fand", "das", "Dar\u00b7ben", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Einst war er zur Fastelabendzeit", "tokens": ["Einst", "war", "er", "zur", "Fas\u00b7te\u00b7la\u00b7bend\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu Sch\u00f6neberg. Das war ein paar Stunden weit.", "tokens": ["Zu", "Sch\u00f6\u00b7ne\u00b7berg", ".", "Das", "war", "ein", "paar", "Stun\u00b7den", "weit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "PDS", "VAFIN", "ART", "PIAT", "NN", "ADJD", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hans Haunerland dacht nicht an Hof und Haus,", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", "dacht", "nicht", "an", "Hof", "und", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hans Haunerland schenkte ein und trank aus,", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", "schenk\u00b7te", "ein", "und", "trank", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da kam die gro\u00dfe Flut des Jahrs", "tokens": ["Da", "kam", "die", "gro\u00b7\u00dfe", "Flut", "des", "Jahrs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schluckt ihm Haus und Hof \u2013 weg wars.", "tokens": ["Und", "schluckt", "ihm", "Haus", "und", "Hof", "\u2013", "weg", "wars", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hans Haunerland lachte: Verschluckt ist verschluckt,", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", "lach\u00b7te", ":", "Ver\u00b7schluckt", "ist", "ver\u00b7schluckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da wird keine Kuh wieder ausgespuckt.", "tokens": ["Da", "wird", "kei\u00b7ne", "Kuh", "wie\u00b7der", "aus\u00b7ge\u00b7spuckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Eine Hufe sind und sieben Katen noch mein,", "tokens": ["Ei\u00b7ne", "Hu\u00b7fe", "sind", "und", "sie\u00b7ben", "Ka\u00b7ten", "noch", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "CARD", "NN", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Wer rechnets mir um in roten Wein?", "tokens": ["Wer", "rech\u00b7nets", "mir", "um", "in", "ro\u00b7ten", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Jetzt bleib ich in Sch\u00f6neberg und lach der Flut,", "tokens": ["Jetzt", "bleib", "ich", "in", "Sch\u00f6\u00b7ne\u00b7berg", "und", "lach", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Weit davon lebt sichs noch einmal so gut.", "tokens": ["Weit", "da\u00b7von", "lebt", "sichs", "noch", "ein\u00b7mal", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVFIN", "PIS", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Eine Hufe und sieben Katen noch.", "tokens": ["Ei\u00b7ne", "Hu\u00b7fe", "und", "sie\u00b7ben", "Ka\u00b7ten", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "CARD", "NN", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hans Haunerlands Hals war ein gro\u00dfes Loch.", "tokens": ["Hans", "Hau\u00b7ner\u00b7lands", "Hals", "war", "ein", "gro\u00b7\u00dfes", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Alle sieben hat er hinuntergesp\u00fclt", "tokens": ["Al\u00b7le", "sie\u00b7ben", "hat", "er", "hin\u00b7un\u00b7ter\u00b7ge\u00b7sp\u00fclt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "CARD", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und hat sie kaum einmal liegen gef\u00fchlt.", "tokens": ["Und", "hat", "sie", "kaum", "ein\u00b7mal", "lie\u00b7gen", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "VVPP", "$."], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Schluckt die See, er schluckt auch,", "tokens": ["Schluckt", "die", "See", ",", "er", "schluckt", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Hans Haunerland, der fr\u00f6hliche Schelm und Schlauch.", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", ",", "der", "fr\u00f6h\u00b7li\u00b7che", "Schelm", "und", "Schlauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als die letzte Kate verflo\u00df,", "tokens": ["Und", "als", "die", "letz\u00b7te", "Ka\u00b7te", "ver\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hans Haunerland die fr\u00f6hlichen Augen schlo\u00df.", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", "die", "fr\u00f6h\u00b7li\u00b7chen", "Au\u00b7gen", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hat alles verschluckt, was sein eigen im Raum,", "tokens": ["Hat", "al\u00b7les", "ver\u00b7schluckt", ",", "was", "sein", "ei\u00b7gen", "im", "Raum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,", "PRELS", "PPOSAT", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bis auf einen m\u00e4chtigen Walnu\u00dfbaum.", "tokens": ["Bis", "auf", "ei\u00b7nen", "m\u00e4ch\u00b7ti\u00b7gen", "Wal\u00b7nu\u00df\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der war ihm zu gro\u00df, den lie\u00df er stehn,", "tokens": ["Der", "war", "ihm", "zu", "gro\u00df", ",", "den", "lie\u00df", "er", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der wollte nicht durch die Kehle gehn.", "tokens": ["Der", "woll\u00b7te", "nicht", "durch", "die", "Keh\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und immer, f\u00fchrt mich der Weg nach dem Ort,", "tokens": ["Und", "im\u00b7mer", ",", "f\u00fchrt", "mich", "der", "Weg", "nach", "dem", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor zwei Jahren war ich das letzte Mal dort,", "tokens": ["Vor", "zwei", "Jah\u00b7ren", "war", "ich", "das", "letz\u00b7te", "Mal", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Dann schlag ich mir ein paar N\u00fcsse herab", "tokens": ["Dann", "schlag", "ich", "mir", "ein", "paar", "N\u00fcs\u00b7se", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "PIAT", "NN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und setz mich vergn\u00fcgt auf Hans Haunerlands Grab.", "tokens": ["Und", "setz", "mich", "ver\u00b7gn\u00fcgt", "auf", "Hans", "Hau\u00b7ner\u00b7lands", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "APPR", "NE", "NE", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und jedesmal, wenn eine Walnu\u00df kracht,", "tokens": ["Und", "je\u00b7des\u00b7mal", ",", "wenn", "ei\u00b7ne", "Wal\u00b7nu\u00df", "kracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "H\u00f6r ich deutlich, wie unten Hans Haunerland lacht.", "tokens": ["H\u00f6r", "ich", "deut\u00b7lich", ",", "wie", "un\u00b7ten", "Hans", "Hau\u00b7ner\u00b7land", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$,", "PWAV", "ADV", "NE", "NE", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.7": {"line.1": {"text": "Hans Haunerland, ja, das war ein Mann!", "tokens": ["Hans", "Hau\u00b7ner\u00b7land", ",", "ja", ",", "das", "war", "ein", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word",