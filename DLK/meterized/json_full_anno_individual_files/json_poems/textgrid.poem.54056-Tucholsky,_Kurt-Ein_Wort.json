{"textgrid.poem.54056": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ein Wort", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht ein Wort durchs ganze Land,", "tokens": ["Es", "geht", "ein", "Wort", "durchs", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch hunderttausend Leben.", "tokens": ["durch", "hun\u00b7dert\u00b7tau\u00b7send", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Wort hat ewigen Bestand,", "tokens": ["Das", "Wort", "hat", "e\u00b7wi\u00b7gen", "Be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du kannst nicht widerstreben.", "tokens": ["du", "kannst", "nicht", "wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Vater sagts,", "tokens": ["Der", "Va\u00b7ter", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "die Mutter sagts,", "tokens": ["die", "Mut\u00b7ter", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "der B\u00fcrger sagts,", "tokens": ["der", "B\u00fcr\u00b7ger", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "der Bauer sagts,", "tokens": ["der", "Bau\u00b7er", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "die Juden und die Arier;", "tokens": ["die", "Ju\u00b7den", "und", "die", "A\u00b7rier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "der Richter sagts,", "tokens": ["der", "Rich\u00b7ter", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "der Lehrer sagts,", "tokens": ["der", "Leh\u00b7rer", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "die Zeitung sagts,", "tokens": ["die", "Zei\u00b7tung", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "der Pfarrer sagts,", "tokens": ["der", "Pfar\u00b7rer", "sagts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "die Chefs und die Proletarier \u2013", "tokens": ["die", "Chefs", "und", "die", "Pro\u00b7le\u00b7ta\u00b7rier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du h\u00f6rst sie alle Tage schrein:", "tokens": ["Du", "h\u00f6rst", "sie", "al\u00b7le", "Ta\u00b7ge", "schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u00bblassassein \u2013!\u00ab", "tokens": ["\u00bb", "las\u00b7sas\u00b7sein", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct"], "pos": ["$(", "VVIMP", "$(", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Es mault das Baby, das man aufgeweckt:", "tokens": ["Es", "mault", "das", "Ba\u00b7by", ",", "das", "man", "auf\u00b7ge\u00b7weckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u203alassassein!\u2039", "tokens": ["\u203a", "las\u00b7sas\u00b7sein", "!", "\u2039"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "es schilt die Amme, wenn sichs vollgek\u00e4ckt:", "tokens": ["es", "schilt", "die", "Am\u00b7me", ",", "wenn", "sichs", "voll\u00b7ge\u00b7k\u00e4ckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u203alassassein!\u2039", "tokens": ["\u203a", "las\u00b7sas\u00b7sein", "!", "\u2039"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "es schallt durchs kinderreiche Haus:", "tokens": ["es", "schallt", "durchs", "kin\u00b7der\u00b7rei\u00b7che", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u203alassassein!\u2039", "tokens": ["\u203a", "las\u00b7sas\u00b7sein", "!", "\u2039"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "manche Erziehung besteht nur aus", "tokens": ["man\u00b7che", "Er\u00b7zie\u00b7hung", "be\u00b7steht", "nur", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Lassassein!", "tokens": ["Las\u00b7sas\u00b7sein", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Papa schimpft mit Fritzchen \u2013 fr\u00fch Rauchen macht krank:", "tokens": ["Pa\u00b7pa", "schimpft", "mit", "Fritz\u00b7chen", "\u2013", "fr\u00fch", "Rau\u00b7chen", "macht", "krank", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$(", "ADJD", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "\u203alassassein!\u2039", "tokens": ["\u203a", "las\u00b7sas\u00b7sein", "!", "\u2039"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Es schl\u00e4ngelt das M\u00e4dchen sich auf der Bank:", "tokens": ["Es", "schl\u00e4n\u00b7gelt", "das", "M\u00e4d\u00b7chen", "sich", "auf", "der", "Bank", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "\u00bbnicht doch . . . lassassein . . . !\u00ab", "tokens": ["\u00bb", "nicht", "doch", ".", ".", ".", "las\u00b7sas\u00b7sein", ".", ".", ".", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "PTKNEG", "ADV", "$.", "$.", "$.", "PTKVZ", "$.", "$.", "$.", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Es r\u00e4t der Freund dem Freunde gut:", "tokens": ["Es", "r\u00e4t", "der", "Freund", "dem", "Freun\u00b7de", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u00bbmensch, lassassein!\u00ab", "tokens": ["\u00bb", "mensch", ",", "las\u00b7sas\u00b7sein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "und der hat dann doch zum Heiraten Mut", "tokens": ["und", "der", "hat", "dann", "doch", "zum", "Hei\u00b7ra\u00b7ten", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VAFIN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "und l\u00e4\u00dfts nicht sein.", "tokens": ["und", "l\u00e4\u00dfts", "nicht", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Wird ein Richter vern\u00fcnftig, bringt ihn Leipzig auf den Trab: . . .", "tokens": ["Wird", "ein", "Rich\u00b7ter", "ver\u00b7n\u00fcnf\u00b7tig", ",", "bringt", "ihn", "Leip\u00b7zig", "auf", "den", "Trab", ":", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "VVFIN", "PPER", "NE", "APPR", "ART", "NN", "$.", "$.", "$.", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Lassassein!", "tokens": ["Las\u00b7sas\u00b7sein", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "zeigt die SPD Mut, wiegelt der Vorstand sie ab: . . .", "tokens": ["zeigt", "die", "SpD", "Mut", ",", "wie\u00b7gelt", "der", "Vor\u00b7stand", "sie", "ab", ":", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "$.", "$.", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Lassassein!", "tokens": ["Las\u00b7sas\u00b7sein", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Demonstrieren die Arbeiter, dann br\u00fcllt die Polizei:", "tokens": ["De\u00b7monst\u00b7rie\u00b7ren", "die", "Ar\u00b7bei\u00b7ter", ",", "dann", "br\u00fcllt", "die", "Po\u00b7li\u00b7zei", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--++--+-+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "\u203alassassein!\u2039", "tokens": ["\u203a", "las\u00b7sas\u00b7sein", "!", "\u2039"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "bei den Nazis steht sie l\u00e4chelnd dabei:", "tokens": ["bei", "den", "Na\u00b7zis", "steht", "sie", "l\u00e4\u00b7chelnd", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "PAV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "\u00bblassassein . . . Nein? Nein.\u00ab", "tokens": ["\u00bb", "las\u00b7sas\u00b7sein", ".", ".", ".", "Nein", "?", "Nein", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$.", "$.", "PTKANT", "$.", "PTKANT", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.25": {"text": "In juristischen W\u00e4lzern steht nur ein Wort:", "tokens": ["In", "ju\u00b7ris\u00b7ti\u00b7schen", "W\u00e4l\u00b7zern", "steht", "nur", "ein", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Lassassein!", "tokens": ["Las\u00b7sas\u00b7sein", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Hundert Schilder verunzieren jeden Ort:", "tokens": ["Hun\u00b7dert", "Schil\u00b7der", "ver\u00b7un\u00b7zie\u00b7ren", "je\u00b7den", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.28": {"text": "Lassassein!", "tokens": ["Las\u00b7sas\u00b7sein", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "George Grosz soll nicht malen. Die Kirche br\u00fcllt sich wund:", "tokens": ["Ge\u00b7or\u00b7ge", "Grosz", "soll", "nicht", "ma\u00b7len", ".", "Die", "Kir\u00b7che", "br\u00fcllt", "sich", "wund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PTKNEG", "VVINF", "$.", "ART", "NN", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "\u203alassassein!\u2039", "tokens": ["\u203a", "las\u00b7sas\u00b7sein", "!", "\u2039"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Pitigrilli soll nicht dichten. Es verbietet Schmutz und Schund:", "tokens": ["Pi\u00b7ti\u00b7gril\u00b7li", "soll", "nicht", "dich\u00b7ten", ".", "Es", "ver\u00b7bie\u00b7tet", "Schmutz", "und", "Schund", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADJA", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "\u203alassassein!\u2039", "tokens": ["\u203a", "las\u00b7sas\u00b7sein", "!", "\u2039"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Das Auto soll nicht fahren. Es droht die Markierung:", "tokens": ["Das", "Au\u00b7to", "soll", "nicht", "fah\u00b7ren", ".", "Es", "droht", "die", "Mar\u00b7kie\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "LaSSASSEIN!", "tokens": ["LaS\u00b7SAS\u00b7SEIN", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "Der Student soll nicht links sein. Es droht die Relegierung:", "tokens": ["Der", "Stu\u00b7dent", "soll", "nicht", "links", "sein", ".", "Es", "droht", "die", "Re\u00b7le\u00b7gie\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "VAINF", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Lassassein!", "tokens": ["Las\u00b7sas\u00b7sein", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Treibt die arme Frau ihre Leibesfrucht ab?", "tokens": ["Treibt", "die", "ar\u00b7me", "Frau", "ih\u00b7re", "Lei\u00b7bes\u00b7frucht", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Lassassein!", "tokens": ["Las\u00b7sas\u00b7sein", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Und noch auf dem Friedhof . . . \u00bbKeine Reden am Grab!\u00ab", "tokens": ["Und", "noch", "auf", "dem", "Fried\u00b7hof", ".", ".", ".", "\u00bb", "Kei\u00b7ne", "Re\u00b7den", "am", "Grab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$.", "$.", "$.", "$(", "PIAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Lassassein \u2013!", "tokens": ["Las\u00b7sas\u00b7sein", "\u2013", "!"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "So sagt jeder, was man nicht tun soll,", "tokens": ["So", "sagt", "je\u00b7der", ",", "was", "man", "nicht", "tun", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und verbietet dem andern die Hucke voll.", "tokens": ["und", "ver\u00b7bi