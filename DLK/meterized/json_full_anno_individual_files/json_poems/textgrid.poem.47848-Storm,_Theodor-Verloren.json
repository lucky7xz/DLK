{"textgrid.poem.47848": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Verloren", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Holdes liegt mir in dem Sinn,", "tokens": ["Was", "Hol\u00b7des", "liegt", "mir", "in", "dem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ich vor Zeit einmal besessen;", "tokens": ["Das", "ich", "vor", "Zeit", "ein\u00b7mal", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df nicht, wo es kommen hin,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wo", "es", "kom\u00b7men", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch, was es war, ist mir vergessen.", "tokens": ["Auch", ",", "was", "es", "war", ",", "ist", "mir", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht \u2013 am fernen Waldesrand,", "tokens": ["Viel\u00b7leicht", "\u2013", "am", "fer\u00b7nen", "Wal\u00b7des\u00b7rand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo ich am lichten Junimorgen", "tokens": ["Wo", "ich", "am", "lich\u00b7ten", "Ju\u00b7ni\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u2013 Die Kinder klein und klein die Sorgen \u2013", "tokens": ["\u2013", "Die", "Kin\u00b7der", "klein", "und", "klein", "die", "Sor\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "KON", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit dir gesessen Hand in Hand,", "tokens": ["Mit", "dir", "ge\u00b7ses\u00b7sen", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Indes vom Fels die Quelle tropfte,", "tokens": ["In\u00b7des", "vom", "Fels", "die", "Quel\u00b7le", "tropf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Amsel schallend schlug im Grund,", "tokens": ["Die", "Am\u00b7sel", "schal\u00b7lend", "schlug", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mein Herz in gleichen Schl\u00e4gen klopfte", "tokens": ["Mein", "Herz", "in", "glei\u00b7chen", "Schl\u00e4\u00b7gen", "klopf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text"