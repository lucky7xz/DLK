{"textgrid.poem.40516": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf eines Berges R\u00fccken", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf eines Berges R\u00fccken", "tokens": ["Auf", "ei\u00b7nes", "Ber\u00b7ges", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df einst der liebe Gott,", "tokens": ["Sa\u00df", "einst", "der", "lie\u00b7be", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ma\u00df mit fr\u00f6hlichen Blicken,", "tokens": ["Und", "ma\u00df", "mit", "fr\u00f6h\u00b7li\u00b7chen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was rings dem Auge sich bot.", "tokens": ["Was", "rings", "dem", "Au\u00b7ge", "sich", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Er sah zu seinen F\u00fc\u00dfen", "tokens": ["Er", "sah", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewalt'ge Berge sich reih'n,", "tokens": ["Ge\u00b7walt'\u00b7ge", "Ber\u00b7ge", "sich", "reih'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und gr\u00fcne W\u00e4lder sprie\u00dfen", "tokens": ["Und", "gr\u00fc\u00b7ne", "W\u00e4l\u00b7der", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und goldne Saaten gedeih'n.", "tokens": ["Und", "gold\u00b7ne", "Saa\u00b7ten", "ge\u00b7deih'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Er sah die Quellen springen,", "tokens": ["Er", "sah", "die", "Quel\u00b7len", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er athmete Blumenduft,", "tokens": ["Er", "ath\u00b7me\u00b7te", "Blu\u00b7men\u00b7duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und h\u00f6rte die V\u00f6gel singen", "tokens": ["Und", "h\u00f6r\u00b7te", "die", "V\u00f6\u00b7gel", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In goldner Morgenluft.", "tokens": ["In", "gold\u00b7ner", "Mor\u00b7gen\u00b7luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da l\u00e4chelte zufrieden", "tokens": ["Da", "l\u00e4\u00b7chel\u00b7te", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er stille vor sich hin;", "tokens": ["Er", "stil\u00b7le", "vor", "sich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Menschen im Thal hernieden", "tokens": ["Die", "Men\u00b7schen", "im", "Thal", "her\u00b7nie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sah'n goldner die Berge gl\u00fchn.", "tokens": ["Sah'n", "gold\u00b7ner", "die", "Ber\u00b7ge", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Er sah nun lange mit Freude", "tokens": ["Er", "sah", "nun", "lan\u00b7ge", "mit", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herab auf seine Welt,", "tokens": ["Her\u00b7ab", "auf", "sei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprach: Bei meinem Eide,", "tokens": ["Und", "sprach", ":", "Bei", "mei\u00b7nem", "Ei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das hab' ich wohl bestellt!", "tokens": ["Das", "hab'", "ich", "wohl", "be\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und reichere Blumend\u00fcfte", "tokens": ["Und", "rei\u00b7che\u00b7re", "Blu\u00b7men\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erquollen bei seinem Wort,", "tokens": ["Er\u00b7quol\u00b7len", "bei", "sei\u00b7nem", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es rollte durch Erd' und L\u00fcfte", "tokens": ["Es", "roll\u00b7te", "durch", "Erd'", "und", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Harmonisches Klingen fort.", "tokens": ["Har\u00b7mo\u00b7ni\u00b7sches", "Klin\u00b7gen", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Welt lag in der Bl\u00fcthe,", "tokens": ["Die", "Welt", "lag", "in", "der", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es l\u00e4chelt' des Herrn Gesicht;", "tokens": ["Es", "l\u00e4\u00b7chelt'", "des", "Herrn", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da klang in seinem Gem\u00fcthe", "tokens": ["Da", "klang", "in", "sei\u00b7nem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Empor ein himmlisch Gedicht.", "tokens": ["Em\u00b7por", "ein", "himm\u00b7lisch", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Da wollt' er in Worte kleiden", "tokens": ["Da", "wollt'", "er", "in", "Wor\u00b7te", "klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und schreiben auf Pergament", "tokens": ["Und", "schrei\u00b7ben", "auf", "Per\u00b7ga\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "All' seine Sch\u00f6pferfreuden,", "tokens": ["All'", "sei\u00b7ne", "Sch\u00f6p\u00b7fer\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie nun sein Herz sie kennt.", "tokens": ["Wie", "nun", "sein", "Herz", "sie", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch als er's drauf besehen,", "tokens": ["Doch", "als", "er's", "drauf", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie's auf dem Blatte steht,", "tokens": ["Wie's", "auf", "dem", "Blat\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da war's auch ihm geschehen,", "tokens": ["Da", "wa\u00b7r's", "auch", "ihm", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wie's manchem Dichter geht:", "tokens": ["Wie's", "man\u00b7chem", "Dich\u00b7ter", "geht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nicht konnt' er treu berichten", "tokens": ["Nicht", "konnt'", "er", "treu", "be\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Herzens warmen Schlag;", "tokens": ["Des", "Her\u00b7zens", "war\u00b7men", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht konnt' er's sch\u00f6ner dichten,", "tokens": ["Nicht", "konnt'", "er's", "sch\u00f6\u00b7ner", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PIS", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als rings es vor ihm lag!", "tokens": ["Als", "rings", "es", "vor", "ihm", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da ri\u00df er's zu tausend St\u00fccken", "tokens": ["Da", "ri\u00df", "er's", "zu", "tau\u00b7send", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und gab's den Winden preis,", "tokens": ["Und", "gab's", "den", "Win\u00b7den", "preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah wieder mit frohen Blicken", "tokens": ["Sah", "wie\u00b7der", "mit", "fro\u00b7hen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf seinen Erdenkreis.", "tokens": ["Auf", "sei\u00b7nen", "Er\u00b7den\u00b7kreis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Doch wie nun hin und wieder", "tokens": ["Doch", "wie", "nun", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wind die St\u00fccke weht,", "tokens": ["Der", "Wind", "die", "St\u00fc\u00b7cke", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward aufs Thal hernieder", "tokens": ["Da", "ward", "aufs", "Thal", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Bl\u00fcthenregen ges\u00e4t! \u2013", "tokens": ["Ein", "Bl\u00fc\u00b7then\u00b7re\u00b7gen", "ge\u00b7s\u00e4t", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Wer Freitags auf der Reise,", "tokens": ["Wer", "Frei\u00b7tags", "auf", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Braucht nicht zu fasten dabei;", "tokens": ["Braucht", "nicht", "zu", "fas\u00b7ten", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVFIN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wer Sonntags auf der Reise,", "tokens": ["Wer", "Sonn\u00b7tags", "auf", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist von der Messe frei.", "tokens": ["Ist", "von", "der", "Mes\u00b7se", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "So hab' ich die\u00df Lied gesungen", "tokens": ["So", "hab'", "ich", "die\u00df", "Lied", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Statt eines Gebetes heut',", "tokens": ["Statt", "ei\u00b7nes", "Ge\u00b7be\u00b7tes", "heut'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Sonntagsglocken umklungen,", "tokens": ["Von", "Sonn\u00b7tags\u00b7glo\u00b7cken", "um\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von Bl\u00fcthen \u00fcberschneit.", "tokens": ["Von", "Bl\u00fc\u00b7then", "\u00fc\u00b7bersc\u00b7hneit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Auf eines Berges R\u00fccken", "tokens": ["Auf", "ei\u00b7nes", "Ber\u00b7ges", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df einst der liebe Gott,", "tokens": ["Sa\u00df", "einst", "der", "lie\u00b7be", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ma\u00df mit fr\u00f6hlichen Blicken,", "tokens": ["Und", "ma\u00df", "mit", "fr\u00f6h\u00b7li\u00b7chen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was rings dem Auge sich bot.", "tokens": ["Was", "rings", "dem", "Au\u00b7ge", "sich", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Er sah zu seinen F\u00fc\u00dfen", "tokens": ["Er", "sah", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewalt'ge Berge sich reih'n,", "tokens": ["Ge\u00b7walt'\u00b7ge", "Ber\u00b7ge", "sich", "reih'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PR