{"dta.poem.10725": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr die jhr h\u00f6ret an/ wie mancher Sturmwind wehet/", "tokens": ["Ihr", "die", "jhr", "h\u00f6\u00b7ret", "an", "/", "wie", "man\u00b7cher", "Sturm\u00b7wind", "we\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "VVFIN", "PTKVZ", "$(", "KOKOM", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch seufftzen ohne zahl in meinen Reimelein/", "tokens": ["Durch", "seufft\u00b7zen", "oh\u00b7ne", "zahl", "in", "mei\u00b7nen", "Rei\u00b7mel\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd einen weiten Bach darin/ voll Threnelein/", "tokens": ["Vnd", "ei\u00b7nen", "wei\u00b7ten", "Bach", "da\u00b7rin", "/", "voll", "Thre\u00b7nel\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$(", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd ein verletztes Hertz voll tausent Wunden sehet", "tokens": ["Vnd", "ein", "ver\u00b7letz\u00b7tes", "Hertz", "voll", "tau\u00b7sent", "Wun\u00b7den", "se\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er lernet wohl hierau\u00df/ wa\u00df man in lieb\u2019 au\u00dfstehet/", "tokens": ["Er", "ler\u00b7net", "wohl", "hier\u00b7au\u00df", "/", "wa\u00df", "man", "in", "lieb'", "au\u00df\u00b7ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$(", "KOUS", "PIS", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Darin die junge Zeit mich lie\u00df ergeben sein/", "tokens": ["Da\u00b7rin", "die", "jun\u00b7ge", "Zeit", "mich", "lie\u00df", "er\u00b7ge\u00b7ben", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Al \u00dfich f\u00fcr wahre lust hielt\u2019 einen falschen schein/", "tokens": ["Al", "\u00dfich", "f\u00fcr", "wah\u00b7re", "lust", "hielt'", "ei\u00b7nen", "fal\u00b7schen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dar\u00fcber mich jetz und hertzliche rew\u2019 vmbf\u00e4het:", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "mich", "jetz", "und", "hertz\u00b7li\u00b7che", "re\u00b7w'", "vmb\u00b7f\u00e4\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Vnd fliehet solche brunst vnd jhre s\u00fcsse Gifft/", "tokens": ["Vnd", "flie\u00b7het", "sol\u00b7che", "brunst", "vnd", "jhre", "s\u00fcs\u00b7se", "Gifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der eiteln sch\u00f6nheit glantz/ die vns das Hertz schnell trifft/", "tokens": ["Der", "ei\u00b7teln", "sch\u00f6n\u00b7heit", "glantz", "/", "die", "vns", "das", "Hertz", "schnell", "trifft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "PRELS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd angst vnd schmertzen vol witzlo\u00df herummer leitet", "tokens": ["Vnd", "angst", "vnd", "schmert\u00b7zen", "vol", "witz\u00b7lo\u00df", "her\u00b7um\u00b7mer", "lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "VVFIN", "ADJD", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ohn tugend ist sch\u00f6nheit nur ein triegliches Kleid/", "tokens": ["Ohn", "tu\u00b7gend", "ist", "sch\u00f6n\u00b7heit", "nur", "ein", "trieg\u00b7li\u00b7ches", "Kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wer solcher dienstbahr ist/ dem lohnet rew\u2019 vnd leidt:", "tokens": ["Wer", "sol\u00b7cher", "dienst\u00b7bahr", "ist", "/", "dem", "loh\u00b7net", "re\u00b7w'", "vnd", "leidt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "PDS", "VAFIN", "$(", "ART", "VVFIN", "NE", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Au\u00df tugent wahre lust allein wird zubereitet,", "tokens": ["Au\u00df", "tu\u00b7gent", "wah\u00b7re", "lust", "al\u00b7lein", "wird", "zu\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}