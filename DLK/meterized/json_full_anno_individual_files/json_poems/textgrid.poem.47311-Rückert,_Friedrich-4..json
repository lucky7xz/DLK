{"textgrid.poem.47311": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6chste Liebe, wo du thronest, la\u00df vor deinem Throne knien", "tokens": ["H\u00f6chs\u00b7te", "Lie\u00b7be", ",", "wo", "du", "thro\u00b7nest", ",", "la\u00df", "vor", "dei\u00b7nem", "Thro\u00b7ne", "kni\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Meine sch\u00f6nsten, ewig deinem Thron geweihten Melodien!", "tokens": ["Mei\u00b7ne", "sch\u00f6ns\u00b7ten", ",", "e\u00b7wig", "dei\u00b7nem", "Thron", "ge\u00b7weih\u00b7ten", "Me\u00b7lo\u00b7dien", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn sie wohlgef\u00e4llig deinem Ohre t\u00f6nen, wenn die Kraft", "tokens": ["Wenn", "sie", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "dei\u00b7nem", "Oh\u00b7re", "t\u00f6\u00b7nen", ",", "wenn", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auch in deine Seele wirket, die du ihnen hast verliehn;", "tokens": ["Auch", "in", "dei\u00b7ne", "See\u00b7le", "wir\u00b7ket", ",", "die", "du", "ih\u00b7nen", "hast", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "La\u00df sie danken, la\u00df sie beten, la\u00df sie fragen, la\u00df sie flehn:", "tokens": ["La\u00df", "sie", "dan\u00b7ken", ",", "la\u00df", "sie", "be\u00b7ten", ",", "la\u00df", "sie", "fra\u00b7gen", ",", "la\u00df", "sie", "flehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wo ist, der ein Stern auf Erden mir aus deiner H\u00f6h' erschien?", "tokens": ["Wo", "ist", ",", "der", "ein", "Stern", "auf", "Er\u00b7den", "mir", "aus", "dei\u00b7ner", "H\u00f6h'", "er\u00b7schien", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Der, sein Haupt mit deinen Rosen kr\u00e4nzend und sein Saitenspiel,", "tokens": ["Der", ",", "sein", "Haupt", "mit", "dei\u00b7nen", "Ro\u00b7sen", "kr\u00e4n\u00b7zend", "und", "sein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Liebetrunken mir vor\u00fcberzog, um mich dir nach zu ziehn;", "tokens": ["Lie\u00b7be\u00b7trun\u00b7ken", "mir", "vor\u00b7\u00fc\u00b7ber\u00b7zog", ",", "um", "mich", "dir", "nach", "zu", "ziehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "KOUI", "PPER", "PPER", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Der in wallenden Gewanden, am gebrochnen S\u00e4ulenschaft", "tokens": ["Der", "in", "wal\u00b7len\u00b7den", "Ge\u00b7wan\u00b7den", ",", "am", "ge\u00b7broch\u00b7nen", "S\u00e4u\u00b7len\u00b7schaft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Lehnend, Lieder str\u00f6mt', auf deren Wog' er selber wollt' entfliehn;", "tokens": ["Leh\u00b7nend", ",", "Lie\u00b7der", "str\u00f6mt'", ",", "auf", "de\u00b7ren", "Wo\u00b7g'", "er", "sel\u00b7ber", "wollt'", "ent\u00b7fliehn", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "VVFIN", "$,", "APPR", "PRELAT", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.11": {"text": "Wo ist der dir Zugeflohne? Sag' mir's, Liebe, wie du einst", "tokens": ["Wo", "ist", "der", "dir", "Zu\u00b7ge\u00b7floh\u00b7ne", "?", "Sag'", "mir's", ",", "Lie\u00b7be", ",", "wie", "du", "einst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "PPER", "NN", "$.", "NN", "NE", "$,", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Ihn beseligt hast auf Erden, wo du nun beseligst ihn?", "tokens": ["Ihn", "be\u00b7se\u00b7ligt", "hast", "auf", "Er\u00b7den", ",", "wo", "du", "nun", "be\u00b7se\u00b7ligst", "ihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "APPR", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Wo, Volkstrachten ausgezogen, Stammabzeichen abgelegt,", "tokens": ["Wo", ",", "Volks\u00b7trach\u00b7ten", "aus\u00b7ge\u00b7zo\u00b7gen", ",", "Stamm\u00b7ab\u00b7zei\u00b7chen", "ab\u00b7ge\u00b7legt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "VVPP", "$,", "NN", "VVPP", "$,"], "meter": "-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.14": {"text": "Schmelzen Kastenunterschied' in deinen ew'gen Harmonien;", "tokens": ["Schmel\u00b7zen", "Kas\u00b7ten\u00b7un\u00b7ter\u00b7schied'", "in", "dei\u00b7nen", "ew'\u00b7gen", "Har\u00b7mo\u00b7ni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Wo ist unter allen Heil'gen aller Zonen (Heil sei dir,", "tokens": ["Wo", "ist", "un\u00b7ter", "al\u00b7len", "Heil'\u00b7gen", "al\u00b7ler", "Zo\u00b7nen", "(", "Heil", "sei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "PIAT", "NN", "PIAT", "NN", "$(", "NN", "VAFIN", "PPER", "$,"], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.16": {"text": "Heilig mir sein Angedenken!) ", "tokens": ["Hei\u00b7lig", "mir", "sein", "An\u00b7ge\u00b7den\u00b7ken", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "PPER", "PPOSAT", "N