{"dta.poem.21421": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch unsre sch\u00f6ne Poesie", "tokens": ["Auch", "uns\u00b7re", "sch\u00f6\u00b7ne", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erlischt, sie ist schon ein wenig", "tokens": ["Er\u00b7lischt", ",", "sie", "ist", "schon", "ein", "we\u00b7nig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ART", "PIS"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erloschen; mit andern K\u00f6nigen stirbt", "tokens": ["Er\u00b7lo\u00b7schen", ";", "mit", "an\u00b7dern", "K\u00f6\u00b7ni\u00b7gen", "stirbt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch Freiligraths Mohrenk\u00f6nig.", "tokens": ["Auch", "Frei\u00b7li\u00b7graths", "Moh\u00b7ren\u00b7k\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eder Enkel wird essen und trinken genug,", "tokens": ["\u201e", "der", "En\u00b7kel", "wird", "es\u00b7sen", "und", "trin\u00b7ken", "ge\u00b7nug", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVINF", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Doch nicht in beschaulicher Stille;", "tokens": ["Doch", "nicht", "in", "be\u00b7schau\u00b7li\u00b7cher", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Es poltert heran ein Spektakelst\u00fcck,", "tokens": ["Es", "pol\u00b7tert", "he\u00b7ran", "ein", "Spek\u00b7ta\u00b7kel\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu Ende geht die Idylle.", "tokens": ["Zu", "En\u00b7de", "geht", "die", "I\u00b7dyl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201eo, k\u00f6nntest du schweigen, ich w\u00fcrde dir", "tokens": ["\u201e", "o", ",", "k\u00f6nn\u00b7test", "du", "schwei\u00b7gen", ",", "ich", "w\u00fcr\u00b7de", "dir"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "$,", "VMFIN", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Buch des Schicksals entsiegeln,", "tokens": ["Das", "Buch", "des", "Schick\u00b7sals", "ent\u00b7sie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich lie\u00dfe dir sp\u00e4tere Zeiten seh\u2019n", "tokens": ["Ich", "lie\u00b7\u00dfe", "dir", "sp\u00e4\u00b7te\u00b7re", "Zei\u00b7ten", "seh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In meinen Zauberspiegeln.", "tokens": ["In", "mei\u00b7nen", "Zau\u00b7ber\u00b7spie\u00b7geln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ewas ich den sterblichen Menschen nie", "tokens": ["\u201e", "was", "ich", "den", "sterb\u00b7li\u00b7chen", "Men\u00b7schen", "nie"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gezeigt, ich m\u00f6cht\u2019 es dir zeigen:", "tokens": ["Ge\u00b7zeigt", ",", "ich", "m\u00f6cht'", "es", "dir", "zei\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Zukunft deines Vaterlands \u2013", "tokens": ["Die", "Zu\u00b7kunft", "dei\u00b7nes", "Va\u00b7ter\u00b7lands", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ach! du kannst nicht schweigen!\u201c", "tokens": ["Doch", "ach", "!", "du", "kannst", "nicht", "schwei\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}