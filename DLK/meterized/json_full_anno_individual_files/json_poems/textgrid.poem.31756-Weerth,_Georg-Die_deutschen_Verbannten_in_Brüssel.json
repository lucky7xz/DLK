{"textgrid.poem.31756": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die deutschen Verbannten in Br\u00fcssel", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und in den Kaffeeh\u00e4usern von Br\u00fcssel,", "tokens": ["Und", "in", "den", "Kaf\u00b7fee\u00b7h\u00e4u\u00b7sern", "von", "Br\u00fcs\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sa\u00dfen sie und weinten", "tokens": ["Da", "sa\u00b7\u00dfen", "sie", "und", "wein\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hingen die Paletots an die W\u00e4nde", "tokens": ["Und", "hin\u00b7gen", "die", "Pa\u00b7le\u00b7tots", "an", "die", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und tranken Mokka mit Zucker und Kognak", "tokens": ["Und", "tran\u00b7ken", "Mok\u00b7ka", "mit", "Zu\u00b7cker", "und", "Ko\u00b7gnak"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und seufzten und jammerten sehr \u2013 wenn", "tokens": ["Und", "seufz\u00b7ten", "und", "jam\u00b7mer\u00b7ten", "sehr", "\u2013", "wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "$(", "KOUS"], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dein sie gedachten, germanische Heimat!", "tokens": ["Dein", "sie", "ge\u00b7dach\u00b7ten", ",", "ger\u00b7ma\u00b7ni\u00b7sche", "Hei\u00b7mat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verbannte waren's. Der Zorn des", "tokens": ["Ver\u00b7bann\u00b7te", "wa\u00b7ren'", "s.", "Der", "Zorn", "des"], "token_info": ["word", "word", "abbreviation", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVIMP", "ART", "NN", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sechsunddrei\u00dfigeinigen deutschen", "tokens": ["Sech\u00b7sund\u00b7drei\u00b7\u00dfi\u00b7gei\u00b7ni\u00b7gen", "deut\u00b7schen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bundestag-Gottes verstie\u00df sie \u2013", "tokens": ["Bun\u00b7des\u00b7tag\u00b7Got\u00b7tes", "ver\u00b7stie\u00df", "sie", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Stie\u00df sie hinaus, die Ge\u00e4chteten,", "tokens": ["Stie\u00df", "sie", "hin\u00b7aus", ",", "die", "Ge\u00e4ch\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lieblos hinaus in des Auslands", "tokens": ["Lieb\u00b7los", "hin\u00b7aus", "in", "des", "Aus\u00b7lands"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APZR", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weiche, sammetgepolsterte Sessel.", "tokens": ["Wei\u00b7che", ",", "sam\u00b7met\u00b7ge\u00b7pols\u00b7ter\u00b7te", "Ses\u00b7sel", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sinnend schaut ich sie oft; und entsetzt dann", "tokens": ["Sin\u00b7nend", "schaut", "ich", "sie", "oft", ";", "und", "ent\u00b7setzt", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV", "$.", "KON", "VVFIN", "ADV"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "H\u00f6rt ich, wie laut sie zu klagen", "tokens": ["H\u00f6rt", "ich", ",", "wie", "laut", "sie", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Erhoben: \u00bbO weh uns! Nimmer", "tokens": ["Er\u00b7ho\u00b7ben", ":", "\u00bb", "O", "weh", "uns", "!", "Nim\u00b7mer"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$.", "$(", "ITJ", "ADV", "PPER", "$.", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Essen wir jetzt mehr deinen", "tokens": ["Es\u00b7sen", "wir", "jetzt", "mehr", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Pumpernickel, Westfalen! und", "tokens": ["Pum\u00b7per\u00b7ni\u00b7ckel", ",", "West\u00b7fa\u00b7len", "!", "und"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$.", "KON"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Posen, deine Kapusta!", "tokens": ["Po\u00b7sen", ",", "dei\u00b7ne", "Ka\u00b7pus\u00b7ta", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht mehr rauschen die Fichten uns deiner", "tokens": ["Nicht", "mehr", "rau\u00b7schen", "die", "Fich\u00b7ten", "uns", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN", "PPER", "PPOSAT"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Seligen Steppen, o Uckermark! Nicht mehr", "tokens": ["Se\u00b7li\u00b7gen", "Step\u00b7pen", ",", "o", "U\u00b7cker\u00b7mark", "!", "Nicht", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "FM", "NE", "$.", "PTKNEG", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "F\u00fchlen den Bi\u00df wir deiner", "tokens": ["F\u00fch\u00b7len", "den", "Bi\u00df", "wir", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "KOUS", "PPER", "PPOSAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Kasernen-Wanzen, o Preu\u00dfen! Und nicht mehr", "tokens": ["Ka\u00b7ser\u00b7nen\u00b7Wan\u00b7zen", ",", "o", "Preu\u00b7\u00dfen", "!", "Und", "nicht", "mehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$.", "KON", "PTKNEG", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sinken entz\u00fcckt wir an deine", "tokens": ["Sin\u00b7ken", "ent\u00b7z\u00fcckt", "wir", "an", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "G\u00e4nsebr\u00fcste, ambrosisches Pommern!", "tokens": ["G\u00e4n\u00b7se\u00b7br\u00fcs\u00b7te", ",", "am\u00b7bro\u00b7si\u00b7sches", "Pom\u00b7mern", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Nicht mehr t\u00f6net der M\u00e4nner der", "tokens": ["Nicht", "mehr", "t\u00f6\u00b7net", "der", "M\u00e4n\u00b7ner", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN", "ART"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bernsteink\u00fcst liberales Gejammer", "tokens": ["Bern\u00b7stein\u00b7k\u00fcst", "li\u00b7be\u00b7ra\u00b7les", "Ge\u00b7jam\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Erfreulich ins Ohr uns! \u2013 Nicht mehr", "tokens": ["Er\u00b7freu\u00b7lich", "ins", "Ohr", "uns", "!", "\u2013", "Nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PPER", "$.", "$(", "PTKNEG", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Werden wir Dome erbaun und", "tokens": ["Wer\u00b7den", "wir", "Do\u00b7me", "er\u00b7baun", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "VVINF", "KON"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Betrinken mit euch uns, ihr", "tokens": ["Be\u00b7trin\u00b7ken", "mit", "euch", "uns", ",", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PPER", "PPER", "$,", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Heiligen K\u00f6lner!", "tokens": ["Hei\u00b7li\u00b7gen", "K\u00f6l\u00b7ner", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ferne die Heimat! Ferne ja alles, was", "tokens": ["Fer\u00b7ne", "die", "Hei\u00b7mat", "!", "Fer\u00b7ne", "ja", "al\u00b7les", ",", "was"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$.", "NN", "ADV", "PIS", "$,", "PWS"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Reiz noch dem Leben verlieh und das Dasein", "tokens": ["Reiz", "noch", "dem", "Le\u00b7ben", "ver\u00b7lieh", "und", "das", "Da\u00b7sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "K\u00f6stlich machte \u2013 und traurig", "tokens": ["K\u00f6st\u00b7lich", "mach\u00b7te", "\u2013", "und", "trau\u00b7rig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "$(", "KON", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sitzen wir, ach, wir gro\u00dfen, blonden", "tokens": ["Sit\u00b7zen", "wir", ",", "ach", ",", "wir", "gro\u00b7\u00dfen", ",", "blon\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "ITJ", "$,", "PPER", "ADJA", "$,", "ADJA"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Teutonen nun unter den kleinen", "tokens": ["Teu\u00b7to\u00b7nen", "nun", "un\u00b7ter", "den", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Br\u00e4unlichen Belgiern!", "tokens": ["Br\u00e4un\u00b7li\u00b7chen", "Bel\u00b7gi\u00b7ern", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "M\u00fcssen Burgunder trinken und", "tokens": ["M\u00fcs\u00b7sen", "Bur\u00b7gun\u00b7der", "trin\u00b7ken", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVINF", "KON"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Leid'gen Champagner und Austern", "tokens": ["Lei\u00b7d'\u00b7gen", "Cham\u00b7pag\u00b7ner", "und", "Aus\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Essen, Ostender, Fasanen und t\u00eate de", "tokens": ["Es\u00b7sen", ",", "Os\u00b7ten\u00b7der", ",", "Fa\u00b7sa\u00b7nen", "und", "t\u00eate", "de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NE", "NE"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Veau en tortue und was sonst noch", "tokens": ["Vea\u00b7u", "en", "tor\u00b7tue", "und", "was", "sonst", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "KON", "PWS", "ADV", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bietet die Fremde an kaum wohl", "tokens": ["Bie\u00b7tet", "die", "Frem\u00b7de", "an", "kaum", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADV", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Genie\u00dfbaren Sachen!", "tokens": ["Ge\u00b7nie\u00df\u00b7ba\u00b7ren", "Sa\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "M\u00fcssen statt lieblich deutscher", "tokens": ["M\u00fcs\u00b7sen", "statt", "lieb\u00b7lich", "deut\u00b7scher"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vergi\u00dfmeinnicht-Kinder des Auslands", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht\u00b7Kin\u00b7der", "des", "Aus\u00b7lands"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schwarzumlockte brennende", "tokens": ["Schwar\u00b7zum\u00b7lock\u00b7te", "bren\u00b7nen\u00b7de"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJA"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Rosen jetzt k\u00fcssen und", "tokens": ["Ro\u00b7sen", "jetzt", "k\u00fcs\u00b7sen", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "VVINF", "KON"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Tanzen Cancan am Sabbat, wo sonst wir", "tokens": ["Tan\u00b7zen", "Can\u00b7can", "am", "Sab\u00b7bat", ",", "wo", "sonst", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "APPRART", "NN", "$,", "PWAV", "ADV", "PPER"], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Br\u00fcnstig gebetet in Odins ragenden Tempeln.", "tokens": ["Br\u00fcns\u00b7tig", "ge\u00b7be\u00b7tet", "in", "O\u00b7dins", "ra\u00b7gen\u00b7den", "Tem\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "M\u00fcssen allein jetzt wandern den dorn'gen", "tokens": ["M\u00fcs\u00b7sen", "al\u00b7lein", "jetzt", "wan\u00b7dern", "den", "dorn'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lebensweg, nicht l\u00e4nger bewacht von", "tokens": ["Le\u00b7bens\u00b7weg", ",", "nicht", "l\u00e4n\u00b7ger", "be\u00b7wacht", "von"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PTKNEG", "ADJD", "VVPP", "APPR"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "V\u00e4terlichen Gendarmen, die gern uns", "tokens": ["V\u00e4\u00b7ter\u00b7li\u00b7chen", "Gen\u00b7dar\u00b7men", ",", "die", "gern", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "PPER"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Stets daheim gesch\u00fctzt vor der Pest", "tokens": ["Stets", "da\u00b7heim", "ge\u00b7sch\u00fctzt", "vor", "der", "Pest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Moderner Ideen und", "tokens": ["Mo\u00b7der\u00b7ner", "I\u00b7deen", "und"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "KON"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Hochverr\u00e4trischer Tollheit!", "tokens": ["Hoch\u00b7ver\u00b7r\u00e4t\u00b7ri\u00b7scher", "Toll\u00b7heit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.10": {"line.1": {"text": "Ach! Verlassen sind wir; und ihr nur", "tokens": ["Ach", "!", "Ver\u00b7las\u00b7sen", "sind", "wir", ";", "und", "ihr", "nur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "VAFIN", "PPER", "$.", "KON", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nehmet noch Anteil an uns, ihr teuren", "tokens": ["Neh\u00b7met", "noch", "An\u00b7teil", "an", "uns", ",", "ihr", "teu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": [