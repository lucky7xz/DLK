word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "$(", "ADV", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Wenn sich der Jugend T\u00fcck und Bo\u00dfheit will erheben/", "tokens": ["Wenn", "sich", "der", "Ju\u00b7gend", "T\u00fcck", "und", "Bo\u00df\u00b7heit", "will", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "So hauet er mit Recht auf ihren R\u00fccken drein.", "tokens": ["So", "hau\u00b7et", "er", "mit", "Recht", "auf", "ih\u00b7ren", "R\u00fc\u00b7cken", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Gott auch/ damit wir uns in S\u00fcnden nicht vergraben/", "tokens": ["Gott", "auch", "/", "da\u00b7mit", "wir", "uns", "in", "S\u00fcn\u00b7den", "nicht", "ver\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "KOUS", "PPER", "PRF", "APPR", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Und mit der tollen Welt zu grund und scheitern gehn/", "tokens": ["Und", "mit", "der", "tol\u00b7len", "Welt", "zu", "grund", "und", "schei\u00b7tern", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "So geisselt er uns offt als wie muthwillge Knaben/", "tokens": ["So", "geis\u00b7selt", "er", "uns", "offt", "als", "wie", "mut\u00b7hwill\u00b7ge", "Kna\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "KOUS", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Damit wir seine Recht und Satzungen verstehn.", "tokens": ["Da\u00b7mit", "wir", "sei\u00b7ne", "Recht", "und", "Sat\u00b7zun\u00b7gen", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Indessen bleibt bey ihm doch stets ein Vater-Hertze:", "tokens": ["In\u00b7des\u00b7sen", "bleibt", "bey", "ihm", "doch", "stets", "ein", "Va\u00b7ter\u00b7Hert\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Als wie ein Schulen-Mann stets seine Jugend liebt.", "tokens": ["Als", "wie", "ein", "Schu\u00b7len\u00b7Mann", "stets", "sei\u00b7ne", "Ju\u00b7gend", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.92": {"text": "Ein Christ als wie das ", "tokens": ["Ein", "Christ", "als", "wie", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "KOKOM", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.93": {"text": "Du must durch keinen Fall den Muth dir lassen binden/", "tokens": ["Du", "must", "durch", "kei\u00b7nen", "Fall", "den", "Muth", "dir", "las\u00b7sen", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "ART", "NN", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Steh hohen Felsen gleich in deinem Thr\u00e4nen Meer;", "tokens": ["Steh", "ho\u00b7hen", "Fel\u00b7sen", "gleich", "in", "dei\u00b7nem", "Thr\u00e4\u00b7nen", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Ein lang und grosser Baum verlacht den Sturm von Winden/", "tokens": ["Ein", "lang", "und", "gros\u00b7ser", "Baum", "ver\u00b7lacht", "den", "Sturm", "von", "Win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "Hingegen reicht das Rohr bald seine Kolben her.", "tokens": ["Hin\u00b7ge\u00b7gen", "reicht", "das", "Rohr", "bald", "sei\u00b7ne", "Kol\u00b7ben", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.97": {"text": "Du hast die Welt durchguckt/ du weist was wir geniessen/", "tokens": ["Du", "hast", "die", "Welt", "durch\u00b7guckt", "/", "du", "weist", "was", "wir", "ge\u00b7nies\u00b7sen", "/"], "token_inf