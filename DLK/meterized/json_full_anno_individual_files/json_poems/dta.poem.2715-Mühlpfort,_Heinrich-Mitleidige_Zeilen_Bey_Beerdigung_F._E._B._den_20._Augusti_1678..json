{"dta.poem.2715": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mitleidige Zeilen  \n  Bey Beerdigung F. E. B. den 20. Augusti  \n 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Brehmer wo du hier nicht sch\u00f6ne Blumen findest/", "tokens": ["Dein", "Breh\u00b7mer", "wo", "du", "hier", "nicht", "sch\u00f6\u00b7ne", "Blu\u00b7men", "fin\u00b7dest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PWAV", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Tuberosen nicht in meinen Zeilen stehn/", "tokens": ["Wo", "Tu\u00b7be\u00b7ro\u00b7sen", "nicht", "in", "mei\u00b7nen", "Zei\u00b7len", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch du Granaten-Bl\u00fcth und die Jesminen bindest/", "tokens": ["Noch", "du", "Gra\u00b7na\u00b7ten\u00b7B\u00b7l\u00fcth", "und", "die", "Jes\u00b7mi\u00b7nen", "bin\u00b7dest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "KON", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "So dencke da\u00df mein Fu\u00df nicht kan auff Rosen gehn.", "tokens": ["So", "den\u00b7cke", "da\u00df", "mein", "Fu\u00df", "nicht", "kan", "auff", "Ro\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPOSAT", "NN", "PTKNEG", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich liege selbsten kranck/ und sol dir Trost zuschicken:", "tokens": ["Ich", "lie\u00b7ge", "selbs\u00b7ten", "kranck", "/", "und", "sol", "dir", "Trost", "zu\u00b7schi\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$(", "KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn hat ein siecher Artzt dem Schmertzen obgesiegt?", "tokens": ["Wenn", "hat", "ein", "sie\u00b7cher", "Artzt", "dem", "Schmert\u00b7zen", "ob\u00b7ge\u00b7siegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mag eine solche Hand die Wunden wol zudr\u00fccken/", "tokens": ["Mag", "ei\u00b7ne", "sol\u00b7che", "Hand", "die", "Wun\u00b7den", "wol", "zu\u00b7dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PIAT", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die von der Gicht gequ\u00e4lt gantz unbeweglich liegt?", "tokens": ["Die", "von", "der", "Gicht", "ge\u00b7qu\u00e4lt", "gantz", "un\u00b7be\u00b7weg\u00b7lich", "liegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wiewol ich bin kein Stein/ ich bin nicht ohn Erbarmen/", "tokens": ["Wie\u00b7wol", "ich", "bin", "kein", "Stein", "/", "ich", "bin", "nicht", "ohn", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PIAT", "NN", "$(", "PPER", "VAFIN", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nun deines Hertzens Hertz und Augen-Trost verf\u00e4llt/", "tokens": ["Nun", "dei\u00b7nes", "Hert\u00b7zens", "Hertz", "und", "Au\u00b7gen\u00b7Trost", "ver\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun statt der s\u00fcssen Lust dich Angst und Noth umbarmen", "tokens": ["Nun", "statt", "der", "s\u00fcs\u00b7sen", "Lust", "dich", "Angst", "und", "Noth", "um\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und bleiche K\u00fcmmern\u00fc\u00df in ihren Banden h\u00e4lt.", "tokens": ["Und", "blei\u00b7che", "K\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "in", "ih\u00b7ren", "Ban\u00b7den", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein verzeihe mir/ du Mann von deutschen Sitten/", "tokens": ["Al\u00b7lein", "ver\u00b7zei\u00b7he", "mir", "/", "du", "Mann", "von", "deut\u00b7schen", "Sit\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PPER", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du Bild der Redligkeit und Abrie\u00df alter Treu/", "tokens": ["Du", "Bild", "der", "Red\u00b7lig\u00b7keit", "und", "Ab\u00b7rie\u00df", "al\u00b7ter", "Treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo nicht mein ", "tokens": ["Wo", "nicht", "mein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PTKNEG", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Und mir der Musen Gunst legt keine Lorbern bey.", "tokens": ["Und", "mir", "der", "Mu\u00b7sen", "Gunst", "legt", "kei\u00b7ne", "Lor\u00b7bern", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Leid/ das hefftig ist/ fragt nicht nach Wort-Gepr\u00e4nge/", "tokens": ["Ein", "Leid", "/", "das", "heff\u00b7tig", "ist", "/", "fragt", "nicht", "nach", "Wor\u00b7tGe\u00b7pr\u00e4n\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "VAFIN", "$(", "VVFIN", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und Kunst und Zierligkeit findt nicht bey Thr\u00e4nen statt:", "tokens": ["Und", "Kunst", "und", "Zier\u00b7lig\u00b7keit", "findt", "nicht", "bey", "Thr\u00e4\u00b7nen", "statt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zu dem ist mir bekand/ da\u00df von dergleichen M\u00e4nge", "tokens": ["Zu", "dem", "ist", "mir", "be\u00b7kand", "/", "da\u00df", "von", "derg\u00b7lei\u00b7chen", "M\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VAFIN", "PPER", "VVFIN", "$(", "KOUS", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dein reicher B\u00fccher Schatz schon einen Vorrath hat.", "tokens": ["Dein", "rei\u00b7cher", "B\u00fc\u00b7cher", "Schatz", "schon", "ei\u00b7nen", "Vor\u00b7rath", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Offt kan ein schlechtes Wort tieff ins Gem\u00fcthe dringen/", "tokens": ["Offt", "kan", "ein", "schlech\u00b7tes", "Wort", "tieff", "ins", "Ge\u00b7m\u00fc\u00b7the", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn es den Uhrsprung nur aus treuem Hertzen nimmt.", "tokens": ["Wenn", "es", "den", "Uhrs\u00b7prung", "nur", "aus", "treu\u00b7em", "Hert\u00b7zen", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein Satz der ist sehr kurtz/ den ich zum Trost wil bringen:", "tokens": ["Mein", "Satz", "der", "ist", "sehr", "kurtz", "/", "den", "ich", "zum", "Trost", "wil", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADV", "ADJD", "$(", "PRELS", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Gott hat/ ", "tokens": ["Gott", "hat", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Warumb es sey geschehn/ das ist nicht auszufragen/", "tokens": ["Wa\u00b7rumb", "es", "sey", "ge\u00b7schehn", "/", "das", "ist", "nicht", "aus\u00b7zu\u00b7fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$(", "PDS", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In dieses Rathhaus guckt kein sterblich Auge nicht.", "tokens": ["In", "die\u00b7ses", "Rath\u00b7haus", "guckt", "kein", "sterb\u00b7lich", "Au\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PIAT", "ADJD", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du must nur mit Gedult den bittern Fall ertragen/", "tokens": ["Du", "must", "nur", "mit", "Ge\u00b7dult", "den", "bit\u00b7tern", "Fall", "er\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Di\u00df heischt dein Christenthum/ di\u00df fordert deine Pflicht.", "tokens": ["Di\u00df", "heischt", "dein", "Chris\u00b7ten\u00b7thum", "/", "di\u00df", "for\u00b7dert", "dei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$(", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich r\u00e4um\u2019 es willig ein/ da\u00df du itzt Myrrhen schmeckest/", "tokens": ["Ich", "r\u00e4um'", "es", "wil\u00b7lig", "ein", "/", "da\u00df", "du", "itzt", "Myr\u00b7rhen", "schme\u00b7ckest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und da\u00df mit Aloe dein Trincken sey vermischt.", "tokens": ["Und", "da\u00df", "mit", "A\u00b7loe", "dein", "Trin\u00b7cken", "sey", "ver\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Ich glaub es/ da\u00df du dich mit Kummer-Nesseln deckest/", "tokens": ["Ich", "glaub", "es", "/", "da\u00df", "du", "dich", "mit", "Kum\u00b7mer\u00b7Nes\u00b7seln", "de\u00b7ckest", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.32": {"text": "Und h\u00e4uffig Seelen-weh die Wunden dir erfrischt.", "tokens": ["Und", "h\u00e4uf\u00b7fig", "See\u00b7len\u00b7weh", "die", "Wun\u00b7den", "dir", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Warumb? dein Schatz ist hin/ ein Weib von seltner Tugend/", "tokens": ["Wa\u00b7rumb", "?", "dein", "Schatz", "ist", "hin", "/", "ein", "Weib", "von", "selt\u00b7ner", "Tu\u00b7gend", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der H\u00e4u\u00dfligkeit Smaragd/ der Fr\u00f6mmigkeit Saphier.", "tokens": ["Der", "H\u00e4u\u00df\u00b7lig\u00b7keit", "Sma\u00b7ragd", "/", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "Sa\u00b7phier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.35": {"text": "Die mit dir hat getheilt Hertz/ Leben/ Leib und Jugend", "tokens": ["Die", "mit", "dir", "hat", "ge\u00b7theilt", "Hertz", "/", "Le\u00b7ben", "/", "Leib", "und", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VAFIN", "VVPP", "NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und die ohn unterla\u00df k\u00f6mmt deinen Augen f\u00fcr.", "tokens": ["Und", "die", "ohn", "un\u00b7ter\u00b7la\u00df", "k\u00f6mmt", "dei\u00b7nen", "Au\u00b7gen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie hat dich mehr geliebt als wohl ihr eigne Seele/", "tokens": ["Sie", "hat", "dich", "mehr", "ge\u00b7liebt", "als", "wohl", "ihr", "eig\u00b7ne", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Zu deinem Wohlfahrts-Stand den besten Grund gelegt:", "tokens": ["Zu", "dei\u00b7nem", "Wohl\u00b7fahrts\u00b7Stand", "den", "bes\u00b7ten", "Grund", "ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie blieb den Schnecken gleich in ihres Hauses H\u00f6le/", "tokens": ["Sie", "blieb", "den", "Schne\u00b7cken", "gleich", "in", "ih\u00b7res", "Hau\u00b7ses", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und hat des Rockens mehr als andrer Lust gepflegt:", "tokens": ["Und", "hat", "des", "Ro\u00b7ckens", "mehr", "als", "an\u00b7drer", "Lust", "ge\u00b7pflegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIAT", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie war Abigail die wei\u00dflich konte rathen/", "tokens": ["Sie", "war", "A\u00b7bi\u00b7gail", "die", "wei\u00df\u00b7lich", "kon\u00b7te", "ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "ADJD", "VMFIN", "VVINF", "$("], "me