{"dta.poem.13100": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewer hat durch ein Gedicht das rauhe Volck gelehrt,", "tokens": ["\u201e", "wer", "hat", "durch", "ein", "Ge\u00b7dicht", "das", "rau\u00b7he", "Volck", "ge\u00b7lehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "350\u201dWarum ein Mensch die Macht des Himmels f\u00fcrchtet, ehrt?", "tokens": ["\"", "Wa\u00b7rum", "ein", "Mensch", "die", "Macht", "des", "Him\u00b7mels", "f\u00fcrch\u00b7tet", ",", "ehrt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewas Krieg und Friede sey? was Recht? was ein Geseze?", "tokens": ["\u201e", "was", "Krieg", "und", "Frie\u00b7de", "sey", "?", "was", "Recht", "?", "was", "ein", "Ge\u00b7se\u00b7ze", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "KON", "NN", "VAFIN", "$.", "PWS", "NN", "$.", "PWS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewas GOtt und die Natur in unsre Sinnen eze?", "tokens": ["\u201e", "was", "Gott", "und", "die", "Na\u00b7tur", "in", "uns\u00b7re", "Sin\u00b7nen", "e\u00b7ze", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewie ward der Tugend Werth demselben angezeigt?", "tokens": ["\u201e", "wie", "ward", "der", "Tu\u00b7gend", "Werth", "dem\u00b7sel\u00b7ben", "an\u00b7ge\u00b7zeigt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "NN", "PDAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewie ward es endlich mir und anderen geneigt?", "tokens": ["\u201e", "wie", "ward", "es", "end\u00b7lich", "mir", "und", "an\u00b7de\u00b7ren", "ge\u00b7neigt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "PPER", "KON", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "355\u201dIch bin der Urbeginn, aus welchem alles flosse,", "tokens": ["\"", "Ich", "bin", "der", "Ur\u00b7be\u00b7ginn", ",", "aus", "wel\u00b7chem", "al\u00b7les", "flos\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewas nach und nach die Welt mit Wohlfart \u00fcbergosse.", "tokens": ["\u201e", "was", "nach", "und", "nach", "die", "Welt", "mit", "Wohl\u00b7fart", "\u00fc\u00b7ber\u00b7gos\u00b7se", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "KON", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}