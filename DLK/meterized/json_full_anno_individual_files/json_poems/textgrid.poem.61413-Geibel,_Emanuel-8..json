{"textgrid.poem.61413": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df mit den Gro\u00dfen zu sein ist g\u00f6ttliches Los. An Achilleus", "tokens": ["Gro\u00df", "mit", "den", "Gro\u00b7\u00dfen", "zu", "sein", "ist", "g\u00f6tt\u00b7li\u00b7ches", "Los", ".", "An", "A\u00b7chil\u00b7leus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PTKZU", "VAINF", "VAFIN", "ADJA", "NN", "$.", "APPR", "NE"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Lehnt sich Patroklos im Kampf, wenn er um Ilium braust;", "tokens": ["Lehnt", "sich", "Pat\u00b7ro\u00b7klos", "im", "Kampf", ",", "wenn", "er", "um", "I\u00b7lium", "braust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Teukros entsendet den Pfeil umschirmt vom Schilde des Ajax,", "tokens": ["Teu\u00b7kros", "ent\u00b7sen\u00b7det", "den", "Pfeil", "um\u00b7schirmt", "vom", "Schil\u00b7de", "des", "A\u00b7jax", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NE", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nestor sitzet, der Greis, neben Odysseus im Rat;", "tokens": ["Nes\u00b7tor", "sit\u00b7zet", ",", "der", "Greis", ",", "ne\u00b7ben", "O\u00b7dys\u00b7seus", "im", "Rat", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "$,", "APPR", "NE", "APPRART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und dann wandelt Homer mit der goldenen Harfe. Begeistert", "tokens": ["Und", "dann", "wan\u00b7delt", "Ho\u00b7mer", "mit", "der", "gol\u00b7de\u00b7nen", "Har\u00b7fe", ".", "Be\u00b7geis\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "NE", "APPR", "ART", "ADJA", "NN", "$.", "NN"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Mit unsterblichem Preis kr\u00f6nt er der Helden Gelock.", "tokens": ["Mit", "uns\u00b7terb\u00b7li\u00b7chem", "Preis", "kr\u00f6nt", "er", "der", "Hel\u00b7den", "Ge\u00b7lock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Aber in kleinlicher Zeit, einsam wie ein Adler, das Gro\u00dfe", "tokens": ["A\u00b7ber", "in", "klein\u00b7li\u00b7cher", "Zeit", ",", "ein\u00b7sam", "wie", "ein", "Ad\u00b7ler", ",", "das", "Gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "K\u00fchn zu versuchen und, wenn blutend der Fittich versagt,", "tokens": ["K\u00fchn", "zu", "ver\u00b7su\u00b7chen", "und", ",", "wenn", "blu\u00b7tend", "der", "Fit\u00b7tich", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "KON", "$,", "KOUS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Noch mit sterbendem Blick nach dem heiligen Ziele zu deuten,", "tokens": ["Noch", "mit", "ster\u00b7ben\u00b7dem", "Blick", "nach", "dem", "hei\u00b7li\u00b7gen", "Zie\u00b7le", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Wahrlich, \u00e4hnlichen Ruhms d\u00fcnkt es mich w\u00fcrdig zu sein.", "tokens": ["Wahr\u00b7lich", ",", "\u00e4hn\u00b7li\u00b7chen", "Ruhms", "d\u00fcnkt", "es", "mich", "w\u00fcr\u00b7dig", "zu", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Gro\u00df mit den Gro\u00dfen zu sein ist g\u00f6ttliches Los. An Achilleus", "tokens": ["Gro\u00df", "mit", "den", "Gro\u00b7\u00dfen", "zu", "sein", "ist", "g\u00f6tt\u00b7li\u00b7ches", "Los", ".", "An", "A\u00b7chil\u00b7leus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PTKZU", "VAINF", "VAFIN", "ADJA", "NN", "$.", "APPR", "NE"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Lehnt sich Patroklos im Kampf, wenn er um Ilium braust;", "tokens": ["Lehnt", "sich", "Pat\u00b7ro\u00b7klos", "im", "Kampf", ",", "wenn", "er", "um", "I\u00b7lium", "braust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter":