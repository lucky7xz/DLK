{"textgrid.poem.40937": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Bruchst\u00fcck einer Epistel", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jumpfere, sitzet mer jez ufs St\u00fcehli do nider und loset,", "tokens": ["Jump\u00b7fe\u00b7re", ",", "sit\u00b7zet", "mer", "jez", "ufs", "St\u00fceh\u00b7li", "do", "ni\u00b7der", "und", "lo\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ADV", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "bis i sag: \u00bbJez gang!\u00ab und hent der im vorige Summer", "tokens": ["bis", "i", "sag", ":", "\u00bb", "Jez", "gang", "!", "\u00ab", "und", "hent", "der", "im", "vo\u00b7ri\u00b7ge", "Sum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "$.", "$(", "FM.xy", "FM.xy", "$.", "$(", "KON", "VVFIN", "ART", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "oberl\u00e4ndisch an mi gschriebe, willi's vergelte.", "tokens": ["o\u00b7ber\u00b7l\u00e4n\u00b7disch", "an", "mi", "gschrie\u00b7be", ",", "wil\u00b7li's", "ver\u00b7gel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "$,", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Bini nit au deheim, wo alles sch\u00f6ner und s\u00fce\u00dfer", "tokens": ["Bi\u00b7ni", "nit", "au", "de\u00b7heim", ",", "wo", "al\u00b7les", "sch\u00f6\u00b7ner", "und", "s\u00fce\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PWAV", "PIS", "ADJD", "KON", "ADJA"], "meter": "---+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "t\u00f6nt in Matten und Feld und in de vert\u00e4flete Stube?", "tokens": ["t\u00f6nt", "in", "Mat\u00b7ten", "und", "Feld", "und", "in", "de", "ver\u00b7t\u00e4f\u00b7le\u00b7te", "Stu\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "KON", "APPR", "NE", "ADJA", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "'s het mi kei Mutter gebore und keini christlige Pate", "tokens": ["'s", "het", "mi", "kei", "Mut\u00b7ter", "ge\u00b7bo\u00b7re", "und", "kei\u00b7ni", "christ\u00b7li\u00b7ge", "Pa\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "PIAT", "NN", "VVFIN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+--+--+--+-+-+-+", "measure": "amphibrach.tetra.plus"}, "line.7": {"text": "hen mi an Taufstei treit. In mine d\u00e4mmrige Tage", "tokens": ["hen", "mi", "an", "Tauf\u00b7stei", "treit", ".", "In", "mi\u00b7ne", "d\u00e4mm\u00b7ri\u00b7ge", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "CARD", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.8": {"text": "het mi kei Brei erquickt. In d'Kirche bini nit gange", "tokens": ["het", "mi", "kei", "Brei", "er\u00b7quickt", ".", "In", "d'\u00b7Kir\u00b7che", "bi\u00b7ni", "nit", "gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "PIAT", "NN", "VVPP", "$.", "FM.la", "FM.la", "FM.la", "FM.l