], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du, die nie m\u00fcde war", "tokens": ["Du", ",", "die", "nie", "m\u00fc\u00b7de", "war"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu schaffen und zu pflegen,", "tokens": ["Zu", "schaf\u00b7fen", "und", "zu", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Gattin, treu und mild,", "tokens": ["O", "Gat\u00b7tin", ",", "treu", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Begleite ", "tokens": ["Be\u00b7glei\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "So bleibt sein Haus ein Bild", "tokens": ["So", "bleibt", "sein", "Haus", "ein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von alter deutscher Art.", "tokens": ["Von", "al\u00b7ter", "deut\u00b7scher", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Mit Gott! mit Gott! fortan", "tokens": ["Mit", "Gott", "!", "mit", "Gott", "!", "for\u00b7tan"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gefragt nicht, noch verwundert;", "tokens": ["Ge\u00b7fragt", "nicht", ",", "noch", "ver\u00b7wun\u00b7dert", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Tropfen Zeit verrann,", "tokens": ["Ein", "Trop\u00b7fen", "Zeit", "ver\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Viertel vom Jahrhundert.", "tokens": ["Ein", "Vier\u00b7tel", "vom", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heil ihm! Heil dem, was kommt!", "tokens": ["Heil", "ihm", "!", "Heil", "dem", ",", "was", "kommt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "ART", "$,", "PWS", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie sich die Wege drehn,", "tokens": ["Wie", "sich", "die", "We\u00b7ge", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir wissen, da\u00df uns frommt,", "tokens": ["Wir", "wis\u00b7sen", ",", "da\u00df", "uns", "frommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was ist und wird geschehn!", "tokens": ["Was", "ist", "und", "wird", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}