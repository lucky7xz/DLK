{"textgrid.poem.62379": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Zur Stilling'schen silbernen Hochzeitsfeier", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Licht und Recht strahlt weit und breit,", "tokens": ["Licht", "und", "Recht", "strahlt", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater ", "tokens": ["Va\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie nach schwerer Pr\u00fcfungszeit", "tokens": ["Wie", "nach", "schwe\u00b7rer", "Pr\u00fc\u00b7fungs\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzt die unbew\u00f6lkte Sonne,", "tokens": ["Gl\u00e4nzt", "die", "un\u00b7be\u00b7w\u00f6lk\u00b7te", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die vers\u00f6hnte K\u00f6nigin,", "tokens": ["Die", "ver\u00b7s\u00f6hn\u00b7te", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf des Lieblings Scheitel hin.", "tokens": ["Auf", "des", "Lieb\u00b7lings", "Schei\u00b7tel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir singen unserm Herrn,", "tokens": ["Wir", "sin\u00b7gen", "un\u00b7serm", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir Gro\u00dfen und wir Kleinen,", "tokens": ["Wir", "Gro\u00b7\u00dfen", "und", "wir", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der uns den hellen Stern", "tokens": ["Der", "uns", "den", "hel\u00b7len", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Himmel lie\u00df erscheinen.", "tokens": ["Am", "Him\u00b7mel", "lie\u00df", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er gab das ew'ge Licht,", "tokens": ["Er", "gab", "das", "ew'\u00b7ge", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er sprach das ew'ge Wort,", "tokens": ["Er", "sprach", "das", "ew'\u00b7ge", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist nah' und fehlet nicht", "tokens": ["Ist", "nah'", "und", "feh\u00b7let", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In keinem Land und Ort.", "tokens": ["In", "kei\u00b7nem", "Land", "und", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Jedem Kr\u00e4fte gab,", "tokens": ["Der", "Je\u00b7dem", "Kr\u00e4f\u00b7te", "gab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Womit er wirk' und schaffe,", "tokens": ["Wo\u00b7mit", "er", "wirk'", "und", "schaf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist des Greisen Stab,", "tokens": ["Er", "ist", "des", "Grei\u00b7sen", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des Schwachen Wehr und Waffe.", "tokens": ["Des", "Schwa\u00b7chen", "Wehr", "und", "Waf\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er schenket Brod und Wein,", "tokens": ["Er", "schen\u00b7ket", "Brod", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ern\u00e4hret Seel' und Leib,", "tokens": ["Er\u00b7n\u00e4h\u00b7ret", "Seel'", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und segnet selber ein", "tokens": ["Und", "seg\u00b7net", "sel\u00b7ber", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den Bund von Mann und Weib.", "tokens": ["Den", "Bund", "von", "Mann", "und", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr, salbe dieses Haus,", "tokens": ["Herr", ",", "sal\u00b7be", "die\u00b7ses", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und heil'ge seine Schwelle,", "tokens": ["Und", "heil'\u00b7ge", "sei\u00b7ne", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geu\u00df deinen Segen aus,", "tokens": ["Geu\u00df", "dei\u00b7nen", "Se\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du rechte Lebensquelle!", "tokens": ["Du", "rech\u00b7te", "Le\u00b7bens\u00b7quel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df einen frischen Born", "tokens": ["La\u00df", "ei\u00b7nen", "fri\u00b7schen", "Born"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Wassers hier entstehn,", "tokens": ["Des", "Was\u00b7sers", "hier", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Kummer, Neid und Zorn", "tokens": ["Und", "Kum\u00b7mer", ",", "Neid", "und", "Zorn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von diesen Pforten gehn.", "tokens": ["Von", "die\u00b7sen", "Pfor\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie klingst du doch so sch\u00f6n,", "tokens": ["Wie", "klingst", "du", "doch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Lied aus alten Tagen,", "tokens": ["O", "Lied", "aus", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf Siegens alten H\u00f6h'n,", "tokens": ["Auf", "Sie\u00b7gens", "al\u00b7ten", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da wohnen treue Sagen,", "tokens": ["Da", "woh\u00b7nen", "treu\u00b7e", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der V\u00e4ter Wort und Lust,", "tokens": ["Der", "V\u00e4\u00b7ter", "Wort", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der V\u00e4ter Sitt' und Art", "tokens": ["Der", "V\u00e4\u00b7ter", "Sitt'", "und", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird noch in frommer Brust", "tokens": ["Wird", "noch", "in", "from\u00b7mer", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein Ehrenschatz bewahrt.", "tokens": ["Ein", "Eh\u00b7ren\u00b7schatz", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Sollst lange dich noch kr\u00e4useln,", "tokens": ["Sollst", "lan\u00b7ge", "dich", "noch", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und L\u00fcfte warm und klar", "tokens": ["Und", "L\u00fcf\u00b7te", "warm", "und", "klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Um seine Schl\u00e4fe s\u00e4useln;", "tokens": ["Um", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "s\u00e4u\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er liebet Feld und Baum,", "tokens": ["Er", "lie\u00b7bet", "Feld", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und Weib und Kind und Herd,", "tokens": ["Und", "Weib", "und", "Kind", "und", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und diesen Erdentraum", "tokens": ["Und", "die\u00b7sen", "Er\u00b7den\u00b7traum"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den Gott einst sch\u00f6n verkl\u00e4rt.", "tokens": ["Den", "Gott", "einst", "sch\u00f6n", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So wandle fort, o Paar,", "tokens": ["So", "wand\u00b7le", "fort", ",", "o", "Paar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Liebe, Fried' und Segen!", "tokens": ["In", "Lie\u00b7be", ",", "Fried'", "und", "Se\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du, die nie m\u00fcde war", "tokens": ["Du", ",", "die", "nie", "m\u00fc\u00b7de", "war"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu schaffen und zu pflegen,", "tokens": ["Zu", "schaf\u00b7fen", "und", "zu", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Gattin, treu und mild,", "tokens": ["O", "Gat\u00b7tin", ",", "treu", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Begleite ", "tokens": ["Be\u00b7glei\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "So bleibt sein Haus ein Bild", "tokens": ["So", "bleibt", "sein", "Haus", "ein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Von alter deutscher Art.", "tokens": ["Von", "al\u00b7ter", "deut\u00b7scher", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit Gott! mit Gott! fortan", "tokens": ["Mit", "Gott", "!", "mit", "Gott", "!", "for\u00b7tan"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gefragt nicht, noch verwundert;", "tokens": ["Ge\u00b7fragt", "nicht", ",", "noch", "ver\u00b7wun\u00b7dert", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Tropfen Zeit verrann,", "tokens": ["Ein", "Trop\u00b7fen", "Zeit", "ver\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Viertel vom Jahrhundert.", "tokens": ["Ein", "Vier\u00b7tel", "vom", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heil ihm! Heil dem, was kommt!", "tokens": ["Heil", "ihm", "!", "Heil", "dem", ",", "was", "kommt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "ART", "$,", "PWS", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wie sich die Wege drehn,", "tokens": ["Wie", "sich", "die", "We\u00b7ge", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir wissen, da\u00df uns frommt,", "tokens": ["Wir", "wis\u00b7sen", ",", "da\u00df", "uns", "frommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was ist und wird geschehn!", "tokens": ["Was", "ist", "und", "wird", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Licht und Recht strahlt weit und breit,", "tokens": ["Licht", "und", "Recht", "strahlt", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vater ", "tokens": ["Va\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie nach schwerer Pr\u00fcfungszeit", "tokens": ["Wie", "nach", "schwe\u00b7rer", "Pr\u00fc\u00b7fungs\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzt die unbew\u00f6lkte Sonne,", "tokens": ["Gl\u00e4nzt", "die