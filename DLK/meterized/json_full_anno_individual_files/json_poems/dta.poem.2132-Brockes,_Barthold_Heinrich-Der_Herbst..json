{"dta.poem.2132": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen k\u00fchler, traubenreicher, mit s\u00fc\u00dfem Obst", "tokens": ["Will\u00b7kom\u00b7men", "k\u00fch\u00b7ler", ",", "trau\u00b7ben\u00b7rei\u00b7cher", ",", "mit", "s\u00fc\u00b7\u00dfem", "Obst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "ADJA", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "beladner Herbst,", "tokens": ["be\u00b7lad\u00b7ner", "Herbst", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der du die W\u00e4lder \u00fcberg\u00fcldest, mit rothem Glanz die", "tokens": ["Der", "du", "die", "W\u00e4l\u00b7der", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7dest", ",", "mit", "ro\u00b7them", "Glanz", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Fr\u00fcchte f\u00e4rbst,", "tokens": ["Fr\u00fcch\u00b7te", "f\u00e4rbst", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die Welt in bunten Flor verh\u00fcllst, das Feld mit feuch-", "tokens": ["Die", "Welt", "in", "bun\u00b7ten", "Flor", "ver\u00b7h\u00fcllst", ",", "das", "Feld", "mit", "feuch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "tem Silber tr\u00e4nkest,", "tokens": ["tem", "Sil\u00b7ber", "tr\u00e4n\u00b7kest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Uns, das mit Lust verzehrte Wild, nebst tausend Schaaren", "tokens": ["Uns", ",", "das", "mit", "Lust", "ver\u00b7zehr\u00b7te", "Wild", ",", "nebst", "tau\u00b7send", "Schaa\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "NE", "$,", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "V\u00f6gel schenkest", "tokens": ["V\u00f6\u00b7gel", "schen\u00b7kest"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Von Wachteln, Krammetsv\u00f6geln, Lerchen, uns Tische,", "tokens": ["Von", "Wach\u00b7teln", ",", "Kram\u00b7mets\u00b7v\u00f6\u00b7geln", ",", "Ler\u00b7chen", ",", "uns", "Ti\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "K\u00fcch' und Keller f\u00fcllst", "tokens": ["K\u00fc\u00b7ch'", "und", "Kel\u00b7ler", "f\u00fcllst"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mit s\u00fc\u00dfem Wein und fettem Mastvieh, mit Lust, so", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Wein", "und", "fet\u00b7tem", "Mast\u00b7vieh", ",", "mit", "Lust", ",", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,", "APPR", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Durst als Hunger stillst,", "tokens": ["Durst", "als", "Hun\u00b7ger", "stillst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Und uns, f\u00fcr k\u00fcnftgen Frost, versorgst! Wer kann", "tokens": ["Und", "uns", ",", "f\u00fcr", "k\u00fcnft\u00b7gen", "Frost", ",", "ver\u00b7sorgst", "!", "Wer", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$.", "PWS", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "die Gaben alle z\u00e4hlen,", "tokens": ["die", "Ga\u00b7ben", "al\u00b7le", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die du uns recht verschwendrisch reichst, da\u00df uns kein", "tokens": ["Die", "du", "uns", "recht", "ver\u00b7schwend\u00b7risch", "reichst", ",", "da\u00df", "uns", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "ADJD", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "PIAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Mangel m\u00f6ge qu\u00e4len", "tokens": ["Man\u00b7gel", "m\u00f6\u00b7ge", "qu\u00e4\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Jm strengen unfruchtbaren Winter! Wir kommen,", "tokens": ["Jm", "stren\u00b7gen", "un\u00b7frucht\u00b7ba\u00b7ren", "Win\u00b7ter", "!", "Wir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "durch dich, auf die Spur", "tokens": ["durch", "dich", ",", "auf", "die", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Und finden: Deine sch\u00f6ne Zeit sey recht die Absicht der", "tokens": ["Und", "fin\u00b7den", ":", "Dei\u00b7ne", "sch\u00f6\u00b7ne", "Zeit", "sey", "recht", "die", "Ab\u00b7sicht", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.20": {"text": "Natur.", "tokens": ["Na\u00b7tur", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Es wirkt das ganze Jahr f\u00fcr dich, in dir f\u00fcr uns.", "tokens": ["Es", "wirkt", "das", "gan\u00b7ze", "Jahr", "f\u00fcr", "dich", ",", "in", "dir", "f\u00fcr", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,", "APPR", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Uns zu ern\u00e4hren,", "tokens": ["Uns", "zu", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.23": {"text": "Jm Frost, wenn unsre Mutter schl\u00e4ft, den Unterhalt", "tokens": ["Jm", "Frost", ",", "wenn", "uns\u00b7re", "Mut\u00b7ter", "schl\u00e4ft", ",", "den", "Un\u00b7ter\u00b7halt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "uns zu gewehren.", "tokens": ["uns", "zu", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Gebenedeyet sey die Liebe, zusammt der Weisheit und", "tokens": ["Ge\u00b7be\u00b7ne\u00b7de\u00b7yet", "sey", "die", "Lie\u00b7be", ",", "zu\u00b7sammt", "der", "Weis\u00b7heit", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "KON"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.26": {"text": "der Macht,", "tokens": ["der", "Macht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Die, in der wunderreichen Ordnung, uns zeigt, da\u00df", "tokens": ["Die", ",", "in", "der", "wun\u00b7der\u00b7rei\u00b7chen", "Ord\u00b7nung", ",", "uns", "zeigt", ",", "da\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "sie an uns gedacht!", "tokens": ["sie", "an", "uns", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}