{"textgrid.poem.57873": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Warraftig", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkt ihr noch jenes sch\u00f6nen Morgens,", "tokens": ["Denkt", "ihr", "noch", "je\u00b7nes", "sch\u00f6\u00b7nen", "Mor\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als es am Goetheplatz gekracht?", "tokens": ["Als", "es", "am", "Goe\u00b7the\u00b7platz", "ge\u00b7kracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tr\u00fcmmerhaufen war das Ende", "tokens": ["Ein", "Tr\u00fcm\u00b7mer\u00b7hau\u00b7fen", "war", "das", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des stolzen Turmgeb\u00e4udes Pracht.", "tokens": ["Des", "stol\u00b7zen", "Turm\u00b7ge\u00b7b\u00e4u\u00b7des", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man lie\u00df sich aber nicht verbl\u00fcffen,", "tokens": ["Man", "lie\u00df", "sich", "a\u00b7ber", "nicht", "ver\u00b7bl\u00fcf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat lustig wieder aufgebaut,", "tokens": ["Hat", "lus\u00b7tig", "wie\u00b7der",