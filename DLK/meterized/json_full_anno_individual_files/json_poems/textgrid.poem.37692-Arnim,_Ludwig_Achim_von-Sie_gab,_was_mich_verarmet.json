{"textgrid.poem.37692": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sie gab, was mich verarmet", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie gab, was mich verarmet,", "tokens": ["Sie", "gab", ",", "was", "mich", "ver\u00b7ar\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir scheidend ihren Mund,", "tokens": ["Mir", "schei\u00b7dend", "ih\u00b7ren", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat sich mein erbarmet,", "tokens": ["Sie", "hat", "sich", "mein", "er\u00b7bar\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach Gott, wem thu' ich's kund!", "tokens": ["Ach", "Gott", ",", "wem", "thu'", "ich's", "kund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kann's nicht in mir lassen,", "tokens": ["Ich", "kann's", "nicht", "in", "mir", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es sprenget meine Brust,", "tokens": ["Es", "spren\u00b7get", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es kann's die Welt nicht fassen,", "tokens": ["Es", "kann's", "die", "Welt", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was mir allein bewu\u00dft.", "tokens": ["Was", "mir", "al\u00b7lein", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie mir der Abend r\u00f6thet,", "tokens": ["Wie", "mir", "der", "A\u00b7bend", "r\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch niemand wissen mu\u00df;", "tokens": ["Noch", "nie\u00b7mand", "wis\u00b7sen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach h\u00e4tt' sie mich get\u00f6dtet", "tokens": ["Ach", "h\u00e4tt'", "sie", "mich", "ge\u00b7t\u00f6d\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im ersten, ersten Ku\u00df!", "tokens": ["Im", "ers\u00b7ten", ",", "ers\u00b7ten", "Ku\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Schmerzen k\u00f6nnt' ich ruhen,", "tokens": ["Von", "Schmer\u00b7zen", "k\u00f6nnt'", "ich", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["AP