"sel'\u00b7ger", "J\u00fcng\u00b7ling", ",", "keusch", "und", "rein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df meine ganze Seele dr\u00fcben,", "tokens": ["La\u00df", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz nur Ihm geopfert sein.", "tokens": ["Mein", "Herz", "nur", "Ihm", "ge\u00b7o\u00b7pfert", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O, lehre mich die Lieb' erkennen,", "tokens": ["O", ",", "leh\u00b7re", "mich", "die", "Lieb'", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jesu Herz so zu mir kehrt,", "tokens": ["Die", "Je\u00b7su", "Herz", "so", "zu", "mir", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mich in Gegenlieb' entbrennen,", "tokens": ["Und", "mich", "in", "Ge\u00b7gen\u00b7lieb'", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ihre Gluth mein Herz verzehrt.", "tokens": ["Bis", "ih\u00b7re", "Gluth", "mein", "Herz", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit Deinen Lilien woll'st Du decken,", "tokens": ["Mit", "Dei\u00b7nen", "Li\u00b7li\u00b7en", "woll'st", "Du", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was meine Bu\u00dfe nicht gel\u00f6scht,", "tokens": ["Was", "mei\u00b7ne", "Bu\u00b7\u00dfe", "nicht", "ge\u00b7l\u00f6scht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00e4ngel all' und all' die Flecken,", "tokens": ["Die", "M\u00e4n\u00b7gel", "all'", "und", "all'", "die", "Fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KON", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die fruchtlos meine Thr\u00e4ne w\u00e4scht.", "tokens": ["Die", "frucht\u00b7los", "mei\u00b7ne", "Thr\u00e4\u00b7ne", "w\u00e4scht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Erfleh' mir, da\u00df dereinst mein Sterben", "tokens": ["Er\u00b7fleh'", "mir", ",", "da\u00df", "de\u00b7reinst", "mein", "Ster\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": 