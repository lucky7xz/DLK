{"textgrid.poem.35888": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Du, in Deinem Lilienkranze,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Du, in Deinem Lilienkranze,", "tokens": ["O", "Du", ",", "in", "Dei\u00b7nem", "Li\u00b7li\u00b7en\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dich preise meine Seele laut,", "tokens": ["Dich", "prei\u00b7se", "mei\u00b7ne", "See\u00b7le", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich, der im reinen Engelglanze", "tokens": ["Dich", ",", "der", "im", "rei\u00b7nen", "En\u00b7gel\u00b7glan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Vaters Antlitz ewig schaut! \u2013", "tokens": ["Des", "Va\u00b7ters", "Ant\u00b7litz", "e\u00b7wig", "schaut", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Du Dein ganzes Erdenleben", "tokens": ["Der", "Du", "Dein", "gan\u00b7zes", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Heiland dienend, fromm und rein,", "tokens": ["Dem", "Hei\u00b7land", "die\u00b7nend", ",", "fromm", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Gottes Vaterhand gegeben,", "tokens": ["In", "Got\u00b7tes", "Va\u00b7ter\u00b7hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O lehr' auch mich Dein Abbild sein!", "tokens": ["O", "lehr'", "auch", "mich", "Dein", "Ab\u00b7bild", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und durch die Macht, die Dir gegeben,", "tokens": ["Und", "durch", "die", "Macht", ",", "die", "Dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Seine Gnade, Deine Treu',", "tokens": ["Durch", "Sei\u00b7ne", "Gna\u00b7de", ",", "Dei\u00b7ne", "Treu'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erfleh' mir, da\u00df mein ganzes Leben", "tokens": ["Er\u00b7fleh'", "mir", ",", "da\u00df", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Jesu Dienst geheiligt sei.", "tokens": ["In", "Je\u00b7su", "Dienst", "ge\u0