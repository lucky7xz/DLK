{"dta.poem.3276": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Amor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Amor, Vater s\u00fcsser Lieder,", "tokens": ["A\u00b7mor", ",", "Va\u00b7ter", "s\u00fcs\u00b7ser", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du mein Ph\u00f6bus, kehre wieder!", "tokens": ["Du", "mein", "Ph\u00f6\u00b7bus", ",", "keh\u00b7re", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehre wieder in mein Herze!", "tokens": ["Keh\u00b7re", "wie\u00b7der", "in", "mein", "Her\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm! doch mit dem schlauen Scherze:", "tokens": ["Komm", "!", "doch", "mit", "dem", "schlau\u00b7en", "Scher\u00b7ze", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm und la\u00df zugleich Ly\u00e4en", "tokens": ["Komm", "und", "la\u00df", "zu\u00b7gleich", "Ly\u00b7\u00e4\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVIMP", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir zur Seite lachend gehen!", "tokens": ["Dir", "zur", "Sei\u00b7te", "la\u00b7chend", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Komm mit einem holden Kinde,", "tokens": ["Komm", "mit", "ei\u00b7nem", "hol\u00b7den", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das mein tr\u00e4ges Herz entz\u00fcnde,", "tokens": ["Das", "mein", "tr\u00e4\u00b7ges", "Herz", "ent\u00b7z\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und durch feuervolle K\u00fcsse", "tokens": ["Und", "durch", "feu\u00b7er\u00b7vol\u00b7le", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zum Horaz mich k\u00fcssen m\u00fcsse!", "tokens": ["Zum", "Ho\u00b7raz", "mich", "k\u00fcs\u00b7sen", "m\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Willst du, Gott der Z\u00e4rtlichkeiten:", "tokens": ["Willst", "du", ",", "Gott", "der", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "La\u00df auch Schmerzen dich begleiten!", "tokens": ["La\u00df", "auch", "Schmer\u00b7zen", "dich", "be\u00b7glei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ich will lieber deine Schmerzen,", "tokens": ["Ich", "will", "lie\u00b7ber", "dei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Als nicht k\u00fcssen und nicht scherzen.", "tokens": ["Als", "nicht", "k\u00fcs\u00b7sen", "und", "nicht", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}