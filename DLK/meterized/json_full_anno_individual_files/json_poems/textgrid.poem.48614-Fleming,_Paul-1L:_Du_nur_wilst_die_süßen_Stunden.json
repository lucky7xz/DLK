ord", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KON", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "gleich erfreut sind Alt' und Junge,", "tokens": ["gleich", "er\u00b7freut", "sind", "Alt'", "und", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "denn die sch\u00f6nste Zeit der Zeiten", "tokens": ["denn", "die", "sch\u00f6ns\u00b7te", "Zeit", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "lehret sie um Freude streiten.", "tokens": ["leh\u00b7ret", "sie", "um", "Freu\u00b7de", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Heuer nim darzu ingleichen", "tokens": ["Heu\u00b7er", "nim", "dar\u00b7zu", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was du vor dem Jahr erspart,", "tokens": ["was", "du", "vor", "dem", "Jahr", "er\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da ihr lieben Leute wart,", "tokens": ["da", "ihr", "lie\u00b7ben", "Leu\u00b7te", "wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wo ich nicht kunt' hin gereichen!", "tokens": ["wo", "ich", "nicht", "kunt'", "hin", "ge\u00b7rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir wolln uns in solcher Sachen", "tokens": ["Wir", "wolln", "uns", "in", "sol\u00b7cher", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "zwiefach mit dir fr\u00f6lich machen.", "tokens": ["zwie\u00b7fach", "mit", "dir", "fr\u00f6\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "La\u00df die sch\u00f6nsten Jungfern bitten,", "tokens": ["La\u00df", "die", "sch\u00f6ns\u00b7ten", "Jung\u00b7fern", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "setz' uns vor Konfect und Wein!", "tokens": ["setz'", "uns", "vor", "Kon\u00b7fect", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute wolln wir lustig sein", "tokens": ["Heu\u00b7te", "wolln", "wir", "lus\u00b7tig", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "nach der sch\u00f6nen Leute Sitten,", "tokens": ["nach", "der", "sch\u00f6\u00b7nen", "Leu\u00b7te", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die nur auf den hohen Schulen", "tokens": ["die", "nur", "auf", "den", "ho\u00b7hen", "Schu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trinken \u00fcben und das Bulen!", "tokens": ["Trin\u00b7ken", "\u00fc\u00b7ben", "und", "das", "Bu\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Lasse gleichsfals zu uns kommen", "tokens": ["Las\u00b7se", "gleichs\u00b7fals", "zu", "uns", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das ber\u00fchmte Saitenspiel,", "tokens": ["das", "be\u00b7r\u00fchm\u00b7te", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "es mag kosten was es will!", "tokens": ["es", "mag", "kos\u00b7ten", "was", "es", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles dient zu deinem Frommen:", "tokens": ["Al\u00b7les", "dient", "zu", "dei\u00b7nem", "From\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df man von dem sch\u00f6nen Tage", "tokens": ["da\u00df", "man", "von", "dem", "sch\u00f6\u00b7nen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "durch das ganze Jahr durch sage!", "tokens": ["durch", "das", "gan\u00b7ze", "Jahr", "durch", "sa\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nicht, da\u00df wir dir vorzuschreiben", "tokens": ["Nicht", ",", "da\u00df", "wir", "dir", "vor\u00b7zu\u00b7schrei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "uns hiermitte nehmen f\u00fcr.", "tokens": ["uns", "hier\u00b7mit\u00b7te", "neh\u00b7men", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, Freund, es steht ganz bei dir!", "tokens": ["Nein", ",", "Freund", ",", "es", "steht", "ganz", "bei", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du wirst dich wol selbst antreiben.", "tokens": ["Du", "wirst", "dich", "wol", "selbst", "an\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir nur sprechen, da\u00df wir wollen", "tokens": ["Wir", "nur", "spre\u00b7chen", ",", "da\u00df", "wir", "wol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "lustig sein, als wie wir sollen.", "tokens": ["lus\u00b7tig", "sein", ",", "als", "wie", "wir", "sol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,", "KOUS", "KOKOM", "PPER", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Begangen in Reval den 8. April 1635.", "tokens": ["Be\u00b7gan\u00b7gen", "in", "Re\u00b7val", "den", "8.", "Ap\u00b7ril", "16\u00b735", "."], "token_info": ["word", "word", "word", "word", "ordinal", "word", "number", "punct"], "pos": ["NN", "APPR", "NE", "ART", "ADJA", "NN", "CARD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}