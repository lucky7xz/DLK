{"dta.poem.2090": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ziegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann nun bey allem auch das Fleisch uns auf verschiedne", "tokens": ["Wann", "nun", "bey", "al\u00b7lem", "auch", "das", "Fleisch", "uns", "auf", "ver\u00b7schied\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PIS", "ADV", "ART", "NN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weise n\u00e4hret,", "tokens": ["Wei\u00b7se", "n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und es dennoch mit wenig Kosten und schlechtem Futter zu", "tokens": ["Und", "es", "den\u00b7noch", "mit", "we\u00b7nig", "Kos\u00b7ten", "und", "schlech\u00b7tem", "Fut\u00b7ter", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PIAT", "NN", "KON", "ADJA", "NN", "PTKZU"], "meter": "-+---+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "erhalten:", "tokens": ["er\u00b7hal\u00b7ten", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So wird mit Recht auch Gott gedankt, sowohl von Jun-", "tokens": ["So", "wird", "mit", "Recht", "auch", "Gott", "ge\u00b7dankt", ",", "so\u00b7wohl", "von", "Jun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "NN", "VVPP", "$,", "KON", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "gen als von Alten,", "tokens": ["gen", "als", "von", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df er uns in den magern Ziegen ein solches n\u00fctzlichs", "tokens": ["Da\u00df", "er", "uns", "in", "den", "ma\u00b7gern", "Zie\u00b7gen", "ein", "sol\u00b7ches", "n\u00fctz\u00b7lichs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "ART", "PIAT", "NN"], "meter": "-----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Thier bescheret.", "tokens": ["Thier", "be\u00b7sche\u00b7ret", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}