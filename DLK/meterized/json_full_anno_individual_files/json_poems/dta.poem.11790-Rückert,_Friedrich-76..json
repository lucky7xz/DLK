{"dta.poem.11790": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der kluge J\u00e4ger sprach zu seinem treuen Hunde:", "tokens": ["Der", "klu\u00b7ge", "J\u00e4\u00b7ger", "sprach", "zu", "sei\u00b7nem", "treu\u00b7en", "Hun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du fange mir, was du erlaufen kannst am Grunde.", "tokens": ["Du", "fan\u00b7ge", "mir", ",", "was", "du", "er\u00b7lau\u00b7fen", "kannst", "am", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur eines fang mir nicht, wenn du's auch k\u00f6nntest fangen,", "tokens": ["Nur", "ei\u00b7nes", "fang", "mir", "nicht", ",", "wenn", "du's", "auch", "k\u00f6nn\u00b7test", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Falken, der an mir will auch mit Treue hangen.", "tokens": ["Den", "Fal\u00b7ken", ",", "der", "an", "mir", "will", "auch", "mit", "Treu\u00b7e", "han\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Denn aus der Luft, in die du dich nicht auf kannst schwingen,", "tokens": ["Denn", "aus", "der", "Luft", ",", "in", "die", "du", "dich", "nicht", "auf", "kannst", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "PTKNEG", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dient der Falke mir den Fang herabzubringen.", "tokens": ["Da", "dient", "der", "Fal\u00b7ke", "mir", "den", "Fang", "her\u00b7ab\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der kluge J\u00e4ger sprach zum treuen Falken dann:", "tokens": ["Der", "klu\u00b7ge", "J\u00e4\u00b7ger", "sprach", "zum", "treu\u00b7en", "Fal\u00b7ken", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hol aus den L\u00fcften mir, was du vermagst, mein Mann.", "tokens": ["Hol", "aus", "den", "L\u00fcf\u00b7ten", "mir", ",", "was", "du", "ver\u00b7magst", ",", "mein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Nur eines sollst du dort nicht holen, k\u00fchner Steiger,", "tokens": ["Nur", "ei\u00b7nes", "sollst", "du", "dort", "nicht", "ho\u00b7len", ",", "k\u00fch\u00b7ner", "Stei\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Reiger, deinen Feind; mein Freund ist auch der Reiger.", "tokens": ["Den", "Rei\u00b7ger", ",", "dei\u00b7nen", "Feind", ";", "mein", "Freund", "ist", "auch", "der", "Rei\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn aus der Flut, in die du nicht hinab kannst dringen,", "tokens": ["Denn", "aus", "der", "Flut", ",", "in", "die", "du", "nicht", "hin\u00b7ab", "kannst", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dient der Reiger mir den Fang heraufzubringen.", "tokens": ["Da", "dient", "der", "Rei\u00b7ger", "mir", "den", "Fang", "her\u00b7auf\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der kluge J\u00e4ger sprach sodann zum treuen Reiher:", "tokens": ["Der", "klu\u00b7ge", "J\u00e4\u00b7ger", "sprach", "so\u00b7dann", "zum", "treu\u00b7en", "Rei\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hole was du kannst mir aus dem vollen Weiher.", "tokens": ["Du", "ho\u00b7le", "was", "du", "kannst", "mir", "aus", "dem", "vol\u00b7len", "Wei\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nur eines h\u00fcte dich zu holen, einen Fisch,", "tokens": ["Nur", "ei\u00b7nes", "h\u00fc\u00b7te", "dich", "zu", "ho\u00b7len", ",", "ei\u00b7nen", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den goldnen, der nicht ist bestimmt f\u00fcr unsern Tisch.", "tokens": ["Den", "gold\u00b7nen", ",", "der", "nicht", "ist", "be\u00b7stimmt", "f\u00fcr", "un\u00b7sern", "Tisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PTKNEG", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Er ist bestimmt, zum Grund des Meers hinab zu dringen,", "tokens": ["Er", "ist", "be\u00b7stimmt", ",", "zum", "Grund", "des", "Meers", "hin\u00b7ab", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPRART", "NN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine Perle draus mir jeden Tag zu bringen.", "tokens": ["Und", "ei\u00b7ne", "Per\u00b7le", "draus", "mir", "je\u00b7den", "Tag", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Perlen reih' ich all' an eine feine Schnur,", "tokens": ["Die", "Per\u00b7len", "reih'", "ich", "all'", "an", "ei\u00b7ne", "fei\u00b7ne", "Schnur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis voll ein Halsband wird, und wenig fehlet nur.", "tokens": ["Bis", "voll", "ein", "Hals\u00b7band", "wird", ",", "und", "we\u00b7nig", "feh\u00b7let", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "VAFIN", "$,", "KON", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Das Halsband dann bekommt, wer mein getreuster Schalk", "tokens": ["Das", "Hals\u00b7band", "dann", "be\u00b7kommt", ",", "wer", "mein", "ge\u00b7treus\u00b7ter", "Schalk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird von euch dreien seyn, Hund, Reiher oder Falk.", "tokens": ["Wird", "von", "euch", "drei\u00b7en", "seyn", ",", "Hund", ",", "Rei\u00b7her", "o\u00b7der", "Falk", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "CARD", "VAINF", "$,", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}}}}