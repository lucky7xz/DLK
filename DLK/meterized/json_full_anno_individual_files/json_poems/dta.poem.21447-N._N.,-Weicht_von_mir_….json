{"dta.poem.21447": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Weicht von mir \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Weicht von mir, ihr Bilder-lockender L\u00fcste,", "tokens": ["Weicht", "von", "mir", ",", "ihr", "Bil\u00b7der\u00b7lo\u00b7cken\u00b7der", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ihr schwellender Leiber wei\u00dfwogende Br\u00fcste,", "tokens": ["Ihr", "schwel\u00b7len\u00b7der", "Lei\u00b7ber", "wei\u00df\u00b7wo\u00b7gen\u00b7de", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr dunkler Augen feuchtschimmernde Gluthen,", "tokens": ["Ihr", "dunk\u00b7ler", "Au\u00b7gen", "feucht\u00b7schim\u00b7mern\u00b7de", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Lippen so s\u00fc\u00df im Ku\u00df zu verbluten!", "tokens": ["Ihr", "Lip\u00b7pen", "so", "s\u00fc\u00df", "im", "Ku\u00df", "zu", "ver\u00b7blu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nicht will ich umschlungen von weichen Armen,", "tokens": ["Nicht", "will", "ich", "um\u00b7schlun\u00b7gen", "von", "wei\u00b7chen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Umkost von des Weibes Dufthauch, dem warmen,", "tokens": ["Um\u00b7kost", "von", "des", "Wei\u00b7bes", "Duf\u00b7thauch", ",", "dem", "war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die Sinne letzen im Taumel der Wonne \u2014", "tokens": ["Die", "Sin\u00b7ne", "let\u00b7zen", "im", "Tau\u00b7mel", "der", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu dir die Seele hinaufstrebt, Ursonne!", "tokens": ["Zu", "dir", "die", "See\u00b7le", "hin\u00b7auf\u00b7strebt", ",", "Ur\u00b7son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O la\u00df mich baden in seliger Klarheit!", "tokens": ["O", "la\u00df", "mich", "ba\u00b7den", "in", "se\u00b7li\u00b7ger", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "O sprich zu mir: ewige g\u00f6ttliche Wahrheit!", "tokens": ["O", "sprich", "zu", "mir", ":", "e\u00b7wi\u00b7ge", "g\u00f6tt\u00b7li\u00b7che", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "$.", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}