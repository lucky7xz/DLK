{"textgrid.poem.36340": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Am Rheine", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du freundlich ernste starke Woge,", "tokens": ["Du", "freund\u00b7lich", "erns\u00b7te", "star\u00b7ke", "Wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vaterland am lieben Rheine,", "tokens": ["Va\u00b7ter\u00b7land", "am", "lie\u00b7ben", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', die Tr\u00e4nen mu\u00df ich weinen,", "tokens": ["Sieh'", ",", "die", "Tr\u00e4\u00b7nen", "mu\u00df", "ich", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil das alles nun verloren!", "tokens": ["Weil", "das", "al\u00b7les", "nun", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Felsen, so die Ritter sich erkoren,", "tokens": ["Die", "Fel\u00b7sen", ",", "so", "die", "Rit\u00b7ter", "sich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schweigend dunkle Klagen trauern,", "tokens": ["Schwei\u00b7gend", "dunk\u00b7le", "Kla\u00b7gen", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch zerst\u00fcckt die alten Mauern", "tokens": ["Noch", "zer\u00b7st\u00fcckt", "die", "al\u00b7ten", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Traurig aus dem Wasser ragen,", "tokens": ["Trau\u00b7rig", "aus", "dem", "Was\u00b7ser", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo in alter Vorzeit Tagen", "tokens": ["Wo", "in", "al\u00b7ter", "Vor\u00b7zeit", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hohe Helden mutig lebten,", "tokens": ["Ho\u00b7he", "Hel\u00b7den", "mu\u00b7tig", "leb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Voll von Lust nach Ruhme strebten;", "tokens": ["Voll", "von", "Lust", "nach", "Ruh\u00b7me", "streb\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Franken, Deutsche und Burgunden,", "tokens": ["Fran\u00b7ken", ",", "Deut\u00b7sche", "und", "Bur\u00b7gun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die nun im dunkeln Strom verschwunden,", "tokens": ["Die", "nun", "im", "dun\u00b7keln", "Strom", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Tapfre Lanzen damals schwungen,", "tokens": ["Tapf\u00b7re", "Lan\u00b7zen", "da\u00b7mals", "schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Noch die deutschen Lieder sungen,", "tokens": ["Noch", "die", "deut\u00b7schen", "Lie\u00b7der", "sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die Verderbnis weit verjugen,", "tokens": ["Die", "Ver\u00b7derb\u00b7nis", "weit", "ver\u00b7ju\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Hand in Hand zum Bunde schlugen,", "tokens": ["Hand", "in", "Hand", "zum", "Bun\u00b7de", "schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "In edlem Rittertume,", "tokens": ["In", "ed\u00b7lem", "Rit\u00b7ter\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Aus aller Tugend Eine Burg zum Ruhme", "tokens": ["Aus", "al\u00b7ler", "Tu\u00b7gend", "Ei\u00b7ne", "Burg", "zum", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Durch alle Land' erbauten;", "tokens": ["Durch", "al\u00b7le", "Land'", "er\u00b7bau\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Da der Mann dem Mann noch traute,", "tokens": ["Da", "der", "Mann", "dem", "Mann", "noch", "trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Deutsche Lust im Walde bl\u00fchte,", "tokens": ["Deut\u00b7sche", "Lust", "im", "Wal\u00b7de", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Glaub' in Demut liebend gl\u00fchte,", "tokens": ["Glaub'", "in", "De\u00b7mut", "lie\u00b7bend", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ach da keiner noch alleine,", "tokens": ["Ach", "da", "kei\u00b7ner", "noch", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PIS", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In des Herzens tiefem Schreine,", "tokens": ["In", "des", "Her\u00b7zens", "tie\u00b7fem", "Schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Um sein Vaterland mu\u00dft' klagen,", "tokens": ["Um", "sein", "Va\u00b7ter\u00b7land", "mu\u00dft'", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Selbst sich bittre Wunden schlagen,", "tokens": ["Selbst", "sich", "bitt\u00b7re", "Wun\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wie ich hier am heil'gen Rheine", "tokens": ["Wie", "ich", "hier", "am", "heil'\u00b7gen", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "ADJA", "NE"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.29": {"text": "Hohen Unmuts Tr\u00e4nen weine.", "tokens": ["Ho\u00b7hen", "Un\u00b7muts", "Tr\u00e4\u00b7nen", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dunkle Trauer zieht mich nieder,", "tokens": ["Dunk\u00b7le", "Trau\u00b7er", "zieht", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will in Wehmut ganz vergehen;", "tokens": ["Will", "in", "Weh\u00b7mut", "ganz", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich sehe, was geschehen,", "tokens": ["Wenn", "ich", "se\u00b7he", ",", "was", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich denke, was gewesen,", "tokens": ["Wenn", "ich", "den\u00b7ke", ",", "was", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will die Brust in Schmerz sich l\u00f6sen! \u2013", "tokens": ["Will", "die", "Brust", "in", "Schmerz", "sich", "l\u00f6\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So fahrt denn wohl, ihr lieben Wogen,", "tokens": ["So", "fahrt", "denn", "wohl", ",", "ihr", "lie\u00b7ben", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich Schmerz und Mut gesogen;", "tokens": ["Wo", "ich", "Schmerz", "und", "Mut", "ge\u00b7so\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn den Mut auch f\u00fchl' ich schlagen,", "tokens": ["Denn", "den", "Mut", "auch", "f\u00fchl'", "ich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und inmitten solcher Klagen", "tokens": ["Und", "in\u00b7mit\u00b7ten", "sol\u00b7cher", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Springt die Quelle starker Jugend,", "tokens": ["Springt", "die", "Quel\u00b7le", "star\u00b7ker", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es waffnet feste Tugend", "tokens": ["Und", "es", "waff\u00b7net", "fes\u00b7te", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Brust mit Heldentreue.", "tokens": ["Uns\u00b7re", "Brust", "mit", "Hel\u00b7den\u00b7treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da entweicht denn alle Reue;", "tokens": ["Da", "ent\u00b7weicht", "denn", "al\u00b7le", "Reu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann ich gleich mit euch nicht leben,", "tokens": ["Kann", "ich", "gleich", "mit", "euch", "nicht", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So ergreift euch doch mein Streben.", "tokens": ["So", "er\u00b7greift", "euch", "doch", "mein", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo ich wandre, wo ich weile,", "tokens": ["Wo", "ich", "wand\u00b7re", ",", "wo", "ich", "wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gl\u00fchen M\u00e4nner, bl\u00fchen Lieder", "tokens": ["Gl\u00fc\u00b7hen", "M\u00e4n\u00b7ner", ",", "bl\u00fc\u00b7hen", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und ich f\u00fchle wohl Vertrauen,", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "wohl", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Auf des Herzens Fels zu bauen,", "tokens": ["Auf", "des", "Her\u00b7zens", "Fels", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Eine neue Burg der Liebe,", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Burg", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die in allem Sturme bliebe,", "tokens": ["Die", "in", "al\u00b7lem", "Stur\u00b7me", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "M\u00e4chtig durch die fernen Zeiten", "tokens": ["M\u00e4ch\u00b7tig", "durch", "die", "fer\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Einen allvereinten Strom zu leiten,", "tokens": ["Ei\u00b7nen", "all\u00b7ver\u00b7ein\u00b7ten", "Strom", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Einen Strom von Lust und Schmerzen,", "tokens": ["Ei\u00b7nen", "Strom", "von", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Alles aus dem eignen Herzen,", "tokens": ["Al\u00b7les", "aus", "dem", "eig\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wo die Lieder all' verschlungen", "tokens": ["Wo", "die", "Lie\u00b7der", "all'", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Alle Herzen wiederklungen,", "tokens": ["Al\u00b7le", "Her\u00b7zen", "wie\u00b7der\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Hohe Freunde dann verb\u00fcndet,", "tokens": ["Ho\u00b7he", "Freun\u00b7de", "dann", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "So der Freude Reich gegr\u00fcndet.", "tokens": ["So", "der", "Freu\u00b7de", "Reich", "ge\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Du freundlich ernste starke Woge,", "tokens": ["Du", "freund\u00b7lich", "erns\u00b7te", "star\u00b7ke", "Wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vaterland am lieben Rheine,", "tokens": ["Va\u00b7ter\u00b7land", "am", "lie\u00b7ben", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh', die Tr\u00e4nen mu\u00df ich weinen,", "tokens": ["Sieh'", ",", "die", "Tr\u00e4\u00b7nen", "mu\u00df", "ich", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil das alles nun verloren!", "tokens": ["Weil", "das", "al\u00b7les", "nun", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Felsen, so die Ritter sich erkoren,", "tokens": ["Die", "Fel\u00b7sen", ",", "so", "die", "Rit\u00b7ter", "sich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schweigend dunkle Klagen trauern,", "tokens": ["Schwei\u00b7gend", "dunk\u00b7le", "Kla\u00b7gen", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch zerst\u00fcckt die alten Mauern", "tokens": ["Noch", "zer\u00b7st\u00fcckt", "die", "al\u00b7ten", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Traurig aus dem Wasser ragen,", "tokens": ["Trau\u00b7rig", "aus", "dem", "Was\u00b7ser", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo in alter Vorzeit Tagen", "tokens": ["Wo", "in", "al\u00b7ter", "Vor\u00b7zeit", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hohe Helden mutig lebten,", "tokens": ["Ho\u00b7he", "Hel\u00b7den", "mu\u00b7tig", "leb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Voll von Lust nach Ruhme strebten;", "tokens": ["Voll", "von", "Lust", "nach", "Ruh\u00b7me", "streb\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Franken, Deutsche und Burgunden,", "tokens": ["Fran\u00b7ken", ",", "Deut\u00b7sche", "und", "Bur\u00b7gun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die nun im dunkeln Strom verschwunden,", "tokens": ["Die", "nun", "im", "dun\u00b7keln", "Strom", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Tapfre Lanzen damals schwungen,", "tokens": ["Tapf\u00b7re", "Lan\u00b7zen", "da\u00b7mals", "schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Noch die deutschen Lieder sungen,", "tokens": ["Noch", "die", "deut\u00b7schen", "Lie\u00b7der", "sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die Verderbnis weit verjugen,", "tokens": ["Die", "Ver\u00b7derb\u00b7nis", "weit", "ver\u00b7ju\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Hand in Hand zum Bunde schlugen,", "tokens": ["Hand", "in", "Hand", "zum", "Bun\u00b7de", "schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "In edlem Rittertume,", "tokens": ["In", "ed\u00b7lem", "Rit\u00b7ter\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Aus aller Tugend Eine Burg zum Ruhme", "tokens": ["Aus", "al\u00b7ler", "Tu\u00b7gend", "Ei\u00b7ne", "Burg", "zum", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Durch alle Land' erbauten;", "tokens": ["Durch", "al\u00b7le", "Land'", "er\u00b7bau\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Da der Mann dem Mann noch traute,", "tokens": ["Da", "der", "Mann", "dem", "Mann", "noch", "trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Deutsche Lust im Walde bl\u00fchte,", "tokens": ["Deut\u00b7sche", "Lust", "im", "Wal\u00b7de", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Glaub' in Demut liebend gl\u00fchte,", "tokens": ["Glaub'", "in", "De\u00b7mut", "lie\u00b7bend", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ach da keiner noch alleine,", "tokens": ["Ach", "da", "kei\u00b7ner", "noch", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PIS", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In des Herzens tiefem Schreine,", "tokens": ["In", "des", "Her\u00b7zens", "tie\u00b7fem", "Schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Um sein Vaterland mu\u00dft' klagen,", "tokens": ["Um", "sein", "Va\u00b7ter\u00b7land", "mu\u00dft'", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Selbst sich bittre Wunden schlagen,", "tokens": ["Selbst", "sich", "bitt\u00b7re", "Wun\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wie ich hier am heil'gen Rheine", "tokens": ["Wie", "ich", "hier", "am", "heil'\u00b7gen", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "ADJA", "NE"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.29": {"text": "Hohen Unmuts Tr\u00e4nen weine.", "tokens": ["Ho\u00b7hen", "Un\u00b7muts", "Tr\u00e4\u00b7nen", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dunkle Trauer zieht mich nieder,", "tokens": ["Dunk\u00b7le", "Trau\u00b7er", "zieht", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will in Wehmut ganz vergehen;", "tokens": ["Will", "in", "Weh\u00b7mut", "ganz", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich sehe, was geschehen,", "tokens": ["Wenn", "ich", "se\u00b7he", ",", "was", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich denke, was gewesen,", "tokens": ["Wenn", "ich", "den\u00b7ke", ",", "was", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will die Brust in Schmerz sich l\u00f6sen! \u2013", "tokens": ["Will", "die", "Brust", "in", "Schmerz", "sich", "l\u00f6\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So fahrt denn wohl, ihr lieben Wogen,", "tokens": ["So", "fahrt", "denn", "wohl", ",", "ihr", "lie\u00b7ben", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich Schmerz und Mut gesogen;", "tokens": ["Wo", "ich", "Schmerz", "und", "Mut", "ge\u00b7so\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn den Mut auch f\u00fchl' ich schlagen,", "tokens": ["Denn", "den", "Mut", "auch", "f\u00fchl'", "ich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und inmitten solcher Klagen", "tokens": ["Und", "in\u00b7mit\u00b7ten", "sol\u00b7cher", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Springt die Quelle starker Jugend,", "tokens": ["Springt", "die", "Quel\u00b7le", "star\u00b7ker", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es waffnet feste Tugend", "tokens": ["Und", "es", "waff\u00b7net", "fes\u00b7te", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsre Brust mit Heldentreue.", "tokens": ["Uns\u00b7re", "Brust", "mit", "Hel\u00b7den\u00b7treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da entweicht denn alle Reue;", "tokens": ["Da", "ent\u00b7weicht", "denn", "al\u00b7le", "Reu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann ich gleich mit euch nicht leben,", "tokens": ["Kann", "ich", "gleich", "mit", "euch", "nicht", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So ergreift euch doch mein Streben.", "tokens": ["So", "er\u00b7greift", "euch", "doch", "mein", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo ich wandre, wo ich weile,", "tokens": ["Wo", "ich", "wand\u00b7re", ",", "wo", "ich", "wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gl\u00fchen M\u00e4nner, bl\u00fchen Lieder", "tokens": ["Gl\u00fc\u00b7hen", "M\u00e4n\u00b7ner", ",", "bl\u00fc\u00b7hen", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und ich f\u00fchle wohl Vertrauen,", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "wohl", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Auf des Herzens Fels zu bauen,", "tokens": ["Auf", "des", "Her\u00b7zens", "Fels", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Eine neue Burg der Liebe,", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Burg", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die in allem Sturme bliebe,", "tokens": ["Die", "in", "al\u00b7lem", "Stur\u00b7me", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "M\u00e4chtig durch die fernen Zeiten", "tokens": ["M\u00e4ch\u00b7tig", "durch", "die", "fer\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Einen allvereinten Strom zu leiten,", "tokens": ["Ei\u00b7nen", "all\u00b7ver\u00b7ein\u00b7ten", "Strom", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Einen Strom von Lust und Schmerzen,", "tokens": ["Ei\u00b7nen", "Strom", "von", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Alles aus dem eignen Herzen,", "tokens": ["Al\u00b7les", "aus", "dem", "eig\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wo die Lieder all' verschlungen", "tokens": ["Wo", "die", "Lie\u00b7der", "all'", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Alle Herzen wiederklungen,", "tokens": ["Al\u00b7le", "Her\u00b7zen", "wie\u00b7der\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Hohe Freunde dann verb\u00fcndet,", "tokens": ["Ho\u00b7he", "Freun\u00b7de", "dann", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "So der Freude Reich gegr\u00fcndet.", "tokens": ["So", "der", "Freu\u00b7de", "Reich", "ge\u00b7gr\u00fcn\u