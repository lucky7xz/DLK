{"dta.poem.18764": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Die Flut der Leidenschaft sie st\u00fcrmt vergebens", "tokens": ["Die", "Flut", "der", "Lei\u00b7den\u00b7schaft", "sie", "st\u00fcrmt", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An\u2019s unbezwungne, feste Land. \u2014", "tokens": ["An's", "un\u00b7be\u00b7zwung\u00b7ne", ",", "fes\u00b7te", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wirft poetische Perlen an den Strand,", "tokens": ["Sie", "wirft", "po\u00b7e\u00b7ti\u00b7sche", "Per\u00b7len", "an", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und das ist schon Gewinn des Lebens.", "tokens": ["Und", "das", "ist", "schon", "Ge\u00b7winn", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "NN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}