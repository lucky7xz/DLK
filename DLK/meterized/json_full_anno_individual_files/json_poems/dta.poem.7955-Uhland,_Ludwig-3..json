{"dta.poem.7955": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Bedeutungsvoll hast du dein K\u00fcnstlerleben", "tokens": ["Be\u00b7deu\u00b7tungs\u00b7voll", "hast", "du", "dein", "K\u00fcnst\u00b7ler\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit jenem frommen, stillen Bild geschlossen:", "tokens": ["Mit", "je\u00b7nem", "from\u00b7men", ",", "stil\u00b7len", "Bild", "ge\u00b7schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Abraham mit seines Stamms Genossen", "tokens": ["Wie", "Ab\u00b7ra\u00b7ham", "mit", "sei\u00b7nes", "Stamms", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Land begr\u00fc\u00dft, das ihm der Herr gegeben.", "tokens": ["Das", "Land", "be\u00b7gr\u00fc\u00dft", ",", "das", "ihm", "der", "Herr", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da lehnen sie auf ihren Wanderst\u00e4ben,", "tokens": ["Da", "leh\u00b7nen", "sie", "auf", "ih\u00b7ren", "Wan\u00b7der\u00b7st\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Wald und Felsenhang noch halb umschlossen,", "tokens": ["Von", "Wald", "und", "Fel\u00b7sen\u00b7hang", "noch", "halb", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch herrlich sehn sie unter sich ergossen", "tokens": ["Doch", "herr\u00b7lich", "sehn", "sie", "un\u00b7ter", "sich", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das weite Land voll Kornes und voll Reben.", "tokens": ["Das", "wei\u00b7te", "Land", "voll", "Kor\u00b7nes", "und", "voll", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bist auch du nun, abgeschiedne Seele,", "tokens": ["So", "bist", "auch", "du", "nun", ",", "ab\u00b7ge\u00b7schied\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dieses Erdelebens rauher Wilde", "tokens": ["Aus", "die\u00b7ses", "Er\u00b7de\u00b7le\u00b7bens", "rau\u00b7her", "Wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An deiner Wandrung frohes Ziel gekommen;", "tokens": ["An", "dei\u00b7ner", "Wand\u00b7rung", "fro\u00b7hes", "Ziel", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und durch das finstre Thor der Grabesh\u00f6hle", "tokens": ["Und", "durch", "das", "finst\u00b7re", "Thor", "der", "Gra\u00b7bes\u00b7h\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erblickst du schon die seligen Gefilde,", "tokens": ["Er\u00b7blickst", "du", "schon", "die", "se\u00b7li\u00b7gen", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das himmlische Verhei\u00dfungsland der Frommen.", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Ver\u00b7hei\u00b7\u00dfungs\u00b7land", "der", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}