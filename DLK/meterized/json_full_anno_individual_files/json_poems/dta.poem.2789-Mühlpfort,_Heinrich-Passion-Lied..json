{"dta.poem.2789": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Passion-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O HErr der hohen Himmels-Kertzen/", "tokens": ["O", "Herr", "der", "ho\u00b7hen", "Him\u00b7mels\u00b7Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Licht der ewig lichten Macht/", "tokens": ["Du", "Licht", "der", "e\u00b7wig", "lich\u00b7ten", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich martern deine tr\u00fcbe Schmertzen", "tokens": ["Mich", "mar\u00b7tern", "dei\u00b7ne", "tr\u00fc\u00b7be", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gehn mir \u00e4ngstiglich zu Hertzen/", "tokens": ["Und", "gehn", "mir", "\u00e4ngs\u00b7tig\u00b7lich", "zu", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich plagt des Todes d\u00fcstre Nacht/", "tokens": ["Mich", "plagt", "des", "To\u00b7des", "d\u00fcst\u00b7re", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wor zu dich mein Verbrechen bracht.", "tokens": ["Wor", "zu", "dich", "mein", "Ver\u00b7bre\u00b7chen", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Schuld-befreyte Licht der Sonnen", "tokens": ["Das", "Schuld\u00b7be\u00b7frey\u00b7te", "Licht", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat/ als es war umb dich geschehn", "tokens": ["Hat", "/", "als", "es", "war", "umb", "dich", "ge\u00b7schehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dem dein Lebens-Oel verronnen/", "tokens": ["Zu", "dem", "dein", "Le\u00b7bens\u00b7O\u00b7el", "ver\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVINF", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit Nacht sein Antlitz \u00fcbersponnen.", "tokens": ["Mit", "Nacht", "sein", "Ant\u00b7litz", "\u00fc\u00b7bers\u00b7pon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es konte nicht das Unrecht sehn/", "tokens": ["Es", "kon\u00b7te", "nicht", "das", "Un\u00b7recht", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df Selaven ihren K\u00e4yser schm\u00e4hn.", "tokens": ["Da\u00df", "Se\u00b7la\u00b7ven", "ih\u00b7ren", "K\u00e4y\u00b7ser", "schm\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie soll denn ich (da meine S\u00fcnden", "tokens": ["Wie", "soll", "denn", "ich", "(", "da", "mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "KON", "PPER", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich st\u00fcrtzen in den S\u00fcnden-Koth/", "tokens": ["Dich", "st\u00fcrt\u00b7zen", "in", "den", "S\u00fcn\u00b7den\u00b7Koth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du dich Unsterblicher l\u00e4st binden", "tokens": ["Du", "dich", "U\u00b7nsterb\u00b7li\u00b7cher", "l\u00e4st", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "ADJD", "VVFIN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hiermit ich Sterblicher kan finden", "tokens": ["Hier\u00b7mit", "ich", "Sterb\u00b7li\u00b7cher", "kan", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "NN", "VMFIN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Was mir die Freyheit schenckt bey GOtt:)", "tokens": ["Was", "mir", "die", "Frey\u00b7heit", "schenckt", "bey", "Gott", ":)"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht seuffzen \u00fcber deine Noth?", "tokens": ["Nicht", "seuff\u00b7zen", "\u00fc\u00b7ber", "dei\u00b7ne", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich habe mich im Schlam gew\u00fchlet", "tokens": ["Ich", "ha\u00b7be", "mich", "im", "Schlam", "ge\u00b7w\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Seelen reines Kleid befleckt/", "tokens": ["Der", "See\u00b7len", "rei\u00b7nes", "Kleid", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bins der sein Gewissen f\u00fchlet/", "tokens": ["Ich", "bins", "der", "sein", "Ge\u00b7wis\u00b7sen", "f\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf den der H\u00f6llen Rabe zielet/", "tokens": ["Auf", "den", "der", "H\u00f6l\u00b7len", "Ra\u00b7be", "zie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den seine schwartze Schuld erschreckt/", "tokens": ["Den", "sei\u00b7ne", "schwart\u00b7ze", "Schuld", "er\u00b7schreckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil mich der Unflats Mantel deckt.", "tokens": ["Weil", "mich", "der", "Un\u00b7flats", "Man\u00b7tel", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich bin ein Koch der S\u00fcnden-Speisen/", "tokens": ["Ich", "bin", "ein", "Koch", "der", "S\u00fcn\u00b7den\u00b7Spei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich richte nichts denn Laster an/", "tokens": ["Ich", "rich\u00b7te", "nichts", "denn", "Las\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Messer ist von Wollust Eisen/", "tokens": ["Mein", "Mes\u00b7ser", "ist", "von", "Wol\u00b7lust", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Eisen kan kein Engel preisen/", "tokens": ["Mein", "Ei\u00b7sen", "kan", "kein", "En\u00b7gel", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es r\u00fchrt noch her vom ersten Mann/", "tokens": ["Es", "r\u00fchrt", "noch", "her", "vom", "ers\u00b7ten", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich nichts Gutes kochen kan.", "tokens": ["Da\u00df", "ich", "nichts", "Gu\u00b7tes", "ko\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie soll sich meine Schwachheit wagen/", "tokens": ["Wie", "soll", "sich", "mei\u00b7ne", "Schwach\u00b7heit", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir aufzuopffern ihren Gru\u00df?", "tokens": ["Dir", "auf\u00b7zuopf\u00b7fern", "ih\u00b7ren", "Gru\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du im Jammer-vollen Zagen/", "tokens": ["Der", "du", "im", "Jam\u00b7mer\u00b7vol\u00b7len", "Za\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast meine Last mir helffen tragen.", "tokens": ["Hast", "mei\u00b7ne", "Last", "mir", "helf\u00b7fen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und durch den falschen Judas Ku\u00df", "tokens": ["Und", "durch", "den", "fal\u00b7schen", "Ju\u00b7das", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Machst in den Himmel einen Fu\u00df.", "tokens": ["Machst", "in", "den", "Him\u00b7mel", "ei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie soll mit Dancken ich erreichen/", "tokens": ["Wie", "soll", "mit", "Dan\u00b7cken", "ich", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Womit du Heyland mich verehrt?", "tokens": ["Wo\u00b7mit", "du", "Hey\u00b7land", "mich", "ver\u00b7ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind das nicht werthe Liebes-Zeichen?", "tokens": ["Sind", "das", "nicht", "wert\u00b7he", "Lie\u00b7bes\u00b7Zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du l\u00e4st den M\u00f6rdern dich vergleichen/", "tokens": ["Du", "l\u00e4st", "den", "M\u00f6r\u00b7dern", "dich", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der du den Frieden stets gelehrt", "tokens": ["Der", "du", "den", "Frie\u00b7den", "stets", "ge\u00b7lehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Aufruhr jederzeit verst\u00f6hrt.", "tokens": ["Und", "Auf\u00b7ruhr", "je\u00b7der\u00b7zeit", "ver\u00b7st\u00f6hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du nimmst das Roh