ord", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "PWS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schlugen die verwandten Flammen", "tokens": ["So", "schlu\u00b7gen", "die", "ver\u00b7wand\u00b7ten", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt in ", "tokens": ["Zu\u00b7letzt", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Trotz deiner und trotz meiner Pflicht.", "tokens": ["Trotz", "dei\u00b7ner", "und", "trotz", "mei\u00b7ner", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "O weine nicht, da\u00df es geschehen,", "tokens": ["O", "wei\u00b7ne", "nicht", ",", "da\u00df", "es", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du mich k\u00fc\u00dftest leis' und zag \u2013", "tokens": ["Da\u00df", "du", "mich", "k\u00fc\u00df\u00b7test", "leis'", "und", "zag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich bei deiner Seufzer Wehen", "tokens": ["Und", "ich", "bei", "dei\u00b7ner", "Seuf\u00b7zer", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ungest\u00fcm und hei\u00dfem Flehen", "tokens": ["Mit", "Un\u00b7ge\u00b7st\u00fcm", "und", "hei\u00b7\u00dfem", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erzitternd an der Brust dir lag.", "tokens": ["Er\u00b7zit\u00b7ternd", "an", "der", "Brust", "dir", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Umschling' dein Haupt mit wildem Mohne,", "tokens": ["Um\u00b7schling'", "dein", "Haupt", "mit", "wil\u00b7dem", "Moh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergessen soll's auf immer sein;", "tokens": ["Ver\u00b7ges\u00b7sen", "soll's", "auf", "im\u00b7mer", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir aber wird's zu herbem Lohne,", "tokens": ["Mir", "a\u00b7ber", "wird's", "zu", "her\u00b7bem", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Frieden in der Brust dir wohne,", "tokens": ["Da\u00df", "Frie\u00b7den", "in", "der", "Brust", "dir", "woh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bin ich dir ferne \u2013 und allein.", "tokens": ["Bin", "ich", "dir", "fer\u00b7ne", "\u2013", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "$(", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wir f\u00fchlen es und m\u00fcssen's leiden \u2013", "tokens": ["Wir", "f\u00fch\u00b7len", "es", "und", "m\u00fcs\u00b7sen's", "lei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df uns auseinander geh'n;", "tokens": ["So", "la\u00df", "uns", "aus\u00b7ein\u00b7an\u00b7der", "geh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stunde schlug schon l\u00e4ngst uns Beiden,", "tokens": ["Die", "Stun\u00b7de", "schlug", "schon", "l\u00e4ngst", "uns", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leb' wohl, leb' wohl, wir m\u00fcssen scheiden,", "tokens": ["Leb'", "wohl", ",", "leb'", "wohl", ",", "wir", "m\u00fcs\u00b7sen", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Leb' wohl, leb' wohl \u2013 auf Nimmerseh'n!", "tokens": ["Leb'", "wohl", ",", "leb'", "wohl", "\u2013", "auf", "Nim\u00b7mer\u00b7seh'n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "$(", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}