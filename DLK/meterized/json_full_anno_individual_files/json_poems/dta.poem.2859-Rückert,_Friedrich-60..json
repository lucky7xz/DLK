{"dta.poem.2859": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist, mein J\u00fcngling, nun in den Erobrungsjahren,", "tokens": ["Du", "bist", ",", "mein", "J\u00fcng\u00b7ling", ",", "nun", "in", "den", "E\u00b7rob\u00b7rungs\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo man erwerben will, und noch nicht mu\u00df bewahren.", "tokens": ["Wo", "man", "er\u00b7wer\u00b7ben", "will", ",", "und", "noch", "nicht", "mu\u00df", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "$,", "KON", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erwirb soviel du kannst, wend' an was du gewannst,", "tokens": ["Er\u00b7wirb", "so\u00b7viel", "du", "kannst", ",", "wend'", "an", "was", "du", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VMFIN", "$,", "KOUS", "APPR", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freue dich, da\u00df du stets weitern Kreis umspannst.", "tokens": ["Und", "freu\u00b7e", "dich", ",", "da\u00df", "du", "stets", "wei\u00b7tern", "Kreis", "um\u00b7spannst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann aber, um nicht ins Unendliche zu flie\u00dfen,", "tokens": ["Dann", "a\u00b7ber", ",", "um", "nicht", "ins", "Un\u00b7end\u00b7li\u00b7che", "zu", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUI", "PTKNEG", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wirst du gen\u00f6thigt seyn dich endlich abzuschlie\u00dfen;", "tokens": ["Wirst", "du", "ge\u00b7n\u00f6t\u00b7higt", "seyn", "dich", "end\u00b7lich", "ab\u00b7zu\u00b7schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VAINF", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann gl\u00fccklich, wenn du aus dem Weitern, das zerscheitert,", "tokens": ["Dann", "gl\u00fcck\u00b7lich", ",", "wenn", "du", "aus", "dem", "Wei\u00b7tern", ",", "das", "zer\u00b7schei\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den heitern Geist gewannst, der Enges dir erweitert.", "tokens": ["Den", "hei\u00b7tern", "Geist", "ge\u00b7wannst", ",", "der", "En\u00b7ges", "dir", "er\u00b7wei\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}