{"textgrid.poem.50790": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ich mag rufen, schreien, wie ich will,", "tokens": ["O", "ich", "mag", "ru\u00b7fen", ",", "schrei\u00b7en", ",", "wie", "ich", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es wird mein Angstruf nimmermehr vernommen;", "tokens": ["Es", "wird", "mein", "Angs\u00b7truf", "nim\u00b7mer\u00b7mehr", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da oben bleibt es, wie da unten, still,", "tokens": ["Da", "o\u00b7ben", "bleibt", "es", ",", "wie", "da", "un\u00b7ten", ",", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer sollte auch zu diesem H\u00fcgel kommen?", "tokens": ["Wer", "soll\u00b7te", "auch", "zu", "die\u00b7sem", "H\u00fc\u00b7gel", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn meine Mutter ist romantisch nicht,", "tokens": ["Denn", "mei\u00b7ne", "Mut\u00b7ter", "ist", "ro\u00b7man\u00b7tisch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und, alt und schwach, bleibt einsam sie zu Hause;", "tokens": ["Und", ",", "alt", "und", "schwach", ",", "bleibt", "ein\u00b7sam", "sie", "zu", "Hau\u00b7se", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ihr das Herz ob meinem Tode bricht,", "tokens": ["Wenn", "ihr", "das", "Herz", "ob", "mei\u00b7nem", "To\u00b7de", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie birgt's und weint in der verschlo\u00dfnen Klause.", "tokens": ["Sie", "birgt's", "und", "weint", "in", "der", "ver\u00b7schlo\u00df\u00b7nen", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja h\u00e4tt ich ein verla\u00dfnes Liebchen nun,", "tokens": ["Ja", "h\u00e4tt", "ich", "ein", "ver\u00b7la\u00df\u00b7nes", "Lieb\u00b7chen", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das vor dem Morgenrot zu klagen k\u00e4me,", "tokens": ["Das", "vor", "dem", "Mor\u00b7gen\u00b7rot", "zu", "kla\u00b7gen", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf meiner k\u00fchlen Erde auszuruhn,", "tokens": ["Auf", "mei\u00b7ner", "k\u00fch\u00b7len", "Er\u00b7de", "aus\u00b7zu\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meinen Jammer wonnevoll vern\u00e4hme!", "tokens": ["Und", "mei\u00b7nen", "Jam\u00b7mer", "won\u00b7ne\u00b7voll", "ver\u00b7n\u00e4h\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Warum hab ich's der ", "tokens": ["Wa\u00b7rum", "hab", "ich's", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PIS", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Da\u00df junge Liebe mir im Herzen sprosse?", "tokens": ["Da\u00df", "jun\u00b7ge", "Lie\u00b7be", "mir", "im", "Her\u00b7zen", "spros\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hab gezaudert und es nicht gewagt \u2013", "tokens": ["Ich", "hab", "ge\u00b7zau\u00b7dert", "und", "es", "nicht", "ge\u00b7wagt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Krankheit kam und diese tolle Posse.", "tokens": ["Die", "Krank\u00b7heit", "kam", "und", "die\u00b7se", "tol\u00b7le", "Pos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O ich mag rufen, schreien, wie ich will,", "tokens": ["O", "ich", "mag", "ru\u00b7fen", ",", "schrei\u00b7en", ",", "wie", "ich", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es wird mein Angstruf nimmermehr vernommen;", "tokens": ["Es", "wird", "mein", "Angs\u00b7truf", "nim\u00b7mer\u00b7mehr", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da oben bleibt es, wie da unten, still,", "tokens": ["Da", "o\u00b7ben", "bleibt", "es", ",", "wie", "da", "un\u00b7ten", ",", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer sollte auch zu diesem H\u00fcgel kommen?", "tokens": ["Wer", "soll\u00b7te", "auch", "zu", "die\u00b7sem", "H\u00fc\u00b7gel", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Denn meine Mutter ist romantisch nicht,", "tokens": ["Denn", "mei\u00b7ne", "Mut\u00b7ter", "ist", "ro\u00b7man\u00b7tisch", "nicht", ","], "token_info": ["word", "wor