{"textgrid.poem.55372": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wandersegen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wanderjahre sind nun angetreten,", "tokens": ["Die", "Wan\u00b7der\u00b7jah\u00b7re", "sind", "nun", "an\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und jeder Schritt des Wandrers ist bedenklich.", "tokens": ["Und", "je\u00b7der", "Schritt", "des", "Wand\u00b7rers", "ist", "be\u00b7denk\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwar pflegt er nicht zu singen und zu beten;", "tokens": ["Zwar", "pflegt", "er", "nicht", "zu", "sin\u00b7gen", "und", "zu", "be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch wendet er, sobald der Pfad verf\u00e4nglich,", "tokens": ["Doch", "wen\u00b7det", "er", ",", "so\u00b7bald", "der", "Pfad", "ver\u00b7f\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den ernsten Blick, wo Nebel ihn umtr\u00fcben,", "tokens": ["Den", "erns\u00b7ten", "Blick", ",", "wo", "Ne\u00b7bel", "ihn", "um\u00b7tr\u00fc\u00b7ben", ","], "toke