{"textgrid.poem.65843": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "117", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beschuld'ge mich, da\u00df ich mit karger Gegengabe", "tokens": ["Be\u00b7schuld'\u00b7ge", "mich", ",", "da\u00df", "ich", "mit", "kar\u00b7ger", "Ge\u00b7gen\u00b7ga\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All deine beste Wohltat hab' erkannt,", "tokens": ["All", "dei\u00b7ne", "bes\u00b7te", "Wohl\u00b7tat", "hab'", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich dein liebstes Herz verabs\u00e4umt habe,", "tokens": ["Da\u00df", "ich", "dein", "liebs\u00b7tes", "Herz", "ver\u00b7ab\u00b7s\u00e4umt", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Woran mich Tag f\u00fcr Tag mit allen Klammern band:", "tokens": ["Wo\u00b7ran", "mich", "Tag", "f\u00fcr", "Tag", "mit", "al\u00b7len", "Klam\u00b7mern", "band", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df ich mich h\u00e4ufig lie\u00df mit fremden Seelen finden,", "tokens": ["Da\u00df", "ich", "mich", "h\u00e4u\u00b7fig", "lie\u00df", "mit", "frem\u00b7den", "See\u00b7len", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein teures Recht vergeudend, meine Pflicht;", "tokens": ["Dein", "teu\u00b7res", "Recht", "ver\u00b7geu\u00b7dend", ",", "mei\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich die Segel aufzog allen Winden,", "tokens": ["Da\u00df", "ich", "die", "Se\u00b7gel", "auf\u00b7zog", "al\u00b7len", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu fernster Flucht aus deinem Angesicht.", "tokens": ["Zu", "ferns\u00b7ter", "Flucht", "aus", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Verzeichne jeden Fehler, alle Launen,", "tokens": ["Ver\u00b7zeich\u00b7ne", "je\u00b7den", "Feh\u00b7ler", ",", "al\u00b7le", "Lau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Verd\u00e4cht'ge mich, wenn die Beweise voll;", "tokens": ["Ver\u00b7d\u00e4cht'\u00b7ge", "mich", ",", "wenn", "die", "Be\u00b7wei\u00b7se", "voll", ";"], "token_info": ["word", "word", "pu