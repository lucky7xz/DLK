"punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df \u00fcber Nebel, \u00fcber Erdget\u00fcmmel,", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "Ne\u00b7bel", ",", "\u00fc\u00b7ber", "Erd\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im sanften Fluge, mit der Taube Schwung", "tokens": ["Im", "sanf\u00b7ten", "Flu\u00b7ge", ",", "mit", "der", "Tau\u00b7be", "Schwung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du aufsteigst, f\u00fchlst in Dir und tr\u00e4gst den Himmel", "tokens": ["Du", "auf\u00b7steigst", ",", "f\u00fchlst", "in", "Dir", "und", "tr\u00e4gst", "den", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In uns mit t\u00e4uschender Beseligung,", "tokens": ["In", "uns", "mit", "t\u00e4u\u00b7schen\u00b7der", "Be\u00b7se\u00b7li\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und l\u00e4ssest, was Du dort in lichten H\u00f6hen", "tokens": ["Und", "l\u00e4s\u00b7sest", ",", "was", "Du", "dort", "in", "lich\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "APPR", "ADJA", "NN"], "met