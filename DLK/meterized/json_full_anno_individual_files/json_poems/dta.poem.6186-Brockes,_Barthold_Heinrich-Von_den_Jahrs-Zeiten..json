{"dta.poem.6186": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns die gautze Welt durchgehn,", "tokens": ["Lasst", "uns", "die", "gaut\u00b7ze", "Welt", "durch\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird man sehn,", "tokens": ["So", "wird", "man", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df alle Ding auf eine Weise,", "tokens": ["Da\u00df", "al\u00b7le", "Ding", "auf", "ei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zwar durch einen Druck geschehn.", "tokens": ["Und", "zwar", "durch", "ei\u00b7nen", "Druck", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man sieht, durch diesen Trieb in ihrem Creise,", "tokens": ["Man", "sieht", ",", "durch", "die\u00b7sen", "Trieb", "in", "ih\u00b7rem", "Crei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Man mag nun, da\u00df die Sonn, wie, oder da\u00df die Erde", "tokens": ["Man", "mag", "nun", ",", "da\u00df", "die", "Sonn", ",", "wie", ",", "o\u00b7der", "da\u00df", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "$,", "KOUS", "ART", "NN", "$,", "PWAV", "$,", "KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Herumgef\u00fchret werde,", "tokens": ["Her\u00b7um\u00b7ge\u00b7f\u00fch\u00b7ret", "wer\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nach unterschiedner Meinung, lehren;", "tokens": ["Nach", "un\u00b7ter\u00b7schied\u00b7ner", "Mei\u00b7nung", ",", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So wird die W\u00fcrckung doch, die eigen dem Bewegen,", "tokens": ["So", "wird", "die", "W\u00fcr\u00b7ckung", "doch", ",", "die", "ei\u00b7gen", "dem", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auf gleiche Weise sich zu Tage legen.", "tokens": ["Auf", "glei\u00b7che", "Wei\u00b7se", "sich", "zu", "Ta\u00b7ge", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}