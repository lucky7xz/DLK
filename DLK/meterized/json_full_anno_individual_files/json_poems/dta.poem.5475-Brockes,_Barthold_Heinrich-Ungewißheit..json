{"dta.poem.5475": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ungewi\u00dfheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser hellen Finsterni\u00df,", "tokens": ["In", "die\u00b7ser", "hel\u00b7len", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In welcher wir auf Erden stecken,", "tokens": ["In", "wel\u00b7cher", "wir", "auf", "Er\u00b7den", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird ein Vern\u00fcnftiger gar leicht entdecken,", "tokens": ["Wird", "ein", "Ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "gar", "leicht", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df alles Wissen ungewi\u00df.", "tokens": ["Da\u00df", "al\u00b7les", "Wis\u00b7sen", "un\u00b7ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ungewi\u00dfheit geht so gar so weit,", "tokens": ["Die", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", "geht", "so", "gar", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df man,", "tokens": ["Da\u00df", "man", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PIS", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Mit Recht und Zuverl\u00e4\u00dfigkeit,", "tokens": ["Mit", "Recht", "und", "Zu\u00b7ver\u00b7l\u00e4\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df alles ungewi\u00df, gewi\u00df kaum sagen kann.", "tokens": ["Da\u00df", "al\u00b7les", "un\u00b7ge\u00b7wi\u00df", ",", "ge\u00b7wi\u00df", "kaum", "sa\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "$,", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}