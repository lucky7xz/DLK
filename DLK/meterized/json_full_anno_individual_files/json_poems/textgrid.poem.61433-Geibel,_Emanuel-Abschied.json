{"textgrid.poem.61433": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb' wohl, leb' wohl, mein Kind, und keine Klage!", "tokens": ["Leb'", "wohl", ",", "leb'", "wohl", ",", "mein", "Kind", ",", "und", "kei\u00b7ne", "Kla\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch einen Ku\u00df, noch eine Neige Wein!", "tokens": ["Noch", "ei\u00b7nen", "Ku\u00df", ",", "noch", "ei\u00b7ne", "Nei\u00b7ge", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So licht und freundlich waren diese Tage,", "tokens": ["So", "licht", "und", "freund\u00b7lich", "wa\u00b7ren", "die\u00b7se", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df freundlich auch den Abschied sein.", "tokens": ["La\u00df", "freund\u00b7lich", "auch", "den", "Ab\u00b7schied", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, wenn hinab zu s\u00fcdlich fernen Borden", "tokens": ["Sieh", ",", "wenn", "hin\u00b7ab", "zu", "s\u00fcd\u00b7lich", "fer\u00b7nen", "Bor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADV", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im langen Wanderzug der Kranich schwirrt,", "tokens": ["Im", "lan\u00b7gen", "Wan\u00b7der\u00b7zug", "der", "Kra\u00b7nich", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begleitet ihn ein Traum vom gr\u00fcnen Norden,", "tokens": ["Be\u00b7glei\u00b7tet", "ihn", "ein", "Traum", "vom", "gr\u00fc\u00b7nen", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er sp\u00fcrt es, da\u00df er wiederkehren wird.", "tokens": ["Er", "sp\u00fcrt", "es", ",", "da\u00df", "er", "wie\u00b7der\u00b7keh\u00b7ren", "wird", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wird auch uns von unserm kurzen Gl\u00fccke", "tokens": ["So", "wird", "auch", "uns", "von", "un\u00b7serm", "kur\u00b7zen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Schimmer fort und fort im Herzen stehn,", "tokens": ["Ein", "Schim\u00b7mer", "fort", "und", "fort", "im", "Her\u00b7zen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos":