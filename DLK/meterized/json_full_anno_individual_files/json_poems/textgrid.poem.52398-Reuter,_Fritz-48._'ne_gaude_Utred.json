{"textgrid.poem.52398": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "48. 'ne gaude Utred", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oll Hinrich Kloth was Schippskaptein.", "tokens": ["Oll", "Hin\u00b7rich", "Kloth", "was", "Schipps\u00b7kap\u00b7tein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PWS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An sinen Burd was dat man ganz gew\u00f6hnlich", "tokens": ["An", "si\u00b7nen", "Burd", "was", "dat", "man", "ganz", "ge\u00b7w\u00f6hn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PWS", "ART", "PIS", "ADV", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Un sihr unnaschig antauseihn,", "tokens": ["Un", "sihr", "un\u00b7na\u00b7schig", "an\u00b7tau\u00b7seihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Un hei ok s\u00fclwst, hei was pers\u00f6nlich", "tokens": ["Un", "hei", "ok", "s\u00fclwst", ",", "hei", "was", "per\u00b7s\u00f6n\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "$,", "ITJ", "PWS", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Up sine Ort nicks weniger as \u00bbfein\u00ab.", "tokens": ["Up", "si\u00b7ne", "Ort", "nicks", "we\u00b7ni\u00b7ger", "as", "\u00bb", "fein", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "ADJD", "$(", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Dat g\u00fcng bi em man muddlig tau,", "tokens": ["Dat", "g\u00fcng", "bi", "em", "man", "mudd\u00b7lig", "tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Indessen, wenn em wer bes\u00f6cht,", "tokens": ["In\u00b7des\u00b7sen", ",", "wenn", "em", "wer", "be\u00b7s\u00f6cht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn kramt hei all'ns en beten t'recht,", "tokens": ["Denn", "kramt", "hei", "all'ns", "en", "be\u00b7ten", "t'\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Namm't mit de Wohrheit nich genau", "tokens": ["Nam\u00b7m't", "mit", "de", "Wohr\u00b7heit", "nich", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN", "PTKNEG", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Un log f\u00f6r sinen Muddelkram", "tokens": ["Un", "log", "f\u00f6r", "si\u00b7nen", "Mud\u00b7del\u00b7kram"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "'ne sch\u00f6ne Utred glik tausam.", "tokens": ["'ne", "sch\u00f6\u00b7ne", "Ut\u00b7red", "glik", "tau\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Un ok sin L\u00fcd', de w\u00fc\u00dften drup tau lopen,", "tokens": ["Un", "ok", "sin", "L\u00fcd'", ",", "de", "w\u00fc\u00df\u00b7ten", "drup", "tau", "lo\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Wenn hei irst log, denn logen s' alltauhopen.", "tokens": ["Wenn", "hei", "irst", "log", ",", "denn", "lo\u00b7gen", "s'", "all\u00b7tau\u00b7ho\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "$,", "KON", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Eins kam sin Reeder taum Bes\u00e4uk.", "tokens": ["Eins", "kam", "sin", "Ree\u00b7der", "taum", "Be\u00b7s\u00e4uk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbjung'\u00ab, seggt nu unse oll Kaptein,", "tokens": ["\u00bb", "jung'", "\u00ab", ",", "seggt", "nu", "un\u00b7se", "oll", "Kap\u00b7tein", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$(", "$,", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmak de Kaj\u00fct uns fixing rein", "tokens": ["\u00bb", "mak", "de", "Ka\u00b7j\u00fct", "uns", "fi\u00b7xing", "rein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "NE", "NE", "PPER", "VVFIN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Un mak dor allens gaud tau R\u00e4uk,", "tokens": ["Un", "mak", "dor", "al\u00b7lens", "gaud", "tau", "R\u00e4uk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Un denn besorg uns fix en moy,", "tokens": ["Un", "denn", "be\u00b7sorg", "uns", "fix", "en", "moy", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "FM", "FM", "FM", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Vullst\u00e4nnig Fr\u00fchst\u00fcck in de Koj'.\u00ab", "tokens": ["Vull\u00b7st\u00e4n\u00b7nig", "Fr\u00fch\u00b7st\u00fcck", "in", "de", "Ko\u00b7j'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "APPR", "NE", "NE", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "De Jung was Niling noch, un Peiter heit'e.", "tokens": ["De", "Jung", "was", "Ni\u00b7ling", "noch", ",", "un", "Pei\u00b7ter", "heit'", "e."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["NE", "NE", "PWS", "NE", "ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch wat sin Herr em heit, dat deiht'e.", "tokens": ["Doch", "wat", "sin", "Herr", "em", "heit", ",", "dat", "deiht'", "e."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "As nu de Kopmann sine Saken", "tokens": ["As", "nu", "de", "Kop\u00b7mann", "si\u00b7ne", "Sa\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "NE", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit unsen Hinrich hett bespraken,", "tokens": ["Mit", "un\u00b7sen", "Hin\u00b7rich", "hett", "be\u00b7spra\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dunn gahn sei in de Koj' rin,", "tokens": ["Dunn", "gahn", "sei", "in", "de", "Ko\u00b7j'", "rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dor ward en sch\u00f6nes Fr\u00fchst\u00fcck sin,", "tokens": ["Dor", "ward", "en", "sch\u00f6\u00b7nes", "Fr\u00fch\u00b7st\u00fcck", "sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "FM", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Blot \u00e4wer'n Disch, dor liggt kein Laken.", "tokens": ["Blot", "\u00e4\u00b7wer'n", "Disch", ",", "dor", "liggt", "kein", "La\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "\u00bbjung'\u00ab, seggt nu unse oll Kaptein,", "tokens": ["\u00bb", "jung'", "\u00ab", ",", "seggt", "nu", "un\u00b7se", "oll", "Kap\u00b7tein", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$(", "$,", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "\u00bbwat hest kein Laken \u00e4werleggt?\u00ab", "tokens": ["\u00bb", "wat", "hest", "kein", "La\u00b7ken", "\u00e4\u00b7wer\u00b7leggt", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "De Jung', de grint em an un seggt:", "tokens": ["De", "Jung'", ",", "de", "grint", "em", "an", "un", "seggt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u00bbje, Herr Kaptein, wi heww'n jo kein.\u00ab", "tokens": ["\u00bb", "je", ",", "Herr", "Kap\u00b7tein", ",", "wi", "hew\u00b7w'n", "jo", "kein", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "NN", "NN", "$,", "PWAV", "NE", "NE", "PIAT", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Na, 't m\u00f6t so g\u00e4hn. \u2013 De Kopmann ett,", "tokens": ["Na", ",", "'t", "m\u00f6t", "so", "g\u00e4hn", ".", "\u2013", "De", "Kop\u00b7mann", "ett", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "NE", "ADV", "VVINF", "$.", "$(", "NE", "NE", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Un as hei sick vern\u00fcchtert hett,", "tokens": ["Un", "as", "hei", "sick", "ver\u00b7n\u00fcch\u00b7tert", "hett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Dunn geiht'e.", "tokens": ["Dunn", "geiht'", "e."], "token_info": ["word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Un de Kaptein, de r\u00f6ppt nu: \u00bbPeiter!\u00ab", "tokens": ["Un", "de", "Kap\u00b7tein", ",", "de", "r\u00f6ppt", "nu", ":", "\u00bb", "Pei\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["NE", "NE", "NN", "$,", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Un b\u00f6gt em dal un klemmt em mang de Bein.", "tokens": ["Un", "b\u00f6gt", "em", "dal", "un", "klemmt", "em", "mang", "de", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "\u00bbt\u00e4uw, du Hallunk! \u2013 Wi hewwn jo kein!", "tokens": ["\u00bb", "t\u00e4uw", ",", "du", "Hal\u00b7lunk", "!", "\u2013", "Wi", "hewwn", "jo", "kein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "PPER", "NN", "$.", "$(", "NE", "NE", "NE", "PIAT", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Kannst du nich segg'n: Dat Dauk is in de Wasch?\u00ab", "tokens": ["Kannst", "du", "nich", "seg\u00b7g'n", ":", "Dat", "Dauk", "is", "in", "de", "Wasch", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "FM", "FM", "FM", "APPR", "NE", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.25": {"text": "Un swabb! un swabb! giwwt dat denn nu Kalasch.", "tokens": ["Un", "swabb", "!", "un", "swabb", "!", "giwwt", "dat", "denn", "nu", "Ka\u00b7lasch", "."], "token_info"