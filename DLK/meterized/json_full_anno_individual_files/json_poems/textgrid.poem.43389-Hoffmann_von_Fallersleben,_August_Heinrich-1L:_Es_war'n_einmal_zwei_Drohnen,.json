{"textgrid.poem.43389": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war'n einmal zwei Drohnen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war'n einmal zwei Drohnen,", "tokens": ["Es", "wa\u00b7r'n", "ein\u00b7mal", "zwei", "Droh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Die wollten nicht werken und frohnen.", "tokens": ["Die", "woll\u00b7ten", "nicht", "wer\u00b7ken", "und", "froh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVINF", "KON", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie qu\u00e4lten beide sich", "tokens": ["Sie", "qu\u00e4l\u00b7ten", "bei\u00b7de", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Um Nahrung gar nicht sehr;", "tokens": ["Um", "Nah\u00b7rung", "gar", "nicht", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die eine th\u00e4t gar nichts,", "tokens": ["Die", "ei\u00b7ne", "th\u00e4t", "gar", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die andre nicht viel mehr.", "tokens": ["Die", "and\u00b7re", "nicht", "viel", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sprach die eine zur andern:", "tokens": ["Da", "sprach", "die", "ei\u00b7ne", "zur", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "APPRART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Beschwerlich ist das Wandern.", "tokens": ["Be\u00b7schwer\u00b7lich", "ist", "das", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df ein sch\u00f6nes Haus", "tokens": ["Ich", "wei\u00df", "ein", "sch\u00f6\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Speis' und Trank vollauf,", "tokens": ["Mit", "Speis'", "und", "Trank", "vol\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm, la\u00df uns dahin richten", "tokens": ["Komm", ",", "la\u00df", "uns", "da\u00b7hin", "rich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fein eilig unsern Lauf.", "tokens": ["Fein", "ei\u00b7lig", "un\u00b7sern", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es sollen uns die Bienen", "tokens": ["Es", "sol\u00b7len", "uns", "die", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Brod schon mit verdienen;", "tokens": ["Das", "Brod", "schon", "mit", "ver\u00b7die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn sie uns nicht lassen", "tokens": ["Und", "wenn", "sie", "uns", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gutwillig in das Haus,", "tokens": ["Gut\u00b7wil\u00b7lig", "in", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So st\u00fcrmen wir hinein", "tokens": ["So", "st\u00fcr\u00b7men", "wir", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und jagen sie hinaus.", "tokens": ["Und", "ja\u00b7gen", "sie", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kaum war die Nacht gewichen,", "tokens": ["Kaum", "war", "die", "Nacht", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So kamen die Drohnen geschlichen;", "tokens": ["So", "ka\u00b7men", "die", "Droh\u00b7nen", "ge\u00b7schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie klopften nicht erst an,", "tokens": ["Sie", "klopf\u00b7ten", "nicht", "erst", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie zogen keck hinein,", "tokens": ["Sie", "zo\u00b7gen", "keck", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man hie\u00df die stolzen G\u00e4ste", "tokens": ["Man", "hie\u00df", "die", "stol\u00b7zen", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ganz gottwillkommen sein.", "tokens": ["Ganz", "gott\u00b7will\u00b7kom\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da lebten im Saus' und Brause", "tokens": ["Da", "leb\u00b7ten", "im", "Saus'", "und", "Brau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Drohnen in dem Hause;", "tokens": ["Die", "Droh\u00b7nen", "in", "dem", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie a\u00dfen und sie tranken", "tokens": ["Sie", "a\u00b7\u00dfen", "und", "sie", "tran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ganzen lieben Tag,", "tokens": ["Den", "gan\u00b7zen", "lie\u00b7ben", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie tranken und sie a\u00dfen", "tokens": ["Sie", "tran\u00b7ken", "und", "sie", "a\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was einer nur vermag.", "tokens": ["Was", "ei\u00b7ner", "nur", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Bienen darob sich beklagten,", "tokens": ["Die", "Bie\u00b7nen", "da\u00b7rob", "sich", "be\u00b7klag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und endlich die G\u00e4ste fragten:", "tokens": ["Und", "end\u00b7lich", "die", "G\u00e4s\u00b7te", "frag\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr Drohnen wollt nichts thun \u2013", "tokens": ["Ihr", "Droh\u00b7nen", "wollt", "nichts", "thun", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sagt an, was f\u00e4llt euch ein?", "tokens": ["Sagt", "an", ",", "was", "f\u00e4llt", "euch", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In unserm Staate hier", "tokens": ["In", "un\u00b7serm", "Staa\u00b7te", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df jeder th\u00e4tig sein.", "tokens": ["Mu\u00df", "je\u00b7der", "th\u00e4\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da sprachen die Drohnen zu ihnen:", "tokens": ["Da", "spra\u00b7chen", "die", "Droh\u00b7nen", "zu", "ih\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihr lieben guten Bienen!", "tokens": ["Ihr", "lie\u00b7ben", "gu\u00b7ten", "Bie\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind von altem Adel,", "tokens": ["Wir", "sind", "von", "al\u00b7tem", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein freigebor'n Geschlecht;", "tokens": ["Ein", "frei\u00b7ge\u00b7bor'n", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ihr uns m\u00fc\u00dft ern\u00e4hren,", "tokens": ["Da\u00df", "ihr", "uns", "m\u00fc\u00dft", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ist ja unser Recht.", "tokens": ["Das", "ist", "ja", "un\u00b7ser", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die adelichen Drohnen,", "tokens": ["Die", "a\u00b7de\u00b7li\u00b7chen", "Droh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie wollten nicht werken und frohnen,", "tokens": ["Sie", "woll\u00b7ten", "nicht", "wer\u00b7ken", "und", "froh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie lebten nach wie vor", "tokens": ["Sie", "leb\u00b7ten", "nach", "wie", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "KOKOM", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In kummerloser Ruh',", "tokens": ["In", "kum\u00b7mer\u00b7lo\u00b7ser", "Ruh'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wurden immer mehr,", "tokens": ["Und", "wur\u00b7den", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und schmausten immerzu.", "tokens": ["Und", "schmaus\u00b7ten", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das hat denn die Bienen verdro\u00dfen,", "tokens": ["Das", "hat", "denn", "die", "Bie\u00b7nen", "ver\u00b7dro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sie haben einen Bund geschlossen.", "tokens": ["Und", "sie", "ha\u00b7ben", "ei\u00b7nen", "Bund", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da war die Sache bald", "tokens": ["Da", "war", "die", "Sa\u00b7che", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und gr\u00fcndlich abgemacht:", "tokens": ["Und", "gr\u00fcnd\u00b7lich", "ab\u00b7ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schlugen eines Tags", "tokens": ["Sie", "schlu\u00b7gen", "ei\u00b7nes", "Tags"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die gro\u00dfe Drohnenschlacht.", "tokens": ["Die", "gro\u00b7\u00dfe", "Droh\u00b7nen\u00b7schlacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es war'n einmal zwei Drohnen,", "tokens": ["Es", "wa\u00b7r'n", "ein\u00b7mal", "zwei", "Droh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Die wollten nicht werken und frohnen.", "tokens": ["Die", "woll\u00b7ten", "nicht", "wer\u00b7ken", "und", "froh\u00b7nen", "."], "tok