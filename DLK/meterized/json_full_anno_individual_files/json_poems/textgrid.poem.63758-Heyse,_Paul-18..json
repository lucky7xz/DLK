{"textgrid.poem.63758": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kannte dieses Haus in fr\u00fchern Tagen,", "tokens": ["Ich", "kann\u00b7te", "die\u00b7ses", "Haus", "in", "fr\u00fc\u00b7hern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da schimmert' es von wei\u00dfen Marmorbildern,", "tokens": ["Da", "schim\u00b7mert'", "es", "von", "wei\u00b7\u00dfen", "Mar\u00b7mor\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von goldnen W\u00e4nden, L\u00fcstern, Wappenschildern,", "tokens": ["Von", "gold\u00b7nen", "W\u00e4n\u00b7den", ",", "L\u00fcs\u00b7tern", ",", "Wap\u00b7pen\u00b7schil\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von stolzer Pracht und \u00fcppigem Behagen.", "tokens": ["Von", "stol\u00b7zer", "Pracht", "und", "\u00fcp\u00b7pi\u00b7gem", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Heut weht hindurch ein Herbsthauch von Entsagen,", "tokens": ["Heut", "weht", "hin\u00b7durch", "ein", "Herbst\u00b7hauch", "von", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der alle Farben d\u00e4mpfen will und mildern,", "tokens": ["Der", "al\u00b7le", "Far\u00b7ben", "d\u00e4mp\u00b7fen", "will", "und", "mil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Haus und Park ein reizendes Verwildern,", "tokens": ["In", "Haus", "und", "Park", "ein", "rei\u00b7zen\u00b7des", "Ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch sch\u00f6ner fast, als da sie Schmuck getragen.", "tokens": ["Noch", "sch\u00f6\u00b7ner", "fast", ",", "als", "da", "sie", "Schmuck", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gleich einer stolzen Seele, die sich lange", "tokens": ["Gleich", "ei\u00b7ner", "stol\u00b7zen", "See\u00b7le", ",", "die", "sich", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewu\u00dft geblieben strenggeme\u00dfner Pflichten", "tokens": ["Be\u00b7wu\u00dft", "ge\u00b7blie\u00b7ben", "streng\u00b7ge\u00b7me\u00df\u00b7ner", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "ADJA", "NN"