{"dta.poem.14224": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied und Wiedersehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! Lust und Mond und Sterne sind vergangen,", "tokens": ["Ach", "!", "Lust", "und", "Mond", "und", "Ster\u00b7ne", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "KON", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Fenster mahnen schon die Morgenwinde:", "tokens": ["Am", "Fens\u00b7ter", "mah\u00b7nen", "schon", "die", "Mor\u00b7gen\u00b7win\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich vom Nacken leis die Arme winde,", "tokens": ["Da\u00df", "ich", "vom", "Na\u00b7cken", "leis", "die", "Ar\u00b7me", "win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die noch im Schlummer lieblich mich umfangen.", "tokens": ["Die", "noch", "im", "Schlum\u00b7mer", "lieb\u00b7lich", "mich", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}