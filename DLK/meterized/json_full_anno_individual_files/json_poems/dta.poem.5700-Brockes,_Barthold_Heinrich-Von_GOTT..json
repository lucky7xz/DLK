{"dta.poem.5700": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewird denn der GOTT, den ihr zum GOTT erlesen,", "tokens": ["\u201e", "wird", "denn", "der", "GoTT", ",", "den", "ihr", "zum", "GoTT", "er\u00b7le\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201eund welcher ", "tokens": ["\u201e", "und", "wel\u00b7cher"], "token_info": ["punct", "word", "word"], "pos": ["$(", "KON", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "\u201eden schwachen Sterblichen dadurch wol mehr bekannt?", "tokens": ["\u201e", "den", "schwa\u00b7chen", "Sterb\u00b7li\u00b7chen", "da\u00b7durch", "wol", "mehr", "be\u00b7kannt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "PAV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nein, nein, ihr Epicurer, nein", "tokens": ["Nein", ",", "nein", ",", "ihr", "E\u00b7pi\u00b7cu\u00b7rer", ",", "nein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPOSAT", "NN", "$,", "PTKANT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es folget Euer Stoltz, von einem falschen Schlu\u00df,", "tokens": ["Es", "fol\u00b7get", "Eu\u00b7er", "Stoltz", ",", "von", "ei\u00b7nem", "fal\u00b7schen", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den ja so falschen Schein.", "tokens": ["Den", "ja", "so", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Gr\u00f6sse GOTTES ists und Sein\u2019 Unendlichkeit,", "tokens": ["Die", "Gr\u00f6s\u00b7se", "GoT\u00b7TES", "ists", "und", "Sein'", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die unser kleiner Geist nicht fassen kan noch mu\u00df.", "tokens": ["Die", "un\u00b7ser", "klei\u00b7ner", "Geist", "nicht", "fas\u00b7sen", "kan", "noch", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "VMFIN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein;", "tokens": ["Al\u00b7lein", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Nichts ist so klar erkannt, als GOTTES ", "tokens": ["Nichts", "ist", "so", "klar", "er\u00b7kannt", ",", "als", "GoT\u00b7TES"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "KOUS", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Nichts deutlicher gef\u00fchlt, als Seine Macht.", "tokens": ["Nichts", "deut\u00b7li\u00b7cher", "ge\u00b7f\u00fchlt", ",", "als", "Sei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "O ihr! die ihr verharrt in eurer Blindheits-Nacht,", "tokens": ["O", "ihr", "!", "die", "ihr", "ver\u00b7harrt", "in", "eu\u00b7rer", "Blind\u00b7heits\u00b7Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$.", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die ihr der Welt", "tokens": ["Die", "ihr", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Ein Ungefehr zur ", "tokens": ["Ein", "Un\u00b7ge\u00b7fehr", "zur"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Indem ihr einen GOTT verneint, wird jedennoch", "tokens": ["In\u00b7dem", "ihr", "ei\u00b7nen", "GoTT", "ver\u00b7neint", ",", "wird", "je\u00b7den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch euren eigenen Verstand,", "tokens": ["Durch", "eu\u00b7ren", "ei\u00b7ge\u00b7nen", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der euch selbst widerspricht,", "tokens": ["Der", "euch", "selbst", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ein Ewigs Seyn empfunden und erkannt.", "tokens": ["Ein", "E\u00b7wigs", "Seyn", "emp\u00b7fun\u00b7den", "und", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Jhr sprecht ja selber: Alles sey", "tokens": ["Ihr", "sprecht", "ja", "sel\u00b7ber", ":", "Al\u00b7les", "sey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PIS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Aus ew\u2019gen Elementen kommen,", "tokens": ["Aus", "ew'\u00b7gen", "E\u00b7le\u00b7men\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Und leugnet ", "tokens": ["Und", "leug\u00b7net"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Jhr widersprecht ja GOTT, der ", "tokens": ["Ihr", "wi\u00b7der\u00b7sprecht", "ja", "GoTT", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Da ihr, o Schande! doch dem Stoff, dem Element,", "tokens": ["Da", "ihr", ",", "o", "Schan\u00b7de", "!", "doch", "dem", "Stoff", ",", "dem", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$.", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein\u2019 Ewigkeit, die blind und falsch ist, g\u00f6nnt.", "tokens": ["Ein'", "E\u00b7wig\u00b7keit", ",", "die", "blind", "und", "falsch", "ist", ",", "g\u00f6nnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}