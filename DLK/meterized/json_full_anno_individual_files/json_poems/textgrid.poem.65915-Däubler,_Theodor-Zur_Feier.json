{"textgrid.poem.65915": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zur Feier", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kythar\u00f6de singt zur Hochzeitsfeier:", "tokens": ["Ein", "Ky\u00b7tha\u00b7r\u00f6\u00b7de", "singt", "zur", "Hoch\u00b7zeits\u00b7fei\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So holder Hymen\u00e4os, Argos' Sohn,", "tokens": ["So", "hol\u00b7der", "Hy\u00b7me\u00b7n\u00e4os", ",", "Ar\u00b7gos'", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NE", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo T\u00f6chter Attikas, vor altem Thron", "tokens": ["Wo", "T\u00f6ch\u00b7ter", "At\u00b7ti\u00b7kas", ",", "vor", "al\u00b7tem", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Demeter, sich wiegten nach der Leier,", "tokens": ["Der", "De\u00b7me\u00b7ter", ",", "sich", "wieg\u00b7ten", "nach", "der", "Lei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Erschienen J\u00fcnglinge, als kecke Freier,", "tokens": ["Er\u00b7schie\u00b7nen", "J\u00fcng\u00b7lin\u00b7ge", ",", "als", "ke\u00b7cke", "Frei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA"