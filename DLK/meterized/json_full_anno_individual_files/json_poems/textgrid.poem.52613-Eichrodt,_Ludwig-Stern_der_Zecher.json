{"textgrid.poem.52613": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Stern der Zecher", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer schenket den Wein", "tokens": ["Wer", "schen\u00b7ket", "den", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Den flammenden ein?", "tokens": ["Den", "flam\u00b7men\u00b7den", "ein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer schenket den Wein, den ich trinke?", "tokens": ["Wer", "schen\u00b7ket", "den", "Wein", ",", "den", "ich", "trin\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es ist die Marianne, die flinke!", "tokens": ["Es", "ist", "die", "Ma\u00b7ri\u00b7an\u00b7ne", ",", "die", "flin\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Becher ist leer \u2013", "tokens": ["Der", "Be\u00b7cher", "ist", "leer", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wer schwebet daher", "tokens": ["Wer", "schwe\u00b7bet", "da\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit v