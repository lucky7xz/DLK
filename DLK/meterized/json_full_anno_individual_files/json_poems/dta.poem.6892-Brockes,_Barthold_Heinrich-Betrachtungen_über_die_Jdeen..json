{"dta.poem.6892": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan man ein heller Licht begehren,", "tokens": ["Kan", "man", "ein", "hel\u00b7ler", "Licht", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu kennen unsern Geist? sein Vorrecht zu erkl\u00e4ren?", "tokens": ["Zu", "ken\u00b7nen", "un\u00b7sern", "Geist", "?", "sein", "Vor\u00b7recht", "zu", "er\u00b7kl\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es zeiget jedes Ding, und unterst\u00fctzt sein Recht.", "tokens": ["Es", "zei\u00b7get", "je\u00b7des", "Ding", ",", "und", "un\u00b7ter\u00b7st\u00fctzt", "sein", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir haben ja gesehn, da\u00df selber, wenn er fehlet,", "tokens": ["Wir", "ha\u00b7ben", "ja", "ge\u00b7sehn", ",", "da\u00df", "sel\u00b7ber", ",", "wenn", "er", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erwegt man ihn allein,", "tokens": ["Er\u00b7wegt", "man", "ihn", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Hoheit und die W\u00fcrd\u2019 an ihm ausnehmend seyn:", "tokens": ["Die", "Ho\u00b7heit", "und", "die", "W\u00fcrd'", "an", "ihm", "aus\u00b7neh\u00b7mend", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df nur er nach eignem Willen wehlet,", "tokens": ["Und", "da\u00df", "nur", "er", "nach", "eig\u00b7nem", "Wil\u00b7len", "weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df er die Sinnen kan zum Guten treiben,", "tokens": ["Da\u00df", "er", "die", "Sin\u00b7nen", "kan", "zum", "Gu\u00b7ten", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und m\u00e4chtig sey Gesetz denselben vorzuschreiben.", "tokens": ["Und", "m\u00e4ch\u00b7tig", "sey", "Ge\u00b7setz", "den\u00b7sel\u00b7ben", "vor\u00b7zu\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}