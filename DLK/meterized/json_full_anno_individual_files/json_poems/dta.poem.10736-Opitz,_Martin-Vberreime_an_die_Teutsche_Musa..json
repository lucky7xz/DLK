{"dta.poem.10736": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Vberreime/ an die Teutsche Musa.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Nvn/ Teutsche Musa/ tritt herf\u00fcr/", "tokens": ["Nvn", "/", "Teut\u00b7sche", "Mu\u00b7sa", "/", "tritt", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df kecklich deine stimm erklingen/", "tokens": ["La\u00df", "keck\u00b7lich", "dei\u00b7ne", "stimm", "er\u00b7klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warumb woltestu f\u00f6rchten dir/", "tokens": ["Wa\u00b7rumb", "wol\u00b7tes\u00b7tu", "f\u00f6rch\u00b7ten", "dir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "VVFIN", "PPER", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "In deiner Mutter sprach zusingen?", "tokens": ["In", "dei\u00b7ner", "Mut\u00b7ter", "sprach", "zu\u00b7sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Meint man/ Teutschlandt sey ohne sinnen?", "tokens": ["Meint", "man", "/", "Teutschlandt", "sey", "oh\u00b7ne", "sin\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "NE", "VAFIN", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll dann der Grichen pracht/", "tokens": ["Soll", "dann", "der", "Gri\u00b7chen", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Oder die R\u00f6misch macht", "tokens": ["O\u00b7der", "die", "R\u00f6\u00b7misch", "macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Poetrei Kleinodt allein gewinnen?", "tokens": ["Der", "Poe\u00b7trei", "Klei\u00b7nodt", "al\u00b7lein", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}