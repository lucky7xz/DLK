{"textgrid.poem.28299": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auff deinen Hochzeit Tag, mein Freund, dir was zu machen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auff deinen Hochzeit Tag, mein Freund, dir was zu machen,", "tokens": ["Auff", "dei\u00b7nen", "Hoch\u00b7zeit", "Tag", ",", "mein", "Freund", ",", "dir", "was", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du mich angesucht. Ich bin zu diesen Sachen", "tokens": ["Hast", "du", "mich", "an\u00b7ge\u00b7sucht", ".", "Ich", "bin", "zu", "die\u00b7sen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "VVPP", "$.", "PPER", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So willig als verpflicht; nim du f\u00fcr lieb nur an", "tokens": ["So", "wil\u00b7lig", "als", "ver\u00b7pflicht", ";", "nim", "du", "f\u00fcr", "lieb", "nur", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "VVFIN", "$.", "VVIMP", "PPER", "APPR", "ADJD", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df, was nicht, wie es soll, ist, aber wie es kan.", "tokens": ["Di\u00df", ",", "was", "nicht", ",", "wie", "es", "soll", ",", "ist", ",", "a\u00b7ber", "wie", "es", "kan", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "$,", "VAFIN", "$,", "KON", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird dann auch die Braut, was du ihr m\u00f6chtest machen,", "tokens": ["So", "wird", "dann", "auch", "die", "Braut", ",", "was", "du", "ihr", "m\u00f6ch\u00b7test", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PWS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So gut es immer ist, belieben und belachen.", "tokens": ["So", "gut", "es", "im\u00b7mer", "ist", ",", "be\u00b7lie\u00b7ben", "und", "be\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das, was ich dir gemacht, hat F\u00fcsse nur allein;", "tokens": ["Das", ",", "was", "ich", "dir", "ge\u00b7macht", ",", "hat", "F\u00fcs\u00b7se", "nur", "al\u00b7lein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "PPER", "VVPP", "$,", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schau aber du, da\u00df dort bei F\u00fcssen H\u00e4nde seyn.", "tokens": ["Schau", "a\u00b7ber", "du", ",", "da\u00df", "dort", "bei", "F\u00fcs\u00b7sen", "H\u00e4n\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punc