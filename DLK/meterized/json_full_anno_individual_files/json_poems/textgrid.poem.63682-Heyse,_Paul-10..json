{"textgrid.poem.63682": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am liebsten hab' ich stets den Blick gelenkt", "tokens": ["Am", "liebs\u00b7ten", "hab'", "ich", "stets", "den", "Blick", "ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf weite Fl\u00e4chen, ferne Horizonte,", "tokens": ["Auf", "wei\u00b7te", "Fl\u00e4\u00b7chen", ",", "fer\u00b7ne", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da meine Jugend in der Mark sich sonnte,", "tokens": ["Da", "mei\u00b7ne", "Ju\u00b7gend", "in", "der", "Mark", "sich", "sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der die Natur kein Hochgebirg geschenkt.", "tokens": ["Der", "die", "Na\u00b7tur", "kein", "Hoch\u00b7ge\u00b7birg", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als \u00f6fter dann mir Sommers war verh\u00e4ngt,", "tokens": ["Als", "\u00f6f\u00b7ter", "dann", "mir", "Som\u00b7mers", "war", "ver\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich in einem Alpenhochtal wohnte,", "tokens": ["Da\u00df", "ich", "in", "ei\u00b7nem", "Al\u00b7pen\u00b7hoch\u00b7tal", "wohn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War mir's, als ob ich frei nicht atmen konnte,", "tokens": ["War", "mir's", ",", "als", "ob", "ich", "frei", "nicht", "at\u00b7men", "konn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von himmelhohen Schroffen rings umschr\u00e4nkt.", "tokens": ["Von", "him\u00b7mel\u00b7ho\u00b7hen", "Schrof\u00b7fen", "rings", "um\u00b7schr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun bitt' ich ab euch stolzen Alpenriesen,", "tokens": ["Nun", "bitt'", "ich", "ab", "euch", "stol\u00b7zen", "Al\u00b7pen\u00b7rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich geklagt, ihr machtet noch mich krank;", "tokens": ["Da\u00df", "ich", "ge\u00b7klagt", ",", "ihr", "mach\u00b7tet", "noch", "mich", "krank", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ADV", "PPER", "ADJD", "$."], "mete