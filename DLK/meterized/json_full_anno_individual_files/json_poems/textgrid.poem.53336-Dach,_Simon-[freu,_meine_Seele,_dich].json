{"textgrid.poem.53336": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[freu, meine Seele, dich]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freu, meine Seele, dich,", "tokens": ["Freu", ",", "mei\u00b7ne", "See\u00b7le", ",", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$,", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dein Abschied n\u00e4hert sich,", "tokens": ["Dein", "Ab\u00b7schied", "n\u00e4\u00b7hert", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Herr wird jetzund kommen,", "tokens": ["Der", "Herr", "wird", "je\u00b7tzund", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' unbesorgten Wahn,", "tokens": ["Hab'", "un\u00b7be\u00b7sorg\u00b7ten", "Wahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist im huy gethan", "tokens": ["Es", "ist", "im", "huy", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NE", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So bist du hingenommen.", "tokens": ["So", "bist", "du", "hin\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie ich umb Abends-Zeit", "tokens": ["Wie", "ich", "umb", "A\u00b7bends\u00b7Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich leg' auff eine Seit,", "tokens": ["Mich", "leg'", "auff", "ei\u00b7ne", "Seit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd gantz nicht kan besinnen,", "tokens": ["Vnd", "gantz", "nicht", "kan", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn mich der Schlaff bef\u00e4llt,", "tokens": ["Wenn", "mich", "der", "Schlaff", "be\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht anders schickt die Welt", "tokens": ["Nicht", "an\u00b7ders", "schickt", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vns durch den Tod von hinnen.", "tokens": ["Vns", "durch", "den", "Tod", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So ist des Glaubens Grund,", "tokens": ["So", "ist", "des", "Glau\u00b7bens", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein h\u00f6chster Trost, dir kunt,", "tokens": ["Dein", "h\u00f6chs\u00b7ter", "Trost", ",", "dir", "kunt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Christus zwar gestorben,", "tokens": ["Da\u00df", "Chris\u00b7tus", "zwar", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch aufferstanden sey,", "tokens": ["Doch", "auf\u00b7fer\u00b7stan\u00b7den", "sey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd uns durch solche Treu", "tokens": ["Vnd", "uns", "durch", "sol\u00b7che", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Leben hab' erworben.", "tokens": ["Das", "Le\u00b7ben", "hab'", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Halt durch des Geistes Arm,", "tokens": ["Halt", "durch", "des", "Geis\u00b7tes", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Weil dir zum Hertzen warm,", "tokens": ["Weil", "dir", "zum", "Hert\u00b7zen", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denselben f\u00e4st umbschlossen,", "tokens": ["Den\u00b7sel\u00b7ben", "f\u00e4st", "umbsc\u00b7hlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So f\u00e4hrst du warlich hin", "tokens": ["So", "f\u00e4hrst", "du", "war\u00b7lich", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als h\u00e4tte deinen Sinn", "tokens": ["Als", "h\u00e4t\u00b7te", "dei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein sanffter Schlaff begossen.", "tokens": ["Ein", "sanff\u00b7ter", "Schlaff", "be\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df aber Fleisch und Haut", "tokens": ["Da\u00df", "a\u00b7ber", "Fleisch", "und", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Stracks die Verwesung schaut,", "tokens": ["Stracks", "die", "Ver\u00b7we\u00b7sung", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll dieses dich bewegen?", "tokens": ["Soll", "die\u00b7ses", "dich", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott wird ihm das Gebein", "tokens": ["Gott", "wird", "ihm", "das", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Befohlen lassen seyn", "tokens": ["Be\u00b7foh\u00b7len", "las\u00b7sen", "seyn"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd deiner Asche pflegen.", "tokens": ["Vnd", "dei\u00b7ner", "A\u00b7sche", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was von dem Himmel r\u00fchrt", "tokens": ["Was", "von", "dem", "Him\u00b7mel", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird Himmel ein gef\u00fchrt,", "tokens": ["Wird", "Him\u00b7mel", "ein", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wirst du, Seele, schweben", "tokens": ["Da", "wirst", "du", ",", "See\u00b7le", ",", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Glantz und Herrligkeit", "tokens": ["In", "Glantz", "und", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd aller Noht befreyt", "tokens": ["Vnd", "al\u00b7ler", "Noht", "be\u00b7freyt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Stets bey dem Herren leben.", "tokens": ["Stets", "bey", "dem", "Her\u00b7ren", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So steh' in deiner Zier,", "tokens": ["So", "steh'", "in", "dei\u00b7ner", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Mitternacht ist hier,", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Br\u00e4utgam k\u00f6mpt gegangen,", "tokens": ["Dein", "Br\u00e4ut\u00b7gam", "k\u00f6mpt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd klopfft auch, ist Er da?", "tokens": ["Vnd", "klopfft", "auch", ",", "ist", "Er", "da", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bist du es, Jesv? Ja!", "tokens": ["Bist", "du", "es", ",", "Jesv", "?", "Ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NE", "$.", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ey komm, du mein Verlangen.", "tokens": ["Ey", "komm", ",", "du", "mein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Erde, gute Nacht,", "tokens": ["O", "Er\u00b7de", ",", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein' h\u00f6chste Lust und Pracht", "tokens": ["Dein'", "h\u00f6chs\u00b7te", "Lust", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist doch versaltzt mit Leiden,", "tokens": ["Ist", "doch", "ver\u00b7saltzt", "mit", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich ende meinen Lauff,", "tokens": ["Ich", "en\u00b7de", "mei\u00b7nen", "Lauff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Heyland nimpt mich auff", "tokens": ["Mein", "Hey\u00b7land", "nimpt", "mich", "auff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In seine Himmels-Freuden.", "tokens": ["In", "sei\u00b7ne", "Him\u00b7mels\u00b7Freu\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Freu, meine Seele, dich,", "tokens": ["Freu", ",", "mei\u00b7ne", "See\u00b7le", ",", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$,", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dein Abschied n\u00e4hert sich,", "tokens": ["Dein", "Ab\u00b7schied", "n\u00e4\u00b7hert", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Herr wird jetzund kommen,", "tokens": ["Der", "Herr", "wird", "je\u00b7tzund", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' unbesorgten Wahn,", "tokens": ["Hab'", "un\u00b7be\u00b7sorg\u00b7ten", "Wahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist im huy gethan", "tokens": ["Es", "ist", "im", "huy", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NE", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So bist du hingenommen.", "tokens": ["So", "bist", "du", "hin\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie ich umb Abends-Zeit", "tokens": ["Wie", "ich", "umb", "A\u00b7bends\u00b7Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich leg' auff eine Seit,", "tokens": ["Mich", "leg'", "auff", "ei\u00b7ne", "Seit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd gantz nicht kan besinnen,", "tokens": ["Vnd", "gantz", "nicht", "kan", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn mich der Schlaff bef\u00e4llt,", "tokens": ["Wenn", "mich", "der", "Schlaff", "be\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht anders schickt die Welt", "tokens": ["Nicht", "an\u00b7ders", "schickt", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vns durch den Tod von hinnen.", "tokens": ["Vns", "durch", "den", "Tod", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So ist des Glaubens Grund,", "tokens": ["So", "ist", "des", "Glau\u00b7bens", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein h\u00f6chster Trost, dir kunt,", "tokens": ["Dein", "h\u00f6chs\u00b7ter", "Trost", ",", "dir", "kunt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Christus zwar gestorben,", "tokens": ["Da\u00df", "Chris\u00b7tus", "zwar", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch aufferstanden sey,", "tokens": ["Doch", "auf\u00b7fer\u00b7stan\u00b7den", "sey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd uns durch solche Treu", "tokens": ["Vnd", "uns", "durch", "sol\u00b7che", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Leben hab' erworben.", "tokens": ["Das", "Le\u00b7ben", "hab'", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Halt durch des Geistes Arm,", "tokens": ["Halt", "durch", "des", "Geis\u00b7tes", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Weil dir zum Hertzen warm,", "tokens": ["Weil", "dir", "zum", "Hert\u00b7zen", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denselben f\u00e4st umbschlossen,", "tokens": ["Den\u00b7sel\u00b7ben", "f\u00e4st", "umbsc\u00b7hlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So f\u00e4hrst du warlich hin", "tokens": ["So", "f\u00e4hrst", "du", "war\u00b7lich", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als h\u00e4tte deinen Sinn", "tokens": ["Als", "h\u00e4t\u00b7te", "dei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein sanffter Schlaff begossen.", "tokens": ["Ein", "sanff\u00b7ter", "Schlaff", "be\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da\u00df aber Fleisch und Haut", "tokens": ["Da\u00df", "a\u00b7ber", "Fleisch", "und", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Stracks die Verwesung schaut,", "tokens": ["Stracks", "die", "Ver\u00b7we\u00b7sung", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll dieses dich bewegen?", "tokens": ["Soll", "die\u00b7ses", "dich", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott wird ihm das Gebein", "tokens": ["Gott", "wird", "ihm", "das", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Befohlen lassen seyn", "tokens": ["Be\u00b7foh\u00b7len", "las\u00b7sen", "seyn"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd deiner Asche pflegen.", "tokens": ["Vnd", "dei\u00b7ner", "A\u00b7sche", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Was von dem Himmel r\u00fchrt", "tokens": ["Was", "von", "dem", "Him\u00b7mel", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wird Himmel ein gef\u00fchrt,", "tokens": ["Wird", "Him\u00b7mel", "ein", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wirst du, Seele, schweben", "tokens": ["Da", "wirst", "du", ",", "See\u00b7le", ",", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Glantz und Herrligkeit", "tokens": ["In", "Glantz", "und", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd aller Noht befreyt", "tokens": ["Vnd", "al\u00b7ler", "Noht", "be\u00b7freyt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Stets bey dem Herren leben.", "tokens": ["Stets", "bey", "dem", "Her\u00b7ren", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "So steh' in deiner Zier,", "tokens": ["So", "steh'", "in", "dei\u00b7ner", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Mitternacht ist hier,", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Br\u00e4utgam k\u00f6mpt gegangen,", "tokens": ["Dein", "Br\u00e4ut\u00b7gam", "k\u00f6mpt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd klopfft auch, ist Er da?", "tokens": ["Vnd", "klopfft", "auch", ",", "ist", "Er", "da", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bist du es, Jesv? Ja!", "tokens": ["Bist", "du", "es", ",", "Jesv", "?", "Ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NE", "$.", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ey komm, du mein Verlangen.", "tokens": ["Ey", "komm", ",", "du", "mein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O Erde, gute Nacht,", "tokens": ["O", "Er\u00b7de", ",", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein' h\u00f6chste Lust und Pracht", "tokens": ["Dein'", "h\u00f6chs\u00b7te", "Lust", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist doch versaltzt mit Leiden,", "tokens": ["Ist", "doch", "ver\u00b7saltzt", "mit", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich ende meinen Lauff,", "tokens": ["Ich", "en\u00b7de", "mei\u00b7nen", "Lauff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Heyland nimpt mich auff", "tokens": ["Mein", "Hey\u00b7land", "nimpt", "mich", "auff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In seine Himmels-Freuden.", "tokens": ["In", "sei\u00b7ne", "Him\u00b7mels\u00b7Freu\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}