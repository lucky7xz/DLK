tetra"}, "line.4": {"text": "Der Verzug bringt hier Gefahr.", "tokens": ["Der", "Ver\u00b7zug", "bringt", "hier", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Befehl soll stets mein Wille", "tokens": ["Dein", "Be\u00b7fehl", "soll", "stets", "mein", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dein Winck mein Leitstern seyn,", "tokens": ["Und", "dein", "Win\u00b7ck", "mein", "Leits\u00b7tern", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Schencke mir nur in der Stille", "tokens": ["Schen\u00b7cke", "mir", "nur", "in", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Deiner Liebe Vorschmack ein!", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", "Vor\u00b7schmack", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Mein Gebl\u00fcte f\u00fchlt den Zunder,", "tokens": ["Mein", "Ge\u00b7bl\u00fc\u00b7te", "f\u00fchlt", "den", "Zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der von deiner Tugend f\u00e4ngt,", "tokens": ["Der", "von", "dei\u00b7ner", "Tu\u00b7gend", "f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also nimmt es mich nicht Wunder,", "tokens": ["Al\u00b7so", "nimmt", "es", "mich", "nicht", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mein Geist an deinem h\u00e4ngt.", "tokens": ["Wenn", "mein", "Geist", "an", "dei\u00b7nem", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mercke nur des Himmels Schl\u00fc\u00dfe,", "tokens": ["Mer\u00b7cke", "nur", "des", "Him\u00b7mels", "Schl\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonst erf\u00e4hrstu von der Reu,", "tokens": ["Sonst", "er\u00b7f\u00e4hrs\u00b7tu", "von", "der", "Reu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df das Honig fremder K\u00fc\u00dfe", "tokens": ["Da\u00df", "das", "Ho\u00b7nig", "frem\u00b7der", "K\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um das Ende bitter sey.", "tokens": ["Um", "das", "En\u00b7de", "bit\u00b7ter", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ach erwege mein Begehren!", "tokens": ["Ach", "er\u00b7we\u00b7ge", "mein", "Be\u00b7geh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Verstand ist scharf genug;", "tokens": ["Dein", "Ver\u00b7stand", "ist", "scharf", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Las mich nicht die Glut verzehren,", "tokens": ["Las", "mich", "nicht", "die", "Glut", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonst wird dich der Todtenkrug", "tokens": ["Sonst", "wird", "dich", "der", "Tod\u00b7ten\u00b7krug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Meiner Asche noch verklagen", "tokens": ["Mei\u00b7ner", "A\u00b7sche", "noch", "ver\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein kalter Leichenstein", "tokens": ["Und", "mein", "kal\u00b7ter", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir so viel zur Nachricht sagen:", "tokens": ["Dir", "so", "viel", "zur", "Nach\u00b7richt", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lerne doch bedachtsam seyn!", "tokens": ["Ler\u00b7ne", "doch", "be\u00b7dacht\u00b7sam", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}