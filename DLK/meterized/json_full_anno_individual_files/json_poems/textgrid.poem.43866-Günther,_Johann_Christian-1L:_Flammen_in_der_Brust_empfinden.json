{"textgrid.poem.43866": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flammen in der Brust empfinden", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flammen in der Brust empfinden", "tokens": ["Flam\u00b7men", "in", "der", "Brust", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dabey nicht Feuer schreyn,", "tokens": ["Und", "da\u00b7bey", "nicht", "Feu\u00b7er", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKNEG", "NN", "VAINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Heist die Ruthen gr\u00f6\u00dfer binden", "tokens": ["Heist", "die", "Ru\u00b7then", "gr\u00f6\u00b7\u00dfer", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein eigner Hencker seyn.", "tokens": ["Und", "sein", "eig\u00b7ner", "Hen\u00b7cker", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Verheelung der Gedancken", "tokens": ["Die", "Ver\u00b7hee\u00b7lung", "der", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Labet keinen d\u00fcrren Mund,", "tokens": ["La\u00b7bet", "kei\u00b7nen", "d\u00fcr\u00b7ren", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Scham verliebter Krancken", "tokens": ["Und", "die", "Scham", "ver\u00b7lieb\u00b7ter", "Kran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Macht das Herze sp\u00e4t gesund.", "tokens": ["Macht", "das", "Her\u00b7ze", "sp\u00e4t", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VVFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum wohlan, mein Geist, entdecke", "tokens": ["Drum", "wo\u00b7hlan", ",", "mein", "Geist", ",", "ent\u00b7de\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dies, was deine Sehnsucht qu\u00e4lt;", "tokens": ["Dies", ",", "was", "dei\u00b7ne", "Sehn\u00b7sucht", "qu\u00e4lt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frisch gewagt kommt bald zum Zwecke,", "tokens": ["Frisch", "ge\u00b7wagt", "kommt", "bald", "zum", "Zwe\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den die Furchtsamkeit verfehlt.", "tokens": ["Den", "die", "Furcht\u00b7sam\u00b7keit", "ver\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, mein Herz, ach schweig und glaube,", "tokens": ["Nein", ",", "mein", "Herz", ",", "ach", "schweig", "und", "glau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein Entdecken hilft dich nicht,", "tokens": ["Dein", "Ent\u00b7de\u00b7cken", "hilft", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil bereits die sch\u00f6ne Taube", "tokens": ["Weil", "be\u00b7reits", "die", "sch\u00f6\u00b7ne", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einem andern sich verspricht.", "tokens": ["Ei\u00b7nem", "an\u00b7dern", "sich", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schweig, mein Herz, und halt die Plagen", "tokens": ["Schweig", ",", "mein", "Herz", ",", "und", "halt", "die", "Pla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Leidenschaft geheim,", "tokens": ["Dei\u00b7ner", "Lei\u00b7den\u00b7schaft", "ge\u00b7heim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerne dein Verh\u00e4ngn\u00fc\u00df tragen,", "tokens": ["Ler\u00b7ne", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Koch aus Wermuth Honigseim!", "tokens": ["Koch", "aus", "Wer\u00b7muth", "Ho\u00b7ni\u00b7gseim", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat die Schickung deinem Fieber", "tokens": ["Hat", "die", "Schi\u00b7ckung", "dei\u00b7nem", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen sch\u00f6nen Arzt versagt,", "tokens": ["Die\u00b7sen", "sch\u00f6\u00b7nen", "Arzt", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ey, so stirb doch zehnmahl lieber,", "tokens": ["Ey", ",", "so", "stirb", "doch", "zehn\u00b7mahl", "lie\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh dein Mund die K\u00fchnheit wagt.", "tokens": ["Eh", "dein", "Mund", "die", "K\u00fchn\u00b7heit", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch getrost, mein Herz, und wage", "tokens": ["Doch", "ge\u00b7trost", ",", "mein", "Herz", ",", "und", "wa\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVPP", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch den allerlezten Streich!", "tokens": ["Noch", "den", "al\u00b7ler\u00b7lez\u00b7ten", "Streich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch getrost! Versuch und schlage", "tokens": ["Doch", "ge\u00b7trost", "!", "Ver\u00b7such", "und", "schla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Felsen durch die Thr\u00e4nen weich!", "tokens": ["Fel\u00b7sen", "durch", "die", "Thr\u00e4\u00b7nen", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kluge Sch\u00f6nheit, meine Funcken", "tokens": ["Klu\u00b7ge", "Sch\u00f6n\u00b7heit", ",", "mei\u00b7ne", "Fun\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcberreicht dir dieses Blat,", "tokens": ["\u00dc\u00b7berr\u00b7eicht", "dir", "die\u00b7ses", "Blat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das mehr na\u00dfes Salz getruncken,", "tokens": ["Das", "mehr", "na\u00b7\u00dfes", "Salz", "ge\u00b7trun\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als dein Mund jezt Zucker hat.", "tokens": ["Als", "dein", "Mund", "jezt", "Zu\u00b7cker", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Z\u00fcrne nicht mit meiner Liebe,", "tokens": ["Z\u00fcr\u00b7ne", "nicht", "mit", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die die Redligkeit gebahr,", "tokens": ["Die", "die", "Red\u00b7lig\u00b7keit", "ge\u00b7bahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4rcke bald die reinen Triebe,", "tokens": ["St\u00e4r\u00b7cke", "bald", "die", "rei\u00b7nen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Verzug bringt hier Gefahr.", "tokens": ["Der", "Ver\u00b7zug", "bringt", "hier", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Befehl soll stets mein Wille", "tokens": ["Dein", "Be\u00b7fehl", "soll", "stets", "mein", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dein Winck mein Leitstern seyn,", "tokens": ["Und", "dein", "Win\u00b7ck", "mein", "Leits\u00b7tern", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Schencke mir nur in der Stille", "tokens": ["Schen\u00b7cke", "mir", "nur", "in", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Deiner Liebe Vorschmack ein!", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", "Vor\u00b7schmack", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Gebl\u00fcte f\u00fchlt den Zunder,", "tokens": ["Mein", "Ge\u00b7bl\u00fc\u00b7te", "f\u00fchlt", "den", "Zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der von deiner Tugend f\u00e4ngt,", "tokens": ["Der", "von", "dei\u00b7ner", "Tu\u00b7gend", "f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also nimmt es mich nicht Wunder,", "tokens": ["Al\u00b7so", "nimmt", "es", "mich", "nicht", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mein Geist an deinem h\u00e4ngt.", "tokens": ["Wenn", "mein", "Geist", "an", "dei\u00b7nem", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mercke nur des Himmels Schl\u00fc\u00dfe,", "tokens": ["Mer\u00b7cke", "nur", "des", "Him\u00b7mels", "Schl\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonst erf\u00e4hrstu von der Reu,", "tokens": ["Sonst", "er\u00b7f\u00e4hrs\u00b7tu", "von", "der", "Reu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df das Honig fremder K\u00fc\u00dfe", "tokens": ["Da\u00df", "das", "Ho\u00b7nig", "frem\u00b7der", "K\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um das Ende bitter sey.", "tokens": ["Um", "das", "En\u00b7de", "bit\u00b7ter", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach erwege mein Begehren!", "tokens": ["Ach", "er\u00b7we\u00b7ge", "mein", "Be\u00b7geh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein Verstand ist scharf genug;", "tokens": ["Dein", "Ver\u00b7stand", "ist", "scharf", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Las mich nicht die Glut verzehren,", "tokens": ["Las", "mich", "nicht", "die", "Glut", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonst wird dich der Todtenkrug", "tokens": ["Sonst", "wird", "dich", "der", "Tod\u00b7ten\u00b7krug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Meiner Asche noch verklagen", "tokens": ["Mei\u00b7ner", "A\u00b7sche", "noch", "ver\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein kalter Leichenstein", "tokens": ["Und", "mein", "kal\u00b7ter", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir so viel zur Nachricht sagen:", "tokens": ["Dir", "so", "viel", "zur", "Nach\u00b7richt", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lerne doch bedachtsam seyn!", "tokens": ["Ler\u00b7ne", "doch", "be\u00b7dacht\u00b7sam", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Flammen in der Brust empfinden", "tokens": ["Flam\u00b7men", "in", "der", "Brust", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dabey nicht Feuer schreyn,", "tokens": ["Und", "da\u00b7bey", "nicht", "Feu\u00b7er", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKNEG", "NN", "VAINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Heist die Ruthen gr\u00f6\u00dfer binden", "tokens": ["Heist", "die", "Ru\u00b7then", "gr\u00f6\u00b7\u00dfer", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein eigner Hencker seyn.", "tokens": ["Und", "sein", "eig\u00b7ner", "Hen\u00b7cker", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Verheelung der Gedancken", "tokens": ["Die", "Ver\u00b7hee\u00b7lung", "der", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Labet keinen d\u00fcrren Mund,", "tokens": ["La\u00b7bet", "kei\u00b7nen", "d\u00fcr\u00b7ren", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Scham verliebter Krancken", "tokens": ["Und", "die", "Scham", "ver\u00b7lieb\u00b7ter", "Kran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Macht das Herze sp\u00e4t gesund.", "tokens": ["Macht", "das", "Her\u00b7ze", "sp\u00e4t", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VVFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Drum wohlan, mein Geist, entdecke", "tokens": ["Drum", "wo\u00b7hlan", ",", "mein", "Geist", ",", "ent\u00b7de\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dies, was deine Sehnsucht qu\u00e4lt;", "tokens": ["Dies", ",", "was", "dei\u00b7ne", "Sehn\u00b7sucht", "qu\u00e4lt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frisch gewagt kommt bald zum Zwecke,", "tokens": ["Frisch", "ge\u00b7wagt", "kommt", "bald", "zum", "Zwe\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den die Furchtsamkeit verfehlt.", "tokens": ["Den", "die", "Furcht\u00b7sam\u00b7keit", "ver\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, mein Herz, ach schweig und glaube,", "tokens": ["Nein", ",", "mein", "Herz", ",", "ach", "schweig", "und", "glau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein Entdecken hilft dich nicht,", "tokens": ["Dein", "Ent\u00b7de\u00b7cken", "hilft", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil bereits die sch\u00f6ne Taube", "tokens": ["Weil", "be\u00b7reits", "die", "sch\u00f6\u00b7ne", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einem andern sich verspricht.", "tokens": ["Ei\u00b7nem", "an\u00b7dern", "sich", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schweig, mein Herz, und halt die Plagen", "tokens": ["Schweig", ",", "mein", "Herz", ",", "und", "halt", "die", "Pla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Leidenschaft geheim,", "tokens": ["Dei\u00b7ner", "Lei\u00b7den\u00b7schaft", "ge\u00b7heim", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerne dein Verh\u00e4ngn\u00fc\u00df tragen,", "tokens": ["Ler\u00b7ne", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Koch aus Wermuth Honigseim!", "tokens": ["Koch", "aus", "Wer\u00b7muth", "Ho\u00b7ni\u00b7gseim", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat die Schickung deinem Fieber", "tokens": ["Hat", "die", "Schi\u00b7ckung", "dei\u00b7nem", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen sch\u00f6nen Arzt versagt,", "tokens": ["Die\u00b7sen", "sch\u00f6\u00b7nen", "Arzt", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ey, so stirb doch zehnmahl lieber,", "tokens": ["Ey", ",", "so", "stirb", "doch", "zehn\u00b7mahl", "lie\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh dein Mund die K\u00fchnheit wagt.", "tokens": ["Eh", "dein", "Mund", "die", "K\u00fchn\u00b7heit", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch getrost, mein Herz, und wage", "tokens": ["Doch", "ge\u00b7trost", ",", "mein", "Herz", ",", "und", "wa\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVPP", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch den allerlezten Streich!", "tokens": ["Noch", "den", "al\u00b7ler\u00b7lez\u00b7ten", "Streich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch getrost! Versuch und schlage", "tokens": ["Doch", "ge\u00b7trost", "!", "Ver\u00b7such", "und", "schla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Felsen durch die Thr\u00e4nen weich!", "tokens": ["Fel\u00b7sen", "durch", "die", "Thr\u00e4\u00b7nen", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kluge Sch\u00f6nheit, meine Funcken", "tokens": ["Klu\u00b7ge", "Sch\u00f6n\u00b7heit", 