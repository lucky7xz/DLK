{"textgrid.poem.41520": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am vierten Sonntage nach hl. drei K\u00f6nige", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann nicht sagen:", "tokens": ["Ich", "kann", "nicht", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u00bbkeiner hat mich gedingt.\u00ab", "tokens": ["\u00bb", "kei\u00b7ner", "hat", "mich", "ge\u00b7dingt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wem soll ich klagen,", "tokens": ["Wem", "soll", "ich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wenn es mich niederzwingt", "tokens": ["Wenn", "es", "mich", "nie\u00b7der\u00b7zwingt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In meine schm\u00e4hlich selbstgeflochtnen Bande?", "tokens": ["In", "mei\u00b7ne", "schm\u00e4h\u00b7lich", "selbst\u00b7ge\u00b7flocht\u00b7nen", "Ban\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vor Millionen hast du mich erw\u00e4hlt,", "tokens": ["Vor", "Mil\u00b7lion\u00b7en", "hast", "du", "mich", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Mir unerme\u00dfnes Handgeld zugez\u00e4hlt", "tokens": ["Mir", "un\u00b7er\u00b7me\u00df\u00b7nes", "Hand\u00b7geld", "zu\u00b7ge\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In deiner Taufe heil'gem Unterpfande.", "tokens": ["In", "dei\u00b7ner", "Tau\u00b7fe", "heil'\u00b7gem", "Un\u00b7ter\u00b7pfan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich kann nicht sagen:", "tokens": ["Ich", "kann", "nicht", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u00bbsiehe, des Tages Last", "tokens": ["\u00bb", "sie\u00b7he", ",", "des", "Ta\u00b7ges", "Last"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab ich getragen!\u00ab", "tokens": ["Hab", "ich", "ge\u00b7tra\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wenn nun zu Duft erbla\u00dft,", "tokens": ["Wenn", "nun", "zu", "Duft", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich meine matte Sonne will verlassen,", "tokens": ["Mich", "mei\u00b7ne", "mat\u00b7te", "Son\u00b7ne", "will", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Garten liegt ein \u00fcbergr\u00fcntes Moor,", "tokens": ["Mein", "Gar\u00b7ten", "liegt", "ein", "\u00fc\u00b7berg\u00b7r\u00fcn\u00b7tes", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und blendend steigt das Irrlicht draus empor,", "tokens": ["Und", "blen\u00b7dend", "steigt", "das", "Irr\u00b7licht", "draus", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Wandrer leitend in den Tod, den nassen!", "tokens": ["Den", "Wand\u00b7rer", "lei\u00b7tend", "in", "den", "Tod", ",", "den", "nas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich kann nicht sagen,", "tokens": ["Ich", "kann", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u00bbsiehe, wer stand mir bei?", "tokens": ["\u00bb", "sie\u00b7he", ",", "wer", "stand", "mir", "bei", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00dfte zagen,", "tokens": ["Ich", "mu\u00df\u00b7te", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Um mich die W\u00fcstenei,", "tokens": ["Um", "mich", "die", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und das Getier, so nimmer dich erkennet.\u00ab", "tokens": ["Und", "das", "Ge\u00b7tier", ",", "so", "nim\u00b7mer", "dich", "er\u00b7ken\u00b7net", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O Gott, du hast, zur Arbeit mir gesellt,", "tokens": ["O", "Gott", ",", "du", "hast", ",", "zur", "Ar\u00b7beit", "mir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "$,", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Viel liebe Seelen rings um mich gestellt,", "tokens": ["Viel", "lie\u00b7be", "See\u00b7len", "rings", "um", "mich", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Worin dein Name unausl\u00f6schlich brennet.", "tokens": ["Wo\u00b7rin", "dein", "Na\u00b7me", "un\u00b7aus\u00b7l\u00f6schlich", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich kann nicht sagen:", "tokens": ["Ich", "kann", "nicht", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u00bbsieh, deine Stimme sprach,", "tokens": ["\u00bb", "sieh", ",", "dei\u00b7ne", "Stim\u00b7me", "sprach", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00dfte wagen,", "tokens": ["Ich", "mu\u00df\u00b7te", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und meine Kraft zerbrach,", "tokens": ["Und", "mei\u00b7ne", "Kraft", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was hast du meine Nahrung mir entzogen?\u00ab", "tokens": ["Was", "hast", "du", "mei\u00b7ne", "Nah\u00b7rung", "mir", "ent\u00b7zo\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Gott, und liegt wohl tief es in der Brust,", "tokens": ["Mein", "Gott", ",", "und", "liegt", "wohl", "tief", "es", "in", "der", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch bin ich gro\u00dfer Kr\u00e4fte mich bewu\u00dft,", "tokens": ["Doch", "bin", "ich", "gro\u00b7\u00dfer", "Kr\u00e4f\u00b7te", "mich", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und in der Angst hab' ich mir selbst gelogen!", "tokens": ["Und", "in", "der", "Angst", "hab'", "ich", "mir", "selbst", "ge\u00b7lo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich mu\u00df verschwinden", "tokens": ["Ich", "mu\u00df", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bis in die tiefste Kluft,", "tokens": ["Bis", "in", "die", "tiefs\u00b7te", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zergehn in Winden", "tokens": ["Zer\u00b7gehn", "in", "Win\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie einer Wolke Duft,", "tokens": ["Wie", "ei\u00b7ner", "Wol\u00b7ke", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn dein Gericht vor meinem Geist wird stehen;", "tokens": ["Wenn", "dein", "Ge\u00b7richt", "vor", "mei\u00b7nem", "Geist", "wird", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du hast mich \u00fcber vieles eingesetzt,", "tokens": ["Du", "hast", "mich", "\u00fc\u00b7ber", "vie\u00b7les", "ein\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ganz verarmt erschein' ich und zerfetzt,", "tokens": ["Und", "ganz", "ver\u00b7armt", "er\u00b7schein'", "ich", "und", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "PPER", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die G\u00fcter dein lie\u00df ich zu Kot vergehen.", "tokens": ["Die", "G\u00fc\u00b7ter", "dein", "lie\u00df", "ich", "zu", "Kot", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVFIN", "PPER", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nichts kann ich sagen,", "tokens": ["Nichts", "kann", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Denn meine Hand ist leer.", "tokens": ["Denn", "mei\u00b7ne", "Hand", "ist", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll ich es wagen,", "tokens": ["Soll", "ich", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gegen die Waagschal' schwer", "tokens": ["Ge\u00b7gen", "die", "Waag\u00b7schal'", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Zu legen meiner Reue sp\u00e4te Triebe?", "tokens": ["Zu", "le\u00b7gen", "mei\u00b7ner", "Reu\u00b7e", "sp\u00e4\u00b7te", "Trie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ist es nur wie des Ersatzes Spott,", "tokens": ["Und", "ist", "es", "nur", "wie", "des", "Er\u00b7sat\u00b7zes", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nichts hab' ich sonst, doch du, o milder Gott,", "tokens": ["Nichts", "hab'", "ich", "sonst", ",", "doch", "du", ",", "o", "mil\u00b7der", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "$,", "KON", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du hast ein gro\u00dfes, gro\u00dfes Wort der Liebe!", "tokens": ["Du", "hast", "ein", "gro\u00b7\u00dfes", ",", "gro\u00b7\u00dfes", "Wort", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich kann nicht sagen:", "tokens": ["Ich", "kann", "nicht", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter"