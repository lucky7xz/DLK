{"textgrid.poem.59221": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Waldv\u00f6gel", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Konnt mich auch sonst mitschwingen", "tokens": ["Konnt", "mich", "auch", "sonst", "mit\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u00dcbers gr\u00fcne Revier,", "tokens": ["\u00dc\u00b7bers", "gr\u00fc\u00b7ne", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hatt ein Herze zum Singen", "tokens": ["Hatt", "ein", "Her\u00b7ze", "zum", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und Fl\u00fcgel wie ihr.", "tokens": ["Und", "Fl\u00fc\u00b7gel", "wie", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KOKOM", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Flog \u00fcber die Felder,", "tokens": ["Flog", "\u00fc\u00b7ber", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da bl\u00fcht' es wie Schnee,", "tokens": ["Da", "bl\u00fcht'", "es", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und herauf durch die W\u00e4lder", "tokens": ["Und", "her\u00b7auf", "durch", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spiegelt' die See.", "tokens": ["Spie\u00b7gelt'", "die", "See", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ein Schiff sah ich gehen", "tokens": ["Ein", "Schiff", "sah", "ich", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fort \u00fcber das Meer,", "tokens": ["Fort", "\u00fc\u00b7ber", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Meinen Liebsten drin stehen \u2013", "tokens": ["Mei\u00b7nen", "Liebs\u00b7ten", "drin", "ste\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dacht meiner nicht mehr.", "tokens": ["Dacht", "mei\u00b7ner", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "PTKNEG", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und die Segel verzogen,", "tokens": ["Und", "die", "Se\u00b7gel", "ver\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und es d\u00e4mmert' das Feld,", "tokens": ["Und", "es", "d\u00e4m\u00b7mert'", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VV