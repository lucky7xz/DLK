{"dta.poem.11025": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Alles geliehenes Gut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! uns geh\u00f6ret von der Erden,", "tokens": ["Ach", "!", "uns", "ge\u00b7h\u00f6\u00b7ret", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf welcher du uns lassen werden,", "tokens": ["Auf", "wel\u00b7cher", "du", "uns", "las\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts. Alles ist und bleibet dein.", "tokens": ["Nichts", ".", "Al\u00b7les", "ist", "und", "blei\u00b7bet", "dein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "PIS", "VAFIN", "KON", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach la\u00df denn alles, deinen Willen", "tokens": ["Ach", "la\u00df", "denn", "al\u00b7les", ",", "dei\u00b7nen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "VVIMP", "ADV", "PIS", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als gute Werkzeug zu erf\u00fcllen,", "tokens": ["Als", "gu\u00b7te", "Werk\u00b7zeug", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Herr, von uns gebrauchet seyn!", "tokens": ["O", "Herr", ",", "von", "uns", "ge\u00b7brau\u00b7chet", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}