{"textgrid.poem.26343": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein heiliger Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich danke dir, du edler Abendgeist.", "tokens": ["Ich", "dan\u00b7ke", "dir", ",", "du", "ed\u00b7ler", "A\u00b7bend\u00b7geist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein letztes Licht mich heute gl\u00fccklich preist.", "tokens": ["Dein", "letz\u00b7tes", "Licht", "mich", "heu\u00b7te", "gl\u00fcck\u00b7lich", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein heiliger Gru\u00df kam mir im Tag gereist", "tokens": ["Ein", "hei\u00b7li\u00b7ger", "Gru\u00df", "kam", "mir", "im", "Tag", "ge\u00b7reist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von ihr, die ferne, s\u00fc\u00dfe Liebe hei\u00dft.", "tokens": ["Von", "ihr", ",", "die", "fer\u00b7ne", ",", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", "hei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "ADV", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein dunkelblauer Berg im Westen schwebt.", "tokens": ["Ein", "dun\u00b7kel\u00b7blau\u00b7er", "Berg", "im", "Wes\u00b7ten", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Breit in die Ewigkeit er sich erhebt", "tokens": ["Breit", "in", "die", "E\u00b7wig\u00b7keit", "er", "sich", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Abendfunken, der im \u00c4ther bebt.", "tokens": ["Zum", "A\u00b7bend\u00b7fun\u00b7ken", ",", "der", "im", "\u00c4\u00b7ther", "bebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Berg ist wie die Brust, die sehnend lebt.", "tokens": ["Der", "Berg", "ist", "wie", "die", "Brust", ",", "die", "seh\u00b7nend", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Fenster lehne ich und danke dir,", "tokens": ["Am", "Fens\u00b7ter", "leh\u00b7ne", "ich", "und", "dan\u00b7ke", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Geist kam segnend heute her zu mir,", "tokens": ["Dein", "Geist", "kam", "seg\u00b7nend", "heu\u00b7te", "her", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADV", "ADV", "APPR", "