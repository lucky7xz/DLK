{"dta.poem.21131": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Mitleid.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer hat ein reizender Gesicht,", "tokens": ["Wer", "hat", "ein", "rei\u00b7zen\u00b7der", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Jungfer Marjonette?", "tokens": ["Als", "Jung\u00b7fer", "Mar\u00b7jo\u00b7net\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein wer h\u00f6rt wohl, da\u00df sie spricht,", "tokens": ["Al\u00b7lein", "wer", "h\u00f6rt", "wohl", ",", "da\u00df", "sie", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie man vermuthet h\u00e4tte?", "tokens": ["Wie", "man", "ver\u00b7mu\u00b7thet", "h\u00e4t\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie neigt sich artig, und steht da,", "tokens": ["Sie", "neigt", "sich", "ar\u00b7tig", ",", "und", "steht", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und sagt aufs h\u00f6chste: Was? und Ja.", "tokens": ["Und", "sagt", "aufs", "h\u00f6chs\u00b7te", ":", "Was", "?", "und", "Ja", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "$.", "PWS", "$.", "KON", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach! sie ist noch Monade!", "tokens": ["Ach", "!", "sie", "ist", "noch", "Mo\u00b7na\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Wahrhaftig, das ist Schade!", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "das", "ist", "Scha\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Finettens Puppenangesicht", "tokens": ["Fi\u00b7net\u00b7tens", "Pup\u00b7pen\u00b7an\u00b7ge\u00b7sicht"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kan noch von fern entz\u00fccken.", "tokens": ["Kan", "noch", "von", "fern", "ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie hat viel Narren, wie sie spricht,", "tokens": ["Sie", "hat", "viel", "Nar\u00b7ren", ",", "wie", "sie", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihren Liebesstricken.", "tokens": ["In", "ih\u00b7ren", "Lie\u00b7bes\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Kluge geht vorbey, und lacht.", "tokens": ["Der", "Klu\u00b7ge", "geht", "vor\u00b7bey", ",", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie macht, mit ihrer Flitterpracht,", "tokens": ["Sie", "macht", ",", "mit", "ih\u00b7rer", "Flit\u00b7ter\u00b7pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Gasse nur Parade.", "tokens": ["Der", "Gas\u00b7se", "nur", "Pa\u00b7ra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wahrhaftig, das ist Schade!", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "das", "ist", "Scha\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Fr\u00e4ulein Hey ist frey im Scherz,", "tokens": ["Mein", "Fr\u00e4u\u00b7lein", "Hey", "ist", "frey", "im", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sanft in ihrer Gnade.", "tokens": ["Und", "sanft", "in", "ih\u00b7rer", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie liebt mein b\u00fcrgerliches Herz", "tokens": ["Sie", "liebt", "mein", "b\u00fcr\u00b7ger\u00b7li\u00b7ches", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ziemlich hohem Grade.", "tokens": ["In", "ziem\u00b7lich", "ho\u00b7hem", "Gra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein ich wei\u00df nicht, wie das ist,", "tokens": ["Al\u00b7lein", "ich", "wei\u00df", "nicht", ",", "wie", "das", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie den Adelstand vergi\u00dft \u2014", "tokens": ["Da\u00df", "sie", "den", "A\u00b7del\u00b7stand", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Lieb ist wohl nur Gnade.", "tokens": ["Die", "Lieb", "ist", "wohl", "nur", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wahrhaftig, das ist Schade.", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "das", "ist", "Scha\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}