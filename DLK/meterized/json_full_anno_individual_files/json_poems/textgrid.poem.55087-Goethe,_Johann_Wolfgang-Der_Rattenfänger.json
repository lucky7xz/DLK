{"textgrid.poem.55087": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Rattenf\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der wohlbekannte S\u00e4nger,", "tokens": ["Ich", "bin", "der", "wohl\u00b7be\u00b7kann\u00b7te", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der vielgereiste Rattenf\u00e4nger,", "tokens": ["Der", "viel\u00b7ge\u00b7reis\u00b7te", "Rat\u00b7ten\u00b7f\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den diese altber\u00fchmte Stadt", "tokens": ["Den", "die\u00b7se", "alt\u00b7be\u00b7r\u00fchm\u00b7te", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewi\u00df besonders n\u00f6tig hat.", "tokens": ["Ge\u00b7wi\u00df", "be\u00b7son\u00b7ders", "n\u00f6\u00b7tig", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00e4ren's Ratten noch so viele,", "tokens": ["Und", "w\u00e4\u00b7ren's", "Rat\u00b7ten", "noch", "so", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4ren Wiesel mit im Spiele;", "tokens": ["Und", "w\u00e4\u00b7ren", "Wie\u00b7sel", "mit", "im", "Spie\u00b7le", ";"], "token_info": ["word", "word", "