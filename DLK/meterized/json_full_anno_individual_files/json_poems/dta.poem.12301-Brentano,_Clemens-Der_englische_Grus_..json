{"dta.poem.12301": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der englische Grus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt gut J\u00e4ger jagen,               ", "tokens": ["Es", "wollt", "gut", "J\u00e4\u00b7ger", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wollt jagen auf Himmels H\u00f6hn,", "tokens": ["Wollt", "ja\u00b7gen", "auf", "Him\u00b7mels", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was begegnet ihm auf der Heiden,", "tokens": ["Was", "be\u00b7geg\u00b7net", "ihm", "auf", "der", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Maria, die Jungfrau sch\u00f6n.", "tokens": ["Ma\u00b7ria", ",", "die", "Jung\u00b7frau", "sch\u00f6n", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der J\u00e4ger, den ich meine,", "tokens": ["Der", "J\u00e4\u00b7ger", ",", "den", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ist uns wohl bekannt,", "tokens": ["Der", "ist", "uns", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er jagt mit einem Engel,", "tokens": ["Er", "jagt", "mit", "ei\u00b7nem", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gabriel ist er genannt.", "tokens": ["Ga\u00b7bri\u00b7el", "ist", "er", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Der J\u00e4ger blie\u00df in sein H\u00f6rnlein,", "tokens": ["Der", "J\u00e4\u00b7ger", "blie\u00df", "in", "sein", "H\u00f6rn\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lautet also wohl:", "tokens": ["Es", "lau\u00b7tet", "al\u00b7so", "wohl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201egegr\u00fc\u00dft seyst du Maria,", "tokens": ["\u201e", "ge\u00b7gr\u00fc\u00dft", "seyst", "du", "Ma\u00b7ria", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201edu bist aller Gnaden voll.", "tokens": ["\u201e", "du", "bist", "al\u00b7ler", "Gna\u00b7den", "voll", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201egegr\u00fc\u00dft seyst du Maria,", "tokens": ["\u201e", "ge\u00b7gr\u00fc\u00dft", "seyst", "du", "Ma\u00b7ria", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201edu edle Jungfrau fein,", "tokens": ["\u201e", "du", "ed\u00b7le", "Jung\u00b7frau", "fein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201edein Leib soll dir geb\u00e4hren,", "tokens": ["\u201e", "dein", "Leib", "soll", "dir", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eein kleines Kindelein.", "tokens": ["\u201e", "ein", "klei\u00b7nes", "Kin\u00b7de\u00b7lein", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201edein Leib soll dir geb\u00e4hren,", "tokens": ["\u201e", "dein", "Leib", "soll", "dir", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eein Kindlein ohn einen Mann,", "tokens": ["\u201e", "ein", "Kin\u00b7dlein", "ohn", "ei\u00b7nen", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201edas Himmel und die Erde", "tokens": ["\u201e", "das", "Him\u00b7mel", "und", "die", "Er\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eeinsmals zwingen kann.\u201c", "tokens": ["\u201e", "eins\u00b7mals", "zwin\u00b7gen", "kann", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Maria die viel reine,", "tokens": ["Ma\u00b7ria", "die", "viel", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PIAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fiel nieder auf ihre Knie,", "tokens": ["Fiel", "nie\u00b7der", "auf", "ih\u00b7re", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann bat sie Gott vom Himmel:", "tokens": ["Dann", "bat", "sie", "Gott", "vom", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edein Will gescheh allhie.\u201c", "tokens": ["\u201e", "dein", "Will", "ge\u00b7scheh", "all\u00b7hie", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201edein Will der soll geschehen,", "tokens": ["\u201e", "dein", "Will", "der", "soll", "ge\u00b7sche\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ART", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eohn Pein und sonder Schmerz.\u201c", "tokens": ["\u201e", "ohn", "Pein", "und", "son\u00b7der", "Schmerz", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da empfing sie Jesum Christum", "tokens": ["Da", "emp\u00b7fing", "sie", "Je\u00b7sum", "Chris\u00b7tum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unter ihr jungfr\u00e4uliches Herz.", "tokens": ["Un\u00b7ter", "ihr", "jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}}}}