{"textgrid.poem.56048": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Ein Doge", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fremde Gesandte sahen, wie sie geizten", "tokens": ["Frem\u00b7de", "Ge\u00b7sand\u00b7te", "sa\u00b7hen", ",", "wie", "sie", "geiz\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "mit ihm und allem was er tat;", "tokens": ["mit", "ihm", "und", "al\u00b7lem", "was", "er", "tat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PIS", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "w\u00e4hrend sie ihn zu seiner Gr\u00f6\u00dfe reizten,", "tokens": ["w\u00e4h\u00b7rend", "sie", "ihn", "zu", "sei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "reiz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "umstellten sie das goldene Dogat", "tokens": ["um\u00b7stell\u00b7ten", "sie", "das", "gol\u00b7de\u00b7ne", "Do\u00b7gat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "mit Sp\u00e4hern und Beschr\u00e4nkern immer mehr,", "tokens": ["mit", "Sp\u00e4\u00b7hern", "und", "Be\u00b7schr\u00e4n\u00b7kern", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADV", "$,"], 