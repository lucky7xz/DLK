{"dta.poem.18167": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gott  ist ein allwissender  \n Richter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "HeRR! du erforschest uns; du ken-", "tokens": ["HeRR", "!", "du", "er\u00b7for\u00b7schest", "uns", ";", "du", "ken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "$.", "PPER", "TRUNC"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "nest unsre Sinnen,", "tokens": ["nest", "uns\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du weist was jeder thut und siehest", "tokens": ["Du", "weist", "was", "je\u00b7der", "thut", "und", "sie\u00b7hest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ihr Beginnen;", "tokens": ["ihr", "Be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Du richtest unser Thun; so wie es dir gef\u00e4llt,", "tokens": ["Du", "rich\u00b7test", "un\u00b7ser", "Thun", ";", "so", "wie", "es", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "KOKOM", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn du als Richter komst am Ende dieser Welt.", "tokens": ["Wenn", "du", "als", "Rich\u00b7ter", "komst", "am", "En\u00b7de", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VVFIN", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! gieb da\u00df wir dies stets bei unsern Thun betrachten,", "tokens": ["Ach", "!", "gieb", "da\u00df", "wir", "dies", "stets", "bei", "un\u00b7sern", "Thun", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "KOUS", "PPER", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Will uns die b\u00f6se Welt nach ihren Urtheil achten", "tokens": ["Will", "uns", "die", "b\u00f6\u00b7se", "Welt", "nach", "ih\u00b7ren", "Ur\u00b7theil", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das gehet uns nicht an: Nur du, nur du allein,", "tokens": ["Das", "ge\u00b7het", "uns", "nicht", "an", ":", "Nur", "du", ",", "nur", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "ADV", "PPER", "$,", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du wirst der Guten Lohn, der B\u00f6sen R\u00e4cher sein.", "tokens": ["Du", "wirst", "der", "Gu\u00b7ten", "Lohn", ",", "der", "B\u00f6\u00b7sen", "R\u00e4\u00b7cher", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}