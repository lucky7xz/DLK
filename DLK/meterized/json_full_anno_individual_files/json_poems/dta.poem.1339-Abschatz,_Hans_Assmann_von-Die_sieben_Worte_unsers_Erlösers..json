{"dta.poem.1339": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die sieben Worte unsers Erl\u00f6sers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Man nimmt die letzten Wort' in Acht/", "tokens": ["Man", "nimmt", "die", "letz\u00b7ten", "Wort'", "in", "Acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die unsre Freunde vorgebracht/", "tokens": ["Die", "uns\u00b7re", "Freun\u00b7de", "vor\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wolte das nicht fassen", "tokens": ["Wer", "wol\u00b7te", "das", "nicht", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PDS", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was JEsus/ der uns selig macht/", "tokens": ["Was", "Je\u00b7sus", "/", "der", "uns", "se\u00b7lig", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$(", "PRELS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Creutz hat hinterlassen.", "tokens": ["Am", "Creutz", "hat", "hin\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erst JEsus zu dem Vater spricht:", "tokens": ["Erst", "Je\u00b7sus", "zu", "dem", "Va\u00b7ter", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00e4ch an dem blinden Volcke nicht", "tokens": ["R\u00e4ch", "an", "dem", "blin\u00b7den", "Vol\u00b7cke", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sie an mir ver\u00fcben/", "tokens": ["Was", "sie", "an", "mir", "ver\u00b7\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "So ist er/ bi\u00df das Leben bricht/", "tokens": ["So", "ist", "er", "/", "bi\u00df", "das", "Le\u00b7ben", "bricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Feinde Freund geblieben.", "tokens": ["Der", "Fein\u00b7de", "Freund", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des HErren ander Wort erfreut", "tokens": ["Des", "Her\u00b7ren", "an\u00b7der", "Wort", "er\u00b7freut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Sch\u00e4cher/ der mit Reu und Leid", "tokens": ["Den", "Sch\u00e4\u00b7cher", "/", "der", "mit", "Reu", "und", "Leid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der S\u00fcnden war umgeben:", "tokens": ["Der", "S\u00fcn\u00b7den", "war", "um\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sage dir/ du wirst noch heut", "tokens": ["Ich", "sa\u00b7ge", "dir", "/", "du", "wirst", "noch", "heut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Himmel mit mir leben.", "tokens": ["Im", "Him\u00b7mel", "mit", "mir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der HErr stellt der bedr\u00e4ngten Schaar", "tokens": ["Der", "Herr", "stellt", "der", "be\u00b7dr\u00e4ng\u00b7ten", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sein sorgend Vater-Hertze dar/", "tokens": ["Sein", "sor\u00b7gend", "Va\u00b7ter\u00b7Hert\u00b7ze", "dar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4st ihnen Schutz und Segen/", "tokens": ["L\u00e4st", "ih\u00b7nen", "Schutz", "und", "Se\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimmt der betr\u00fcbten Mutter wahr/", "tokens": ["Nimmt", "der", "be\u00b7tr\u00fcb\u00b7ten", "Mut\u00b7ter", "wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Heist ihr den J\u00fcnger pflegen.", "tokens": ["Heist", "ihr", "den", "J\u00fcn\u00b7ger", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihn d\u00fcrstet nach der Menschen Heyl/", "tokens": ["Ihn", "d\u00fcrs\u00b7tet", "nach", "der", "Men\u00b7schen", "Heyl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn d\u00fcrstet/ weil des H\u00f6chsten Pfeil", "tokens": ["Ihn", "d\u00fcrs\u00b7tet", "/", "weil", "des", "H\u00f6chs\u00b7ten", "Pfeil"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Glieder Safft verkreischet;", "tokens": ["Der", "Glie\u00b7der", "Safft", "ver\u00b7krei\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach dencke/ da\u00df er auch ein Theil", "tokens": ["Ach", "den\u00b7cke", "/", "da\u00df", "er", "auch", "ein", "Theil"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bu\u00df-Thr\u00e4nen von dir heischet!", "tokens": ["Bu\u00df\u00b7Thr\u00e4\u00b7nen", "von", "dir", "hei\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Merck auff/ o Hertz/ und dencke nach/", "tokens": ["Merck", "auff", "/", "o", "Hertz", "/", "und", "den\u00b7cke", "nach", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "$(", "FM", "NN", "$(", "KON", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum er zu dem Vater sprach:", "tokens": ["Wa\u00b7rum", "er", "zu", "dem", "Va\u00b7ter", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hast du mich verlassen.", "tokens": ["Wie", "hast", "du", "mich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schwer ists/ was der Mensch verbrach/", "tokens": ["So", "schwer", "ists", "/", "was", "der", "Mensch", "ver\u00b7brach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$(", "PWS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auff seine Schultern fassen!", "tokens": ["Auff", "sei\u00b7ne", "Schul\u00b7tern", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es ist vollbracht/ die Schrifft erf\u00fcllt/", "tokens": ["Es", "ist", "voll\u00b7bracht", "/", "die", "Schrifft", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schuld bezahlt/ der Zorn gestillt/", "tokens": ["Die", "Schuld", "be\u00b7zahlt", "/", "der", "Zorn", "ge\u00b7stillt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt man den Heyland ruffen.", "tokens": ["H\u00f6rt", "man", "den", "Hey\u00b7land", "ruf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Di\u00df Wort/ draus Trost und Leben quillt/", "tokens": ["Di\u00df", "Wort", "/", "draus", "Trost", "und", "Le\u00b7ben", "quillt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "PAV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4lt uns den Himmel offen!", "tokens": ["H\u00e4lt", "uns", "den", "Him\u00b7mel", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Vater/ meinen matten Geist/", "tokens": ["O", "Va\u00b7ter", "/", "mei\u00b7nen", "mat\u00b7ten", "Geist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich nunmehr vom Leibe reist/", "tokens": ["Der", "sich", "nun\u00b7mehr", "vom", "Lei\u00b7be", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Befehl ich deinen H\u00e4nden!", "tokens": ["Be\u00b7fehl", "ich", "dei\u00b7nen", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer so versorgt das Leben schleust/", "tokens": ["Wer", "so", "ver\u00b7sorgt", "das", "Le\u00b7ben", "schleust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kan seliglich vollenden.", "tokens": ["Kan", "se\u00b7lig\u00b7lich", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Ach Worte voller Lebens-Safft!", "tokens": ["Ach", "Wor\u00b7te", "vol\u00b7ler", "Le\u00b7bens\u00b7\u00b7S\u00b7afft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bin ich mit Sorg und Angst behafft/", "tokens": ["Bin", "ich", "mit", "Sorg", "und", "Angst", "be\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschreckt mich Tod und S\u00fcnde/", "tokens": ["Er\u00b7schreckt", "mich", "Tod", "und", "S\u00fcn\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So hilff/ da\u00df ich derselben Krafft/", "tokens": ["So", "hilff", "/", "da\u00df", "ich", "der\u00b7sel\u00b7ben", "Krafft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "KOUS", "PPER", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O JEsu/ stets empfinde.", "tokens": ["O", "Je\u00b7su", "/", "stets", "emp\u00b7fin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}