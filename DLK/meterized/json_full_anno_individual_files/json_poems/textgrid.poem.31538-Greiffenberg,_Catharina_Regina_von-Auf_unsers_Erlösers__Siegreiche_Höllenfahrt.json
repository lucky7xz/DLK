{"textgrid.poem.31538": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Auf unsers Erl\u00f6sers / Siegreiche H\u00f6llenfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fuhrest in die H\u00f6ll / mein Heiland / als ein Steger /", "tokens": ["Du", "fuh\u00b7rest", "in", "die", "H\u00f6ll", "/", "mein", "Hei\u00b7land", "/", "als", "ein", "Ste\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nicht / nach zu leiden mehr: es war bereit vollbracht!", "tokens": ["nicht", "/", "nach", "zu", "lei\u00b7den", "mehr", ":", "es", "war", "be\u00b7reit", "voll\u00b7bracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$(", "APPR", "PTKZU", "VVINF", "ADV", "$.", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nur / da\u00df du sie zerst\u00f6rst / und f\u00fchrst im Sieges-Pracht", "tokens": ["nur", "/", "da\u00df", "du", "sie", "zer\u00b7st\u00f6rst", "/", "und", "f\u00fchrst", "im", "Sie\u00b7ges\u00b7Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$(", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das H\u00f6llisch Geister-Heer / die Seeligkeits-bekrieger.", "tokens": ["das", "H\u00f6l\u00b7lisch", "Geis\u00b7ter\u00b7Heer", "/", "die", "See\u00b7