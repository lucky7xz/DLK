{"textgrid.poem.67432": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das innere Olympia", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind die G\u00e4ste versammelt, so l\u00e4\u00dft die Harfe sich h\u00f6ren;", "tokens": ["Sind", "die", "G\u00e4s\u00b7te", "ver\u00b7sam\u00b7melt", ",", "so", "l\u00e4\u00dft", "die", "Har\u00b7fe", "sich", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sitzt der Richter, so tritt Redner und Sprecher vor ihn.", "tokens": ["Sitzt", "der", "Rich\u00b7ter", ",", "so", "tritt", "Red\u00b7ner", "und", "Spre\u00b7cher", "vor", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Griechenland ist beisammen: da singen Dichter, es k\u00e4mpfen", "tokens": ["Grie\u00b7chen\u00b7land", "ist", "bei\u00b7sam\u00b7men", ":", "da", "sin\u00b7gen", "Dich\u00b7ter", ",", "es", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "VVPP", "$.", "ADV", "ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "K\u00e4mpfer, der L\u00e4ufer l\u00e4uft, blickebefl\u00fcgelt, zum Ziel.", "tokens": ["K\u00e4mp\u00b7fer", ",", "der", "L\u00e4u\u00b7fer", "l\u00e4uft", ",", "bli\u00b7cke\u00b7be\u00b7fl\u00fc\u00b7gelt", ",", "zum", "Ziel", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Aber zur innern Harfe, zum Spruch der richtenden Seele", "tokens": ["A\u00b7ber", "zur", "in\u00b7nern", "Har\u00b7fe", ",", "zum", "Spruch", "der", "rich\u00b7ten\u00b7den", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und zum Kampfe, zum Lauf nach der Vollkommenheit Kranz", "tokens": ["Und", "zum", "Kamp\u00b7fe", ",", "zum", "Lauf", "nach", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "Kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "APPRART", "NN", "APPR", "ART", "NN", "NN"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Darf es keiner Versammlung und keiner Blicke; Du bist Dir", "tokens": ["Darf", "es", "kei\u00b7ner", "Ver\u00b7samm\u00b7lung", "und", "kei\u00b7ner", "Bli\u00b7cke", ";", "Du", "bist", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "$.", "PPER", "VAFIN", "PPER"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "H\u00f6rer und Harf' und Gesang, L\u00e4ufer und Richter und Ziel.", "tokens": ["H\u00f6\u00b7rer", "und", "Ha\u00b7rf'", "und", "Ge\u00b7sang", ",", "L\u00e4u\u00b7fer", "und", "Rich\u00b7ter", "und", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN"