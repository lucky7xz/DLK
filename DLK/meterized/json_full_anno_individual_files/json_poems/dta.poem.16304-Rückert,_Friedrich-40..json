{"dta.poem.16304": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir bringen unsern Preis der Morgensonne dar,", "tokens": ["Wir", "brin\u00b7gen", "un\u00b7sern", "Preis", "der", "Mor\u00b7gen\u00b7son\u00b7ne", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hell die Sch\u00f6pfung macht und unsre Seele klar.", "tokens": ["Die", "hell", "die", "Sch\u00f6p\u00b7fung", "macht", "und", "uns\u00b7re", "See\u00b7le", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vor ihrer Ankunft geht der Morgenwind als Bothe,", "tokens": ["Vor", "ih\u00b7rer", "An\u00b7kunft", "geht", "der", "Mor\u00b7gen\u00b7wind", "als", "Bo\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihres Einzugs Fahn' erscheint im Morgenrothe.", "tokens": ["Und", "ih\u00b7res", "Ein\u00b7zugs", "Fahn'", "er\u00b7scheint", "im", "Mor\u00b7gen\u00b7ro\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Schauer meldet sie; und nun erscheint sie gleich,", "tokens": ["Ein", "Schau\u00b7er", "mel\u00b7det", "sie", ";", "und", "nun", "er\u00b7scheint", "sie", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nimmt mit einem Blick Besitz von ihrem Reich.", "tokens": ["Und", "nimmt", "mit", "ei\u00b7nem", "Blick", "Be\u00b7sitz", "von", "ih\u00b7rem", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Nebelschleier hebt sie von den Berggestalten,", "tokens": ["Den", "Ne\u00b7bel\u00b7schlei\u00b7er", "hebt", "sie", "von", "den", "Berg\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dr\u00e4ngt den Rest der Nacht zur\u00fcck in Th\u00e4lerfalten.", "tokens": ["Und", "dr\u00e4ngt", "den", "Rest", "der", "Nacht", "zu\u00b7r\u00fcck", "in", "Th\u00e4\u00b7ler\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie f\u00fcllt mit Glanz das Thal gleich einer Opferschale,", "tokens": ["Sie", "f\u00fcllt", "mit", "Glanz", "das", "Thal", "gleich", "ei\u00b7ner", "Op\u00b7fer\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen eignen Stral trinkt jede Blum' im Thale.", "tokens": ["Und", "ei\u00b7nen", "eig\u00b7nen", "Stral", "trinkt", "je\u00b7de", "Blum'", "im", "Tha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wie die Blum' in Lust zum Licht empor sich richtet,", "tokens": ["Und", "wie", "die", "Blum'", "in", "Lust", "zum", "Licht", "em\u00b7por", "sich", "rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "APPRART", "NN", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat in Menschenbrust Bewu\u00dftseyn sich gelichtet.", "tokens": ["So", "hat", "in", "Men\u00b7schen\u00b7brust", "Be\u00b7wu\u00df\u00b7tseyn", "sich", "ge\u00b7lich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Traumschattengaukelei, Nachtt\u00e4uschungstruggespinnst,", "tokens": ["Traum\u00b7schat\u00b7ten\u00b7gau\u00b7ke\u00b7lei", ",", "Nacht\u00b7t\u00e4u\u00b7schungs\u00b7trug\u00b7ge\u00b7spinnst", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerrei\u00dft, Licht der Natur, wo du den Sieg gewinnst.", "tokens": ["Zer\u00b7rei\u00dft", ",", "Licht", "der", "Na\u00b7tur", ",", "wo", "du", "den", "Sieg", "ge\u00b7winnst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Streck' aus die Stralenhand, das Opfer zu empfangen,", "tokens": ["Streck'", "aus", "die", "Stra\u00b7len\u00b7hand", ",", "das", "Op\u00b7fer", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das dir die Sch\u00f6pfung bringt und Herzen voll Verlangen.", "tokens": ["Das", "dir", "die", "Sch\u00f6p\u00b7fung", "bringt", "und", "Her\u00b7zen", "voll", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "KON", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Erheb mit deinem Blick und st\u00fctze, wie die Ranken", "tokens": ["Er\u00b7heb", "mit", "dei\u00b7nem", "Blick", "und", "st\u00fct\u00b7ze", ",", "wie", "die", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Baumes, thauschwer sich aufrichtende Gedanken.", "tokens": ["Des", "Bau\u00b7mes", ",", "thausc\u00b7hwer", "sich", "auf\u00b7rich\u00b7ten\u00b7de", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-++-++-+-+-", "measure": "unknown.measure.septa"}}, "stanza.10": {"line.1": {"text": "Die W\u00fcnsch' und Hoffnungen, die Vors\u00e4tz' und Entschl\u00fcsse,", "tokens": ["Die", "W\u00fcn\u00b7sch'", "und", "Hoff\u00b7nun\u00b7gen", ",", "die", "Vor\u00b7s\u00e4tz'", "und", "Ent\u00b7schl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Beleb', erfrische, st\u00e4rk' und zieh wie Sommersch\u00fcsse.", "tokens": ["Be\u00b7leb'", ",", "er\u00b7fri\u00b7sche", ",", "st\u00e4rk'", "und", "zieh", "wie", "Som\u00b7mer\u00b7sch\u00fcs\u00b7se", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Gib allen Knospen, da\u00df sie sich zur Bl\u00fct' entfalten,", "tokens": ["Gib", "al\u00b7len", "Knos\u00b7pen", ",", "da\u00df", "sie", "sich", "zur", "Bl\u00fct'", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und allen Blumen, da\u00df sie sich nach dir gestalten.", "tokens": ["Und", "al\u00b7len", "Blu\u00b7men", ",", "da\u00df", "sie", "sich", "nach", "dir", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Und allen Herzen gib, nach Blumenart zu wandeln,", "tokens": ["Und", "al\u00b7len", "Her\u00b7zen", "gib", ",", "nach", "Blu\u00b7men\u00b7art", "zu", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVIMP", "$,", "APPR", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unwandelbar zum Licht gewandt, im Licht zu wandeln.", "tokens": ["Un\u00b7wan\u00b7del\u00b7bar", "zum", "Licht", "ge\u00b7wandt", ",", "im", "Licht", "zu", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Das ist das Fr\u00fchgebet, das wir dir tragen vor;", "tokens": ["Das", "ist", "das", "Fr\u00fch\u00b7ge\u00b7bet", ",", "das", "wir", "dir", "tra\u00b7gen", "vor", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trag es empor zu dir, und \u00fcber dich empor!", "tokens": ["Trag", "es", "em\u00b7por", "zu", "dir", ",", "und", "\u00fc\u00b7ber", "dich", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "APPR", "PPER", "$,", "KON", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Denn als ein Mittler gehst du durch der Sch\u00f6pfung Mitte,", "tokens": ["Denn", "als", "ein", "Mitt\u00b7ler", "gehst", "du", "durch", "der", "Sch\u00f6p\u00b7fung", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu bringen Oberen der untern Wesen Bitte.", "tokens": ["Zu", "brin\u00b7gen", "O\u00b7be\u00b7ren", "der", "un\u00b7tern", "We\u00b7sen", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Bring zu der Sonne sie, die dich am Faden leitet,", "tokens": ["Bring", "zu", "der", "Son\u00b7ne", "sie", ",", "die", "dich", "am", "Fa\u00b7den", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df die sie bringe der, in deren Dienst sie schreitet.", "tokens": ["Da\u00df", "die", "sie", "brin\u00b7ge", "der", ",", "in", "de\u00b7ren", "Dienst", "sie", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVFIN", "ART", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Der goldne Eimer reicht von immer h\u00f6hern Sonnen", "tokens": ["Der", "gold\u00b7ne", "Ei\u00b7mer", "reicht", "von", "im\u00b7mer", "h\u00f6\u00b7hern", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu immer h\u00f6hern bis zum h\u00f6chsten Sonnenbronnen.", "tokens": ["Zu", "im\u00b7mer", "h\u00f6\u00b7hern", "bis", "zum", "h\u00f6chs\u00b7ten", "Son\u00b7nen\u00b7bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Dort f\u00fcllt ihr mit dem Thau den Eimer, der uns letzt;", "tokens": ["Dort", "f\u00fcllt", "ihr", "mit", "dem", "Thau", "den", "Ei\u00b7mer", ",", "der", "uns", "letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dorthin, mit Dank gef\u00fcllt, tragt mir den leeren jetzt!", "tokens": ["Dor\u00b7thin", ",", "mit", "Dank", "ge\u00b7f\u00fcllt", ",", "tragt", "mir", "den", "lee\u00b7ren", "jetzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}