{"textgrid.poem.35089": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das Kapellchen am See", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gl\u00f6cklein h\u00f6r ich klingen", "tokens": ["Ein", "Gl\u00f6c\u00b7klein", "h\u00f6r", "ich", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl \u00fcber den lauschenden See;", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "lau\u00b7schen\u00b7den", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Das will mir immer bringen", "tokens": ["Das", "will", "mir", "im\u00b7mer", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wonniges Gl\u00fcck und ein Weh.", "tokens": ["Ein", "won\u00b7ni\u00b7ges", "Gl\u00fcck", "und", "ein", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ein Gl\u00fcck, weil es mich mahnet:", "tokens": ["Ein", "Gl\u00fcck", ",", "weil", "es", "mich", "mah\u00b7net", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbfahr \u00fcber, und glaube an mich;", "tokens": ["\u00bb", "fahr", "\u00fc\u00b7ber", ",", "und", "glau\u00b7be", "an", "mich", ";"], "token_info": ["pu