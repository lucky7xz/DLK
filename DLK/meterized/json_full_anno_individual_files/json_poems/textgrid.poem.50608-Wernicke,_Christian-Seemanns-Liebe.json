{"textgrid.poem.50608": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Seemanns-Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df man ein Fr\u00e4ulein preist, indem man sie betr\u00fcget,", "tokens": ["Da\u00df", "man", "ein", "Fr\u00e4u\u00b7lein", "preist", ",", "in\u00b7dem", "man", "sie", "be\u00b7tr\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sie durch Umschweif' oft zu \u00fcberreden strebt,", "tokens": ["Und", "sie", "durch", "Um\u00b7schweif'", "oft", "zu", "\u00fc\u00b7berr\u00b7e\u00b7den", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df man die H\u00e4nd' ihr k\u00fc\u00dft und ihr zu F\u00fc\u00dfen lieget,", "tokens": ["Da\u00df", "man", "die", "H\u00e4nd'", "ihr", "k\u00fc\u00dft", "und", "ihr", "zu", "F\u00fc\u00b7\u00dfen", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PPER", "VVFIN", "KON", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem man sie empor bis an die Sterne hebt,", "tokens": ["In\u00b7dem", "man", "sie", "em\u00b7por", "bis", "an", "die", "Ster\u00b7ne", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"]