{"dta.poem.822": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  Auf den Sontag de\u00df fl\u00fcchtigen Messias/  \n oder nach der Beschneydung  JESU.  \n Matth. 2.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dem das zarte Kind in sanfftem Schlaffe lieget/ ", "tokens": ["In", "dem", "das", "zar\u00b7te", "Kind", "in", "sanff\u00b7tem", "Schlaf\u00b7fe", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd Joseph ohne sorg\u2019/ in dem der Bluthund wacht/", "tokens": ["Vnd", "Jo\u00b7se\u00b7ph", "oh\u00b7ne", "sor\u00b7g'", "/", "in", "dem", "der", "Blut\u00b7hund", "wacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+--+----+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Vnd rasend/ doth voll furcht/ nach w\u00fcrge Schwerdten", "tokens": ["Vnd", "ra\u00b7send", "/", "doth", "voll", "furcht", "/", "nach", "w\u00fcr\u00b7ge", "Schwerd\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$(", "ADJD", "ADJD", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "tracht/ ", "tokens": ["tracht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ja schon durch sein befehl der Friedens-F\u00fcrst bekrieget;", "tokens": ["Ja", "schon", "durch", "sein", "be\u00b7fehl", "der", "Frie\u00b7dens\u00b7F\u00fcrst", "be\u00b7krie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schawt Gott/ der weise Gott der aller Sinn obsieget", "tokens": ["Schawt", "Gott", "/", "der", "wei\u00b7se", "Gott", "der", "al\u00b7ler", "Sinn", "ob\u00b7sie\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$(", "ART", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff dieses Feindes ranck von seinem Thron vnd lacht.", "tokens": ["Auff", "die\u00b7ses", "Fein\u00b7des", "ranck", "von", "sei\u00b7nem", "Thron", "vnd", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch f\u00fchrt sein Engel ab. der bey noch stiller Nacht", "tokens": ["Auch", "f\u00fchrt", "sein", "En\u00b7gel", "ab", ".", "der", "bey", "noch", "stil\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "ART", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Joseph weichen heist eh\u2019r als der Grim sich r\u00fcget.", "tokens": ["Den", "Jo\u00b7se\u00b7ph", "wei\u00b7chen", "heist", "eh'r", "als", "der", "Grim", "sich", "r\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVINF", "VAFIN", "ADJD", "KOKOM", "ART", "NE", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "St\u00f6\u00dft schon das wilde Volck/ der blinden Juden Land", "tokens": ["St\u00f6\u00dft", "schon", "das", "wil\u00b7de", "Volck", "/", "der", "blin\u00b7den", "Ju\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den eignen K\u00f6nig aus; mu\u00df dennoch Nilus strand/", "tokens": ["Den", "eig\u00b7nen", "K\u00f6\u00b7nig", "aus", ";", "mu\u00df", "den\u00b7noch", "Ni\u00b7lus", "strand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "VMFIN", "ADV", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd Pharos pr\u00e4chtigs Reich/ jhm zu gebotte stehen.", "tokens": ["Vnd", "Pha\u00b7ros", "pr\u00e4ch\u00b7tigs", "Reich", "/", "jhm", "zu", "ge\u00b7bot\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$(", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drumb zag ich nimmermehr/ vor meiner Feinde Macht/", "tokens": ["Drumb", "zag", "ich", "nim\u00b7mer\u00b7mehr", "/", "vor", "mei\u00b7ner", "Fein\u00b7de", "Macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$(", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn der/ so nimmer schl\u00e4fft noch schlummert/ vor mich", "tokens": ["Wenn", "der", "/", "so", "nim\u00b7mer", "schl\u00e4fft", "noch", "schlum\u00b7mert", "/", "vor", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "$(", "ADV", "ADV", "VVFIN", "ADV", "VVFIN", "$(", "APPR", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "wacht ", "tokens": ["wacht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wei\u00df ich/ wenn/ wie/ wohin/ vnd wem ich sol entgehen.", "tokens": ["Wei\u00df", "ich", "/", "wenn", "/", "wie", "/", "wo\u00b7hin", "/", "vnd", "wem", "ich", "sol", "ent\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "$(", "PWAV", "$(", "PWAV", "$(", "KON", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}