{"dta.poem.14684": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger und J\u00e4gerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und w\u00e4r' das sch\u00f6ne Hirschlein todt,", "tokens": ["Und", "w\u00e4r'", "das", "sch\u00f6\u00b7ne", "Hirsc\u00b7hlein", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So m\u00f6cht' ich jagen l\u00e4nger nicht;", "tokens": ["So", "m\u00f6cht'", "ich", "ja\u00b7gen", "l\u00e4n\u00b7ger", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Scheint \u00fcber'n Wald der Morgenroth:", "tokens": ["Scheint", "\u00fc\u00b7ber'n", "Wald", "der", "Mor\u00b7gen\u00b7roth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fct' sch\u00f6nes Hirschlein, h\u00fcte dich!", "tokens": ["H\u00fct'", "sch\u00f6\u00b7nes", "Hirsc\u00b7hlein", ",", "h\u00fc\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}