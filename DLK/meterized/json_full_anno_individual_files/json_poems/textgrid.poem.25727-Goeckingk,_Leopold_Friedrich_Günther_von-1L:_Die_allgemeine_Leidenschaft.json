{"textgrid.poem.25727": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die allgemeine Leidenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die allgemeine Leidenschaft", "tokens": ["Die", "all\u00b7ge\u00b7mei\u00b7ne", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht der Ruhm!", "tokens": ["Ist", "nicht", "der", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zum mindsten nicht. Denn ach! der Deutsche rafft", "tokens": ["Zum", "minds\u00b7ten", "nicht", ".", "Denn", "ach", "!", "der", "Deut\u00b7sche", "rafft"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "PTKNEG", "$.", "KON", "XY", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach Golde nur. Was k\u00fcmmert ihn die Schande?", "tokens": ["Nach", "Gol\u00b7de", "nur", ".", "Was", "k\u00fcm\u00b7mert", "ihn", "die", "Schan\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was thut der Deutsche nicht f\u00fcr Geld! \u2013", "tokens": ["Was", "thut", "der", "Deut\u00b7sche", "nicht", "f\u00fcr", "Geld", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein sch\u00f6ner Ruhm! Ein Sprichwort aller Zonen!", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Ruhm", "!", "Ein", "Sprich\u00b7wort", "al\u00b7ler", "Zo\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie? bauet er, dem Britten gleich, sein Feld?", "tokens": ["Wie", "?", "bau\u00b7et", "er", ",", "dem", "Brit\u00b7ten", "gleich", ",", "sein", "Feld", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und n\u00e4hrt es schon zu viele Millionen?", "tokens": ["Und", "n\u00e4hrt", "es", "schon", "zu", "vie\u00b7le", "Mil\u00b7lion\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Mu\u00df darum sich dein schlanker Sohn,", "tokens": ["Mu\u00df", "da\u00b7rum", "sich", "dein", "schlan\u00b7ker", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Klopffechtern gleich, f\u00fcr einen Fremdling raufen?", "tokens": ["Klopf\u00b7fech\u00b7tern", "gleich", ",", "f\u00fcr", "ei\u00b7nen", "Fremd\u00b7ling", "rau\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und ach! zu eines stolzern Volkes Hohn,", "tokens": ["Und", "ach", "!", "zu", "ei\u00b7nes", "stol\u00b7zern", "Vol\u00b7kes", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sein tapfres Blut f\u00fcr theures Brod verkaufen?", "tokens": ["Sein", "tapf\u00b7res", "Blut", "f\u00fcr", "theu\u00b7res", "Brod", "ver\u00b7kau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bist du, Germanien, so karg", "tokens": ["Bist", "du", ",", "Ger\u00b7ma\u00b7ni\u00b7en", ",", "so", "karg"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wohl darum schon mit deinem weiten Boden,", "tokens": ["Wohl", "da\u00b7rum", "schon", "mit", "dei\u00b7nem", "wei\u00b7ten", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Da\u00df du dem Juden, kaum f\u00fcr seinen Sarg", "tokens": ["Da\u00df", "du", "dem", "Ju\u00b7den", ",", "kaum", "f\u00fcr", "sei\u00b7nen", "Sarg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Verg\u00f6nnest, nur ein Pl\u00e4tzchen umzuroden?", "tokens": ["Ver\u00b7g\u00f6n\u00b7nest", ",", "nur", "ein", "Pl\u00e4tz\u00b7chen", "um\u00b7zu\u00b7ro\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Mu\u00dft darum du zum Wolgastrand'", "tokens": ["Mu\u00dft", "da\u00b7rum", "du", "zum", "Wol\u00b7ga\u00b7strand'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und Vandaliciens", "tokens": ["Und", "Van\u00b7da\u00b7li\u00b7ci\u00b7ens"], "token_info": ["word", "word"], "pos": ["KON", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Die Kinder mit stiefm\u00fctterlicher Hand", "tokens": ["Die", "Kin\u00b7der", "mit", "stief\u00b7m\u00fct\u00b7ter\u00b7li\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Fortpeitschen, und der Mutter Haus ver\u00f6den?", "tokens": ["Fort\u00b7peit\u00b7schen", ",", "und", "der", "Mut\u00b7ter", "Haus", "ver\u00b7\u00f6\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "O nein! So viel auch ihrer sind,", "tokens": ["O", "nein", "!", "So", "viel", "auch", "ih\u00b7rer", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "ADV", "ADV", "ADV", "PPOSAT", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Du h\u00e4ttest Platz f\u00fcr alle deine Kinder.", "tokens": ["Du", "h\u00e4t\u00b7test", "Platz", "f\u00fcr", "al\u00b7le", "dei\u00b7ne", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Der Aberglaube macht' einst Frankreich blind,", "tokens": ["Der", "A\u00b7berg\u00b7lau\u00b7be", "macht'", "einst", "Fran\u00b7kreich", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Dich aber macht der Golddurst dreimal blinder.", "tokens": ["Dich", "a\u00b7ber", "macht", "der", "Gold\u00b7durst", "drei\u00b7mal", "blin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Zwar schwimmt dein Schiff ins fernste Meer,", "tokens": ["Zwar", "schwimmt", "dein", "Schiff", "ins", "ferns\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Dein Wagen f\u00e4hrt zu Pohlen und zu Ungern;", "tokens": ["Dein", "Wa\u00b7gen", "f\u00e4hrt", "zu", "Poh\u00b7len", "und", "zu", "Un\u00b7gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Doch macht Ein Mi\u00dfwachs deine Speicher leer,", "tokens": ["Doch", "macht", "Ein", "Mi\u00df\u00b7wachs", "dei\u00b7ne", "Spei\u00b7cher", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "So mu\u00df zu Haus dein eignes Kind verhungern.", "tokens": ["So", "mu\u00df", "zu", "Haus", "dein", "eig\u00b7nes", "Kind", "ver\u00b7hun\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Bist du wohl kl\u00fcger, als der Geitz,", "tokens": ["Bist", "du", "wohl", "kl\u00fc\u00b7ger", ",", "als", "der", "Geitz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der traurig darbt bei angef\u00fcllten Kasten?", "tokens": ["Der", "trau\u00b7rig", "darbt", "bei", "an\u00b7ge\u00b7f\u00fcll\u00b7ten", "Kas\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wann durfte deine \u00e4rmre Schwester selbst, die Schweitz,", "tokens": ["Wann", "durf\u00b7te", "dei\u00b7ne", "\u00e4rm\u00b7re", "Schwes\u00b7ter", "selbst", ",", "die", "Schweitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "$,", "PRELS", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auch ohne Frankreichs Gold, noch jemals fasten?", "tokens": ["Auch", "oh\u00b7ne", "Fran\u00b7kreichs", "Gold", ",", "noch", "je\u00b7mals", "fas\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wenn dem, der in der Sonne brennt,", "tokens": ["Wenn", "dem", ",", "der", "in", "der", "Son\u00b7ne", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Um deinen Boden m\u00fchsam umzupfl\u00fcgen,", "tokens": ["Um", "dei\u00b7nen", "Bo\u00b7den", "m\u00fch\u00b7sam", "um\u00b7zu\u00b7pfl\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Dein Geitz kaum einen Labetrunk noch g\u00f6nnt:", "tokens": ["Dein", "Geitz", "kaum", "ei\u00b7nen", "La\u00b7be\u00b7trunk", "noch", "g\u00f6nnt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Wie k\u00f6nnen noch sich deine Speicher biegen?", "tokens": ["Wie", "k\u00f6n\u00b7nen", "noch", "sich", "dei\u00b7ne", "Spei\u00b7cher", "bie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Bist du so reich: wo ist dein Gold?", "tokens": ["Bist", "du", "so", "reich", ":", "wo", "ist", "dein", "Gold", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wo baust du neue Br\u00fccken itzt? Wo sagen", "tokens": ["Wo", "baust", "du", "neu\u00b7e", "Br\u00fc\u00b7cken", "itzt", "?", "Wo", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$.", "PWAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Die Steine schon die Stunden uns? Wo rollt", "tokens": ["Die", "Stei\u00b7ne", "schon", "die", "Stun\u00b7den", "uns", "?", "Wo", "rollt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PPER", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Auf Stra\u00dfen neuer Appier der Wagen?", "tokens": ["Auf", "Stra\u00b7\u00dfen", "neu\u00b7er", "Ap\u00b7pier", "der", "Wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.41": {"text": "Wenn du dich Rom zu seyn erk\u00fchnst:", "tokens": ["Wenn", "du", "dich", "Rom", "zu", "seyn", "er\u00b7k\u00fchnst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NE", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Worin bist du wohl Rom schon gleich geworden?", "tokens": ["Wo\u00b7rin", "bist", "du", "wohl", "Rom", "schon", "gleich", "ge\u00b7wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "NE", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Du hast ja nicht einmal f\u00fcr das Verdienst", "tokens": ["Du", "hast", "ja", "nicht", "ein\u00b7mal", "f\u00fcr", "das", "Ver\u00b7dienst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Ruhm bringender Plebejer, einen Orden?", "tokens": ["Ruhm", "brin\u00b7gen\u00b7der", "Ple\u00b7be\u00b7jer", ",", "ei\u00b7nen", "Or\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "++--+-++-+-", "measure": "trochaic.hexa.relaxed"}, "line.45": {"text": "Und ahmst du ja den Schwestern nach,", "tokens": ["Und", "ahmst", "du", "ja", "den", "Schwes\u00b7tern", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und forderst den Erfinder auf zu Wachen:", "tokens": ["Und", "for\u00b7derst", "den", "Er\u00b7fin\u00b7der", "auf", "zu", "Wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "O so bezahlst du ihm sein Oel, doch ach!", "tokens": ["O", "so", "be\u00b7zahlst", "du", "ihm", "sein", "O\u00b7el", ",", "doch", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$,", "ADV", "ADV", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.48": {"text": "Um ihn ber\u00fchmt, dich l\u00e4cherlich zu machen.", "tokens": ["Um", "ihn", "be\u00b7r\u00fchmt", ",", "dich", "l\u00e4\u00b7cher\u00b7lich", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Germanien! was k\u00f6nntest du", "tokens":