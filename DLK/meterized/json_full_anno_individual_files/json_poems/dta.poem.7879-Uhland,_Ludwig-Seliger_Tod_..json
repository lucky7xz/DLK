{"dta.poem.7879": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Seliger Tod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Gestorben war ich", "tokens": ["Ge\u00b7stor\u00b7ben", "war", "ich"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.2": {"text": "Vor Liebeswonne;", "tokens": ["Vor", "Lie\u00b7bes\u00b7won\u00b7ne", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Begraben lag ich", "tokens": ["Be\u00b7gra\u00b7ben", "lag", "ich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In ihren Armen;", "tokens": ["In", "ih\u00b7ren", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Erwecket ward ich", "tokens": ["Er\u00b7we\u00b7cket", "ward", "ich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "VAFIN", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Von ihren K\u00fcssen;", "tokens": ["Von", "ih\u00b7ren", "K\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Den Himmel sah ich", "tokens": ["Den", "Him\u00b7mel", "sah", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In ihren Augen.", "tokens": ["In", "ih\u00b7ren", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}