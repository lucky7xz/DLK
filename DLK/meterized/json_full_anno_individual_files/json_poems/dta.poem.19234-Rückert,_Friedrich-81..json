{"dta.poem.19234": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein treuer Kampfgeno\u00df dem Menschen ist das Ro\u00df,", "tokens": ["Ein", "treu\u00b7er", "Kampf\u00b7ge\u00b7no\u00df", "dem", "Men\u00b7schen", "ist", "das", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scheut keines Schwertes Blitz und f\u00fcrchtet kein Gescho\u00df.", "tokens": ["Scheut", "kei\u00b7nes", "Schwer\u00b7tes", "Blitz", "und", "f\u00fcrch\u00b7tet", "kein", "Ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So ist ihm ohne Fehl ein Diener das Kamel,", "tokens": ["So", "ist", "ihm", "oh\u00b7ne", "Fehl", "ein", "Die\u00b7ner", "das", "Ka\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gehorsam beugt es sich und hebt sich dem Befehl.", "tokens": ["Ge\u00b7hor\u00b7sam", "beugt", "es", "sich", "und", "hebt", "sich", "dem", "Be\u00b7fehl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ro\u00df aber und Kamel sind unter sich in Feindschaft,", "tokens": ["Ro\u00df", "a\u00b7ber", "und", "Ka\u00b7mel", "sind", "un\u00b7ter", "sich", "in", "Feind\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VAFIN", "APPR", "PRF", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Im Dienst des Menschen nur gekommen in Gemeinschaft.", "tokens": ["Im", "Dienst", "des", "Men\u00b7schen", "nur", "ge\u00b7kom\u00b7men", "in", "Ge\u00b7mein\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So ist auch von Natur entzweiet Hund und Katze,", "tokens": ["So", "ist", "auch", "von", "Na\u00b7tur", "ent\u00b7zwei\u00b7et", "Hund", "und", "Kat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertragen m\u00fc\u00dfen sie sich doch an Einem Platze.", "tokens": ["Ver\u00b7tra\u00b7gen", "m\u00fc\u00b7\u00dfen", "sie", "sich", "doch", "an", "Ei\u00b7nem", "Plat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du kanst dich gleich erfreun verschiedenster Gestalten,", "tokens": ["Du", "kanst", "dich", "gleich", "er\u00b7freun", "ver\u00b7schie\u00b7dens\u00b7ter", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lern' auseinander nur die streitenden zu halten.", "tokens": ["Lern'", "aus\u00b7ein\u00b7an\u00b7der", "nur", "die", "strei\u00b7ten\u00b7den", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dich machte die Natur zum Herren ihrer Sch\u00e4tze,", "tokens": ["Dich", "mach\u00b7te", "die", "Na\u00b7tur", "zum", "Her\u00b7ren", "ih\u00b7rer", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit du glichest aus all ihre Gegens\u00e4tze.", "tokens": ["Da\u00b7mit", "du", "gli\u00b7chest", "aus", "all", "ih\u00b7re", "Ge\u00b7gen\u00b7s\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}