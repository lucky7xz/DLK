{"dta.poem.611": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uberschrifft \u00fcber eine Wiese/ so einer von  \n dem Lands-Herrn geschenckt bekommen/ so ihm zwar  \n von einigen Mi\u00dfg\u00f6nnern streitig gemacht wor-  \n den/ die er aber dennoch behalten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mi\u00dfgunst und der Neid erfunden tausend R\u00e4ncke/", "tokens": ["Die", "Mi\u00df\u00b7gunst", "und", "der", "Neid", "er\u00b7fun\u00b7den", "tau\u00b7send", "R\u00e4n\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein es war umsonst/ der Anschlag ging nicht an/", "tokens": ["Al\u00b7lein", "es", "war", "um\u00b7sonst", "/", "der", "An\u00b7schlag", "ging", "nicht", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$(", "ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der K\u00f6nig l\u00e4\u00dft ihm nun sein F\u00fcrstliches Geschencke", "tokens": ["Der", "K\u00f6\u00b7nig", "l\u00e4\u00dft", "ihm", "nun", "sein", "F\u00fcrst\u00b7li\u00b7ches", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und keiner findet sich/ der was darwieder kan.", "tokens": ["Und", "kei\u00b7ner", "fin\u00b7det", "sich", "/", "der", "was", "dar\u00b7wie\u00b7der", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "$(", "ART", "PWS", "PAV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So mu\u00df/ was GOtt verh\u00e4ngt/ ein gutes Ziel erreichen/", "tokens": ["So", "mu\u00df", "/", "was", "Gott", "ver\u00b7h\u00e4ngt", "/", "ein", "gu\u00b7tes", "Ziel", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "PWS", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und solte gleich der Neid aus Ungedult erbleichen.", "tokens": ["Und", "sol\u00b7te", "gleich", "der", "Neid", "aus", "Un\u00b7ge\u00b7dult", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}