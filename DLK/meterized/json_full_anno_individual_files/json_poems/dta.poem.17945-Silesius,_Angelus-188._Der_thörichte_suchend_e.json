{"dta.poem.17945": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "188.  Der th\u00f6richte suchend e", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Suchstu was und vermeinst das GOtt nicht alles sey/", "tokens": ["Suchs\u00b7tu", "was", "und", "ver\u00b7meinst", "das", "Gott", "nicht", "al\u00b7les", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "KON", "VVFIN", "ART", "NN", "PTKNEG", "PIS", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gehstu GOtt und alls in Ewigkeit f\u00fcrbey.", "tokens": ["So", "gehs\u00b7tu", "Gott", "und", "alls", "in", "E\u00b7wig\u00b7keit", "f\u00fcr\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}