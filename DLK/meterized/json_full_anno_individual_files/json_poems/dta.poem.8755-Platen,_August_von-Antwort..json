{"dta.poem.8755": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll dies kindische Verzagen,", "tokens": ["Was", "soll", "dies", "kin\u00b7di\u00b7sche", "Ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies eitle W\u00fcnschen ohne Halt?", "tokens": ["Dies", "eit\u00b7le", "W\u00fcn\u00b7schen", "oh\u00b7ne", "Halt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da du der Welt nicht kannst entsagen,", "tokens": ["Da", "du", "der", "Welt", "nicht", "kannst", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erob're dir sie mit Gewalt!", "tokens": ["Er\u00b7ob'\u00b7re", "dir", "sie", "mit", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und k\u00f6nntest du dich auch entfernen,", "tokens": ["Und", "k\u00f6nn\u00b7test", "du", "dich", "auch", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es triebe Sehnsucht dich zur\u00fcck;", "tokens": ["Es", "trie\u00b7be", "Sehn\u00b7sucht", "dich", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ach, die Menschen lieben lernen,", "tokens": ["Denn", "ach", ",", "die", "Men\u00b7schen", "lie\u00b7ben", "ler\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist das einz'ge wahre Gl\u00fcck!", "tokens": ["Es", "ist", "das", "einz'\u00b7ge", "wah\u00b7re", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Unwiderruflich dorrt die Bl\u00fcthe,", "tokens": ["Un\u00b7wi\u00b7der\u00b7ruf\u00b7lich", "dorrt", "die", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Unwiderruflich w\u00e4chst das Kind,", "tokens": ["Un\u00b7wi\u00b7der\u00b7ruf\u00b7lich", "w\u00e4chst", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Abgr\u00fcnde liegen im Gem\u00fcthe,", "tokens": ["Ab\u00b7gr\u00fcn\u00b7de", "lie\u00b7gen", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die tiefer als die H\u00f6lle sind.", "tokens": ["Die", "tie\u00b7fer", "als", "die", "H\u00f6l\u00b7le", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du siehst sie, doch du fliehst vor\u00fcber,", "tokens": ["Du", "siehst", "sie", ",", "doch", "du", "fliehst", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im gl\u00fccklichen, im ernsten Lauf,", "tokens": ["Im", "gl\u00fcck\u00b7li\u00b7chen", ",", "im", "erns\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem frohen Tage folgt ein tr\u00fcber,", "tokens": ["Dem", "fro\u00b7hen", "Ta\u00b7ge", "folgt", "ein", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch alles wiegt zulezt sich auf.", "tokens": ["Doch", "al\u00b7les", "wiegt", "zu\u00b7lezt", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie der Mond, im leichten Schweben,", "tokens": ["Und", "wie", "der", "Mond", ",", "im", "leich\u00b7ten", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald rein und bald in Wolken steht,", "tokens": ["Bald", "rein", "und", "bald", "in", "Wol\u00b7ken", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schwinde wechselnd dir das Leben,", "tokens": ["So", "schwin\u00b7de", "wech\u00b7selnd", "dir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis es in Wellen untergeht.", "tokens": ["Bis", "es", "in", "Wel\u00b7len", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}