{"dta.poem.10488": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Den K\u00f6nig Wiswamitra,", "tokens": ["Den", "K\u00f6\u00b7nig", "Wis\u00b7wa\u00b7mi\u00b7tra", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den treibt's ohne Rast und Ruh',", "tokens": ["Den", "treibt's", "oh\u00b7ne", "Rast", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er will durch Kampf und B\u00fc\u00dfung", "tokens": ["Er", "will", "durch", "Kampf", "und", "B\u00fc\u00b7\u00dfung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwerben Wasischtas Kuh.", "tokens": ["Er\u00b7wer\u00b7ben", "Wa\u00b7sischtas", "Kuh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O, K\u00f6nig Wiswamitra,", "tokens": ["O", ",", "K\u00f6\u00b7nig", "Wis\u00b7wa\u00b7mi\u00b7tra", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O, welch ein Ochs bist du,", "tokens": ["O", ",", "welch", "ein", "Ochs", "bist", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du so viel k\u00e4mpfest und b\u00fc\u00dfest,", "tokens": ["Da\u00df", "du", "so", "viel", "k\u00e4mp\u00b7fest", "und", "b\u00fc\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles f\u00fcr eine Kuh!", "tokens": ["Und", "Al\u00b7les", "f\u00fcr", "ei\u00b7ne", "Kuh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}