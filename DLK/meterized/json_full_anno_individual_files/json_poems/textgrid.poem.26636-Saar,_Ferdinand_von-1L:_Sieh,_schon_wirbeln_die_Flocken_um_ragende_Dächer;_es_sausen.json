{"textgrid.poem.26636": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, schon wirbeln die Flocken um ragende D\u00e4cher; es sausen", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, schon wirbeln die Flocken um ragende D\u00e4cher; es sausen", "tokens": ["Sieh", ",", "schon", "wir\u00b7beln", "die", "Flo\u00b7cken", "um", "ra\u00b7gen\u00b7de", "D\u00e4\u00b7cher", ";", "es", "sau\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$.", "PPER", "VVINF"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Eisige Winde mit Macht durch die rings offene Stadt.", "tokens": ["Ei\u00b7si\u00b7ge", "Win\u00b7de", "mit", "Macht", "durch", "die", "rings", "of\u00b7fe\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Ja, der Winter ist da! Mit ihm erschienen die Freuden,", "tokens": ["Ja", ",", "der", "Win\u00b7ter", "ist", "da", "!", "Mit", "ihm", "er\u00b7schie\u00b7nen", "die", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADV", "$.", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Welche der St\u00e4dter schon l\u00e4ngst sommerverdrossen ersehnt.", "tokens": ["Wel\u00b7che", "der", "St\u00e4d\u00b7ter", "schon", "l\u00e4ngst", "som\u00b7mer\u00b7ver\u00b7dros\u00b7sen", "er\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Alle Theater gef\u00fcllt, Applaus ersch\u00fcttert den Tonsaal \u2013", "tokens": ["Al\u00b7le", "The\u00b7a\u00b7ter", "ge\u00b7f\u00fcllt", ",", "Ap\u00b7plaus", "er\u00b7sch\u00fct\u00b7tert", "den", "Ton\u00b7sa\u00b7al", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "NE", "VVFIN", "ART", "NN", "$("], "meter": "--+---+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und so bewegt sich auch Wien wieder im alten Geleis.", "tokens": ["Und", "so", "be\u00b7wegt", "sich", "auch", "Wi\u00b7en", "wie\u00b7der", "im", "al\u00b7ten", "Ge\u00b7leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADV", "NE", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Amt und Gesch\u00e4ft durchkreuzen die Stra\u00dfen, auf glitschigem Pflaster", "tokens": ["Amt", "und", "Ge\u00b7sch\u00e4ft", "durch\u00b7kreu\u00b7zen", "die", "Stra\u00b7\u00dfen", ",", "auf", "glit\u00b7schi\u00b7gem", "Pflas\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Humpelt der Omnibus, rast der Fiaker dahin;", "tokens": ["Hum\u00b7pelt", "der", "Om\u00b7ni\u00b7bus", ",", "rast", "der", "Fi\u00b7a\u00b7ker", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Equipagen dazwischen, von stolzen Trabern gezogen,", "tokens": ["E\u00b7qui\u00b7pa\u00b7gen", "da\u00b7zwi\u00b7schen", ",", "von", "stol\u00b7zen", "Tra\u00b7bern", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Halten vor jedem Palast, wo man Besuche empf\u00e4ngt;", "tokens": ["Hal\u00b7ten", "vor", "je\u00b7dem", "Pa\u00b7last", ",", "wo", "man", "Be\u00b7su\u00b7che", "emp\u00b7f\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Stattliche Leute zu Fu\u00df vereint der gewohnte Spaziergang,", "tokens": ["Statt\u00b7li\u00b7che", "Leu\u00b7te", "zu", "Fu\u00df", "ver\u00b7eint", "der", "ge\u00b7wohn\u00b7te", "Spa\u00b7zier\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Wohlig in Pelze geh\u00fcllt, schreiten sie \u00fcber den Ring.", "tokens": ["Woh\u00b7lig", "in", "Pel\u00b7ze", "ge\u00b7h\u00fcllt", ",", "schrei\u00b7ten", "sie", "\u00fc\u00b7ber", "den", "Ring", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Aber vergn\u00fcglicher noch hineilen die Sch\u00f6nen zum Eisplatz,", "tokens": ["A\u00b7ber", "ver\u00b7gn\u00fcg\u00b7li\u00b7cher", "noch", "hi\u00b7nei\u00b7len", "die", "Sch\u00f6\u00b7nen", "zum", "Eis\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Wo der geschmeidige Wuchs sich am geschmeidigsten zeigt.", "tokens": ["Wo", "der", "ge\u00b7schmei\u00b7di\u00b7ge", "Wuchs", "sich", "am", "ge\u00b7schmei\u00b7digs\u00b7ten", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Knapp umschlie\u00dft ihn die w\u00e4rmende Jacke; auf braunen und blonden", "tokens": ["Knapp", "um\u00b7schlie\u00dft", "ihn", "die", "w\u00e4r\u00b7men\u00b7de", "Ja\u00b7cke", ";", "auf", "brau\u00b7nen", "und", "blon\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "APPR", "ADJA", "KON", "ADJA"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "H\u00e4uptern sitzen kokett M\u00fctzen mit Zobel verbr\u00e4mt.", "tokens": ["H\u00e4up\u00b7tern", "sit\u00b7zen", "ko\u00b7kett", "M\u00fct\u00b7zen", "mit", "Zo\u00b7bel", "ver\u00b7br\u00e4mt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Hui, wie fliegt sich's dahin auf leicht einritzendem Schlittschuh,", "tokens": ["Hui", ",", "wie", "fliegt", "sich's", "da\u00b7hin", "auf", "leicht", "ein\u00b7rit\u00b7zen\u00b7dem", "Schlitt\u00b7schuh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "PWAV", "VVFIN", "PIS", "PAV", "APPR", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Den mit bebender Hand kniend der J\u00fcngling geschnallt!", "tokens": ["Den", "mit", "be\u00b7ben\u00b7der", "Hand", "kni\u00b7end", "der", "J\u00fcng\u00b7ling", "ge\u00b7schnallt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "ART", "NN", "VVPP", "$."], "meter": "-----+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Sieh nur den zierlichen Reigen! Es trennen und flieh'n sich die Paare,", "tokens": ["Sieh", "nur", "den", "zier\u00b7li\u00b7chen", "Rei\u00b7gen", "!", "Es", "tren\u00b7nen", "und", "flieh'n", "sich", "die", "Paa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$.", "PPER", "VVINF", "KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Aber in reizendem Bug kehren sie wieder zur\u00fcck.", "tokens": ["A\u00b7ber", "in", "rei\u00b7zen\u00b7dem", "Bug", "keh\u00b7ren", "sie", "wie\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Liebliches Meiden und Finden \u2013 gemeinsam wonniges Kreisen,", "tokens": ["Lieb\u00b7li\u00b7ches", "Mei\u00b7den", "und", "Fin\u00b7den", "\u2013", "ge\u00b7mein\u00b7sam", "won\u00b7ni\u00b7ges", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$(", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Bis die D\u00e4mmerung webt um das lebendige Bild.", "tokens": ["Bis", "die", "D\u00e4m\u00b7me\u00b7rung", "webt", "um", "das", "le\u00b7ben\u00b7di\u00b7ge", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Aber da zuckt auch empor das elektrische Licht und umschimmert", "tokens": ["A\u00b7ber", "da", "zuckt", "auch", "em\u00b7por", "das", "e\u00b7lekt\u00b7ri\u00b7sche", "Licht", "und", "um\u00b7schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PTKVZ", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Magisch den spiegelnden Plan und die Gestalten darauf.", "tokens": ["Ma\u00b7gisch", "den", "spie\u00b7geln\u00b7den", "Plan", "und", "die", "Ge\u00b7stal\u00b7ten", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "ART", "NN", "PAV", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ach, wer entfernte sich jetzt? Erstarren die Finger im M\u00fcffchen,", "tokens": ["Ach", ",", "wer", "ent\u00b7fern\u00b7te", "sich", "jetzt", "?", "Er\u00b7star\u00b7ren", "die", "Fin\u00b7ger", "im", "M\u00fcf\u00b7fchen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PRF", "ADV", "$.", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Sp\u00fcrt auch das N\u00e4schen den Frost \u2013 lodert in Flammen das Herz.", "tokens": ["Sp\u00fcrt", "auch", "das", "N\u00e4\u00b7schen", "den", "Frost", "\u2013", "lo\u00b7dert", "in", "Flam\u00b7men", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$(", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sieh, schon wirbeln die Flocken um ragende D\u00e4cher; es sausen", "tokens": ["Sieh", ",", "schon", "wir\u00b7beln", "die", "Flo\u00b7cken", "um", "ra\u00b7gen\u00b7de", "D\u00e4\u00b7cher", ";", "es", "sau\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$.", "PPER", "VVINF"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Eisige Winde mit Macht durch die rings offene Stadt.", "tokens": ["Ei\u00b7si\u00b7ge", "Win\u00b7de", "mit", "Macht", "durch", "die", "rings", "of\u00b7fe\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Ja, der Winter ist da! Mit ihm erschienen die Freuden,", "tokens": ["Ja", ",", "der", "Win\u00b7ter", "ist", "da", "!", "Mit", "ihm", "er\u00b7schie\u00b7nen", "die", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADV", "$.", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Welche der St\u00e4dter schon l\u00e4ngs