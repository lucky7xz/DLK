"word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.104": {"text": "Es ist der HErr mein GOtt, er thue, was ihm lieb.", "tokens": ["Es", "ist", "der", "Herr", "mein", "Gott", ",", "er", "thue", ",", "was", "ihm", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.105": {"text": "So wird mir selbst der trost in meinen mund gegeben:", "tokens": ["So", "wird", "mir", "selbst", "der", "trost", "in", "mei\u00b7nen", "mund", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.106": {"text": "Ich seh die thr\u00e4nen schon in freuden-wein verkehrt,", "tokens": ["Ich", "seh", "die", "thr\u00e4\u00b7nen", "schon", "in", "freu\u00b7den\u00b7wein", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.107": {"text": "Weil der, so ewig stirbt, die thr\u00e4nen nur begehrt;", "tokens": ["Weil", "der", ",", "so", "e\u00b7wig", "stirbt", ",", "die", "thr\u00e4\u00b7nen", "nur", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.108": {"text": "Nicht aber ihr gemahl, der kan unsterblich leben.", "tokens": ["Nicht", "a\u00b7ber", "ihr", "ge\u00b7mahl", ",", "der", "kan", "uns\u00b7terb\u00b7lich", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "$,", "PRELS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}