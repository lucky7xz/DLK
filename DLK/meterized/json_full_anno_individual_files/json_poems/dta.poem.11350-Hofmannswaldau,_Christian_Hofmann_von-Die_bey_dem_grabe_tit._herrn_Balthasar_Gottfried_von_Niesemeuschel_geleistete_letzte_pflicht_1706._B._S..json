{"dta.poem.11350": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bey dem grabe  tit.  herrn Balthasar  \n Gottfried von Niesemeuschel geleistete  \n letzte pflicht 1706.  \n B. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hochseeliger! dein grab ist schon vorl\u00e4ngst gebaut;", "tokens": ["Hoch\u00b7see\u00b7li\u00b7ger", "!", "dein", "grab", "ist", "schon", "vor\u00b7l\u00e4ngst", "ge\u00b7baut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vier jahre haben dir den sarg bi\u00dfher gezimmert,", "tokens": ["Vier", "jah\u00b7re", "ha\u00b7ben", "dir", "den", "sarg", "bi\u00df\u00b7her", "ge\u00b7zim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ART", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da kaum ein schwacher blick die welt noch angeschimmert:", "tokens": ["Da", "kaum", "ein", "schwa\u00b7cher", "blick", "die", "welt", "noch", "an\u00b7ge\u00b7schim\u00b7mert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du sturbest tag f\u00fcr tag, nicht wie man sonnen schaut,", "tokens": ["Du", "stur\u00b7best", "tag", "f\u00fcr", "tag", ",", "nicht", "wie", "man", "son\u00b7nen", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,", "PTKNEG", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach ihrem untergang zur morgen-r\u00f6the kehren;", "tokens": ["Nach", "ih\u00b7rem", "un\u00b7ter\u00b7gang", "zur", "mor\u00b7gen\u00b7r\u00f6\u00b7the", "keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein balsam wolte sich nur nach und nach verzehren,", "tokens": ["Dein", "bal\u00b7sam", "wol\u00b7te", "sich", "nur", "nach", "und", "nach", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VMFIN", "PRF", "ADV", "APPR", "KON", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und deine kranckheit war prophete von der nacht,", "tokens": ["Und", "dei\u00b7ne", "kran\u00b7ck\u00b7heit", "war", "pro\u00b7phe\u00b7te", "von", "der", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "Die endlich in der grufft uns alle gleiche macht.", "tokens": ["Die", "end\u00b7lich", "in", "der", "grufft", "uns", "al\u00b7le", "glei\u00b7che", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "PPER", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So gieng dein edler geist schon durch die morschen glieder,", "tokens": ["So", "gieng", "dein", "ed\u00b7ler", "geist", "schon", "durch", "die", "mor\u00b7schen", "glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie ein geweyhter hauch, zu seiner quelle hin:", "tokens": ["Wie", "ein", "ge\u00b7weyh\u00b7ter", "hauch", ",", "zu", "sei\u00b7ner", "quel\u00b7le", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und also war bey dir der himmels-b\u00fcrger sinn,", "tokens": ["Und", "al\u00b7so", "war", "bey", "dir", "der", "him\u00b7mels\u00b7b\u00fcr\u00b7ger", "sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sterben, eh man stirbt, so sterben sie nicht wieder.", "tokens": ["Die", "ster\u00b7ben", ",", "eh", "man", "stirbt", ",", "so", "ster\u00b7ben", "sie", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$,", "KOUS", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kein wunder, wenn der leib der seelen lazareth,", "tokens": ["Kein", "wun\u00b7der", ",", "wenn", "der", "leib", "der", "see\u00b7len", "la\u00b7za\u00b7reth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df die bekerckerten sich aus den fesseln sehnen,", "tokens": ["Da\u00df", "die", "be\u00b7ker\u00b7cker\u00b7ten", "sich", "aus", "den", "fes\u00b7seln", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Wann ihnen tod und grufft den weg zur freyheit b\u00e4hnen,", "tokens": ["Wann", "ih\u00b7nen", "tod", "und", "grufft", "den", "weg", "zur", "frey\u00b7heit", "b\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "VVFIN", "ART", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Natur hegt eine gluth, die nach der h\u00f6he geht,", "tokens": ["Na\u00b7tur", "hegt", "ei\u00b7ne", "gluth", ",", "die", "nach", "der", "h\u00f6\u00b7he", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und heisset brunnen auch ins meer sich wieder st\u00fcrtzen:", "tokens": ["Und", "heis\u00b7set", "brun\u00b7nen", "auch", "ins", "meer", "sich", "wie\u00b7der", "st\u00fcrt\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "APPRART", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn lauter wermuth will des lebens kost verw\u00fcrtzen,", "tokens": ["Wenn", "lau\u00b7ter", "wer\u00b7muth", "will", "des", "le\u00b7bens", "kost", "ver\u00b7w\u00fcrt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "So leckt der matte hirsch nach einem frischen na\u00df:", "tokens": ["So", "leckt", "der", "mat\u00b7te", "hirsch", "nach", "ei\u00b7nem", "fri\u00b7schen", "na\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein schiff sticht durch die see, und folget dem compa\u00df:", "tokens": ["Ein", "schiff", "sticht", "durch", "die", "see", ",", "und", "fol\u00b7get", "dem", "com\u00b7pa\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der pilgrim flieht den wald, wo m\u00f6rder in den gruben:", "tokens": ["Der", "pil\u00b7grim", "flieht", "den", "wald", ",", "wo", "m\u00f6r\u00b7der", "in", "den", "gru\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der tauben fl\u00fcgel eilt, wenn es zur arche geht:", "tokens": ["Der", "tau\u00b7ben", "fl\u00fc\u00b7gel", "eilt", ",", "wenn", "es", "zur", "ar\u00b7che", "geht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es freut sich Jsrael, wenn es am Jordan steht:", "tokens": ["Es", "freut", "sich", "Js\u00b7rael", ",", "wenn", "es", "am", "Jor\u00b7dan", "steht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NE", "$,", "KOUS", "PPER", "APPRART", "NE", "VVFIN", "$."], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.24": {"text": "Und Loth geht jauchzende aus Sodom von den buben;", "tokens": ["Und", "Loth", "geht", "jauch\u00b7zen\u00b7de", "aus", "So\u00b7dom", "von", "den", "bu\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJA", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "So war dein sinn, dein hertz, du \u00fcberirrd\u2019scher geist!", "tokens": ["So", "war", "dein", "sinn", ",", "dein", "hertz", ",", "du", "\u00fc\u00b7ber\u00b7irrd'\u00b7scher", "geist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Schon l\u00e4ngst der todten welt und ihrer lust gestorben:", "tokens": ["Schon", "l\u00e4ngst", "der", "tod\u00b7ten", "welt", "und", "ih\u00b7rer", "lust", "ge\u00b7stor\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du hattest einen zug vom creutz-magnet erworben,", "tokens": ["Du", "hat\u00b7test", "ei\u00b7nen", "zug", "vom", "creutz\u00b7ma\u00b7gnet", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der immer \u00fcber sich die kr\u00e4ffte steigen heist.", "tokens": ["Der", "im\u00b7mer", "\u00fc\u00b7ber", "sich", "die", "kr\u00e4ff\u00b7te", "stei\u00b7gen", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PRF", "ART", "ADJA", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was auf der erden war, schien dir, wie glas-crystallen;", "tokens": ["Was", "auf", "der", "er\u00b7den", "war", ",", "schien", "dir", ",", "wie", "glas\u00b7cry\u00b7stal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PWAV", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Hingegen liessest du die sch\u00e4tze dir gefallen,", "tokens": ["Hin\u00b7ge\u00b7gen", "lies\u00b7sest", "du", "die", "sch\u00e4t\u00b7ze", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Die uns die ewigkeit in ihrer tieffe zeigt.", "tokens": ["Die", "uns", "die", "e\u00b7wig\u00b7keit", "in", "ih\u00b7rer", "tief\u00b7fe", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein mensch, der nur sein knie vor mammons throne beugt,", "tokens": ["Ein", "mensch", ",", "der", "nur", "sein", "knie", "vor", "mam\u00b7mons", "thro\u00b7ne", "beugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Stirbt freylich, wenn er stirbt, mit halb-gebrochnem hertzen;", "tokens": ["Stirbt", "frey\u00b7lich", ",", "wenn", "er", "stirbt", ",", "mit", "halb\u00b7ge\u00b7broch\u00b7nem", "hert\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dir war der abschied leicht, den du vorl\u00e4ngst gemacht,", "tokens": ["Dir", "war", "der", "ab\u00b7schied", "leicht", ",", "den", "du", "vor\u00b7l\u00e4ngst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Indem du alle welt so w\u00fcrdig nicht geacht,", "tokens": ["In\u00b7dem", "du", "al\u00b7le", "welt", "so", "w\u00fcr\u00b7dig", "nicht", "ge\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Des himmels theures gut dagegen zu verschertzen.", "tokens": ["Des", "him\u00b7mels", "theu\u00b7res", "gut", "da\u00b7ge\u00b7gen", "zu", "ver\u00b7schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Unsch\u00e4tzbarer gewinn! der nichts zu etwas macht:", "tokens": ["Un\u00b7sch\u00e4tz\u00b7ba\u00b7rer", "ge\u00b7winn", "!", "der", "nichts", "zu", "et\u00b7was", "macht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "PIS", "PTKA", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein reichthum, der uns mu\u00df auch in die grufft begleiten.", "tokens": ["Ein", "reicht\u00b7hum", ",", "der", "uns", "mu\u00df", "auch", "in", "die", "grufft", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Last heidnische vernunfft um einen balsam streiten,", "tokens": ["Last", "heid\u00b7ni\u00b7sche", "ver\u00b7nunfft", "um", "ei\u00b7nen", "bal\u00b7sam", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.40": {"text": "Dadurch das leben wird im sterben wiederbracht!", "tokens": ["Da\u00b7durch", "das", "le\u00b7ben", "wird", "im", "ster\u00b7ben", "wie\u00b7der\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PDS", "VVINF", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ein Christe lachet nur, wenn man Mausolens staube", "tokens": ["Ein", "Chris\u00b7te", "la\u00b7chet", "nur", ",", "wenn", "man", "Mau\u00b7so\u00b7lens", "stau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KOUS", "PIS", "NN", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Ein lebendiges grab in der gemahlin macht;", "tokens": ["Ein", "le\u00b7ben\u00b7di\u00b7ges", "grab", "in", "der", "ge\u00b7mah\u00b7lin", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Hier redet GOttes mund, und hier bekennt der glaube,", "tokens": ["Hier", "re\u00b7det", "Got\u00b7tes", "mund", ",", "und", "hier", "be\u00b7kennt", "der", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,", "KON", "ADV", "VVFIN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Da\u00df, wer in Christo stirbt, zum leben wird gebracht.", "tokens": ["Da\u00df", ",", "wer", "in", "Chris\u00b7to", "stirbt", ",", "zum", "le\u00b7ben", "wird", "ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "APPR", "NE", "VVFIN", "$,", "APPRART", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Der tod ist todt! O sieg, der mehr als palmen tr\u00e4get!", "tokens": ["Der", "tod", "ist", "todt", "!", "O", "sieg", ",", "der", "mehr", "als", "pal\u00b7men", "tr\u00e4\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "NE", "VVFIN", "$,", "PRELS", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Das grab heist nur die ruh, das sterben ein gewinn:", "tokens": ["Das", "grab", "heist", "nur", "die", "ruh", ",", "das", "ster\u00b7ben", "ein", "ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Der hafen bringet uns zur guten hoffnung hin:", "tokens": ["Der", "ha\u00b7fen", "brin\u00b7get", "uns", "zur", "gu\u00b7ten", "hoff\u00b7nung", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der leib wird in den sand zum schatze beygeleget.", "tokens": ["Der", "leib", "wird", "in", "den", "sand", "zum", "schat\u00b7ze", "bey\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "VVFIN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Noch mehr hat dich dein tod unsterblich hier gemacht:", "tokens": ["Noch", "mehr", "hat", "dich", "dein", "tod", "uns\u00b7terb\u00b7lich", "hier", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "w