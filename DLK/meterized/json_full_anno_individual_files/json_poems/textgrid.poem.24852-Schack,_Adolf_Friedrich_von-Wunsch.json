{"textgrid.poem.24852": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn uns von zitternder Wimper", "tokens": ["Wenn", "uns", "von", "zit\u00b7tern\u00b7der", "Wim\u00b7per"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Wonnez\u00e4hre tropft,", "tokens": ["Die", "Won\u00b7ne\u00b7z\u00e4h\u00b7re", "tropft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn bebend Lippe an Lippe h\u00e4ngt", "tokens": ["Wenn", "be\u00b7bend", "Lip\u00b7pe", "an", "Lip\u00b7pe", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "NN", "APPR", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und Ader an Ader klopft,", "tokens": ["Und", "A\u00b7der", "an", "A\u00b7der", "klopft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was kann uns die Erde noch bieten fortan,", "tokens": ["Was", "kann", "uns", "die", "Er\u00b7de", "noch", "bie\u00b7ten", "for\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Das matt nicht erbleichen mu\u00df?", "tokens": ["Das", "matt", "nicht", "er\u00b7blei\u00b7chen", "mu\u00df", "?"], "token_info": ["word", "word", "word", "w