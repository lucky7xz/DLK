{"textgrid.poem.66482": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcr w\u00fcrdigen Inhalt ging ich heute formsuchend", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr w\u00fcrdigen Inhalt ging ich heute formsuchend", "tokens": ["F\u00fcr", "w\u00fcr\u00b7di\u00b7gen", "In\u00b7halt", "ging", "ich", "heu\u00b7te", "form\u00b7su\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hoch in der Musen vollgestopfte Dachkammer,", "tokens": ["Hoch", "in", "der", "Mu\u00b7sen", "voll\u00b7ge\u00b7stopf\u00b7te", "Dach\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo die Modele stehn von allen Versma\u00dfen.", "tokens": ["Wo", "die", "Mo\u00b7de\u00b7le", "stehn", "von", "al\u00b7len", "Ver\u00b7sma\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Un\u00fcbersehbar schien der bunte Wirrwarr mir.", "tokens": ["Un\u00b7\u00fc\u00b7ber\u00b7seh\u00b7bar", "schien", "der", "bun\u00b7te", "Wirr\u00b7warr", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier standen der Antike Metra reih'nweise:", "tokens": ["Hier", "stan\u00b7den", "der", "An\u00b7ti\u00b7ke", "Me\u00b7tra", "reih'n\u00b7wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gewichtiger Chorges\u00e4nge schwere Grundrythmen,", "tokens": ["Ge\u00b7wich\u00b7ti\u00b7ger", "Chor\u00b7ge\u00b7s\u00e4n\u00b7ge", "schwe\u00b7re", "Grund\u00b7ryth\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+--", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dann Odenkrams verhundertfachte Nachbildung,", "tokens": ["Dann", "O\u00b7den\u00b7krams", "ver\u00b7hun\u00b7dert\u00b7fach\u00b7te", "Nach\u00b7bil\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Senare dort; daneben gleich, zur Abschreckung,", "tokens": ["Se\u00b7na\u00b7re", "dort", ";", "da\u00b7ne\u00b7ben", "gleich", ",", "zur", "Ab\u00b7schre\u00b7ckung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PAV", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+---+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Des zopfigen Alexandriners Pfuschmachwerk;", "tokens": ["Des", "zop\u00b7fi\u00b7gen", "A\u00b7le\u00b7xan\u00b7dri\u00b7ners", "Pfuschmach\u00b7werk", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Ganz Rom und Hellas, bis zum letzten Sechsf\u00fc\u00dfler.", "tokens": ["Ganz", "Rom", "und", "Hel\u00b7las", ",", "bis", "zum", "letz\u00b7ten", "Sechs\u00b7f\u00fc\u00df\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Nach diesem, mittelalterlich mit Vierstabung", "tokens": ["Nach", "die\u00b7sem", ",", "mit\u00b7tel\u00b7al\u00b7ter\u00b7lich", "mit", "Viers\u00b7ta\u00b7bung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.12": {"text": "Allitterirenden Urgesangs Granitbl\u00f6cke;", "tokens": ["Al\u00b7lit\u00b7te\u00b7ri\u00b7ren\u00b7den", "Ur\u00b7ge\u00b7sangs", "Gra\u00b7nit\u00b7bl\u00f6\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Dann Nibelungenstrophenbauwerks Spitzbogen,", "tokens": ["Dann", "Ni\u00b7be\u00b7lun\u00b7gen\u00b7stro\u00b7phen\u00b7bau\u00b7werks", "Spitz\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.14": {"text": "Voll gothisch hoch erhabner Heldensteinbildung;", "tokens": ["Voll", "go\u00b7thisch", "hoch", "er\u00b7hab\u00b7ner", "Hel\u00b7den\u00b7stein\u00b7bil\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Dazu der ganzen Minnedichtung Verskr\u00e4nzlein", "tokens": ["Da\u00b7zu", "der", "gan\u00b7zen", "Min\u00b7ne\u00b7dich\u00b7tung", "Ver\u00b7skr\u00e4nz\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Sprachfig\u00fcrchen in reizumbl\u00fchter Vielfachheit.", "tokens": ["Und", "Sprach\u00b7fi\u00b7g\u00fcr\u00b7chen", "in", "rei\u00b7zum\u00b7bl\u00fch\u00b7ter", "Viel\u00b7fach\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Dann kam handwerklichen Meistersangs Holzschnitzwerk,", "tokens": ["Dann", "kam", "hand\u00b7werk\u00b7li\u00b7chen", "Meis\u00b7ter\u00b7sangs", "Holz\u00b7schnitz\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Sehr eulenspiegelscheckig, aber grundehrsam,", "tokens": ["Sehr", "eu\u00b7len\u00b7spie\u00b7gel\u00b7sche\u00b7ckig", ",", "a\u00b7ber", "grun\u00b7dehr\u00b7sam", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durchg\u00e4ngig doch urv\u00e4term\u00f6belwurmstichig.", "tokens": ["Durch\u00b7g\u00e4n\u00b7gig", "doch", "ur\u00b7v\u00e4\u00b7ter\u00b7m\u00f6\u00b7bel\u00b7wurm\u00b7sti\u00b7chig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und immer mehr! Dort stehn in Renaissancegrazie", "tokens": ["Und", "im\u00b7mer", "mehr", "!", "Dort", "stehn", "in", "Re\u00b7nais\u00b7san\u00b7ce\u00b7gra\u00b7zie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$.", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Romanischen Geistes zierlich feine Formspiele.", "tokens": ["Ro\u00b7ma\u00b7ni\u00b7schen", "Geis\u00b7tes", "zier\u00b7lich", "fei\u00b7ne", "Form\u00b7spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Hier des Terzinengangs Reliefbild, friesartig", "tokens": ["Hier", "des", "Ter\u00b7zi\u00b7nen\u00b7gangs", "Re\u00b7lief\u00b7bild", ",", "fries\u00b7ar\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "NN", "$,", "ADJD"], "meter": "--+---+-+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Dahingedehnt:, dort der Ottaverime schalkhaftes", "tokens": ["Da\u00b7hin\u00b7ge\u00b7dehnt", ":", ",", "dort", "der", "Ot\u00b7ta\u00b7ve\u00b7ri\u00b7me", "schalk\u00b7haf\u00b7tes"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "$,", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Amorettenb\u00fcstenv\u00f6lkchen; endlich unz\u00e4hlbar", "tokens": ["A\u00b7mo\u00b7ret\u00b7ten\u00b7b\u00fcs\u00b7ten\u00b7v\u00f6l\u00b7kchen", ";", "end\u00b7lich", "un\u00b7z\u00e4hl\u00b7bar"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Sonnettengef\u00e4\u00df und Schalen h\u00f6chster Formfeinheit.", "tokens": ["Son\u00b7net\u00b7ten\u00b7ge\u00b7f\u00e4\u00df", "und", "Scha\u00b7len", "h\u00f6chs\u00b7ter", "Form\u00b7fein\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}, "line.26": {"text": "Sogar des maurischen Stils Gaselenlaubwerk war", "tokens": ["So\u00b7gar", "des", "mau\u00b7ri\u00b7schen", "Stils", "Ga\u00b7se\u00b7len\u00b7laub\u00b7werk", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "VAFIN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Einreimig ausgestellt zur Wahl nur. \u2013 Da stand ich,", "tokens": ["Ein\u00b7rei\u00b7mig", "aus\u00b7ge\u00b7stellt", "zur", "Wahl", "nur", ".", "\u2013", "Da", "stand", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und sah und sah, und weilte, g\u00e4nzlich unschl\u00fcssig,", "tokens": ["Und", "sah", "und", "sah", ",", "und", "weil\u00b7te", ",", "g\u00e4nz\u00b7lich", "un\u00b7schl\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "In welch Geh\u00e4us des tausendfachen Formreichthums", "tokens": ["In", "welch", "Ge\u00b7h\u00e4us", "des", "tau\u00b7send\u00b7fa\u00b7chen", "Form\u00b7reicht\u00b7hums"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich kneten sollte die Gedankenthonmasse", "tokens": ["Ich", "kne\u00b7ten", "soll\u00b7te", "die", "Ge\u00b7dan\u00b7ken\u00b7thon\u00b7mas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Zum Wiegenlied dem formverst\u00e4ndigen Kunstfreunde.", "tokens": ["Zum", "Wie\u00b7gen\u00b7lied", "dem", "form\u00b7ver\u00b7st\u00e4n\u00b7di\u00b7gen", "Kunst\u00b7freun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Bis endlich, unten tief im letzten Staubwinkel", "tokens": ["Bis", "end\u00b7lich", ",", "un\u00b7ten", "tief", "im", "letz\u00b7ten", "Staub\u00b7win\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.33": {"text": "Die m\u00e4chtige Form ich sah von Pindars Festhymnen.", "tokens": ["Die", "m\u00e4ch\u00b7ti\u00b7ge", "Form", "ich", "sah", "von", "Pin\u00b7dars", "Fes\u00b7thym\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Entschlossen, scheu' ich nicht das schwierige Durchkriechen", "tokens": ["Ent\u00b7schlos\u00b7sen", ",", "scheu'", "ich", "nicht", "das", "schwie\u00b7ri\u00b7ge", "Durch\u00b7krie\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Durch aufgestapelt tausendj\u00e4hrige Verstr\u00fcmmer,", "tokens": ["Durch", "auf\u00b7ge\u00b7sta\u00b7pelt", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ge", "Ver\u00b7str\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Hervorzuziehn den Fund. Da \u2013 bricht, der Hauptst\u00fctze", "tokens": ["Her\u00b7vor\u00b7zu\u00b7ziehn", "den", "Fund", ".", "Da", "\u2013", "bricht", ",", "der", "Haupt\u00b7st\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "KOUS", "$(", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.37": {"text": "Beraubt, von allen Brettern des Parna\u00dfgiebels", "tokens": ["Be\u00b7raubt", ",", "von", "al\u00b7len", "Bret\u00b7tern", "des", "Par\u00b7na\u00df\u00b7gie\u00b7bels"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Das ganze Formeninventar mit Staubwolken", "tokens": ["Das", "gan\u00b7ze", "For\u00b7me\u00b7nin\u00b7ven\u00b7tar", "mit", "Staub\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.39": {"text": "Und Krachen \u00fcber mir zusammen! Tod\u00e4hnlich", "tokens": ["Und", "Kra\u00b7chen", "\u00fc\u00b7ber", "mir", "zu\u00b7sam\u00b7men", "!", "Tod\u00b7\u00e4hn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "APPR", "PPER", "PTKVZ", "$.", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.40": {"text": "Erschrocken, halb erdr\u00fcckt, erstickt, voll Quetschungen,", "tokens": ["Er\u00b7schro\u00b7cken", ",", "halb", "er\u00b7dr\u00fcckt", ",", "er\u00b7stickt", ",", "voll", "Quet\u00b7schun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,", "VVPP", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Tauml' ich bei Seit', erreich' die Th\u00fcr, zur\u00fcckgreifend", "tokens": ["Tauml'", "ich", "bei", "Seit'", ",", "e