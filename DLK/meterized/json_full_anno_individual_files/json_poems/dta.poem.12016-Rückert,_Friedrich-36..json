{"dta.poem.12016": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist es denn, das du begreifst von Gott und Welt?", "tokens": ["Was", "ist", "es", "denn", ",", "das", "du", "be\u00b7greifst", "von", "Gott", "und", "Welt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht mehr als was und wie es in den Sinn dir f\u00e4llt.", "tokens": ["Nicht", "mehr", "als", "was", "und", "wie", "es", "in", "den", "Sinn", "dir", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOUS", "PIS", "KON", "PWAV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ihm gef\u00e4llt, das nimmt dein Sinn an ungestr\u00e4ubt;", "tokens": ["Was", "ihm", "ge\u00b7f\u00e4llt", ",", "das", "nimmt", "dein", "Sinn", "an", "un\u00b7ge\u00b7str\u00e4ubt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gegen das, was ihm misf\u00e4llt, ist er bet\u00e4ubt.", "tokens": ["Und", "ge\u00b7gen", "das", ",", "was", "ihm", "mis\u00b7f\u00e4llt", ",", "ist", "er", "be\u00b7t\u00e4ubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Die Weisen m\u00f6gen uns beweisen was sie wollen,", "tokens": ["Die", "Wei\u00b7sen", "m\u00f6\u00b7gen", "uns", "be\u00b7wei\u00b7sen", "was", "sie", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erweisen mu\u00df sichs uns, wenn wir es glauben sollen.", "tokens": ["Er\u00b7wei\u00b7sen", "mu\u00df", "sichs", "uns", ",", "wenn", "wir", "es", "glau\u00b7ben", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PPER", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}