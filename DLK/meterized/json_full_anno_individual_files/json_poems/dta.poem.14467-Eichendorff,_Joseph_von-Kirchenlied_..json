{"dta.poem.14467": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Kirchenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Deinen Jesus in den Armen", "tokens": ["Dei\u00b7nen", "Je\u00b7sus", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber'n Strom der Zeit gestellt,", "tokens": ["Ue\u00b7ber'n", "Strom", "der", "Zeit", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als das himmlische Erbarmen", "tokens": ["Als", "das", "himm\u00b7li\u00b7sche", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fctest Du getreu die Welt,", "tokens": ["H\u00fc\u00b7test", "Du", "ge\u00b7treu", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df im Sturm, der tr\u00fcbe weht,", "tokens": ["Da\u00df", "im", "Sturm", ",", "der", "tr\u00fc\u00b7be", "weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir kein Kind verloren geht.", "tokens": ["Dir", "kein", "Kind", "ver\u00b7lo\u00b7ren", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}