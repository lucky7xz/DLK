 "measure": "trochaic.tetra"}, "line.25": {"text": "Auch den Liebling nicht. Verstand ich den singenden Seher;", "tokens": ["Auch", "den", "Lieb\u00b7ling", "nicht", ".", "Ver\u00b7stand", "ich", "den", "sin\u00b7gen\u00b7den", "Se\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$.", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "O so sprang ich auf, und entfloh.", "tokens": ["O", "so", "sprang", "ich", "auf", ",", "und", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Ach jetzt wurde nicht Bach, nicht Baum; ich wurd' einathmend,", "tokens": ["Ach", "jetzt", "wur\u00b7de", "nicht", "Bach", ",", "nicht", "Baum", ";", "ich", "wurd'", "ei\u00b7nath\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PTKNEG", "NE", "$,", "PTKNEG", "NE", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Jener frohen V\u00f6gelchen Grab!", "tokens": ["Je\u00b7ner", "fro\u00b7hen", "V\u00f6\u00b7gel\u00b7chen", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.29": {"text": "Aber nun sang sie auch, wie sie nie gesungen, mein Liebling,", "tokens": ["A\u00b7ber", "nun", "sang", "sie", "auch", ",", "wie", "sie", "nie", "ge\u00b7sun\u00b7gen", ",", "mein", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "