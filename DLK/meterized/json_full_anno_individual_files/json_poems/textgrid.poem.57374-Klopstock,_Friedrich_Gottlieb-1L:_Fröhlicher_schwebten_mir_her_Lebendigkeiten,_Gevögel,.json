{"textgrid.poem.57374": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00f6hlicher schwebten mir her Lebendigkeiten, Gev\u00f6gel,", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00f6hlicher schwebten mir her Lebendigkeiten, Gev\u00f6gel,", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "schweb\u00b7ten", "mir", "her", "Le\u00b7ben\u00b7dig\u00b7kei\u00b7ten", ",", "Ge\u00b7v\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "NN", "$,", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Oder Gew\u00fcrm,", "tokens": ["O\u00b7der", "Ge\u00b7w\u00fcrm", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Welche das Auge nicht sieht, so den hohen Sinus funkeln", "tokens": ["Wel\u00b7che", "das", "Au\u00b7ge", "nicht", "sieht", ",", "so", "den", "ho\u00b7hen", "Si\u00b7nus", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.4": {"text": "Sieht, und des Himmels weisslichen Pfad:", "tokens": ["Sieht", ",", "und", "des", "Him\u00b7mels", "weiss\u00b7li\u00b7chen", "Pfad", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch ersp\u00e4ht's durch der Kunst Kristalle diess den Atomen", "tokens": ["Doch", "er\u00b7sp\u00e4ht's", "durch", "der", "Kunst", "Kris\u00b7tal\u00b7le", "diess", "den", "A\u00b7to\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Nahverwandte Gew\u00fcrm.", "tokens": ["Nah\u00b7ver\u00b7wand\u00b7te", "Ge\u00b7w\u00fcrm", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Solche geheime Leben umwimmelten jetzt mich; es waren", "tokens": ["Sol\u00b7che", "ge\u00b7hei\u00b7me", "Le\u00b7ben", "um\u00b7wim\u00b7mel\u00b7ten", "jetzt", "mich", ";", "es", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "PPER", "$.", "PPER", "VAFIN"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Unter dem lieblichen Heer", "tokens": ["Un\u00b7ter", "dem", "lieb\u00b7li\u00b7chen", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Bienchen auch, und T\u00e4ubchen, und Untrenbare, Johannes-", "tokens": ["Bien\u00b7chen", "auch", ",", "und", "T\u00e4ub\u00b7chen", ",", "und", "Un\u00b7tren\u00b7ba\u00b7re", ",", "Jo\u00b7han\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Wurmchen, Schw\u00e4nchen, ein Chor", "tokens": ["Wurm\u00b7chen", ",", "Schw\u00e4n\u00b7chen", ",", "ein", "Chor"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Philomelen. An einer sich h\u00f6hlenden, sterbenden Ulme", "tokens": ["Phi\u00b7lo\u00b7me\u00b7len", ".", "An", "ei\u00b7ner", "sich", "h\u00f6h\u00b7len\u00b7den", ",", "ster\u00b7ben\u00b7den", "Ul\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "APPR", "ART", "PRF", "ADJD", "$,", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Sass ich, und am rieselnden Bach;", "tokens": ["Sass", "ich", ",", "und", "am", "rie\u00b7seln\u00b7den", "Bach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "H\u00f6rete horchend dem Bache, der Nachtigall horchender. Jetzo", "tokens": ["H\u00f6\u00b7re\u00b7te", "hor\u00b7chend", "dem", "Ba\u00b7che", ",", "der", "Nach\u00b7ti\u00b7gall", "hor\u00b7chen\u00b7der", ".", "Jet\u00b7zo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$.", "NE"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Schwebte n\u00e4her die wimmelnde Schaar,", "tokens": ["Schweb\u00b7te", "n\u00e4\u00b7her", "die", "wim\u00b7meln\u00b7de", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Schwebte (das wusste sie nicht) zu dem Grabe! Die Schl\u00fcnde des Baumes", "tokens": ["Schweb\u00b7te", "(", "das", "wuss\u00b7te", "sie", "nicht", ")", "zu", "dem", "Gra\u00b7be", "!", "Die", "Schl\u00fcn\u00b7de", "des", "Bau\u00b7mes"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$(", "APPR", "ART", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Dufteten Pest, der st\u00fcrzende Bach", "tokens": ["Duf\u00b7te\u00b7ten", "Pest", ",", "der", "st\u00fcr\u00b7zen\u00b7de", "Bach"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Wagte Tod; und wie nah war ihnen der webende Heerzug,", "tokens": ["Wag\u00b7te", "Tod", ";", "und", "wie", "nah", "war", "ih\u00b7nen", "der", "we\u00b7ben\u00b7de", "Heer\u00b7zug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PWAV", "ADJD", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Welcher, immer gewendet, sich nun,", "tokens": ["Wel\u00b7cher", ",", "im\u00b7mer", "ge\u00b7wen\u00b7det", ",", "sich", "nun", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "$,", "ADV", "VVPP", "$,", "PRF", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Schnell wie der Wink, herwirbelte, dann sich fernte. Wie leicht ach", "tokens": ["Schnell", "wie", "der", "Wink", ",", "her\u00b7wir\u00b7bel\u00b7te", ",", "dann", "sich", "fern\u00b7te", ".", "Wie", "leicht", "ach"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "VVFIN", "$,", "ADV", "PRF", "VVFIN", "$.", "PWAV", "ADJD", "XY"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Konten ihm Grab", "tokens": ["Kon\u00b7ten", "ihm", "Grab"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Werden des Baches, oder des Baums Abgr\u00fcnde! Mich hatten", "tokens": ["Wer\u00b7den", "des", "Ba\u00b7ches", ",", "o\u00b7der", "des", "Baums", "Ab\u00b7gr\u00fcn\u00b7de", "!", "Mich", "hat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "NN", "$.", "PPER", "VAFIN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Tr\u00e4ume der dunkeln Pforte geweckt;", "tokens": ["Tr\u00e4u\u00b7me", "der", "dun\u00b7keln", "Pfor\u00b7te", "ge\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Doch ich vergass der guten. Die Nachtigall schmettert', als ob sie", "tokens": ["Doch", "ich", "ver\u00b7gass", "der", "gu\u00b7ten", ".", "Die", "Nach\u00b7ti\u00b7gall", "schmet\u00b7tert'", ",", "als", "ob", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "$.", "ART", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "PPER"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "Warnte; allein ich verstand", "tokens": ["Warn\u00b7te", ";", "al\u00b7lein", "ich", "ver\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Auch den Liebling nicht. Verstand ich den singenden Seher;", "tokens": ["Auch", "den", "Lieb\u00b7ling", "nicht", ".", "Ver\u00b7stand", "ich", "den", "sin\u00b7gen\u00b7den", "Se\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$.", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "O so sprang ich auf, und entfloh.", "tokens": ["O", "so", "sprang", "ich", "auf", ",", "und", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Ach jetzt wurde nicht Bach, nicht Baum; ich wurd' einathmend,", "tokens": ["Ach", "jetzt", "wur\u00b7de", "nicht", "Bach", ",", "nicht", "Baum", ";", "ich", "wurd'", "ei\u00b7nath\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PTKNEG", "NE", "$,", "PTKNEG", "NE", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Jener frohen V\u00f6gelchen Grab!", "tokens": ["Je\u00b7ner", "fro\u00b7hen", "V\u00f6\u00b7gel\u00b7chen", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.29": {"text": "Aber nun sang sie auch, wie sie nie gesungen, mein Liebling,", "tokens": ["A\u00b7ber", "nun", "sang", "sie", "auch", ",", "wie", "sie", "nie", "ge\u00b7sun\u00b7gen", ",", "mein", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Fl\u00f6tete Wehmuth, wie sie,", "tokens": ["Fl\u00f6\u00b7te\u00b7te", "Weh\u00b7muth", ",", "wie", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.31": {"text": "Selber als Mutter, nie nicht gefl\u00f6tet, wenn noch die Feder", "tokens": ["Sel\u00b7ber", "als", "Mut\u00b7ter", ",", "nie", "nicht", "ge\u00b7fl\u00f6\u00b7tet", ",", "wenn", "noch", "die", "Fe\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "$,", "ADV", "PTKNEG", "VVPP", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "+--+-+--+-++-+-", "measure": "iambic.septa.invert"}, "line.32": {"text": "Flog, und der Geyer vom Blute noch trof.", "tokens": ["Flog", ",", "und", "der", "Ge\u00b7yer", "vom", "Blu\u00b7te", "noch", "trof", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ART", "NN", "APPRART", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.33": {"text": "Ungl\u00fcckseliger! z\u00fcrnte sie mir, dir weint' an der Lippe", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ger", "!", "z\u00fcrn\u00b7te", "sie", "mir", ",", "dir", "weint'", "an", "der", "Lip\u00b7pe"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+----+-+-+-+-+-", "measure": "dactylic.init"}, "line.34": {"text": "Wehklag'; und du h\u00f6rtest nicht hin!", "tokens": ["Weh\u00b7klag'", ";", "und", "du", "h\u00f6r\u00b7test", "nicht", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.35": {"text": "Weh dir! Sterbegesang der Philomelchen erscholl dir;", "tokens": ["Weh", "dir", "!", "Ster\u00b7be\u00b7ge\u00b7sang", "der", "Phi\u00b7lo\u00b7mel\u00b7chen", "er\u00b7scholl", "dir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Und du athmetest, athmetest fort!", "tokens": ["Und", "du", "ath\u00b7me\u00b7test", ",", "ath\u00b7me\u00b7test", "fort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Fr\u00f6hlicher schwebten mir her Lebendigkeiten, Gev\u00f6gel,", "tokens": ["Fr\u00f6h\u00b7li\u00b7cher", "schweb\u00b7ten", "mir", "her", "Le\u00b7ben\u00b7dig\u00b7kei\u00b7ten", ",", "Ge\u00b7v\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "NN", "$,", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Oder Gew\u00fcrm,", "tokens": ["O\u00b7der", "Ge\u00b7w\u00fcrm", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Welche das Auge nicht sieht, so den hohen Sinus funkeln", "tokens": ["Wel\u00b7che", "das", "Au\u00b7ge", "nicht", "sieht", ",", "so", "den", "ho\u00b7hen", "Si\u00b7nus", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.4": {"text": "Sieht, und des Himmels weisslichen Pfad:", "tokens": ["Sieht", ",", "und", "des", "Him\u00b7mels", "weiss\u00b7li\u00b7chen", "Pfad", ":"], "token_info": ["word", "punct", "wor