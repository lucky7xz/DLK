{"textgrid.poem.48024": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war zur schimmernden Maienzeit,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war zur schimmernden Maienzeit,", "tokens": ["Das", "war", "zur", "schim\u00b7mern\u00b7den", "Mai\u00b7en\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "da sang ich Lieder voll Lust und Leid:", "tokens": ["da", "sang", "ich", "Lie\u00b7der", "voll", "Lust", "und", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "des Waldquells Rauschen, der V\u00f6gel Singen,", "tokens": ["des", "Wald\u00b7quells", "Rau\u00b7schen", ",", "der", "V\u00f6\u00b7gel", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "in t\u00f6nende Reime t\u00e4t ich's bringen.", "tokens": ["in", "t\u00f6\u00b7nen\u00b7de", "Rei\u00b7me", "t\u00e4t", "ich's", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wenn ich der kommenden Lust gedacht \u2013", "tokens": ["Und", "wenn", "ich", "der", "kom\u00b7men\u00b7den", "Lust", "ge\u00b7dacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word