{"textgrid.poem.37462": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Jungfern gibt es in Dorf und Stadt,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Jungfern gibt es in Dorf und Stadt,", "tokens": ["Zwei", "Jung\u00b7fern", "gibt", "es", "in", "Dorf", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie leben best\u00e4ndig im Kriege,", "tokens": ["Sie", "le\u00b7ben", "be\u00b7st\u00e4n\u00b7dig", "im", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Wahrheit, die niemand gerne hat,", "tokens": ["Die", "Wahr\u00b7heit", ",", "die", "nie\u00b7mand", "ger\u00b7ne", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die scharmante L\u00fcge.", "tokens": ["Und", "die", "schar\u00b7man\u00b7te", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Vor jener, weil sie stolz und pr\u00fcd", "tokens": ["Vor", "je\u00b7ner", ",", "weil", "sie", "stolz", "und", "pr\u00fcd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "$,", "KOUS", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und voll moralischer N\u00fccken,", "tokens": ["Und", "voll", "mo\u00b7ra\u00b7li\u00b7scher", "N\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sucht jeder, der sie nur kommen sieht,", "tokens": ["Sucht", "je\u00b7der", ",", "der", "sie", "nur", "kom\u00b7men", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich schleunigst wegzudr\u00fccken.", "tokens": ["Sich", "schleu\u00b7nigst", "weg\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die andre, obwohl ihr nicht zu traun,", "tokens": ["Die", "and\u00b7re", ",", "ob\u00b7wohl", "ihr", "nicht", "zu", "traun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird t\u00e4glich beliebter und kecker,", "tokens": ["Wird", "t\u00e4g\u00b7lich", "be\u00b7lieb\u00b7ter", "und", "ke\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und wenn wir sie von hinten beschaun,", "tokens": ["Und", "wenn", "wir", "sie", "von", "hin\u00b7ten", "be\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So hat sie einen H\u00f6cker.", "tokens": ["So", "hat", "sie", "ei\u00b7nen", "H\u00f6\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zwei Jungfern gibt es in Dorf und Stadt,", "tokens": ["Zwei", "Jung\u00b7fern", "gibt", "es", "in", "Dorf", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie leben best\u00e4ndig im Kriege,", "tokens": ["Sie", "le\u00b7ben", "be\u00b7st\u00e4n\u00b7dig", "im", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Wahrheit, die niemand gerne hat,", "tokens": ["Die", "Wahr\u00b7heit", ",", "die", "nie\u00b7mand", "ger\u00b7ne", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die scharmante L\u00fcge.", "tokens": ["Und", "die", "schar\u00b7man\u00b7te", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Vor jener, weil sie stolz und pr\u00fcd", "tokens": ["Vor", "je\u00b7ner", ",", "weil", "sie", "stolz", "und", "pr\u00fcd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "$,", "KOUS", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "me