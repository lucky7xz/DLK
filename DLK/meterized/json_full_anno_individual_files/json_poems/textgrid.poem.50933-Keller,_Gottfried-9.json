, "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn wir durch die Lande schweifen;", "tokens": ["Wenn", "wir", "durch", "die", "Lan\u00b7de", "schwei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glauben nichts, als was wir sehn", "tokens": ["Glau\u00b7ben", "nichts", ",", "als", "was", "wir", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PRELS", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit unsern Sinnen greifen!", "tokens": ["Und", "mit", "un\u00b7sern", "Sin\u00b7nen", "grei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Halten nichts auf hohlen Dunst,", "tokens": ["Hal\u00b7ten", "nichts", "auf", "hoh\u00b7len", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6gen nichts auf Worte geben;", "tokens": ["M\u00f6\u00b7gen", "nichts", "auf", "Wor\u00b7te", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch verstehen wir die Kunst,", "tokens": ["Doch", "ver\u00b7ste\u00b7hen", "wir", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.te