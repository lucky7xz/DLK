{"textgrid.poem.32805": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit in der N\u00e4h' ich mein H\u00e4nschen gesehn,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit in der N\u00e4h' ich mein H\u00e4nschen gesehn,", "tokens": ["Seit", "in", "der", "N\u00e4h'", "ich", "mein", "H\u00e4n\u00b7schen", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Deucht mir kein anderer Bauer mehr sch\u00f6n;", "tokens": ["Deucht", "mir", "kein", "an\u00b7de\u00b7rer", "Bau\u00b7er", "mehr", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Blauere Augen, und r\u00f6tere Wangen,", "tokens": ["Blau\u00b7e\u00b7re", "Au\u00b7gen", ",", "und", "r\u00f6\u00b7te\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Z\u00e4hne, die wei\u00dfer und lieblicher prangen,", "tokens": ["Z\u00e4h\u00b7ne", ",", "die", "wei\u00b7\u00dfer", "und", "lieb\u00b7li\u00b7cher", "pran\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "KON", "ADJD", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Halten kein M\u00e4dchen im Dorfe gefangen.", "tokens": ["Hal\u00b7ten", "kein", "M\u00e4d\u00b7chen", "im", "Dor\u00b7fe", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Morgens, und abends gedenk' ich an ihn,", "tokens": ["Mor\u00b7gens", ",", "und", "a\u00b7bends", "ge\u00b7denk'", "ich", "an", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wandle zum Spiegel wohl zehenmal hin;", "tokens": ["Wand\u00b7le", "zum", "Spie\u00b7gel", "wohl", "ze\u00b7hen\u00b7mal", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lege bald dieses, bald jenes zurechte;", "tokens": ["Le\u00b7ge", "bald", "die\u00b7ses", ",", "bald", "je\u00b7nes", "zu\u00b7rech\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PDAT", "$,", "ADV", "PDS", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Frage mich, wenn ich die Haare mir flechte,", "tokens": ["Fra\u00b7ge", "mich", ",", "wenn", "ich", "die", "Haa\u00b7re", "mir", "flech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Ob ich vor andern gefallen ihm m\u00f6chte?", "tokens": ["Ob", "ich", "vor", "an\u00b7dern", "ge\u00b7fal\u00b7len", "ihm", "m\u00f6ch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct