{"dta.poem.20333": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Floriden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich qv\u00e4lte neulich mich in meinen krancken sinnen/", "tokens": ["Ich", "qv\u00e4l\u00b7te", "neu\u00b7lich", "mich", "in", "mei\u00b7nen", "kran\u00b7cken", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die augen stunden mir voll wasser und voll glut/", "tokens": ["Die", "au\u00b7gen", "stun\u00b7den", "mir", "voll", "was\u00b7ser", "und", "voll", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ADJA", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die zunge zw\u00e4ngte sich/ ihr klagen zu beginnen/", "tokens": ["Die", "zun\u00b7ge", "zw\u00e4ng\u00b7te", "sich", "/", "ihr", "kla\u00b7gen", "zu", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "PPER", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch war der athem nicht viel besser als der muth.", "tokens": ["Doch", "war", "der", "at\u00b7hem", "nicht", "viel", "bes\u00b7ser", "als", "der", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den himmel schaut ich an/ von dem mein leiden kommen/", "tokens": ["Den", "him\u00b7mel", "schaut", "ich", "an", "/", "von", "dem", "mein", "lei\u00b7den", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "APPR", "ART", "PPOSAT", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dann den reinen leib/ der mich zum sclaven macht/", "tokens": ["Und", "dann", "den", "rei\u00b7nen", "leib", "/", "der", "mich", "zum", "scla\u00b7ven", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ob ich mir gleich viel zu sagen f\u00fcrgenommen/", "tokens": ["Und", "ob", "ich", "mir", "gleich", "viel", "zu", "sa\u00b7gen", "f\u00fcr\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ward es endlich doch in diese reime bracht:", "tokens": ["So", "ward", "es", "end\u00b7lich", "doch", "in", "die\u00b7se", "rei\u00b7me", "bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier ist ein reiner geist/ von reiner zucht entz\u00fcndet/", "tokens": ["Hier", "ist", "ein", "rei\u00b7ner", "geist", "/", "von", "rei\u00b7ner", "zucht", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem weder schein noch schmuck hat eine brunst erregt.", "tokens": ["Dem", "we\u00b7der", "schein", "noch", "schmuck", "hat", "ei\u00b7ne", "brunst", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "ADJD", "ADV", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der seine hoffnung blo\u00df auff deine tugend gr\u00fcndet/", "tokens": ["Der", "sei\u00b7ne", "hoff\u00b7nung", "blo\u00df", "auff", "dei\u00b7ne", "tu\u00b7gend", "gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und seinen gantzen sinn zu deinen f\u00fcssen legt.", "tokens": ["Und", "sei\u00b7nen", "gant\u00b7zen", "sinn", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wei\u00df/ du liebest nicht von liebe viel zu h\u00f6ren/", "tokens": ["Ich", "wei\u00df", "/", "du", "lie\u00b7best", "nicht", "von", "lie\u00b7be", "viel", "zu", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "APPR", "VVFIN", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil thorheit ingemein der liebe richtschnur ist;", "tokens": ["Weil", "thor\u00b7heit", "in\u00b7ge\u00b7mein", "der", "lie\u00b7be", "richt\u00b7schnur", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch glaube/ Florida/ ich will dich nicht beth\u00f6ren/", "tokens": ["Doch", "glau\u00b7be", "/", "Flo\u00b7ri\u00b7da", "/", "ich", "will", "dich", "nicht", "be\u00b7th\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "NE", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So wenig als ich mir die thorheit auserkiest.", "tokens": ["So", "we\u00b7nig", "als", "ich", "mir", "die", "thor\u00b7heit", "aus\u00b7er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich liebe keinen schmuck/ ich ehre keine seide/", "tokens": ["Ich", "lie\u00b7be", "kei\u00b7nen", "schmuck", "/", "ich", "eh\u00b7re", "kei\u00b7ne", "sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie ist der w\u00fcrmer werck/ und auch der w\u00fcrmer kost/", "tokens": ["Sie", "ist", "der", "w\u00fcr\u00b7mer", "werck", "/", "und", "auch", "der", "w\u00fcr\u00b7mer", "kost", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein auge sehnt sich nicht so sehr nach einem kleide/", "tokens": ["Mein", "au\u00b7ge", "sehnt", "sich", "nicht", "so", "sehr", "nach", "ei\u00b7nem", "klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "ADV", "ADV", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was gold und perlen sind/ ist mir genung bewust.", "tokens": ["Was", "gold", "und", "per\u00b7len", "sind", "/", "ist", "mir", "ge\u00b7nung", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVINF", "VAFIN", "$(", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Dein reden ohne falsch hat meinen sinn gebunden/", "tokens": ["Dein", "re\u00b7den", "oh\u00b7ne", "falsch", "hat", "mei\u00b7nen", "sinn", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "APPR", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein schertzen ohne list legt mir die f\u00e4ssel an;", "tokens": ["Dein", "schert\u00b7zen", "oh\u00b7ne", "list", "legt", "mir", "die", "f\u00e4s\u00b7sel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die reine lieblichkeit/ so ich bey dir gefunden/", "tokens": ["Die", "rei\u00b7ne", "lieb\u00b7lich\u00b7keit", "/", "so", "ich", "bey", "dir", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Macht da\u00df ich Florida nicht wohl verlassen kan.", "tokens": ["Macht", "da\u00df", "ich", "Flo\u00b7ri\u00b7da", "nicht", "wohl", "ver\u00b7las\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "NE", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Bedencke/ was du thust/ und dencke/ da\u00df die flammen", "tokens": ["Be\u00b7den\u00b7cke", "/", "was", "du", "thust", "/", "und", "den\u00b7cke", "/", "da\u00df", "die", "flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "PWS", "PPER", "VVFIN", "$(", "KON", "VVFIN", "$(", "KOUS", "ART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So reine sind wie du/ von der sie kommen sind;", "tokens": ["So", "rei\u00b7ne", "sind", "wie", "du", "/", "von", "der", "sie", "kom\u00b7men", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "KOKOM", "PPER", "$(", "APPR", "PRELS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "La\u00df endlich blick und blick/ und ku\u00df und ku\u00df zusammen/", "tokens": ["La\u00df", "end\u00b7lich", "blick", "und", "blick", "/", "und", "ku\u00df", "und", "ku\u00df", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch da\u00df die keuschheit stets sich zum geferden find.", "tokens": ["Doch", "da\u00df", "die", "keuschheit", "stets", "sich", "zum", "ge\u00b7fer\u00b7den", "find", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PRF", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.29": {"text": "Ein ku\u00df der ist mein ziel/ und meines wunsches ende;", "tokens": ["Ein", "ku\u00df", "der", "ist", "mein", "ziel", "/", "und", "mei\u00b7nes", "wun\u00b7sches", "en\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mehr schreib ich itzo nicht/ mich rufft die s\u00fcsse ruh.", "tokens": ["Mehr", "schreib", "ich", "it\u00b7zo", "nicht", "/", "mich", "rufft", "die", "s\u00fcs\u00b7se", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Immittelst k\u00fc\u00df ich dir die wollen-weiche h\u00e4nde/", "tokens": ["Im\u00b7mit\u00b7telst", "k\u00fc\u00df", "ich", "dir", "die", "wol\u00b7len\u00b7wei\u00b7che", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und schliesse diesen reim/ doch nicht die hoffnung zu.", "tokens": ["Und", "schlies\u00b7se", "die\u00b7sen", "reim", "/", "doch", "nicht", "die", "hoff\u00b7nung", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$(", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}