, ",", "noch", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig werde scheiden,", "tokens": ["E\u00b7wig", "wer\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und da\u00df ihrer Tr\u00fcbsahl Lohn", "tokens": ["Und", "da\u00df", "ih\u00b7rer", "Tr\u00fcb\u00b7sahl", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sey die Krohn", "tokens": ["Sey", "die", "Krohn"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Aller ewgen Freuden.", "tokens": ["Al\u00b7ler", "ew\u00b7gen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "S\u00fcnden-Pracht und Gl\u00fcckes-Schein", "tokens": ["S\u00fcn\u00b7den\u00b7Pracht", "und", "Gl\u00fc\u00b7ckes\u00b7Schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt sie Himmlisch gro\u00df mit F\u00fcssen,", "tokens": ["Tritt", "sie", "Himm\u00b7lisch", "gro\u00df", "mit", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist an Lieb und Glauben rein,", "tokens": ["Ist", "an", "Lieb", "und", "Glau\u00b7ben", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilig am Gewissen,", "tokens": ["Hei\u00b7lig", "am", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Darumb Fried und Freud im Geist,", "tokens": ["Da\u00b7rumb", "Fried", "und", "Freud", "im", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Allermeist", "tokens": ["Al\u00b7ler\u00b7meist"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sie bedienen m\u00fcssen.", "tokens": ["Sie", "be\u00b7die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Herr wenn nimbst du mich von mir", "tokens": ["Herr", "wenn", "nimbst", "du", "mich", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ADV", "PPER", "PRF", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erwehlst mich f\u00fcr den deinen,", "tokens": ["Und", "er\u00b7wehlst", "mich", "f\u00fcr", "den", "dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich mag in heilger Zier,", "tokens": ["Da\u00df", "ich", "mag", "in", "heil\u00b7ger", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets vor dir erscheinen", "tokens": ["Stets", "vor", "dir", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und dich, O mein Eigenthumb,", "tokens": ["Und", "dich", ",", "O", "mein", "Ei\u00b7gen\u00b7thumb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiederumb", "tokens": ["Wie\u00b7de\u00b7rumb"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Halte f\u00fcr den meinen?", "tokens": ["Hal\u00b7te", "f\u00fcr", "den", "mei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Meine krancke Seel ist matt", "tokens": ["Mei\u00b7ne", "kran\u00b7cke", "Seel", "ist", "matt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verk\u00f6mbt gantz f\u00fcr Verlangen,", "tokens": ["Und", "ver\u00b7k\u00f6mbt", "gantz", "f\u00fcr", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen Kummer, den sie hat", "tokens": ["Al\u00b7len", "Kum\u00b7mer", ",", "den", "sie", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nur dich zu fangen,", "tokens": ["Ist", "nur", "dich", "zu", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und von Welt und S\u00fcnden lo\u00df,", "tokens": ["Und", "von", "Welt", "und", "S\u00fcn\u00b7den", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir stets blo\u00df", "tokens": ["Dir", "stets", "blo\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Br\u00fcnstig anzuhangen.", "tokens": ["Br\u00fcns\u00b7tig", "an\u00b7zu\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.21": {"line.1": {"text": "La\u00df mein Hort ohn' unterla\u00df", "tokens": ["La\u00df", "mein", "Hort", "ohn'", "un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich mit dir vereinigt leben,", "tokens": ["Mich", "mit", "dir", "ver\u00b7ei\u00b7nigt", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirck in mir der Erden Ha\u00df,", "tokens": ["Wirck", "in", "mir", "der", "Er\u00b7den", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich dir ergeben:", "tokens": ["Da\u00df", "ich", "dir", "er\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Keine Lust, darauf die Welt", "tokens": ["Kei\u00b7ne", "Lust", ",", "da\u00b7rauf", "die", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Etwas helt,", "tokens": ["Et\u00b7was", "helt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "In mir lasse schweben.", "tokens": ["In", "mir", "las\u00b7se", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.22": {"line.1": {"text": "Da\u00df ich hab' in Lieb' und Noht", "tokens": ["Da\u00df", "ich", "hab'", "in", "Lieb'", "und", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blo\u00df an dir die h\u00f6chste Freude,", "tokens": ["Blo\u00df", "an", "dir", "die", "h\u00f6chs\u00b7te", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kranckheit, Bl\u00f6sse, Schmach und Todt", "tokens": ["Kran\u00b7ck\u00b7heit", ",", "Bl\u00f6s\u00b7se", ",", "Schmach", "und", "Todt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gern' und willig leide,", "tokens": ["Gern'", "und", "wil\u00b7lig", "lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und ist denn mein St\u00fcndlein hier,", "tokens": ["Und", "ist", "denn", "mein", "St\u00fcnd\u00b7lein", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar zu dir", "tokens": ["Gar", "zu", "dir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "In mein Erbreich scheide.", "tokens": ["In", "mein", "Er\u00b7breich", "schei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}