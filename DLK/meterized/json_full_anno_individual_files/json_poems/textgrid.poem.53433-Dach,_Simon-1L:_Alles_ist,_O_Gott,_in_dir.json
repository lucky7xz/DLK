mbic.tri.relaxed"}, "line.3": {"text": "In der Liebe, da\u00df ihr Muth", "tokens": ["In", "der", "Lie\u00b7be", ",", "da\u00df", "ihr", "Muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen ihn mu\u00df brennen:", "tokens": ["Ge\u00b7gen", "ihn", "mu\u00df", "bren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn in ihm besitzet Sie,", "tokens": ["Denn", "in", "ihm", "be\u00b7sit\u00b7zet", "Sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was man je", "tokens": ["Was", "man", "je"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sch\u00f6nes m\u00f6chte nennen.", "tokens": ["Sch\u00f6\u00b7nes", "m\u00f6ch\u00b7te", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Weder Furcht noch Sorge legt", "tokens": ["We\u00b7der", "Furcht", "noch", "Sor\u00b7ge", "legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in ihrer Liebe Kertzen:", "tokens": ["Sich", "in", "ih\u00b7rer", "Lie\u00b7be", "Kert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie seinetwegen tr\u00e4gt", "tokens": ["Denn", "sie", "sei\u00