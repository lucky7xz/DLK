{"textgrid.poem.53433": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles ist, O Gott, in dir", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles ist, O Gott, in dir", "tokens": ["Al\u00b7les", "ist", ",", "O", "Gott", ",", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VAFIN", "$,", "NE", "NN", "$,", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberschw\u00e4nglich, Wei\u00dfheit, Leben,", "tokens": ["\u00dc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", ",", "Wei\u00df\u00b7heit", ",", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freude, Reichthumb, Macht und Zier,", "tokens": ["Freu\u00b7de", ",", "Reicht\u00b7humb", ",", "Macht", "und", "Zier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschen Pracht daneben,", "tokens": ["Men\u00b7schen", "Pracht", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PAV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihr Verstand, Gewalt und Lust", "tokens": ["Ihr", "Ver\u00b7stand", ",", "Ge\u00b7walt", "und", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist nur Wust,", "tokens": ["Ist", "nur", "Wust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schaum und Schatten eben.", "tokens": ["Schaum", "und", "Schat\u00b7ten", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wol der Seelen, welche dich", "tokens": ["Wol", "der", "See\u00b7len", ",", "wel\u00b7che", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einig f\u00fcr ihr Theil erwehlet,", "tokens": ["Ei\u00b7nig", "f\u00fcr", "ihr", "Theil", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Glauben inniglich", "tokens": ["Und", "im", "Glau\u00b7ben", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mit dir verm\u00e4hlet!", "tokens": ["Sich", "mit", "dir", "ver\u00b7m\u00e4h\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "O was Gn\u00fcg erdenckt ein Sinn,", "tokens": ["O", "was", "Gn\u00fcg", "er\u00b7denckt", "ein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was Gewinn,", "tokens": ["Was", "Ge\u00b7winn", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "So den Edlen f\u00e4hlet?", "tokens": ["So", "den", "Ed\u00b7len", "f\u00e4h\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Weg, O Herrlichkeit der Welt!", "tokens": ["Weg", ",", "O", "Herr\u00b7lich\u00b7keit", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weint ihr K\u00f6nig' aller Enden,", "tokens": ["Weint", "ihr", "K\u00f6\u00b7nig'", "al\u00b7ler", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die das Gl\u00fcck erhaben helt", "tokens": ["Die", "das", "Gl\u00fcck", "er\u00b7ha\u00b7ben", "helt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit untreuen H\u00e4nden,", "tokens": ["Mit", "un\u00b7treu\u00b7en", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Eure Hoheit ist ein Radt", "tokens": ["Eu\u00b7re", "Ho\u00b7heit", "ist", "ein", "Radt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Bladt,", "tokens": ["Und", "ein", "Bladt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Das sich leicht kan wenden.", "tokens": ["Das", "sich", "leicht", "kan", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sie hat Gott das theure Gut,", "tokens": ["Sie", "hat", "Gott", "das", "theu\u00b7re", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der giebt ihr sich zu erkennen", "tokens": ["Der", "giebt", "ihr", "sich", "zu", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Liebe, da\u00df ihr Muth", "tokens": ["In", "der", "Lie\u00b7be", ",", "da\u00df", "ihr", "Muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen ihn mu\u00df brennen:", "tokens": ["Ge\u00b7gen", "ihn", "mu\u00df", "bren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn in ihm besitzet Sie,", "tokens": ["Denn", "in", "ihm", "be\u00b7sit\u00b7zet", "Sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was man je", "tokens": ["Was", "man", "je"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sch\u00f6nes m\u00f6chte nennen.", "tokens": ["Sch\u00f6\u00b7nes", "m\u00f6ch\u00b7te", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Weder Furcht noch Sorge legt", "tokens": ["We\u00b7der", "Furcht", "noch", "Sor\u00b7ge", "legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in ihrer Liebe Kertzen:", "tokens": ["Sich", "in", "ih\u00b7rer", "Lie\u00b7be", "Kert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie seinetwegen tr\u00e4gt", "tokens": ["Denn", "sie", "sei\u00b7net\u00b7we\u00b7gen", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit standhafftem Hertzen", "tokens": ["Mit", "stand\u00b7haff\u00b7tem", "Hert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "D\u00fcrfftig, nackt, verachtet seyn,", "tokens": ["D\u00fcr\u00b7ff\u00b7tig", ",", "nackt", ",", "ver\u00b7ach\u00b7tet", "seyn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kranckheit-Pein,", "tokens": ["Kranck\u00b7heit\u00b7Pein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ja auch Todes-Schmertzen.", "tokens": ["Ja", "auch", "To\u00b7des\u00b7Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Denn sie wei\u00df bey wem sie h\u00e4lt,", "tokens": ["Denn", "sie", "wei\u00df", "bey", "wem", "sie", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und da\u00df sie von ihm nicht Leyden,", "tokens": ["Und", "da\u00df", "sie", "von", "ihm", "nicht", "Ley\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Nicht Gewalt, noch Zeit, noch Welt", "tokens": ["Nicht", "Ge\u00b7walt", ",", "noch", "Zeit", ",", "noch", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig werde scheiden,", "tokens": ["E\u00b7wig", "wer\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und da\u00df ihrer Tr\u00fcbsahl Lohn", "tokens": ["Und", "da\u00df", "ih\u00b7rer", "Tr\u00fcb\u00b7sahl", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sey die Krohn", "tokens": ["Sey", "die", "Krohn"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Aller ewgen Freuden.", "tokens": ["Al\u00b7ler", "ew\u00b7gen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "S\u00fcnden-Pracht und Gl\u00fcckes-Schein", "tokens": ["S\u00fcn\u00b7den\u00b7Pracht", "und", "Gl\u00fc\u00b7ckes\u00b7Schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt sie Himmlisch gro\u00df mit F\u00fcssen,", "tokens": ["Tritt", "sie", "Himm\u00b7lisch", "gro\u00df", "mit", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist an Lieb und Glauben rein,", "tokens": ["Ist", "an", "Lieb", "und", "Glau\u00b7ben", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilig am Gewissen,", "tokens": ["Hei\u00b7lig", "am", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Darumb Fried und Freud im Geist,", "tokens": ["Da\u00b7rumb", "Fried", "und", "Freud", "im", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Allermeist", "tokens": ["Al\u00b7ler\u00b7meist"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sie bedienen m\u00fcssen.", "tokens": ["Sie", "be\u00b7die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Herr wenn nimbst du mich von mir", "tokens": ["Herr", "wenn", "nimbst", "du", "mich", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ADV", "PPER", "PRF", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erwehlst mich f\u00fcr den deinen,", "tokens": ["Und", "er\u00b7wehlst", "mich", "f\u00fcr", "den", "dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich mag in heilger Zier,", "tokens": ["Da\u00df", "ich", "mag", "in", "heil\u00b7ger", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets vor dir erscheinen", "tokens": ["Stets", "vor", "dir", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und dich, O mein Eigenthumb,", "tokens": ["Und", "dich", ",", "O", "mein", "Ei\u00b7gen\u00b7thumb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiederumb", "tokens": ["Wie\u00b7de\u00b7rumb"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Halte f\u00fcr den meinen?", "tokens": ["Hal\u00b7te", "f\u00fcr", "den", "mei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Meine krancke Seel ist matt", "tokens": ["Mei\u00b7ne", "kran\u00b7cke", "Seel", "ist", "matt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verk\u00f6mbt gantz f\u00fcr Verlangen,", "tokens": ["Und", "ver\u00b7k\u00f6mbt", "gantz", "f\u00fcr", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allen Kummer, den sie hat", "tokens": ["Al\u00b7len", "Kum\u00b7mer", ",", "den", "sie", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nur dich zu fangen,", "tokens": ["Ist", "nur", "dich", "zu", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und von Welt und S\u00fcnden lo\u00df,", "tokens": ["Und", "von", "Welt", "und", "S\u00fcn\u00b7den", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir stets blo\u00df", "tokens": ["Dir", "stets", "blo\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Br\u00fcnstig anzuhangen.", "tokens": ["Br\u00fcns\u00b7tig", "an\u00b7zu\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "La\u00df mein Hort ohn' unterla\u00df", "tokens": ["La\u00df", "mein", "Hort", "ohn'", "un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich mit dir vereinigt leben,", "token