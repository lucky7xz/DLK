{"dta.poem.6111": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bis hieher haben wir erblickt,", "tokens": ["Bis", "hie\u00b7her", "ha\u00b7ben", "wir", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df durch die Ordnungen, die unver\u00e4nderlich,", "tokens": ["Da\u00df", "durch", "die", "Ord\u00b7nun\u00b7gen", ",", "die", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Theilchen, so am mindsten ungeschickt", "tokens": ["Die", "Theil\u00b7chen", ",", "so", "am", "minds\u00b7ten", "un\u00b7ge\u00b7schickt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "ADJA", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich zu bewegen, und die sich", "tokens": ["Sich", "zu", "be\u00b7we\u00b7gen", ",", "und", "die", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "KON", "PRELS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am wenigsten verwickeln und verbinden,", "tokens": ["Am", "we\u00b7nigs\u00b7ten", "ver\u00b7wi\u00b7ckeln", "und", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Am allerfl\u00fcchtigsten und schnellsten seyn:", "tokens": ["Am", "al\u00b7ler\u00b7fl\u00fcch\u00b7tigs\u00b7ten", "und", "schnells\u00b7ten", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.7": {"text": "Da\u00df sie zur\u00fcck- und von sich jagen", "tokens": ["Da\u00df", "sie", "zu\u00b7r\u00fcck", "und", "von", "sich", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "TRUNC", "KON", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Theilchen, die sich insgemein", "tokens": ["Die", "Theil\u00b7chen", ",", "die", "sich", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schnell in einander schlagen,", "tokens": ["Schnell", "in", "ein\u00b7an\u00b7der", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und da\u00df dadurch, da\u00df sie sich fest zusammen legen", "tokens": ["Und", "da\u00df", "da\u00b7durch", ",", "da\u00df", "sie", "sich", "fest", "zu\u00b7sam\u00b7men", "le\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PAV", "$,", "KOUS", "PPER", "PRF", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dieselben sich viel schwer- und langsamer bewegen.", "tokens": ["Die\u00b7sel\u00b7ben", "sich", "viel", "schwer", "und", "lang\u00b7sa\u00b7mer", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "PRF", "ADV", "TRUNC", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}