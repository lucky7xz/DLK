n!", "tokens": ["So", "schlag", "der", "Teu\u00b7fel", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es steht die Welt in Flammen,", "tokens": ["Es", "steht", "die", "Welt", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie schwatzen noch zusammen,", "tokens": ["Sie", "schwat\u00b7zen", "noch", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie lange soll das dauern?", "tokens": ["Wie", "lan\u00b7ge", "soll", "das", "dau\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem K\u00f6nig Schach, ihr Bauern!", "tokens": ["Dem", "K\u00f6\u00b7nig", "Schach", ",", "ihr", "Bau\u00b7ern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Parla \u2013 Parla \u2013 Parlament,", "tokens": ["Dein", "Par\u00b7la", "\u2013", "Par\u00b7la", "\u2013", "Par\u00b7la\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NE", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Volk, mach ihm ein End!", "tokens": ["O", "Volk", ",", "mach", "ihm", "ein", "End", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ART", "NN