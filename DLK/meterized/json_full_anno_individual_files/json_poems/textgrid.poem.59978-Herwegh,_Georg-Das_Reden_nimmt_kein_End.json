3", "tokens": ["Ma\u00b7thy", ",", "der", "Schuft", ",", "Mi\u00b7nis\u00b7ter", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Triumph, ihr Herrn Philister!", "tokens": ["Tri\u00b7umph", ",", "ihr", "Herrn", "Phi\u00b7lis\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Parla \u2013 Parla \u2013 Parlament", "tokens": ["Im", "Par\u00b7la", "\u2013", "Par\u00b7la", "\u2013", "Par\u00b7la\u00b7ment"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "$(", "NE", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Reden nimmt kein End!", "tokens": ["Das", "Re\u00b7den", "nimmt", "kein", "End", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu Frankfurt an dem Main \u2013", "tokens": ["Zu", "Frank\u00b7furt", "an", "dem", "Main", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die W\u00e4sche wird nicht rein;", "tokens": ["Die", "W\u00e4\u00b7sche", "wird", "nicht", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie b\u00fcrsten, und sie b\u00fcrsten,", "tokens": ["Sie", "b\u00fcrs\u00b7ten", ",", "und", "sie", "b\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die F\u00fcrsten bleiben F\u00fcrsten,", "tokens": ["Die", "F\u00fcrs\u00b7ten", "blei\u00b7ben", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Mohren bleiben Mohren", "tokens": ["Die", "Moh\u00b7ren", "blei\u00b7ben", "Moh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trotz aller Professoren", "tokens": ["Trotz", "al\u00b7ler", "Pro\u00b7fes\u00b7so\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Parla \u2013 Parla \u2013 Parlament", "tokens": ["Im", "Par\u00b7la", "\u2013", "Par\u00b7la", "\u2013", "Par\u00b7la\u00b7ment"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "$(", "NE", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Reden nimmt kein End!", "tokens": ["Das", "Re\u00b7den", "nimmt", "kein", "End", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu Frankfurt an dem Main \u2013", "tokens": ["Zu", "Frank\u00b7furt", "an", "dem", "Main", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist alles Trug und Schein.", "tokens": ["Ist", "al\u00b7les", "Trug", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "l