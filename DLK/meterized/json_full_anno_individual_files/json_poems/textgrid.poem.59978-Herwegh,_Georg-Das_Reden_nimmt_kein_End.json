asure": "iambic.tri"}, "line.7": {"text": "Im Parla \u2013 Parla \u2013 Parlament", "tokens": ["Im", "Par\u00b7la", "\u2013", "Par\u00b7la", "\u2013", "Par\u00b7la\u00b7ment"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "$(", "NE", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Reden nimmt kein End!", "tokens": ["Das", "Re\u00b7den", "nimmt", "kein", "End", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu Frankfurt an dem Main \u2013", "tokens": ["Zu", "Frank\u00b7furt", "an", "dem", "Main", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da wird man uns befrein;", "tokens": ["Da", "wird", "man", "uns", "be\u00b7fr\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man wird die Republiken", "tokens": ["Man", "wird", "die", "Re\u00b7pub\u00b7li\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Mutterleib ersticken,", "tokens": ["Im", "Mut\u00b7ter\u00b7leib", "er\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Bassermann und Welcker", "tokens": ["Und", "Bas\u00b7ser\u00b7mann", "und", "Wel\u00b7cker"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Begl\u00fccken dann die V\u00f6lker", "to