{"textgrid.poem.25367": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was waren Frauen anders dir als Spiel,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was waren Frauen anders dir als Spiel,", "tokens": ["Was", "wa\u00b7ren", "Frau\u00b7en", "an\u00b7ders", "dir", "als", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "ADV", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der du dich bettetest in soviel Liebesstunden:", "tokens": ["Der", "du", "dich", "bet\u00b7te\u00b7test", "in", "so\u00b7viel", "Lie\u00b7bess\u00b7tun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast nie andres als ein St\u00fcck von dir gefunden,", "tokens": ["Du", "hast", "nie", "and\u00b7res", "als", "ein", "St\u00fcck", "von", "dir", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "KOKOM", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und niemals fand dein Suchen sich das Ziel.", "tokens": ["Und", "nie\u00b7mals", "fand", "dein", "Su\u00b7chen", "sich", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du strebtest, dich im Hellen zu befreien,", "tokens": ["D