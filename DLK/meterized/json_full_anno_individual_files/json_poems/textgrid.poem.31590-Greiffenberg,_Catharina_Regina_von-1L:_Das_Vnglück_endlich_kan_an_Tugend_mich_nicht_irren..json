{"textgrid.poem.31590": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Vngl\u00fcck endlich kan an Tugend mich nicht irren.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Vngl\u00fcck endlich kan an Tugend mich nicht irren.", "tokens": ["Das", "Vn\u00b7gl\u00fcck", "end\u00b7lich", "kan", "an", "Tu\u00b7gend", "mich", "nicht", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "APPR", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es mundert sie vielmehr / bl\u00e4st ihre Funken auf /", "tokens": ["Es", "mun\u00b7dert", "sie", "viel\u00b7mehr", "/", "bl\u00e4st", "ih\u00b7re", "Fun\u00b7ken", "auf", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "VVFIN", "PPOSAT", "NN", "APPR", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und bringt des Muhtes H\u00e4ngst in dapffern Helden-Lauff.", "tokens": ["und", "bringt", "des", "Muh\u00b7tes", "H\u00e4ngst", "in", "dapf\u00b7fern", "Hel\u00b7den\u00b7Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wann es mir Tugend mehrt / so kan ich nichts verliehren.", "tokens": ["wann", "es", "mir", "Tu\u00b7gend", "mehrt", "/", "so", "kan", "ich", "nichts", "ver\u00b7lieh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "po