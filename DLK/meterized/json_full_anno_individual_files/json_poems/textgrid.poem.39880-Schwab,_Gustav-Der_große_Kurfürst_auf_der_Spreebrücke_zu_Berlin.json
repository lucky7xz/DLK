{"textgrid.poem.39880": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der gro\u00dfe Kurf\u00fcrst auf der Spreebr\u00fccke zu Berlin", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013 H\u00f6rt, wie es ergangen;", "tokens": ["\u2013", "H\u00f6rt", ",", "wie", "es", "er\u00b7gan\u00b7gen", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ich ging mit Scheu und halbem Bangen", "tokens": ["Ich", "ging", "mit", "Scheu", "und", "hal\u00b7bem", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dieser kaum verschwundnen Nacht", "tokens": ["In", "die\u00b7ser", "kaum", "ver\u00b7schwund\u00b7nen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor\u00fcber an des Bildes Pracht.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "des", "Bil\u00b7des", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es rauschten wunderlich die hellen", "tokens": ["Es", "rauschten", "wun\u00b7der\u00b7lich", "die", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mondbegl\u00e4nzten Flusseswellen,", "tokens": ["Und", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Flus\u00b7ses\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und warfen klar den Schein zur\u00fccke", "tokens": ["Und", "war\u00b7fen", "klar", "den", "Schein", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des ehrnen Bildes auf der Br\u00fccke.", "tokens": ["Des", "ehr\u00b7nen", "Bil\u00b7des", "auf", "der", "Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es war mir, als ob all sich nahten", "tokens": ["Es", "war", "mir", ",", "als", "ob", "all", "sich", "nah\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PIAT", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Geister seiner gro\u00dfen Thaten,", "tokens": ["Die", "Geis\u00b7ter", "sei\u00b7ner", "gro\u00b7\u00dfen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und hielten wunderbaren Tanz", "tokens": ["Und", "hiel\u00b7ten", "wun\u00b7der\u00b7ba\u00b7ren", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Rings um die S\u00e4ul' im Mondenglanz.", "tokens": ["Rings", "um", "die", "S\u00e4ul'", "im", "Mon\u00b7den\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da kam mir bei die alte Sage,", "tokens": ["Da", "kam", "mir", "bei", "die", "al\u00b7te", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df sie sich dreh' beim Zw\u00f6lfeschlage.", "tokens": ["Da\u00df", "sie", "sich", "dreh'", "beim", "Zw\u00f6l\u00b7fesc\u00b7hla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ein th\u00f6richt M\u00e4rlein ist's, ich dacht',", "tokens": ["Ein", "th\u00f6\u00b7richt", "M\u00e4r\u00b7lein", "ist's", ",", "ich", "dacht'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Indem so schl\u00e4gt es Mitternacht.", "tokens": ["In\u00b7dem", "so", "schl\u00e4gt", "es", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da f\u00e4ngt der Boden an zu beben,", "tokens": ["Da", "f\u00e4ngt", "der", "Bo\u00b7den", "an", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein tr\u00e4umrisch Wiehern anzuheben", "tokens": ["Ein", "tr\u00e4um\u00b7risch", "Wie\u00b7hern", "an\u00b7zu\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Beginnt das m\u00e4cht'ge Ro\u00df von Erz;", "tokens": ["Be\u00b7ginnt", "das", "m\u00e4cht'\u00b7ge", "Ro\u00df", "von", "Erz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und m\u00e4hlig, sieh! \u2013 mir schlug das Herz \u2013", "tokens": ["Und", "m\u00e4h\u00b7lig", ",", "sieh", "!", "\u2013", "mir", "schlug", "das", "Herz", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVIMP", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Dreht sich der F\u00fcrst mit seinem Pferde.", "tokens": ["Dreht", "sich", "der", "F\u00fcrst", "mit", "sei\u00b7nem", "Pfer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Das hoch sich aufb\u00e4umt, von der Erde.", "tokens": ["Das", "hoch", "sich", "auf\u00b7b\u00e4umt", ",", "von", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und wie es wieder stehet fest,", "tokens": ["Und", "wie", "es", "wie\u00b7der", "ste\u00b7het", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Er also sich vernehmen l\u00e4\u00dft:", "tokens": ["Er", "al\u00b7so", "sich", "ver\u00b7neh\u00b7men", "l\u00e4\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "\u00bbich sah nach allen Seiten hin", "tokens": ["\u00bb", "ich", "sah", "nach", "al\u00b7len", "Sei\u00b7ten", "hin"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "In meiner guten Stadt Berlin,", "tokens": ["In", "mei\u00b7ner", "gu\u00b7ten", "Stadt", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich halt' um jede Mitternacht", "tokens": ["Ich", "halt'", "um", "je\u00b7de", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Noch immer treue F\u00fcrstenwacht;", "tokens": ["Noch", "im\u00b7mer", "treu\u00b7e", "F\u00fcrs\u00b7ten\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So schaut' ich jetzo nach den Linden,", "tokens": ["So", "schaut'", "ich", "jet\u00b7zo", "nach", "den", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NE", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Dort ist mein treuster Freund zu finden,", "tokens": ["Dort", "ist", "mein", "treus\u00b7ter", "Freund", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Er ist auch deiner, Kind! \u2013 Franz Horn,", "tokens": ["Er", "ist", "auch", "dei\u00b7ner", ",", "Kind", "!", "\u2013", "Franz", "Horn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "$,", "NN", "$.", "$(", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ein Mann von deutschem Schrot und Korn;", "tokens": ["Ein", "Mann", "von", "deut\u00b7schem", "Schrot", "und", "Korn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ich mocht' ihn immer gern erblicken", "tokens": ["Ich", "mocht'", "ihn", "im\u00b7mer", "gern", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Als J\u00fcngling wandeln diese Br\u00fccken,", "tokens": ["Als", "J\u00fcng\u00b7ling", "wan\u00b7deln", "die\u00b7se", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Da schlug mir an die eh'rne Brust", "tokens": ["Da", "schlug", "mir", "an", "die", "eh'r\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sein t\u00fccht'ger Scherz oft recht mit Lust,", "tokens": ["Sein", "t\u00fccht'\u00b7ger", "Scherz", "oft", "recht", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Denn freudig hat er mein gedacht,", "tokens": ["Denn", "freu\u00b7dig", "hat", "er", "mein", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "So oft er diesen Weg gemacht.", "tokens": ["So", "oft", "er", "die\u00b7sen", "Weg", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und was er mir gethan als Mann,", "tokens": ["Und", "was", "er", "mir", "ge\u00b7than", "als", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Du wei\u00dft's recht gut, du triebst ihn an.", "tokens": ["Du", "wei\u00dft's", "recht", "gut", ",", "du", "triebst", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Wer mich in seinem Buch gelesen,", "tokens": ["Wer", "mich", "in", "sei\u00b7nem", "Buch", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Der kennt mein Handeln und mein Wesen,", "tokens": ["Der", "kennt", "mein", "Han\u00b7deln", "und", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Von meinem Leibe zeugt dies Erz,", "tokens": ["Von", "mei\u00b7nem", "Lei\u00b7be", "zeugt", "dies", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "In seinem Buche wohnt mein Herz.", "tokens": ["In", "sei\u00b7nem", "Bu\u00b7che", "wohnt", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Er feiert einen sch\u00f6nen Tag,", "tokens": ["Er", "fei\u00b7ert", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Darum ich ihn wohl gr\u00fc\u00dfen mag.", "tokens": ["Da\u00b7rum", "ich", "ihn", "wohl", "gr\u00fc\u00b7\u00dfen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "PPER", "PPER", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Geh', treues Kind, und ihm bedeute,", "tokens": ["Geh'", ",", "treu\u00b7es", "Kind", ",", "und", "ihm", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Da\u00df ich sein denk' im Himmel heute:", "tokens": ["Da\u00df", "ich", "sein", "denk'", "im", "Him\u00b7mel", "heu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAINF", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Und will er mein noch ferner denken,", "tokens": ["Und", "will", "er", "mein", "noch", "fer\u00b7ner", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Werd' ich ihm Kraft und Segen schenken,", "tokens": ["Werd'", "ich", "ihm", "Kraft", "und", "Se\u00b7gen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Zu schreiben von der Preu\u00dfenkrone", "tokens": ["Zu", "schrei\u00b7ben", "von", "der", "Preu\u00b7\u00dfen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Und von dem K\u00f6nig, meinem Sohne!\u00ab", "tokens": ["Und", "von", "dem", "K\u00f6\u00b7nig", ",", "mei\u00b7nem", "Soh\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Er schwieg, hin sank ich an der S\u00e4ule,", "tokens": ["Er", "schwieg", ",", "hin", "sank", "ich", "an", "der", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Und schlummert' eine gute Weile,", "tokens": ["Und", "schlum\u00b7mert'", "ei\u00b7ne", "gu\u00b7te", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Und wie ich wieder aufgewacht,", "tokens": ["Und", "wie", "ich", "wie\u00b7der", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Da war vergangen schon die Nacht,", "tokens": ["Da", "war", "ver\u00b7gan\u00b7gen", "schon", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Und hieher kam ich, ihm's zu k\u00fcnden. \u2013", "tokens": ["Und", "hie\u00b7her", "kam", "ich", ",", "ihm's", "zu", "k\u00fcn\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u2013 H\u00f6rt, wie es ergangen;", "tokens": ["\u2013", "H\u00f6rt", ",", "wie", "es", "er\u00b7gan\u00b7gen", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ich ging mit Scheu und halbem Bangen", "tokens": ["Ich", "ging", "mit", "Scheu", "und", "hal\u00b7bem", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dieser kaum verschwundnen Nacht", "tokens": ["In", "die\u00b7ser", "kaum", "ver\u00b7schwund\u00b7nen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor\u00fcber an des Bildes Pracht.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "des", "Bil\u00b7des", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es rauschten wunderlich die hellen", "tokens": ["Es", "rauschten", "wun\u00b7der\u00b7lich", "die", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mondbegl\u00e4nzten Flusseswellen,", "tokens": ["Und", "mond\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Flus\u00b7ses\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und warfen klar den Schein zur\u00fccke", "tokens": ["Und", "war\u00b7fen", "klar", "den", "Schein", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des ehrnen Bildes auf der Br\u00fccke.", "tokens": ["Des", "ehr\u00b7nen", "Bil\u00b7des", "auf", "der", "Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es war mir, als ob all sich nahten", "tokens": ["Es", "war", "mir", ",", "als", "ob", "all", "sich", "nah\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PIAT", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Geister seiner gro\u00dfen Thaten,", "tokens": ["Die", "Geis\u00b7ter", "sei\u00b7ner", "gro\u00b7\u00dfen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und hielten wunderbaren Tanz", "tokens": ["Und", "hiel\u00b7ten", "wun\u00b7der\u00b7ba\u00b7ren", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Rings um die S\u00e4ul' im Mondenglanz.", "tokens": ["Rings", "um", "die", "S\u00e4ul'", "im", "Mon\u00b7den\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da kam mir bei die alte Sage,", "tokens": ["Da", "kam", "mir", "bei", "die", "al\u00b7te", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df sie sich dreh' beim Zw\u00f6lfeschlage.", "tokens": ["Da\u00df", "sie", "sich", "dreh'", "beim", "Zw\u00f6l\u00b7fesc\u00b7hla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ein th\u00f6richt M\u00e4rlein ist's, ich dacht',", "tokens": ["Ein", "th\u00f6\u00b7richt", "M\u00e4r\u00b7lein", "ist's", ",", "ich", "dacht'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Indem so schl\u00e4gt es Mitternacht.", "tokens": ["In\u00b7dem", "so", "schl\u00e4gt", "es", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da f\u00e4ngt der Boden an zu beben,", "tokens": ["Da", "f\u00e4ngt", "der", "Bo\u00b7den", "an", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein tr\u00e4umrisch Wiehern anzuheben", "tokens": ["Ein", "tr\u00e4um\u00b7risch", "Wie\u00b7hern", "an\u00b7zu\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Beginnt das m\u00e4cht'ge Ro\u00df von Erz;", "tokens": ["Be\u00b7ginnt", "das", "m\u00e4cht'\u00b7ge", "Ro\u00df", "von", "Erz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und m\u00e4hlig, sieh! \u2013 mir schlug das Herz \u2013", "tokens": ["Und", "m\u00e4h\u00b7lig", ",", "sieh", "!", "\u2013", "mir", "schlug", "das", "Herz", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVIMP", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Dreht sich der F\u00fcrst mit seinem Pferde.", "toke