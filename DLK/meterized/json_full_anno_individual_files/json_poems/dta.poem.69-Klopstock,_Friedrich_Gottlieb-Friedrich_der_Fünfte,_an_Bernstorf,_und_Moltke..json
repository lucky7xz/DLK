{"dta.poem.69": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Friedrich der F\u00fcnfte,  \n an Bernstorf, und Moltke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Eingeh\u00fcllet in Nacht, jetzt, da die beeisten Gebirge,", "tokens": ["Ein\u00b7ge\u00b7h\u00fcl\u00b7let", "in", "Nacht", ",", "jetzt", ",", "da", "die", "be\u00b7eis\u00b7ten", "Ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und der einsame Wald", "tokens": ["Und", "der", "ein\u00b7sa\u00b7me", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Stumm und menschenlos ruhn, jetzt eil\u2019 ich, gefl\u00fcgel-", "tokens": ["Stumm", "und", "men\u00b7schen\u00b7los", "ruhn", ",", "jetzt", "eil'", "ich", ",", "ge\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "KON", "ADJD", "VVINF", "$,", "ADV", "VVFIN", "PPER", "$,", "TRUNC"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Meine Gedanken euch zu,", "tokens": ["Mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "euch", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "W\u00fcrdige Freunde des Besten der K\u00f6nige! Leiseren Lautes", "tokens": ["W\u00fcr\u00b7di\u00b7ge", "Freun\u00b7de", "des", "Bes\u00b7ten", "der", "K\u00f6\u00b7ni\u00b7ge", "!", "Lei\u00b7se\u00b7ren", "Lau\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "ART", "NN", "$.", "NN", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Sang ihn mein furchtsames Lied;", "tokens": ["Sang", "ihn", "mein", "furcht\u00b7sa\u00b7mes", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber euch sag\u2019 ich sie ganz des vollen Herzens Em-", "tokens": ["A\u00b7ber", "euch", "sag'", "ich", "sie", "ganz", "des", "vol\u00b7len", "Her\u00b7zens", "Em"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Wie das Herz sie empfand,", "tokens": ["Wie", "das", "Herz", "sie", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ohne des Zweifels versuchenden Ton; so offen ich sage,", "tokens": ["Oh\u00b7ne", "des", "Zwei\u00b7fels", "ver\u00b7su\u00b7chen\u00b7den", "Ton", ";", "so", "of\u00b7fen", "ich", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Da\u00df dem Sieger bey Sorr", "tokens": ["Da\u00df", "dem", "Sie\u00b7ger", "bey", "Sorr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Julianus zum Muster zu klein, und, ein Christ zu", "tokens": ["Ju\u00b7li\u00b7a\u00b7nus", "zum", "Mus\u00b7ter", "zu", "klein", ",", "und", ",", "ein", "Christ", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "PTKA", "ADJD", "$,", "KON", "$,", "ART", "NN", "PTKZU"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "W\u00fcrdig Friederich ist.", "tokens": ["W\u00fcr\u00b7dig", "Frie\u00b7de\u00b7rich", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Aber das ist ein Gedanke voll Nacht: Er wird es nicht", "tokens": ["A\u00b7ber", "das", "ist", "ein", "Ge\u00b7dan\u00b7ke", "voll", "Nacht", ":", "Er", "wird", "es", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ADJD", "NN", "$.", "PPER", "VAFIN", "PPER", "PTKNEG"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Da sein Freund ihm entschlief,", "tokens": ["Da", "sein", "Freund", "ihm", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Und, entflohen dem Labyrinthe, gewi\u00df war, es herrsche", "tokens": ["Und", ",", "ent\u00b7flo\u00b7hen", "dem", "La\u00b7by\u00b7rin\u00b7the", ",", "ge\u00b7wi\u00df", "war", ",", "es", "herr\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Jesus! und richte die Welt!", "tokens": ["Je\u00b7sus", "!", "und", "rich\u00b7te", "die", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Blieb der l\u00e4chelnde K\u00f6nig sich gleich. Zwar weinte", "tokens": ["Blieb", "der", "l\u00e4\u00b7cheln\u00b7de", "K\u00f6\u00b7nig", "sich", "gleich", ".", "Zwar", "wein\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PRF", "ADV", "$.", "ADV", "VVFIN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Um den Freund, der ihm starb!", "tokens": ["Um", "den", "Freund", ",", "der", "ihm", "starb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Oft, da dem Todten sein Moos schon begann, ging Frie-", "tokens": ["Oft", ",", "da", "dem", "Tod\u00b7ten", "sein", "Moos", "schon", "be\u00b7gann", ",", "ging", "Frie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "VVFIN", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ohne Zeugen zu seyn.", "tokens": ["Oh\u00b7ne", "Zeu\u00b7gen", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Ernste Muse, verla\u00df den wehmuthsvollen Gedanken,", "tokens": ["Erns\u00b7te", "Mu\u00b7se", ",", "ver\u00b7la\u00df", "den", "weh\u00b7muths\u00b7vol\u00b7len", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Der dich traurig vertieft,", "tokens": ["Der", "dich", "trau\u00b7rig", "ver\u00b7tieft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Wecke zu Silbert\u00f6nen die Leyer, die frohere, wenn sie,", "tokens": ["We\u00b7cke", "zu", "Sil\u00b7ber\u00b7t\u00f6\u00b7nen", "die", "Le\u00b7yer", ",", "die", "fro\u00b7he\u00b7re", ",", "wenn", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,", "KOUS", "PPER", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.24": {"text": "Scandinaviens Stolz,", "tokens": ["Scan\u00b7di\u00b7na\u00b7vi\u00b7ens", "Stolz", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Auch der Deutschen, besingt. Der nennt der Mensch-", "tokens": ["Auch", "der", "Deut\u00b7schen", ",", "be\u00b7singt", ".", "Der", "nennt", "der", "Men\u00b7sch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "$.", "ART", "VVFIN", "ART", "TRUNC"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Welcher Friederich nennt!", "tokens": ["Wel\u00b7cher", "Frie\u00b7de\u00b7rich", "nennt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "V\u00f6lker werden ihn einst, den Liebensw\u00fcrdigen, nennen,", "tokens": ["V\u00f6l\u00b7ker", "wer\u00b7den", "ihn", "einst", ",", "den", "Lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7gen", ",", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "VVINF", "$,"], "meter": "+-+--+-+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Und der denkende Mann", "tokens": ["Und", "der", "den\u00b7ken\u00b7de", "Mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Wird mit richtendem Blick sein sch\u00f6nes Leben betrachten,", "tokens": ["Wird", "mit", "rich\u00b7ten\u00b7dem", "Blick", "sein", "sch\u00f6\u00b7nes", "Le\u00b7ben", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Keinen finden, wie ihn!", "tokens": ["Kei\u00b7nen", "fin\u00b7den", ",", "wie", "ihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "VVINF", "$,", "PWAV", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Dann wird, jenen furchtbaren Tag, den die Muse", "tokens": ["Dann", "wird", ",", "je\u00b7nen", "furcht\u00b7ba\u00b7ren", "Tag", ",", "den", "die", "Mu\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PDAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Jetzo stammelnd besingt,", "tokens": ["Jet\u00b7zo", "stam\u00b7melnd", "be\u00b7singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Wenn im Tempel der Ehre die Lorbeern alle ver-", "tokens": ["Wenn", "im", "Tem\u00b7pel", "der", "Eh\u00b7re", "die", "Lor\u00b7beern", "al\u00b7le", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "ART", "NN", "PIAT", "TRUNC"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.34": {"text": "Und kein Ruhm mehr besch\u00fctzt,", "tokens": ["Und", "kein", "Ruhm", "mehr", "be\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Ach den Tag wird dann der sanften Menschlichkeit", "tokens": ["Ach", "den", "Tag", "wird", "dann", "der", "sanf\u00b7ten", "Menschlich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Wie ihr Leben einst war!", "tokens": ["Wie", "ihr", "Le\u00b7ben", "einst", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VAFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}