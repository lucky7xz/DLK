{"dta.poem.11840": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kannst aufs Feld nicht gehn, ohn' irgend eine Blume", "tokens": ["Du", "kannst", "aufs", "Feld", "nicht", "gehn", ",", "ohn'", "ir\u00b7gend", "ei\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "$,", "KOUI", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu finden, welche sagt von ihres Sch\u00f6pfers Ruhme.", "tokens": ["Zu", "fin\u00b7den", ",", "wel\u00b7che", "sagt", "von", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht in Gesellschaft kannst du gehn, ohn' ein Gesicht", "tokens": ["Nicht", "in", "Ge\u00b7sell\u00b7schaft", "kannst", "du", "gehn", ",", "ohn'", "ein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,", "KOUI", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sehn, das deinem Bild vom Menschen widerspricht.", "tokens": ["Zu", "sehn", ",", "das", "dei\u00b7nem", "Bild", "vom", "Men\u00b7schen", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum unter Blumen bleib, und lerne Menschen meiden!", "tokens": ["Drum", "un\u00b7ter", "Blu\u00b7men", "bleib", ",", "und", "ler\u00b7ne", "Men\u00b7schen", "mei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVFIN", "$,", "KON", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Menschen k\u00f6nnten dir die Blumen selbst verleiden.", "tokens": ["Die", "Men\u00b7schen", "k\u00f6nn\u00b7ten", "dir", "die", "Blu\u00b7men", "selbst", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch lieben lernest du, wo du bei Blumen bist,", "tokens": ["Doch", "lie\u00b7ben", "ler\u00b7nest", "du", ",", "wo", "du", "bei", "Blu\u00b7men", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschen selber, der unliebenswurdig ist.", "tokens": ["Den", "Men\u00b7schen", "sel\u00b7ber", ",", "der", "un\u00b7lie\u00b7bens\u00b7wur\u00b7dig", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}