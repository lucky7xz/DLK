{"dta.poem.8336": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Gesundheit eine unerkand-  \n te Wollthat GOttes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir wissen \u00f6fters nicht, wie gros des\nH\u00f6chsten Gaben,", "tokens": ["Wir", "wis\u00b7sen", "\u00f6f\u00b7ters", "nicht", ",", "wie", "gros", "des", "H\u00f6chs\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange wir sie noch in dem Ge-", "tokens": ["So", "lan\u00b7ge", "wir", "sie", "noch", "in", "dem", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PPER", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein wenn sie verscherzt, so wird es erst bedacht,", "tokens": ["Al\u00b7lein", "wenn", "sie", "ver\u00b7scherzt", ",", "so", "wird", "es", "erst", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie gros das Kleinod sey, das man vorher ver-", "tokens": ["Wie", "gros", "das", "Klei\u00b7nod", "sey", ",", "das", "man", "vor\u00b7her", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$,", "PRELS", "PIS", "ADV", "TRUNC"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wir k\u00f6nnen den Beweis gleich auser Zweifel sezzen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "den", "Be\u00b7weis", "gleich", "au\u00b7ser", "Zwei\u00b7fel", "sez\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn man nur \u00fcberdenkt, wie wir geringe sch\u00e4z-", "tokens": ["Wenn", "man", "nur", "\u00fc\u00b7ber\u00b7denkt", ",", "wie", "wir", "ge\u00b7rin\u00b7ge", "sch\u00e4\u00b7z"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$,", "PWAV", "PPER", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das allerbeste Gut, das man Gesundheit nennt,", "tokens": ["Das", "al\u00b7ler\u00b7bes\u00b7te", "Gut", ",", "das", "man", "Ge\u00b7sund\u00b7heit", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das keiner eher nicht, als wenn er krank erkennt.", "tokens": ["Das", "kei\u00b7ner", "e\u00b7her", "nicht", ",", "als", "wenn", "er", "krank", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "PTKNEG", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie viele finden sich, die auf den Leib los st\u00fcrmen,", "tokens": ["Wie", "vie\u00b7le", "fin\u00b7den", "sich", ",", "die", "auf", "den", "Leib", "los", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "$,", "PRELS", "APPR", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie w\u00fcrden diese nicht mit Sorgfalt ihn beschir-", "tokens": ["Wie", "w\u00fcr\u00b7den", "die\u00b7se", "nicht", "mit", "Sorg\u00b7falt", "ihn", "be\u00b7schir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PDS", "PTKNEG", "APPR", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn sie erkenneten, da\u00df in der Zeitlichkeit,", "tokens": ["Wenn", "sie", "er\u00b7ken\u00b7ne\u00b7ten", ",", "da\u00df", "in", "der", "Zeit\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kein gr\u00f6\u00dfres Gut, was uns mit dem Genus er-", "tokens": ["Kein", "gr\u00f6\u00df\u00b7res", "Gut", ",", "was", "uns", "mit", "dem", "Ge\u00b7nus", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Als die Gesundheit sey? Der HErr hat uns das Le-", "tokens": ["Als", "die", "Ge\u00b7sund\u00b7heit", "sey", "?", "Der", "Herr", "hat", "uns", "das", "Le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$.", "ART", "NN", "VAFIN", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch seine Huld geschenkt, den Leib den er gege-", "tokens": ["Durch", "sei\u00b7ne", "Huld", "ge\u00b7schenkt", ",", "den", "Leib", "den", "er", "ge\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "ART", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Erh\u00e4lt sein Odem nur, sonst w\u00fcrden wir zu Staub,", "tokens": ["Er\u00b7h\u00e4lt", "sein", "O\u00b7dem", "nur", ",", "sonst", "w\u00fcr\u00b7den", "wir", "zu", "Staub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In der Verg\u00e4nglichkeit, ein nichtger Todes Raub.", "tokens": ["In", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ",", "ein", "nicht\u00b7ger", "To\u00b7des", "Raub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie viele finden sich, die wenn sie nicht erkranken,", "tokens": ["Wie", "vie\u00b7le", "fin\u00b7den", "sich", ",", "die", "wenn", "sie", "nicht", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "$,", "PRELS", "KOUS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "F\u00fcr den gesunden Leib, der h\u00f6chsten Vorsicht dan-", "tokens": ["F\u00fcr", "den", "ge\u00b7sun\u00b7den", "Leib", ",", "der", "h\u00f6chs\u00b7ten", "Vor\u00b7sicht", "dan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie viele wenden so des Leibes Kr\u00e4fte an,", "tokens": ["Wie", "vie\u00b7le", "wen\u00b7den", "so", "des", "Lei\u00b7bes", "Kr\u00e4f\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie ein Gesunder soll, durch ", "tokens": ["Wie", "ein", "Ge\u00b7sun\u00b7der", "soll", ",", "durch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "$,", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Dies ist Beweis genug, da\u00df viele nicht erkennen,", "tokens": ["Dies", "ist", "Be\u00b7weis", "ge\u00b7nug", ",", "da\u00df", "vie\u00b7le", "nicht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADV", "$,", "KOUS", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df die Gesundheit sey, ein grosser Schaz zu nen-", "tokens": ["Da\u00df", "die", "Ge\u00b7sund\u00b7heit", "sey", ",", "ein", "gros\u00b7ser", "Schaz", "zu", "nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Allein so bald der Leib der Krankheit Uebel f\u00fchlt,", "tokens": ["Al\u00b7lein", "so", "bald", "der", "Leib", "der", "Krank\u00b7heit", "Ue\u00b7bel", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Des Fiebers strenge Wuth durch Mark und Adern", "tokens": ["Des", "Fie\u00b7bers", "stren\u00b7ge", "Wuth", "durch", "Mark", "und", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Da lernet man zuerst, was wir gehabt, was feh-", "tokens": ["Da", "ler\u00b7net", "man", "zu\u00b7erst", ",", "was", "wir", "ge\u00b7habt", ",", "was", "feh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "PRELS", "PPER", "VAPP", "$,", "PWS", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wenn die bange Noth die Glieder l\u00e4hmt und", "tokens": ["Und", "wenn", "die", "ban\u00b7ge", "Noth", "die", "Glie\u00b7der", "l\u00e4hmt", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Der Pulsschlag hizzig geht; so ruft und flehet man,", "tokens": ["Der", "Puls\u00b7schlag", "hiz\u00b7zig", "geht", ";", "so", "ruft", "und", "fle\u00b7het", "man", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "ADV", "VVFIN", "KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit heissester Andacht den besten Helfer an:", "tokens": ["Mit", "heis\u00b7ses\u00b7ter", "An\u00b7dacht", "den", "bes\u00b7ten", "Hel\u00b7fer", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Der Kranke der gelobt, vor allen sich zu h\u00fcten,", "tokens": ["Der", "Kran\u00b7ke", "der", "ge\u00b7lobt", ",", "vor", "al\u00b7len", "sich", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVPP", "$,", "APPR", "PIAT", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was in dem Leibe k\u00f6nn ihm solche Qual aus-", "tokens": ["Was", "in", "dem", "Lei\u00b7be", "k\u00f6nn", "ihm", "sol\u00b7che", "Qual", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "PPER", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Die Hizze mindert sich, und beim ged\u00e4mpften Lauf,", "tokens": ["Die", "Hiz\u00b7ze", "min\u00b7dert", "sich", ",", "und", "beim", "ge\u00b7d\u00e4mpf\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Des ungest\u00fcmen Bluts, h\u00f6rts Herzeklopfen auf,", "tokens": ["Des", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Bluts", ",", "h\u00f6rts", "Her\u00b7ze\u00b7klop\u00b7fen", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Puls schl\u00e4gt langsamer, das Blut ist abgek\u00fch-", "tokens": ["Die", "Puls", "schl\u00e4gt", "lang\u00b7sa\u00b7mer", ",", "das", "Blut", "ist", "ab\u00b7ge\u00b7k\u00fch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Andacht nimmt auch ab, die man vorher ge-", "tokens": ["Die", "An\u00b7dacht", "nimmt", "auch", "ab", ",", "die", "man", "vor\u00b7her", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "PRELS", "PIS", "ADV", "TRUNC"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.35": {"text": "Kaum ist der Leib gest\u00e4rkt; so fliegt der heilge Sinn,", "tokens": ["Kaum", "ist", "der", "Leib", "ge\u00b7st\u00e4rkt", ";", "so", "fliegt", "der", "heil\u00b7ge", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn blos die Angst erregt, mit seiner Ursach hin:", "tokens": ["Denn", "blos", "die", "Angst", "er\u00b7regt", ",", "mit", "sei\u00b7ner", "Ur\u00b7sach", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Krankheit ist vorbei, der Schwelger s\u00e4uft von", "tokens": ["Die", "Krank\u00b7heit", "ist", "vor\u00b7bei", ",", "der", "Schwel\u00b7ger", "s\u00e4uft", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.38": {"text": "Er handelt wieder so, da\u00df es ihm mu\u00df gereuen.", "tokens": ["Er", "han\u00b7delt", "wie\u00b7der", "so", ",", "da\u00df", "es", "ihm", "mu\u00df", "ge\u00b7reu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wer dieses nur bedenkt, der f\u00e4llt dem Sazze bei,", "tokens": ["Wer", "die\u00b7ses", "nur", "be\u00b7denkt", ",", "der", "f\u00e4llt", "dem", "Saz\u00b7ze", "bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df ein gesunder Leib des H\u00f6chsten Wollthat sey,", "tokens": ["Da\u00df", "ein", "ge\u00b7sun\u00b7der", "Leib", "des", "H\u00f6chs\u00b7ten", "Woll\u00b7that", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die nicht erkennet wird, bis da\u00df sie ist verlezzet;", "tokens": ["Die", "nicht", "er\u00b7ken\u00b7net", "wird", ",", "bis", "da\u00df", "sie", "ist", "ver\u00b7lez\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "VAFIN", "$,", "KON", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Bei einer Krankheits Qual wird sie 