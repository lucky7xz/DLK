{"dta.poem.775": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  Au\u00df dem dritten Buch.  \n Eben desselben wercks. An den gefangenen  \n  Dic\u00e6us.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mutter enger Leib hilt erstlich dich gefangen/ ", "tokens": ["Der", "Mut\u00b7ter", "en\u00b7ger", "Leib", "hilt", "erst\u00b7lich", "dich", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als deine Seele ward in Fleisch vnd Bein verstrickt:", "tokens": ["Als", "dei\u00b7ne", "See\u00b7le", "ward", "in", "Fleisch", "vnd", "Bein", "ver\u00b7strickt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bald du dieses Licht/ das s\u00fc\u00dfe Licht erblickt/", "tokens": ["So", "bald", "du", "die\u00b7ses", "Licht", "/", "das", "s\u00fc\u00b7\u00dfe", "Licht", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PDAT", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bist du in newe Band vnd Kercker eingegangen.", "tokens": ["Bist", "du", "in", "ne\u00b7we", "Band", "vnd", "Ker\u00b7cker", "ein\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Was ist die grosse Welt? ein Blockhau\u00df/ da verlangen/", "tokens": ["Was", "ist", "die", "gros\u00b7se", "Welt", "?", "ein", "Block\u00b7hau\u00df", "/", "da", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "$(", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd Angst vnd schwere noth mit str\u00e4ngen fesseln dr\u00fcckt/", "tokens": ["Vnd", "Angst", "vnd", "schwe\u00b7re", "noth", "mit", "str\u00e4n\u00b7gen", "fes\u00b7seln", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn vns der freye Todt/ au\u00df diesen Ketten r\u00fcckt/", "tokens": ["Wenn", "vns", "der", "frey\u00b7e", "Todt", "/", "au\u00df", "die\u00b7sen", "Ket\u00b7ten", "r\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn nimbt die grufft in Hafft/ die gantz erbla\u00dften wangen.", "tokens": ["Denn", "nimbt", "die", "grufft", "in", "Hafft", "/", "die", "gantz", "er\u00b7bla\u00df\u00b7ten", "wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$(", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wa\u00df ist die Freyheit doch/ die nirgend wird gefunden/", "tokens": ["Wa\u00df", "ist", "die", "Frey\u00b7heit", "doch", "/", "die", "nir\u00b7gend", "wird", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$(", "ART", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist eh\u2019 als du bist/ vnd weil du bist gebunden/", "tokens": ["Du", "bist", "eh'", "als", "du", "bist", "/", "vnd", "weil", "du", "bist", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "KOUS", "PPER", "VAFIN", "$(", "KON", "KOUS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bindest dich selb-selbst in Furcht vnd Sorgen eyn.", "tokens": ["Du", "bin\u00b7dest", "dich", "selb\u00b7\u00b7selbst", "in", "Furcht", "vnd", "Sor\u00b7gen", "eyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch! wer mit schnellem geist kan durch die Wolcken rennen/", "tokens": ["Doch", "!", "wer", "mit", "schnel\u00b7lem", "geist", "kan", "durch", "die", "Wol\u00b7cken", "ren\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "PWS", "APPR", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd stricke/ die verlust vnd hoffnung/ w\u00fcrckt zutrennen;", "tokens": ["Vnd", "stri\u00b7cke", "/", "die", "ver\u00b7lust", "vnd", "hoff\u00b7nung", "/", "w\u00fcrckt", "zu\u00b7tren\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ART", "NN", "KON", "NN", "$(", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan/ ob jhn Diamant gleich b\u00fcnde/ freye seyn.", "tokens": ["Kan", "/", "ob", "jhn", "Di\u00b7a\u00b7mant", "gleich", "b\u00fcn\u00b7de", "/", "frey\u00b7e", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$(", "KOUS", "PPER", "NN", "ADV", "ADJA", "$(", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}