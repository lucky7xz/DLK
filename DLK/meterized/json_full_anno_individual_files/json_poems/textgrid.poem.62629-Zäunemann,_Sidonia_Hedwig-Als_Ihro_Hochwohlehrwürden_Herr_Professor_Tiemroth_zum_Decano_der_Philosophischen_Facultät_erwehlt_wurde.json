{"textgrid.poem.62629": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Als Ihro Hochwohlehrw\u00fcrden Herr Professor Tiemroth zum Decano der Philosophischen Facult\u00e4t erwehlt wurde", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gienge ", "tokens": ["Es", "gien\u00b7ge"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Beschaute die B\u00e4ume, wie sch\u00f6n sie floriren;", "tokens": ["Be\u00b7schau\u00b7te", "die", "B\u00e4u\u00b7me", ",", "wie", "sch\u00f6n", "sie", "flo\u00b7ri\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die mancherley Farben vergn\u00fcgten den Sinn,", "tokens": ["Die", "man\u00b7cher\u00b7ley", "Far\u00b7ben", "ver\u00b7gn\u00fcg\u00b7ten", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und zogen denselben fast g\u00e4nzlich dahin.", "tokens": ["Und", "zo\u00b7gen", "den\u00b7sel\u00b7ben", "fast", "g\u00e4nz\u00b7lich", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "ADV", "ADJD", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da kame das Gl\u00fccke mit l\u00e4chelnden Mienen,", "tokens": ["Da", "ka\u00b7me", "das", "Gl\u00fc\u00b7cke", "mit", "l\u00e4\u00b7cheln\u00b7den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und sagte: ", "tokens": ["Und", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Was lachest du \u00fcber der G\u00e4rten ihr Gl\u00fcck?", "tokens": ["Was", "la\u00b7chest", "du", "\u00fc\u00b7ber", "der", "G\u00e4r\u00b7ten", "ihr", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Gedenke viel lieber an deines zur\u00fcck;", "tokens": ["Ge\u00b7den\u00b7ke", "viel", "lie\u00b7ber", "an", "dei\u00b7nes", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "PPOSAT", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Das heute beginnet besonders zu bl\u00fchen;", "tokens": ["Das", "heu\u00b7te", "be\u00b7gin\u00b7net", "be\u00b7son\u00b7ders", "zu", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Du wirst Dich desselben gewi\u00df nicht entziehen.", "tokens": ["Du", "wirst", "Dich", "des\u00b7sel\u00b7ben", "ge\u00b7wi\u00df", "nicht", "ent\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PDS", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Es sinnet ", "tokens": ["Es", "sin\u00b7net"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Und rufet dem Gl\u00fccke; und f\u00f6rdert den Lauf.", "tokens": ["Und", "ru\u00b7fet", "dem", "Gl\u00fc\u00b7cke", ";", "und", "f\u00f6r\u00b7dert", "den", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Wie? sagte ", "tokens": ["Wie", "?", "sag\u00b7te"], "token_info": ["word", "punct", "word"], "pos": ["PWAV", "$.", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Ich glaube, du scherzest, und wilst mich vexiren?", "tokens": ["Ich", "glau\u00b7be", ",", "du", "scher\u00b7zest", ",", "und", "wilst", "mich", "ve\u00b7xi\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.15": {"text": "Drauf kame ein Weibsbild, ihr Name hie\u00df ", "tokens": ["Drauf", "ka\u00b7me", "ein", "Weibs\u00b7bild", ",", "ihr", "Na\u00b7me", "hie\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Und sagte: ", "tokens": ["Und", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Was jetzo das Gl\u00fccke dir freudig berichtet,", "tokens": ["Was", "jet\u00b7zo", "das", "Gl\u00fc\u00b7cke", "dir", "freu\u00b7dig", "be\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.18": {"text": "Das sagt es mit Wahrheit, es ist nichts erdichtet.", "tokens": ["Das", "sagt", "es", "mit", "Wahr\u00b7heit", ",", "es", "ist", "nichts", "er\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$,", "PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Drum bin ich erschienen, da\u00df ich dich von hier", "tokens": ["Drum", "bin", "ich", "er\u00b7schie\u00b7nen", ",", "da\u00df", "ich", "dich", "von", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PRF", "APPR", "ADV"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.20": {"text": "Zum Helicon bringe, zur sch\u00f6neren Zier.", "tokens": ["Zum", "He\u00b7li\u00b7con", "brin\u00b7ge", ",", "zur", "sch\u00f6\u00b7ne\u00b7ren", "Zier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Besinn dich nicht lange; verlasse den Garten,", "tokens": ["Be\u00b7sinn", "dich", "nicht", "lan\u00b7ge", ";", "ver\u00b7las\u00b7se", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.22": {"text": "La\u00df mich und das Gl\u00fccke nicht fernerhin warten.", "tokens": ["La\u00df", "mich", "und", "das", "Gl\u00fc\u00b7cke", "nicht", "fer\u00b7ner\u00b7hin", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Drauf wurde ", "tokens": ["Drauf", "wur\u00b7de"], "token_info": ["word", "word"], "pos": ["PAV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Daher Ihn kein Leiden und Unfall ber\u00fchrt.", "tokens": ["Da\u00b7her", "Ihn", "kein", "Lei\u00b7den", "und", "Un\u00b7fall", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.25": {"text": "Sie schritten behende, hier galt kein Verziehen,", "tokens": ["Sie", "schrit\u00b7ten", "be\u00b7hen\u00b7de", ",", "hier", "galt", "kein", "Ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "$,", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Weil ihnen der Himmel gut Wetter verliehen.", "tokens": ["Weil", "ih\u00b7nen", "der", "Him\u00b7mel", "gut", "Wet\u00b7ter", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.27": {"text": "Bald zeigte sich ihnen Sophiens Pallast,", "tokens": ["Bald", "zeig\u00b7te", "sich", "ih\u00b7nen", "So\u00b7phi\u00b7ens", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "NE", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.28": {"text": "Der reichlich mit B\u00e4umen und Wasser umfa\u00dft.", "tokens": ["Der", "reich\u00b7lich", "mit", "B\u00e4u\u00b7men", "und", "Was\u00b7ser", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.29": {"text": "Es lachte Sophia, da diese drey kamen,", "tokens": ["Es", "lach\u00b7te", "So\u00b7phia", ",", "da", "die\u00b7se", "drey", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "KOUS", "PDAT", "CARD", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Und ihre Behausung zum Aufenthalt nahmen.", "tokens": ["Und", "ih\u00b7re", "Be\u00b7hau\u00b7sung", "zum", "Auf\u00b7ent\u00b7halt", "nah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.31": {"text": "Sie reichte ", "tokens": ["Sie", "reich\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.32": {"text": "Und sagte mit Freuden: Weil du mir verwandt,", "tokens": ["Und", "sag\u00b7te", "mit", "Freu\u00b7den", ":", "Weil", "du", "mir", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Und g\u00e4nzlich ergeben; so will sichs geziemen,", "tokens": ["Und", "g\u00e4nz\u00b7lich", "er\u00b7ge\u00b7ben", ";", "so", "will", "sichs", "ge\u00b7zie\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "ADV", "VMFIN", "PIS", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.34": {"text": "Da\u00df meine Geliebten Dich ehren und r\u00fchmen.", "tokens": ["Da\u00df", "mei\u00b7ne", "Ge\u00b7lieb\u00b7ten", "Dich", "eh\u00b7ren", "und", "r\u00fch\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.35": {"text": "Ich hab an die Meinen den Ausspruch gethan:", "tokens": ["Ich", "hab", "an", "die", "Mei\u00b7nen", "den", "Aus\u00b7spruch", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.36": {"text": "Bringt heute ", "tokens": ["Bringt", "heu\u00b7te"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Die S\u00f6hne Sophiens erschienen mit Freuden,", "tokens": ["Die", "S\u00f6h\u00b7ne", "So\u00b7phi\u00b7ens", "er\u00b7schie\u00b7nen", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.38": {"text": "Und sagten: wir werden uns billig bescheiden,", "tokens": ["Und", "sag\u00b7ten", ":", "wir", "wer\u00b7den", "uns", "bil\u00b7lig", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.39": {"text": "Wir werden verrichten, was dein Mund gelehrt,", "tokens": ["Wir", "wer\u00b7den", "ver\u00b7rich\u00b7ten", ",", "was", "dein", "Mund", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Weil solches selbst unsre Belustgung vermehrt.", "tokens": ["Weil", "sol\u00b7ches", "selbst", "uns\u00b7re", "Be\u00b7lust\u00b7gung", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.41": {"text": "Sie f\u00fchrten ", "tokens": ["Sie", "f\u00fchr\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Hier strahlte der g\u00fcldne und silberne Schimmer.", "tokens": ["Hier", "strahl\u00b7te", "der", "g\u00fcld\u00b7ne", "und", "sil\u00b7ber\u00b7ne", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.43": {"text": "Hier sah man die Ehre, hier war auch ihr Thron?", "tokens": ["Hier", "sah", "man", "die", "Eh\u00b7re", ",", "hier", "war", "auch", "ihr", "Thron", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.44": {"text": "Sie fragte ganz freundlich: Wem geb ich den Lohn?", "tokens": ["Sie", "frag\u00b7te", "ganz", "freund\u00b7lich", ":", "Wem", "geb", "ich", "den", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.45": {"text": "Die Antwort kam schleunigst: Wir m\u00fcssen bekennen,", "tokens": ["Die", "Ant\u00b7wort", "kam", "schleu\u00b7nigst", ":", "Wir", "m\u00fcs\u00b7sen", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Wir fordern ihn ohne Bedenken darzu,", "tokens": ["Wir", "for\u00b7dern", "ihn", "oh\u00b7ne", "Be\u00b7den\u00b7ken", "dar\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.47": {"text": "Denn seine Erk\u00e4ntni\u00df verschaffet uns Ruh.", "tokens": ["Denn", "sei\u00b7ne", "Er\u00b7k\u00e4nt\u00b7ni\u00df", "ver\u00b7schaf\u00b7fet", "uns", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.48": {"text": "Wir m\u00fcssen ihn ehren und sonderlich preisen.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "ihn", "eh\u00b7ren", "und", "son\u00b7der\u00b7lich", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.49": {"text": "Drauf sagte die Ehre: o herrlicher Schlu\u00df!", "tokens": ["Drauf", "sag\u00b7te", "die", "Eh\u00b7re", ":", "o", "herr\u00b7li\u00b7cher", "Schlu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$.", "FM", "ADJA", "