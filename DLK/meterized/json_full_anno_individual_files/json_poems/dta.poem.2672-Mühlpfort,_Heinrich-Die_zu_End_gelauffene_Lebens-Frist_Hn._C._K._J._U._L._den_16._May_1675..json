{"dta.poem.2672": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die zu End gelauffene Lebens-Frist  \n  Hn. C. K.   J. U. L.   den 16. May 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol/ Hochgelahrter K\u00fchn/ die Frist ist nun geendet/", "tokens": ["Wol", "/", "Hoch\u00b7ge\u00b7lahr\u00b7ter", "K\u00fchn", "/", "die", "Frist", "ist", "nun", "ge\u00b7en\u00b7det", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es setzt die Angst der Welt dir keinen Tag mehr an.", "tokens": ["Es", "setzt", "die", "Angst", "der", "Welt", "dir", "kei\u00b7nen", "Tag", "mehr", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "PIAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ursach die du hast erheblich eingewendet/", "tokens": ["Die", "Ur\u00b7sach", "die", "du", "hast", "er\u00b7heb\u00b7lich", "ein\u00b7ge\u00b7wen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gilt vor dem Richter-Stuhl der alle richten kan.", "tokens": ["Gilt", "vor", "dem", "Rich\u00b7ter\u00b7Stuhl", "der", "al\u00b7le", "rich\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es sind die Ferien zu ruhen eingefallen/", "tokens": ["Es", "sind", "die", "Fe\u00b7ri\u00b7en", "zu", "ru\u00b7hen", "ein\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du so lange Zeit so sehnlich hast gehofft.", "tokens": ["Die", "du", "so", "lan\u00b7ge", "Zeit", "so", "sehn\u00b7lich", "hast", "ge\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ruh\u2019 wol/ bi\u00df dermaleinst wird GOttes Stimm erschallen/", "tokens": ["Ruh'", "wol", "/", "bi\u00df", "der\u00b7ma\u00b7leinst", "wird", "Got\u00b7tes", "Stimm", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "APPR", "ADV", "VAFIN", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dich aus deinem Grab zum grossen Richt-Tag rufft.", "tokens": ["Und", "dich", "aus", "dei\u00b7nem", "Grab", "zum", "gros\u00b7sen", "Richt\u00b7Tag", "rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwar wollen wir auch hier dein Richter-Ampt noch preisen/", "tokens": ["Zwar", "wol\u00b7len", "wir", "auch", "hier", "dein", "Rich\u00b7ter\u00b7Ampt", "noch", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zu gemeinem Heyl du r\u00fchmlich hast gef\u00fchrt/", "tokens": ["Das", "zu", "ge\u00b7mei\u00b7nem", "Heyl", "du", "r\u00fchm\u00b7lich", "hast", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "ADJA", "NN", "PPER", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir wollen unsre Pflicht und letzten Dienst erweisen/", "tokens": ["Wir", "wol\u00b7len", "uns\u00b7re", "Pflicht", "und", "letz\u00b7ten", "Dienst", "er\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nachdencken auff ein Lied das deine Leiche ziert.", "tokens": ["Nach\u00b7den\u00b7cken", "auff", "ein", "Lied", "das", "dei\u00b7ne", "Lei\u00b7che", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und ob die Tugend zwar in eignen Ehren-Kronen/", "tokens": ["Und", "ob", "die", "Tu\u00b7gend", "zwar", "in", "eig\u00b7nen", "Eh\u00b7ren\u00b7Kro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lichtem Purpur-Kleid\u2019 als eine F\u00fcrstin prangt/", "tokens": ["Und", "lich\u00b7tem", "Pur\u00b7pur\u00b7Kleid'", "als", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "prangt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan durch die Treffligkeit der Thaten selbst ihr lohnen/", "tokens": ["Kan", "durch", "die", "Tref\u00b7flig\u00b7keit", "der", "Tha\u00b7ten", "selbst", "ihr", "loh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und weitgesuchten Ruhm noch Farben nicht verlangt;", "tokens": ["Und", "weit\u00b7ge\u00b7such\u00b7ten", "Ruhm", "noch", "Far\u00b7ben", "nicht", "ver\u00b7langt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So mutz doch unser Grab von beyden eines dulden/", "tokens": ["So", "mutz", "doch", "un\u00b7ser", "Grab", "von", "bey\u00b7den", "ei\u00b7nes", "dul\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wen frey den Urtheils-Spruch die kluge Nachwelt spricht/", "tokens": ["Wen", "frey", "den", "Urt\u00b7heils\u00b7Spruch", "die", "klu\u00b7ge", "Nach\u00b7welt", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wir entnommen sind/ Verfolgung/ Neid und Hulden/", "tokens": ["Und", "wir", "ent\u00b7nom\u00b7men", "sind", "/", "Ver\u00b7fol\u00b7gung", "/", "Neid", "und", "Hul\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie Verdienst\u2019 erhebt und Laster str\u00e4fflich richt.", "tokens": ["Da\u00df", "sie", "Ver\u00b7dienst'", "er\u00b7hebt", "und", "Las\u00b7ter", "str\u00e4ff\u00b7lich", "richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hochwerther Seeliger/ gewi\u00df da\u00df dir zu Ehren", "tokens": ["Hoch\u00b7wert\u00b7her", "See\u00b7li\u00b7ger", "/", "ge\u00b7wi\u00df", "da\u00df", "dir", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "ADV", "KOUS", "PPER", "APPR", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Astr", "tokens": ["Astr"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die Musen im Parna\u00df ihr Beyleid lassen h\u00f6ren/", "tokens": ["Die", "Mu\u00b7sen", "im", "Par\u00b7na\u00df", "ihr", "Bey\u00b7leid", "las\u00b7sen", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Hippocrenens Brunn voll Jammer-Fluthen quillt.", "tokens": ["Und", "Hip\u00b7po\u00b7cre\u00b7nens", "Brunn", "voll", "Jam\u00b7mer\u00b7Flu\u00b7then", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es zeugt noch Leucoris wie embsig du gewesen", "tokens": ["Es", "zeugt", "noch", "Leu\u00b7co\u00b7ris", "wie", "emb\u00b7sig", "du", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE", "KOKOM", "ADJD", "PPER", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie du umb den Verstand der Rechte dich bem\u00fcht/", "tokens": ["Wie", "du", "umb", "den", "Ver\u00b7stand", "der", "Rech\u00b7te", "dich", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df dein geh\u00e4uffter Flei\u00df und unauffh\u00f6rlich Lesen/", "tokens": ["Bi\u00df", "dein", "ge\u00b7h\u00e4uff\u00b7ter", "Flei\u00df", "und", "un\u00b7auff\u00b7h\u00f6r\u00b7lich", "Le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Lorbeer-Krantz erzielt der unverwe\u00dflich bl\u00fcht.", "tokens": ["Den", "Lor\u00b7beer\u00b7Krantz", "er\u00b7zielt", "der", "un\u00b7ver\u00b7we\u00df\u00b7lich", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Als dir die Themis nun den Tempel auffgeschlossen/", "tokens": ["Als", "dir", "die", "The\u00b7mis", "nun", "den", "Tem\u00b7pel", "auff\u00b7ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihrem Heiligthum zum Priester dich geweyht.", "tokens": ["In", "ih\u00b7rem", "Hei\u00b7lig\u00b7thum", "zum", "Pries\u00b7ter", "dich", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du ihrer W\u00fcrden Prei\u00df/ der Grossen Gunst genossen/", "tokens": ["Du", "ih\u00b7rer", "W\u00fcr\u00b7den", "Prei\u00df", "/", "der", "Gros\u00b7sen", "Gunst", "ge\u00b7nos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und das gelehrte Haupt der Purpur-Hut erfreut/", "tokens": ["Und", "das", "ge\u00b7lehr\u00b7te", "Haupt", "der", "Pur\u00b7pur\u00b7Hut", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lockt dich das Vaterland in seine Scho\u00df und Armen/", "tokens": ["Lockt", "dich", "das", "Va\u00b7ter\u00b7land", "in", "sei\u00b7ne", "Scho\u00df", "und", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dal\u00e4st du wol vergn\u00fcgt der Wei\u00dfheit Fr\u00fcchte schaun.", "tokens": ["Da\u00b7l\u00e4st", "du", "wol", "ver\u00b7gn\u00fcgt", "der", "Wei\u00df\u00b7heit", "Fr\u00fcch\u00b7te", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es g\u00f6nnt dir Feur und Herd/ worbey du solst erwarmen/", "tokens": ["Es", "g\u00f6nnt", "dir", "Feur", "und", "Herd", "/", "wor\u00b7bey", "du", "solst", "er\u00b7war\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "PWAV", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wil das Stadt-Vogts-Ampt bed\u00e4chtig dir vertraun.", "tokens": ["Und", "wil", "das", "Stadt\u00b7Vogts\u00b7Ampt", "be\u00b7d\u00e4ch\u00b7tig", "dir", "ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Wahl ist wol geschehn. Du hast Gericht gesessen/", "tokens": ["Die", "Wahl", "ist", "wol", "ge\u00b7schehn", ".", "Du", "hast", "Ge\u00b7richt", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "PPER", "VAFIN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gleich wie ein Richter pflegt der von Begierden frey/", "tokens": ["Gleich", "wie", "ein", "Rich\u00b7ter", "pflegt", "der", "von", "Be\u00b7gier\u00b7den", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "ART", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du hast der Sachen Grund und Umbstand wol ermessen/", "tokens": ["Du", "hast", "der", "Sa\u00b7chen", "Grund", "und", "U\u00b7mbstand", "wol", "er\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es hat dich nie verblendt Geschenck und Heucheley.", "tokens": ["Es", "hat", "dich", "nie", "ver\u00b7blendt", "Ge\u00b7schenck", "und", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Themis Augen sind vergebens nicht verbunden/", "tokens": ["Der", "The\u00b7mis", "Au\u00b7gen", "sind", "ver\u00b7ge\u00b7bens", "nicht", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dieweil ein Richter soll gantz unpartheyisch seyn.", "tokens": ["Die\u00b7weil", "ein", "Rich\u00b7ter", "soll", "gantz", "un\u00b7par\u00b7they\u00b7isch", "seyn", "."], "token_info": ["word", "w