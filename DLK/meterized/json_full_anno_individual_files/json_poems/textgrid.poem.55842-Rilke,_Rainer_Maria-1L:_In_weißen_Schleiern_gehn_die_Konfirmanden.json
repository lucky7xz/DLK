{"textgrid.poem.55842": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: In wei\u00dfen Schleiern gehn die Konfirmanden", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In wei\u00dfen Schleiern gehn die Konfirmanden", "tokens": ["In", "wei\u00b7\u00dfen", "Schlei\u00b7ern", "gehn", "die", "Kon\u00b7fir\u00b7man\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "tief in das neue Gr\u00fcn der G\u00e4rten ein.", "tokens": ["tief", "in", "das", "neu\u00b7e", "Gr\u00fcn", "der", "G\u00e4r\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie haben ihre Kindheit \u00fcberstanden,", "tokens": ["Sie", "ha\u00b7ben", "ih\u00b7re", "Kind\u00b7heit", "\u00fc\u00b7bers\u00b7tan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und was jetzt kommt, wird anders sein.", "tokens": ["und", "was", "jetzt", "kommt", ",", "wird", "an\u00b7ders", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O kommt es denn! Beginnt jetzt nicht die Pause,", "tokens": ["O", "kommt", "es", "denn", "!", "Be\u00b7ginnt", "jetzt", "nicht", "die", "Pau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das Warten auf den n\u00e4chsten Stundenschlag?", "tokens": ["das", "War\u00b7ten", "auf", "den", "n\u00e4chs\u00b7ten", "Stun\u00b7den\u00b7schlag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Fest ist aus, und es wird laut im Hause,", "tokens": ["Das", "Fest", "ist", "aus", ",", "und", "es", "wird", "laut", "im", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "KON", "PPER", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und trauriger vergeht der Nachmittag...", "tokens": ["und", "trau\u00b7ri\u00b7ger", "ver\u00b7geht", "der", "Nach\u00b7mit\u00b7tag", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das war ein Aufstehn zu dem wei\u00dfen Kleide", "tokens": ["Das", "war", "ein", "Auf\u00b7stehn", "zu", "dem", "wei\u00b7\u00dfen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und dann durch Gassen ein geschm\u00fccktes Gehn", "tokens": ["und", "dann", "durch", "Gas\u00b7sen", "ein", "ge\u00b7schm\u00fcck\u00b7tes", "Gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und eine Kirche, innen k\u00fchl wie Seide,", "tokens": ["und", "ei\u00b7ne", "Kir\u00b7che", ",", "in\u00b7nen", "k\u00fchl", "wie", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und lange Kerzen waren wie Alleen,", "tokens": ["und", "lan\u00b7ge", "Ker\u00b7zen", "wa\u00b7ren", "wie", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und alle Lichter schienen wie Geschmeide,", "tokens": ["und", "al\u00b7le", "Lich\u00b7ter", "schie\u00b7nen", "wie", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "von feierlichen Augen angesehn.", "tokens": ["von", "fei\u00b7er\u00b7li\u00b7chen", "Au\u00b7gen", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und es war still, als der Gesang begann:", "tokens": ["Und", "es", "war", "still", ",", "als", "der", "Ge\u00b7sang", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wie Wolken stieg er in der W\u00f6lbung an", "tokens": ["Wie", "Wol\u00b7ken", "stieg", "er", "in", "der", "W\u00f6l\u00b7bung", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und wurde hell im Niederfall; und linder", "tokens": ["und", "wur\u00b7de", "hell", "im", "Nie\u00b7der\u00b7fall", ";", "und", "lin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "APPRART", "NN", "$.", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "denn Regen fiel er in die wei\u00dfen Kinder.", "tokens": ["denn", "Re\u00b7gen", "fiel", "er", "in", "die", "wei\u00b7\u00dfen", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie im Wind bewegte sich ihr Wei\u00df,", "tokens": ["Und", "wie", "im", "Wind", "be\u00b7weg\u00b7te", "sich", "ihr", "Wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und wurde leise bunt in seinen Falten", "tokens": ["und", "wur\u00b7de", "lei\u00b7se", "bunt", "in", "sei\u00b7nen", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und schien verborgne Blumen zu enthalten \u2013:", "tokens": ["und", "schien", "ver\u00b7borg\u00b7ne", "Blu\u00b7men", "zu", "ent\u00b7hal\u00b7ten", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJA", "