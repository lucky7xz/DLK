{"dta.poem.19581": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das verlorene Schwert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Der Gallier letzte Burg und Stadt erlag", "tokens": ["Der", "Gal\u00b7lier", "letz\u00b7te", "Burg", "und", "Stadt", "er\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach einem letzten durchgek\u00e4mpften Tag", "tokens": ["Nach", "ei\u00b7nem", "letz\u00b7ten", "durch\u00b7ge\u00b7k\u00e4mpf\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Julius C\u00e4sar tritt in ihren Hain,", "tokens": ["Und", "Ju\u00b7lius", "C\u00e4\u00b7sar", "tritt", "in", "ih\u00b7ren", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In ihren stillen G\u00f6ttertempel ein.", "tokens": ["In", "ih\u00b7ren", "stil\u00b7len", "G\u00f6t\u00b7ter\u00b7tem\u00b7pel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Weihgeschenke sieht geh\u00e4uft er dort,", "tokens": ["Die", "Weih\u00b7ge\u00b7schen\u00b7ke", "sieht", "ge\u00b7h\u00e4uft", "er", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Gold und Silber manchen lichten Hort", "tokens": ["Von", "Gold", "und", "Sil\u00b7ber", "man\u00b7chen", "lich\u00b7ten", "Hort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und edeln Raub. Doch \u00fcber Hort und Schatz", "tokens": ["Und", "e\u00b7deln", "Raub", ".", "Doch", "\u00fc\u00b7ber", "Hort", "und", "Schatz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "KON", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hangt ein erbeutet Schwert am Ehrenplatz.", "tokens": ["Hangt", "ein", "er\u00b7beu\u00b7tet", "Schwert", "am", "Eh\u00b7ren\u00b7platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVPP", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es ist die R\u00f6merklinge kurz und schlicht \u2014", "tokens": ["Es", "ist", "die", "R\u00f6\u00b7mer\u00b7klin\u00b7ge", "kurz", "und", "schlicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Des Juliers scharfer Blick verl\u00e4\u00dft sie nicht,", "tokens": ["Des", "Ju\u00b7liers", "schar\u00b7fer", "Blick", "ver\u00b7l\u00e4\u00dft", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er haftet auf der Waffe wie gebannt,", "tokens": ["Er", "haf\u00b7tet", "auf", "der", "Waf\u00b7fe", "wie", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sie d\u00e4ucht dem Sieger wunderlich bekannt!", "tokens": ["Sie", "d\u00e4ucht", "dem", "Sie\u00b7ger", "wun\u00b7der\u00b7lich", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mit einem L\u00e4cheln deutet er empor:", "tokens": ["Mit", "ei\u00b7nem", "L\u00e4\u00b7cheln", "deu\u00b7tet", "er", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u201eein armer Fechter der sein Schwert verlor!\u201c", "tokens": ["\u201e", "ein", "ar\u00b7mer", "Fech\u00b7ter", "der", "sein", "Schwert", "ver\u00b7lor", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da ruft ein junger Gallier aufgebracht:", "tokens": ["Da", "ruft", "ein", "jun\u00b7ger", "Gal\u00b7lier", "auf\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "\u201edu selbst verlorest's im Gedr\u00e4ng der Schlacht!\u201c", "tokens": ["\u201e", "du", "selbst", "ver\u00b7lo\u00b7rest's", "im", "Ge\u00b7dr\u00e4ng", "der", "Schlacht", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit zorn'ger Faust ergreift's ein Legionar \u2014", "tokens": ["Mit", "zorn'\u00b7ger", "Faust", "er\u00b7greift's", "ein", "Le\u00b7gi\u00b7o\u00b7nar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "\u201enein, tapfrer Strabo, la\u00df es dem Altar!", "tokens": ["\u201e", "nein", ",", "tapf\u00b7rer", "Stra\u00b7bo", ",", "la\u00df", "es", "dem", "Al\u00b7tar", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJA", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Verloren ging's in steilem Siegeslauf", "tokens": ["Ver\u00b7lo\u00b7ren", "ging's", "in", "stei\u00b7lem", "Sie\u00b7ges\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und hei\u00dfem Ringen. G\u00f6tter hoben's auf.\u201c", "tokens": ["Und", "hei\u00b7\u00dfem", "Rin\u00b7gen", ".", "G\u00f6t\u00b7ter", "ho\u00b7ben's", "auf", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}