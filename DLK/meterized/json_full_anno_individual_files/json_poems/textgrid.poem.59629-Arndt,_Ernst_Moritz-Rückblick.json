{"textgrid.poem.59629": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckblick", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, weine nur und schau' zur\u00fcck", "tokens": ["Ja", ",", "wei\u00b7ne", "nur", "und", "schau'", "zu\u00b7r\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In goldne Jugendferne,", "tokens": ["In", "gold\u00b7ne", "Ju\u00b7gend\u00b7fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja, suche nur das alte Gl\u00fcck,", "tokens": ["Ja", ",", "su\u00b7che", "nur", "das", "al\u00b7te", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten hellen Sterne,", "tokens": ["Die", "al\u00b7ten", "hel\u00b7len", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den alten Lenz im Bl\u00fctenkranz \u2013", "tokens": ["Den", "al\u00b7ten", "Lenz", "im", "Bl\u00fc\u00b7ten\u00b7kranz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was blieb von jener Wonne ganz?", "tokens": ["Was", "blieb", "von", "je\u00b7ner", "Won\u00b7ne", "ganz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach ferne! Ach ferne!", "tokens": ["Ach", "fer\u00b7ne", "!", "Ach", "fer\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ITJ", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie hast du jenes edle Gut,", "tokens": ["Wie", "hast", "du", "je\u00b7nes", "ed\u00b7le", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das dir der Herr vertrauet,", "tokens": ["Das", "dir", "der", "Herr", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So frische Kraft, so k\u00fchnen Mut", "tokens": ["So", "fri\u00b7sche", "Kraft", ",", "so", "k\u00fch\u00b7nen", "Mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Arbeit durchgebauet?", "tokens": ["In", "Ar\u00b7beit", "durch\u00b7ge\u00b7bau\u00b7et", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf! Vorgezeigt das Kapital,", "tokens": ["Auf", "!", "Vor\u00b7ge\u00b7zeigt", "das", "Ka\u00b7pi\u00b7tal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gemehret zehn- und hundertmal!", "tokens": ["Ge\u00b7meh\u00b7ret", "zehn", "und", "hun\u00b7dert\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "TRUNC", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dir grauet? Dir grauet?", "tokens": ["Dir", "grau\u00b7et", "?", "Dir", "grau\u00b7et", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Mir graut, und eine Tr\u00e4ne f\u00e4llt", "tokens": ["Mir", "graut", ",", "und", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "f\u00e4llt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Saitenspiel des Lebens,", "tokens": ["Ins", "Sai\u00b7ten\u00b7spiel", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sind die T\u00f6ne weggeschnellt,", "tokens": ["Wie", "sind", "die", "T\u00f6\u00b7ne", "weg\u00b7ge\u00b7schnellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kl\u00e4nge h\u00f6chsten Strebens", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "h\u00f6chs\u00b7ten", "Stre\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Tr\u00e4umerei und Narretei!", "tokens": ["In", "Tr\u00e4u\u00b7me\u00b7rei", "und", "Nar\u00b7re\u00b7tei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fernher t\u00f6nt ein dumpfer S