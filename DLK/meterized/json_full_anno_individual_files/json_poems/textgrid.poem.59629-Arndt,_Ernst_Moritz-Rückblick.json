: "Und aus der Seele Tiefen singt", "tokens": ["Und", "aus", "der", "See\u00b7le", "Tie\u00b7fen", "singt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Spruch der Ewigkeiten:", "tokens": ["Der", "Spruch", "der", "E\u00b7wig\u00b7kei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbknie nieder, Mensch, und werde klein!", "tokens": ["\u00bb", "knie", "nie\u00b7der", ",", "Mensch", ",", "und", "wer\u00b7de", "klein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "NN", "$,", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott, der dich schuf, wird gn\u00e4dig sein:", "tokens": ["Gott", ",", "der", "dich", "schuf", ",", "wird", "gn\u00e4\u00b7dig", "sein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Beim Schreiten ist Gleiten.\u00ab", "tokens": ["Beim", "Schrei\u00b7ten", "ist", "Glei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "O frommer Spruch, wohl machst du klein", "tokens": ["O", "from\u00b7mer", "Spruch", ",", "wohl", "machst", "du", "klein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und machst doch stark im Streiten;", "tokens": ["Und", "machst", "doch", "stark", "im", "Strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wandr' ich wohlgemut hinein,", "tokens": ["So", "wandr'", "ich", "wohl\u00b7ge\u00b7mut", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo's schreiten gilt und gleiten.", "tokens": ["Wo's", "schrei\u00b7ten", "gilt", "und", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Spruch singt noch, und der macht frei:", "tokens": ["Ein", "Spruch", "singt", "noch", ",", "und", "der", "macht", "frei", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "ART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbgott ist dabei, Gott stehet bei", "tokens": ["\u00bb", "gott", "ist", "da\u00b7bei", ",", "Gott", "ste\u00b7het", "bei"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "PAV", "$,", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Schreiten, im Gleiten.\u00ab", "tokens": ["Im", "Schrei\u00b7ten", ",", "im", "Glei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}