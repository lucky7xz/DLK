{"dta.poem.8695": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Robert und der Invalide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Es bellen \u2014 sagtest du \u2014 zum Mondenschein", "tokens": ["Es", "bel\u00b7len", "sag\u00b7test", "du", "zum", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$(", "VVFIN", "PPER", "$(", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Hunde; \u2014 ja \u2014 den Hunden h\u00e4tt' ich sollen,", "tokens": ["Die", "Hun\u00b7de", ";", "ja", "den", "Hun\u00b7den", "h\u00e4tt'", "ich", "sol\u00b7len", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "PTKANT", "$(", "ART", "NN", "VAFIN", "PPER", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als einst der laute Ruf zur Schlacht erschollen,", "tokens": ["Als", "einst", "der", "lau\u00b7te", "Ruf", "zur", "Schlacht", "er\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Futter werfen lieber vor mein Bein,", "tokens": ["Zum", "Fut\u00b7ter", "wer\u00b7fen", "lie\u00b7ber", "vor", "mein", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als da\u00df ich's im berauschten Sturmesflug", "tokens": ["Als", "da\u00df", "ich's", "im", "be\u00b7rauschten", "Stur\u00b7mes\u00b7flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zum blutgetr\u00e4nkten Opferherde trug.", "tokens": ["Zum", "blut\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Op\u00b7fer\u00b7her\u00b7de", "trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum Opferherde trug ich's? \u2014 Herd der K\u00fcche", "tokens": ["Zum", "Op\u00b7fer\u00b7her\u00b7de", "trug", "ich's", "?", "Herd", "der", "K\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$.", "$(", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "War jenes Leipzigfeld voll Flamm' und Rauch!", "tokens": ["War", "je\u00b7nes", "Leip\u00b7zig\u00b7feld", "voll", "Flamm'", "und", "Rauch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zerrissne Glieder, Leichen, Donnerfl\u00fcche,", "tokens": ["Zer\u00b7riss\u00b7ne", "Glie\u00b7der", ",", "Lei\u00b7chen", ",", "Don\u00b7ner\u00b7fl\u00fc\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Gebrochne Waisen-, Mutterherzen auch,", "tokens": ["Ge\u00b7broch\u00b7ne", "Wai\u00b7sen", ",", "Mut\u00b7ter\u00b7her\u00b7zen", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "TRUNC", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das Schlachtgefl\u00fcgel auch, \u2014 vom b\u00f6sen Wetter", "tokens": ["Das", "Schlacht\u00b7ge\u00b7fl\u00fc\u00b7gel", "auch", ",", "vom", "b\u00f6\u00b7sen", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "$(", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Napoleon gejagt aus Frankreichs Auen: \u2014", "tokens": ["Na\u00b7po\u00b7le\u00b7on", "ge\u00b7jagt", "aus", "Fran\u00b7kreichs", "Au\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVPP", "APPR", "NE", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das Alles ward vom Chor der Freiheitsretter", "tokens": ["Das", "Al\u00b7les", "ward", "vom", "Chor", "der", "Frei\u00b7heits\u00b7ret\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In ein Gericht zusammen dort gehauen,", "tokens": ["In", "ein", "Ge\u00b7richt", "zu\u00b7sam\u00b7men", "dort", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Woran das Gl\u00fcck nun der Aristokraten", "tokens": ["Wo\u00b7ran", "das", "Gl\u00fcck", "nun", "der", "A\u00b7ris\u00b7to\u00b7kra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sich schwelgend m\u00e4stet, da zu ihrer Schmach", "tokens": ["Sich", "schwel\u00b7gend", "m\u00e4s\u00b7tet", ",", "da", "zu", "ih\u00b7rer", "Schmach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Im Lande zieh'n verst\u00fcmmelte Soldaten,", "tokens": ["Im", "Lan\u00b7de", "zieh'n", "ver\u00b7st\u00fcm\u00b7mel\u00b7te", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und betteln m\u00fcssen um ein mildes Dach.", "tokens": ["Und", "bet\u00b7teln", "m\u00fcs\u00b7sen", "um", "ein", "mil\u00b7des", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Man hat ein Glied vom Leibe mir gerissen,", "tokens": ["Man", "hat", "ein", "Glied", "vom", "Lei\u00b7be", "mir", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Den schlechten Rest dem Hunger vorgeschmissen.", "tokens": ["Den", "schlech\u00b7ten", "Rest", "dem", "Hun\u00b7ger", "vor\u00b7ge\u00b7schmis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Das sind die Menschen ohne Dank nicht werth,", "tokens": ["Das", "sind", "die", "Men\u00b7schen", "oh\u00b7ne", "Dank", "nicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Da\u00df ich f\u00fcr sie gezogen einst mein Schwert,", "tokens": ["Da\u00df", "ich", "f\u00fcr", "sie", "ge\u00b7zo\u00b7gen", "einst", "mein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Da\u00df ich ein Bettelkr\u00fcppel auf der Heide,", "tokens": ["Da\u00df", "ich", "ein", "Bet\u00b7tel\u00b7kr\u00fcp\u00b7pel", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Umhinke, ", "tokens": ["Um\u00b7hin\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Und decke meinen Leib mit ", "tokens": ["Und", "de\u00b7cke", "mei\u00b7nen", "Leib", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Bis diese dumpfe Trommel ausgeschlagen", "tokens": ["Bis", "die\u00b7se", "dump\u00b7fe", "Trom\u00b7mel", "aus\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Den Trauermarsch: das Herz da stille steht,", "tokens": ["Den", "Trau\u00b7er\u00b7marsch", ":", "das", "Herz", "da", "stil\u00b7le", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und den vergessnen Staub der Wind verweht! \u2014", "tokens": ["Und", "den", "ver\u00b7gess\u00b7nen", "Staub", "der", "Wind", "ver\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}