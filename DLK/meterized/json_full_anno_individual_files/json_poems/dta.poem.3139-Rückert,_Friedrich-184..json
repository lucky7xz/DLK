{"dta.poem.3139": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "184.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der B\u00fccher sind zu viel, um noch so viel zu gelten;", "tokens": ["Der", "B\u00fc\u00b7cher", "sind", "zu", "viel", ",", "um", "noch", "so", "viel", "zu", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "PIS", "$,", "KOUI", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wohlfeil ist die Meng', und theuer nur was selten.", "tokens": ["Denn", "wohl\u00b7feil", "ist", "die", "Meng'", ",", "und", "theu\u00b7er", "nur", "was", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "ADV", "PWS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit ihnen ists wie mit den Menschen selbst gethan;", "tokens": ["Mit", "ih\u00b7nen", "ists", "wie", "mit", "den", "Men\u00b7schen", "selbst", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "KOKOM", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den, der mit vielen lebt, gehn wenig n\u00e4her an.", "tokens": ["Den", ",", "der", "mit", "vie\u00b7len", "lebt", ",", "gehn", "we\u00b7nig", "n\u00e4\u00b7her", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$,", "VVFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man sieht sie an, allein wer kann sie alle nennen,", "tokens": ["Man", "sieht", "sie", "an", ",", "al\u00b7lein", "wer", "kann", "sie", "al\u00b7le", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "PWS", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkennen ihren Werth, wie sie vor\u00fcber rennen?", "tokens": ["Er\u00b7ken\u00b7nen", "ih\u00b7ren", "Werth", ",", "wie", "sie", "vor\u00b7\u00fc\u00b7ber", "ren\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich leb' in kleiner Stadt, sie ist mir fast zu gro\u00df;", "tokens": ["Ich", "leb'", "in", "klei\u00b7ner", "Stadt", ",", "sie", "ist", "mir", "fast", "zu", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All seine Nachbarn liebt man auf dem Dorfe blo\u00df.", "tokens": ["All", "sei\u00b7ne", "Nach\u00b7barn", "liebt", "man", "auf", "dem", "Dor\u00b7fe", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dort hat man keine Wahl, man braucht die ganze Zahl;", "tokens": ["Dort", "hat", "man", "kei\u00b7ne", "Wahl", ",", "man", "braucht", "die", "gan\u00b7ze", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIAT", "NN", "$,", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier stellt zumal die Qual sich ein mit Zahl und Wahl.", "tokens": ["Hier", "stellt", "zu\u00b7mal", "die", "Qual", "sich", "ein", "mit", "Zahl", "und", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PRF", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich aber ungequ\u00e4lt hab' einen Freund gew\u00e4hlt,", "tokens": ["Ich", "a\u00b7ber", "un\u00b7ge\u00b7qu\u00e4lt", "hab'", "ei\u00b7nen", "Freund", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mir die B\u00fccher w\u00e4hlt, da\u00df mich die Zahl nicht qu\u00e4lt.", "tokens": ["Der", "mir", "die", "B\u00fc\u00b7cher", "w\u00e4hlt", ",", "da\u00df", "mich", "die", "Zahl", "nicht", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}