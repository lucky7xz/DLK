.tri"}, "line.3": {"text": "Wei\u00df man einmal, was man will,", "tokens": ["Wei\u00df", "man", "ein\u00b7mal", ",", "was", "man", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheut man keine Plage.", "tokens": ["Scheut", "man", "kei\u00b7ne", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "M\u00fctterchen zergr\u00fcbelt sich,", "tokens": ["M\u00fct\u00b7ter\u00b7chen", "zer\u00b7gr\u00fc\u00b7belt", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Streicht die wei\u00dfen Haare,", "tokens": ["Streicht", "die", "wei\u00b7\u00dfen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4umt so mancherlei f\u00fcr mich,", "tokens": ["Tr\u00e4umt", "so", "man\u00b7cher\u00b7lei", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umt sich nicht das Wahre.", "tokens": ["Tr\u00e4umt", "sich", "nicht", "das", "Wah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Schrecklich ist die Einsamkeit", "tokens": ["Schreck\u00b7lich", "ist", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur auf Gottes Erden.", "tokens": ["Nur", "auf", "Got\u00b7tes", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00f6n ist auch ein Gl\u00fcck zu zweit,", "tokens": ["Sch\u00f6n", "ist", "auch", "ein", "Gl\u00fcck", "zu", "zweit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will's zu dritt nicht werden.", "tokens": ["Will's", "zu", "dritt", "nicht", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVFIN", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Kommen viele Jahre noch,", "tokens": ["Kom\u00b7men", "vie\u00b7le", "Jah\u00b7re", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Langes kaltes Sterben;", "tokens": ["Lan\u00b7ges", "kal\u00b7tes", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durft ein einzig Mal ich doch", "tokens": ["Durft", "ein", "ein\u00b7zig", "Mal", "ich", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um mein Schicksal werben!", "tokens": ["Um", "mein", "Schick\u00b7sal", "wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Not und Schande, Angst und Pein,", "tokens": ["Not", "und", "Schan\u00b7de", ",", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles will ich tragen.", "tokens": ["Al\u00b7les", "will", "ich", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird es nur kein ", "tokens": ["Wird", "es", "nur", "kein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PIAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Will ich gar nicht klagen.", "tokens": ["Will", "ich", "gar", "nicht", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}