{"textgrid.poem.42195": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Franziskas Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df die Mutter doch so gut,", "tokens": ["Wei\u00df", "die", "Mut\u00b7ter", "doch", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann die \u00c4pfel reifen.", "tokens": ["Wann", "die", "\u00c4p\u00b7fel", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ihr eigen Fleisch und Blut", "tokens": ["Und", "ihr", "ei\u00b7gen", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will sie nicht begreifen!", "tokens": ["Will", "sie", "nicht", "be\u00b7grei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich nicht so trostlos w\u00e4r,", "tokens": ["Wenn", "ich", "nicht", "so", "trost\u00b7los", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ging's mir wohl um Treue;", "tokens": ["Ging's", "mir", "wohl", "um", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kommt das Gl\u00fcck von ungef\u00e4hr,", "tokens": ["Kommt", "das", "Gl\u00fcck", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgt ihm keine Reue.", "tokens": ["Folgt", "ihm", "kei\u00b7ne", "Reu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Seht euch nur dies Leben an,", "tokens": ["Seht", "euch", "nur", "dies", "Le\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fchner, Enten, G\u00e4nse \u2013", "tokens": ["H\u00fch\u00b7ner", ",", "En\u00b7ten", ",", "G\u00e4n\u00b7se", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dr\u00fcben schwingt der Schnittersmann", "tokens": ["Dr\u00fc\u00b7ben", "schwingt", "der", "Schnit\u00b7ters\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die blanke Sense.", "tokens": ["Schon", "die", "blan\u00b7ke", "Sen\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Baut ich auf den lieben Gott,", "tokens": ["Baut", "ich", "auf", "den", "lie\u00b7ben", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Baut auf meine Karten,", "tokens": ["Baut", "auf", "mei\u00b7ne", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ward bei beiden mir zum Spott,", "tokens": ["Ward", "bei", "bei\u00b7den", "mir", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lernte flei\u00dfig warten!", "tokens": ["Lern\u00b7te", "flei\u00b7\u00dfig", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Zwanzig Sommer sind vorbei,", "tokens": ["Zwan\u00b7zig", "Som\u00b7mer", "sind", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Armes kurzes Leben \u2013", "tokens": ["Ar\u00b7mes", "kur\u00b7zes", "Le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hast nun einen s\u00fc\u00dfen Mai", "tokens": ["Hast", "nun", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich doch gegeben!", "tokens": ["Heim\u00b7lich", "doch", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ist die Nacht nicht gar so still,", "tokens": ["Ist", "die", "Nacht", "nicht", "gar", "so", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiller wird's am Tage;", "tokens": ["Stil\u00b7ler", "wird's", "am", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wei\u00df man einmal, was man will,", "tokens": ["Wei\u00df", "man", "ein\u00b7mal", ",", "was", "man", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheut man keine Plage.", "tokens": ["Scheut", "man", "kei\u00b7ne", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "M\u00fctterchen zergr\u00fcbelt sich,", "tokens": ["M\u00fct\u00b7ter\u00b7chen", "zer\u00b7gr\u00fc\u00b7belt", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Streicht die wei\u00dfen Haare,", "tokens": ["Streicht", "die", "wei\u00b7\u00dfen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4umt so mancherlei f\u00fcr mich,", "tokens": ["Tr\u00e4umt", "so", "man\u00b7cher\u00b7lei", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umt sich nicht das Wahre.", "tokens": ["Tr\u00e4umt", "sich", "nicht", "das", "Wah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Schrecklich ist die Einsamkeit", "tokens": ["Schreck\u00b7lich", "ist", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur auf Gottes Erden.", "tokens": ["Nur", "auf", "Got\u00b7tes", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sch\u00f6n ist auch ein Gl\u00fcck zu zweit,", "tokens": ["Sch\u00f6n", "ist", "auch", "ein", "Gl\u00fcck", "zu", "zweit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will's zu dritt nicht werden.", "tokens": ["Will's", "zu", "dritt", "nicht", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVFIN", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Kommen viele Jahre noch,", "tokens": ["Kom\u00b7men", "vie\u00b7le", "Jah\u00b7re", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Langes kaltes Sterben;", "tokens": ["Lan\u00b7ges", "kal\u00b7tes", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durft ein einzig Mal ich doch", "tokens": ["Durft", "ein", "ein\u00b7zig", "Mal", "ich", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um mein Schicksal werben!", "tokens": ["Um", "mein", "Schick\u00b7sal", "wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Not und Schande, Angst und Pein,", "tokens": ["Not", "und", "Schan\u00b7de", ",", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles will ich tragen.", "tokens": ["Al\u00b7les", "will", "ich", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird es nur kein ", "tokens": ["Wird", "es", "nur", "kein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PIAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Will ich gar nicht klagen.", "tokens": ["Will", "ich", "gar", "nicht", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Wei\u00df die Mutter doch so gut,", "tokens": ["Wei\u00df", "die", "Mut\u00b7ter", "doch", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measur