asure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Seht euch nur dies Leben an,", "tokens": ["Seht", "euch", "nur", "dies", "Le\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fchner, Enten, G\u00e4nse \u2013", "tokens": ["H\u00fch\u00b7ner", ",", "En\u00b7ten", ",", "G\u00e4n\u00b7se", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dr\u00fcben schwingt der Schnittersmann", "tokens": ["Dr\u00fc\u00b7ben", "schwingt", "der", "Schnit\u00b7ters\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die blanke Sense.", "tokens": ["Schon", "die", "blan\u00b7ke", "Sen\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Baut ich auf den lieben Gott,", "tokens": ["Baut", "ich", "auf", "den", "lie\u00b7ben", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Baut auf meine Karten,", "tokens": ["Baut", "auf", "mei\u00b7ne", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ward bei beiden mir zum Spott,", "tokens": ["Ward", "bei", "bei\u00b7den", "mir", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "