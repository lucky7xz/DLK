{"dta.poem.187": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er  Bellandren  auf einer Hochzeit sahe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.85", "sv:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sagt Augen/ sagt es doch/ seh ich Bellandren hier?", "tokens": ["Sagt", "Au\u00b7gen", "/", "sagt", "es", "doch", "/", "seh", "ich", "Bel\u00b7land\u00b7ren", "hier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "PPER", "ADV", "$(", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ja! denn ihr Angesicht geht allen ", "tokens": ["Ja", "!", "denn", "ihr", "An\u00b7ge\u00b7sicht", "geht", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KON", "PPOSAT", "NN", "VVFIN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es sieht die ", "tokens": ["Es", "sieht", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Schaut wie man unvermerckt ein Gifft hierein mu\u00df saugen/", "tokens": ["Schaut", "wie", "man", "un\u00b7ver\u00b7merckt", "ein", "Gifft", "hier\u00b7ein", "mu\u00df", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PIS", "ADV", "ART", "NN", "PAV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das man vor Lust nicht schmeckt bis es die Wirckung thut/", "tokens": ["Das", "man", "vor", "Lust", "nicht", "schmeckt", "bis", "es", "die", "Wir\u00b7ckung", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "NN", "PTKNEG", "VVFIN", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und spielend durchgew\u00fchlt Gehirn/ Hertz/ Marck und Blut/", "tokens": ["Und", "spie\u00b7lend", "durch\u00b7ge\u00b7w\u00fchlt", "Ge\u00b7hirn", "/", "Hertz", "/", "Marck", "und", "Blut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch zagt nicht/ denn ihr seht auch ein Paar sch\u00f6ne Br\u00fcste", "tokens": ["Doch", "zagt", "nicht", "/", "denn", "ihr", "seht", "auch", "ein", "Paar", "sch\u00f6\u00b7ne", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$(", "KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die heilen allen Schmertz durch ungemeine L\u00fcste.", "tokens": ["Die", "hei\u00b7len", "al\u00b7len", "Schmertz", "durch", "un\u00b7ge\u00b7mei\u00b7ne", "L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es wird der Liebes-Brand gel\u00f6schet und gek\u00fchlt/", "tokens": ["Es", "wird", "der", "Lie\u00b7bes\u00b7Brand", "ge\u00b7l\u00f6\u00b7schet", "und", "ge\u00b7k\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn der entz\u00fcckte Geist um Brust und Lippen spielt/", "tokens": ["Wenn", "der", "ent\u00b7z\u00fcck\u00b7te", "Geist", "um", "Brust", "und", "Lip\u00b7pen", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch Augen schlie\u00dft euch zu/ ihr habt hier nichts zu hoffen/", "tokens": ["Doch", "Au\u00b7gen", "schlie\u00dft", "euch", "zu", "/", "ihr", "habt", "hier", "nichts", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKZU", "$(", "PPER", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Euch steht das Auge zwar/ doch nicht der Busen offen.", "tokens": ["Euch", "steht", "das", "Au\u00b7ge", "zwar", "/", "doch", "nicht", "der", "Bu\u00b7sen", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$(", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}