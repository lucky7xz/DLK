{"dta.poem.4278": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbter Geist, besinne dich!", "tokens": ["Be\u00b7tr\u00fcb\u00b7ter", "Geist", ",", "be\u00b7sin\u00b7ne", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist dir selber hinderlich;", "tokens": ["Du", "bist", "dir", "sel\u00b7ber", "hin\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du \u00fcberlegest nicht den Wehrt", "tokens": ["Du", "\u00fc\u00b7berl\u00b7e\u00b7gest", "nicht", "den", "Wehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Guten, so dir Gott beschehrt.", "tokens": ["Des", "Gu\u00b7ten", ",", "so", "dir", "Gott", "be\u00b7schehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die\u00df ist der Grund, warum die Welt,", "tokens": ["Die\u00df", "ist", "der", "Grund", ",", "wa\u00b7rum", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gott erschuf, dir nicht gef\u00e4llt.", "tokens": ["Die", "Gott", "er\u00b7schuf", ",", "dir", "nicht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch jedes Sinnes offnes Thor", "tokens": ["Durch", "je\u00b7des", "Sin\u00b7nes", "off\u00b7nes", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt Gott dir tausend Sch\u00e4tze vor,", "tokens": ["Legt", "Gott", "dir", "tau\u00b7send", "Sch\u00e4t\u00b7ze", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er baut in solchem Ueberflu\u00df", "tokens": ["Er", "baut", "in", "sol\u00b7chem", "Ue\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir so viel G\u00fcter zum Genu\u00df.", "tokens": ["Dir", "so", "viel", "G\u00fc\u00b7ter", "zum", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Du brauchest, nimmst und n\u00fctzest sie;", "tokens": ["Du", "brau\u00b7chest", ",", "nimmst", "und", "n\u00fct\u00b7zest", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch du erfreust dich ihrer nie.", "tokens": ["Doch", "du", "er\u00b7freust", "dich", "ih\u00b7rer", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie manchen Schatz, wie manches Gut", "tokens": ["Wie", "man\u00b7chen", "Schatz", ",", "wie", "man\u00b7ches", "Gut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Reicht dir die Erde, Luft und Fluht?", "tokens": ["Reicht", "dir", "die", "Er\u00b7de", ",", "Luft", "und", "Fluht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zinsen dir Trank, Speis und Kleid,", "tokens": ["Sie", "zin\u00b7sen", "dir", "Trank", ",", "Speis", "und", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Nohtdurft, zur Bequemlichkeit.", "tokens": ["Zur", "Noht\u00b7durft", ",", "zur", "Be\u00b7quem\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was zeiget dir der Sonnen Licht", "tokens": ["Was", "zei\u00b7get", "dir", "der", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr ungez\u00e4hlte Sch\u00f6nheit nicht?", "tokens": ["F\u00fcr", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Sch\u00f6n\u00b7heit", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es zeugt und zeigt ihr Wunder-Strahl", "tokens": ["Es", "zeugt", "und", "zeigt", "ihr", "Wun\u00b7der\u00b7Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir W\u00e4lder, Felder, Berg\u2019 und Thal,", "tokens": ["Dir", "W\u00e4l\u00b7der", ",", "Fel\u00b7der", ",", "Ber\u00b7g'", "und", "Thal", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Geb\u00fcsche, G\u00e4rten, Bluhmen, Gras,", "tokens": ["Ge\u00b7b\u00fc\u00b7sche", ",", "G\u00e4r\u00b7ten", ",", "Bluh\u00b7men", ",", "Gras", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Korn, Obst, zusamt der Reben Na\u00df.", "tokens": ["Korn", ",", "Obst", ",", "zu\u00b7samt", "der", "Re\u00b7ben", "Na\u00df", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit V\u00f6geln, Fischen, Thieren, Wild", "tokens": ["Mit", "V\u00f6\u00b7geln", ",", "Fi\u00b7schen", ",", "Thie\u00b7ren", ",", "Wild"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat Gott f\u00fcr dich die Welt erf\u00fcllt.", "tokens": ["Hat", "Gott", "f\u00fcr", "dich", "die", "Welt", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du sprichst: Die\u00df alles ist zwar wahr:", "tokens": ["Du", "sprichst", ":", "Die\u00df", "al\u00b7les", "ist", "zwar", "wahr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "PIS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allein, wie vielerley Gefahr,", "tokens": ["Al\u00b7lein", ",", "wie", "vie\u00b7ler\u00b7ley", "Ge\u00b7fahr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gram, Ungl\u00fcck, Sorgen, Herzeleid", "tokens": ["Gram", ",", "Un\u00b7gl\u00fcck", ",", "Sor\u00b7gen", ",", "Her\u00b7ze\u00b7leid"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbittern meine Lebens-Zeit!", "tokens": ["Ver\u00b7bit\u00b7tern", "mei\u00b7ne", "Le\u00b7bens\u00b7Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man h\u00f6rt fast nichts, als Streit und Zank,", "tokens": ["Man", "h\u00f6rt", "fast", "nichts", ",", "als", "Streit", "und", "Zank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald bin ich, bald die Meinen krank.", "tokens": ["Bald", "bin", "ich", ",", "bald", "die", "Mei\u00b7nen", "krank", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In unser Jugend, wie bekannt,", "tokens": ["In", "un\u00b7ser", "Ju\u00b7gend", ",", "wie", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir voll Schw\u00e4ch\u2019 und Unverstand;", "tokens": ["Sind", "wir", "voll", "Schw\u00e4ch'", "und", "Un\u00b7ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Grwachsen, foltert uns die Kraft", "tokens": ["Gr\u00b7wach\u00b7sen", ",", "fol\u00b7tert", "uns", "die", "Kraft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verf\u00fchrerischer Leidenschaft,", "tokens": ["Ver\u00b7f\u00fch\u00b7re\u00b7ri\u00b7scher", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sch\u00e4umet das erhitzte Blut,", "tokens": ["Dann", "sch\u00e4u\u00b7met", "das", "er\u00b7hitz\u00b7te", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann spornt uns Stolz und Uebermuht.", "tokens": ["Dann", "spornt", "uns", "Stolz", "und", "Ue\u00b7ber\u00b7muht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wird man denn mit den Jahren alt,", "tokens": ["Wird", "man", "denn", "mit", "den", "Jah\u00b7ren", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird Geist und C\u00f6rper ungestalt,", "tokens": ["Wird", "Geist", "und", "C\u00f6r\u00b7per", "un\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns plagt die Furcht, uns nagt der Neid,", "tokens": ["Uns", "plagt", "die", "Furcht", ",", "uns", "nagt", "der", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geiz, Argwohn, Unlust, Gr\u00e4mlichkeit,", "tokens": ["Geiz", ",", "Arg\u00b7wohn", ",", "Un\u00b7lust", ",", "Gr\u00e4m\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nebst Mangel plagt uns manche Noht,", "tokens": ["Nebst", "Man\u00b7gel", "plagt", "uns", "man\u00b7che", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und endlich w\u00fcrget uns der Tod.", "tokens": ["Und", "end\u00b7lich", "w\u00fcr\u00b7get", "uns", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die\u00df sind die Plagen, so ich hier", "tokens": ["Die\u00df", "sind", "die", "Pla\u00b7gen", ",", "so", "ich", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meinem Wesen selbst versp\u00fchr.", "tokens": ["An", "mei\u00b7nem", "We\u00b7sen", "selbst", "ver\u00b7sp\u00fchr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein, wer z\u00e4hlt die Qual und Pein,", "tokens": ["Al\u00b7lein", ",", "wer", "z\u00e4hlt", "die", "Qual", "und", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wir noch unterw\u00fcrfig seyn,", "tokens": ["Der", "wir", "noch", "un\u00b7ter\u00b7w\u00fcr\u00b7fig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch uns andrer Bosheit kr\u00e4nkt,", "tokens": ["Wo\u00b7durch", "uns", "an\u00b7drer", "Bos\u00b7heit", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und uns mit bitterm Wermuht tr\u00e4nkt?", "tokens": ["Und", "uns", "mit", "bit\u00b7term", "Wer\u00b7muht", "tr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ha\u00df, Undank, Ungerechtigkeit,", "tokens": ["Ha\u00df", ",", "Un\u00b7dank", ",", "Un\u00b7ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verleumdung, Mi\u00dfgunst, Zank und Streit,", "tokens": ["Ver\u00b7leum\u00b7dung", ",", "Mi\u00df\u00b7gunst", ",", "Zank", "und", "Streit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfolgung, List und Heucheley,", "tokens": ["Ver\u00b7fol\u00b7gung", ",", "List", "und", "Heu\u00b7che\u00b7ley", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdacht, Betrug, Verr\u00e4hterey,", "tokens": ["Ver\u00b7dacht", ",", "Be\u00b7trug", ",", "Ver\u00b7r\u00e4h\u00b7te\u00b7rey", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie oft th\u00fcrmt noch ein Ungl\u00fccks-Meer,", "tokens": ["Wie", "oft", "th\u00fcrmt", "noch", "ein", "Un\u00b7gl\u00fccks\u00b7Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey allen Lastern, auf uns her.", "tokens": ["Bey", "al\u00b7len", "Las\u00b7tern", ",", "au