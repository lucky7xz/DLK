{"dta.poem.18052": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann gieng \u00fcber Feld an einem Tag", "tokens": ["Ein", "Mann", "gieng", "\u00fc\u00b7ber", "Feld", "an", "ei\u00b7nem", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da eine Menge Schnees lag, ", "tokens": ["Da", "ei\u00b7ne", "Men\u00b7ge", "Schnees", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er kam in einen Wald sehr tief,", "tokens": ["Er", "kam", "in", "ei\u00b7nen", "Wald", "sehr", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo er lang in der Jrre lief.", "tokens": ["Wo", "er", "lang", "in", "der", "Jr\u00b7re", "lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Er litt von Hunger grosse Noth,", "tokens": ["Er", "litt", "von", "Hun\u00b7ger", "gros\u00b7se", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Frost erwartet er den Tod.", "tokens": ["Von", "Frost", "er\u00b7war\u00b7tet", "er", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Gl\u00fcck war, als er sich so weit vergieng,", "tokens": ["Sein", "Gl\u00fcck", "war", ",", "als", "er", "sich", "so", "weit", "ver\u00b7gieng", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df ihn ein wilder Mann mit G\u00fctigkeit empfieng.", "tokens": ["Da\u00df", "ihn", "ein", "wil\u00b7der", "Mann", "mit", "G\u00fc\u00b7tig\u00b7keit", "emp\u00b7fi\u00b7eng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Er nahm ihn in sein Haus, und hielt ihn wohl;", "tokens": ["Er", "nahm", "ihn", "in", "sein", "Haus", ",", "und", "hielt", "ihn", "wohl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie jeder Wirth mit seinem Gaste soll.", "tokens": ["Wie", "je\u00b7der", "Wirth", "mit", "sei\u00b7nem", "Gas\u00b7te", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So bald er in die H\u00fctte kam,", "tokens": ["So", "bald", "er", "in", "die", "H\u00fct\u00b7te", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Begunt er wegen Frostes lahm", "tokens": ["Be\u00b7gunt", "er", "we\u00b7gen", "Fros\u00b7tes", "lahm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In die gefrohrne Hand zu hauchen.", "tokens": ["In", "die", "ge\u00b7frohr\u00b7ne", "Hand", "zu", "hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sein Wirth fragt ihn, warum er das gethan;", "tokens": ["Sein", "Wirth", "fragt", "ihn", ",", "wa\u00b7rum", "er", "das", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Darauf antwortet ihm der fremde Mann:", "tokens": ["Da\u00b7rauf", "ant\u00b7wor\u00b7tet", "ihm", "der", "frem\u00b7de", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ich hauche darum in die H\u00e4nde,", "tokens": ["Ich", "hau\u00b7che", "da\u00b7rum", "in", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Damit die W\u00e4rme sich dahin zur\u00fccke wende.", "tokens": ["Da\u00b7mit", "die", "W\u00e4r\u00b7me", "sich", "da\u00b7hin", "zu\u00b7r\u00fc\u00b7cke", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "PAV", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Waldmann sprach: Das ist sehr gut.", "tokens": ["Der", "Wald\u00b7mann", "sprach", ":", "Das", "ist", "sehr", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die W\u00e4rme ists, was dir wohl thut.", "tokens": ["Die", "W\u00e4r\u00b7me", "ists", ",", "was", "dir", "wohl", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Er macht ein Feur, und satzt ihn nieder;", "tokens": ["Er", "macht", "ein", "Feur", ",", "und", "satzt", "ihn", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Von grossem Frost half er ihm wieder.", "tokens": ["Von", "gros\u00b7sem", "Frost", "half", "er", "ihm", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Indem sie so beym Feuer sassen,", "tokens": ["In\u00b7dem", "sie", "so", "beym", "Feu\u00b7er", "sas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wollt es sein Wirth nicht bey dem halben lassen.", "tokens": ["Wollt", "es", "sein", "Wirth", "nicht", "bey", "dem", "hal\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Er sah wohl, da\u00df er hungrig war,", "tokens": ["Er", "sah", "wohl", ",", "da\u00df", "er", "hung\u00b7rig", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Er stellt ihm gute Speise dar;", "tokens": ["Er", "stellt", "ihm", "gu\u00b7te", "Spei\u00b7se", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und reicht ihm einen Trunk: Trinck nur, er ist gesund.", "tokens": ["Und", "reicht", "ihm", "ei\u00b7nen", "Trunk", ":", "Trinck", "nur", ",", "er", "ist", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$.", "VVIMP", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Fremde setzt den Becher an den Mund,", "tokens": ["Der", "Frem\u00b7de", "setzt", "den", "Be\u00b7cher", "an", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und trinckt; doch wird er bald gewahr,", "tokens": ["Und", "trinckt", ";", "doch", "wird", "er", "bald", "ge\u00b7wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da\u00df dieser Tranck gekochet war.", "tokens": ["Da\u00df", "die\u00b7ser", "Tranck", "ge\u00b7ko\u00b7chet", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Er brannt ihn auf die Zung: Alsbald blies er daran.", "tokens": ["Er", "brannt", "ihn", "auf", "die", "Zung", ":", "Als\u00b7bald", "blies", "er", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Da sprach zu ihm der wilde Mann:", "tokens": ["Da", "sprach", "zu", "ihm", "der", "wil\u00b7de", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Was soll das seyn, was hast du izt gethan?", "tokens": ["Was", "soll", "das", "seyn", ",", "was", "hast", "du", "izt", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "VAINF", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Thu mir es kund. Er sprach: Zu hei\u00df ist mir der ", "tokens": ["Thu", "mir", "es", "kund", ".", "Er", "sprach", ":", "Zu", "hei\u00df", "ist", "mir", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$.", "PTKA", "ADJD", "VAFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Ich blase dieserwegen drein,", "tokens": ["Ich", "bla\u00b7se", "die\u00b7ser\u00b7we\u00b7gen", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Damit er k\u00e4lter m\u00f6gte seyn.", "tokens": ["Da\u00b7mit", "er", "k\u00e4l\u00b7ter", "m\u00f6g\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Der Waldmann sprach: Wie? tr\u00e4gst du hei\u00df und kalt", "tokens": ["Der", "Wald\u00b7mann", "sprach", ":", "Wie", "?", "tr\u00e4gst", "du", "hei\u00df", "und", "kalt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "$.", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "In einem Mund? W\u00fcrd ich steinalt", "tokens": ["In", "ei\u00b7nem", "Mund", "?", "W\u00fcrd", "ich", "stei\u00b7nalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "So k\u00f6nnt ichs dir zu b\u00f6sem nicht vergessen.", "tokens": ["So", "k\u00f6nnt", "ichs", "dir", "zu", "b\u00f6\u00b7sem", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Auch hab ich mich ", "tokens": ["Auch", "hab", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.40": {"text": "Aus meinem Hause mu\u00df der Mann,", "tokens": ["Aus", "mei\u00b7nem", "Hau\u00b7se", "mu\u00df", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Der hei\u00df und kalt im Munde haben kan.", "tokens": ["Der", "hei\u00df", "und", "kalt", "im", "Mun\u00b7de", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "De\u00dfwegen raff dich auf, und geh hinaus,", "tokens": ["De\u00df\u00b7we\u00b7gen", "raff", "dich", "auf", ",", "und", "geh", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Du bleibest nicht in meinem Haus.", "tokens": ["Du", "blei\u00b7best", "nicht", "in", "mei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}