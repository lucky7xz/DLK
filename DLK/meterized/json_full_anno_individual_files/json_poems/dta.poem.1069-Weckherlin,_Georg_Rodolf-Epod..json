{"dta.poem.1069": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.71", "cy:0.14", "cs:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alda nu bald nach vnd nach", "tokens": ["Al\u00b7da", "nu", "bald", "nach", "vnd", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der subtil Griech gedichtet/", "tokens": ["Was", "der", "sub\u00b7til", "Griech", "ge\u00b7dich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NE", "NE", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der s\u00fc\u00df-schwer r\u00f6mer berichtet", "tokens": ["Der", "s\u00fc\u00df\u00b7schwer", "r\u00f6\u00b7mer", "be\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die reich Engl\u00e4ndische sprach/", "tokens": ["Die", "reich", "En\u00b7gl\u00e4n\u00b7di\u00b7sche", "sprach", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$("], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Was die Hochteutsche au\u00dflegen/", "tokens": ["Was", "die", "Hoch\u00b7teut\u00b7sche", "au\u00df\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$("], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Der Toscaner lieblichkeit/", "tokens": ["Der", "Tos\u00b7ca\u00b7ner", "lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Flandern geschwindigkeit/", "tokens": ["Der", "Flan\u00b7dern", "ge\u00b7schwin\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Was auch die Spanier w\u00e4gen/", "tokens": ["Was", "auch", "die", "Spa\u00b7nier", "w\u00e4\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vnd was vns je zugelassen", "tokens": ["Vnd", "was", "vns", "je", "zu\u00b7ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Deine Jugent wol erfassen.", "tokens": ["Dei\u00b7ne", "Ju\u00b7gent", "wol", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}