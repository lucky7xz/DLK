{"dta.poem.18842": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Mensch die Erde sch\u00e4tzet,", "tokens": ["Wenn", "der", "Mensch", "die", "Er\u00b7de", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Sonne sie bescheinet,", "tokens": ["Weil", "die", "Son\u00b7ne", "sie", "be\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Rebe sich ergetzet", "tokens": ["An", "der", "Re\u00b7be", "sich", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dem scharfen Messer weinet,", "tokens": ["Die", "dem", "schar\u00b7fen", "Mes\u00b7ser", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sie f\u00fchlt dass ihre S\u00e4fte,", "tokens": ["Da", "sie", "f\u00fchlt", "dass", "ih\u00b7re", "S\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohlgekocht, die Welt erquickend,", "tokens": ["Wohl\u00b7ge\u00b7kocht", ",", "die", "Welt", "er\u00b7qui\u00b7ckend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden regsam vielen Kr\u00e4ften,", "tokens": ["Wer\u00b7den", "reg\u00b7sam", "vie\u00b7len", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber mehreren erstickend;", "tokens": ["A\u00b7ber", "meh\u00b7re\u00b7ren", "er\u00b7sti\u00b7ckend", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weiss er das der Glut zu danken", "tokens": ["Weiss", "er", "das", "der", "Glut", "zu", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die das alles l\u00e4sst gedeihen;", "tokens": ["Die", "das", "al\u00b7les", "l\u00e4sst", "ge\u00b7dei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIS", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wird Betrunkner stammlend wanken,", "tokens": ["Wird", "Be\u00b7trunk\u00b7ner", "stamm\u00b7lend", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "M\u00e4ssiger wird sich singend freuen.", "tokens": ["M\u00e4s\u00b7si\u00b7ger", "wird", "sich", "sin\u00b7gend", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}