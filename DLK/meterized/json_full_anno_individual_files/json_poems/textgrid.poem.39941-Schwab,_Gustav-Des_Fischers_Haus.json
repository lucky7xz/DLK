{"textgrid.poem.39941": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Des Fischers Haus", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Haus hat der Fischer gebaut,", "tokens": ["Sein", "Haus", "hat", "der", "Fi\u00b7scher", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Es stehet dicht an den Wellen,", "tokens": ["Es", "ste\u00b7het", "dicht", "an", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der blauen Flut sich's beschaut,", "tokens": ["In", "der", "blau\u00b7en", "Flut", "sich's", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Als spr\u00e4ch' es: wer kann mich f\u00e4llen?", "tokens": ["Als", "spr\u00e4ch'", "es", ":", "wer", "kann", "mich", "f\u00e4l\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Mauern, die sind so dicht,", "tokens": ["Die", "Mau\u00b7ern", ",", "die", "sind", "so", "dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Voll Korn und Wein sind die R\u00e4ume,", "tokens": ["Voll", "Korn", "und", "Wein", "sind", "die", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es zittert das Sonnenlicht", "tokens": ["Es", "zit\u00b7tert", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herunter durch Bl\u00fctenb\u00e4ume.", "tokens": ["Her\u00b7un\u00b7ter", "durch", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Reben winken herein", "tokens": ["Und", "Re\u00b7ben", "win\u00b7ken", "her\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Von gr\u00fcnen, schirmenden H\u00fcgeln,", "tokens": ["Von", "gr\u00fc\u00b7nen", ",", "schir\u00b7men\u00b7den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die lassen den Nord nicht ein,", "tokens": ["Die", "las\u00b7sen", "den", "Nord", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die umhaucht nur der West mit den Fl\u00fcgeln.", "tokens": ["Die", "um\u00b7haucht", "nur", "der", "West", "mit", "den", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und am Ufer der Fischer steht,", "tokens": ["Und", "am", "U\u00b7fer", "der", "Fi\u00b7scher", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es spielt sein Netz in den Wellen,", "tokens": ["Es", "spielt", "sein", "Netz", "in", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Umsonst ihr euch wendet und dreht,", "tokens": ["Um\u00b7sonst", "ihr", "euch", "wen\u00b7det", "und", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ihr Karpfen, ihr zarten Forellen!", "tokens": ["Ihr", "Karp\u00b7fen", ",", "ihr", "zar\u00b7ten", "Fo\u00b7rel\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Sein frevelnder Arm euch zieht", "tokens": ["Sein", "fre\u00b7veln\u00b7der", "Arm", "euch", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im engen Garn an's Gestade;", "tokens": ["Im", "en\u00b7gen", "Garn", "an's", "Ge\u00b7sta\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kein armes Fischlein entflieht,", "tokens": ["Kein", "ar\u00b7mes", "Fisc\u00b7hlein", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das kleinste nicht findet Gnade.", "tokens": ["Das", "kleins\u00b7te", "nicht", "fin\u00b7det", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "VVFIN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Auf steiget kein Wasserweib,", "tokens": ["Auf", "stei\u00b7get", "kein", "Was\u00b7ser\u00b7weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Euch zu retten, ihr stillen, ihr guten!", "tokens": ["Euch", "zu", "ret\u00b7ten", ",", "ihr", "stil\u00b7len", ",", "ihr", "gu\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "VVINF", "$,", "PPOSAT", "ADJA", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und lockt mit dem seligen Leib", "tokens": ["Und", "lockt", "mit", "dem", "se\u00b7li\u00b7gen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ihn hinab in die schwellenden Fluten.", "tokens": ["Ihn", "hin\u00b7ab", "in", "die", "schwel\u00b7len\u00b7den", "Flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "\u00bbich bin der Herrscher im See,", "tokens": ["\u00bb", "ich", "bin", "der", "Herr\u00b7scher", "im", "See", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ein K\u00f6nig im Reiche der Wogen!\u00ab", "tokens": ["Ein", "K\u00f6\u00b7nig", "im", "Rei\u00b7che", "der", "Wo\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So spricht er und schnellt in die H\u00f6h'", "tokens": ["So", "spricht", "er", "und", "schnellt", "in", "die", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Den schweren Angel im Bogen.", "tokens": ["Den", "schwe\u00b7ren", "An\u00b7gel", "im", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und euer Leben ist aus,", "tokens": ["Und", "eu\u00b7er", "Le\u00b7ben", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der Fischer, mit frohem Behagen,", "tokens": ["Der", "Fi\u00b7scher", ",", "mit", "fro\u00b7hem", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er tritt in das stattliche Haus,", "tokens": ["Er", "tritt", "in", "das", "statt\u00b7li\u00b7che", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "An den harten Stein euch zu schlagen.", "tokens": ["An", "den", "har\u00b7ten", "Stein", "euch", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Er legt sich auf weichen Pf\u00fchl,", "tokens": ["Er", "legt", "sich", "auf", "wei\u00b7chen", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Gold und Beute zu tr\u00e4umen; \u2013", "tokens": ["Von", "Gold", "und", "Beu\u00b7te", "zu", "tr\u00e4u\u00b7men", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Nacht, so sicher und k\u00fchl,", "tokens": ["O", "Nacht", ",", "so", "si\u00b7cher", "und", "k\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wo Hamen und Angel s\u00e4umen!", "tokens": ["Wo", "Ha\u00b7men", "und", "An\u00b7gel", "s\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Da regt sich das Leben im Grund,", "tokens": ["Da", "regt", "sich", "das", "Le\u00b7ben", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Da wimmelt's von Karpf' und Forelle,", "tokens": ["Da", "wim\u00b7melt's", "von", "Ka\u00b7rp\u00b7f'", "und", "Fo\u00b7rel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da nagt's mit gesch\u00e4ftigem Mund", "tokens": ["Da", "nagt's", "mit", "ge\u00b7sch\u00e4f\u00b7ti\u00b7gem", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und schl\u00fcpft unter's Ufer im Quelle.", "tokens": ["Und", "schl\u00fcpft", "un\u00b7ter's", "U\u00b7fer", "im", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Und fr\u00fche beim Morgenrot", "tokens": ["Und", "fr\u00fc\u00b7he", "beim", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Fischer kommt mit den Flechten;", "tokens": ["Der", "Fi\u00b7scher", "kommt", "mit", "den", "Flech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Am Tage drohet der Tod,", "tokens": ["Am", "Ta\u00b7ge", "dro\u00b7het", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die Rache schafft in den N\u00e4chten.", "tokens": ["Die", "Ra\u00b7che", "schafft", "in", "den", "N\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Von Jahr zu Jahr sie nicht ruht,", "tokens": ["Von", "Jahr", "zu", "Jahr", "sie", "nicht", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Alten zeigen's den Jungen;", "tokens": ["Die", "Al\u00b7ten", "zei\u00b7gen's", "den", "Jun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis da\u00df die schweigende Flut", "tokens": ["Bis", "da\u00df", "die", "schwei\u00b7gen\u00b7de", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ist unter das Haus gedrungen;", "tokens": ["Ist", "un\u00b7ter", "das", "Haus", "ge\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Bis da\u00df in sinkender Nacht,", "tokens": ["Bis", "da\u00df", "in", "sin\u00b7ken\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wo der Fischer tr\u00e4umt auf dem Pf\u00fchle,", "tokens": ["Wo", "der", "Fi\u00b7scher", "tr\u00e4umt", "auf", "dem", "Pf\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Haus, das gewaltige, kracht,", "tokens": ["Das", "Haus", ",", "das", "ge\u00b7wal\u00b7ti\u00b7ge", ",", "kracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Versinkt in der Wogen Gew\u00fchle.", "tokens": ["Ver\u00b7sinkt", "in", "der", "Wo\u00b7gen", "Ge\u00b7w\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.14": {"line.1": {"text": "Ausgie\u00dfet sich Korn und Wein,", "tokens": ["Aus\u00b7gie\u00b7\u00dfet", "sich", "Korn", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es \u00f6ffnet der See den Rachen,", "tokens": ["Es", "\u00f6ff\u00b7net", "der", "See", "den", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es schlingt den M\u00f6rder hinein,", "tokens": ["Es", "schlingt", "den", "M\u00f6r\u00b7der", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Er hat nicht Zeit zum Erwachen.", "tokens": ["Er", "hat", "nicht", "Zeit", "zum", "Er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Die G\u00e4rten, die B\u00e4ume zugleich,", "tokens": ["Die", "G\u00e4r\u00b7ten", ",", "die", "B\u00e4u\u00b7me", "zu\u00b7gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sie schwinden, sie setzen sich nieder,", "tokens": ["Sie", "schwin\u00b7den", ",", "sie", "set\u00b7zen", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es spielen im freien Reich", "tokens": ["Es", "spie\u00b7len", "im", "frei\u00b7en", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Fische, die fr\u00f6hlichen, wieder.", "tokens": ["Die", "Fi\u00b7sche", ",", "die", "fr\u00f6h\u00b7li\u00b7chen", ",", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.16": {"line.1": {"text": "Sein Haus hat der Fischer gebaut,", "tokens": ["Sein", "Haus", "hat", "der", "Fi\u00b7scher", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Es stehet dicht an den Wellen,", "tokens": ["Es", "ste\u00b7het", "dicht", "an", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der blauen Flut sich's beschaut,", "tokens": ["In", "der", "blau\u00b7en", "Flut", "sich's", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Als spr\u00e4ch' es: wer kann mich f\u00e4llen?", "tokens": ["Als", "spr\u00e4ch'", "es", ":", "wer", "kann", "mich", "f\u00e4l\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.17": {"line.1": {"text": "Die Mauern, die sind so dicht,", "tokens": ["Die", "Mau\u00b7ern", ",", "die", "sind", "so", "dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Voll Korn und Wein sind die R\u00e4ume,", "tokens": ["Voll", "Korn", "und", "Wein", "sind", "die", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es zittert das Sonnenlicht", "tokens": ["Es", "zit\u00b7tert", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herunter durch Bl\u00fctenb\u00e4ume.", "tokens": ["Her\u00b7un\u00b7ter", "durch", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Und Reben winken herein", "tokens": ["Und", "Re\u00b7ben", "win\u00b7ken", "her\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Von gr\u00fcnen, schirmenden H\u00fcgeln,", "tokens": ["Von", "gr\u00fc\u00b7nen", ",", "schir\u00b7men\u00b7den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die lassen den Nord nicht ein,", "tokens": ["Die", "las\u00b7sen", "den", "Nord", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die umhaucht nur der West mit den Fl\u00fcgeln.", "tokens": ["Die", "um\u00b7haucht", "nur", "der", "West", "mit", "den", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.19": {"line.1": {"text": "Und am Ufer der Fischer steht,", "tokens": ["Und", "am", "U\u00b7fer", "der", "Fi\u00b7scher", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+