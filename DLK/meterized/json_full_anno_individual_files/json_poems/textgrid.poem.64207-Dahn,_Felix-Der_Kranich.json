{"textgrid.poem.64207": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranich", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, wo die letzten, lichten jungen Erlen", "tokens": ["Hier", ",", "wo", "die", "letz\u00b7ten", ",", "lich\u00b7ten", "jun\u00b7gen", "Er\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "$,", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Vorwacht stehn des Walds von Kloster Zell,", "tokens": ["Auf", "Vor\u00b7wacht", "stehn", "des", "Walds", "von", "Klos\u00b7ter", "Zell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am braunen Moosquell, drin die raschen Schmerlen", "tokens": ["Am", "brau\u00b7nen", "Moos\u00b7quell", ",", "drin", "die", "ra\u00b7schen", "Schmer\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie dunkle Schatten fliehn und h\u00fcpfen schnell,", "tokens": ["Wie", "dunk\u00b7le", "Schat\u00b7ten", "fliehn", "und", "h\u00fcp\u00b7fen", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo tief im breiten Tal mit Silberperlen", "tokens": ["Wo", "tief", "im", "brei\u00b7ten", "Tal", "mit", "Sil\u00b7ber\u00b7per\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der gelbe Main manchmal emporblitzt hell", "tokens": ["Der", "gel\u00b7be", "Main", "manch\u00b7mal", "em\u00b7por\u00b7blitzt", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In stolz geschwungnem, leisem, sanftem Gleiten, \u2013", "tokens": ["In", "stolz", "ge\u00b7schwung\u00b7nem", ",", "lei\u00b7sem", ",", "sanf\u00b7tem", "Glei\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hier ruh' ich oft, gedenkend andrer Zeiten.", "tokens": ["Hier", "ruh'", "ich", "oft", ",", "ge\u00b7den\u00b7kend", "an\u00b7drer", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Frost hat schon der Buchen Laub und Eichen", "tokens": ["Der", "Frost", "hat", "schon", "der", "Bu\u00b7chen", "Laub", "und", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Goldrot gef\u00e4rbt: es lasten voll gereift", "tokens": ["Gol\u00b7drot", "ge\u00b7f\u00e4rbt", ":", "es", "las\u00b7ten", "voll", "ge\u00b7reift"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "PPER", "VVFIN", "ADJD", "VVPP"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Die Trauben dort am \u00bb", "tokens": ["Die", "Trau\u00b7ben", "dort", "am", "\u00bb"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Wildschwan singend durch die N\u00e4chte streift,", "tokens": ["Der", "Wild\u00b7schwan", "sin\u00b7gend", "durch", "die", "N\u00e4ch\u00b7te", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch hier im Abendd\u00e4mmer seh' ich streichen", "tokens": ["Doch", "hier", "im", "A\u00b7bend\u00b7d\u00e4m\u00b7mer", "seh'", "ich", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Kranich, der die Wanderstrophe pfeift:", "tokens": ["Den", "Kra\u00b7nich", ",", "der", "die", "Wan\u00b7der\u00b7stro\u00b7phe", "pfeift", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er zieht gen S\u00fcden \u00fcber Meer und Eiland:", "tokens": ["Er", "zieht", "gen", "S\u00fc\u00b7den", "\u00fc\u00b7ber", "Meer", "und", "Ei\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jerusalem \u2013 dich sucht er und den Heiland. \u2013", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lem", "\u2013", "dich", "sucht", "er", "und", "den", "Hei\u00b7land", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "PPER", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da steigt ein Bild mir auf blickferner L\u00e4nder:", "tokens": ["Da", "steigt", "ein", "Bild", "mir", "auf", "blick\u00b7fer\u00b7ner", "L\u00e4n\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch dort ein Strom, der z\u00f6gernd gleitend rinnt", "tokens": ["Auch", "dort", "ein", "Strom", ",", "der", "z\u00f6\u00b7gernd", "glei\u00b7tend", "rinnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Fu\u00dfe gelb gebrannter H\u00fcgelr\u00e4nder.", "tokens": ["Am", "Fu\u00b7\u00dfe", "gelb", "ge\u00b7brann\u00b7ter", "H\u00fc\u00b7gel\u00b7r\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drei Palmen nicken dort im Abendwind:", "tokens": ["Drei", "Pal\u00b7men", "ni\u00b7cken", "dort", "im", "Ab\u00b7end\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Horch, Rossewiehern \u2013 flatternde Gew\u00e4nder \u2013", "tokens": ["Horch", ",", "Ros\u00b7se\u00b7wie\u00b7hern", "\u2013", "flat\u00b7tern\u00b7de", "Ge\u00b7w\u00e4n\u00b7der", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Allahruf: \u2013 der W\u00fcste rasch Gesind'", "tokens": ["Und", "Al\u00b7la\u00b7hruf", ":", "\u2013", "der", "W\u00fcs\u00b7te", "rasch", "Ge\u00b7sind'"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "$(", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umtobt uns rings \u2013 es schwirrt von Pfeil und Speeren \u2013", "tokens": ["Um\u00b7tobt", "uns", "rings", "\u2013", "es", "schwirrt", "von", "Pfeil", "und", "Spee\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da st\u00fcrzt mein Hengst \u2013 jetzt gilt's, dem Tode wehren \u2013!", "tokens": ["Da", "st\u00fcrzt", "mein", "Hengst", "\u2013", "jetzt", "gilt's", ",", "dem", "To\u00b7de", "weh\u00b7ren", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "$,", "ART", "NN", "VVINF", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schon birst mein Helm vor'm Damaszener Schwerte,", "tokens": ["Schon", "birst", "mein", "Helm", "vor'm", "Da\u00b7mas\u00b7ze\u00b7ner", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den langen Kreuzschild spaltet mir ein Beil \u2013", "tokens": ["Den", "lan\u00b7gen", "Kreuz\u00b7schild", "spal\u00b7tet", "mir", "ein", "Beil", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da springt Er bei, mein edler Sturmgef\u00e4hrte,", "tokens": ["Da", "springt", "Er", "bei", ",", "mein", "ed\u00b7ler", "Sturm\u00b7ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er selbst, sein Leib mein Schild: \u2013 da zischt ein Pfeil", "tokens": ["Er", "selbst", ",", "sein", "Leib", "mein", "Schild", ":", "\u2013", "da", "zischt", "ein", "Pfeil"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ins Herz ihm, in das todestreu bew\u00e4hrte!", "tokens": ["Ins", "Herz", "ihm", ",", "in", "das", "to\u00b7des\u00b7treu", "be\u00b7w\u00e4hr\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "APPR", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O Kranich, hemme dort des Fluges Eil',", "tokens": ["O", "Kra\u00b7nich", ",", "hem\u00b7me", "dort", "des", "Flu\u00b7ges", "Eil'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo um den W\u00fcstenbronn drei Palmen