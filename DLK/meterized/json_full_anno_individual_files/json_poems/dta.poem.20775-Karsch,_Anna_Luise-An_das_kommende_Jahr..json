{"dta.poem.20775": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An das kommende Jahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Gott der geistigen Monaden               ", "tokens": ["Als", "Gott", "der", "geis\u00b7ti\u00b7gen", "Mo\u00b7na\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vorausgez\u00e4hlte Myriaden", "tokens": ["Vor\u00b7aus\u00b7ge\u00b7z\u00e4hl\u00b7te", "My\u00b7ria\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Reich der M\u00f6glichkeit allsehend \u00fcberblickt;", "tokens": ["Im", "Reich", "der", "M\u00f6g\u00b7lich\u00b7keit", "all\u00b7se\u00b7hend", "\u00fc\u00b7berb\u00b7lickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da sahe dich, du Punkt der Zeiten,", "tokens": ["Da", "sa\u00b7he", "dich", ",", "du", "Punkt", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Auge, dem die Dunkelheiten", "tokens": ["Sein", "Au\u00b7ge", ",", "dem", "die", "Dun\u00b7kel\u00b7hei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit tausend Sonnen sind geschm\u00fcckt.", "tokens": ["Mit", "tau\u00b7send", "Son\u00b7nen", "sind", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O k\u00e4mst du doch mit vollen Schwingen,", "tokens": ["O", "k\u00e4mst", "du", "doch", "mit", "vol\u00b7len", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und br\u00e4chtest goldne Zeit, von der die Dichter singen,", "tokens": ["Und", "br\u00e4ch\u00b7test", "gold\u00b7ne", "Zeit", ",", "von", "der", "die", "Dich\u00b7ter", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und f\u00e4ndest, au\u00dfer Greis und Kind,", "tokens": ["Und", "f\u00e4n\u00b7dest", ",", "au\u00b7\u00dfer", "Greis", "und", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Noch Menschen, die der goldnen Jahre w\u00fcrdig sind!", "tokens": ["Noch", "Men\u00b7schen", ",", "die", "der", "gold\u00b7nen", "Jah\u00b7re", "w\u00fcr\u00b7dig", "sind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}