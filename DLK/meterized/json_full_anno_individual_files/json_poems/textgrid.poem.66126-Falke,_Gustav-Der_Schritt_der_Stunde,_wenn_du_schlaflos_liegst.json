{"textgrid.poem.66126": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Schritt der Stunde, wenn du schlaflos liegst", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schritt der Stunde, wenn du schlaflos liegst,", "tokens": ["Der", "Schritt", "der", "Stun\u00b7de", ",", "wenn", "du", "schlaf\u00b7los", "liegst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und die Gedanken sich wie Schwalben jagen,", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", "sich", "wie", "Schwal\u00b7ben", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sehnend du bis an die Sterne fliegst", "tokens": ["Wenn", "seh\u00b7nend", "du", "bis", "an", "die", "Ster\u00b7ne", "fliegst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und leer zur\u00fcckkehrst, fl\u00fcgellahm, zerschlagen.", "tokens": ["Und", "leer", "zu\u00b7r\u00fcck\u00b7kehrst", ",", "fl\u00fc\u00b7gel\u00b7lahm", ",", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Schritt der Stunde, wenn du schlaflos liegst,", "tokens": ["Der", "Schritt", "der", "Stun\u00b7de", ",", "wenn