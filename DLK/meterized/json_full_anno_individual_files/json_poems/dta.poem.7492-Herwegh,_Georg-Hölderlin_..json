{"dta.poem.7492": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6lderlin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Klugen leiten sicher stets die Horen,", "tokens": ["Den", "Klu\u00b7gen", "lei\u00b7ten", "si\u00b7cher", "stets", "die", "Ho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur mit dem Genius spielen oft die Winde;", "tokens": ["Nur", "mit", "dem", "Ge\u00b7nius", "spie\u00b7len", "oft", "die", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Da\u00df er, so Gl\u00fcck, wie Ungl\u00fcck, fr\u00fcher finde,", "tokens": ["Da\u00df", "er", ",", "so", "Gl\u00fcck", ",", "wie", "Un\u00b7gl\u00fcck", ",", "fr\u00fc\u00b7her", "fin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "NN", "$,", "PWAV", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird er mit Schwingen in die Welt geboren.", "tokens": ["Wird", "er", "mit", "Schwin\u00b7gen", "in", "die", "Welt", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch bleibt ihm treu die Gottheit zugeschworen;", "tokens": ["Doch", "bleibt", "ihm", "treu", "die", "Got\u00b7theit", "zu\u00b7ge\u00b7schwo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie legt am b\u00f6sen Tag dem armen Kinde", "tokens": ["Sie", "legt", "am", "b\u00f6\u00b7sen", "Tag", "dem", "ar\u00b7men", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit weicher Hand ums Aug' des Wahnsinns Binde,", "tokens": ["Mit", "wei\u00b7cher", "Hand", "ums", "Aug'", "des", "Wahn\u00b7sinns", "Bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es nie sehe, was das Herz verloren.", "tokens": ["Da\u00df", "es", "nie", "se\u00b7he", ",", "was", "das", "Herz", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Die G\u00f6tter haben freundlich Dein gedacht,", "tokens": ["Die", "G\u00f6t\u00b7ter", "ha\u00b7ben", "freund\u00b7lich", "Dein", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Du so fromm gehalten einst in Ehren,", "tokens": ["Die", "Du", "so", "fromm", "ge\u00b7hal\u00b7ten", "einst", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Nichts Irdisches kann f\u00fcrder Dich versehren,", "tokens": ["Nichts", "Ir\u00b7di\u00b7sches", "kann", "f\u00fcr\u00b7der", "Dich", "ver\u00b7seh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und reiner, denn ein Stern zum Schoo\u00df der Nacht,", "tokens": ["Und", "rei\u00b7ner", ",", "denn", "ein", "Stern", "zum", "Schoo\u00df", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "KON", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wirst Du zur\u00fcck zur gro\u00dfen Mutter kehren.", "tokens": ["Wirst", "Du", "zu\u00b7r\u00fcck", "zur", "gro\u00b7\u00dfen", "Mut\u00b7ter", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}