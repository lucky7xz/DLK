{"textgrid.poem.56174": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Iv", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwindende, du kennst die T\u00fcrme nicht.", "tokens": ["Schwin\u00b7den\u00b7de", ",", "du", "kennst", "die", "T\u00fcr\u00b7me", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Doch nun sollst du einen Turm gewahren", "tokens": ["Doch", "nun", "sollst", "du", "ei\u00b7nen", "Turm", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "mit dem wunderbaren", "tokens": ["mit", "dem", "wun\u00b7der\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Raum in dir. Verschlie\u00df dein Angesicht.", "tokens": ["Raum", "in", "dir", ".", "Ver\u00b7schlie\u00df", "dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aufgerichtet hast du ihn", "tokens": ["Auf\u00b7ge\u00b7rich\u00b7tet", "hast", "du", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ahnungslos mit Blick und Wink und Wendung.", "tokens": ["ah\u00b7nungs\u00b7los", "mit", "Blick", "und", "Wink", "und", "Wen\u00b7dung", "."], "toke