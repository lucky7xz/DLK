sure": "amphibrach.tetra"}, "line.6": {"text": "\u00bbwas soll diese blinde, angelaufene Scheibe?\u00ab", "tokens": ["\u00bb", "was", "soll", "die\u00b7se", "blin\u00b7de", ",", "an\u00b7ge\u00b7lau\u00b7fe\u00b7ne", "Schei\u00b7be", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PDAT", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.22": {"line.1": {"text": "Ein einziges, gro\u00dfes Elend war mir dieser Winter.", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", ",", "gro\u00b7\u00dfes", "E\u00b7lend", "war", "mir", "die\u00b7ser", "Win\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.23": {"line.1": {"text": "Da, mitten in der Nacht,", "tokens": ["Da", ",", "mit\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gestern,", "tokens": ["Ge\u00b7stern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wachte meine Seele auf, und ich f\u00fchlte es hell:", "tokens": ["Wach\u00b7te", "mei\u00b7ne", "See\u00b7le", "auf", ",", "und", "ich", "f\u00fchl\u00b7te", "es", "hell", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sie hob Fl\u00fcgel wieder, meine Seele,", "tokens": ["Sie", "hob", "Fl\u00fc\u00b7gel", "wie\u00b7der", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und sie ist wieder Schmetterling.", "tokens": ["Und", "sie", "ist", "wie\u00b7der", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Und ich wei\u00df: Zwei blaue, leuchtende Blumen", "tokens": ["Und", "ich", "wei\u00df", ":", "Zwei", "blau\u00b7e", ",", "leuch\u00b7ten\u00b7de", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "CARD", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sucht sie, und nie noch kostete sie solche", "tokens": ["Sucht", "sie", ",", "und", "nie", "noch", "kos\u00b7te\u00b7te", "sie", "sol\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADV", "ADV", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fc\u00dfigkeit, wie in diesen beiden", "tokens": ["S\u00fc\u00b7\u00dfig\u00b7keit", ",", "wie", "in", "die\u00b7sen", "bei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PDAT", "PIAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Blauen Blumen ist.", "tokens": ["Blau\u00b7en", "Blu\u00b7men", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}