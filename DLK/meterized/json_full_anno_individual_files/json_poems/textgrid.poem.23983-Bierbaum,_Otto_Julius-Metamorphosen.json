{"textgrid.poem.23983": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Metamorphosen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Winterkrank war meine Seele,", "tokens": ["Win\u00b7ter\u00b7krank", "war", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie kroch wie eine faule Kr\u00f6te", "tokens": ["Und", "sie", "kroch", "wie", "ei\u00b7ne", "fau\u00b7le", "Kr\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zwischen kalten Steinen.", "tokens": ["Zwi\u00b7schen", "kal\u00b7ten", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "An den leeren Stunden klebte sie", "tokens": ["An", "den", "lee\u00b7ren", "Stun\u00b7den", "kleb\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie eine m\u00fcde Fliege am angelaufenen,", "tokens": ["Wie", "ei\u00b7ne", "m\u00fc\u00b7de", "Flie\u00b7ge", "am", "an\u00b7ge\u00b7lau\u00b7fe\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Undurchsichtigen Fensterglas.", "tokens": ["Un\u00b7durch\u00b7sich\u00b7ti\u00b7gen", "Fens\u00b7ter\u00b7glas", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Sonst war meine Seele ein Schmetterling,", "tokens": ["Sonst", "war", "mei\u00b7ne", "See\u00b7le", "ein", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein leichter, feiner, bl\u00fctenverliebter Schmetterling,", "tokens": ["Ein", "leich\u00b7ter", ",", "fei\u00b7ner", ",", "bl\u00fc\u00b7ten\u00b7ver\u00b7lieb\u00b7ter", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der sich im Sonnenscheine von weichen Winden", "tokens": ["Der", "sich", "im", "Son\u00b7nen\u00b7schei\u00b7ne", "von", "wei\u00b7chen", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gerne tragen lie\u00df, wie ein Blumenblatt;", "tokens": ["Ger\u00b7ne", "tra\u00b7gen", "lie\u00df", ",", "wie", "ein", "Blu\u00b7men\u00b7blatt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und er steckte sein Saugr\u00fcsselchen gerne in alle S\u00fc\u00dfigkeit,", "tokens": ["Und", "er", "steck\u00b7te", "sein", "Saug\u00b7r\u00fcs\u00b7sel\u00b7chen", "ger\u00b7ne", "in", "al\u00b7le", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "--+--+-+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und er berauschte sich gerne am Tausendblumengeist,", "tokens": ["Und", "er", "be\u00b7rauschte", "sich", "ger\u00b7ne", "am", "Tau\u00b7send\u00b7blu\u00b7men\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und im offenen, samenstaubduftigen Scho\u00dfe \u00fcppiger,", "tokens": ["Und", "im", "of\u00b7fe\u00b7nen", ",", "sa\u00b7men\u00b7staub\u00b7duf\u00b7ti\u00b7gen", "Scho\u00b7\u00dfe", "\u00fcp\u00b7pi\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+--+--+-+--", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Buttergelber Rosen schlief er gerne,", "tokens": ["But\u00b7ter\u00b7gel\u00b7ber", "Ro\u00b7sen", "schlief", "er", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Der sorgenlose, leichtsinnige,", "tokens": ["Der", "sor\u00b7gen\u00b7lo\u00b7se", ",", "leicht\u00b7sin\u00b7ni\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Frei schwebende Schmetterling meiner Seele.", "tokens": ["Frei", "schwe\u00b7ben\u00b7de", "Schmet\u00b7ter\u00b7ling", "mei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "++--+-++-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wei\u00dft du noch, meine Seele, wie du zum letztenmale", "tokens": ["Wei\u00dft", "du", "noch", ",", "mei\u00b7ne", "See\u00b7le", ",", "wie", "du", "zum", "letz\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPRART", "ADJA"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schmetterling warst?", "tokens": ["Schmet\u00b7ter\u00b7ling", "warst", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Das war ein heller, herber Tag,", "tokens": ["Das", "war", "ein", "hel\u00b7ler", ",", "her\u00b7ber", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hell wie ein braunes M\u00e4dchenauge,", "tokens": ["Hell", "wie", "ein", "brau\u00b7nes", "M\u00e4d\u00b7chen\u00b7au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dem der Spott lacht:", "tokens": ["In", "dem", "der", "Spott", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u00bbliebe, \u2013 was ist denn das?\u00ab", "tokens": ["\u00bb", "lie\u00b7be", ",", "\u2013", "was", "ist", "denn", "das", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "$(", "PWS", "VAFIN", "ADV", "PDS", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Solch ein Tag wars: Herbstbeginn.", "tokens": ["Solch", "ein", "Tag", "wars", ":", "Herbst\u00b7be\u00b7ginn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da flogst du, meine leichtgl\u00e4ubige Seele,", "tokens": ["Da", "flogst", "du", ",", "mei\u00b7ne", "leicht\u00b7gl\u00e4u\u00b7bi\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch die kalte Helligkeit und suchtest Bl\u00fcten;", "tokens": ["Durch", "die", "kal\u00b7te", "Hel\u00b7lig\u00b7keit", "und", "such\u00b7test", "Bl\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Aber fallende Raschelbl\u00e4tter,", "tokens": ["A\u00b7ber", "fal\u00b7len\u00b7de", "Ra\u00b7schel\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Niederzitternd in zagender Schw\u00e4che,", "tokens": ["Nie\u00b7der\u00b7zit\u00b7ternd", "in", "za\u00b7gen\u00b7der", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "St\u00f6rten deinen Flug, und du wurdest verzagt", "tokens": ["St\u00f6r\u00b7ten", "dei\u00b7nen", "Flug", ",", "und", "du", "wur\u00b7dest", "ver\u00b7zagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und frorst in dieser leeren Helle.", "tokens": ["Und", "frorst", "in", "die\u00b7ser", "lee\u00b7ren", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da wurdest du ein kriechendes Thier, meine Seele,", "tokens": ["Da", "wur\u00b7dest", "du", "ein", "krie\u00b7chen\u00b7des", "Thier", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und du hast dich verkrochen vor dem lieblosen Winter", "tokens": ["Und", "du", "hast", "dich", "ver\u00b7kro\u00b7chen", "vor", "dem", "lieb\u00b7lo\u00b7sen", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und dumpf geschlafen.", "tokens": ["Und", "dumpf", "ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Ohne Seele,", "tokens": ["Oh\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ohne Liebe,", "tokens": ["Oh\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ohne Rausch und Taumel ging ich", "tokens": ["Oh\u00b7ne", "Rausch", "und", "Tau\u00b7mel", "ging", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch diesen Winter, ein verdrossener Kr\u00fcppel,", "tokens": ["Durch", "die\u00b7sen", "Win\u00b7ter", ",", "ein", "ver\u00b7dros\u00b7se\u00b7ner", "Kr\u00fcp\u00b7pel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und sah ich die Sonne, so fragte mein Auge:", "tokens": ["Und", "sah", "ich", "die", "Son\u00b7ne", ",", "so", "frag\u00b7te", "mein", "Au\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "\u00bbwas soll diese blinde, angelaufene Scheibe?\u00ab", "tokens": ["\u00bb", "was", "soll", "die\u00b7se", "blin\u00b7de", ",", "an\u00b7ge\u00b7lau\u00b7fe\u00b7ne", "Schei\u00b7be", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PDAT", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.10": {"line.1": {"text": "Ein einziges, gro\u00dfes Elend war mir dieser Winter.", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", ",", "gro\u00b7\u00dfes", "E\u00b7lend", "war", "mir", "die\u00b7ser", "Win\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.11": {"line.1": {"text": "Da, mitten in der Nacht,", "tokens": ["Da", ",", "mit\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gestern,", "tokens": ["Ge\u00b7stern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wachte meine Seele auf, und ich f\u00fchlte es hell:", "tokens": ["Wach\u00b7te", "mei\u00b7ne", "See\u00b7le", "auf", ",", "und", "ich", "f\u00fchl\u00b7te", "es", "hell", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sie hob Fl\u00fcgel wieder, meine Seele,", "tokens": ["Sie", "hob", "Fl\u00fc\u00b7gel", "wie\u00b7der", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Und sie ist wieder Schmetterling.", "tokens": ["Und", "sie", "ist", "wie\u00b7der", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Und ich wei\u00df: Zwei blaue, leuchtende Blumen", "tokens": ["Und", "ich", "wei\u00df", ":", "Zwei", "blau\u00b7e", ",", "leuch\u00b7ten\u00b7de", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "CARD", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sucht sie, und nie noch kostete sie solche", "tokens": ["Sucht", "sie", ",", "und", "nie", "noch", "kos\u00b7te\u00b7te", "sie", "sol\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADV", "ADV", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fc\u00dfigkeit, wie in diesen beiden", "tokens": ["S\u00fc\u00b7\u00dfig\u00b7keit", ",", "wie", "in", "die\u00b7sen", "bei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PDAT", "PIAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Blauen Blumen ist.", "tokens": ["Blau\u00b7en", "Blu\u00b7men", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Winterkrank war meine Seele,", "tokens": ["Win\u00b7ter\u00b7krank", "war", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie kroch wie eine faule Kr\u00f6te", "tokens": ["Und", "sie", "kroch", "wie", "ei\u00b7ne", "fau\u00b7le", "Kr\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zwischen kalten Steinen.", "tokens": ["Zwi\u00b7schen", "kal\u00b7ten", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "An den leeren Stunden klebte sie", "tokens": ["An", "den", "lee\u00b7ren", "Stun\u00b7den", "kleb\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie eine m\u00fcde Fliege am angelaufenen,", "tokens": ["Wie", "ei\u00b7ne", "m\u00fc\u00b7de", "Flie\u00b7ge", "am", "an\u00b7ge\u00b7lau\u00b7fe\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Undurchsichtigen Fensterglas.", "tokens": ["Un\u00b7durch\u00b7sich\u00b7ti\u00b7gen", "Fens\u00b7ter\u00b7glas", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.15": {"line.1": {"text": "Sonst war meine Seele ein Schmetterling,", "tokens": ["Sonst", "war", "mei\u00b7ne", "See\u00b7le", "ein", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein leichter, feiner, bl\u00fctenverliebter Schmetterling,", "tokens": ["Ein", "leich\u00b7ter", ",", "fei\u00b7ner", ",", "bl\u00fc\u00b7ten\u00b7ver\u00b7lieb\u00b7ter", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der sich im Sonnenscheine von weichen Winden", "tokens": ["Der", "sich", "im", "Son\u00b7nen\u00b7schei\u00b7ne", "von", "wei\u00b7chen", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gerne tragen lie\u00df, wie ein Blumenblatt;", "tokens": ["Ger\u00b7ne", "tra\u00b7gen", "lie\u00df", ",", "wie", "ein", "Blu\u00b7men\u00b7blatt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und er steckte sein Saugr\u00fcsselchen gerne in alle S\u00fc\u00dfigkeit,", "tokens": ["Und", "er", "steck\u00b7te", "sein", "Saug\u00b7r\u00fcs\u00b7sel\u00b7chen", "ger\u00b7ne", "in", "al\u00b7le", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "--+--+-+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und er berauschte sich gerne am Tausendblumengeist,", "tokens": ["Und", "er", "be\u00b7rauschte", "sich", "ger\u00b7ne", "am", "Tau\u00b7send\u00b7blu\u00b7men\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und im offenen, samenstaubduftigen Scho\u00dfe \u00fcppiger,", "tokens": ["Und", "im", "of\u00b7fe\u00b7nen", ",", "sa\u00b7men\u00b7staub\u00b7duf\u00b7ti\u00b7gen", "Scho\u00b7\u00dfe", "\u00fcp\u00b7pi\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+--+--+-+--", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Buttergelber Rosen schlief er gerne,", "tokens": ["But\u00b7ter\u00b7gel\u00b7ber", "Ro\u00b7sen", "s