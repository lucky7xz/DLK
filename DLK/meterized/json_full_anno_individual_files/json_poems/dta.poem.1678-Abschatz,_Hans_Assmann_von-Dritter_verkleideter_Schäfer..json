{"dta.poem.1678": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Dritter verkleideter Sch\u00e4fer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Weil uns noch die Jahre g\u00fcnnen ", "tokens": ["Weil", "uns", "noch", "die", "Jah\u00b7re", "g\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu gen\u00fcssen unsrer Zeit/", "tokens": ["Zu", "ge\u00b7n\u00fcs\u00b7sen", "uns\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So lasst keinen Tag verrinnen/", "tokens": ["So", "lasst", "kei\u00b7nen", "Tag", "ver\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kinder/ sonder Fr\u00f6ligkeit.", "tokens": ["Kin\u00b7der", "/", "son\u00b7der", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lasst uns leben wohl vergn\u00fcgt", "tokens": ["Lasst", "uns", "le\u00b7ben", "wohl", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil es Zeit und Gl\u00fccke f\u00fcgt.", "tokens": ["Weil", "es", "Zeit", "und", "Gl\u00fc\u00b7cke", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geht in Matten auff und nieder/", "tokens": ["Geht", "in", "Mat\u00b7ten", "auff", "und", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "KON", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut die bunten Heerden an/", "tokens": ["Schaut", "die", "bun\u00b7ten", "Heer\u00b7den", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dem Bocke/ wie dem Widder", "tokens": ["Wie", "dem", "Bo\u00b7cke", "/", "wie", "dem", "Wid\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$(", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaf und Gei\u00dfe liebeln kan:", "tokens": ["Schaf", "und", "Gei\u00b7\u00dfe", "lie\u00b7beln", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebet so wie sie vergn\u00fcgt/", "tokens": ["Le\u00b7bet", "so", "wie", "sie", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "PPER", "VVPP", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Weil es Zeit und Gl\u00fccke f\u00fcgt.", "tokens": ["Weil", "es", "Zeit", "und", "Gl\u00fc\u00b7cke", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traurigkeit heilt keine Wunden", "tokens": ["Trau\u00b7rig\u00b7keit", "heilt", "kei\u00b7ne", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcllt den leeren S\u00e4ckel nicht:", "tokens": ["F\u00fcllt", "den", "lee\u00b7ren", "S\u00e4\u00b7ckel", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brauchet eurer Jugend Stunden/", "tokens": ["Brau\u00b7chet", "eu\u00b7rer", "Ju\u00b7gend", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh den Mutt das Alter bricht/", "tokens": ["Eh", "den", "Mutt", "das", "Al\u00b7ter", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebet so mit uns vergn\u00fcgt/", "tokens": ["Le\u00b7bet", "so", "mit", "uns", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Wie es Zeit und Gl\u00fccke f\u00fcgt.", "tokens": ["Wie", "es", "Zeit", "und", "Gl\u00fc\u00b7cke", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}