{"textgrid.poem.41382": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Papagei", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Cuba war ein Papagei,", "tokens": ["In", "Cu\u00b7ba", "war", "ein", "Pa\u00b7pa\u00b7gei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den neckt' ein jeder um die Wette;", "tokens": ["Den", "neckt'", "ein", "je\u00b7der", "um", "die", "Wet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein einziger gestand, da\u00df er gelehrig sei,", "tokens": ["Kein", "ein\u00b7zi\u00b7ger", "ge\u00b7stand", ",", "da\u00df", "er", "ge\u00b7leh\u00b7rig", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch da\u00df ihn die Natur recht sch\u00f6n befiedert h\u00e4tte.", "tokens": ["Noch", "da\u00df", "ihn", "die", "Na\u00b7tur", "recht", "sch\u00f6n", "be\u00b7fie\u00b7dert", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "ADJD", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er wird drauf nach Madrid gebracht;", "tokens": ["Er", "wird", "drauf", "nach", "Mad\u00b7rid", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da \u00fcbertrifft sein Witz die kl\u00fcgsten Papageien;", "tokens": ["Da", "\u00fc\u00b7bert\u00b7rifft", "sein", "Witz", "die", "kl\u00fcgs\u00b7ten", "Pa\u00b7pa\u00b7gei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So oft der muntre Psittich lacht,", "tokens": ["So", "oft", "der", "mun\u00b7tre", "Psit\u00b7tich", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So oft er etwas nachgemacht,", "tokens": ["So", "oft", "er", "et\u00b7was", "nach\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Scheint \u00fcber seine Kunst sich alles zu erfreuen;", "tokens": ["Scheint", "\u00fc\u00b7ber", "sei\u00b7ne", "Kunst", "sich", "al\u00b7les", "zu", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PRF", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sogar sein ernster Herr in seiner Brillenpracht.", "tokens": ["So\u00b7gar", "sein", "erns\u00b7ter", "Herr", "in", "sei\u00b7ner", "Bril\u00b7len\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er tr\u00f6stet sich in diesem Stande,", "tokens": ["Er", "tr\u00f6s\u00b7tet", "sich", "in", "die\u00b7sem", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo seinem Wunsche nichts gebricht;", "tokens": ["Wo", "sei\u00b7nem", "Wun\u00b7sche", "nichts", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schaut, spricht er, Kluge gelten nicht,", "tokens": ["Schaut", ",", "spricht", "er", ",", "Klu\u00b7ge", "gel\u00b7ten", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als au\u00dfer ihrem Vaterlande.", "tokens": ["Als", "au\u00b7\u00dfer", "ih\u00b7rem", "Va\u00b7ter\u00b7lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In Cuba war ein Papagei,", "tokens": ["In", "Cu\u00b7ba", "war", "ein", "Pa\u00b7pa\u00b7gei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den neckt' ein jeder um die Wette;", "tokens": ["Den", "neckt'", "ein", "je\u00b7der", "um", "die", "Wet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {