{"dta.poem.16015": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagest, da\u00df die Welt so unvollkommen ist,", "tokens": ["Du", "kla\u00b7gest", ",", "da\u00df", "die", "Welt", "so", "un\u00b7voll\u00b7kom\u00b7men", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fragst, warum? Weil du so unvollkommen bist.", "tokens": ["Und", "fragst", ",", "wa\u00b7rum", "?", "Weil", "du", "so", "un\u00b7voll\u00b7kom\u00b7men", "bist", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "$.", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du vollkommen w\u00e4rst, w\u00e4r auch die Welt vollkommen,", "tokens": ["Wenn", "du", "voll\u00b7kom\u00b7men", "w\u00e4rst", ",", "w\u00e4r", "auch", "die", "Welt", "voll\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Unvollkommenheit w\u00e4r ihr von dir genommen.", "tokens": ["Die", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", "w\u00e4r", "ihr", "von", "dir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie will Vollkommenheit nur mit dir selbst empfahn,", "tokens": ["Sie", "will", "Voll\u00b7kom\u00b7men\u00b7heit", "nur", "mit", "dir", "selbst", "em\u00b7pfahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du bist noch so weit zur\u00fcck auf dieser Bahn.", "tokens": ["Und", "du", "bist", "noch", "so", "weit", "zu\u00b7r\u00fcck", "auf", "die\u00b7ser", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "PTKVZ", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dank' ihr da\u00df sie mit dir will halten gleichen Schritt,", "tokens": ["Dank'", "ihr", "da\u00df", "sie", "mit", "dir", "will", "hal\u00b7ten", "glei\u00b7chen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "ADJA", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und spute dich, da\u00df sie auch vorwerts kommt damit!", "tokens": ["Und", "spu\u00b7te", "dich", ",", "da\u00df", "sie", "auch", "vor\u00b7werts", "kommt", "da\u00b7mit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}