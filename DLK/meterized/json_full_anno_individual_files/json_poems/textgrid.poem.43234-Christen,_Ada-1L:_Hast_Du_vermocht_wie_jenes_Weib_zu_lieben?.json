{"textgrid.poem.43234": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast Du vermocht wie jenes Weib zu lieben?", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast Du vermocht wie jenes Weib zu lieben?", "tokens": ["Hast", "Du", "ver\u00b7mocht", "wie", "je\u00b7nes", "Weib", "zu", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KOKOM", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast Dich lieblos einst von mir gerissen,", "tokens": ["Du", "hast", "Dich", "lieb\u00b7los", "einst", "von", "mir", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ich war krank, so krank an Wahn und Wissen,", "tokens": ["Und", "ich", "war", "krank", ",", "so", "krank", "an", "Wahn", "und", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hast's erkannt, Du hast ja einst geschrieben:", "tokens": ["Du", "hast's", "er\u00b7kannt", ",", "Du", "hast", "ja", "einst", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.6": {"text": "So schlicht und r\u00fchrend damals Deine Klagen,", "tokens": ["So", "schlicht", "und", "r\u00fch\u00b7rend", "da\u00b7mals", "Dei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was Du gef\u00fcrchtet, hat dies Weib ertragen,", "tokens": ["Was", "Du", "ge\u00b7f\u00fcrch\u00b7tet", ",", "hat", "dies", "Weib", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und der Geliebte ist von ihr gegangen. \u2013", "tokens": ["Und", "der", "Ge\u00b7lieb\u00b7te", "ist", "von", "ihr", "ge\u00b7gan\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Durch meine Seele wogt ein dumpfer Jammer", "tokens": ["Durch", "mei\u00b7ne", "See\u00b7le", "wogt", "ein", "dum\u00b7pfer", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und alle Pfeiler meines Lebens wanken,", "tokens": ["Und", "al\u00b7le", "Pfei\u00b7ler", "mei\u00b7nes", "Le\u00b7bens", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein Heer von Fragen \u2013 schlummernde Gedanken", "tokens": ["Ein", "Heer", "von", "Fra\u00b7gen", "\u2013", "schlum\u00b7mern\u00b7de", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Weckt Jene, die nun stirbt in meiner Kammer ...", "tokens": ["Weckt", "Je\u00b7ne", ",", "die", "nun", "stirbt", "in", "mei\u00b7ner", "Kam\u00b7mer", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "PRELS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hast Du vermocht wie jenes Weib zu lieben?", "tokens": ["Hast", "Du", "ver\u00b7mocht", "wie", "je\u00b7nes", "Weib", "zu", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KOKOM", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast Dich lieblos einst von mir gerissen,", "tokens": ["Du", "hast", "Dich", "lieb\u00b7los", "einst", "von", "mir", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ich war krank, so krank an Wahn und Wissen,", "tokens": ["Und", "ich", "war", "krank", ",", "so", "krank", "an", "Wahn", "und", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hast's erkannt, Du hast ja einst gesch