{"textgrid.poem.66697": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Mein Pakt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als ich auf diesen Planeten kam", "tokens": ["Und", "als", "ich", "auf", "die\u00b7sen", "Pla\u00b7ne\u00b7ten", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Aus Urweltmeeren geschwommen,", "tokens": ["Aus", "Ur\u00b7welt\u00b7mee\u00b7ren", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da hab ich, bevor ich den B\u00fcrgerbrief nahm,", "tokens": ["Da", "hab", "ich", ",", "be\u00b7vor", "ich", "den", "B\u00fcr\u00b7ger\u00b7brief", "nahm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Gut vorweg mir genommen:", "tokens": ["Ein", "Gut", "vor\u00b7weg", "mir", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich hab unter einer Bedingung nur", "tokens": ["Ich", "hab", "un\u00b7ter", "ei\u00b7ner", "Be\u00b7din\u00b7gung", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Dem Erdgeist bewilligt die Steuer,", "tokens": ["Dem", "Erd\u00b7geist", "be\u00b7wil\u00b7ligt", "die", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Bekr\u00e4ftigt mit elementarem Schwur", "tokens": ["Be\u00b7kr\u00e4f\u00b7tigt", "mit", "e\u00b7lem\u00b7en\u00b7ta\u00b7rem", "Schwur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Bei Erde, Wind, Wasser und Feuer.", "tokens": ["Bei", "Er\u00b7de", ",", "Wind", ",", "Was\u00b7ser", "und", "Feu\u00b7er", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er solle, was auf seinem sausenden Rund", "tokens": ["Er", "sol\u00b7le", ",", "was", "auf", "sei\u00b7nem", "sau\u00b7sen\u00b7den", "Rund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Mir immerdar m\u00f6ge passieren,", "tokens": ["Mir", "im\u00b7mer\u00b7dar", "m\u00f6\u00b7ge", "pas\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mir unmittelbare Freiheit und", "tokens": ["Mir", "un\u00b7mit\u00b7tel\u00b7ba\u00b7re", "Frei\u00b7heit", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "KON"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vollmacht von Gott garantieren.", "tokens": ["Voll\u00b7macht", "von", "Gott", "ga\u00b7ran\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ein unverletzliches Lebenslehn", "tokens": ["Ein", "un\u00b7ver\u00b7letz\u00b7li\u00b7ches", "Le\u00b7bens\u00b7lehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zu wahren aus allen Tr\u00fcmmern:", "tokens": ["Zu", "wah\u00b7ren", "aus", "al\u00b7len", "Tr\u00fcm\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ohne Hoffnung und Furcht auf mir selber zu stehn,", "tokens": ["Oh\u00b7ne", "Hoff\u00b7nung", "und", "Furcht", "auf", "mir", "sel\u00b7ber", "zu", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Mich um Menschengekl\u00e4ff nicht zu k\u00fcmmern.", "tokens": ["Mich", "um", "Men\u00b7schen\u00b7ge\u00b7kl\u00e4ff", "nicht", "zu", "k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Dem einzigen Meister im Herzen mir,", "tokens": ["Dem", "ein\u00b7zi\u00b7gen", "Meis\u00b7ter", "im", "Her\u00b7zen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Meinem Gott zu dienen voll Freuden,", "tokens": ["Mei\u00b7nem", "Gott", "zu", "die\u00b7nen", "voll", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "ADJD", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die Liebe zum h\u00f6chsten Leben hier", "tokens": ["Die", "Lie\u00b7be", "zum", "h\u00f6chs\u00b7ten", "Le\u00b7ben", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht als falscher Knecht zu vergeuden.", "tokens": ["Nicht", "als", "fal\u00b7scher", "Knecht", "zu", "ver\u00b7geu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dem einzigen Herrn zu halten die Treu", "tokens": ["Dem", "ein\u00b7zi\u00b7gen", "Herrn", "zu", "hal\u00b7ten", "die", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In allen F\u00e4hrden und Schlingen,", "tokens": ["In", "al\u00b7len", "F\u00e4hr\u00b7den", "und", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und immer vom Grund der Seele neu", "tokens": ["Und", "im\u00b7mer", "vom", "Grund", "der", "See\u00b7le", "neu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Freiwillig mein Opfer zu bringen.", "tokens": ["Frei\u00b7wil\u00b7lig", "mein", "Op\u00b7fer", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Das war die Bedingung, das war der Pakt,", "tokens": ["Das", "war", "die", "Be\u00b7din\u00b7gung", ",", "das", "war", "der", "Pakt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den ich mit dem Erdgeist geschlossen,", "tokens": ["Den", "ich", "mit", "dem", "Erd\u00b7geist", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Als ich aus Urweltmeeren nackt", "tokens": ["Als", "ich", "aus", "Ur\u00b7welt\u00b7mee\u00b7ren", "nackt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ans ird'sche Gestade geflossen.", "tokens": ["Ans", "ird'\u00b7sche", "Ge\u00b7sta\u00b7de", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und anders \u2013 bei diesem Federstrich! \u2013", "tokens": ["Und", "an\u00b7ders", "\u2013", "bei", "die\u00b7sem", "Fe\u00b7der\u00b7strich", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Lern' ich