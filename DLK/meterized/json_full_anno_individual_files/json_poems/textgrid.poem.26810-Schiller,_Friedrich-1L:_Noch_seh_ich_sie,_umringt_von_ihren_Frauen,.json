{"textgrid.poem.26810": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch seh ich sie, umringt von ihren Frauen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch seh ich sie, umringt von ihren Frauen,", "tokens": ["Noch", "seh", "ich", "sie", ",", "um\u00b7ringt", "von", "ih\u00b7ren", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die herrlichste von allen stand sie da,", "tokens": ["Die", "herr\u00b7lichs\u00b7te", "von", "al\u00b7len", "stand", "sie", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie eine Sonne war sie anzuschauen,", "tokens": ["Wie", "ei\u00b7ne", "Son\u00b7ne", "war", "sie", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich stand von fern und wagte mich nicht nah,", "tokens": ["Ich", "stand", "von", "fern", "und", "wag\u00b7te", "mich", "nicht", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es fa\u00dfte mich mit wollustvollem Grauen,", "tokens": ["Es", "fa\u00df\u00b7te", "mich", "mit", "wol\u00b7lust\u00b7vol\u00b7lem", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als ich den Glanz vor mir verbreitet sah,", "tokens": ["Als", "ich", "den", "Glanz", "vor", "mir", "ver\u00b7brei\u00b7tet", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch schnell, als h\u00e4tten Fl\u00fcgel mich getragen,", "tokens": ["Doch", "schnell", ",", "als", "h\u00e4t\u00b7ten", "Fl\u00fc\u00b7gel", "mich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ergriff es mich, die Saiten anzuschlagen.", "tokens": ["Er\u00b7griff", "es", "mich", ",", "die", "Sai\u00b7ten", "an\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was ich in jenem Augenblick empfunden", "tokens": ["Was", "ich", "in", "je\u00b7nem", "Au\u00b7gen\u00b7blick", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was ich sang, vergebens sinn ich nach,", "tokens": ["Und", "was", "ich", "sang", ",", "ver\u00b7ge\u00b7bens", "sinn", "ich", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein neu Organ hatt ich in mir gefunden,", "tokens": ["Ein", "neu", "Or\u00b7gan", "hatt", "ich", "in", "mir", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das meines Herzens heilge Regung sprach,", "tokens": ["Das", "mei\u00b7nes", "Her\u00b7zens", "heil\u00b7ge", "Re\u00b7gung", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Seele wars, die, jahrelang gebunden,", "tokens": ["Die", "See\u00b7le", "wars", ",", "die", ",", "jah\u00b7re\u00b7lang", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch alle Fesseln jetzt auf einmal brach", "tokens": ["Durch", "al\u00b7le", "Fes\u00b7seln", "jetzt", "auf", "ein\u00b7mal", "brach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "APPR", "ADV", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Und T\u00f6ne fand in ihren tiefsten Tiefen,", "tokens": ["Und", "T\u00f6\u00b7ne", "fand", "in", "ih\u00b7ren", "tiefs\u00b7ten", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die ungeahnt und g\u00f6ttlich in ihr schliefen.", "tokens": ["Die", "un\u00b7ge\u00b7ahnt", "und", "g\u00f6tt\u00b7lich", "in", "ihr", "schlie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als die Saiten lange schon geschwiegen,", "tokens": ["Und", "als", "die", "Sai\u00b7ten", "lan\u00b7ge", "schon", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele endlich mir zur\u00fccke kam,", "tokens": ["Die", "See\u00b7le", "end\u00b7lich", "mir", "zu\u00b7r\u00fc\u00b7cke", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sah ich in den engelgleichen Z\u00fcgen", "tokens": ["Da", "sah", "ich", "in", "den", "en\u00b7gel\u00b7glei\u00b7chen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe ringen mit der holden Scham,", "tokens": ["Die", "Lie\u00b7be", "rin\u00b7gen", "mit", "der", "hol\u00b7den", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und alle Himmel glaubt' ich zu erfliegen,", "tokens": ["Und", "al\u00b7le", "Him\u00b7mel", "glaubt'", "ich", "zu", "er\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als ich das leise s\u00fc\u00dfe Wort vernahm \u2013", "tokens": ["Als", "ich", "das", "lei\u00b7se", "s\u00fc\u00b7\u00dfe", "Wort", "ver\u00b7nahm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O droben nur in selger Geister Ch\u00f6ren", "tokens": ["O", "dro\u00b7ben", "nur", "in", "sel\u00b7ger", "Geis\u00b7ter", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Werd ich des Tones Wohllaut wieder h\u00f6ren!", "tokens": ["Werd", "ich", "des", "To\u00b7nes", "Wohl\u00b7laut", "wie\u00b7der", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbdas treue Herz, das trostlos sich verzehrt", "tokens": ["\u00bb", "das", "treu\u00b7e", "Herz", ",", "das", "trost\u00b7los", "sich", "ver\u00b7zehrt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und still bescheiden nie gewagt zu sprechen,", "tokens": ["Und", "still", "be\u00b7schei\u00b7den", "nie", "ge\u00b7wagt", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich kenne den ihm selbst verborgnen Wert,", "tokens": ["Ich", "ken\u00b7ne", "den", "ihm", "selbst", "ver\u00b7borg\u00b7nen", "Wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am rohen Gl\u00fcck will ich das Edle r\u00e4chen.", "tokens": ["Am", "ro\u00b7hen", "Gl\u00fcck", "will", "ich", "das", "Ed\u00b7le", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem Armen sei das sch\u00f6nste Los beschert,", "tokens": ["Dem", "Ar\u00b7men", "sei", "das", "sch\u00f6ns\u00b7te", "Los", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur Liebe darf der Liebe Blume brechen.", "tokens": ["Nur", "Lie\u00b7be", "darf", "der", "Lie\u00b7be", "Blu\u00b7me", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der sch\u00f6nste Schatz geh\u00f6rt dem Herzen an,", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Schatz", "ge\u00b7h\u00f6rt", "dem", "Her\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das ihn erwidern und empfinden kann.\u00ab", "tokens": ["Das", "ihn", "er\u00b7wi\u00b7dern", "und", "emp\u00b7fin\u00b7den", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PPER", "VVINF", "KON", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Noch seh ich sie, umringt von ihren Frauen,", "tokens": ["Noch", "seh", "ich", "sie", ",", "um\u00b7ringt", "von", "ih\u00b7ren", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die herrlichste von allen stand sie da,", "tokens": ["Die", "herr\u00b7lichs\u00b7te", "von", "al\u00b7len", "stand", "sie", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie eine Sonne war sie anzuschauen,", "tokens": ["Wie", "ei\u00b7ne", "Son\u00b7ne", "war", "sie", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich stand von fern und wagte mich nicht nah,", "tokens": ["Ich", "stand", "von", "fern", "und", "wag\u00b7te", "mich", "nicht", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es fa\u00dfte mich mit wollustvollem Grauen,", "tokens": ["Es", "fa\u00df\u00b7te", "mich", "mit", "wol\u00b7lust\u00b7vol\u00b7lem", "Grau\u00b7en", ","], "token_info": ["word", "word", "w