{"textgrid.poem.26732": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin segelt das Schiff? Es tr\u00e4gt sidonische M\u00e4nner,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin segelt das Schiff? Es tr\u00e4gt sidonische M\u00e4nner,", "tokens": ["Wo\u00b7hin", "se\u00b7gelt", "das", "Schiff", "?", "Es", "tr\u00e4gt", "si\u00b7do\u00b7ni\u00b7sche", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die von dem frierenden Nord bringen den Bernstein, das Zinn.", "tokens": ["Die", "von", "dem", "frie\u00b7ren\u00b7den", "Nord", "brin\u00b7gen", "den", "Bern\u00b7stein", ",", "das", "Zinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Trag es gn\u00e4dig, Neptun, und wiegt es schonend, ihr Winde,", "tokens": ["Trag", "es", "gn\u00e4\u00b7dig", ",", "Nep\u00b7tun", ",", "und", "wiegt", "es", "scho\u00b7nend", ",", "ihr", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+----+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In bewirtender Bucht rausch ihm ein trinkbarer Quell.", "tokens": ["In", "be\u00b7wir\u00b7ten\u00b7der", "Bucht", "rausch", "ihm", "ein", "t