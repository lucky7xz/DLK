{"dta.poem.2157": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Atheist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch, der auf der Welt gelebt, und sich nicht", "tokens": ["Ein", "Mensch", ",", "der", "auf", "der", "Welt", "ge\u00b7lebt", ",", "und", "sich", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,", "KON", "PRF", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "an der Welt vergn\u00fcget,", "tokens": ["an", "der", "Welt", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Den Sch\u00f6pfer nicht in ihr gefunden, und nicht in ihrer", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "nicht", "in", "ihr", "ge\u00b7fun\u00b7den", ",", "und", "nicht", "in", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPOSAT", "VVPP", "$,", "KON", "PTKNEG", "APPR", "PPOSAT"], "meter": "-+-++--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ordnung, Pracht", "tokens": ["Ord\u00b7nung", ",", "Pracht"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und Sch\u00f6nheit einen Stral der Weisheit, die sie so wun-", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "ei\u00b7nen", "Stral", "der", "Weis\u00b7heit", ",", "die", "sie", "so", "wun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "derbar gef\u00fcget,", "tokens": ["der\u00b7bar", "ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bemerkt, sein\u2019 unumschr\u00e4nkte Macht", "tokens": ["Be\u00b7merkt", ",", "sein'", "un\u00b7um\u00b7schr\u00e4nk\u00b7te", "Macht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht angebetet, nicht bewundert, und seiner Liebe nicht", "tokens": ["Nicht", "an\u00b7ge\u00b7be\u00b7tet", ",", "nicht", "be\u00b7wun\u00b7dert", ",", "und", "sei\u00b7ner", "Lie\u00b7be", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "$,", "PTKNEG", "VVPP", "$,", "KON", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "genossen,", "tokens": ["ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Da sich doch so viel Segensstr\u00f6m\u2019 in solcher F\u00fcll\u2019 auf ihn", "tokens": ["Da", "sich", "doch", "so", "viel", "Se\u00b7gens\u00b7str\u00f6m'", "in", "sol\u00b7cher", "F\u00fcll'", "auf", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ADV", "PIAT", "NN", "APPR", "PIAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "ergossen,", "tokens": ["er\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Der, da der Sch\u00f6pfer so viel Guts durch sein Gesch\u00f6pf", "tokens": ["Der", ",", "da", "der", "Sch\u00f6p\u00b7fer", "so", "viel", "Guts", "durch", "sein", "Ge\u00b7sch\u00f6pf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ihm hier erwiesen,", "tokens": ["ihm", "hier", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Dem gro\u00dfen Geber nicht gedankt und seine G\u00fcte nicht", "tokens": ["Dem", "gro\u00b7\u00dfen", "Ge\u00b7ber", "nicht", "ge\u00b7dankt", "und", "sei\u00b7ne", "G\u00fc\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVPP", "KON", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "gepriesen,", "tokens": ["ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "H\u00e4tt er auch aller Welt Metall, ja, h\u00e4tt er K\u00f6nigreich\u2019", "tokens": ["H\u00e4tt", "er", "auch", "al\u00b7ler", "Welt", "Me\u00b7tall", ",", "ja", ",", "h\u00e4tt", "er", "K\u00f6\u00b7nig\u00b7reich'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "NN", "$,", "PTKANT", "$,", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "erworben,", "tokens": ["er\u00b7wor\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Hat, als ein Atheist, gelebt, ist, als ein Atheist, ge-", "tokens": ["Hat", ",", "als", "ein", "A\u00b7theist", ",", "ge\u00b7lebt", ",", "ist", ",", "als", "ein", "A\u00b7theist", ",", "ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "KOUS", "ART", "NN", "$,", "VVPP", "$,", "VAFIN", "$,", "KOUS", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "storben.", "tokens": ["stor\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}