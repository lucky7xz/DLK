{"textgrid.poem.37348": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es geht ja leider nur soso", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geht ja leider nur soso", "tokens": ["Es", "geht", "ja", "lei\u00b7der", "nur", "so\u00b7so"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier auf der Welt, sprach Salomo.", "tokens": ["Hier", "auf", "der", "Welt", ",", "sprach", "Sa\u00b7lo\u00b7mo", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies war verzeihlich. Das Geschnatter", "tokens": ["Dies", "war", "ver\u00b7zeih\u00b7lich", ".", "Das", "Ge\u00b7schnat\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von tausend Frauen, denn die hatt' er,", "tokens": ["Von", "tau\u00b7send", "Frau\u00b7en", ",", "denn", "die", "hatt'", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "KON", "PDS", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Macht auch den Besten ungerecht.", "tokens": ["Macht", "auch", "den", "Bes\u00b7ten", "un\u00b7ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Uns aber geht es nicht so schlecht.", "tokens": ["Uns", "a\u0