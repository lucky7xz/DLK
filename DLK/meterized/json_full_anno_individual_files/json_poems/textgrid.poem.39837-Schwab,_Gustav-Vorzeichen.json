{"textgrid.poem.39837": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Vorzeichen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft, wenn ich einen langen Tag verloren,", "tokens": ["Oft", ",", "wenn", "ich", "ei\u00b7nen", "lan\u00b7gen", "Tag", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit sehnsuchtsvollem Harren, eitlem Hoffen,", "tokens": ["Mit", "sehn\u00b7suchts\u00b7vol\u00b7lem", "Har\u00b7ren", ",", "eit\u00b7lem", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht auf den trauten Wegen Sie getroffen", "tokens": ["Nicht", "auf", "den", "trau\u00b7ten", "We\u00b7gen", "Sie", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Liebe, G\u00f6tter, Gl\u00fcck umsonst beschworen:", "tokens": ["Und", "Lie\u00b7be", ",", "G\u00f6t\u00b7ter", ",", "Gl\u00fcck", "um\u00b7sonst", "be\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Tritt Nachts der Traum still zu dem armen Thoren", "tokens": ["Tritt", "Nachts", "der", "Traum", "still", "zu", "dem", "ar\u00b7men", "Tho\u00b7ren"], "token_info": ["