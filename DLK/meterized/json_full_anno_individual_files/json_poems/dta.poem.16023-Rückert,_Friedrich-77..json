{"dta.poem.16023": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr sagt, den Glanz des Lichts zu h\u00f6hen dient der Schatten;", "tokens": ["Ihr", "sagt", ",", "den", "Glanz", "des", "Lichts", "zu", "h\u00f6\u00b7hen", "dient", "der", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcr die K\u00f6rperwelt will ich euch das gestatten.", "tokens": ["Und", "f\u00fcr", "die", "K\u00f6r\u00b7per\u00b7welt", "will", "ich", "euch", "das", "ge\u00b7stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch f\u00fcr die Geisterwelt was soll des B\u00f6sen Schatten,", "tokens": ["Doch", "f\u00fcr", "die", "Geis\u00b7ter\u00b7welt", "was", "soll", "des", "B\u00f6\u00b7sen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PWS", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nie dem reinen Licht des Guten sich kann gatten?", "tokens": ["Der", "nie", "dem", "rei\u00b7nen", "Licht", "des", "Gu\u00b7ten", "sich", "kann", "gat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ART", "NN", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ohnm\u00e4chtig scheint die Kraft des Lichtes zu ermatten,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "scheint", "die", "Kraft", "des", "Lich\u00b7tes", "zu", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nicht in seinen Glanz aufl\u00f6sen kann die Schatten.", "tokens": ["Das", "nicht", "in", "sei\u00b7nen", "Glanz", "auf\u00b7l\u00f6\u00b7sen", "kann", "die", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie aber k\u00f6nnten sich ins Licht aufl\u00f6sen Schatten,", "tokens": ["Wie", "a\u00b7ber", "k\u00f6nn\u00b7ten", "sich", "ins", "Licht", "auf\u00b7l\u00f6\u00b7sen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PRF", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem sie selber sich verstockt dagegen hatten?", "tokens": ["Nach\u00b7dem", "sie", "sel\u00b7ber", "sich", "ver\u00b7stockt", "da\u00b7ge\u00b7gen", "hat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "VVFIN", "PAV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer l\u00f6st den Widerspruch? Ein Ausweg kommt zu Statten:", "tokens": ["Wer", "l\u00f6st", "den", "Wi\u00b7der\u00b7spruch", "?", "Ein", "Aus\u00b7weg", "kommt", "zu", "Stat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Licht wird er nicht, es wird in sich zunicht der Schatten.", "tokens": ["Licht", "wird", "er", "nicht", ",", "es", "wird", "in", "sich", "zu\u00b7nicht", "der", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "APPR", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In Selbverzehrung wird des B\u00f6sen Grimm ersatten;", "tokens": ["In", "Selb\u00b7ver\u00b7zeh\u00b7rung", "wird", "des", "B\u00f6\u00b7sen", "Grimm", "er\u00b7sat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rein bleibt des Guten Licht, wo blieb des B\u00f6sen Schatten?", "tokens": ["Rein", "bleibt", "des", "Gu\u00b7ten", "Licht", ",", "wo", "blieb", "des", "B\u00f6\u00b7sen", "Schat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}