{"dta.poem.16237": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "291.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist schon, weil ich bin; denn also f\u00fchl' ich mich,", "tokens": ["Du", "bist", "schon", ",", "weil", "ich", "bin", ";", "denn", "al\u00b7so", "f\u00fchl'", "ich", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPER", "VAFIN", "$.", "KON", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich durch mich nichts bin, und alles bin durch dich.", "tokens": ["Da\u00df", "ich", "durch", "mich", "nichts", "bin", ",", "und", "al\u00b7les", "bin", "durch", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PIS", "VAFIN", "$,", "KON", "PIS", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der du zum lebenden Beweise dir mich schufest;", "tokens": ["Der", "du", "zum", "le\u00b7ben\u00b7den", "Be\u00b7wei\u00b7se", "dir", "mich", "schu\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dich zu beweisen, ist, wozu du mich berufest:", "tokens": ["Dich", "zu", "be\u00b7wei\u00b7sen", ",", "ist", ",", "wo\u00b7zu", "du", "mich", "be\u00b7ru\u00b7fest", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Dich zu beweisen durch mich selbst mir und der Welt,", "tokens": ["Dich", "zu", "be\u00b7wei\u00b7sen", "durch", "mich", "selbst", "mir", "und", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "PPER", "ADV", "PPER", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die den Beweis von dir nicht kennt, den sie enth\u00e4lt.", "tokens": ["Die", "den", "Be\u00b7weis", "von", "dir", "nicht", "kennt", ",", "den", "sie", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "PTKNEG", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}