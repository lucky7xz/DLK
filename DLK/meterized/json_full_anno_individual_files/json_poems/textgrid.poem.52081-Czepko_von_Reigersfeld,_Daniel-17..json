{"textgrid.poem.52081": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nechst gieng mein guter Freund vor seiner Liebsten Th\u00fcr", "tokens": ["Nechst", "gieng", "mein", "gu\u00b7ter", "Freund", "vor", "sei\u00b7ner", "Liebs\u00b7ten", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lie\u00df wie auff den Marckt die leichten Schenckel gleiten,", "tokens": ["Und", "lie\u00df", "wie", "auff", "den", "Marckt", "die", "leich\u00b7ten", "Schen\u00b7ckel", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sah' als wie ein Falck am Haus an allen Seiten,", "tokens": ["Und", "sah'", "als", "wie", "ein", "Falck", "am", "Haus", "an", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "KOKOM", "ART", "NN", "APPRART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob durch das lichte Gla\u00df gl\u00e4ntzt ihre rundte Zier.", "tokens": ["Ob", "durch", "das", "lich\u00b7te", "Gla\u00df", "gl\u00e4ntzt", "ih\u00b7re", "rund\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun schau, er wolt' als sie den Wirbel auff wil drehn,", "tokens": ["Nun", "schau", ",", "er", "wolt'", "als", "sie", "den", "Wir\u00b7bel", "auff", "wil", "drehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "VMFIN", "KOUS", "PPER", "ART", "NN", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort \u00fcber das Gerinn in Capriolen fliegen,", "tokens": ["Dort", "\u00fc\u00b7ber", "das", "Ge\u00b7rinn", "in", "Cap\u00b7ri\u00b7o\u00b7len", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Weh! der Nestel brach, die Hosen blieben liegen,", "tokens": ["O", "Weh", "!", "der", "Nes\u00b7tel", "brach", ",", "die", "Ho\u00b7sen", "blie\u00b7ben", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was drunter lag, halt ich, wolt' ich auffs Fenster sehn.", "tokens": ["Was", "drun\u00b7ter", "lag", ",", "halt", "ich", ",", "wolt'", "ich", "auffs", "Fens\u00b7ter", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanz