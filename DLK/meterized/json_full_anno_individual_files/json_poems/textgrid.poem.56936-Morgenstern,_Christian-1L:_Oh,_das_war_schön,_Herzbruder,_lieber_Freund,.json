{"textgrid.poem.56936": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oh, das war sch\u00f6n, Herzbruder, lieber Freund,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh, das war sch\u00f6n, Herzbruder, lieber Freund,", "tokens": ["Oh", ",", "das", "war", "sch\u00f6n", ",", "Herz\u00b7bru\u00b7der", ",", "lie\u00b7ber", "Freund", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ADJD", "$,", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als wir die kalte, klare Weihenacht", "tokens": ["als", "wir", "die", "kal\u00b7te", ",", "kla\u00b7re", "Wei\u00b7he\u00b7nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ausfuhren \u00fcbers eingeschneite Land!", "tokens": ["aus\u00b7fuh\u00b7ren", "\u00fc\u00b7bers", "ein\u00b7ge\u00b7schnei\u00b7te", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durchs Astgewirr der Pappelb\u00e4ume brach", "tokens": ["Durchs", "Ast\u00b7ge\u00b7wirr", "der", "Pap\u00b7pel\u00b7b\u00e4u\u00b7me", "brach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "der stillen Felder meilenweites Wei\u00df;", "tokens": ["der", "stil\u00b7len", "Fel\u00b7der", "mei\u00b7len\u00b7wei\u00b7tes", "Wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die Erde ward uns wieder einmal rund,", "tokens": ["die", "Er\u00b7de", "ward", "uns", "wie\u00b7der", "ein\u00b7mal", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und unser Geist ein Vogel \u00fcber ihr.", "tokens": ["und", "un\u00b7ser", "Geist", "ein", "Vo\u00b7gel", "\u00fc\u00b7ber", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Pferde dampften, und mit ihrem Trab", "tokens": ["Die", "Pfer\u00b7de", "dampf\u00b7ten", ",", "und", "mit", "ih\u00b7rem", "Trab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "im Takte scholl das traute Schellenzeug;", "tokens": ["im", "Tak\u00b7te", "scholl", "das", "trau\u00b7te", "Schel\u00b7len\u00b7zeug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "des Kutschers Riesenmantel flatterte", "tokens": ["des", "Kut\u00b7schers", "Rie\u00b7sen\u00b7man\u00b7tel", "flat\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und holte seine Sensen-Gei\u00dfel aus,", "tokens": ["und", "hol\u00b7te", "sei\u00b7ne", "Sen\u00b7sen\u00b7Gei\u00b7\u00dfel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "so war es Kronos selber, der uns fuhr.", "tokens": ["so", "war", "es", "Kro\u00b7nos", "sel\u00b7ber", ",", "der", "uns", "fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So sa\u00dfen wir nachdenklich Seit' an Seit',", "tokens": ["So", "sa\u00b7\u00dfen", "wir", "nach\u00b7denk\u00b7lich", "Seit'", "an", "Seit'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "mit seiner jungen Hoffnung jeder still,", "tokens": ["mit", "sei\u00b7ner", "jun\u00b7gen", "Hoff\u00b7nung", "je\u00b7der", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "und jeder still mit seiner jungen Not.", "tokens": ["und", "je\u00b7der", "still", "mit", "sei\u00b7ner", "jun\u00b7gen", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Da pl\u00f6tzlich, als der Blick sich grenzenlos", "tokens": ["Da", "pl\u00f6tz\u00b7lich", ",", "als", "der", "Blick", "sich", "gren\u00b7zen\u00b7los"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "auf \u00c4cker \u00f6ffnete \u2013 ein wei\u00dfer Blitz \u2013", "tokens": ["auf", "\u00c4\u00b7cker", "\u00f6ff\u00b7ne\u00b7te", "\u2013", "ein", "wei\u00b7\u00dfer", "Blitz", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "ein blendend Meteor! \u2013 und wieder Nacht.", "tokens": ["ein", "blen\u00b7dend", "Me\u00b7te\u00b7or", "!", "\u2013", "und", "wie\u00b7der", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$.", "$(", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "(mir hat einmal ein Weib aus meiner Hand", "tokens": ["(", "mir", "hat", "ein\u00b7mal", "ein", "Weib", "aus", "mei\u00b7ner", "Hand"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "den Lebenslauf des Meteors gesagt.)", "tokens": ["den", "Le\u00b7bens\u00b7lauf", "des", "Me\u00b7te\u00b7ors", "ge\u00b7sagt", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die K\u00e4lte schnitt, und knirschend sang der Schnee.", "tokens": ["Die", "K\u00e4l\u00b7te", "schnitt", ",", "und", "knir\u00b7schend", "sang", "der", "Schnee", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wir wandten um, und als die alte Stadt", "tokens": ["Wir", "wand\u00b7ten", "um", ",", "und", "als", "die", "al\u00b7te", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "nun wieder n\u00e4her kam, da gl\u00e4nzte hier", "tokens": ["nun", "wie\u00b7der", "n\u00e4\u00b7her", "kam", ",", "da", "gl\u00e4nz\u00b7te", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "und gl\u00e4nzte dort ein baum-erhelltes Haus:", "tokens": ["und", "gl\u00e4nz\u00b7te", "dort", "ein", "baum\u00b7er\u00b7hell\u00b7tes", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Es war mir wie ein tiefes, fernes Lied", "tokens": ["Es", "war", "mir", "wie", "ein", "tie\u00b7fes", ",", "fer\u00b7nes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "von Erdenkinder Hoffen und Geduld \u2013:", "tokens": ["von", "Er\u00b7den\u00b7kin\u00b7der", "Hof\u00b7fen", "und", "Ge\u00b7duld", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ein bi\u00dfchen Lieb' und Licht, \u2013 und schon ein Fest! \u2013", "tokens": ["Ein", "bi\u00df\u00b7chen", "Lieb'", "und", "Licht", ",", "\u2013", "und", "schon", "ein", "Fest", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word"