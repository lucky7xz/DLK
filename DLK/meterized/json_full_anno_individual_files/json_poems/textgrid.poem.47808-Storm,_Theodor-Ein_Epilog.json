{"textgrid.poem.47808": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ein Epilog", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab es mir zum Trost ersonnen", "tokens": ["Ich", "hab", "es", "mir", "zum", "Trost", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser Zeit der schweren Not,", "tokens": ["In", "die\u00b7ser", "Zeit", "der", "schwe\u00b7ren", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dieser Bl\u00fctezeit der Schufte,", "tokens": ["In", "die\u00b7ser", "Bl\u00fc\u00b7te\u00b7zeit", "der", "Schuf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser Zeit von Salz und Brot.", "tokens": ["In", "die\u00b7ser", "Zeit", "von", "Salz", "und", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich zage nicht, es mu\u00df sich wenden,", "tokens": ["Ich", "za\u00b7ge", "nicht", ",", "es", "mu\u00df", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und heiter wird die Welt erstehn,", "tokens": ["Und", "hei\u00b7ter", "wird", "die", "Welt", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kann der echte Keim des Lebens", "tokens": ["Es", "kann", "der", "ech\u00b7te", "Keim", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht ohne Frucht verlorengehn.", "tokens": ["Nicht", "oh\u00b7ne", "Frucht", "ver\u00b7lo\u00b7ren\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Klang von Fr\u00fchlingsungewittern,", "tokens": ["Der", "Klang", "von", "Fr\u00fch\u00b7lin\u00b7gsun\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem wir schauernd sind erwacht,", "tokens": ["Von", "dem", "wir", "schau\u00b7ernd", "sind", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dem noch alle Wipfel rauschen,", "tokens": ["Von", "dem", "noch", "al\u00b7le", "Wip\u00b7fel", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er kommt noch einmal, \u00fcber Nacht!", "tokens": ["Er", "kommt", "noch", "ein\u00b7mal", ",", "\u00fc\u00b7ber