{"textgrid.poem.63968": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern noch ein d\u00fcrftig Ding,", "tokens": ["Ge\u00b7stern", "noch", "ein", "d\u00fcrf\u00b7tig", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das so grau und albern ging,", "tokens": ["Das", "so", "grau", "und", "al\u00b7bern", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts an ihm zu sehen \u2013", "tokens": ["Nichts", "an", "ihm", "zu", "se\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und mu\u00df heut behutsam sein,", "tokens": ["Und", "mu\u00df", "heut", "be\u00b7hut\u00b7sam", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie wenn im Mai die Bl\u00fcten schnei'n,", "tokens": ["Wie", "wenn", "im", "Mai", "die", "Bl\u00fc\u00b7ten", "schnei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df nicht all verwehen.", "tokens": ["Da\u00df", "nicht", "all", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PIAT", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wie wenn ich Bl\u00fcten an mir habe,", "tokens": ["Wie", "wenn", "ich", "Bl\u00fc\u00b7ten", "an", "mir", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als sei ich eine Gottesgabe, \u2013", "tokens": ["Als", "sei", "ich", "ei\u00b7ne", "Got\u00b7tes\u00b7ga\u00b7be", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein reines Wunder bin ich ja,", "tokens": ["Ein", "rei\u00b7nes", "Wun\u00b7der", "bin", "ich", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie nie ich eins mit Augen sah.", "tokens": ["Wie", "nie", "ich", "eins", "mit", "Au\u00b7gen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mu\u00df mich sehr zusammennehmen", "tokens": ["Und", "mu\u00df", "mich", "sehr", "zu\u00b7sam\u00b7men\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sch\u00e4men.", "tokens": ["Und", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text":