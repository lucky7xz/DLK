{"dta.poem.12280": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied gegen Karl   V .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00fct dich Baur ich komm,", "tokens": ["H\u00fct", "dich", "Baur", "ich", "komm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mach dich bald davon;", "tokens": ["Mach", "dich", "bald", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hauptmann gieb uns Geld,", "tokens": ["Haupt\u00b7mann", "gieb", "uns", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "W\u00e4hrend wir im Feld,", "tokens": ["W\u00e4h\u00b7rend", "wir", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "M\u00e4del komm heran,", "tokens": ["M\u00e4\u00b7del", "komm", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "F\u00fcg dich zu der Kann.", "tokens": ["F\u00fcg", "dich", "zu", "der", "Kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}