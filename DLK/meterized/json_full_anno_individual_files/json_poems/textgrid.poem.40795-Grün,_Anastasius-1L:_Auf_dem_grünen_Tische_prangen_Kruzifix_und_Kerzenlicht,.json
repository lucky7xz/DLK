{"textgrid.poem.40795": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf dem gr\u00fcnen Tische prangen Kruzifix und Kerzenlicht,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem gr\u00fcnen Tische prangen Kruzifix und Kerzenlicht,", "tokens": ["Auf", "dem", "gr\u00fc\u00b7nen", "Ti\u00b7sche", "pran\u00b7gen", "Kru\u00b7zi\u00b7fix", "und", "Ker\u00b7zen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sch\u00f6ff' und R\u00e4the, schwarzgekleidet, sitzen ernst dort zu Gericht;", "tokens": ["Sch\u00f6ff'", "und", "R\u00e4\u00b7the", ",", "schwarz\u00b7ge\u00b7klei\u00b7det", ",", "sit\u00b7zen", "ernst", "dort", "zu", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn sie luden vor die Schranken unsre Zeit, die Frevlerin,", "tokens": ["Denn", "sie", "lu\u00b7den", "vor", "die", "Schran\u00b7ken", "uns\u00b7re", "Zeit", ",", "die", "Frev\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Weil sie tr\u00fcb' und unheildrohend und von sturmbewegtem Sinn!", "tokens": ["Weil", "sie", "tr\u00fcb'", "und", "un\u00b7heil\u00b7dro\u00b7hend", "und", "von", "sturm\u00b7be\u00b7weg\u00b7tem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD", "KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Doch es kommt nicht die Gerufne, denn die Zeit, sie hat nicht Zeit,", "tokens": ["Doch", "es", "kommt", "nicht", "die", "Ge\u00b7ruf\u00b7ne", ",", "denn", "die", "Zeit", ",", "sie", "hat", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "KON", "ART", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kann nicht stille stehn im Saale weltlicher Gerechtigkeit,", "tokens": ["Kann", "nicht", "stil\u00b7le", "stehn", "im", "Saa\u00b7le", "welt\u00b7li\u00b7cher", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "VVFIN", "APPRART", "NN", "PWAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "W\u00e4hrend sie zwei Stunden harren, ist sie schon zwei Stunden fern!", "tokens": ["W\u00e4h\u00b7rend", "sie", "zwei", "Stun\u00b7den", "har\u00b7ren", ",", "ist", "sie", "schon", "zwei", "Stun\u00b7den", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVINF", "$,", "VAFIN", "PPER", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch sie sendet ihren Anwalt, also sprechend, zu den Herrn:", "tokens": ["Doch", "sie", "sen\u00b7det", "ih\u00b7ren", "An\u00b7walt", ",", "al\u00b7so", "spre\u00b7chend", ",", "zu", "den", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbl\u00e4stert nicht die Zeit, die reine! Schm\u00e4ht ihr sie, so schm\u00e4ht ihr euch!", "tokens": ["\u00bb", "l\u00e4s\u00b7tert", "nicht", "die", "Zeit", ",", "die", "rei\u00b7ne", "!", "Schm\u00e4ht", "ihr", "sie", ",", "so", "schm\u00e4ht", "ihr", "euch", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "$.", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Denn es ist die Zeit dem wei\u00dfen, unbeschriebnen Blatte gleich:", "tokens": ["Denn", "es", "ist", "die", "Zeit", "dem", "wei\u00b7\u00dfen", ",", "un\u00b7be\u00b7schrieb\u00b7nen", "Blat\u00b7te", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Das Papier ist ohne Makel, doch die Schrift darauf seid ihr!", "tokens": ["Das", "Pa\u00b7pier", "ist", "oh\u00b7ne", "Ma\u00b7kel", ",", "doch", "die", "Schrift", "da\u00b7rauf", "seid", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,", "ADV", "ART", "NN", "PAV", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wenn die Schrift nicht just erbaulich, nun, was kann das Blatt daf\u00fcr?", "tokens": ["Wenn", "die", "Schrift", "nicht", "just", "er\u00b7bau\u00b7lich", ",", "nun", ",", "was", "kann", "das", "Blatt", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "$,", "PWS", "VMFIN", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Ein Pokal durchsicht'gen Glases ist die Zeit: so hell, so rein!", "tokens": ["Ein", "Po\u00b7kal", "durch\u00b7sicht'\u00b7gen", "Gla\u00b7ses", "ist", "die", "Zeit", ":", "so", "hell", ",", "so", "rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ART", "NN", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wollt des s\u00fc\u00dfen Weins ihr schl\u00fcrfen, gie\u00dft nicht eure Hefen drein!", "tokens": ["Wollt", "des", "s\u00fc\u00b7\u00dfen", "Weins", "ihr", "schl\u00fcr\u00b7fen", ",", "gie\u00dft", "nicht", "eu\u00b7re", "He\u00b7fen", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und es ist die Zeit ein Wohnhaus, nahm ganz stattlich sonst sich aus,", "tokens": ["Und", "es", "ist", "die", "Zeit", "ein", "Wohn\u00b7haus", ",", "nahm", "ganz", "statt\u00b7lich", "sonst", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "ADJD", "ADV", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Freilich seit ihr eingezogen, scheint es oft ein Narrenhaus.", "tokens": ["Frei\u00b7lich", "seit", "ihr", "ein\u00b7ge\u00b7zo\u00b7gen", ",", "scheint", "es", "oft", "ein", "Nar\u00b7ren\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Seht, es ist die Zeit ein Saatfeld; \u2013 da ihr Disteln ausges\u00e4t,", "tokens": ["Seht", ",", "es", "ist", "die", "Zeit", "ein", "Saat\u00b7feld", ";", "\u2013", "da", "ihr", "Dis\u00b7teln", "aus\u00b7ge\u00b7s\u00e4t", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$.", "$(", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ei, wie k\u00f6nnt ihr drob euch wundern, da\u00df es nicht voll Rosen steht?", "tokens": ["Ei", ",", "wie", "k\u00f6nnt", "ihr", "drob", "euch", "wun\u00b7dern", ",", "da\u00df", "es", "nicht", "voll", "Ro\u00b7sen", "steht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "C\u00e4sar ficht auf solchem Felde Schlachten der Unsterblichkeit,", "tokens": ["C\u00e4\u00b7sar", "ficht", "auf", "sol\u00b7chem", "Fel\u00b7de", "Schlach\u00b7ten", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Doch auch Memmen, zum Entlaufen, ist es sattsam gro\u00df und weit.", "tokens": ["Doch", "auch", "Mem\u00b7men", ",", "zum", "Ent\u00b7lau\u00b7fen", ",", "ist", "es", "satt\u00b7sam", "gro\u00df", "und", "weit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "APPRART", "NN", "$,", "VAFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Zeit ist eine stumme Harfe; \u2013 pr\u00fcft ein St\u00fcmper ihre Kraft,", "tokens": ["Zeit", "ist", "ei\u00b7ne", "stum\u00b7me", "Har\u00b7fe", ";", "\u2013", "pr\u00fcft", "ein", "St\u00fcm\u00b7per", "ih\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "$(", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Heulen jammernd Hund und Kater in der ganzen Nachbarschaft! \u2013", "tokens": ["Heu\u00b7len", "jam\u00b7mernd", "Hund", "und", "Ka\u00b7ter", "in", "der", "gan\u00b7zen", "Nach\u00b7bar\u00b7schaft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nun wohlan, so greift begeistert, wie Amphion fest darein,", "tokens": ["Nun", "wo\u00b7hlan", ",", "so", "greift", "be\u00b7geis\u00b7tert", ",", "wie", "Am\u00b7phi\u00b7on", "fest", "da\u00b7rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "VVPP", "$,", "PWAV", "NN", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df auch Strom und Wald euch lausche, Leben fahre in den Stein!\u00ab", "tokens": ["Da\u00df", "auch", "Strom", "und", "Wald", "euch", "lau\u00b7sche", ",", "Le\u00b7ben", "fah\u00b7re", "in", "den", "Stein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Auf dem gr\u00fcnen Tische prangen Kruzifix und Kerzenlicht,", "tokens": ["Auf", "dem", "gr\u00fc\u00b7nen", "Ti\u00b7sche", "pran\u00b7gen", "Kru\u00b7zi\u00b7fix", "und", "Ker\u00b7zen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sch\u00f6ff' und R\u00e4the, schwarzgekleidet, sitzen ernst dort zu Gericht;", "tokens": ["Sch\u00f6ff'", "und", "R\u00e4\u00b7the", ",", "schwarz\u00b7ge\u00b7klei\u00b7det", ",", "sit\u00b7zen", "ernst", "dort", "zu", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "ADV", "APPR