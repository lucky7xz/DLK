{"dta.poem.10892": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "R\u00e4tzel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat jemand wo die Welt gesehn, mit einiger Aufmerksamkeit,", "tokens": ["Hat", "je\u00b7mand", "wo", "die", "Welt", "ge\u00b7sehn", ",", "mit", "ei\u00b7ni\u00b7ger", "Auf\u00b7merk\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PWAV", "ART", "NN", "VVPP", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und ist vor andern weit gereiset,", "tokens": ["Und", "ist", "vor", "an\u00b7dern", "weit", "ge\u00b7rei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der nenne mir, nebst Ort und Art und Volk, absonderlich die", "tokens": ["Der", "nen\u00b7ne", "mir", ",", "nebst", "Ort", "und", "Art", "und", "Volk", ",", "ab\u00b7son\u00b7der\u00b7lich", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$,", "ADJD", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "In welcher jeder seine Mutter, mit ihrer Kinder F\u00fc\u00dfen,", "tokens": ["In", "wel\u00b7cher", "je\u00b7der", "sei\u00b7ne", "Mut\u00b7ter", ",", "mit", "ih\u00b7rer", "Kin\u00b7der", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}}}}