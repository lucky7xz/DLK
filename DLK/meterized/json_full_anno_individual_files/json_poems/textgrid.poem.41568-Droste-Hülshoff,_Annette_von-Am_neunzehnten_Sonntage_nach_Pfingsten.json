{"textgrid.poem.41568": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am neunzehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich dich liebe, Gott, es ist", "tokens": ["Ob", "ich", "dich", "lie\u00b7be", ",", "Gott", ",", "es", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir unbewu\u00dft.", "tokens": ["Mir", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Oft mein' ich, da\u00df nur du es bist,", "tokens": ["Oft", "mein'", "ich", ",", "da\u00df", "nur", "du", "es", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was diese Brust", "tokens": ["Was", "die\u00b7se", "Brust"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In aller andrer Liebe Schein", "tokens": ["In", "al\u00b7ler", "an\u00b7drer", "Lie\u00b7be", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und d\u00e4mmerndem Verlangen", "tokens": ["Und", "d\u00e4m\u00b7mern\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie eine S\u00fchnungsfackel rein", "tokens": ["Wie", "ei\u00b7ne", "S\u00fch\u00b7nungs\u00b7fa\u00b7ckel", "rein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00e4lt gnadenvoll umfangen.", "tokens": ["H\u00e4lt", "gna\u00b7den\u00b7voll", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn zu dem Edelsten der Geist", "tokens": ["Wenn", "zu", "dem", "E\u00b7dels\u00b7ten", "der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sich frei erhebt,", "tokens": ["Sich", "frei", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was als Gedanke ihn umkreist", "tokens": ["Was", "als", "Ge\u00b7dan\u00b7ke", "ihn", "um\u00b7kreist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "KOUS", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch lebt,", "tokens": ["Und", "den\u00b7noch", "lebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Unsichtbar, wesenlos doch nicht,", "tokens": ["Un\u00b7sicht\u00b7bar", ",", "we\u00b7sen\u00b7los", "doch", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fern, dennoch allerwegen,", "tokens": ["Fern", ",", "den\u00b7noch", "al\u00b7ler\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wes Spur aus Menschenauge spricht", "tokens": ["Wes", "Spur", "aus", "Men\u00b7schen\u00b7au\u00b7ge", "spricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und aus der Tr\u00e4ne Segen:", "tokens": ["Und", "aus", "der", "Tr\u00e4\u00b7ne", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann bin ich wohlgetr\u00f6stet und", "tokens": ["Dann", "bin", "ich", "wohl\u00b7ge\u00b7tr\u00f6s\u00b7tet", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebet entsteigt", "tokens": ["Ge\u00b7bet", "ent\u00b7steigt"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So zuversichtlich meinem Mund,", "tokens": ["So", "zu\u00b7ver\u00b7sicht\u00b7lich", "mei\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als sei gereicht", "tokens": ["Als", "sei", "ge\u00b7reicht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VAFIN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In fremder oder deiner Lieb'", "tokens": ["In", "frem\u00b7der", "o\u00b7der", "dei\u00b7ner", "Lieb'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u2013 Wer hat es je ergr\u00fcndet? \u2013", "tokens": ["\u2013", "Wer", "hat", "es", "je", "er\u00b7gr\u00fcn\u00b7det", "?", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "All was dem echten Sehnen lieb,", "tokens": ["All", "was", "dem", "ech\u00b7ten", "Seh\u00b7nen", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PWS", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und deinen Odem k\u00fcndet.", "tokens": ["Und", "dei\u00b7nen", "O\u00b7dem", "k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch f\u00fchl' ich dann zu andrer Zeit", "tokens": ["Doch", "f\u00fchl'", "ich", "dann", "zu", "an\u00b7drer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Haar dem Haupt", "tokens": ["Wie", "Haar", "dem", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der finstren Erde mich geweiht,", "tokens": ["Der", "finst\u00b7ren", "Er\u00b7de", "mich", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So machtberaubt;", "tokens": ["So", "macht\u00b7be\u00b7raubt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wenn in dem Freunde mich entz\u00fcckt", "tokens": ["Wenn", "in", "dem", "Freun\u00b7de", "mich", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst wie ein Reiz das Fehlen,", "tokens": ["Selbst", "wie", "ein", "Reiz", "das", "Feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Schw\u00e4chen, an mein Herz gedr\u00fcckt,", "tokens": ["Die", "Schw\u00e4\u00b7chen", ",", "an", "mein", "Herz", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir keiner d\u00fcrfte stehlen:", "tokens": ["Mir", "kei\u00b7ner", "d\u00fcrf\u00b7te", "steh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da w\u00e4r' es Gottes Zeichen nur", "tokens": ["Da", "w\u00e4r'", "es", "Got\u00b7tes", "Zei\u00b7chen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich erkannt,", "tokens": ["Was", "ich", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und nicht die s\u00fcndige Natur", "tokens": ["Und", "nicht", "die", "s\u00fcn\u00b7di\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "B\u00f6t' ihre Hand,", "tokens": ["B\u00f6t'", "ih\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wenn der Geliebten Tugend ich", "tokens": ["Wenn", "der", "Ge\u00b7lieb\u00b7ten", "Tu\u00b7gend", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Ehrfurcht mag ertragen,", "tokens": ["In", "Ehr\u00b7furcht", "mag", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch fleckenloser sicherlich", "tokens": ["Doch", "fle\u00b7cken\u00b7lo\u00b7ser", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz w\u00fcrd' k\u00e4lter schlagen?", "tokens": ["Mein", "Herz", "w\u00fcrd'", "k\u00e4l\u00b7ter", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weh! eine kalte Wolke f\u00e4hrt", "tokens": ["Weh", "!", "ei\u00b7ne", "kal\u00b7te", "Wol\u00b7ke", "f\u00e4hrt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es \u00fcber mich,", "tokens": ["Es", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie dem Damokles unterm Schwert", "tokens": ["Wie", "dem", "Da\u00b7mok\u00b7les", "un\u00b7term", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Wange blich,", "tokens": ["Die", "Wan\u00b7ge", "blich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wie einem, der an Ufers Rand", "tokens": ["Wie", "ei\u00b7nem", ",", "der", "an", "U\u00b7fers", "Rand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "$,", "PRELS", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich spiegelt, l\u00e4chelt, trinket,", "tokens": ["Sich", "spie\u00b7gelt", ",", "l\u00e4\u00b7chelt", ",", "trin\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn sacht entschl\u00fcpft der falsche Sand", "tokens": ["Wenn", "sacht", "ent\u00b7schl\u00fcpft", "der", "fal\u00b7sche", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und seine St\u00e4tte sinket.", "tokens": ["Und", "sei\u00b7ne", "St\u00e4t\u00b7te", "sin\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Retter, Retter, der auch f\u00fcr", "tokens": ["O", "Ret\u00b7ter", ",", "Ret\u00b7ter", ",", "der", "auch", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "PRELS", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Toren litt,", "tokens": ["Die", "To\u00b7ren", "litt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erscheine, eh die Welle mir", "tokens": ["Er\u00b7schei\u00b7ne", ",", "eh", "die", "Wel\u00b7le", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Haupte glitt!", "tokens": ["Zum", "Haup\u00b7te", "glitt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Greif aus mit deiner starken Hand,", "tokens": ["Greif", "aus", "mit", "dei\u00b7ner", "star\u00b7ken", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch k\u00e4mpf' ich gen die Wogen;", "tokens": ["Noch", "k\u00e4mpf'", "ich", "gen", "die", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So manchen hast du ja ans Land", "tokens": ["So", "man\u00b7chen", "hast", "du", "ja", "ans", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus tiefem Schlamm gezogen!", "tokens": ["Aus", "tie\u00b7fem", "Schlamm", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hab' ich dem Schlamme mich entwirrt,", "tokens": ["Hab'", "ich", "dem", "Schlam\u00b7me", "mich", "ent\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ganz und recht,", "tokens": ["So", "ganz", "und", "recht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dann erst zu deinem Bildnis wird", "tokens": ["Dann", "erst", "zu", "dei\u00b7nem", "Bild\u00b7nis", "wird"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sehnsucht echt;", "tokens": ["Die", "Sehn\u00b7sucht", "echt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dann darf ich lieben stark, gesund,", "tokens": ["Dann", "darf", "ich", "lie\u00b7ben", "stark", ",", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn' alle Schmach und Hehle", "tokens": ["Ohn'", "al\u00b7le", "Schmach", "und", "Heh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus meines ganzen Herzens Grund", "tokens": ["Aus", "mei\u00b7nes", "gan\u00b7zen", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und meiner ganzen Seele.", "tokens": ["Und", "mei\u00b7ner", "gan\u00b7zen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ob ich dich liebe, Gott, es ist", "tokens": ["Ob", "ich", "dich", "lie\u00b7be", ",", "Gott", ",", "es", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir unbewu\u00dft.", "tokens": ["Mir", "un\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Oft mein' ich, da\u00df nur du es bist,", "tokens": ["Oft", "mein'", "ich", ",", "da\u00df", "nur", "du", "es", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was diese Brust", "tokens": ["Was", "die\u00b7se", "Brust"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In aller andrer Liebe Schein", "tokens": ["In", "al\u00b7ler", "an\u00b7drer", "Lie\u00b7be", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und d\u00e4mmerndem Verlangen", "tokens": ["Und", "d\u00e4m\u00b7mern\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie eine S\u00fchnungsfackel rein", "tokens": ["Wie", "ei\u00b7ne", "S\u00fch\u00b7nungs\u00b7fa\u00b7ckel", "rein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00e4lt gnadenvoll umfangen.", "tokens": ["H\u00e4lt", "gna\u00b7den\u00b7voll", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn zu dem Edelsten der Geist", "tokens": ["Wenn", "zu", "dem", "E\u00b7dels\u00b7ten", "der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Sich frei erhebt,", "tokens": ["Sich", "frei", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was als Gedanke ihn umkreist", "tokens": ["Was", "als", "Ge\u00b7dan\u00b7ke", "ihn", "um\u00b7kreist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "KOUS", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch lebt,", "tokens": ["Und", "den\u00b7noch", "lebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Unsichtbar, wesenlos doch nicht,", "tokens": ["Un\u00b7sicht\u00b7bar", ",", "we\u00b7sen\u00b7los", "doch", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fern, dennoch allerwegen,", "tokens": ["Fern", ",", "den\u00b7noch", "al\u00b7ler\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "lin