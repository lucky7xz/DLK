{"textgrid.poem.52347": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "65. Dei Werr", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer kennt nich Fuhrmann Matzen? hei 's bekannt", "tokens": ["Wer", "kennt", "nich", "Fuhr\u00b7mann", "Mat\u00b7zen", "?", "hei", "'s", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "NN", "NN", "$.", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "D\u00f6rch't ganze meckelb\u00f6rgsche Land.", "tokens": ["D\u00f6rch't", "gan\u00b7ze", "me\u00b7ckel\u00b7b\u00f6rg\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un wer kennt nich den Herrn von Schmitten,", "tokens": ["Un", "wer", "kennt", "nich", "den", "Herrn", "von", "Schmit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dei ok bekannt is as en Witten?", "tokens": ["Dei", "ok", "be\u00b7kannt", "is", "as", "en", "Wit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Na, von dei Beiden will ick Jug vetellen", "tokens": ["Na", ",", "von", "dei", "Bei\u00b7den", "will", "ick", "Jug", "ve\u00b7tel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein St\u00fcck, dat sall Jug woll gefallen: \u2013", "tokens": ["Ein", "St\u00fcck", ",", "dat", "sall", "Jug", "woll", "ge\u00b7fal\u00b7len", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dei Herr von Schmitt, dei h\u00fcrt nich tau dei Hellen.", "tokens": ["Dei", "Herr", "von", "Schmitt", ",", "dei", "h\u00fcrt", "nich", "tau", "dei", "Hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Un wenn em Eine von Jug allen", "tokens": ["Un", "wenn", "em", "Ei\u00b7ne", "von", "Jug", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PIS", "ART", "APPR", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hett kennt, dei seggt gewi\u00df,", "tokens": ["Hett", "kennt", ",", "dei", "seggt", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Dat hei sogoa man'n b\u00e4ten d\u00e4mlich is. \u2013", "tokens": ["Dat", "hei", "so\u00b7goa", "man'n", "b\u00e4\u00b7ten", "d\u00e4m\u00b7lich", "is", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Na, Herr von Schmitt, dei f\u00fchrt mal in Gesch\u00e4ften", "tokens": ["Na", ",", "Herr", "von", "Schmitt", ",", "dei", "f\u00fchrt", "mal", "in", "Ge\u00b7sch\u00e4f\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "APPR", "NN", "$,", "ADV", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit Matzen ut, un deese l\u00f6\u00f6g nah Kr\u00e4ften", "tokens": ["Mit", "Mat\u00b7zen", "ut", ",", "un", "dee\u00b7se", "l\u00f6\u00f6g", "nah", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "FM", "FM", "FM", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Un dehr em so v\u00e4hl T\u00fcggs upbinnen,", "tokens": ["Un", "dehr", "em", "so", "v\u00e4hl", "T\u00fcggs", "up\u00b7bin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dat kein vier Pier dat trecken k\u00fcnnen.", "tokens": ["Dat", "kein", "vier", "Pier", "dat", "tre\u00b7cken", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "CARD", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Bi ditt Vetellen kehmen sei am En'n", "tokens": ["Bi", "ditt", "Ve\u00b7tel\u00b7len", "keh\u00b7men", "sei", "am", "En'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "VVINF", "VAFIN", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "In ein l\u00fctt n\u00fcdlich D\u00f6rp herin,", "tokens": ["In", "ein", "l\u00fctt", "n\u00fcd\u00b7lich", "D\u00f6rp", "he\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJD", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Un as sei um dei Eck 'r\u00fcm wen'n,", "tokens": ["Un", "as", "sei", "um", "dei", "Eck", "'r\u00fcm", "wen'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Rechtsch \u00fcm en Gohrenhuus, doa st\u00fcnn", "tokens": ["Rechtsch", "\u00fcm", "en", "Goh\u00b7ren\u00b7hu\u00b7us", ",", "doa", "st\u00fcnn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "'ne grote Tafel upgericht't,", "tokens": ["'ne", "gro\u00b7te", "Ta\u00b7fel", "up\u00b7ge\u00b7richt't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dorup st\u00fcnn schr\u00e4ben ein Gedicht.", "tokens": ["Do\u00b7rup", "st\u00fcnn", "schr\u00e4\u00b7ben", "ein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbholt doch mal still! wat's dit?\u00ab", "tokens": ["\u00bb", "holt", "doch", "mal", "still", "!", "wat's", "dit", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "ADV", "PTKVZ", "$.", "NE", "NE", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.22": {"text": "R\u00f6pt Matzen tau dei Herr von Schmitt,", "tokens": ["R\u00f6pt", "Mat\u00b7zen", "tau", "dei", "Herr", "von", "Schmitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bbick m\u00f6t doch seihn, wat dit deiht wesen,", "tokens": ["\u00bb", "ick", "m\u00f6t", "doch", "seihn", ",", "wat", "dit", "deiht", "we\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVINF", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Holt doch mal still. Ick will't mal lesen.\u00ab", "tokens": ["Holt", "doch", "mal", "still", ".", "Ick", "will't", "mal", "le\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Un doamit stiggt denn af uns' Eddelmann,", "tokens": ["Un", "do\u00b7a\u00b7mit", "stiggt", "denn", "af", "un\u00b7s'", "Ed\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KON", "NE", "NE", "NE", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Un deiht en b\u00e4ten neege 'ran spazieren,", "tokens": ["Un", "deiht", "en", "b\u00e4\u00b7ten", "ne\u00b7e\u00b7ge", "'ran", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVINF", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.27": {"text": "Un f\u00e4ngt nu an tau baukstafieren,", "tokens": ["Un", "f\u00e4ngt", "nu", "an", "tau", "bauk\u00b7sta\u00b7fie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Un steiht un steiht, un kickt dei Tafel an,", "tokens": ["Un", "steiht", "un", "steiht", ",", "un", "kickt", "dei", "Ta\u00b7fel", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "VVFIN", "$,", "FM", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "As unse Kauh dat niege Dua:", "tokens": ["As", "un\u00b7se", "Kauh", "dat", "nie\u00b7ge", "Dua", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "\u00bboh Wandre! acht' Kunst un Natua.\u00ab", "tokens": ["\u00bb", "oh", "Wand\u00b7re", "!", "acht'", "Kunst", "un", "Na\u00b7tua", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "$.", "CARD", "NN", "FM", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Hei sch\u00fcttelt mit 'n Kopp, \u00bbHm\u00ab, seggt hei endlich,", "tokens": ["Hei", "sch\u00fct\u00b7telt", "mit", "'n", "Kopp", ",", "\u00bb", "Hm", "\u00ab", ",", "seggt", "hei", "end\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,", "$(", "XY", "$(", "$,", "VVFIN", "NE", "ADV", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "\u00bbdei Spruch is doch siehr unvest\u00e4ndlich.\u00ab \u2013", "tokens": ["\u00bb", "dei", "Spruch", "is", "doch", "siehr", "un\u00b7ves\u00b7t\u00e4nd\u00b7lich", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "FM", "ADV", "ADV", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbja gneedge Herr\u00ab, seggt Fuhrmann Matz,", "tokens": ["\u00bb", "ja", "gneed\u00b7ge", "Herr", "\u00ab", ",", "seggt", "Fuhr\u00b7mann", "Matz", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "$(", "$,", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Un sett't sich 'rup nah sienen Kutscheplatz,", "tokens": ["Un", "sett't", "sich", "'rup", "nah", "sie\u00b7nen", "Kut\u00b7sche\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "\u00bbv\u00f6r mienen Oogen is't Latien;", "tokens": ["\u00bb", "v\u00f6r", "mie\u00b7nen", "O\u00b7o\u00b7gen", "is't", "La\u00b7ti\u00b7en", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Dat weit ick woll, den Spruch vesteiht kein Schwien!\u00ab", "tokens": ["Dat", "weit", "ick", "woll", ",", "den", "Spruch", "ves\u00b7teiht", "kein", "Schwi\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "$,", "ART", "NN", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Doch Herr von Schmitt, dei beert in einem fuat", "tokens": ["Doch", "Herr", "von", "Schmitt", ",", "dei", "beert", "in", "ei\u00b7nem", "fuat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "\u00bboh Wandre! acht' Kunst un Natua;", "tokens": ["\u00bb", "oh", "Wand\u00b7re", "!", "acht'", "Kunst", "un", "Na\u00b7tua", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$.", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Oh Wandre! acht Kunst un Natua.\u00ab", "tokens": ["Oh", "Wand\u00b7re", "!", "acht", "Kunst", "un", "Na\u00b7tua", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "VVFIN", "$.", "CARD", "NN", "FM", "NE", "$.", "$("], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.40": {"text": "\u00bbih laaten S' doch dat Beeren, gneedge Herr\u00ab,", "tokens": ["\u00bb", "ih", "laa\u00b7ten", "S'", "doch", "dat", "Bee\u00b7ren", ",", "gneed\u00b7ge", "Herr", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ADJA", "NN", "$(", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Seggt Matz tau em, \u00bbwat gelt dei Werr?", "tokens": ["Seggt", "Matz", "tau", "em", ",", "\u00bb", "wat", "gelt", "dei", "Werr", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "$(", "PWS", "ADJD", "PPOSAT", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.42": {"text": "Sei k\u00e4nen bet tau't neegste D\u00f6rp den ollen", "tokens": ["Sei", "k\u00e4\u00b7nen", "bet", "tau't", "neegs\u00b7te", "D\u00f6rp", "den", "ol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVINF", "VVFIN", "ADJD", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "D\u00e4mlichen Spruch nich in den Sinn behollen;", "tokens": ["D\u00e4m\u00b7li\u00b7chen", "Spruch", "nich", "in", "den", "Sinn", "be\u00b7hol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.44": {"text": "Ick sett drei Dahle gegen ehren", "tokens": ["Ick", "sett", "drei", "Dah\u00b7le", "ge\u00b7gen", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "CARD", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Meersch\u00fcmen'n Piepenkopp;", "tokens": ["Meer\u00b7sch\u00fc\u00b7men'n", "Pie\u00b7pen\u00b7kopp", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.46": {"text": "Sei seihn mi goa nich ut, as ob", "tokens": ["Sei", "seihn", "mi", "goa", "nich", "ut", ",", "as", "ob"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Sei siehr beh\u00f6llen'n von Gedanken wieren.\u00ab", "tokens": ["Sei", "siehr", "be\u00b7h\u00f6l\u00b7len'n", "von", "Ge\u00b7dan\u00b7ken", "wie\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-