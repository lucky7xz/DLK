{"dta.poem.21164": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Clavier .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Echo meiner Klagen,", "tokens": ["Du", "E\u00b7cho", "mei\u00b7ner", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein treues Saitenspiel,", "tokens": ["Mein", "treu\u00b7es", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun k\u00f6mmt nach tr\u00fcben Tagen", "tokens": ["Nun", "k\u00f6mmt", "nach", "tr\u00fc\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Nacht, der Sorgen Ziel.", "tokens": ["Die", "Nacht", ",", "der", "Sor\u00b7gen", "Ziel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gehorcht mir, sanfte Saiten,", "tokens": ["Ge\u00b7horcht", "mir", ",", "sanf\u00b7te", "Sai\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und helft mein Leid bestreiten \u2014", "tokens": ["Und", "helft", "mein", "Leid", "be\u00b7strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch nein, la\u00dft mir mein Leid,", "tokens": ["Doch", "nein", ",", "la\u00dft", "mir", "mein", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und meine Z\u00e4rtlichkeit.", "tokens": ["Und", "mei\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich untr\u00f6stbar scheine,", "tokens": ["Wenn", "ich", "un\u00b7tr\u00f6st\u00b7bar", "schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lieb ich doch meinen Schmerz;", "tokens": ["Lieb", "ich", "doch", "mei\u00b7nen", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich einsam weine,", "tokens": ["Und", "wenn", "ich", "ein\u00b7sam", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weint doch ein liebend Herz.", "tokens": ["Weint", "doch", "ein", "lie\u00b7bend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Zeit nur ist verlohren,", "tokens": ["Die", "Zeit", "nur", "ist", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ich mit goldnen Thoren,", "tokens": ["Die", "ich", "mit", "gold\u00b7nen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bey Spiel und Wein und Pracht,", "tokens": ["Bey", "Spiel", "und", "Wein", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So f\u00fchllos durchgelacht.", "tokens": ["So", "f\u00fchl\u00b7los", "durch\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jhr, holde Saiten, klinget", "tokens": ["Ihr", ",", "hol\u00b7de", "Sai\u00b7ten", ",", "klin\u00b7get"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In sanfter Harmonie!", "tokens": ["In", "sanf\u00b7ter", "Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flieht, was die Oper singet,", "tokens": ["Flieht", ",", "was", "die", "O\u00b7per", "sin\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und folgt der Phantasie.", "tokens": ["Und", "folgt", "der", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seyd sanft, wie meine Liebe,", "tokens": ["Seyd", "sanft", ",", "wie", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Besinget ihre Triebe,", "tokens": ["Be\u00b7sin\u00b7get", "ih\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und zeigt durch eure Macht,", "tokens": ["Und", "zeigt", "durch", "eu\u00b7re", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df sie euch siegend macht.", "tokens": ["Da\u00df", "sie", "euch", "sie\u00b7gend", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}