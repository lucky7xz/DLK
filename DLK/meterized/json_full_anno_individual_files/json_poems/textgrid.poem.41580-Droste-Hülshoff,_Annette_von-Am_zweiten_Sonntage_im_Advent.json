{"textgrid.poem.41580": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zweiten Sonntage im Advent", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bleibst du, Wolke, die den Menschensohn", "tokens": ["Wo", "bleibst", "du", ",", "Wol\u00b7ke", ",", "die", "den", "Men\u00b7schen\u00b7sohn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll tragen?", "tokens": ["Soll", "tra\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Seh ich das Morgenrot im Osten schon", "tokens": ["Seh", "ich", "das", "Mor\u00b7gen\u00b7rot", "im", "Os\u00b7ten", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht leise ragen?", "tokens": ["Nicht", "lei\u00b7se", "ra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Dunkel steigen, die Zeit rollt matt und gleich.", "tokens": ["Die", "Dun\u00b7kel", "stei\u00b7gen", ",", "die", "Zeit", "rollt", "matt", "und", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ich seh es flimmern, aber bleich ach, bleich!", "tokens": ["Ich", "seh", "es", "flim\u00b7mern", ",", "a\u00b7ber", "bleich", "ach", ",", "bleich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "ADV", "ADJD", "ITJ", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein eignes Sinnen ist es was da quillt", "tokens": ["Mein", "eig\u00b7nes", "Sin\u00b7nen", "ist", "es", "was", "da", "quillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PWS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entz\u00fcndet,", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie aus dem Teiche gr\u00fcn und schlammerf\u00fcllt", "tokens": ["Wie", "aus", "dem", "Tei\u00b7che", "gr\u00fcn", "und", "schlam\u00b7mer\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich wohl entbindet", "tokens": ["Sich", "wohl", "ent\u00b7bin\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ein Fl\u00e4mmchen und vom Schilfgest\u00f6hn umwankt", "tokens": ["Ein", "Fl\u00e4mm\u00b7chen", "und", "vom", "Schilf\u00b7ge\u00b7st\u00f6hn", "um\u00b7wankt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Unsicher in dem grauen Dunste schwankt.", "tokens": ["Un\u00b7si\u00b7cher", "in", "dem", "grau\u00b7en", "Duns\u00b7te", "schwankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "So mu\u00df die allerk\u00fchnste Phantasie", "tokens": ["So", "mu\u00df", "die", "al\u00b7ler\u00b7k\u00fchns\u00b7te", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ermatten;", "tokens": ["Er\u00b7mat\u00b7ten", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So in der Mondesscheibe sah ich nie", "tokens": ["So", "in", "der", "Mon\u00b7des\u00b7schei\u00b7be", "sah", "ich", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Berges Schatten", "tokens": ["Des", "Ber\u00b7ges", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Gewi\u00df, ob ein Kolo\u00df die Formen zog,", "tokens": ["Ge\u00b7wi\u00df", ",", "ob", "ein", "Ko\u00b7lo\u00df", "die", "For\u00b7men", "zog", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ob eine Tr\u00e4ne mich im Auge trog.", "tokens": ["Ob", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "mich", "im", "Au\u00b7ge", "trog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So ragt und w\u00e4lzt sich in der Zukunft Reich", "tokens": ["So", "ragt", "und", "w\u00e4lzt", "sich", "in", "der", "Zu\u00b7kunft", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Schemen.", "tokens": ["Ein", "Sche\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mein Sinnen sonder Kraft, Gedanke bleich \u2013", "tokens": ["Mein", "Sin\u00b7nen", "son\u00b7der", "Kraft", ",", "Ge\u00b7dan\u00b7ke", "bleich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer will mir nehmen", "tokens": ["Wer", "will", "mir", "neh\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Das Hoffen, was ich in des Herzens Schrein", "tokens": ["Das", "Hof\u00b7fen", ",", "was", "ich", "in", "des", "Her\u00b7zens", "Schrein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gehegt als meiner Armut Edelstein?", "tokens": ["Ge\u00b7hegt", "als", "mei\u00b7ner", "Ar\u00b7mut", "E\u00b7del\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gib dich gefangen, t\u00f6richter Verstand!", "tokens": ["Gib", "dich", "ge\u00b7fan\u00b7gen", ",", "t\u00f6\u00b7rich\u00b7ter", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Steig nieder", "tokens": ["Steig", "nie\u00b7der"], "token_info": ["word", "word"], "pos": ["NN", "PTKVZ"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und z\u00fcnde an des Glaubens reinem Brand", "tokens": ["Und", "z\u00fcn\u00b7de", "an", "des", "Glau\u00b7bens", "rei\u00b7nem", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein D\u00f6chtlein wieder!", "tokens": ["Dein", "D\u00f6cht\u00b7lein", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die arme Lampe, deren matter Hauch", "tokens": ["Die", "ar\u00b7me", "Lam\u00b7pe", ",", "de\u00b7ren", "mat\u00b7ter", "Hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verdumpft, erstickt in eignen Qualmes Rauch.", "tokens": ["Ver\u00b7dumpft", ",", "er\u00b7stickt", "in", "eig\u00b7nen", "Qual\u00b7mes", "Rauch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du seltsam r\u00e4tselhaft Gesch\u00f6pf aus Ton", "tokens": ["Du", "selt\u00b7sam", "r\u00e4t\u00b7sel\u00b7haft", "Ge\u00b7sch\u00f6pf", "aus", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Kr\u00e4ften,", "tokens": ["Mit", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die leben, w\u00fchlen, zischen wie zum Hohn", "tokens": ["Die", "le\u00b7ben", ",", "w\u00fch\u00b7len", ",", "zi\u00b7schen", "wie", "zum", "Hohn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In allen S\u00e4ften,", "tokens": ["In", "al\u00b7len", "S\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "O bade deinen w\u00fcsten Fiebertraum", "tokens": ["O", "ba\u00b7de", "dei\u00b7nen", "w\u00fcs\u00b7ten", "Fie\u00b7ber\u00b7traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im einz'gen Quell, der ohne Schlamm und Schaum!", "tokens": ["Im", "einz'\u00b7gen", "Quell", ",", "der", "oh\u00b7ne", "Schlamm", "und", "Schaum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wehr ab, sto\u00df fort, was gleich dem frechen Feind", "tokens": ["Wehr", "ab", ",", "sto\u00df", "fort", ",", "was", "gleich", "dem", "fre\u00b7chen", "Feind"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir sendet", "tokens": ["Dir", "sen\u00b7det"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Die Macht, so wetterleuchtet und verneint;", "tokens": ["Die", "Macht", ",", "so", "wet\u00b7ter\u00b7leuch\u00b7tet", "und", "ver\u00b7neint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und starr gewendet", "tokens": ["Und", "starr", "ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie zum Polarstern halt das eine fest,", "tokens": ["Wie", "zum", "Po\u00b7lars\u00b7tern", "halt", "das", "ei\u00b7ne", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVFIN", "ART", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sein Wort, sein heilig Wort \u2013 und Schach dem Rest!", "tokens": ["Sein", "Wort", ",", "sein", "hei\u00b7lig", "Wort", "\u2013", "und", "Schach", "dem", "Rest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "$(", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dann wirst du auf der Wolke deinen Herrn", "tokens": ["Dann", "wirst", "du", "auf", "der", "Wol\u00b7ke", "dei\u00b7nen", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erkennen,", "tokens": ["Er\u00b7ken\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dann sind Jahrtausende nicht kalt und fern,", "tokens": ["Dann", "sind", "Jahr\u00b7tau\u00b7sen\u00b7de", "nicht", "kalt", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zitternd nennen", "tokens": ["Und", "zit\u00b7ternd", "nen\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Darfst du der Worte Wort, des Lebens Mark,", "tokens": ["Darfst", "du", "der", "Wor\u00b7te", "Wort", ",", "des", "Le\u00b7bens", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn dem Geheimnis deine Seele stark.", "tokens": ["Wenn", "dem", "Ge\u00b7heim\u00b7nis", "dei\u00b7ne", "See\u00b7le", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und heute schon, es steht in Gottes Hand,", "tokens": ["Und", "heu\u00b7te", "schon", ",", "es", "steht", "in", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erschauen", "tokens": ["Er\u00b7schau\u00b7en"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Magst du den Heiland in der Seele Brand,", "tokens": ["Magst", "du", "den", "Hei\u00b7land", "in", "der", "See\u00b7le", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gl\u00fchndem Vertrauen.", "tokens": ["Gl\u00fchn\u00b7dem", "Ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zerfallen m\u00f6gen Erd und Himmels H\u00f6hn,", "tokens": ["Zer\u00b7fal\u00b7len", "m\u00f6\u00b7gen", "Erd", "und", "Him\u00b7mels", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch seine Worte werden nicht vergehn.", "tokens": ["Doch", "sei\u00b7ne", "Wor\u00b7te", "wer\u00b7den", "nicht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wo bleibst du, Wolke, die den Menschensohn", "tokens": ["Wo", "bleibst", "du", ",", "Wol\u00b7ke", ",", "die", "den", "Men\u00b7schen\u00b7sohn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll tragen?", "tokens": ["Soll", "tra\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Seh ich das Morgenrot im Osten schon", "tokens": ["Seh", "ich", "das", "Mor