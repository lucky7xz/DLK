{"textgrid.poem.24095": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Pulchra ut sol, clara ut lux", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Kirche sah ich goldne Statuen", "tokens": ["In", "ei\u00b7ner", "Kir\u00b7che", "sah", "ich", "gold\u00b7ne", "Sta\u00b7tu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Engeln, die auf ihrer Schultern Macht und Pracht", "tokens": ["Von", "En\u00b7geln", ",", "die", "auf", "ih\u00b7rer", "Schul\u00b7tern", "Macht", "und", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Chorgew\u00f6lbe trugen. Wie aus Griechenland,", "tokens": ["Das", "Chor\u00b7ge\u00b7w\u00f6l\u00b7be", "tru\u00b7gen", ".", "Wie", "aus", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit klarem Antlitz, rosenkranzgeschm\u00fcckt,", "tokens": ["Mit", "kla\u00b7rem", "Ant\u00b7litz", ",", "ro\u00b7sen\u00b7kranz\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Goldlockig, edel standen sie und l\u00e4chelten.", "tokens": ["Gold\u00b7lo\u00b7ckig", ",", "e\u00b7del", "stan\u00b