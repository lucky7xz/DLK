{"dta.poem.18642": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A uf die  G egenwart drey ho-  \n her Bedienten/ so Sterne hiessen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00f6chst begl\u00fcckter Ort/ du sch\u00f6nes Lust revier!", "tokens": ["Du", "h\u00f6chst", "be\u00b7gl\u00fcck\u00b7ter", "Ort", "/", "du", "sch\u00f6\u00b7nes", "Lust", "re\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dreyer Sternen Gold in deinen Creysse strahlet.", "tokens": ["Da", "drey\u00b7er", "Ster\u00b7nen", "Gold", "in", "dei\u00b7nen", "Creys\u00b7se", "strah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schau wie der Himmel dich mit seiner eignen Zier", "tokens": ["Schau", "wie", "der", "Him\u00b7mel", "dich", "mit", "sei\u00b7ner", "eig\u00b7nen", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "So unverhofft erfreut/ und deine Gegend mahlet.", "tokens": ["So", "un\u00b7ver\u00b7hofft", "er\u00b7freut", "/", "und", "dei\u00b7ne", "Ge\u00b7gend", "mah\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja siehe h\u00f6chst vergn\u00fcgt/ da\u00df deine rare Pracht", "tokens": ["Ja", "sie\u00b7he", "h\u00f6chst", "ver\u00b7gn\u00fcgt", "/", "da\u00df", "dei\u00b7ne", "ra\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "VVIMP", "ADV", "VVPP", "$(", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den andern Sternen selbst den gr\u00f6sten Wett-Streit macht.", "tokens": ["Den", "an\u00b7dern", "Ster\u00b7nen", "selbst", "den", "gr\u00f6s\u00b7ten", "Wet\u00b7tStreit", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sterne leuchten nur bey schwartzer Nacht empor/", "tokens": ["Die", "Ster\u00b7ne", "leuch\u00b7ten", "nur", "bey", "schwart\u00b7zer", "Nacht", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedem ein h\u00f6her Licht den frohen Tag begl\u00fccket.", "tokens": ["Je\u00b7dem", "ein", "h\u00f6\u00b7her", "Licht", "den", "fro\u00b7hen", "Tag", "be\u00b7gl\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch dieser Treflichkeit steigt \u00fcberall empor/", "tokens": ["Doch", "die\u00b7ser", "Tref\u00b7lich\u00b7keit", "steigt", "\u00fc\u00b7be\u00b7rall", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem sich Ph\u00f6bus selbst mit ihren Strahlen schm\u00fccket.", "tokens": ["In\u00b7dem", "sich", "Ph\u00f6\u00b7bus", "selbst", "mit", "ih\u00b7ren", "Strah\u00b7len", "schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Tugend/ welche nie bey dunckler Nacht vergeht/", "tokens": ["Die", "Tu\u00b7gend", "/", "wel\u00b7che", "nie", "bey", "dunck\u00b7ler", "Nacht", "ver\u00b7geht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat ihren Wunder-Glantz bey Tag und Nacht erh\u00f6ht.", "tokens": ["Hat", "ih\u00b7ren", "Wun\u00b7der\u00b7Glantz", "bey", "Tag", "und", "Nacht", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn da Sebastian/ die Sonne dieser Zeit/", "tokens": ["Denn", "da", "Se\u00b7bas\u00b7ti\u00b7an", "/", "die", "Son\u00b7ne", "die\u00b7ser", "Zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "$(", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns unterth\u00e4nige mit Gnaden Strahlen letzet/", "tokens": ["Uns", "un\u00b7ter\u00b7th\u00e4\u00b7ni\u00b7ge", "mit", "Gna\u00b7den", "Strah\u00b7len", "let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So hat ein ander Licht uns auch noch mehr erfreut/", "tokens": ["So", "hat", "ein", "an\u00b7der", "Licht", "uns", "auch", "noch", "mehr", "er\u00b7freut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil der Riegerungs-Pol mit Sternen ist versetzet/", "tokens": ["Weil", "der", "Rie\u00b7ge\u00b7rungs\u00b7Pol", "mit", "Ster\u00b7nen", "ist", "ver\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Das man durch dero Strahl beseelet r\u00fchmen kanz", "tokens": ["Das", "man", "durch", "de\u00b7ro", "Strahl", "be\u00b7see\u00b7let", "r\u00fch\u00b7men", "kanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "APPR", "PRELAT", "NN", "VVFIN", "VVINF", "VMFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Die Sonne z\u00fcndet hier der Wei\u00dfheit Phar", "tokens": ["Die", "Son\u00b7ne", "z\u00fcn\u00b7det", "hier", "der", "Wei\u00df\u00b7heit", "Phar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein gantz ergebner Knecht verehrt denn geb\u00fcckt/", "tokens": ["Ein", "gantz", "er\u00b7geb\u00b7ner", "Knecht", "ver\u00b7ehrt", "denn", "ge\u00b7b\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die Sterne/ so vor jetzt das gantze Land genieset/", "tokens": ["Die", "Ster\u00b7ne", "/", "so", "vor", "jetzt", "das", "gant\u00b7ze", "Land", "ge\u00b7nie\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df/ das weil die Sonn' auf tieffe Th\u00e4ler blickt/", "tokens": ["Ich", "wei\u00df", "/", "das", "weil", "die", "Sonn'", "auf", "tief\u00b7fe", "Th\u00e4\u00b7ler", "blickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PDS", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dero G\u00fctigkeit auf diese Zeilen fliesset.", "tokens": ["Da\u00df", "de\u00b7ro", "G\u00fc\u00b7tig\u00b7keit", "auf", "die\u00b7se", "Zei\u00b7len", "flies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Geist der w\u00fcnschet sich bey dero Gnaden-Schein/", "tokens": ["Mein", "Geist", "der", "w\u00fcn\u00b7schet", "sich", "bey", "de\u00b7ro", "Gna\u00b7den\u00b7Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "PRF", "APPR", "PRELAT", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Sterne m\u00f6gen auch mein Gl\u00fccks-Gestirne seyn.", "tokens": ["Die", "Ster\u00b7ne", "m\u00f6\u00b7gen", "auch", "mein", "Gl\u00fccks\u00b7Ge\u00b7stir\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}