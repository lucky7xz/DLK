{"dta.poem.906": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "MeNSCHHEIT", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Menschheit vor Feuerschl\u00fcnden aufgestellt,", "tokens": ["Menschheit", "vor", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ein Trommelwirbel, dunkler Krieger Stirnen,", "tokens": ["Ein", "Trom\u00b7mel\u00b7wir\u00b7bel", ",", "dunk\u00b7ler", "Krie\u00b7ger", "Stir\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schritte durch Blutnebel; schwarzes Eisen schellt,", "tokens": ["Schrit\u00b7te", "durch", "Blut\u00b7ne\u00b7bel", ";", "schwar\u00b7zes", "Ei\u00b7sen", "schellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Verzweiflung, Nacht in traurigen Gehirnen:", "tokens": ["Ver\u00b7zwei\u00b7flung", ",", "Nacht", "in", "trau\u00b7ri\u00b7gen", "Ge\u00b7hir\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier Evas Schatten, Jagd und rotes Geld.", "tokens": ["Hier", "E\u00b7vas", "Schat\u00b7ten", ",", "Jagd", "und", "ro\u00b7tes", "Geld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gew\u00f6lk, das Licht durchbricht, das Abendmahl.", "tokens": ["Ge\u00b7w\u00f6lk", ",", "das", "Licht", "durch\u00b7bricht", ",", "das", "A\u00b7bend\u00b7mahl", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es wohnt in Brot und Wein ein sanftes Schweigen", "tokens": ["Es", "wohnt", "in", "Brot", "und", "Wein", "ein", "sanf\u00b7tes", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jene sind versammelt zw\u00f6lf an Zahl.", "tokens": ["Und", "je\u00b7ne", "sind", "ver\u00b7sam\u00b7melt", "zw\u00f6lf", "an", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "VVPP", "CARD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nachts schrein im Schlaf sie unter \u00d6lbaumzweigen;", "tokens": ["Nachts", "schrein", "im", "Schlaf", "sie", "un\u00b7ter", "\u00d6l\u00b7baum\u00b7zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sankt Thomas taucht die Hand ins Wundenmal.", "tokens": ["Sankt", "Tho\u00b7mas", "taucht", "die", "Hand", "ins", "Wun\u00b7den\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}