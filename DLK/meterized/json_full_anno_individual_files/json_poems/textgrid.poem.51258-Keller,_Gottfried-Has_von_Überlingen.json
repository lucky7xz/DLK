{"textgrid.poem.51258": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Has von \u00dcberlingen", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war der Has von \u00dcberlingen,", "tokens": ["Es", "war", "der", "Has", "von", "\u00dc\u00b7ber\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der scheut' den M\u00e4rzen wie den Tod;", "tokens": ["Der", "scheut'", "den", "M\u00e4r\u00b7zen", "wie", "den", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn in die Glieder f\u00fchlt' er dringen", "tokens": ["Denn", "in", "die", "Glie\u00b7der", "f\u00fchlt'", "er", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihm des Alters leise Not.", "tokens": ["Mit", "ihm", "des", "Al\u00b7ters", "lei\u00b7se", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann nun die Morgenl\u00fcfte wehten", "tokens": ["Wann", "nun", "die", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", "weh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach letzten Hornungs Mitternacht,", "tokens": ["Nach", "letz\u00b7ten", "Hor\u00b7nungs", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah man ihn vor die T\u00fcre treten", "tokens": ["Sah", "man", "ihn", "vor", "die", "T\u00fc\u00b7re", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie einen Krieger auf die Wacht.", "tokens": ["Wie", "ei\u00b7nen", "Krie\u00b7ger", "auf", "die", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Krebs geschnallt um Brust und R\u00fccken,", "tokens": ["Den", "Krebs", "ge\u00b7schnallt", "um", "Brust", "und", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf grauem Kopf den Eisenhut,", "tokens": ["Auf", "grau\u00b7em", "Kopf", "den", "Ei\u00b7sen\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umschient die Glieder ohne L\u00fccken:", "tokens": ["Um\u00b7schient", "die", "Glie\u00b7der", "oh\u00b7ne", "L\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das schien ihm f\u00fcr den M\u00e4rzen gut!", "tokens": ["Das", "schien", "ihm", "f\u00fcr", "den", "M\u00e4r\u00b7zen", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den langen Degen an der Seite,", "tokens": ["Den", "lan\u00b7gen", "De\u00b7gen", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Halmbart in beschuhter Hand,", "tokens": ["Die", "Halm\u00b7bart", "in", "be\u00b7schuh\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erwartet' er den Feind zum Streite,", "tokens": ["Er\u00b7war\u00b7tet'", "er", "den", "Feind", "zum", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis sich erhellten See und Land.", "tokens": ["Bis", "sich", "er\u00b7hell\u00b7ten", "See", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbhei, falscher Mars! willst du es wagen?", "tokens": ["\u00bb", "hei", ",", "fal\u00b7scher", "Mars", "!", "willst", "du", "es", "wa\u00b7gen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADJA", "NN", "$.", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dir sag ich ab und biete dir,", "tokens": ["Dir", "sag", "ich", "ab", "und", "bie\u00b7te", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Hieb und Sto\u00df gerecht zu schlagen", "tokens": ["Auf", "Hieb", "und", "Sto\u00df", "ge\u00b7recht", "zu", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ums teure Leben, jetzt und hier!", "tokens": ["Ums", "teu\u00b7re", "Le\u00b7ben", ",", "jetzt", "und", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Willst du an Herz und Mark mir greifen,", "tokens": ["Willst", "du", "an", "Herz", "und", "Mark", "mir", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du T\u00fcckebold, so komm heran!", "tokens": ["Du", "T\u00fc\u00b7cke\u00b7bold", ",", "so", "komm", "he\u00b7ran", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lehre dich ein Liedlein pfeifen,", "tokens": ["Ich", "leh\u00b7re", "dich", "ein", "Lied\u00b7lein", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du findest einen Martismann!\u00ab", "tokens": ["Du", "fin\u00b7dest", "ei\u00b7nen", "Mar\u00b7tis\u00b7mann", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Fuhr dann dem Alten rauh entgegen", "tokens": ["Fuhr", "dann", "dem", "Al\u00b7ten", "rauh", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Staubgew\u00f6lk im Sonnenschein,", "tokens": ["Ein", "Staub\u00b7ge\u00b7w\u00f6lk", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schauer auch von Schnee und Regen,", "tokens": ["Ein", "Schau\u00b7er", "auch", "von", "Schnee", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hieb und stach er m\u00e4chtig drein.", "tokens": ["So", "hieb", "und", "stach", "er", "m\u00e4ch\u00b7tig", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn in dem Duste sah er drohen", "tokens": ["Denn", "in", "dem", "Dus\u00b7te", "sah", "er", "dro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gegner mit gez\u00fccktem Speer;", "tokens": ["Den", "Geg\u00b7ner", "mit", "ge\u00b7z\u00fcck\u00b7tem", "Speer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum schlug er, bis der Spuk entflohen,", "tokens": ["Drum", "schlug", "er", ",", "bis", "der", "Spuk", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und blickte siegreich um sich her.", "tokens": ["Und", "blick\u00b7te", "sieg\u00b7reich", "um", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ein Trunk von goldnem Rebenblute", "tokens": ["Ein", "Trunk", "von", "gold\u00b7nem", "Re\u00b7ben\u00b7blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erquickt' ihn nach bestandnem Streit,", "tokens": ["Er\u00b7quickt'", "ihn", "nach", "be\u00b7stand\u00b7nem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und er geno\u00df mit frohem Mute", "tokens": ["Und", "er", "ge\u00b7no\u00df", "mit", "fro\u00b7hem", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Fr\u00fchlings neue Herrlichkeit.", "tokens": ["Des", "Fr\u00fch\u00b7lings", "neu\u00b7e", "Herr\u00b7lich\u00b7keit", "."], "token_info"