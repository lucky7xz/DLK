{"textgrid.poem.62720": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Allerunterth\u00e4nigstes Sendschreiben an Ihro K\u00f6nigl. Majest\u00e4t von Pohlen und Churf\u00fcrstl. Durchl. zu Sachsen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held, K\u00f6nig, Churf\u00fcrst, Herr!", "tokens": ["Held", ",", "K\u00f6\u00b7nig", ",", "Chur\u00b7f\u00fcrst", ",", "Herr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Ehrfurcht spricht mich frey,", "tokens": ["Die", "Ehr\u00b7furcht", "spricht", "mich", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df die Verwegenheit nicht mit im Spiele sey;", "tokens": ["Da\u00df", "die", "Ver\u00b7we\u00b7gen\u00b7heit", "nicht", "mit", "im", "Spie\u00b7le", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "APPR", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Ehrfurcht, sage ich begleitet meine Reimen.", "tokens": ["Die", "Ehr\u00b7furcht", ",", "sa\u00b7ge", "ich", "be\u00b7glei\u00b7tet", "mei\u00b7ne", "Rei\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich kan ", "tokens": ["Ich", "kan"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Dir meine Schuldigkeit in Demuth darzuthun.", "tokens": ["Dir", "mei\u00b7ne", "Schul\u00b7dig\u00b7keit", "in", "De\u00b7muth", "dar\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es liesse mich mein Geist durchaus nicht l\u00e4nger ruhn,", "tokens": ["Es", "lies\u00b7se", "mich", "mein", "Geist", "durc\u00b7haus", "nicht", "l\u00e4n\u00b7ger", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir grossem K\u00f6nige, durch schreiben, reimen, Dichten,", "tokens": ["Dir", "gros\u00b7sem", "K\u00f6\u00b7ni\u00b7ge", ",", "durch", "schrei\u00b7ben", ",", "rei\u00b7men", ",", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "VVINF", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nach edler Dichter Art, ein Denkmaal aufzurichten.", "tokens": ["Nach", "ed\u00b7ler", "Dich\u00b7ter", "Art", ",", "ein", "Denk\u00b7maal", "auf\u00b7zu\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jedoch was schreibe ich? Was Denkmaal! warlich nein!", "tokens": ["Je\u00b7doch", "was", "schrei\u00b7be", "ich", "?", "Was", "Denk\u00b7maal", "!", "war\u00b7lich", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "$.", "PWS", "NN", "$.", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Dichter und Poet wird nicht verm\u00f6gend seyn,", "tokens": ["Ein", "Dich\u00b7ter", "und", "Po\u00b7et", "wird", "nicht", "ver\u00b7m\u00f6\u00b7gend", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kiel, Dinte und Papir, durch Flei\u00df und M\u00fch zu stiften.", "tokens": ["Kiel", ",", "Din\u00b7te", "und", "Pa\u00b7pir", ",", "durch", "Flei\u00df", "und", "M\u00fch", "zu", "stif\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist voller Glanz und Ruhm; Ein K\u00f6nigliches Wesen,", "tokens": ["Ist", "vol\u00b7ler", "Glanz", "und", "Ruhm", ";", "Ein", "K\u00f6\u00b7nig\u00b7li\u00b7ches", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kan man aus deinem Geist, Wort, Werk und Mienen lesen.", "tokens": ["Kan", "man", "aus", "dei\u00b7nem", "Geist", ",", "Wort", ",", "Werk", "und", "Mie\u00b7nen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Herr! dadurch baust du dir das gr\u00f6ste Ehrenmaal.", "tokens": ["Herr", "!", "da\u00b7durch", "baust", "du", "dir", "das", "gr\u00f6s\u00b7te", "Eh\u00b7ren\u00b7maal", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Man hat in alter Zeit so eine grosse Zahl", "tokens": ["Man", "hat", "in", "al\u00b7ter", "Zeit", "so", "ei\u00b7ne", "gros\u00b7se", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von S\u00e4ulen nicht gesehn, als man jetzt Herzen schauet,", "tokens": ["Von", "S\u00e4u\u00b7len", "nicht", "ge\u00b7sehn", ",", "als", "man", "jetzt", "Her\u00b7zen", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,", "KOUS", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Worin man ", "tokens": ["Wo\u00b7rin", "man"], "token_info": ["word", "word"], "pos": ["PWAV", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Kein Dichter hat nicht noth, da\u00df er Gedichte schreibt;", "tokens": ["Kein", "Dich\u00b7ter", "hat", "nicht", "noth", ",", "da\u00df", "er", "Ge\u00b7dich\u00b7te", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er hat genug zu thun, wenn er die Wahrheit treibt:", "tokens": ["Er", "hat", "ge\u00b7nug", "zu", "thun", ",", "wenn", "er", "die", "Wahr\u00b7heit", "treibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er darf ", "tokens": ["Er", "darf"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "So wirds ihn warlich nicht an Stof und Einfall fehlen.", "tokens": ["So", "wirds", "ihn", "war\u00b7lich", "nicht", "an", "Stof", "und", "Ein\u00b7fall", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Ehrfurcht und die Pflicht reitzt manchen Dichter-Kiel", "tokens": ["Die", "Ehr\u00b7furcht", "und", "die", "Pflicht", "reitzt", "man\u00b7chen", "Dich\u00b7ter\u00b7Kiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df er ", "tokens": ["Da\u00df", "er"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Vor deinem Ohren stimt, und lieblich von dir singet,", "tokens": ["Vor", "dei\u00b7nem", "Oh\u00b7ren", "stimt", ",", "und", "lieb\u00b7lich", "von", "dir", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und dadurch seiner Pflicht das schuldge Opfer bringet.", "tokens": ["Und", "da\u00b7durch", "sei\u00b7ner", "Pflicht", "das", "schuld\u00b7ge", "Op\u00b7fer", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Drum w\u00fcrd es warlich mir die gr\u00f6\u00dfte Schande seyn,", "tokens": ["Drum", "w\u00fcrd", "es", "war\u00b7lich", "mir", "die", "gr\u00f6\u00df\u00b7te", "Schan\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PPER", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn ich zur\u00fccke blieb. Mir fiel zwar \u00f6fters ein.", "tokens": ["Wenn", "ich", "zu\u00b7r\u00fc\u00b7cke", "blieb", ".", "Mir", "fiel", "zwar", "\u00f6f\u00b7ters", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du m\u00f6chtest ", "tokens": ["Du", "m\u00f6ch\u00b7test"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Der nahm den Zweifel weg, und trieb mich kr\u00e4ftig an,", "tokens": ["Der", "nahm", "den", "Zwei\u00b7fel", "weg", ",", "und", "trieb", "mich", "kr\u00e4f\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So hab ichs auf Befehl, und auch aus Pflicht gethan.", "tokens": ["So", "hab", "ichs", "auf", "Be\u00b7fehl", ",", "und", "auch", "aus", "Pflicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "NN", "$,", "KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und bitte demuthsvoll, wirf jetzt auf meine Lieder", "tokens": ["Und", "bit\u00b7te", "de\u00b7muths\u00b7voll", ",", "wirf", "jetzt", "auf", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "VVIMP", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Auch einen Gnaden-Strahl. Dein K\u00f6nigs Angesicht", "tokens": ["Auch", "ei\u00b7nen", "Gna\u00b7den\u00b7Strahl", ".", "Dein", "K\u00f6\u00b7nigs", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verachte und verschm\u00e4h mein lallend Singen nicht.", "tokens": ["Ver\u00b7ach\u00b7te", "und", "ver\u00b7schm\u00e4h", "mein", "lal\u00b7lend", "Sin\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "ADJD", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Kein Anfall m\u00fcsse nie dein hohes Herze kr\u00e4nken.", "tokens": ["Kein", "An\u00b7fall", "m\u00fcs\u00b7se", "nie", "dein", "ho\u00b7hes", "Her\u00b7ze", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mir aber wollest du ein gn\u00e4dig Auge schenken", "tokens": ["Mir", "a\u00b7ber", "wol\u00b7lest", "du", "ein", "gn\u00e4\u00b7dig", "Au\u00b7ge", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Held, K\u00f6nig, Churf\u00fcrst, Herr!", "tokens": ["Held", ",", "K\u00f6\u00b7nig", ",", "Chur\u00b7f\u00fcrst", ",", "Herr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Ehrfurcht spricht mich frey,", "tokens": ["Die", "Ehr\u00b7furcht", "spricht", "mich", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df die Verwegenheit nicht mit im Spiele sey;", "tokens": ["Da\u00df", "die", "Ver\u00b7we\u00b7gen\u00b7heit", "nicht", "mit", "im", "Spie\u00b7le", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "APPR", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Ehrfurcht, sage ich begleitet meine Reimen.", "tokens": ["Die", "Ehr\u00b7furcht", ",", "sa\u00b7ge", "ich", "be\u00b7glei\u00b7tet", "mei\u00b7ne", "Rei\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich kan ", "tokens": ["Ich", "kan"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Dir meine Schuldigkeit in Demuth darzuthun.", "tokens": ["Dir", "mei\u00b7ne", "Schul\u00b7dig\u00b7keit", "in", "De\u00b7muth", "dar\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es liesse mich mein Geist durchaus nicht l\u00e4nger ruhn,", "tokens": ["Es", "lies\u00b7se", "mich", "mein", "Geist", "durc\u00b7haus", "nicht", "l\u00e4n\u00b7ger", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir grossem K\u00f6nige, durch schreiben, reimen, Dichten,", "tokens": ["Dir", "gros\u00b7sem", "K\u00f6\u00b7ni\u00b7ge", ",", "durch", "schrei\u00b7ben", ",", "rei\u00b7men", ",", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "VVINF", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nach edler Dichter Art, ein Denkmaal aufzurichten.", "tokens": ["Nach", "ed\u00b7ler", "Dich\u00b7ter", "Art", ",", "ein", "Denk\u00b7maal", "auf\u00b7zu\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", 