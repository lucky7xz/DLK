}, "line.14": {"text": "Kan man aus deinem Geist, Wort, Werk und Mienen lesen.", "tokens": ["Kan", "man", "aus", "dei\u00b7nem", "Geist", ",", "Wort", ",", "Werk", "und", "Mie\u00b7nen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Herr! dadurch baust du dir das gr\u00f6ste Ehrenmaal.", "tokens": ["Herr", "!", "da\u00b7durch", "baust", "du", "dir", "das", "gr\u00f6s\u00b7te", "Eh\u00b7ren\u00b7maal", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Man hat in alter Zeit so eine grosse Zahl", "tokens": ["Man", "hat", "in", "al\u00b7ter", "Zeit", "so", "ei\u00b7ne", "gros\u00b7se", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von S\u00e4ulen nicht gesehn, als man jetzt Herzen schauet,", "tokens": ["Von", "S\u00e4u\u00b7len", "nicht", "ge\u00b7sehn", ",", "als", "man", "jetzt", "Her\u00b7zen", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,", "KOUS", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Worin man ", "tokens": ["Wo\u00b7rin", "man"], "token_info": ["word", "word"], "pos": ["PWAV", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Kein Dichter hat nicht noth, da\u00df er Gedichte schreibt;", "tokens": ["Kein", "Dich\u00b7ter", "hat", "nicht", "noth", ",", "da\u00df", "er", "Ge\u00b7dich\u00b7te", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er hat genug zu thun, wenn er die Wahrheit treibt:", "tokens": ["Er", "hat", "ge\u00b7nug", "zu", "thun", ",", "wenn", "er", "die", "Wahr\u00b7heit", "treibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er darf ", "tokens": ["Er", "darf"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "So wirds ihn warlich nicht an Stof und Einfall fehlen.", "tokens": ["So", "wirds", "ihn", "war\u00b7lich", "nicht", "an", "Stof", "und", "Ein\u00b7fall", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Ehrfurcht und die Pflicht reitzt manchen Dichter-Kiel", "tokens": ["Die", "Ehr\u00b7furcht", "und", "die", "Pflicht", "reitzt", "man\u00b7chen", "Dich\u00b7ter\u00b7Kiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df er ", "tokens": ["Da\u00df", "er"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Vor deinem Ohren stimt, und lieblich von dir singet,", "tokens": ["Vor", "dei\u00b7nem", "Oh\u00b7ren", "stimt", ",", "und", "lieb\u00b7lich", "von", "dir", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und dadurch seiner Pflicht das schuldge Opfer bringet.", "tokens": ["Und", "da\u00b7durch", "sei\u00b7ner", "Pflicht", "das", "schuld\u00b7ge", "Op\u00b7fer", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Drum w\u00fcrd es warlich mir die gr\u00f6\u00dfte Schande seyn,", "tokens": ["Drum", "w\u00fcrd", "es", "war\u00b7lich", "mir", "die", "gr\u00f6\u00df\u00b7te", "Schan\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PPER", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn ich zur\u00fccke blieb. Mir fiel zwar \u00f6fters ein.", "tokens": ["Wenn", "ich", "zu\u00b7r\u00fc\u00b7cke", "blieb", ".", "Mir", "fiel", "zwar", "\u00f6f\u00b7ters", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du m\u00f6chtest ", "tokens": ["Du", "m\u00f6ch\u00b7test"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Der nahm den Zweifel weg, und trieb mich kr\u00e4ftig an,", "tokens": ["Der", "nahm", "den", "Zwei\u00b7fel", "weg", ",", "und", "trieb", "mich", "kr\u00e4f\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So hab ichs auf Befehl, und auch aus Pflicht gethan.", "tokens": ["So", "hab", "ichs", "auf", "Be\u00b7fehl", ",", "und", "auch", "aus", "Pflicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "NN", "$,", "KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und bitte demuthsvoll, wirf jetzt auf meine Lieder", "tokens": ["Und", "bit\u00b7te", "de\u00b7muths\u00b7voll", ",", "wirf", "jetzt", "auf", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "VVIMP", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Auch einen Gnaden-Strahl. Dein K\u00f6nigs Angesicht", "tokens": ["Auch", "ei\u00b7nen", "Gna\u00b7den\u00b7Strahl", ".", "Dein", "K\u00f6\u00b7nigs", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verachte und verschm\u00e4h mein lallend Singen nicht.", "tokens": ["Ver\u00b7ach\u00b7te", "und", "v