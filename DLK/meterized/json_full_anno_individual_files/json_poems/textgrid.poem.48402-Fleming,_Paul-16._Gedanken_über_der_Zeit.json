{"textgrid.poem.48402": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16. Gedanken \u00fcber der Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr lebet in der Zeit und kennt doch keine Zeit;", "tokens": ["Ihr", "le\u00b7bet", "in", "der", "Zeit", "und", "kennt", "doch", "kei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so wi\u00dft, ihr Menschen, nicht von und in was ihr seid.", "tokens": ["so", "wi\u00dft", ",", "ihr", "Men\u00b7schen", ",", "nicht", "von", "und", "in", "was", "ihr", "seid", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "KON", "APPR", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df wi\u00dft ihr, da\u00df ihr seid in einer Zeit geboren", "tokens": ["Di\u00df", "wi\u00dft", "ihr", ",", "da\u00df", "ihr", "seid", "in", "ei\u00b7ner", "Zeit", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und da\u00df ihr werdet auch in einer Zeit verloren.", "tokens": ["und", "da\u00df", "ihr", "wer\u00b7det", "auch", "in", "ei\u00b7ner", "Zeit", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was aber war die Zeit, die euch in sich gebracht?", "tokens": ["Was", "a\u00b7ber", "war", "die", "Zeit", ",", "die", "euch", "in", "sich", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und was wird diese sein, die euch zu nichts mehr macht?", "tokens": ["Und", "was", "wird", "die\u00b7se", "sein", ",", "die", "euch", "zu", "nichts", "mehr", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PDS", "VAINF", "$,", "PRELS", "PPER", "APPR", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Zeit ist was und nichts, der Mensch in gleichem Falle,", "tokens": ["Die", "Zeit", "ist", "was", "und", "nichts", ",", "der", "Mensch", "in", "glei\u00b7chem", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "KON", "PIS", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "doch was dasselbe was und nichts sei, zweifeln alle.", "tokens": ["doch", "was", "das\u00b7sel\u00b7be", "was", "und", "nichts", "sei", ",", "zwei\u00b7feln", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PDAT", "PWS", "KON", "PIS", "VAFIN", "$,", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Zeit, die stirbt in sich und zeugt sich auch aus sich.", "tokens": ["Die", "Zeit", ",", "die", "stirbt", "in", "sich", "und", "zeugt", "sich", "auch", "aus", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PRF", "KON", "VVFIN", "PRF", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Di\u00df k\u00f6mmt aus mir und dir, von dem du bist und ich.", "tokens": ["Di\u00df", "k\u00f6mmt", "aus", "mir", "und", "dir", ",", "von", "dem", "du", "bist", "und", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "KON", "PPER", "$,", "APPR", "PRELS", "PPER", "VAFIN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Mensch ist in der Zeit; sie ist in ihm ingleichen,", "tokens": ["Der", "Mensch", "ist", "in", "der", "Zeit", ";", "sie", "ist", "in", "ihm", "in\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "doch aber mu\u00df der Mensch, wenn sie noch bleibet, weichen.", "tokens": ["doch", "a\u00b7ber", "mu\u00df", "der", "Mensch", ",", "wenn", "sie", "noch", "blei\u00b7bet", ",", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Zeit ist, was ihr seid, und ihr seid, was die Zeit,", "tokens": ["Die", "Zeit", "ist", ",", "was", "ihr", "seid", ",", "und", "ihr", "seid", ",", "was", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "PPER", "VAFIN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "nur da\u00df ihr wenger noch, als was die Zeit ist, seid.", "tokens": ["nur", "da\u00df", "ihr", "wen\u00b7ger", "noch", ",", "als", "was", "die", "Zeit", "ist", ",", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "$,", "KOUS", "PIS", "ART", "NN", "VAFIN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach da\u00df doch jene Zeit, die ohne Zeit ist, k\u00e4me", "tokens": ["Ach", "da\u00df", "doch", "je\u00b7ne", "Zeit", ",", "die", "oh\u00b7ne", "Zeit", "ist", ",", "k\u00e4\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "KOUS", "ADV", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und uns aus dieser Zeit in ihre Zeiten n\u00e4hme,", "tokens": ["und", "uns", "aus", "die\u00b7ser", "Zeit", "in", "ih\u00b7re", "Zei\u00b7ten", "n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "