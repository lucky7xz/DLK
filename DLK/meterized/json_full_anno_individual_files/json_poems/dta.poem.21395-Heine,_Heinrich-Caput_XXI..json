{"dta.poem.21395": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie alte B\u00f6rse ist verbrannt,", "tokens": ["\u201e", "die", "al\u00b7te", "B\u00f6r\u00b7se", "ist", "ver\u00b7brannt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo unsere V\u00e4ter gewandelt,", "tokens": ["Wo", "un\u00b7se\u00b7re", "V\u00e4\u00b7ter", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und mit einander Jahrhunderte lang", "tokens": ["Und", "mit", "ein\u00b7an\u00b7der", "Jahr\u00b7hun\u00b7der\u00b7te", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "NN", "ADJD"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "So redlich als m\u00f6glich gehandelt.", "tokens": ["So", "red\u00b7lich", "als", "m\u00f6g\u00b7lich", "ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u201edie Bank, die silberne Seele der Stadt,", "tokens": ["\u201e", "die", "Bank", ",", "die", "sil\u00b7ber\u00b7ne", "See\u00b7le", "der", "Stadt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die B\u00fccher wo eingeschrieben", "tokens": ["Und", "die", "B\u00fc\u00b7cher", "wo", "ein\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PWAV", "VVPP"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jedweden Mannes Banko-Werth,", "tokens": ["Jed\u00b7we\u00b7den", "Man\u00b7nes", "Ban\u00b7ko\u00b7Werth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gottlob! sie sind uns geblieben!", "tokens": ["Gott\u00b7lob", "!", "sie", "sind", "uns", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201egottlob! man kollektirte f\u00fcr uns", "tokens": ["\u201e", "gott\u00b7lob", "!", "man", "kol\u00b7lek\u00b7tir\u00b7te", "f\u00fcr", "uns"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$.", "PIS", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Selbst bei den fernsten Nazionen \u2013", "tokens": ["Selbst", "bei", "den", "ferns\u00b7ten", "Na\u00b7zi\u00b7o\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein gutes Gesch\u00e4ft \u2013 die Collekte betrug", "tokens": ["Ein", "gu\u00b7tes", "Ge\u00b7sch\u00e4ft", "\u2013", "die", "Col\u00b7lek\u00b7te", "be\u00b7trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wohl an die acht Millionen.", "tokens": ["Wohl", "an", "die", "acht", "Mil\u00b7lion\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eaus allen L\u00e4ndern flo\u00df das Geld", "tokens": ["\u201e", "aus", "al\u00b7len", "L\u00e4n\u00b7dern", "flo\u00df", "das", "Geld"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsre offnen H\u00e4nde,", "tokens": ["In", "uns\u00b7re", "off\u00b7nen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch Victualien nahmen wir an,", "tokens": ["Auch", "Vic\u00b7tua\u00b7li\u00b7en", "nah\u00b7men", "wir", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Verschm\u00e4hten keine Spende.", "tokens": ["Ver\u00b7schm\u00e4h\u00b7ten", "kei\u00b7ne", "Spen\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}