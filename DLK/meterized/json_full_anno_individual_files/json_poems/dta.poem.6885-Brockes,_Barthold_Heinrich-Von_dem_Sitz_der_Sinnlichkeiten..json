{"dta.poem.6885": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der weise SCHOEPFFER aller Welt", "tokens": ["Der", "wei\u00b7se", "ScHOE\u00b7PF\u00b7FER", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hat auch hierinn den Glantz der Weisheit vorgestellt,", "tokens": ["Hat", "auch", "hie\u00b7rinn", "den", "Glantz", "der", "Weis\u00b7heit", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da der geheime Zwang die C\u00f6rper unterh\u00e4lt.", "tokens": ["Da", "der", "ge\u00b7hei\u00b7me", "Zwang", "die", "C\u00f6r\u00b7per", "un\u00b7ter\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie w\u00e4r es m\u00fcglich, da\u00df die Harmonie best\u00e4nde,", "tokens": ["Wie", "w\u00e4r", "es", "m\u00fcg\u00b7lich", ",", "da\u00df", "die", "Har\u00b7mo\u00b7nie", "be\u00b7st\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn unsre Seele nicht", "tokens": ["Wenn", "uns\u00b7re", "See\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf eine Art w\u00e4r zugericht\u2019t,", "tokens": ["Auf", "ei\u00b7ne", "Art", "w\u00e4r", "zu\u00b7ge\u00b7richt't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie f\u00fcr sich", "tokens": ["Da\u00df", "sie", "f\u00fcr", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Von allen, welches \u00e4userlich,", "tokens": ["Von", "al\u00b7len", ",", "wel\u00b7ches", "\u00e4u\u00b7ser\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Druck, die W\u00fcrckungen empf\u00e4nde?", "tokens": ["Den", "Druck", ",", "die", "W\u00fcr\u00b7ckun\u00b7gen", "em\u00b7pf\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Wenn dieses sollt auf andre Art geschehn;", "tokens": ["Wenn", "die\u00b7ses", "sollt", "auf", "and\u00b7re", "Art", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VMFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Unm\u00f6glich k\u00f6nten wir bestehn.", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "k\u00f6n\u00b7ten", "wir", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nothwendig m\u00fcsten wir so starcke Triebe sp\u00fcren,", "tokens": ["Noth\u00b7wen\u00b7dig", "m\u00fcs\u00b7ten", "wir", "so", "star\u00b7cke", "Trie\u00b7be", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die uns durch sie best\u00e4ndig r\u00fchren.", "tokens": ["Die", "uns", "durch", "sie", "be\u00b7st\u00e4n\u00b7dig", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Denn durch erkennen blos allein", "tokens": ["Denn", "durch", "er\u00b7ken\u00b7nen", "blos", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "VVINF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Desjenigen, was unsern Leib beweget,", "tokens": ["Des\u00b7je\u00b7ni\u00b7gen", ",", "was", "un\u00b7sern", "Leib", "be\u00b7we\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und etwan Pein und Plagen ihm erreget,", "tokens": ["Und", "et\u00b7wan", "Pein", "und", "Pla\u00b7gen", "ihm", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "W\u00fcrd\u2019 unsrer Seele nicht so schnell zu helffen seyn.", "tokens": ["W\u00fcrd'", "uns\u00b7rer", "See\u00b7le", "nicht", "so", "schnell", "zu", "helf\u00b7fen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}