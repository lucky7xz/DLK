{"dta.poem.23445": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Soldaten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schrecklich sind die Meinungskriege!", "tokens": ["Wie", "schreck\u00b7lich", "sind", "die", "Mei\u00b7nungs\u00b7krie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weh ihm wer daf\u00fcr k\u00e4mpft und ficht!", "tokens": ["Weh", "ihm", "wer", "da\u00b7f\u00fcr", "k\u00e4mpft", "und", "ficht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PWS", "PAV", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zwar Niederlagen oder Siege", "tokens": ["Zwar", "Nie\u00b7der\u00b7la\u00b7gen", "o\u00b7der", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entehren ihren K\u00e4mpfer nicht.", "tokens": ["E\u00b7nteh\u00b7ren", "ih\u00b7ren", "K\u00e4mp\u00b7fer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch seine Haut zu Markte tragen", "tokens": ["Doch", "sei\u00b7ne", "Haut", "zu", "Mark\u00b7te", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr eine Handvoll Lohn und Sold \u2014", "tokens": ["F\u00fcr", "ei\u00b7ne", "Hand\u00b7voll", "Lohn", "und", "Sold"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kann ein Lumpenhund nur wagen,", "tokens": ["Das", "kann", "ein", "Lum\u00b7pen\u00b7hund", "nur", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4tt' es selber Gott gewollt.", "tokens": ["Und", "h\u00e4tt'", "es", "sel\u00b7ber", "Gott", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}