{"dta.poem.11762": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen standen frisch erquickt auf d\u00fcrrer Au,", "tokens": ["Die", "Blu\u00b7men", "stan\u00b7den", "frisch", "er\u00b7quickt", "auf", "d\u00fcr\u00b7rer", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn jede hatt' im Mund ihr Tr\u00f6pflein Morgenthau.", "tokens": ["Denn", "je\u00b7de", "hatt'", "im", "Mund", "ihr", "Tr\u00f6pf\u00b7lein", "Mor\u00b7gent\u00b7hau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das hatten sie bei Nacht zur Tageskost empfangen.", "tokens": ["Das", "hat\u00b7ten", "sie", "bei", "Nacht", "zur", "Ta\u00b7ges\u00b7kost", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sprachen: Schwestern, la\u00dft uns nun mit Wen'gem langen!", "tokens": ["Sie", "spra\u00b7chen", ":", "Schwes\u00b7tern", ",", "la\u00dft", "uns", "nun", "mit", "Wen'\u00b7gem", "lan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Lang ist der hei\u00dfe Tag, der uns versengt die Glieder,", "tokens": ["Lang", "ist", "der", "hei\u00b7\u00dfe", "Tag", ",", "der", "uns", "ver\u00b7sengt", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und erst der Abend bringt uns eine Labung wieder.", "tokens": ["Und", "erst", "der", "A\u00b7bend", "bringt", "uns", "ei\u00b7ne", "La\u00b7bung", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie wachten hin den Tag so still alsob sie schliefen,", "tokens": ["Sie", "wach\u00b7ten", "hin", "den", "Tag", "so", "still", "al\u00b7sob", "sie", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchschliefen k\u00fchl die Nacht, erwachten fr\u00fch und riefen:", "tokens": ["Durch\u00b7schlie\u00b7fen", "k\u00fchl", "die", "Nacht", ",", "er\u00b7wach\u00b7ten", "fr\u00fch", "und", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wir armen Schwestern, ach, heut m\u00fcssen wir verschmachten,", "tokens": ["Wir", "ar\u00b7men", "Schwes\u00b7tern", ",", "ach", ",", "heut", "m\u00fcs\u00b7sen", "wir", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ITJ", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da die gewohnte Lab' uns nicht die Stunden brachten.", "tokens": ["Da", "die", "ge\u00b7wohn\u00b7te", "Lab'", "uns", "nicht", "die", "Stun\u00b7den", "brach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wir armen Schwestern, ach! die goldne Morgenstunde", "tokens": ["Wir", "ar\u00b7men", "Schwes\u00b7tern", ",", "ach", "!", "die", "gold\u00b7ne", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ITJ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kam selber ohn' ihr Gold, ohn' ihren Thau im Munde.", "tokens": ["Kam", "sel\u00b7ber", "ohn'", "ihr", "Gold", ",", "ohn'", "ih\u00b7ren", "Thau", "im", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,", "KOUI", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch eine rief im Kreis: Still! junge Jahrespflanzen,", "tokens": ["Doch", "ei\u00b7ne", "rief", "im", "Kreis", ":", "Still", "!", "jun\u00b7ge", "Jah\u00b7res\u00b7pflan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "$.", "ADJD", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr kennt die Stunde nur, und nicht die Zeit im Ganzen.", "tokens": ["Ihr", "kennt", "die", "Stun\u00b7de", "nur", ",", "und", "nicht", "die", "Zeit", "im", "Gan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "KON", "PTKNEG", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihr bl\u00fcht am Boden hin, geweckt vom Fr\u00fchlingshauch,", "tokens": ["Ihr", "bl\u00fcht", "am", "Bo\u00b7den", "hin", ",", "ge\u00b7weckt", "vom", "Fr\u00fch\u00b7lings\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sommer durch zum Herbst; ich aber bl\u00fch' am Strauch.", "tokens": ["Den", "Som\u00b7mer", "durch", "zum", "Herbst", ";", "ich", "a\u00b7ber", "bl\u00fch'", "am", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$.", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Jung wie ihr selbst, hab' ich vor euch des Strauchs Bejahrung", "tokens": ["Jung", "wie", "ihr", "selbst", ",", "hab'", "ich", "vor", "euch", "des", "Strauchs", "Be\u00b7jah\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "ADV", "$,", "VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voraus, und so vernehmt die Stimme der Erfahrung:", "tokens": ["Vo\u00b7raus", ",", "und", "so", "ver\u00b7nehmt", "die", "Stim\u00b7me", "der", "Er\u00b7fah\u00b7rung", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Weil heut, auf den ihr hofft, der Thau nicht eingetroffen,", "tokens": ["Weil", "heut", ",", "auf", "den", "ihr", "hofft", ",", "der", "Thau", "nicht", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deswegen grade d\u00fcrft ihr nun auf Regen hoffen.", "tokens": ["Des\u00b7we\u00b7gen", "gra\u00b7de", "d\u00fcrft", "ihr", "nun", "auf", "Re\u00b7gen", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Die Mutter, deren Brust ihr bl\u00fchet eingesenkt,", "tokens": ["Die", "Mut\u00b7ter", ",", "de\u00b7ren", "Brust", "ihr", "bl\u00fc\u00b7het", "ein\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die bald von unten euch und bald von oben tr\u00e4nkt;", "tokens": ["Die", "bald", "von", "un\u00b7ten", "euch", "und", "bald", "von", "o\u00b7ben", "tr\u00e4nkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "PPER", "KON", "ADV", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Sie wei\u00df am besten wol, wodurch ihr Kind gedeiht,", "tokens": ["Sie", "wei\u00df", "am", "bes\u00b7ten", "wol", ",", "wo\u00b7durch", "ihr", "Kind", "ge\u00b7deiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch das verschiedne gibt sie nicht zu gleicher Zeit.", "tokens": ["Doch", "das", "ver\u00b7schied\u00b7ne", "gibt", "sie", "nicht", "zu", "glei\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Wenn, eh zur Luft sie steigt, Erdfeuchtigkeit zur Erden", "tokens": ["Wenn", ",", "eh", "zur", "Luft", "sie", "steigt", ",", "Erd\u00b7feuch\u00b7tig\u00b7keit", "zur", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herabf\u00e4llt, wird sie Thau, und kann nicht Wolke werden.", "tokens": ["Her\u00b7ab\u00b7f\u00e4llt", ",", "wird", "sie", "Thau", ",", "und", "kann", "nicht", "Wol\u00b7ke", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "NN", "$,", "KON", "VMFIN", "PTKNEG", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Wenn h\u00f6her steigt der Dunst, euch nicht als Thau erquickt,", "tokens": ["Wenn", "h\u00f6\u00b7her", "steigt", "der", "Dunst", ",", "euch", "nicht", "als", "Thau", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "NN", "$,", "PPER", "PTKNEG", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann wird f\u00fcr euch im Blau der Mantel grau gestrickt.", "tokens": ["Dann", "wird", "f\u00fcr", "euch", "im", "Blau", "der", "Man\u00b7tel", "grau", "ge\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Denn wenn die Mutter eins entzieht, gibt sie dagegen", "tokens": ["Denn", "wenn", "die", "Mut\u00b7ter", "eins", "ent\u00b7zieht", ",", "gibt", "sie", "da\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das andre; da ihr Thau nicht kam, so kommt ihr Regen. \u2014", "tokens": ["Das", "and\u00b7re", ";", "da", "ihr", "Thau", "nicht", "kam", ",", "so", "kommt", "ihr", "Re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$.", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Die Blumen lauschten noch, da h\u00f6rten sie es rauschen,", "tokens": ["Die", "Blu\u00b7men", "lauschten", "noch", ",", "da", "h\u00f6r\u00b7ten", "sie", "es", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und hoffnungsvoller noch begannen sie zu lauschen.", "tokens": ["Und", "hoff\u00b7nungs\u00b7vol\u00b7ler", "noch", "be\u00b7gan\u00b7nen", "sie", "zu", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Und als hernieder nun der Regengu\u00df gerauscht,", "tokens": ["Und", "als", "her\u00b7nie\u00b7der", "nun", "der", "Re\u00b7gen\u00b7gu\u00df", "ge\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da senkten sie besch\u00e4mt die H\u00e4upter s\u00fc\u00dfberauscht.", "tokens": ["Da", "senk\u00b7ten", "sie", "be\u00b7sch\u00e4mt", "die", "H\u00e4up\u00b7ter", "s\u00fc\u00df\u00b7be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}