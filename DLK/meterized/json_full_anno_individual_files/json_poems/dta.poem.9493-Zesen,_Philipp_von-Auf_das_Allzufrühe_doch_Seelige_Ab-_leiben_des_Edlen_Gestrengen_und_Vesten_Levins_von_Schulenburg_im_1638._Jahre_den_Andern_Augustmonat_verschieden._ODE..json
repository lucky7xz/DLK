{"dta.poem.9493": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Allzufr\u00fche doch Seelige Ab-  \n leiben des Edlen/ Gestrengen und Vesten Levins von  \n Schulenburg im 1638. Jahre/ den Andern  \n Augustmonat verschieden.  \n  ODE.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr/ die Jhr mit Tugend pranget", "tokens": ["Ihr", "/", "die", "Ihr", "mit", "Tu\u00b7gend", "pran\u00b7get"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PRELS", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und der Kunst ergeben seyd:", "tokens": ["und", "der", "Kunst", "er\u00b7ge\u00b7ben", "seyd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhr/ die Jhr zu dieser Zeit", "tokens": ["Ihr", "/", "die", "Ihr", "zu", "die\u00b7ser", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PRELS", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Castalinnen hanget/", "tokens": ["An", "den", "Cas\u00b7ta\u00b7lin\u00b7nen", "han\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die nach Zucht und Tugend gehn/", "tokens": ["Die", "nach", "Zucht", "und", "Tu\u00b7gend", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht Jhr nicht allhier auch stehn", "tokens": ["Seht", "Ihr", "nicht", "all\u00b7hier", "auch", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auff der Bahren Eures gleichen?", "tokens": ["Auff", "der", "Bah\u00b7ren", "Eu\u00b7res", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach wie dessen Lippen bleichen/", "tokens": ["Ach", "wie", "des\u00b7sen", "Lip\u00b7pen", "blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PDS", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welche sonsten wichen nicht", "tokens": ["Wel\u00b7che", "sons\u00b7ten", "wi\u00b7chen", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "VVFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Einem unter diesen hauffen/", "tokens": ["Ei\u00b7nem", "un\u00b7ter", "die\u00b7sen", "hauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "So in Pallas Tempel lauffen!", "tokens": ["So", "in", "Pal\u00b7las", "Tem\u00b7pel", "lauf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Denen keine Kunst gebricht.", "tokens": ["De\u00b7nen", "kei\u00b7ne", "Kunst", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhm war Wei\u00dfheit/ Kunst und Adel", "tokens": ["Jhm", "war", "Wei\u00df\u00b7heit", "/", "Kunst", "und", "A\u00b7del"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allezeit gantz unterthan;", "tokens": ["Al\u00b7le\u00b7zeit", "gantz", "un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Gute hieng Jhm an/", "tokens": ["Al\u00b7les", "Gu\u00b7te", "hieng", "Jhm", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand gab Jhm einen Tadel:", "tokens": ["Nie\u00b7mand", "gab", "Jhm", "ei\u00b7nen", "Ta\u00b7del", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch mu\u00df der Menschen-Fra\u00df", "tokens": ["Den\u00b7noch", "mu\u00df", "der", "Men\u00b7schen\u00b7Fra\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhn zubrechen wie ein Gla\u00df.", "tokens": ["Jhn", "zu\u00b7bre\u00b7chen", "wie", "ein", "Gla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hier/ hier sicht man/ wem das Leben", "tokens": ["Hier", "/", "hier", "sicht", "man", "/", "wem", "das", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "ADV", "VVFIN", "PIS", "$(", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller Menschen sey ergeben.", "tokens": ["Al\u00b7ler", "Men\u00b7schen", "sey", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Todt es w\u00e4chset alles dir/", "tokens": ["Todt", "es", "w\u00e4ch\u00b7set", "al\u00b7les", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dir ist alles untergeben/", "tokens": ["Dir", "ist", "al\u00b7les", "un\u00b7ter\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "(heist es) unser aller Leben", "tokens": ["(", "heist", "es", ")", "un\u00b7ser", "al\u00b7ler", "Le\u00b7ben"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$(", "PPOSAT", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nichtg/ fl\u00fcchtig bleibet hier:", "tokens": ["Nichtg", "/", "fl\u00fcch\u00b7tig", "blei\u00b7bet", "hier", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jhr aber/ die Jhr seht/ den Leichnam vor Euch tragen/", "tokens": ["Ihr", "a\u00b7ber", "/", "die", "Ihr", "seht", "/", "den", "Leich\u00b7nam", "vor", "Euch", "tra\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "PRELS", "PPER", "VVFIN", "$(", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kr\u00e4nckt Euch nicht so sehr", "tokens": ["Kr\u00e4nckt", "Euch", "nicht", "so", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sondern g\u00f6nnet mehr", "tokens": ["Son\u00b7dern", "g\u00f6n\u00b7net", "mehr"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jhm seine Ruh; Was hilffet Euch das klagen?", "tokens": ["Jhm", "sei\u00b7ne", "Ruh", ";", "Was", "hilf\u00b7fet", "Euch", "das", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er ist vorangeschickt/ und machet Euch die Bahn", "tokens": ["Er", "ist", "vor\u00b7an\u00b7ge\u00b7schickt", "/", "und", "ma\u00b7chet", "Euch", "die", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu dem Leben/", "tokens": ["Zu", "dem", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Da wird schweben", "tokens": ["Da", "wird", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sein hoher Geist mit Freuden angethan.", "tokens": ["Sein", "ho\u00b7her", "Geist", "mit", "Freu\u00b7den", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}