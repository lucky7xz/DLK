{"dta.poem.10909": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kann man den ein Grunzen sehn? Kann uns eine W\u00fc-\nsteney,", "tokens": ["Kann", "man", "den", "ein", "Grun\u00b7zen", "sehn", "?", "Kann", "uns", "ei\u00b7ne", "W\u00fc", "ste\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ART", "NN", "VVINF", "$.", "VMFIN", "PPER", "ART", "TRUNC", "NN", "$,"], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die nicht gegenw\u00e4rtig, schrecken?", "tokens": ["Die", "nicht", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ",", "schre\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die\u00df beydes n\u00fctzlich sey,", "tokens": ["Da\u00df", "die\u00df", "bey\u00b7des", "n\u00fctz\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PIS", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann uns dieses Blatt entdecken.", "tokens": ["Kann", "uns", "die\u00b7ses", "Blatt", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebt nicht diese Bache fast? decket sie nicht so nat\u00fcrlich,", "tokens": ["Lebt", "nicht", "die\u00b7se", "Ba\u00b7che", "fast", "?", "de\u00b7cket", "sie", "nicht", "so", "na\u00b7t\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PDAT", "NN", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und so sorgsam ihre Zucht, die halb wild und halb po\u00dfirlich?", "tokens": ["Und", "so", "sorg\u00b7sam", "ih\u00b7re", "Zucht", ",", "die", "halb", "wild", "und", "halb", "po\u00df\u00b7ir\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Solche Wunder wirkt die Kunst. Aber la\u00dft uns die Copey", "tokens": ["Sol\u00b7che", "Wun\u00b7der", "wirkt", "die", "Kunst", ".", "A\u00b7ber", "la\u00dft", "uns", "die", "Co\u00b7pey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$.", "KON", "VVIMP", "PPER", "ART", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Zu dem Urbild, der Natur, diese, zu dem Sch\u00f6pfer leiten,", "tokens": ["Zu", "dem", "Ur\u00b7bild", ",", "der", "Na\u00b7tur", ",", "die\u00b7se", ",", "zu", "dem", "Sch\u00f6p\u00b7fer", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,", "PDS", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Jhn zu ehren, als die Quell der erschaffnen Seltenheiten.", "tokens": ["Jhn", "zu", "eh\u00b7ren", ",", "als", "die", "Quell", "der", "er\u00b7schaff\u00b7nen", "Sel\u00b7ten\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}