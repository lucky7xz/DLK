{"dta.poem.10217": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Bl\u00fcmekens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Kleine Bl&#252;ten, anspruchslose Blumen,             ", "tokens": ["Klei\u00b7ne", "Bl", "&#252;", "ten", ",", "an\u00b7spruchs\u00b7lo\u00b7se", "Blu\u00b7men", ","], "token_info": ["word", "word", "XML_entity", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "FM", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Waldrandschmuck und Wiesendurcheinander,", "tokens": ["Wald\u00b7rand\u00b7schmuck", "und", "Wie\u00b7sen\u00b7durch\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Rote, wei\u00dfe, gelbe, blaue Blumen", "tokens": ["Ro\u00b7te", ",", "wei\u00b7\u00dfe", ",", "gel\u00b7be", ",", "blau\u00b7e", "Blu\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nahm ich im Vorbeigehn mit nach Hause.", "tokens": ["Nahm", "ich", "im", "Vor\u00b7bei\u00b7gehn", "mit", "nach", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Kamen alte, liebe Zeiten wieder:", "tokens": ["Ka\u00b7men", "al\u00b7te", ",", "lie\u00b7be", "Zei\u00b7ten", "wie\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "$,", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auf den Feldern wehten gr\u00fcne H\u00e4lmchen,", "tokens": ["Auf", "den", "Fel\u00b7dern", "weh\u00b7ten", "gr\u00fc\u00b7ne", "H\u00e4lm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "S\u00fc\u00df im Erlenbusche sang der Stieglitz,", "tokens": ["S\u00fc\u00df", "im", "Er\u00b7len\u00b7bu\u00b7sche", "sang", "der", "Stieg\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Eine ganze Welt von Unschuld sang er", "tokens": ["Ei\u00b7ne", "gan\u00b7ze", "Welt", "von", "Un\u00b7schuld", "sang", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Mir und dir.", "tokens": ["Mir", "und", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Nun, seit Jahren, ordnen deine H\u00e4nde", "tokens": ["Nun", ",", "seit", "Jah\u00b7ren", ",", "ord\u00b7nen", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Perlenschnur und Rosen in den Haaren.", "tokens": ["Per\u00b7len\u00b7schnur", "und", "Ro\u00b7sen", "in", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie viel sch\u00f6ner, junge Frau doch schm\u00fcckten", "tokens": ["Wie", "viel", "sch\u00f6\u00b7ner", ",", "jun\u00b7ge", "Frau", "doch", "schm\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$,", "ADJA", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kleine Blumen dich, die einst wir pfl\u00fcckten,", "tokens": ["Klei\u00b7ne", "Blu\u00b7men", "dich", ",", "die", "einst", "wir", "pfl\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ich und du.", "tokens": ["Ich", "und", "du", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}