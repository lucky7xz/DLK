{"dta.poem.16243": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "297.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit jeder Sprache mehr, die du erlernst, befreist", "tokens": ["Mit", "je\u00b7der", "Spra\u00b7che", "mehr", ",", "die", "du", "er\u00b7lernst", ",", "be\u00b7freist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du einen bisdaher in dir gebundnen Geist,", "tokens": ["Du", "ei\u00b7nen", "bis\u00b7da\u00b7her", "in", "dir", "ge\u00b7bund\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der jetzo th\u00e4tig wird mit eigner Denkverbindung,", "tokens": ["Der", "jet\u00b7zo", "th\u00e4\u00b7tig", "wird", "mit", "eig\u00b7ner", "Denk\u00b7ver\u00b7bin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir aufschlie\u00dft unbekannt gewesne Weltempfindung,", "tokens": ["Dir", "auf\u00b7schlie\u00dft", "un\u00b7be\u00b7kannt", "ge\u00b7wes\u00b7ne", "Wel\u00b7temp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Empfindung, wie ein 