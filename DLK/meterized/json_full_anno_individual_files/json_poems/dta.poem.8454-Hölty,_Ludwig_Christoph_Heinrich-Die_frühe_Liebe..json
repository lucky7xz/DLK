{"dta.poem.8454": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die fr\u00fche Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Pflegten h\u00fcbsche M\u00e4gdelein", "tokens": ["Pfleg\u00b7ten", "h\u00fcb\u00b7sche", "M\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine liebste Augenweide,", "tokens": ["Mei\u00b7ne", "liebs\u00b7te", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mehr als Pupp' und Ball, zu sein.", "tokens": ["Mehr", "als", "Pupp'", "und", "Ball", ",", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "KON", "NN", "$,", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich vergass der Vogelnester,", "tokens": ["Ich", "ver\u00b7gass", "der", "Vo\u00b7gel\u00b7nes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warf mein Steckenpferd ins Gras,", "tokens": ["Warf", "mein", "Ste\u00b7cken\u00b7pferd", "ins", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann am Baum bei meiner Schwester", "tokens": ["Wann", "am", "Baum", "bei", "mei\u00b7ner", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine sch\u00f6ne Dirne sass;", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Dir\u00b7ne", "sass", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freute mich der muntern Dirne,", "tokens": ["Freu\u00b7te", "mich", "der", "mun\u00b7tern", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres rothen Wangenpaars,", "tokens": ["Ih\u00b7res", "ro\u00b7then", "Wan\u00b7gen\u00b7paars", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihres Mundes, ihrer Stirne,", "tokens": ["Ih\u00b7res", "Mun\u00b7des", ",", "ih\u00b7rer", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres blonden Lockenhaars;", "tokens": ["Ih\u00b7res", "blon\u00b7den", "Lo\u00b7cken\u00b7haars", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Blickt' auf Busentuch und Mieder,", "tokens": ["Blickt'", "auf", "Bu\u00b7sen\u00b7tuch", "und", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinterw\u00e4rts gelehnt am Baum;", "tokens": ["Hin\u00b7ter\u00b7w\u00e4rts", "ge\u00b7lehnt", "am", "Baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streckte dann ins Gras mich nieder,", "tokens": ["Streck\u00b7te", "dann", "ins", "Gras", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dicht an ihres Kleides Saum.", "tokens": ["Dicht", "an", "ih\u00b7res", "Klei\u00b7des", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ich weiland that als Knabe,", "tokens": ["Was", "ich", "wei\u00b7land", "that", "als", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werd' ich wahrlich immer thun,", "tokens": ["Werd'", "ich", "wahr\u00b7lich", "im\u00b7mer", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ich werd' im k\u00fchlen Grabe", "tokens": ["Bis", "ich", "werd'", "im", "k\u00fch\u00b7len", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neben meinen V\u00e4tern ruhn.", "tokens": ["Ne\u00b7ben", "mei\u00b7nen", "V\u00e4\u00b7tern", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}