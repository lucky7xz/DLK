{"textgrid.poem.47063": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, S\u00fcdwind, komm heran zu mir und schaue,", "tokens": ["Auf", ",", "S\u00fcd\u00b7wind", ",", "komm", "he\u00b7ran", "zu", "mir", "und", "schau\u00b7e", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "PTKVZ", "APPR", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hier, erbl\u00fcht in sch\u00f6nsten Farbentinten,", "tokens": ["Wie", "hier", ",", "er\u00b7bl\u00fcht", "in", "sch\u00f6ns\u00b7ten", "Far\u00b7ben\u00b7tin\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Winterfenster stehn drei Hyazinthen,", "tokens": ["Im", "Win\u00b7ter\u00b7fens\u00b7ter", "stehn", "drei", "Hy\u00b7a\u00b7zin\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rot eine, eine wei\u00df' und eine blaue.", "tokens": ["Rot", "ei\u00b7ne", ",", "ei\u00b7ne", "wei\u00df'", "und", "ei\u00b7ne", "blau\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "ART", "ADJA", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sch\u00fcttl' ihre duft'gen Glocken und trag' laue", "tokens": ["Sch\u00fcttl'", "ih\u00b7re", "duft'\u00b7gen", "Glo\u00b7cken", "und", "trag'", "lau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "ADJA"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gew\u00fcrze hin zu meiner Kaltgesinnten,", "tokens": ["Ge\u00b7w\u00fcr\u00b7ze", "hin", "zu", "mei\u00b7ner", "Kalt\u00b7ge\u00b7sinn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort, wo sie schl\u00e4ft, in ihrer Kammer hinten,", "tokens": ["Dort", ",", "wo", "sie", "schl\u00e4ft", ",", "in", "ih\u00b7rer", "Kam\u00b7mer", "hin\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "R\u00fchr' ihr bereiftes Fenster an und taue.", "tokens": ["R\u00fchr'", "ihr", "be\u00b7reif\u00b7tes", "Fens\u00b7ter", "an", "und", "tau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tau' dich hinein bis hin zu ihrem Schlafe,", "tokens": ["Tau'", "dich", "hin\u00b7ein", "bis", "hin", "zu", "ih\u00b7rem", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und findest du ihr Herz, wie es umstricket", "tokens": ["Und", "fin\u00b7dest", "du", "ihr", "Herz", ",", "wie", "es", "um\u00b7stri\u00b7cket"], "token_info": ["word", "word", "word"