{"dta.poem.9153": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n Der ungewissen Liebhaber Nacht-Gedancken.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich armes kind! wie einsam mu\u00df ich leben/", "tokens": ["Ich", "ar\u00b7mes", "kind", "!", "wie", "ein\u00b7sam", "mu\u00df", "ich", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PWAV", "ADJD", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie mu\u00df ich noch in eitel hoffnung schweben/", "tokens": ["Wie", "mu\u00df", "ich", "noch", "in", "ei\u00b7tel", "hoff\u00b7nung", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich dencke viel/ und wei\u00df doch nicht wohin/", "tokens": ["Ich", "den\u00b7cke", "viel", "/", "und", "wei\u00df", "doch", "nicht", "wo\u00b7hin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es wundert mich da\u00df ich so lustig bin.", "tokens": ["Es", "wun\u00b7dert", "mich", "da\u00df", "ich", "so", "lus\u00b7tig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2. Wo werd ich noch mein ruhepl\u00e4tzgen finden/", "tokens": ["Wo", "werd", "ich", "noch", "mein", "ru\u00b7he\u00b7pl\u00e4tz\u00b7gen", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und welcher ort wird sich mit mir verbinden/", "tokens": ["Und", "wel\u00b7cher", "ort", "wird", "sich", "mit", "mir", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich blicke zwar ins liebste vaterland;", "tokens": ["Ich", "bli\u00b7cke", "zwar", "ins", "liebs\u00b7te", "va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch Gottes rath ist mir noch unbekannt.", "tokens": ["Doch", "Got\u00b7tes", "rath", "ist", "mir", "noch", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "3. Wie mu\u00df wol itzt das liebe seelgen liegen/", "tokens": ["Wie", "mu\u00df", "wol", "itzt", "das", "lie\u00b7be", "seel\u00b7gen", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das mich einmahl im lieben soll vergn\u00fcgen/", "tokens": ["Das", "mich", "ein\u00b7mahl", "im", "lie\u00b7ben", "soll", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPRART", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie liegt vielleicht in ihrer sanfften ruh/", "tokens": ["Sie", "liegt", "viel\u00b7leicht", "in", "ih\u00b7rer", "sanff\u00b7ten", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dr\u00fcckt das liecht der sch\u00f6nen augen zu.", "tokens": ["Und", "dr\u00fcckt", "das", "liecht", "der", "sch\u00f6\u00b7nen", "au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "4. Sie wird an mich den abend nicht gedencken/", "tokens": ["Sie", "wird", "an", "mich", "den", "a\u00b7bend", "nicht", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "ADV", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich mich mu\u00df im Vorrath gleichsam kr\u00e4ncken/", "tokens": ["Da", "ich", "mich", "mu\u00df", "im", "Vor\u00b7rath", "gleich\u00b7sam", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPRART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo sie ja den kopff ietzund zerbricht", "tokens": ["Und", "wo", "sie", "ja", "den", "kopff", "ie\u00b7tzund", "zer\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wei\u00df sie doch von meiner liebe nicht.", "tokens": ["So", "wei\u00df", "sie", "doch", "von", "mei\u00b7ner", "lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "5. Das ist gewi\u00df/ die m\u00e4dgen so ich kenne/", "tokens": ["Das", "ist", "ge\u00b7wi\u00df", "/", "die", "m\u00e4d\u00b7gen", "so", "ich", "ken\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$(", "ART", "ADJA", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die ich schon mit ihren namen nenne/", "tokens": ["Und", "die", "ich", "schon", "mit", "ih\u00b7ren", "na\u00b7men", "nen\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die sind mir wohl ohn zweiffel nicht beschert/", "tokens": ["Die", "sind", "mir", "wohl", "ohn", "zweif\u00b7fel", "nicht", "be\u00b7schert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich habe sie auch nie so weit begehrt.", "tokens": ["Ich", "ha\u00b7be", "sie", "auch", "nie", "so", "weit", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "6. Es ist noch zeit/ da\u00df ich ein kind erw\u00e4hle/", "tokens": ["Es", "ist", "noch", "zeit", "/", "da\u00df", "ich", "ein", "kind", "er\u00b7w\u00e4h\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am leibe sch\u00f6n und sittsam an der seele:", "tokens": ["Am", "lei\u00b7be", "sch\u00f6n", "und", "sitt\u00b7sam", "an", "der", "see\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Di\u00df ist genug? sonst sey sie arm und reich/", "tokens": ["Di\u00df", "ist", "ge\u00b7nug", "?", "sonst", "sey", "sie", "arm", "und", "reich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gro\u00df oder klein/ es gilt mir alles gleich.", "tokens": ["Gro\u00df", "o\u00b7der", "klein", "/", "es", "gilt", "mir", "al\u00b7les", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$(", "PPER", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "7. Wiewol ich kan mein wort nicht von mir geben/", "tokens": ["Wie\u00b7wol", "ich", "kan", "mein", "wort", "nicht", "von", "mir", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer wei\u00df/ ob ich das gl\u00fccke werd erleben", "tokens": ["Wer", "wei\u00df", "/", "ob", "ich", "das", "gl\u00fc\u00b7cke", "werd", "er\u00b7le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PDS", "VVFIN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie mancher giebt der eitelkeit valet", "tokens": ["Wie", "man\u00b7cher", "giebt", "der", "ei\u00b7tel\u00b7keit", "va\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eh er die spur der s\u00fcssen liebe geht.", "tokens": ["Eh", "er", "die", "spur", "der", "s\u00fcs\u00b7sen", "lie\u00b7be", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "VVFIN", "VVFIN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.8": {"line.1": {"text": "8. Es mag drum seyn/ ich bin bereit zu allen/", "tokens": ["Es", "mag", "drum", "seyn", "/", "ich", "bin", "be\u00b7reit", "zu", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "VAINF", "$(", "PPER", "VAFIN", "ADJD", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Himmels-schlu\u00df sol mir allzeit gefallen/", "tokens": ["Des", "Him\u00b7mels\u00b7schlu\u00df", "sol", "mir", "all\u00b7zeit", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich streite nicht/ ich bleibe gern allein", "tokens": ["Ich", "strei\u00b7te", "nicht", "/", "ich", "blei\u00b7be", "gern", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wil auch gern der liebe dienstbar seyn.", "tokens": ["Ich", "wil", "auch", "gern", "der", "lie\u00b7be", "dienst\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "9. Dieselbe mag es nach belieben karten/", "tokens": ["Die\u00b7sel\u00b7be", "mag", "es", "nach", "be\u00b7lie\u00b7ben", "kar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VMFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wil den trost in aller still erwarten/", "tokens": ["Ich", "wil", "den", "trost", "in", "al\u00b7ler", "still", "er\u00b7war\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PIAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schlaf wohl mein kind/ mein unbekanntes ich/", "tokens": ["Schlaf", "wohl", "mein", "kind", "/", "mein", "un\u00b7be\u00b7kann\u00b7tes", "ich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schlaf du nur wol/ ich wache schon vor dich.", "tokens": ["Schlaf", "du", "nur", "wol", "/", "ich", "wa\u00b7che", "schon", "vor", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "$(", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}