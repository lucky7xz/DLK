{"dta.poem.19410": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "143.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein eigner Zauber liegt im langgewohnten Alten,", "tokens": ["Ein", "eig\u00b7ner", "Zau\u00b7ber", "liegt", "im", "lang\u00b7ge\u00b7wohn\u00b7ten", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auch im Neuen ist ein eigner Reiz enthalten.", "tokens": ["Doch", "auch", "im", "Neu\u00b7en", "ist", "ein", "eig\u00b7ner", "Reiz", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du l\u00e4ssest bald von dem, dich bald von dem verf\u00fchren,", "tokens": ["Du", "l\u00e4s\u00b7sest", "bald", "von", "dem", ",", "dich", "bald", "von", "dem", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "$,", "PPER", "ADV", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie etwas dort dein Herz, hier deinen Sinn mag r\u00fchren.", "tokens": ["Wie", "et\u00b7was", "dort", "dein", "Herz", ",", "hier", "dei\u00b7nen", "Sinn", "mag", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Welt in Zwiespalt h\u00e4ngt halb ab von Vorurtheilen,", "tokens": ["Die", "Welt", "in", "Zwies\u00b7palt", "h\u00e4ngt", "halb", "ab", "von", "Vor\u00b7urt\u00b7hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADJD", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halb Neuerungen nach, nicht hier noch dort zu heilen.", "tokens": ["Halb", "Neu\u00b7e\u00b7run\u00b7gen", "nach", ",", "nicht", "hier", "noch", "dort", "zu", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKVZ", "$,", "PTKNEG", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer zwischen Neu und Alt sich in der Mitte h\u00e4lt", "tokens": ["Wer", "zwi\u00b7schen", "Neu", "und", "Alt", "sich", "in", "der", "Mit\u00b7te", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NE", "KON", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An das was gilt wie galt, vermittelt erst die Welt.", "tokens": ["An", "das", "was", "gilt", "wie", "galt", ",", "ver\u00b7mit\u00b7telt", "erst", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VVFIN", "KOKOM", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}