{"dta.poem.2777": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus des Hugonis gottseeligen Begierden/  \n  Desiderium V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So ist Vergessenheit im Himmel auch zu finden/", "tokens": ["So", "ist", "Ver\u00b7ges\u00b7sen\u00b7heit", "im", "Him\u00b7mel", "auch", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch Ganymedes hat den Schlaf-Trunck einge-", "tokens": ["Welch", "Ga\u00b7ny\u00b7me\u00b7des", "hat", "den", "Schlaf\u00b7Trunck", "ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "TRUNC"], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Wie da\u00df die G\u00f6tter nicht mehr unsre Werck\u2019 ergr\u00fcnden/", "tokens": ["Wie", "da\u00df", "die", "G\u00f6t\u00b7ter", "nicht", "mehr", "uns\u00b7re", "Werck", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat denn die Himmlischen der Lethe-Flu\u00df getr\u00e4nckt.", "tokens": ["Hat", "denn", "die", "Himm\u00b7li\u00b7schen", "der", "Lethe\u00b7Flu\u00df", "ge\u00b7tr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Vergist es/ oder scheints/ mein Licht/ nur zu vergessen/", "tokens": ["Ver\u00b7gist", "es", "/", "o\u00b7der", "scheints", "/", "mein", "Licht", "/", "nur", "zu", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KON", "VVFIN", "$(", "PPOSAT", "NN", "$(", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn es in Zweiffel zeucht das Werck so es gemacht.", "tokens": ["Wenn", "es", "in", "Zweif\u00b7fel", "zeucht", "das", "Werck", "so", "es", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ists! so entdeck ichs bald! wo nicht/ wie zu ermessen/", "tokens": ["Ists", "!", "so", "ent\u00b7deck", "ichs", "bald", "!", "wo", "nicht", "/", "wie", "zu", "er\u00b7mes\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "PIS", "ADV", "$.", "PWAV", "PTKNEG", "$(", "KOKOM", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus einem Erdenklo\u00df bin ich von dir erdacht.", "tokens": ["Aus", "ei\u00b7nem", "Er\u00b7den\u00b7klo\u00df", "bin", "ich", "von", "dir", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und fragstu wo? der Ort ist kundbar aller Enden/", "tokens": ["Und", "frags\u00b7tu", "wo", "?", "der", "Ort", "ist", "kund\u00b7bar", "al\u00b7ler", "En\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWAV", "$.", "ART", "NN", "VAFIN", "ADJD", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Stund nicht ein Garten da/ wo unser Anherr schlief/", "tokens": ["Stund", "nicht", "ein", "Gar\u00b7ten", "da", "/", "wo", "un\u00b7ser", "An\u00b7herr", "schlief", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ADV", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Pflag nicht ein klarer Bronn sein Silber auszusenden/", "tokens": ["Pflag", "nicht", "ein", "kla\u00b7rer", "Bronn", "sein", "Sil\u00b7ber", "aus\u00b7zu\u00b7sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das von vier R\u00f6hren starck durch Blum und Kr\u00e4uterlief.", "tokens": ["Das", "von", "vier", "R\u00f6h\u00b7ren", "starck", "durch", "Blum", "und", "Kr\u00e4u\u00b7ter\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "CARD", "NN", "ADJD", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Begehrstu auch die Zeit? es war nach wenig Tagen/", "tokens": ["Be\u00b7gehrs\u00b7tu", "auch", "die", "Zeit", "?", "es", "war", "nach", "we\u00b7nig", "Ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als mit des Meeres Tamm die Erde sich gesch\u00fctzt.", "tokens": ["Als", "mit", "des", "Mee\u00b7res", "Tamm", "die", "Er\u00b7de", "sich", "ge\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NE", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was mehr/ du magst darumb nur die Geschichte fragen/", "tokens": ["Was", "mehr", "/", "du", "magst", "da\u00b7rumb", "nur", "die", "Ge\u00b7schich\u00b7te", "fra\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$(", "PPER", "VMFIN", "PAV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es war der Wunder Klo\u00df roth als mit Blut bespritzt.", "tokens": ["Es", "war", "der", "Wun\u00b7der", "Klo\u00df", "roth", "als", "mit", "Blut", "be\u00b7spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADJD", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du hast drey Finger Sand darzu noch untermenget/", "tokens": ["Du", "hast", "drey", "Fin\u00b7ger", "Sand", "dar\u00b7zu", "noch", "un\u00b7ter\u00b7men\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "NN", "PAV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Di\u00df war der erste Zeugk aus dem mein C\u00f6rper stieg/", "tokens": ["Di\u00df", "war", "der", "ers\u00b7te", "Zeugk", "aus", "dem", "mein", "C\u00f6r\u00b7per", "stieg", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So da\u00df an der Geburt mein Ausgang gleichfals hanget.", "tokens": ["So", "da\u00df", "an", "der", "Ge\u00b7burt", "mein", "Aus\u00b7gang", "gleich\u00b7fals", "han\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Der ich aus Staube kam/ im Staube wieder lig.", "tokens": ["Der", "ich", "aus", "Stau\u00b7be", "kam", "/", "im", "Stau\u00b7be", "wie\u00b7der", "lig."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$(", "APPRART", "NN", "ADV", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ein T\u00f6pffer wenn er Tohn auf Samos Ackern siehet/", "tokens": ["Ein", "T\u00f6pf\u00b7fer", "wenn", "er", "Tohn", "auf", "Sa\u00b7mos", "A\u00b7ckern", "sie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "NN", "APPR", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So f\u00fchrt er ein Gef\u00e4\u00df aus leichter Erden auf/", "tokens": ["So", "f\u00fchrt", "er", "ein", "Ge\u00b7f\u00e4\u00df", "aus", "leich\u00b7ter", "Er\u00b7den", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und ob er Anfangs zwar mit Graben sich bem\u00fchet/", "tokens": ["Und", "ob", "er", "An\u00b7fangs", "zwar", "mit", "Gra\u00b7ben", "sich", "be\u00b7m\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bi\u00df da\u00df die Erde reich wird durch des Wassers Lauf/", "tokens": ["Bi\u00df", "da\u00df", "die", "Er\u00b7de", "reich", "wird", "durch", "des", "Was\u00b7sers", "Lauf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "ADJD", "VAFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "So drehet er hernach die niemals stille Scheibe/", "tokens": ["So", "dre\u00b7het", "er", "her\u00b7nach", "die", "nie\u00b7mals", "stil\u00b7le", "Schei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bi\u00df da\u00df ein grosser Krug w\u00e4chst unter seiner Hand/", "tokens": ["Bi\u00df", "da\u00df", "ein", "gros\u00b7ser", "Krug", "w\u00e4chst", "un\u00b7ter", "sei\u00b7ner", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er w\u00e4chst/ doch da\u00df er kaum dieselbe Stunde bleibe/", "tokens": ["Er", "w\u00e4chst", "/", "doch", "da\u00df", "er", "kaum", "die\u00b7sel\u00b7be", "Stun\u00b7de", "blei\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "KOUS", "PPER", "ADV", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dann brichts und wird wie vor ein wenig Koth und Sand.", "tokens": ["Dann", "brichts", "und", "wird", "wie", "vor", "ein", "we\u00b7nig", "Koth", "und", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VAFIN", "KOKOM", "APPR", "ART", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auf wenig besserm Grund steht mein gebrechlich Leben/", "tokens": ["Auf", "we\u00b7nig", "bes\u00b7serm", "Grund", "steht", "mein", "ge\u00b7brech\u00b7lich", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein leichter Schatten wird die letzten Tag umbziehn/", "tokens": ["Ein", "leich\u00b7ter", "Schat\u00b7ten", "wird", "die", "letz\u00b7ten", "Tag", "um\u00b7bziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Warumb da vor sich selbst die Jahre bald erheben/", "tokens": ["Wa\u00b7rumb", "da", "vor", "sich", "selbst", "die", "Jah\u00b7re", "bald", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PRF", "ADV", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Will denn die Zeit so schnell mit ihren Pferden fliehn.", "tokens": ["Will", "denn", "die", "Zeit", "so", "schnell", "mit", "ih\u00b7ren", "Pfer\u00b7den", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Staub ist all zu sehr geneiget zum Verderben/", "tokens": ["Mein", "Staub", "ist", "all", "zu", "sehr", "ge\u00b7nei\u00b7get", "zum", "Ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "PTKA", "ADV", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mein Leben w\u00e4r es gleich/ nicht dr\u00fccket da verschwindt/", "tokens": ["Mein", "Le\u00b7ben", "w\u00e4r", "es", "gleich", "/", "nicht", "dr\u00fc\u00b7cket", "da", "ver\u00b7schwindt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$(", "PTKNEG", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df von Crystallen nicht die dort den Himmel f\u00e4rben/", "tokens": ["Da\u00df", "von", "Crys\u00b7tal\u00b7len", "nicht", "die", "dort", "den", "Him\u00b7mel", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PTKNEG", "ART", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Glieder mir gemacht und angesetzet sind.", "tokens": ["Die", "Glie\u00b7der", "mir", "ge\u00b7macht", "und", "an\u00b7ge\u00b7set\u00b7zet", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ach da\u00df ich doch nicht bin als wie die lichten Sterne", "tokens": ["Ach", "da\u00df", "ich", "doch", "nicht", "bin", "als", "wie", "die", "lich\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "PTKNEG", "VAFIN", "KOUS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Gott geschaffen hat aus Spiegel-heller Fluth/", "tokens": ["Die", "Gott", "ge\u00b7schaf\u00b7fen", "hat", "aus", "Spie\u00b7gel\u00b7hel\u00b7ler", "Fluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Mein Leben englisch ist und von dem C\u00f6rper ferne", "tokens": ["Mein", "Le\u00b7ben", "eng\u00b7lisch", "ist", "und", "von", "dem", "C\u00f6r\u00b7per", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "KON", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie in Elysien der heilgen Seelen Glut/", "tokens": ["Wie", "in", "E\u00b7ly\u00b7si\u00b7en", "der", "heil\u00b7gen", "See\u00b7len", "Glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandri