{"textgrid.poem.36718": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Apollo von Belvedere", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6ttlicher Sieger, du z\u00fcrnst, dein Angesicht flammet von Unmuth?", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Sie\u00b7ger", ",", "du", "z\u00fcrnst", ",", "dein", "An\u00b7ge\u00b7sicht", "flam\u00b7met", "von", "Un\u00b7muth", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ist's, weil die bessere Welt, weil der Olymp dir entflohn?", "tokens": ["Ist's", ",", "weil", "die", "bes\u00b7se\u00b7re", "Welt", ",", "weil", "der", "O\u00b7lymp", "dir", "ent\u00b7flohn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Ach dir nahen die Musen nicht mehr, du z\u00fcrnst zu gewaltig,", "tokens": ["Ach", "dir", "na\u00b7hen", "die", "Mu\u00b7sen", "nicht", "mehr", ",", "du", "z\u00fcrnst", "zu", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "ART", "NN", "PTKNEG", "ADV", "$,", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ach das verdorbene Geschlecht schirmet Apollo nicht mehr.", "tokens": ["Ach", "das", "ver\u00b7dor\u00b7be\u00b7ne", "Ge\u00b7schlecht", "sch