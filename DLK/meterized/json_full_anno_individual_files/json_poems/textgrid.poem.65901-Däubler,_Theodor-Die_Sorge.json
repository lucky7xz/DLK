{"textgrid.poem.65901": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Sorge", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den H\u00e4nden Demeters entgleiten Garben,", "tokens": ["Den", "H\u00e4n\u00b7den", "De\u00b7me\u00b7ters", "ent\u00b7glei\u00b7ten", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Finger kr\u00fcmmt sich, der die Sichel h\u00e4lt:", "tokens": ["Der", "Fin\u00b7ger", "kr\u00fcmmt", "sich", ",", "der", "die", "Si\u00b7chel", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Besorgtheit wogt das Feld. Verzweiflung. Viel", "tokens": ["Be\u00b7sorg\u00b7theit", "wogt", "das", "Feld", ".", "Ver\u00b7zwei\u00b7flung", ".", "Viel"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geknicktheit wettert hin, wo Schwache starben.", "tokens": ["Ge\u00b7knickt\u00b7heit", "wet\u00b7tert", "hin", ",", "wo", "Schwa\u00b7che", "star\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Land verschwanden nicht des Herbstes Farben;", "tokens": ["Im", "Land", "ver\u00b7schwan\u00b7den", "nicht", "des", "Herbs\u00b7tes", "Far\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Hadespurpur naht als sanftes Ziel,", "tokens": ["Wie", "Ha\u00b7des\u00b7pur\u00b7pur", "naht", "als", "sanf\u00b7tes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zerbluten, doch entflammen auch \u2013 bei Spiel \u2013", "tokens": ["Zer\u00b7blu\u00b7ten", ",", "doch", "ent\u00b7flam\u00b7men", "auch", "\u2013", "bei", "Spiel", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "ADV", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Daktylen oft, wenn Blutgesch\u00f6pfe darben.", "tokens": ["Dak\u00b7ty\u00b7len", "oft", ",", "wenn", "Blut\u00b7ge\u00b7sch\u00f6p\u00b7fe", "dar\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo Demeter nun ruht, versinkt als Stufe", "tokens": ["Wo", "De\u00b7me\u00b7ter", "nun", "ruht", ",", "ver\u00b7sinkt", "als", "Stu\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "$,", "VVFIN", "KOUS", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Zum Reich Erbleichender, beseelt der Stein.", "tokens": ["Zum", "Reich", "Er\u00b7blei\u00b7chen\u00b7der", ",", "be\u00b7seelt", "der", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN"