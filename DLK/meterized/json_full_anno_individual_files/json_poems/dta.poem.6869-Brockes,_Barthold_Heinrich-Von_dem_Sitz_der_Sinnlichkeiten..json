{"dta.poem.6869": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Wesen des Gehirns, und wieder r\u00fcckwerts f\u00fchret", "tokens": ["Jm", "We\u00b7sen", "des", "Ge\u00b7hirns", ",", "und", "wie\u00b7der", "r\u00fcck\u00b7werts", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KON", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie vorhin gedacht;", "tokens": ["Was", "sie", "vor\u00b7hin", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Formiret unsre Seel, obgleich der Leib nicht wacht,", "tokens": ["For\u00b7mi\u00b7ret", "uns\u00b7re", "Seel", ",", "ob\u00b7gleich", "der", "Leib", "nicht", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn da\u00df was \u00e4usserlichs darbey;", "tokens": ["Ohn", "da\u00df", "was", "\u00e4us\u00b7ser\u00b7lichs", "dar\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In sich doch eine Schilderey.", "tokens": ["In", "sich", "doch", "ei\u00b7ne", "Schil\u00b7de\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch diese Erd-Me\u00df-Kunst, die sicher und nat\u00fcrlich,", "tokens": ["Durch", "die\u00b7se", "Erd\u00b7Me\u00df\u00b7Kunst", ",", "die", "si\u00b7cher", "und", "na\u00b7t\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Stellt sie den weiten Creis sich deutlich und fig\u00fcrlich,", "tokens": ["Stellt", "sie", "den", "wei\u00b7ten", "Creis", "sich", "deut\u00b7lich", "und", "fi\u00b7g\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So, wie sie ihn des Tages sahe, f\u00fcr:", "tokens": ["So", ",", "wie", "sie", "ihn", "des", "Ta\u00b7ges", "sa\u00b7he", ",", "f\u00fcr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und zeiget durch die Z\u00fcg\u2019, die sie behielt, in ihr,", "tokens": ["Und", "zei\u00b7get", "durch", "die", "Z\u00fcg'", ",", "die", "sie", "be\u00b7hielt", ",", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Des Himmels und der Erden Zier.", "tokens": ["Des", "Him\u00b7mels", "und", "der", "Er\u00b7den", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}