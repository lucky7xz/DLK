{"textgrid.poem.37737": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abendempfindung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Tage nehmen ab!", "tokens": ["Wie", "die", "Ta\u00b7ge", "neh\u00b7men", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkel wird's hier wie ein Grab,", "tokens": ["Dun\u00b7kel", "wird's", "hier", "wie", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abend gl\u00e4nzt, die B\u00e4um am Fenster", "tokens": ["A\u00b7bend", "gl\u00e4nzt", ",", "die", "B\u00e4um", "am", "Fens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Funkeln, rauschen hin und her,", "tokens": ["Fun\u00b7keln", ",", "rau\u00b7schen", "hin", "und", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Schwalben, wie Gespenster", "tokens": ["Und", "die", "Schwal\u00b7ben", ",", "wie", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schiessen pfeilschnell kreutz und quer.", "tokens": ["Schies\u00b7sen", "pfeil\u00b7schnell", "kreutz", "und", "quer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In den B\u00fcchern wird es tr\u00fcbe", "tokens": ["In", "den", "B\u00fc\u00b7chern", "wird", "es", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller Sinn mir fast vergeht,", "tokens": ["Al\u00b7ler", "Sinn", "mir", "fast", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Zwielicht scheut der Weisheit Liebe,", "tokens": ["Zwie\u00b7licht", "scheut", "der", "Weis\u00b7heit", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lieb im Freyen sich ergeht;", "tokens": ["Lieb", "im", "Frey\u00b7en", "sich", "er\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die N\u00e4chte nehmen zu", "tokens": ["Und", "die", "N\u00e4ch\u00b7te", "neh\u00b7men", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Zu der Liebe Lust und Ruh.", "tokens": ["Zu", "der", "Lie\u00b7be", "Lust", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie die Tage nehm