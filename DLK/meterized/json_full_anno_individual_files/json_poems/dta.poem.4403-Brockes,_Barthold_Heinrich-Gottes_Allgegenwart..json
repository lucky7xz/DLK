{"dta.poem.4403": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Allgegenwart.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grosser GOtt! ich stehe stille,", "tokens": ["Gros\u00b7ser", "Gott", "!", "ich", "ste\u00b7he", "stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erstaun\u2019 ob aller F\u00fclle", "tokens": ["Und", "er\u00b7staun'", "ob", "al\u00b7ler", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Vollenkommenheit,", "tokens": ["Al\u00b7ler", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Pracht und Lieblichkeit,", "tokens": ["Al\u00b7ler", "Pracht", "und", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ich, wo ich geh\u2019 und stehe,", "tokens": ["Die", "ich", ",", "wo", "ich", "geh'", "und", "ste\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Ergetzen h\u00f6r\u2019 und sehe,", "tokens": ["Mit", "Er\u00b7get\u00b7zen", "h\u00f6r'", "und", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Sonderlich zu dieser Zeit.", "tokens": ["Son\u00b7der\u00b7lich", "zu", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df mich sehen, la\u00df mich h\u00f6ren,", "tokens": ["La\u00df", "mich", "se\u00b7hen", ",", "la\u00df", "mich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Grosses All, zu Deinen Ehren,", "tokens": ["Gros\u00b7ses", "All", ",", "zu", "Dei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles, was ich h\u00f6r\u2019 und sehe!", "tokens": ["Al\u00b7les", ",", "was", "ich", "h\u00f6r'", "und", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}