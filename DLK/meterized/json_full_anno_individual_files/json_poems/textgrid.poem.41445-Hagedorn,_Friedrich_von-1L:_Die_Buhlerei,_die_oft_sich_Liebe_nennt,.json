{"textgrid.poem.41445": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Buhlerei, die oft sich Liebe nennt,", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Buhlerei, die oft sich Liebe nennt,", "tokens": ["Die", "Buh\u00b7le\u00b7rei", ",", "die", "oft", "sich", "Lie\u00b7be", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erscheint, vermummt, an einem Fest der Freude,", "tokens": ["Er\u00b7scheint", ",", "ver\u00b7mummt", ",", "an", "ei\u00b7nem", "Fest", "der", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im schlausten Putz, in Neadarnens Kleide,", "tokens": ["Im", "schlaus\u00b7ten", "Putz", ",", "in", "Nea\u00b7dar\u00b7nens", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wovon den Werth sie und die Jugend kennt.", "tokens": ["Wo\u00b7von", "den", "Werth", "sie", "und", "die", "Ju\u00b7gend", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Auge spielt, die freien Blicke fliegen,", "tokens": ["Ihr", "Au\u00b7ge", "spielt", ",", "die", "frei\u00b7en", "Bli\u00b7cke", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Herz und Brust voll wilder Seufzer wallt:", "tokens": ["Wie", "Herz", "und", "Brust", "voll", "wil\u00b7der", "Seuf\u00b7zer", "wallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Lust, Vorwitz, Scherz, Bewundrung und Vergn\u00fcgen", "tokens": ["Lust", ",", "Vor\u00b7witz", ",", "Scherz", ",", "Be\u00b7wund\u00b7rung", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fliehn schnell herzu, und loben die Gestalt.", "tokens": ["Fliehn", "schnell", "her\u00b7zu", ",", "und", "lo\u00b7ben", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Man folgt nur ihr, den holden Reiz zu sehen.", "tokens": ["Man", "folgt", "nur", "ihr", ",", "den", "hol\u00b7den", "Reiz", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPER", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wer sieht nicht gern, was so gefallen kann?", "tokens": ["Wer", "sieht", "nicht", "gern", ",", "was", "so", "ge\u00b7fal\u00b7len", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie neigt sich tief, um sch\u00f6ner wegzugehen.", "tokens": ["Sie", "neigt", "sich", "tief", ",", "um", "sch\u00f6\u00b7ner", "weg\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "KOUI", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da fragt die Lust: Wo treffen wir dich an?", "tokens": ["Da", "fragt", "die", "Lust", ":", "Wo", "tref\u00b7fen", "wir", "dich", "an", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Entdeck es frei. Dich nimmer zu verfehlen,", "tokens": ["Ent\u00b7deck", "es", "frei", ".", "Dich", "nim\u00b7mer", "zu", "ver\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$.", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dich oft zu sehn, w\u00fcnscht niemand mehr, als ich.", "tokens": ["Dich", "oft", "zu", "sehn", ",", "w\u00fcnscht", "nie\u00b7mand", "mehr", ",", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sie lacht und spricht: Wer kann sich Nachbarn w\u00e4hlen?", "