{"dta.poem.8684": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Unbest\u00e4ndigkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich dies und das beginne,", "tokens": ["Da\u00df", "ich", "dies", "und", "das", "be\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "KON", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute g'rad und morgen quer,", "tokens": ["Heu\u00b7te", "g'\u00b7rad", "und", "mor\u00b7gen", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Gegen das, was heut' ich minne,", "tokens": ["Ge\u00b7gen", "das", ",", "was", "heut'", "ich", "min\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen richte Spie\u00df und Speer:", "tokens": ["Mor\u00b7gen", "rich\u00b7te", "Spie\u00df", "und", "Speer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sollte das so sehr dich wundern,", "tokens": ["Soll\u00b7te", "das", "so", "sehr", "dich", "wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du mein consequenter Mann?!", "tokens": ["Du", "mein", "con\u00b7se\u00b7quen\u00b7ter", "Mann", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner von den Erdenplundern", "tokens": ["Kei\u00b7ner", "von", "den", "Er\u00b7den\u00b7plun\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange mich behalten kann!", "tokens": ["Lan\u00b7ge", "mich", "be\u00b7hal\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heute bin ich zum Exempel", "tokens": ["Heu\u00b7te", "bin", "ich", "zum", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ganz ein Metaphysikus;", "tokens": ["Ganz", "ein", "Me\u00b7ta\u00b7phy\u00b7si\u00b7kus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen schallt in Themis Tempel", "tokens": ["Mor\u00b7gen", "schallt", "in", "The\u00b7mis", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein unsteter Menschenfu\u00df.", "tokens": ["Mein", "un\u00b7ste\u00b7ter", "Men\u00b7schen\u00b7fu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Heute steh' ich Nachts am Giebel,", "tokens": ["Heu\u00b7te", "steh'", "ich", "Nachts", "am", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suche Jungfrau, Stier und B\u00e4r;", "tokens": ["Su\u00b7che", "Jung\u00b7frau", ",", "Stier", "und", "B\u00e4r", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen les' ich in der Bibel,", "tokens": ["Mor\u00b7gen", "les'", "ich", "in", "der", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uebermorgen im Homer.", "tokens": ["Ue\u00b7ber\u00b7mor\u00b7gen", "im", "Ho\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Blickt mein Geist im Wissensdrange", "tokens": ["Blickt", "mein", "Geist", "im", "Wis\u00b7sens\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "O dann pa\u00dft er auch nicht lange,", "tokens": ["O", "dann", "pa\u00dft", "er", "auch", "nicht", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht er drinnen nichts erhellt;", "tokens": ["Sieht", "er", "drin\u00b7nen", "nichts", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und er guckt zu einem andern", "tokens": ["Und", "er", "guckt", "zu", "ei\u00b7nem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die finstre Welt hinein;", "tokens": ["In", "die", "finst\u00b7re", "Welt", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df von hier auch weiter wandern,", "tokens": ["Mu\u00df", "von", "hier", "auch", "wei\u00b7ter", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nirgends auch nur Lampenschein!", "tokens": ["Nir\u00b7gends", "auch", "nur", "Lam\u00b7pen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Freilich, wenn du unabwendig", "tokens": ["Frei\u00b7lich", ",", "wenn", "du", "un\u00b7ab\u00b7wen\u00b7dig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starrest in dasselbe Loch,", "tokens": ["Star\u00b7rest", "in", "das\u00b7sel\u00b7be", "Loch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird's vor deinem Blick lebendig,", "tokens": ["Wird's", "vor", "dei\u00b7nem", "Blick", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Ausharren lohnt sich doch;", "tokens": ["Dein", "Aus\u00b7har\u00b7ren", "lohnt", "sich", "doch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn die Augen dir erlahmen,", "tokens": ["Denn", "die", "Au\u00b7gen", "dir", "er\u00b7lah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Gespenster malen sich", "tokens": ["Und", "Ge\u00b7spens\u00b7ter", "ma\u00b7len", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Fensters leeren Rahmen:", "tokens": ["In", "des", "Fens\u00b7ters", "lee\u00b7ren", "Rah\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und man nennt den Weisen dich!", "tokens": ["Und", "man", "nennt", "den", "Wei\u00b7sen", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}