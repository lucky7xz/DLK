{"textgrid.poem.25068": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch lebst du, sch\u00f6ner Gott des Lichts! Ob auch", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch lebst du, sch\u00f6ner Gott des Lichts! Ob auch", "tokens": ["Noch", "lebst", "du", ",", "sch\u00f6\u00b7ner", "Gott", "des", "Lichts", "!", "Ob", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$.", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein letzter Tempel l\u00e4ngst zerfallen", "tokens": ["Dein", "letz\u00b7ter", "Tem\u00b7pel", "l\u00e4ngst", "zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nie mehr bei der Lyderfl\u00f6ten Hauch", "tokens": ["Und", "nie", "mehr", "bei", "der", "Ly\u00b7der\u00b7fl\u00f6\u00b7ten", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Delphi fromme Ch\u00f6re schallen:", "tokens": ["In", "Del\u00b7phi", "from\u00b7me", "Ch\u00f6\u00b7re", "schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noh flammen Hellas' Felsh\u00f6hn dir, Apoll,", "tokens": ["Noh", "flam\u00b7men", "Hel\u00b7las'", "Fels\u00b7h\u00f6hn", "dir", ",", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bei jedem Fr\u00fchrot als Alt\u00e4re,", "tokens": ["Bei", "je\u00b7dem", "Fr\u00fch\u00b7rot", "als", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch donnern bei Korinth mit Flutgeroll", "tokens": ["Noch", "don\u00b7nern", "bei", "Ko\u00b7rinth", "mit", "Flut\u00b7ge\u00b7roll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Hymnus dir die beiden Meere.", "tokens": ["Den", "Hym\u00b7nus", "dir", "die", "bei\u00b7den", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wem, von h\u00f6herm Drang entflammt, das Herz", "tokens": ["Und", "wem", ",", "von", "h\u00f6\u00b7herm", "Drang", "ent\u00b7flammt", ",", "das", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "$,", "APPR", "ADJA", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hinausstrebt aus der Zeiten Enge", "tokens": ["Hin\u00b7aus\u00b7strebt", "aus", "der", "Zei\u00b7ten", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dir, so wie die Blume sonnenw\u00e4rts,", "tokens": ["Zu", "dir", ",", "so", "wie", "die", "Blu\u00b7me", "son\u00b7nen\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "KOKOM", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O K\u00f6nig ewiger Ges\u00e4nge,", "tokens": ["O", "K\u00f6\u00b7nig", "e\u00b7wi\u00b7ger", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Antlitz wendet er; nach Griechenland", "tokens": ["Das", "Ant\u00b7litz", "wen\u00b7det", "er", ";", "nach", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fchrst du ihn heim in wachen Tr\u00e4umen", "tokens": ["F\u00fchrst", "du", "ihn", "heim", "in", "wa\u00b7chen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4ssest ihm am Munde, voll zum Rand", "tokens": ["Und", "l\u00e4s\u00b7sest", "ihm", "am", "Mun\u00b7de", ",", "voll", "zum", "Rand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Dichtung G\u00f6tterbecher sch\u00e4umen.", "tokens": ["Der", "Dich\u00b7tung", "G\u00f6t\u00b7ter\u00b7be\u00b7cher", "sch\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht dr\u00e4ngen Bl\u00e4tter sich im Wald so dicht,", "tokens": ["Nicht", "dr\u00e4n\u00b7gen", "Bl\u00e4t\u00b7ter", "sich", "im", "Wald", "so", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PRF", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die vom Ge\u00e4st der Herbstwind wehte,", "tokens": ["Die", "vom", "Ge\u00b7\u00e4st", "der", "Herbst\u00b7wind", "weh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie drunten, Tr\u00fcmmerschicht auf Tr\u00fcmmerschicht,", "tokens": ["Wie", "drun\u00b7ten", ",", "Tr\u00fcm\u00b7mer\u00b7schicht", "auf", "Tr\u00fcm\u00b7mer\u00b7schicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verschollene Hellenenst\u00e4dte;", "tokens": ["Ver\u00b7schol\u00b7le\u00b7ne", "Hel\u00b7le\u00b7nen\u00b7st\u00e4d\u00b7te", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Hinweggeschwemmt hat der Barbaren Flut", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7schwemmt", "hat", "der", "Bar\u00b7ba\u00b7ren", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Volk der Griechen von der Erde;", "tokens": ["Das", "Volk", "der", "Grie\u00b7chen", "von", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein neu Geschlecht entfacht die Opferglut", "tokens": ["Ein", "neu", "Ge\u00b7schlecht", "ent\u00b7facht", "die", "Op\u00b7fer\u00b7glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf eines neuen Gottes Herde.", "tokens": ["Auf", "ei\u00b7nes", "neu\u00b7en", "Got\u00b7tes", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wenn mein Blick vom Hange des Parna\u00df", "tokens": ["Doch", "wenn", "mein", "Blick", "vom", "Han\u00b7ge", "des", "Par\u00b7na\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dahinschweift l\u00e4ngs der Felsen Fu\u00dfe,", "tokens": ["Da\u00b7hin\u00b7schweift", "l\u00e4ngs", "der", "Fel\u00b7sen", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo hier und da aus Schutt von Tempeln bla\u00df", "tokens": ["Wo", "hier", "und", "da", "aus", "Schutt", "von", "Tem\u00b7peln", "bla\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "KON", "ADV", "APPR", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufragt ein hagres Bild der Bu\u00dfe,", "tokens": ["Auf\u00b7ragt", "ein", "hag\u00b7res", "Bild", "der", "Bu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Oft fernher h\u00f6r' ich deiner Leier Klang,", "tokens": ["Oft", "fern\u00b7her", "h\u00f6r'", "ich", "dei\u00b7ner", "Lei\u00b7er", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und hell beginnt die Luft zu strahlen;", "tokens": ["Und", "hell", "be\u00b7ginnt", "die", "Luft", "zu", "strah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du nahst! Ambrosisch Duften quillt beim Gang", "tokens": ["Du", "nahst", "!", "A\u00b7mbro\u00b7sisch", "Duf\u00b7ten", "quillt", "beim", "Gang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von deinen goldenen Sandalen.", "tokens": ["Von", "dei\u00b7nen", "gol\u00b7de\u00b7nen", "San\u00b7da\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und fortgenommen von Gebirg und Flur", "tokens": ["Und", "fort\u00b7ge\u00b7nom\u00b7men", "von", "Ge\u00b7birg", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist der Ver\u00f6dung Fluch, und wieder,", "tokens": ["Ist", "der", "Ve\u00b7r\u00f6\u00b7dung", "Fluch", ",", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von dumpfem Alpdruck frei, schl\u00e4gt die Natur", "tokens": ["Von", "dum\u00b7pfem", "A\u00b7lpdruck", "frei", ",", "schl\u00e4gt", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Empor die schweren Augenlider,", "tokens": ["Em\u00b7por", "die", "schwe\u00b7ren", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und Tempeld\u00e4cher blicken marmorwei\u00df", "tokens": ["Und", "Tem\u00b7pel\u00b7d\u00e4\u00b7cher", "bli\u00b7cken", "mar\u00b7mor\u00b7wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch Lorbeerwipfel und Platanen,", "tokens": ["Durch", "Lor\u00b7beer\u00b7wip\u00b7fel", "und", "Pla\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und durch die Zweige hin rauscht dir zum Preis", "tokens": ["Und", "durch", "die", "Zwei\u00b7ge", "hin", "rauscht", "dir", "zum", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.8": {"text": "Der Schall von festlichen P\u00e4anen.", "tokens": ["Der", "Schall", "von", "fest\u00b7li\u00b7chen", "P\u00e4\u00b7a\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So, mag ein neuer Gotensturm Ruin", "tokens": ["So", ",", "mag", "ein", "neu\u00b7er", "Go\u00b7ten\u00b7sturm", "Ru\u00b7in"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt von heute auch bereiten,", "tokens": ["Der", "Welt", "von", "heu\u00b7te", "auch", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4chelnd, in ew'ger Jugend hin durch ihn,", "tokens": ["L\u00e4\u00b7chelnd", ",", "in", "ew'\u00b7ger", "Ju\u00b7gend", "hin", "durch", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gott des Gesanges, wirst du schreiten;", "tokens": ["Gott", "des", "Ge\u00b7san\u00b7ges", ",", "wirst", "du", "schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wie Strahlen schon vor Morgen nach und nach", "tokens": ["Wie", "Strah\u00b7len", "schon", "vor", "Mor\u00b7gen", "nach", "und", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "APPR", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Licht der Berge Haupt verkl\u00e4ren,", "tokens": ["Mit", "Licht", "der", "Ber\u00b7ge", "Haupt", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Spielt um die Stirne dir der junge Tag,", "tokens": ["Spielt", "um", "die", "Stir\u00b7ne", "dir", "der", "jun\u00b7ge", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wo wieder dich die Menschen ehren.", "tokens": ["Wo", "wie\u00b7der", "dich", "die", "Men\u00b7schen", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch lebst du, sch\u00f6ner Gott des Lichts! Ob auch", "tokens": ["Noch", "lebst", "du", ",", "sch\u00f6\u00b7ner", "Gott", "des", "Lichts", "!", "Ob", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$.", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein letzter Tempel l\u00e4ngst zerfallen", "tokens": ["Dein", "letz\u00b7ter", "Tem\u00b7pel", "l\u00e4ngst", "zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nie mehr bei der Lyderfl\u00f6ten Hauch", "tokens": ["Und", "nie", "mehr", "bei", "der", "Ly\u00b7der\u00b7fl\u00f6\u00b7ten", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Delphi fromme Ch\u00f6re schallen:"