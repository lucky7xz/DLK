{"textgrid.poem.67136": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Prinzessin Auguste", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch freundlichz\u00f6gernd scheidet vom Auge dir", "tokens": ["Noch", "freund\u00b7lich\u00b7z\u00f6\u00b7gernd", "schei\u00b7det", "vom", "Au\u00b7ge", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Jahr, und in hesperischer Milde gl\u00e4nzt", "tokens": ["Das", "Jahr", ",", "und", "in", "hes\u00b7pe\u00b7ri\u00b7scher", "Mil\u00b7de", "gl\u00e4nzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Winterhimmel \u00fcber deinen", "tokens": ["Der", "Win\u00b7ter\u00b7him\u00b7mel", "\u00fc\u00b7ber", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00e4rten, den dichtrischen, immergr\u00fcnen.", "tokens": ["G\u00e4r\u00b7ten", ",", "den", "dich\u00b7tri\u00b7schen", ",", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und da ich deines Festes gedacht und sann,", "tokens": ["Und", "da", "ich", "dei\u00b7nes", "Fes\u00b7tes", "ge\u00b7dacht", "und", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was ich dir dankend reichte, da weilten noch", "tokens": ["Was", "ich", "dir", "dan\u00b7kend", "reich\u00b7te", ",", "da", "weil\u00b7ten", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Pfade Blumen, da\u00df sie dir zur", "tokens": ["Am", "Pfa\u00b7de", "Blu\u00b7men", ",", "da\u00df", "sie", "dir", "zur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$,", "KOUS", "PPER", "PPER", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bl\u00fchenden Krone, du Edle, w\u00fcrden.", "tokens": ["Bl\u00fc\u00b7hen\u00b7den", "Kro\u00b7ne", ",", "du", "Ed\u00b7le", ",", "w\u00fcr\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJA", "$,", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch andres beut dir, Gr\u00f6\u00dferes, hoher Geist!", "tokens": ["Doch", "and\u00b7res", "beut", "dir", ",", "Gr\u00f6\u00b7\u00dfe\u00b7res", ",", "ho\u00b7her", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die festlichere Zeit, denn es hallt hinab", "tokens": ["Die", "fest\u00b7li\u00b7che\u00b7re", "Zeit", ",", "denn", "es", "hallt", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Berge das Gewitter, sieh! und", "tokens": ["Am", "Ber\u00b7ge", "das", "Ge\u00b7wit\u00b7ter", ",", "sieh", "!", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVIMP", "$.", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Klar, wie die ruhigen Sterne, gehen", "tokens": ["Klar", ",", "wie", "die", "ru\u00b7hi\u00b7gen", "Ster\u00b7ne", ",", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aus langem Zweifel reine Gestalten auf;", "tokens": ["Aus", "lan\u00b7gem", "Zwei\u00b7fel", "rei\u00b7ne", "Ge\u00b7stal\u00b7ten", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So d\u00fcnkt es mir; und einsam, o F\u00fcrstin! ist", "tokens": ["So", "d\u00fcnkt", "es", "mir", ";", "und", "ein\u00b7sam", ",", "o", "F\u00fcrs\u00b7tin", "!", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "KON", "ADJD", "$,", "FM", "NN", "$.", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Herz der Freigebornen wohl nicht", "tokens": ["Das", "Herz", "der", "Frei\u00b7ge\u00b7bor\u00b7nen", "wohl", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "L\u00e4nger im eigenen Gl\u00fcck; denn w\u00fcrdig", "tokens": ["L\u00e4n\u00b7ger", "im", "ei\u00b7ge\u00b7nen", "Gl\u00fcck", ";", "denn", "w\u00fcr\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "$.", "KON", "ADJD"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Gesellt im Lorbeer ihm der Hero\u00eb sich,", "tokens": ["Ge\u00b7sellt", "im", "Lor\u00b7beer", "ihm", "der", "He\u00b7ro\u00eb", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "PPER", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6ngereifte, echte; die Weisen auch,", "tokens": ["Der", "sch\u00f6n\u00b7ge\u00b7reif\u00b7te", ",", "ech\u00b7te", ";", "die", "Wei\u00b7sen", "auch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$.", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Unsern, sind es wert; sie blicken", "tokens": ["Die", "Un\u00b7sern", ",", "sind", "es", "wert", ";", "sie", "bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Still aus der H\u00f6he des Lebens, die ernsten Alten.", "tokens": ["Still", "aus", "der", "H\u00f6\u00b7he", "des", "Le\u00b7bens", ",", "die", "erns\u00b7ten", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "Geringe d\u00fcnkt der tr\u00e4umende S\u00e4nger sich,", "tokens": ["Ge\u00b7rin\u00b7ge", "d\u00fcnkt", "der", "tr\u00e4u\u00b7men\u00b7de", "S\u00e4n\u00b7ger", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Kindern gleich am m\u00fc\u00dfigen Saitenspiel,", "tokens": ["Und", "Kin\u00b7dern", "gleich", "am", "m\u00fc\u00b7\u00dfi\u00b7gen", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn ihn der Edlen Gl\u00fcck, wenn ihn die", "tokens": ["Wenn", "ihn", "der", "Ed\u00b7len", "Gl\u00fcck", ",", "wenn", "ihn", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tat und der Ernst der Gewaltgen aufweckt.", "tokens": ["Tat", "und", "der", "Ernst", "der", "Ge\u00b7walt\u00b7gen", "auf\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch herrlicht mir dein Name das Lied; dein Fest", "tokens": ["Doch", "herr\u00b7licht", "mir", "dein", "Na\u00b7me", "das", "Lied", ";", "dein", "Fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Augusta! durft ich feiern; Beruf ist mirs,", "tokens": ["Au\u00b7gus\u00b7ta", "!", "durft", "ich", "fei\u00b7ern", ";", "Be\u00b7ruf", "ist", "mirs", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "VVINF", "$.", "NN", "VAFIN", "NE", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zu r\u00fchmen H\u00f6hers, darum gab die", "tokens": ["Zu", "r\u00fch\u00b7men", "H\u00f6\u00b7hers", ",", "da\u00b7rum", "gab", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "NN", "$,", "PAV", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprache der Gott und den Dank ins Herz mir.", "tokens": ["Spra\u00b7che", "der", "Gott", "und", "den", "Dank", "ins", "Herz", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "O da\u00df von diesem freudigen Tage mir", "tokens": ["O", "da\u00df", "von", "die\u00b7sem", "freu\u00b7di\u00b7gen", "Ta\u00b7ge", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "APPR", "PDAT", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch meine Zeit beginne, da\u00df endlich auch", "tokens": ["Auch", "mei\u00b7ne", "Zeit", "be\u00b7gin\u00b7ne", ",", "da\u00df", "end\u00b7lich", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mir ein Gesang in deinen Hainen,", "tokens": ["Mir", "ein", "Ge\u00b7sang", "in", "dei\u00b7nen", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Edle! gedeihe, der deiner wert sei.", "tokens": ["Ed\u00b7le", "!", "ge\u00b7dei\u00b7he", ",", "der", "dei\u00b7ner", "wert", "sei", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$.", "VVFIN", "$,", "PRELS", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Noch freundlichz\u00f6gernd scheidet vom Auge dir", "tokens": ["Noch", "freund\u00b7lich\u00b7z\u00f6\u00b7gernd", "schei\u00b7det", "vom", "Au\u00b7ge", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Jahr, und in hesperischer Milde gl\u00e4nzt", "tokens": ["Das", "Jahr", ",", "und", "in", "hes\u00b7pe\u00b7ri\u00b7scher", "Mil\u00b7de", "gl\u00e4nzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Winterhimmel \u00fcber deinen", "tokens": ["Der", "Win\u00b7ter\u00b7him\u00b7mel", "\u00fc\u00b7ber", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00e4rten, den dichtrischen, immergr\u00fcnen.", "tokens": ["G\u00e4r\u00b7ten", ",", "den", "dich\u00b7tri\u00b7schen", ",", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Und da ich deines Festes gedacht und sann,", "tokens": ["Und", "da", 