, "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "L\u00e4nger im eigenen Gl\u00fcck; denn w\u00fcrdig", "tokens": ["L\u00e4n\u00b7ger", "im", "ei\u00b7ge\u00b7nen", "Gl\u00fcck", ";", "denn", "w\u00fcr\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "$.", "KON", "ADJD"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Gesellt im Lorbeer ihm der Hero\u00eb sich,", "tokens": ["Ge\u00b7sellt", "im", "Lor\u00b7beer", "ihm", "der", "He\u00b7ro\u00eb", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "PPER", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6ngereifte, echte; die Weisen auch,", "tokens": ["Der", "sch\u00f6n\u00b7ge\u00b7reif\u00b7te", ",", "ech\u00b7te", ";", "die", "Wei\u00b7sen", "auch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$.", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Unsern, sind es wert; sie blicken", "tokens": ["Die", "Un\u00b7sern", ",", "sind", "es", "wert", ";", "sie", "bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Still aus der H\u00f6he des Lebens, die ernsten Alten.", "tokens": ["Still", "aus", "der", "H\u00f6\u00b7he", "des", "Le\u00b7bens", ",", "die", "erns\u00b7ten", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART