{"textgrid.poem.53086": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "An Caspar Rodemann zum Namenstage", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was suchen wir euch doch, Herr ", "tokens": ["Was", "su\u00b7chen", "wir", "euch", "doch", ",", "Herr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "$,", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Hat um uns hergelegt, rei\u00dft Clotho strenge Hand", "tokens": ["Hat", "um", "uns", "her\u00b7ge\u00b7legt", ",", "rei\u00dft", "Clo\u00b7tho", "stren\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Lebensfaden ab. Gott l\u00e4sst davor ihn winden", "tokens": ["Den", "Le\u00b7bens\u00b7fa\u00b7den", "ab", ".", "Gott", "l\u00e4sst", "da\u00b7vor", "ihn", "win\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "NN", "VVFIN", "PAV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Band der Ewigkeit des Lebens, das nicht schwinden", "tokens": ["Ein", "Band", "der", "E\u00b7wig\u00b7keit", "des", "Le\u00b7bens", ",", "das", "nicht", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch Abgang nehmen soll, versetzt ihn in ein Land,", "tokens": ["Noch", "Ab\u00b7gang", "neh\u00b7men", "soll", ",", "ver\u00b7setzt", "ihn", "in", "ein", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Ruh und Freude bindt. Wir stehn von uns getrannt", "tokens": ["Das", "Ruh", "und", "Freu\u00b7de", "bindt", ".", "Wir", "stehn", "von", "uns", "ge\u00b7trannt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und k\u00f6nnen unsrer selbst noch nicht Erg\u00e4nzung finden.", "tokens": ["Und", "k\u00f6n\u00b7nen", "uns\u00b7rer", "selbst", "noch", "nicht", "Er\u00b7g\u00e4n\u00b7zung", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADV", "ADV", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nehmt aber gleichwohl hin ein Band ohn allen Schein.", "tokens": ["Nehmt", "a\u00b7ber", "gleich\u00b7wohl", "hin", "ein", "Band", "ohn", "al\u00b7len", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwartz zeigt Betr\u00fcbte, schwartz m\u00fc\u00dft ihr gebunden seyn,", "tokens": ["Schwartz", "zeigt", "Be\u00b7tr\u00fcb\u00b7te", ",", "schwartz", "m\u00fc\u00dft", "ihr", "ge\u00b7bun\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "ADJD", "VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein bessers ziemt euch nicht vor diesmal zu begehren.", "tokens": ["Ein", "bes\u00b7sers", "ziemt", "euch", "nicht", "vor", "dies\u00b7mal", "zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach, nehmt es hin! Lebt wohl! Erkennet unsre Pflicht,", "tokens": ["Ach", ",", "nehmt", "es", "hin", "!", "Lebt", "wohl", "!", "Er\u00b7ken\u00b7net", "uns\u00b7re", "Pflicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "ADV", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mangelt es ihm auch an sch\u00f6ner Zierrath nicht.", "tokens": ["Doch", "man\u00b7gelt", "es", "ihm", "auch", "an", "sch\u00f6\u00b7ner", "Zier\u00b7rath", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was f\u00fchrt es? Perlen. Was? Ja, Perlen unsrer Z\u00e4hren.", "tokens": ["Was", "f\u00fchrt", "es", "?", "Per\u00b7len", ".", "Was", "?", "Ja", ",", "Per\u00b7len", "uns\u00b7rer", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "