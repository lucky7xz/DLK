{"dta.poem.8494": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Unter der Linde.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war an einem Morgen,", "tokens": ["Es", "war", "an", "ei\u00b7nem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6glein sangen s\u00fc\u00df,", "tokens": ["Die", "V\u00f6\u00b7glein", "san\u00b7gen", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber\u2019m Raine wallte", "tokens": ["Und", "\u00fc\u00b7ber'm", "Rai\u00b7ne", "wall\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sch\u00f6nste Blumenvlie\u00df.", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Blu\u00b7men\u00b7vlie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das B\u00f6rnlein mir zur Seite", "tokens": ["Das", "B\u00f6rn\u00b7lein", "mir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sprach leise, leise fort,", "tokens": ["Sprach", "lei\u00b7se", ",", "lei\u00b7se", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit halbgeschlossnen Augen", "tokens": ["Mit", "halb\u00b7ge\u00b7schloss\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sa\u00df ich und lauschte dort.", "tokens": ["Sa\u00df", "ich", "und", "lauschte", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich sah die Schmetterlinge", "tokens": ["Ich", "sah", "die", "Schmet\u00b7ter\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich jagen durch das Licht,", "tokens": ["Sich", "ja\u00b7gen", "durch", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und der Libelle Fl\u00fcgel", "tokens": ["Und", "der", "Li\u00b7bel\u00b7le", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mir zittern am Gesicht.", "tokens": ["Mir", "zit\u00b7tern", "am", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Still sa\u00df ich wie gestorben", "tokens": ["Still", "sa\u00df", "ich", "wie", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "KOKOM", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und lie\u00df mir wohlig sein,", "tokens": ["Und", "lie\u00df", "mir", "woh\u00b7lig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich mit den Bl\u00fcthenflocken", "tokens": ["Mich", "mit", "den", "Bl\u00fc\u00b7then\u00b7flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Vom Lindenzweig bestreu\u2019n.", "tokens": ["Vom", "Lin\u00b7den\u00b7zweig", "be\u00b7streu'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Sitz war dicht am Wege,", "tokens": ["Mein", "Sitz", "war", "dicht", "am", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich konnte ruhig sp\u00e4h\u2019n;", "tokens": ["Ich", "konn\u00b7te", "ru\u00b7hig", "sp\u00e4h'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch mich, verh\u00fcllt vom Strauche,", "tokens": ["Doch", "mich", ",", "ver\u00b7h\u00fcllt", "vom", "Strau\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich hat man nicht geseh\u2019n;", "tokens": ["Mich", "hat", "man", "nicht", "ge\u00b7seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn knarrend Wagen rollten,", "tokens": ["Wenn", "knar\u00b7rend", "Wa\u00b7gen", "roll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dann drang zu mir der Staub,", "tokens": ["Dann", "drang", "zu", "mir", "der", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn die V\u00f6glein h\u00fcpften,", "tokens": ["Und", "wenn", "die", "V\u00f6\u00b7glein", "h\u00fcpf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dann zitterte das Laub.", "tokens": ["Dann", "zit\u00b7ter\u00b7te", "das", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und nahe mir am Hange", "tokens": ["Und", "na\u00b7he", "mir", "am", "Han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u2019ne alte Buche stand,", "tokens": ["'ne", "al\u00b7te", "Bu\u00b7che", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um die der ernste Eppich", "tokens": ["Um", "die", "der", "erns\u00b7te", "Ep\u00b7pich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sich hoch und h\u00f6her wand.", "tokens": ["Sich", "hoch", "und", "h\u00f6\u00b7her", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein d\u00fcstres Gr\u00fcn umrankte", "tokens": ["Sein", "d\u00fcst\u00b7res", "Gr\u00fcn", "um\u00b7rank\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch manchen kranken Zweig;", "tokens": ["Noch", "man\u00b7chen", "kran\u00b7ken", "Zweig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch die gesunden spielten", "tokens": ["Doch", "die", "ge\u00b7sun\u00b7den", "spiel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie doppelt gr\u00fcn und reich.", "tokens": ["Wie", "dop\u00b7pelt", "gr\u00fcn", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es war im Maienmonde,", "tokens": ["Es", "war", "im", "Mai\u00b7en\u00b7mon\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Bl\u00e4tter atlaszart;", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "at\u00b7las\u00b7zart", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie hast du alter Knabe", "tokens": ["Wie", "hast", "du", "al\u00b7ter", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So frisches Herz bewahrt?", "tokens": ["So", "fri\u00b7sches", "Herz", "be\u00b7wahrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf einer Seite thr\u00e4nend", "tokens": ["Auf", "ei\u00b7ner", "Sei\u00b7te", "thr\u00e4\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und auf der andern licht,", "tokens": ["Und", "auf", "der", "an\u00b7dern", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zeigst du auf gr\u00fcner S\u00e4ule", "tokens": ["Zeigst", "du", "auf", "gr\u00fc\u00b7ner", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ein Janusangesicht.", "tokens": ["Ein", "Ja\u00b7nus\u00b7an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da dacht\u2019 ich eines Freundes,", "tokens": ["Da", "dacht'", "ich", "ei\u00b7nes", "Freun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "De\u00df Locken grau und lind,", "tokens": ["De\u00df", "Lo\u00b7cken", "grau", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein armes Wrack sein K\u00f6rper,", "tokens": ["Ein", "ar\u00b7mes", "Wrack", "sein", "K\u00f6r\u00b7per", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ach, sein Herz ein Kind;", "tokens": ["Und", "ach", ",", "sein", "Herz", "ein", "Kind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich d\u00fcnkt, ich sah ihn neigen", "tokens": ["Mich", "d\u00fcnkt", ",", "ich", "sah", "ihn", "nei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Thr\u00e4nen auf ein Grab,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "auf", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wieder Blumen streuen", "tokens": ["Und", "wie\u00b7der", "Blu\u00b7men", "streu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In eine Wieg\u2019 herab.", "tokens": ["In", "ei\u00b7ne", "Wieg'", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da weckten Rinderglocken", "tokens": ["Da", "weck\u00b7ten", "Rin\u00b7der\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich aus den Phantasey\u2019n;", "tokens": ["Mich", "aus", "den", "Phan\u00b7ta\u00b7sey'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein tr\u00fcber Staubeswirbel", "tokens": ["Ein", "tr\u00fc\u00b7ber", "Stau\u00b7bes\u00b7wir\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Drang durch\u2019s Geb\u00fcsch herein,", "tokens": ["Drang", "durch's", "Ge\u00b7b\u00fcsch", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und mit Geschrei und Schelten", "tokens": ["Und", "mit", "Ge\u00b7schrei", "und", "Schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ri\u00df einen Epheustab", "tokens": ["Ri\u00df", "ei\u00b7nen", "E\u00b7pheust\u00b7ab"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Treiberknecht vom Baume", "tokens": ["Der", "Trei\u00b7berk\u00b7necht", "vom", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und trieb sein Vieh bergab.", "tokens": ["Und", "trieb", "sein", "Vieh", "berg\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mir war, als ob gesch\u00e4digt", "tokens": ["Mir", "war", ",", "als", "ob", "ge\u00b7sch\u00e4\u00b7digt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein frommes Leben sei;", "tokens": ["Ein", "from\u00b7mes", "Le\u00b7ben", "sei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch horch, was trabt so neckend", "tokens": ["Doch", "horch", ",", "was", "trabt", "so", "ne\u00b7ckend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "PWS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So drall und knapp herbei?", "tokens": ["So", "drall", "und", "knapp", "her\u00b7bei", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das R\u00e4nzel auf dem R\u00fccken,", "tokens": ["Das", "R\u00e4n\u00b7zel", "auf", "dem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Barett im blonden Haar,", "tokens": ["Ba\u00b7rett", "im", "blon\u00b7den", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kommt ein Student gepfiffen,", "tokens": ["Kommt", "ein", "Stu\u00b7dent", "ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ein lustiger Scholar.", "tokens": ["Ein", "lus\u00b7ti\u00b7ger", "Scho\u00b7lar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201do pescator del onde&#8220;", "tokens": ["\"", "o", "pes\u00b7ca\u00b7tor", "del", "on\u00b7de", "&#8220;"], "token_inf