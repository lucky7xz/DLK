{"textgrid.poem.57884": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der Egestorffschen Stra\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Egestorffschen Stra\u00dfe", "tokens": ["An", "der", "E\u00b7ges\u00b7torff\u00b7schen", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Standen H\u00e4user stolz und k\u00fchn,", "tokens": ["Stan\u00b7den", "H\u00e4u\u00b7ser", "stolz", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Decken sind zerfallen,", "tokens": ["Ih\u00b7re", "De\u00b7cken", "sind", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Risse durch die W\u00e4nde ziehn.", "tokens": ["Ris\u00b7se", "durch", "die", "W\u00e4n\u00b7de", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00f6hlich hatte man gebauet", "tokens": ["Fr\u00f6h\u00b7lich", "hat\u00b7te", "man", "ge\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PIS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Geld und ohne Lehm,", "tokens": ["Oh\u00b7ne", "Geld", "und", "oh\u00b7ne", "Lehm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN",