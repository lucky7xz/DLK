{"dta.poem.20425": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Je kleiner unser reich/ ie gr\u00f6sser unsre st\u00e4rcke/", "tokens": ["Je", "klei\u00b7ner", "un\u00b7ser", "reich", "/", "ie", "gr\u00f6s\u00b7ser", "uns\u00b7re", "st\u00e4r\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJD", "$(", "ADV", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir schrecken manche brust/ und stopffen manchen mund;", "tokens": ["Wir", "schre\u00b7cken", "man\u00b7che", "brust", "/", "und", "stopf\u00b7fen", "man\u00b7chen", "mund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die federn werden stumpff in r\u00fchmung unsrer wercke/", "tokens": ["Die", "fe\u00b7dern", "wer\u00b7den", "stumpff", "in", "r\u00fch\u00b7mung", "uns\u00b7rer", "wer\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VAFIN", "ADJD", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und manch verbrochnes wort thut unsre kr\u00e4ffte kund.", "tokens": ["Und", "manch", "ver\u00b7broch\u00b7nes", "wort", "thut", "uns\u00b7re", "kr\u00e4ff\u00b7te", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das hertze klopfft f\u00fcr uns/ die glieder lernen zittern/", "tokens": ["Das", "hert\u00b7ze", "klopfft", "f\u00fcr", "uns", "/", "die", "glie\u00b7der", "ler\u00b7nen", "zit\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$(", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wer die wahre wort f\u00fcr nichts und nichtig h\u00e4lt/", "tokens": ["Und", "wer", "die", "wah\u00b7re", "wort", "f\u00fcr", "nichts", "und", "nich\u00b7tig", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "APPR", "PIS", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Demselben soll der strahl von unserm plitz ersch\u00fcttern/", "tokens": ["Dem\u00b7sel\u00b7ben", "soll", "der", "strahl", "von", "un\u00b7serm", "plitz", "er\u00b7sch\u00fct\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zum zeugni\u00df unsrer macht/ zur warnung dieser welt.", "tokens": ["Zum", "zeug\u00b7ni\u00df", "uns\u00b7rer", "macht", "/", "zur", "war\u00b7nung", "die\u00b7ser", "welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "VVFIN", "$(", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}