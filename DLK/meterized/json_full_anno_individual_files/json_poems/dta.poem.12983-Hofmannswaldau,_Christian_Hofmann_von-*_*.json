{"dta.poem.12983": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die eigenlieb ist doch die allerd\u00fcmmste liebe!", "tokens": ["Die", "ei\u00b7gen\u00b7lieb", "ist", "doch", "die", "al\u00b7ler\u00b7d\u00fcmms\u00b7te", "lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dennoch opffert ihr selbst die gelehrte welt.", "tokens": ["Und", "den\u00b7noch", "opf\u00b7fert", "ihr", "selbst", "die", "ge\u00b7lehr\u00b7te", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl diesem, dem das lob der schmeichler nicht gef\u00e4llt!", "tokens": ["Wohl", "die\u00b7sem", ",", "dem", "das", "lob", "der", "schmeich\u00b7ler", "nicht", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach wenn doch Barbon nicht noch l\u00e4nger b\u00fccher schriebe!", "tokens": ["Ach", "wenn", "doch", "Bar\u00b7bon", "nicht", "noch", "l\u00e4n\u00b7ger", "b\u00fc\u00b7cher", "schrie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "NN", "PTKNEG", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er ist zu arm an witz; Kurtz: er ist gar nicht klug.", "tokens": ["Er", "ist", "zu", "arm", "an", "witz", ";", "Kurtz", ":", "er", "ist", "gar", "nicht", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPR", "NE", "$.", "ADJD", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "An reichthum fehlt ihm nichts, GOtt hat ihm viel beschieden;", "tokens": ["An", "reicht\u00b7hum", "fehlt", "ihm", "nichts", ",", "Gott", "hat", "ihm", "viel", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIS", "$,", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Inzwischen ist der narr mit diesem schlecht zufrieden,", "tokens": ["I\u00b7nzwi\u00b7schen", "ist", "der", "narr", "mit", "die\u00b7sem", "schlecht", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PDAT", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An jenem aber hat sein hertze gar genug.", "tokens": ["An", "je\u00b7nem", "a\u00b7ber", "hat", "sein", "hert\u00b7ze", "gar", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}