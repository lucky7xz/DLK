{"dta.poem.3179": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Ausspruch hat zuerst ein starr Gesetz gethan:", "tokens": ["Den", "Aus\u00b7spruch", "hat", "zu\u00b7erst", "ein", "starr", "Ge\u00b7setz", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleiches um Gleiches, Aug' um Auge, Zahn um Zahn.", "tokens": ["Glei\u00b7ches", "um", "Glei\u00b7ches", ",", "Aug'", "um", "Au\u00b7ge", ",", "Zahn", "um", "Zahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Ein milderes Gesetz der Liebe sprach dagegen:", "tokens": ["Ein", "mil\u00b7de\u00b7res", "Ge\u00b7setz", "der", "Lie\u00b7be", "sprach", "da\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liebt euren Feind und gebt dem, der euch flucht, den Segen.", "tokens": ["Liebt", "eu\u00b7ren", "Feind", "und", "gebt", "dem", ",", "der", "euch", "flucht", ",", "den", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch weil fast \u00fcber's Maa\u00df der Menschheit dieses geht,", "tokens": ["Doch", "weil", "fast", "\u00fc\u00b7ber's", "Maa\u00df", "der", "Menschheit", "die\u00b7ses", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPRART", "NN", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Hielt so die Mitte der arabische Prophet:", "tokens": ["Hielt", "so", "die", "Mit\u00b7te", "der", "a\u00b7ra\u00b7bi\u00b7sche", "Pro\u00b7phet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Vergeltet, wie man euch vergolten, aber treiben", "tokens": ["Ver\u00b7gel\u00b7tet", ",", "wie", "man", "euch", "ver\u00b7gol\u00b7ten", ",", "a\u00b7ber", "trei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dar\u00fcber sollt ihr's nicht, darunter d\u00fcrft ihr bleiben.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "sollt", "ih\u00b7r's", "nicht", ",", "da\u00b7run\u00b7ter", "d\u00fcrft", "ihr", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "$,", "PAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Vergeltet! aber wenn ihr wollt euch vom Vergelt", "tokens": ["Ver\u00b7gel\u00b7tet", "!", "a\u00b7ber", "wenn", "ihr", "wollt", "euch", "vom", "Ver\u00b7gelt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "ADV", "KOUS", "PPER", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Enthalten, besser ist's f\u00fcr den, der sich enth\u00e4lt.", "tokens": ["Ent\u00b7hal\u00b7ten", ",", "bes\u00b7ser", "ist's", "f\u00fcr", "den", ",", "der", "sich", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VAFIN", "APPR", "ART", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}