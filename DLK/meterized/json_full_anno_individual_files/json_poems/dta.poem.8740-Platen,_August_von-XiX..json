{"dta.poem.8740": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Verschlossen im Krystalle:", "tokens": ["Ver\u00b7schlos\u00b7sen", "im", "Krys\u00b7tal\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will Keiner ihn zerklopfen,", "tokens": ["Will", "Kei\u00b7ner", "ihn", "zer\u00b7klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich ihm frey entwalle?", "tokens": ["Da\u00df", "ich", "ihm", "frey", "ent\u00b7wal\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur durch das Glas beschauen", "tokens": ["Nur", "durch", "das", "Glas", "be\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann ich der Blumen eine:", "tokens": ["Kann", "ich", "der", "Blu\u00b7men", "ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O d\u00fcrft' ich auf sie thauen", "tokens": ["O", "d\u00fcrft'", "ich", "auf", "sie", "thau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Morgensonnenscheine!", "tokens": ["Im", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7schei\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}