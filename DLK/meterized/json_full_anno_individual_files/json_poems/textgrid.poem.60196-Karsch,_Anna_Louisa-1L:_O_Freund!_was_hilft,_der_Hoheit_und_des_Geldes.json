{"textgrid.poem.60196": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Freund! was hilft, der Hoheit und des Geldes", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freund! was hilft, der Hoheit und des Geldes", "tokens": ["O", "Freund", "!", "was", "hilft", ",", "der", "Ho\u00b7heit", "und", "des", "Gel\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besitzer seyn, in dieser Welt,", "tokens": ["Be\u00b7sit\u00b7zer", "seyn", ",", "in", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Sterblichen, der wie das Gras des Feldes", "tokens": ["Dem", "Sterb\u00b7li\u00b7chen", ",", "der", "wie", "das", "Gras", "des", "Fel\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hervork\u00f6mmt, w\u00e4chset, welkt und niederf\u00e4llt?", "tokens": ["Her\u00b7vor\u00b7k\u00f6mmt", ",", "w\u00e4ch\u00b7set", ",", "welkt", "und", "nie\u00b7der\u00b7f\u00e4llt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Ueberflu\u00df und im Ger\u00e4usch der Ehre", "tokens": ["Im", "Ue\u00b7berf\u00b7lu\u00df", "und", "im", "Ge\u00b7r\u00e4usch", "der", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey Saitenspiel und T\u00e4nzen seyn,", "tokens": ["Bey", "Sai\u00b7ten\u00b7spiel", "und", "T\u00e4n\u00b7zen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reizt nicht das Auge; nichts nimmt das Geh\u00f6re", "tokens": ["Reizt", "nicht", "das", "Au\u00b7ge", ";", "nichts", "nimmt", "das", "Ge\u00b7h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$.", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und den sonst nimmer satten Busen ein,", "tokens": ["Und", "den", "sonst", "nim\u00b7mer", "sat\u00b7ten", "Bu\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bald von dem zerbrechlichen Geb\u00e4ude", "tokens": ["So", "bald", "von", "dem", "zer\u00b7brech\u00b7li\u00b7chen", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Theil mit Schmerzen wird durchnagt.", "tokens": ["Ein", "Theil", "mit", "Schmer\u00b7zen", "wird", "durch\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der kranke Mensch ists, der zur lauten Freude", "tokens": ["Der", "kran\u00b7ke", "Mensch", "ists", ",", "der", "zur", "lau\u00b7ten", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Scherz und Lachen: du bist Thorheit! sagt.", "tokens": ["Zum", "Scherz", "und", "La\u00b7chen", ":", "du", "bist", "Thor\u00b7heit", "!", "sagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$.", "PPER", "VAFIN", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Reiche w\u00fchlt in seines Goldes Haufen:", "tokens": ["Der", "Rei\u00b7che", "w\u00fchlt", "in", "sei\u00b7nes", "Gol\u00b7des", "Hau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Abgott haucht nicht Leben ein.", "tokens": ["Sein", "Ab\u00b7gott", "haucht", "nicht", "Le\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesundheit oder Jahre noch zu kaufen", "tokens": ["Ge\u00b7sund\u00b7heit", "o\u00b7der", "Jah\u00b7re", "noch", "zu", "kau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dazu sind beyde Welten viel zu klein.", "tokens": ["Da\u00b7zu", "sind", "bey\u00b7de", "Wel\u00b7ten", "viel", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das kranke M\u00e4dchen fodert auf ihr Lager", "tokens": ["Das", "kran\u00b7ke", "M\u00e4d\u00b7chen", "fo\u00b7dert", "auf", "ihr", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Spiegel, zittert und erschrickt", "tokens": ["Den", "Spie\u00b7gel", ",", "zit\u00b7tert", "und", "er\u00b7schrickt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie auf ihrer Wange, bla\u00df und mager", "tokens": ["Wenn", "sie", "auf", "ih\u00b7rer", "Wan\u00b7ge", ",", "bla\u00df", "und", "ma\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Todes drohende Gestalt erblickt!", "tokens": ["Des", "To\u00b7des", "dro\u00b7hen\u00b7de", "Ge\u00b7stalt", "er\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Den J\u00fcngling wirft, trotz der belebten Glieder,", "tokens": ["Den", "J\u00fcng\u00b7ling", "wirft", ",", "trotz", "der", "be\u00b7leb\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trotz seines Muths im Angesicht,", "tokens": ["Trotz", "sei\u00b7nes", "Muths", "im", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Riesen-Arm ein Fieber schnell danieder.", "tokens": ["Mit", "Rie\u00b7sen\u00b7Arm", "ein", "Fie\u00b7ber", "schnell", "da\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Witz, Jugend, St\u00e4rke, alles half ihm nicht!", "tokens": ["Witz", ",", "Ju\u00b7gend", ",", "St\u00e4r\u00b7ke", ",", "al\u00b7les", "half", "ihm", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Weitbezwinger! (Nationen krochen", "tokens": ["Der", "Weit\u00b7be\u00b7zwin\u00b7ger", "!", "(", "Na\u00b7ti\u00b7o\u00b7nen", "kro\u00b7chen"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Staub und horchten sein Geboth \u2013)", "tokens": ["Im", "Staub", "und", "horch\u00b7ten", "sein", "Ge\u00b7both", "\u2013", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Krank liegt er machtlos. O! sein Blick gebrochen", "tokens": ["Krank", "liegt", "er", "macht\u00b7los", ".", "O", "!", "sein", "Blick", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$.", "NE", "$.", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Befiehlt nicht mehr. Im Auge sitzt der Tod.", "tokens": ["Be\u00b7fiehlt", "nicht", "mehr", ".", "Im", "Au\u00b7ge", "sitzt", "der", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "$.", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Weise, der vom Himmel, bis zur Erde", "tokens": ["Der", "Wei\u00b7se", ",", "der", "vom", "Him\u00b7mel", ",", "bis", "zur", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Cederbaum zum kleinsten Kraut", "tokens": ["Vom", "Ce\u00b7der\u00b7baum", "zum", "kleins\u00b7ten", "Kraut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erkenntni\u00df hat, fragt unter der Beschwerde", "tokens": ["Er\u00b7kennt\u00b7ni\u00df", "hat", ",", "fragt", "un\u00b7ter", "der", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht, ob der Ruhm ihm Ehren-S\u00e4ulen baut?", "tokens": ["Nicht", ",", "ob", "der", "Ruhm", "ihm", "Eh\u00b7ren\u00b7S\u00e4u\u00b7len", "baut", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der, dem sein Schiff auf ungebahntem Meere", "tokens": ["Der", ",", "dem", "sein", "Schiff", "auf", "un\u00b7ge\u00b7bahn\u00b7tem", "Mee\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel Lasten Reichthums zugebracht,", "tokens": ["Viel", "Las\u00b7ten", "Reicht\u00b7hums", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimmt, wenn sein Eigenthum ganz China w\u00e4re", "tokens": ["Nimmt", ",", "wenn", "sein", "Ei\u00b7gen\u00b7thum", "ganz", "Chi\u00b7na", "w\u00e4\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "NE", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nichts mit als nur die weisse Todten-Pracht.", "tokens": ["Nichts", "mit", "als", "nur", "die", "weis\u00b7se", "Tod\u00b7ten\u00b7Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "KOKOM", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Nichts folgt dem Grossen, der in vollem Glanze", "tokens": ["Nichts", "folgt", "dem", "Gros\u00b7sen", ",", "der", "in", "vol\u00b7lem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beneidet von dem P\u00f6bel sa\u00df.", "tokens": ["Be\u00b7nei\u00b7det", "von", "dem", "P\u00f6\u00b7bel", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Herrn des Gartens folgt kaum eine Pflanze,", "tokens": ["Dem", "Herrn", "des", "Gar\u00b7tens", "folgt", "kaum", "ei\u00b7ne", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die irgend einer, der ihn nicht verga\u00df", "tokens": ["Die", "ir\u00b7gend", "ei\u00b7ner", ",", "der", "ihn", "nicht", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Mit Thr\u00e4nen feuchtet, aus der Erde reisset.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "feuch\u00b7tet", ",", "aus", "der", "Er\u00b7de", "reis\u00b7set", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie auf des Freundes Grab versetzt,", "tokens": ["Sie", "auf", "des", "Freun\u00b7des", "Grab", "ver\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ewig ihre Bl\u00e4tter gr\u00fcnen heisset", "tokens": ["Und", "e\u00b7wig", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "gr\u00fc\u00b7nen", "heis\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einem Staube, den er heilig sch\u00e4tzt!", "tokens": ["Auf", "ei\u00b7nem", "Stau\u00b7be", ",", "den", "er", "hei\u00b7lig", "sch\u00e4tzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "O! fand mein Sulzer in des Gartens Raume", "tokens": ["O", "!", "fand", "mein", "Sul\u00b7zer", "in", "des", "Gar\u00b7tens", "Rau\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht der Cypressen junge Zucht?", "tokens": ["Nicht", "der", "Cyp\u00b7res\u00b7sen", "jun\u00b7ge", "Zucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wird sie auf jenem Grabe nicht zum Baume", "tokens": ["Wird", "sie", "auf", "je\u00b7nem", "Gra\u00b7be", "nicht", "zum", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den oft ein Sohn, die Gr\u00e4ber denkend sucht?", "tokens": ["Den", "oft", "ein", "Sohn", ",", "die", "Gr\u00e4\u00b7ber", "den\u00b7kend", "sucht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Hin\u00fcber durch die hohe Sternen-Pforte",