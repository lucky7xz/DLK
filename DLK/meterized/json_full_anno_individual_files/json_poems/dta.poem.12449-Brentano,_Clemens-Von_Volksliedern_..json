{"dta.poem.12449": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von Volksliedern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drum gehe tapfer an, mein Sohn, mein Kriegsgenosse,", "tokens": ["Drum", "ge\u00b7he", "tap\u00b7fer", "an", ",", "mein", "Sohn", ",", "mein", "Kriegs\u00b7ge\u00b7nos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlag ritterlich darein, dein Leben unverdrossen", "tokens": ["Schlag", "rit\u00b7ter\u00b7lich", "da\u00b7rein", ",", "dein", "Le\u00b7ben", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "PAV", "$,", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcrs Vaterland aufsez, von dem du frey es auch", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", "auf\u00b7sez", ",", "von", "dem", "du", "frey", "es", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "ADJD", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zuvor empfangen hast, das ist der Deutschen Brauch.", "tokens": ["Zu\u00b7vor", "emp\u00b7fan\u00b7gen", "hast", ",", "das", "ist", "der", "Deut\u00b7schen", "Brauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Herz und Auge la\u00df mit Eifers Flamme brennen,", "tokens": ["Dein", "Herz", "und", "Au\u00b7ge", "la\u00df", "mit", "Ei\u00b7fers", "Flam\u00b7me", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein menschliche Gewalt wird dich vom andern trennen.", "tokens": ["Kein", "menschli\u00b7che", "Ge\u00b7walt", "wird", "dich", "vom", "an\u00b7dern", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Es weht von deinem Haupt die Fahne bald hinweg,", "tokens": ["Es", "weht", "von", "dei\u00b7nem", "Haupt", "die", "Fah\u00b7ne", "bald", "hin\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Jugend Uebermuth, der Unordnung erweckt.", "tokens": ["Der", "Ju\u00b7gend", "Ue\u00b7ber\u00b7muth", ",", "der", "Un\u00b7ord\u00b7nung", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kannst du nicht fechten mehr, du kannst mit deiner Stimme,", "tokens": ["Kannst", "du", "nicht", "fech\u00b7ten", "mehr", ",", "du", "kannst", "mit", "dei\u00b7ner", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kannst du nicht rufen mehr, mit deiner Augen Grimme", "tokens": ["Kannst", "du", "nicht", "ru\u00b7fen", "mehr", ",", "mit", "dei\u00b7ner", "Au\u00b7gen", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Feinden Abbruch thun in deinem Heldenmuth,", "tokens": ["Den", "Fein\u00b7den", "Ab\u00b7bruch", "thun", "in", "dei\u00b7nem", "Hel\u00b7den\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur w\u00fcnschend, da\u00df du theur verkaufen m\u00f6gst dein Blut.", "tokens": ["Nur", "w\u00fcn\u00b7schend", ",", "da\u00df", "du", "theur", "ver\u00b7kau\u00b7fen", "m\u00f6gst", "dein", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im Feuer sey bedacht, wie du das Lob erwerbest,", "tokens": ["Im", "Feu\u00b7er", "sey", "be\u00b7dacht", ",", "wie", "du", "das", "Lob", "er\u00b7wer\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df du in m\u00e4nnlicher Postur und Stellung sterbest,", "tokens": ["Da\u00df", "du", "in", "m\u00e4nn\u00b7li\u00b7cher", "Pos\u00b7tur", "und", "Stel\u00b7lung", "ster\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An deinem Ort bestehst fest mit den F\u00fc\u00dfen dein,", "tokens": ["An", "dei\u00b7nem", "Ort", "be\u00b7stehst", "fest", "mit", "den", "F\u00fc\u00b7\u00dfen", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und bei\u00df die Z\u00e4hn zusamm und beyde Lefzen ein.", "tokens": ["Und", "bei\u00df", "die", "Z\u00e4hn", "zu\u00b7samm", "und", "bey\u00b7de", "Lef\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df deine Wunden sich lobw\u00fcrdig all befinden,", "tokens": ["Da\u00df", "dei\u00b7ne", "Wun\u00b7den", "sich", "lob\u00b7w\u00fcr\u00b7dig", "all", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADJD", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da vorne auf der Brust, und keine nicht dahinten,", "tokens": ["Da", "vor\u00b7ne", "auf", "der", "Brust", ",", "und", "kei\u00b7ne", "nicht", "da\u00b7hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "KON", "PIAT", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df dich dein Feind der Tod im Tod bewundernd zier,", "tokens": ["Da\u00df", "dich", "dein", "Feind", "der", "Tod", "im", "Tod", "be\u00b7wun\u00b7dernd", "zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Vater im Gesicht dein ernstes Leben sp\u00fcr.", "tokens": ["Dein", "Va\u00b7ter", "im", "Ge\u00b7sicht", "dein", "erns\u00b7tes", "Le\u00b7ben", "sp\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Sohn, wer Tyrannei ge\u00fcbriget will leben,", "tokens": ["Mein", "Sohn", ",", "wer", "Ty\u00b7ran\u00b7nei", "ge\u00b7\u00fcb\u00b7ri\u00b7get", "will", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "NN", "VVPP", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df seines Lebens sich freiwillig vor begeben,", "tokens": ["Mu\u00df", "sei\u00b7nes", "Le\u00b7bens", "sich", "frei\u00b7wil\u00b7lig", "vor", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer nur des Tods begehrt, wer nur frisch geht dahin,", "tokens": ["Wer", "nur", "des", "Tods", "be\u00b7gehrt", ",", "wer", "nur", "frisch", "geht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,", "PWS", "ADV", "ADJD", "VVFIN", "PAV", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der hat den Sieg und dann das Leben zu Gewinn.", "tokens": ["Der", "hat", "den", "Sieg", "und", "dann", "das", "Le\u00b7ben", "zu", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}