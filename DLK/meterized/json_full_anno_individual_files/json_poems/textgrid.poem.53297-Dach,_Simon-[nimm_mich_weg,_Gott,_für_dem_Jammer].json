tokens": ["Mach", "es", ",", "Herr", ",", "mit", "mir", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich wil auffgel\u00f6set seyn,", "tokens": ["Ich", "wil", "auff\u00b7ge\u00b7l\u00f6\u00b7set", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Komm in Gnaden, komm behende!", "tokens": ["Komm", "in", "Gna\u00b7den", ",", "komm", "be\u00b7hen\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bring mich in mein K\u00e4mmerlein!", "tokens": ["Bring", "mich", "in", "mein", "K\u00e4m\u00b7mer\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Seele nimm zu dir,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "nimm", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df sie nicht seyn mit gestorben,", "tokens": ["La\u00df", "sie", "nicht", "seyn", "mit", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VAINF", "APPR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil den Himmel Christus jhr", "tokens": ["Weil", "den", "Him\u00b7mel", "Chris\u00b7tus", "jhr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch sein thewres Blut erworben.", "tokens": ["Durch", "sein", "thew\u00b7res", "Blut", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}