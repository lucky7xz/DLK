{"textgrid.poem.60545": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Des Teufels Lied vom Aristokraten", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lobe den Aristokraten;", "tokens": ["Ich", "lo\u00b7be", "den", "A\u00b7ris\u00b7to\u00b7kra\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er des Adels rechte V\u00f6lle,", "tokens": ["Hat", "er", "des", "A\u00b7dels", "rech\u00b7te", "V\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist er vorweg schon halb geraten", "tokens": ["Ist", "er", "vor\u00b7weg", "schon", "halb", "ge\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zugerichtet f\u00fcr die H\u00f6lle.", "tokens": ["Und", "zu\u00b7ge\u00b7rich\u00b7tet", "f\u00fcr", "die", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer besser schon sich d\u00fcnkt und echter,", "tokens": ["Wer", "bes\u00b7ser", "schon", "sich", "d\u00fcnkt", "und", "ech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "PRF", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blo\u00df weil er lebt, als ganze Scharen,", "tokens": ["Blo\u00df", "weil", "er", "lebt", ",", "als", "gan\u00b7ze", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der wird gewi\u00df zur Grube schlechter", "tokens": ["Der", "wird", "ge\u00b7wi\u00df", "zur", "Gru\u00b7be", "schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als all die Tausend niederfahren.", "tokens": ["Als", "all", "die", "Tau\u00b7send", "nie\u00b7der\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was sch\u00fctzen mag die Niedern, Rohen", "tokens": ["Was", "sch\u00fct\u00b7zen", "mag", "die", "Nie\u00b7dern", ",", "Ro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVINF", "VMFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor meiner Finger scharfen Griffen:", "tokens": ["Vor", "mei\u00b7ner", "Fin\u00b7ger", "schar\u00b7fen", "Grif\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Natur und Liebe \u2013 wird dem Hohen", "tokens": ["Na\u00b7tur", "und", "Lie\u00b7be", "\u2013", "wird", "dem", "Ho\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$(", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon in der Kindheit abgeschliffen.", "tokens": ["Schon", "in", "der", "Kind\u00b7heit", "ab\u00b7ge\u00b7schlif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geschieden von der schlechten Rotte", "tokens": ["Ge\u00b7schie\u00b7den", "von", "der", "schlech\u00b7ten", "Rot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volkes sitzt der Edelreine", "tokens": ["Des", "Vol\u00b7kes", "sitzt", "der", "E\u00b7del\u00b7rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In seiner lieben Ahnengrotte", "tokens": ["In", "sei\u00b7ner", "lie\u00b7ben", "Ah\u00b7nen\u00b7grot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So k\u00fchl, erhaben und alleine.", "tokens": ["So", "k\u00fchl", ",", "er\u00b7ha\u00b7ben", "und", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vor\u00fcber braust an seinem Saale", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "braust", "an", "sei\u00b7nem", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Volk mit Not- und Dampfgewerben,", "tokens": ["Das", "Volk", "mit", "Not", "und", "Dampf\u00b7ge\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schwingen ihm die Festpokale,", "tokens": ["Sie", "schwin\u00b7gen", "ihm", "die", "Fest\u00b7po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man lebt \u2013 und eilt, f\u00fcr ihn zu sterben.", "tokens": ["Man", "lebt", "\u2013", "und", "eilt", ",", "f\u00fcr", "ihn", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KON", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch Ruh ist in des Edlen Kammer,", "tokens": ["Doch", "Ruh", "ist", "in", "des", "Ed\u00b7len", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er die Lebensm\u00fch nicht sp\u00fcre,", "tokens": ["Da\u00df", "er", "die", "Le\u00b7bens\u00b7m\u00fch", "nicht", "sp\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und jeden Seufzer mu\u00df der Jammer", "tokens": ["Und", "je\u00b7den", "Seuf\u00b7zer", "mu\u00df", "der", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlucken still vor seiner T\u00fcre.", "tokens": ["Ver\u00b7schlu\u00b7cken", "still", "vor", "sei\u00b7ner", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O k\u00f6stlich ist die stille Schonung,", "tokens": ["O", "k\u00f6st\u00b7lich", "ist", "die", "stil\u00b7le", "Scho\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn deutlich h\u00f6rts der Mann der Gnaden,", "tokens": ["Denn", "deut\u00b7lich", "h\u00f6rts", "der", "Mann", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn s\u00fc\u00df ert\u00f6nt um seine Wohnung", "tokens": ["Wenn", "s\u00fc\u00df", "er\u00b7t\u00f6nt", "um", "sei\u00b7ne", "Woh\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Luft von meinen Serenaden.", "tokens": ["Die", "Luft", "von", "mei\u00b7nen", "Se\u00b7re\u00b7na\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er setzt in ", "tokens": ["Er", "setzt", "in"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Bewundernd singen es die Schranzen,", "tokens": ["Be\u00b7wun\u00b7dernd", "sin\u00b7gen", "es", "die", "Schran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN"