RT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich, ruft die Saat, hat Gott gemacht;", "tokens": ["Mich", ",", "ruft", "die", "Saat", ",", "hat", "Gott", "ge\u00b7macht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ART", "NN", "$,", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bringt unserm Sch\u00f6pfer Ehre!", "tokens": ["Bringt", "un\u00b7serm", "Sch\u00f6p\u00b7fer", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Mensch, ein Leib, den deine Hand", "tokens": ["Der", "Mensch", ",", "ein", "Leib", ",", "den", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wunderbar bereitet;", "tokens": ["So", "wun\u00b7der\u00b7bar", "be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch, ein Geist, den sein Verstand,", "tokens": ["Der", "Mensch", ",", "ein", "Geist", ",", "den", "sein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich zu erkennen, leitet;", "tokens": ["Dich", "zu", "er\u00b7ken\u00b7nen", ",", "lei\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."