en!", "tokens": ["Und", "al\u00b7le", "Welt", "sag", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und alle Welt f\u00fcrcht ihren Herrn,", "tokens": ["Und", "al\u00b7le", "Welt", "f\u00fcrcht", "ih\u00b7ren", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hoff auf ihn, und dien ihm gern!", "tokens": ["Und", "hoff", "auf", "ihn", ",", "und", "di\u00b7en", "ihm", "gern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wer wollte Gott nicht dienen?", "tokens": ["Wer", "woll\u00b7te", "Gott", "nicht", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}