{"textgrid.poem.36874": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Preis des Sch\u00f6pfers", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, o Sch\u00f6pfer! deine Macht,", "tokens": ["Wenn", "ich", ",", "o", "Sch\u00f6p\u00b7fer", "!", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Weisheit deiner Wege,", "tokens": ["Die", "Weis\u00b7heit", "dei\u00b7ner", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Liebe, die f\u00fcr alle wacht,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "f\u00fcr", "al\u00b7le", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anbetend \u00fcberlege:", "tokens": ["An\u00b7be\u00b7tend", "\u00fc\u00b7berl\u00b7e\u00b7ge", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "So wei\u00df ich, von Bewundrung voll,", "tokens": ["So", "wei\u00df", "ich", ",", "von", "Be\u00b7wund\u00b7rung", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht, wie ich dich erheben soll,", "tokens": ["Nicht", ",", "wie", "ich", "dich", "er\u00b7he\u00b7ben", "soll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Gott, mein Herr und Vater!", "tokens": ["Mein", "Gott", ",", "mein", "Herr", "und", "Va\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Auge sieht, wohin es blickt,", "tokens": ["Mein", "Au\u00b7ge", "sieht", ",", "wo\u00b7hin", "es", "blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wunder deiner Werke.", "tokens": ["Die", "Wun\u00b7der", "dei\u00b7ner", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel, pr\u00e4chtig ausgeschm\u00fcckt,", "tokens": ["Der", "Him\u00b7mel", ",", "pr\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Preist dich, du Gott der St\u00e4rke!", "tokens": ["Preist", "dich", ",", "du", "Gott", "der", "St\u00e4r\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer hat die Sonn an ihm erh\u00f6ht?", "tokens": ["Wer", "hat", "die", "Sonn", "an", "ihm", "er\u00b7h\u00f6ht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer kleidet sie mit Majest\u00e4t?", "tokens": ["Wer", "klei\u00b7det", "sie", "mit", "Ma\u00b7jes\u00b7t\u00e4t", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer ruft dem Heer der Sterne?", "tokens": ["Wer", "ruft", "dem", "Heer", "der", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer mi\u00dft dem Winde seinen Lauf?", "tokens": ["Wer", "mi\u00dft", "dem", "Win\u00b7de", "sei\u00b7nen", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer hei\u00dft die Himmel regnen?", "tokens": ["Wer", "hei\u00dft", "die", "Him\u00b7mel", "reg\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer schlie\u00dft den Scho\u00df der Erden auf,", "tokens": ["Wer", "schlie\u00dft", "den", "Scho\u00df", "der", "Er\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Vorrat uns zu segnen?", "tokens": ["Mit", "Vor\u00b7rat", "uns", "zu", "seg\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Gott der Macht und Herrlichkeit!", "tokens": ["O", "Gott", "der", "Macht", "und", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott, deine G\u00fcte reicht so weit,", "tokens": ["Gott", ",", "dei\u00b7ne", "G\u00fc\u00b7te", "reicht", "so", "weit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So weit die Wolken reichen!", "tokens": ["So", "weit", "die", "Wol\u00b7ken", "rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dich predigt Sonnenschein und Sturm,", "tokens": ["Dich", "pre\u00b7digt", "Son\u00b7nen\u00b7schein", "und", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich preist der Sand am Meere.", "tokens": ["Dich", "preist", "der", "Sand", "am", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bringt, ruft auch der geringste Wurm,", "tokens": ["Bringt", ",", "ruft", "auch", "der", "ge\u00b7rings\u00b7te", "Wurm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt meinem Sch\u00f6pfer Ehre!", "tokens": ["Bringt", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich, ruft der Baum in seiner Pracht,", "tokens": ["Mich", ",", "ruft", "der", "Baum", "in", "sei\u00b7ner", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich, ruft die Saat, hat Gott gemacht;", "tokens": ["Mich", ",", "ruft", "die", "Saat", ",", "hat", "Gott", "ge\u00b7macht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ART", "NN", "$,", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bringt unserm Sch\u00f6pfer Ehre!", "tokens": ["Bringt", "un\u00b7serm", "Sch\u00f6p\u00b7fer", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Mensch, ein Leib, den deine Hand", "tokens": ["Der", "Mensch", ",", "ein", "Leib", ",", "den", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wunderbar bereitet;", "tokens": ["So", "wun\u00b7der\u00b7bar", "be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch, ein Geist, den sein Verstand,", "tokens": ["Der", "Mensch", ",", "ein", "Geist", ",", "den", "sein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich zu erkennen, leitet;", "tokens": ["Dich", "zu", "er\u00b7ken\u00b7nen", ",", "lei\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Der Mensch, der Sch\u00f6pfung Ruhm und Preis,", "tokens": ["Der", "Mensch", ",", "der", "Sch\u00f6p\u00b7fung", "Ruhm", "und", "Preis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist sich ein t\u00e4glicher Beweis", "tokens": ["Ist", "sich", "ein", "t\u00e4g\u00b7li\u00b7cher", "Be\u00b7weis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von deiner G\u00fct und Gr\u00f6\u00dfe.", "tokens": ["Von", "dei\u00b7ner", "G\u00fct", "und", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erheb ihn ewig, o mein Geist!", "tokens": ["Er\u00b7heb", "ihn", "e\u00b7wig", ",", "o", "mein", "Geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebe seinen Namen!", "tokens": ["Er\u00b7he\u00b7be", "sei\u00b7nen", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott, unser Vater, sei gepreist,", "tokens": ["Gott", ",", "un\u00b7ser", "Va\u00b7ter", ",", "sei", "ge\u00b7preist", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Welt sag Amen!", "tokens": ["Und", "al\u00b7le", "Welt", "sag", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und alle Welt f\u00fcrcht ihren Herrn,", "tokens": ["Und", "al\u00b7le", "Welt", "f\u00fcrcht", "ih\u00b7ren", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hoff auf ihn, und dien ihm gern!", "tokens": ["Und", "hoff", "auf", "ihn", ",", "und", "di\u00b7en", "ihm", "gern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wer wollte Gott nicht dienen?", "tokens": ["Wer", "woll\u00b7te", "Gott", "nicht", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn ich, o Sch\u00f6pfer! deine Macht,", "tokens": ["Wenn", "ich", ",", "o", "Sch\u00f6p\u00b7fer", "!", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "pu