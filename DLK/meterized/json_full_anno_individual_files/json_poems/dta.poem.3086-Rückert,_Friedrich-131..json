{"dta.poem.3086": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "131.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weisheit lehr' ich dich, die mich das Leben lehrte;", "tokens": ["Die", "Weis\u00b7heit", "lehr'", "ich", "dich", ",", "die", "mich", "das", "Le\u00b7ben", "lehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Weisheit anderwerts gelernt ist nicht von Werthe.", "tokens": ["Denn", "Weis\u00b7heit", "an\u00b7der\u00b7werts", "ge\u00b7lernt", "ist", "nicht", "von", "Wert\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "VAFIN", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Deswegen also wird von Werth und von Gewicht", "tokens": ["Des\u00b7we\u00b7gen", "al\u00b7so", "wird", "von", "Werth", "und", "von", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VAFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr dich besonders auch nicht seyn mein Unterricht.", "tokens": ["F\u00fcr", "dich", "be\u00b7son\u00b7ders", "auch", "nicht", "seyn", "mein", "Un\u00b7ter\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "PTKNEG", "VAINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein ich will dir auch nicht mein Gelerntes geben,", "tokens": ["Al\u00b7lein", "ich", "will", "dir", "auch", "nicht", "mein", "Ge\u00b7lern\u00b7tes", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich lehren will ich nur, zu lernen selbst vom Leben.", "tokens": ["Dich", "leh\u00b7ren", "will", "ich", "nur", ",", "zu", "ler\u00b7nen", "selbst", "vom", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "ADV", "$,", "PTKZU", "VVINF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn, ob das Leben wol ist aller Lehre voll,", "tokens": ["Denn", ",", "ob", "das", "Le\u00b7ben", "wol", "ist", "al\u00b7ler", "Leh\u00b7re", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADV", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst mu\u00df man lernen, wie von ihm man lernen soll.", "tokens": ["Erst", "mu\u00df", "man", "ler\u00b7nen", ",", "wie", "von", "ihm", "man", "ler\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,", "PWAV", "APPR", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}