{"textgrid.poem.64518": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "142. Grabschrift einiger Steine", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Fremder, wie du auch den Weg hieher genommen,", "tokens": ["O", "Frem\u00b7der", ",", "wie", "du", "auch", "den", "Weg", "hie\u00b7her", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du magst aus klein Paris", "tokens": ["Du", "magst", "aus", "klein", "Pa\u00b7ris"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJD", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So hast du, wenn du nicht gar fallend schon geflucht,", "tokens": ["So", "hast", "du", ",", "wenn", "du", "nicht", "gar", "fal\u00b7lend", "schon", "ge\u00b7flucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gewi\u00df doch falschen Grund durch diesen Schlamm gesucht,", "tokens": ["Ge\u00b7wi\u00df", "doch", "fal\u00b7schen", "Grund", "durch", "die\u00b7sen", "Schlamm", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und w\u00fcnschest, da\u00df doch hier in dem verdammten Wege", "tokens": ["Und", "w\u00fcn\u00b7schest", ",", "da\u00df", "doch", "hier", "in", "dem", "ver\u00b7damm\u00b7ten", "We\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zum Trost des Wanderers vom Hainberg Marmor", "tokens": ["Zum", "Trost", "des", "Wan\u00b7de\u00b7rers", "vom", "Hain\u00b7berg", "Mar\u00b7mor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wi\u00df, diesen ganzen Raum, soweit dein Blick sich streckt,", "tokens": ["Wi\u00df", ",", "die\u00b7sen", "gan\u00b7zen", "Raum", ",", "so\u00b7weit", "dein", "Blick", "sich", "streckt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir Steine haben den vor kurzem noch bedeckt.", "tokens": ["Wir", "Stei\u00b7ne", "ha\u00b7ben", "den", "vor", "kur\u00b7zem", "noch", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "APPR", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem Feuer trotzten wir in ungeheuern Massen,", "tokens": ["Dem", "Feu\u00b7er", "trotz\u00b7ten", "wir", "in", "un\u00b7ge\u00b7heu\u00b7ern", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O Menschen, wie verf\u00fchrt euch Rach' und Eifer nicht!", "tokens": ["O", "Men\u00b7schen", ",", "wie", "ver\u00b7f\u00fchrt", "euch", "Rach'", "und", "Ei\u00b7fer", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil uns ", "tokens": ["Weil", "uns"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Uns, die wir uns darauf den Weg zu bessern freuten,", "tokens": ["Uns", ",", "die", "wir", "uns", "da\u00b7rauf", "den", "Weg", "zu", "bes\u00b7sern", "freu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PRF", "PAV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Uns w\u00fcrdigt euer Zorn nicht einmal zu beschreiten,", "tokens": ["Uns", "w\u00fcr\u00b7digt", "eu\u00b7er", "Zorn", "nicht", "ein\u00b7mal", "zu", "be\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "H\u00f6hlt m\u00fchsam Erdreich aus, darein ihr uns versenkt,", "tokens": ["H\u00f6hlt", "m\u00fch\u00b7sam", "Er\u00b7dreich", "aus", ",", "da\u00b7rein", "ihr", "uns", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKVZ", "$,", "PAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Da\u00df Niemand mehr an uns, wir arme Steine! denkt.", "tokens": ["Da\u00df", "Nie\u00b7mand", "mehr", "an", "uns", ",", "wir", "ar\u00b7me", "Stei\u00b7ne", "!", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Verdorrend, weil auf uns sich seine Wurzeln strecken,", "tokens": ["Ver\u00b7dor\u00b7rend", ",", "weil", "auf", "uns", "sich", "sei\u00b7ne", "Wur\u00b7zeln", "stre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPR", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird euren Enkeln uns vielleicht ein Baum entdecken.", "tokens": ["Wird", "eu\u00b7ren", "En\u00b7keln", "uns", "viel\u00b7leicht", "ein", "Baum", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das Schicksal, das uns traf, o Fremder, wei\u00dft du nun,", "tokens": ["Das", "Schick\u00b7sal", ",", "das", "uns", "traf", ",", "o", "Frem\u00b7der", ",", "wei\u00dft", "du", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "La\u00df den bespr\u00fctzten Fu\u00df bey unserm Grabe ruhn,", "tokens": ["La\