{"textgrid.poem.49024": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf, ihr muntern Br\u00fcder,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, ihr muntern Br\u00fcder,", "tokens": ["Auf", ",", "ihr", "mun\u00b7tern", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jubelt mit mir Lieder!", "tokens": ["Ju\u00b7belt", "mit", "mir", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nimmer kommt uns wieder", "tokens": ["Nim\u00b7mer", "kommt", "uns", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Frohe Jugendzeit!", "tokens": ["Fro\u00b7he", "Ju\u00b7gend\u00b7zeit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sei den leichten Scherzen,", "tokens": ["Sei", "den", "leich\u00b7ten", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und dem Gott der Herzen", "tokens": ["Und", "dem", "Gott", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dieser Tag geweiht.", "tokens": ["Die\u00b7ser", "Tag", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00dft an hellen Tagen", "tokens": ["La\u00dft", "an", "hel\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Alte Narren klagen!", "tokens": ["Al\u00b7te", "Nar\u00b7ren", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich mit Grillen plagen,", "tokens": ["Sich", "mit", "Gril\u00b7len", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist dem Bl\u00f6den s\u00fc\u00df!", "tokens": ["Ist", "dem", "Bl\u00f6\u00b7den", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Weise scheuchen Sorgen,", "tokens": ["Wei\u00b7se", "scheu\u00b7chen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sorgen f\u00fcr den Morgen!", "tokens": ["Sor\u00b7gen", "f\u00fcr", "den", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Heute bleibt gewi\u00df!", "tokens": ["Heu\u00b7te", "bleibt", "ge\u00b7wi\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Silberharfen klingen,", "tokens": ["Sil\u00b7ber\u00b7har\u00b7fen", "klin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freie M\u00e4dchen singen,", "tokens": ["Frei\u00b7e", "M\u00e4d\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Br\u00fcder, la\u00dft uns springen,", "tokens": ["Br\u00fc\u00b7der", ",", "la\u00dft", "uns", "sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Springen goldnen Wein!", "tokens": ["Sprin\u00b7gen", "gold\u00b7nen", "Wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wo sich Scherze wiegen,", "tokens": ["Wo", "sich", "Scher\u00b7ze", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Blonde Locken fliegen,", "tokens": ["Blon\u00b7de", "Lo\u00b7cken", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kann man luftig sein!", "tokens": ["Kann", "man", "luf\u00b7tig", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Tanzet um die F\u00e4sser!", "tokens": ["Tan\u00b7zet", "um", "die", "F\u00e4s\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Freude lachet besser,", "tokens": ["Freu\u00b7de", "la\u00b7chet", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sto\u00dfet ihr die Gl\u00e4ser!", "tokens": ["Sto\u00b7\u00dfet", "ihr", "die", "Gl\u00e4\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Welch ein froher Klang!", "tokens": ["Welch", "ein", "fro\u00b7her", "Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Klingt die Urne besser", "tokens": ["Klingt", "die", "Ur\u00b7ne", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "An Cocyts Gew\u00e4sser,", "tokens": ["An", "Co\u00b7cyts", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Oder Grabgesang?", "tokens": ["O\u00b7der", "Grab\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Freier M\u00e4dchen Nicken,", "tokens": ["Frei\u00b7er", "M\u00e4d\u00b7chen", "Ni\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Runde Busen schm\u00fccken,", "tokens": ["Run\u00b7de", "Bu\u00b7sen", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weiche H\u00e4nde dr\u00fccken,", "tokens": ["Wei\u00b7che", "H\u00e4n\u00b7de", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "O wie s\u00fc\u00df, wie s\u00fc\u00df!", "tokens": ["O", "wie", "s\u00fc\u00df", ",", "wie", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Unter frohen Ch\u00f6ren", "tokens": ["Un\u00b7ter", "fro\u00b7hen", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Volle Becher leeren,", "tokens": ["Vol\u00b7le", "Be\u00b7cher", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "O wie s\u00fc\u00df, wie s\u00fc\u00df!", "tokens": ["O", "wie", "s\u00fc\u00df", ",", "wie", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "J\u00fcnglinge, die Losen,", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", ",", "die", "Lo\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Werfen uns mit Rosen,", "tokens": ["Wer\u00b7fen", "uns", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df wir ihre blo\u00dfen,", "tokens": ["Da\u00df", "wir", "ih\u00b7re", "blo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wei\u00dfen Arme sehn.", "tokens": ["Wei\u00b7\u00dfen", "Ar\u00b7me", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "L\u00f6set mit mir B\u00e4nder!", "tokens": ["L\u00f6\u00b7set", "mit", "mir", "B\u00e4n\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Streift die Brustgew\u00e4nder!", "tokens": ["Streift", "die", "Brust\u00b7ge\u00b7w\u00e4n\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nackend sind sie sch\u00f6n!", "tokens": ["Na\u00b7ckend", "sind", "sie", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Spiegle mir, du kleine", "tokens": ["Spieg\u00b7le", "mir", ",", "du", "klei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Blonde, hier im Weine", "tokens": ["Blon\u00b7de", ",", "hier", "im", "Wei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deine wei\u00dfe, reine", "tokens": ["Dei\u00b7ne", "wei\u00b7\u00dfe", ",", "rei\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Marmorbrust geschwind!", "tokens": ["Mar\u00b7mor\u00b7brust", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O! du schwebst im Weine,", "tokens": ["O", "!", "du", "schwebst", "im", "Wei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie im Strahlenhaine", "tokens": ["Wie", "im", "Strah\u00b7len\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ein verg\u00f6ttert Kind!", "tokens": ["Ein", "ver\u00b7g\u00f6t\u00b7tert", "Kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "K\u00fc\u00dfchen h\u00f6r' ich tauschen,", "tokens": ["K\u00fc\u00df\u00b7chen", "h\u00f6r'", "ich", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kleine Pfeilchen rauschen,", "tokens": ["Klei\u00b7ne", "Pfeil\u00b7chen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Amorn seh' ich lauschen,", "tokens": ["A\u00b7morn", "seh'", "ich", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fr\u00f6hlich h\u00fcpft er her.", "tokens": ["Fr\u00f6h\u00b7lich", "h\u00fcpft", "er", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Neben seiner Seite", "tokens": ["Ne\u00b7ben", "sei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schwingt die lose Freude", "tokens": ["Schwingt", "die", "lo\u00b7se", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ihren gr\u00fcnen Speer.", "tokens": ["Ih\u00b7ren", "gr\u00fc\u00b7nen", "Speer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "\u00dcber Schwanenbetten", "tokens": ["\u00dc\u00b7ber", "Schwa\u00b7nen\u00b7bet\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fl\u00fcchten sie, und retten", "tokens": ["Fl\u00fcch\u00b7ten", "sie", ",", "und", "ret\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich in Blumenketten;", "tokens": ["Sich", "in", "Blu\u00b7men\u00b7ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Cypris tanzt herab.", "tokens": ["Cyp\u00b7ris", "tanzt", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ihren Sohn zu strafen,", "tokens": ["Ih\u00b7ren", "Sohn", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der 