{"dta.poem.1361": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ruhe der Seelen in GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ach/ wo soll ich Ruhe finden\nF\u00fcr den Ohnmachts-vollen Geist?", "tokens": ["Ach", "/", "wo", "soll", "ich", "Ru\u00b7he", "fin\u00b7den", "F\u00fcr", "den", "Ohn\u00b7machts\u00b7vol\u00b7len", "Geist", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "PWAV", "VMFIN", "PPER", "NN", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn der Sonnen Glantz verreist/", "tokens": ["Wenn", "der", "Son\u00b7nen", "Glantz", "ver\u00b7reist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Tages Kr\u00e4ffte schwinden/", "tokens": ["Und", "des", "Ta\u00b7ges", "Kr\u00e4ff\u00b7te", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eil ich zwar dem Lager zu/", "tokens": ["Eil", "ich", "zwar", "dem", "La\u00b7ger", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wo findt das Hertze Ruh?", "tokens": ["Doch", "wo", "findt", "das", "Hert\u00b7ze", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PDS", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach der rauhen St\u00fcrme Bellen/", "tokens": ["Nach", "der", "rau\u00b7hen", "St\u00fcr\u00b7me", "Bel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche manches Schiff bekriegt/", "tokens": ["Wel\u00b7che", "man\u00b7ches", "Schiff", "be\u00b7kriegt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Furcht und Angst gewiegt/", "tokens": ["Un\u00b7ter", "Furcht", "und", "Angst", "ge\u00b7wiegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilt es bey gestillten Wellen", "tokens": ["Eilt", "es", "bey", "ge\u00b7still\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auff den sichren Hafen zu/", "tokens": ["Auff", "den", "sich\u00b7ren", "Ha\u00b7fen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo findt meine Seele Ruh?", "tokens": ["Wo", "findt", "mei\u00b7ne", "See\u00b7le", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Nimmt der lange Tag ein Ende/", "tokens": ["Nimmt", "der", "lan\u00b7ge", "Tag", "ein", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So h\u00f6rt auch der Arbeit Lauff", "tokens": ["So", "h\u00f6rt", "auch", "der", "Ar\u00b7beit", "Lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und das heisse Schwitzen auff/", "tokens": ["Und", "das", "heis\u00b7se", "Schwit\u00b7zen", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcde R\u00fccken/ matte H\u00e4nde", "tokens": ["M\u00fc\u00b7de", "R\u00fc\u00b7cken", "/", "mat\u00b7te", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$(", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden frey von ihrer Last:", "tokens": ["Wer\u00b7den", "frey", "von", "ih\u00b7rer", "Last", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo findt meine Seele Rast?", "tokens": ["Wo", "findt", "mei\u00b7ne", "See\u00b7le", "Rast", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Auff der Wolcken nasses Weinen", "tokens": ["Auff", "der", "Wol\u00b7cken", "nas\u00b7ses", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeiget sich der Sonnen Licht;", "tokens": ["Zei\u00b7get", "sich", "der", "Son\u00b7nen", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der kalte Winter bricht", "tokens": ["Wenn", "der", "kal\u00b7te", "Win\u00b7ter", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df die Fr\u00fchlings-Lust erscheinen/", "tokens": ["Mu\u00df", "die", "Fr\u00fch\u00b7lings\u00b7Lust", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr mein tr\u00fcbes Hertz allein", "tokens": ["F\u00fcr", "mein", "tr\u00fc\u00b7bes", "Hertz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will nicht Licht/ nicht Sonne seyn.", "tokens": ["Will", "nicht", "Licht", "/", "nicht", "Son\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "NN", "$(", "PTKNEG", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vogel nehmen unter Zweigen", "tokens": ["Vo\u00b7gel", "neh\u00b7men", "un\u00b7ter", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcner B\u00e4ume sichern Stand/", "tokens": ["Gr\u00fc\u00b7ner", "B\u00e4u\u00b7me", "si\u00b7chern", "Stand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thiere streichen durch das Land/", "tokens": ["Thie\u00b7re", "strei\u00b7chen", "durch", "das", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben doch ihr Lager eigen/", "tokens": ["Ha\u00b7ben", "doch", "ihr", "La\u00b7ger", "ei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhen offt bey d\u00fcstrer Nacht/", "tokens": ["Ru\u00b7hen", "offt", "bey", "d\u00fcst\u00b7rer", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn mein feuchtes Auge wacht.", "tokens": ["Wenn", "mein", "feuch\u00b7tes", "Au\u00b7ge", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob ich bey den eitlen Freuden", "tokens": ["Ob", "ich", "bey", "den", "eit\u00b7len", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irrdsche Ruhe suchen will/", "tokens": ["Irrd\u00b7sche", "Ru\u00b7he", "su\u00b7chen", "will", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fehl ich doch das rechte Ziel.", "tokens": ["Fehl", "ich", "doch", "das", "rech\u00b7te", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lust wird mir zu Last und Leyden/", "tokens": ["Lust", "wird", "mir", "zu", "Last", "und", "Ley\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NE", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich mich zu zwingen thu:", "tokens": ["Was", "ich", "mich", "zu", "zwin\u00b7gen", "thu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo findt meine Seele Ruh?", "tokens": ["Wo", "findt", "mei\u00b7ne", "See\u00b7le", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Nehm ich Zuflucht zu den H\u00f6hen", "tokens": ["Nehm", "ich", "Zu\u00b7flucht", "zu", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Dieser ungetreuen Welt/", "tokens": ["Die\u00b7ser", "un\u00b7ge\u00b7treu\u00b7en", "Welt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches Fall-Bret ist gestellt/", "tokens": ["Man\u00b7ches", "Fall\u00b7Bret", "ist", "ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man sicher denckt zu stehen/", "tokens": ["Wo", "man", "si\u00b7cher", "denckt", "zu", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gunst verschwindt in einem Nu;", "tokens": ["Gunst", "ver\u00b7schwindt", "in", "ei\u00b7nem", "Nu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo findt meine Seele Ruh?", "tokens": ["Wo", "findt", "mei\u00b7ne", "See\u00b7le", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Herr des Himmels und der Erden/", "tokens": ["Herr", "des", "Him\u00b7mels", "und", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeige mir die gutte Bahn/", "tokens": ["Zei\u00b7ge", "mir", "die", "gut\u00b7te", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich Ruhe finden kan/", "tokens": ["Wo", "ich", "Ru\u00b7he", "fin\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ich sicher f\u00fcr Beschwerden/", "tokens": ["Wo", "ich", "si\u00b7cher", "f\u00fcr", "Be\u00b7schwer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "tr