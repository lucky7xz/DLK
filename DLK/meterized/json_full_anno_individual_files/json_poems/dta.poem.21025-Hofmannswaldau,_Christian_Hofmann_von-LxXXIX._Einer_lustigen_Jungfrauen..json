{"dta.poem.21025": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIX.  \n Einer lustigen Jungfrauen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hier lieget ", "tokens": ["Hier", "lie\u00b7get"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Jhr Mund hat nie gew\u00fcntscht ein eigen Grab zu", "tokens": ["Ihr", "Mund", "hat", "nie", "ge\u00b7w\u00fcnt\u00b7scht", "ein", "ei\u00b7gen", "Grab", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "haben.", "tokens": ["ha\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sie bat der Freunde Hand zu schreiben auf den", "tokens": ["Sie", "bat", "der", "Freun\u00b7de", "Hand", "zu", "schrei\u00b7ben", "auf", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Stein/", "tokens": ["Stein", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Gleich wie der K\u00f6rper war/ so sol die Grabstadt seyn.", "tokens": ["Gleich", "wie", "der", "K\u00f6r\u00b7per", "war", "/", "so", "sol", "die", "Grab\u00b7stadt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VAFIN", "$(", "ADV", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}