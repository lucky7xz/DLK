 "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "NE", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die jetzt noch schlafen? gib die Gesetze, gib", "tokens": ["Die", "jetzt", "noch", "schla\u00b7fen", "?", "gib", "die", "Ge\u00b7set\u00b7ze", ",", "gib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ADV", "VVINF", "$.", "VVIMP", "ART", "NN", "$,", "VVIMP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Uns Leben, siege, Meister, du nur", "tokens": ["Uns", "Le\u00b7ben", ",", "sie\u00b7ge", ",", "Meis\u00b7ter", ",", "du", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "VVFIN", "$,", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hast der Eroberung Recht, wie Bacchus.", "tokens": ["Hast", "der", "Er\u00b7o\u00b7be\u00b7rung", "Recht", ",", "wie", "Bac\u00b7chus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "PWAV", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Nicht, was wohl sonst des Menschen Geschick und Sorg", "tokens": ["Nicht", ",", "was", "wohl", "sonst", "des", "Men\u00b7schen", "Ge\u00b7schick", "und", "Sorg"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Haus und unter offenem Himmel ist,", "tokens": ["Im", "Haus", "und", "un\u00b7ter", "of\u00b7fe\u00b7nem", "Him\u00b7mel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos"