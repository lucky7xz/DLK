er": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Damit wir bleiben m\u00f6gen, die Augen zu.", "tokens": ["Da\u00b7mit", "wir", "blei\u00b7ben", "m\u00f6\u00b7gen", ",", "die", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht liebt er Wildes! Doch es zwinget", "tokens": ["Nicht", "liebt", "er", "Wil\u00b7des", "!", "Doch", "es", "zwin\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmer die weite Gewalt den Himmel.", "tokens": ["Nim\u00b7mer", "die", "wei\u00b7te", "Ge\u00b7walt", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.15": {"line.1": {"text": "Noch ists auch gut, zu weise zu sein. Ihn kennt", "tokens": ["Noch", "ists", "auch", "gut", ",", "zu", "wei\u00b7se", "zu", "sein", ".", "Ihn", "kennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "PTKA", "ADJD", "PTKZU", "VAINF", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Dank. Doch nicht beh\u00e4lt er es leicht allein,", "tokens": ["Der", "Dank", ".", "Doch", "nicht", "be\u00b7h\u00e4lt", "er", "es", "leicht", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "PTKNEG", "VVFIN", "PPER", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gern gesellt, damit verstehn sie", "tokens": ["Und", "gern", "ge\u00b7sellt", ",", "da\u00b7mit", "ver\u00b7stehn", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "PAV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Helfen, zu anderen sich ein Dichter.", "tokens": ["Hel\u00b7fen", ",", "zu", "an\u00b7de\u00b7ren", "sich", "ein", "Dich\u00b7ter."], "token_info": ["word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "$,", "APPR", "PIS", "PRF", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.16": {"line.1": {"text": "Furchtlos bleibt aber, so er es mu\u00df, der Mann", "tokens": ["Furcht\u00b7los", "bleibt", "a\u00b7ber", ",", "so", "er", "es", "mu\u00df", ",", "der", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$,", "ADV", "PPER", "PPER", "VMFIN", "$,", "ART", "NN"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Einsam vor Gott, es sch\u00fctzet die Einfalt ihn,", "tokens": ["Ein\u00b7sam", "vor", "Gott", ",", "es", "sch\u00fct\u00b7zet", "die", "Ein\u00b7falt", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und keiner Waffen brauchts und keiner", "tokens": ["Und", "kei\u00b7ner", "Waf\u00b7fen", "brauchts", "und", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Listen, so lange, bis Gottes Fehl hilft.", "tokens": 