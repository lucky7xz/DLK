{"textgrid.poem.67148": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Ganges Ufer h\u00f6rten des Freudengotts", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Ganges Ufer h\u00f6rten des Freudengotts", "tokens": ["Des", "Gan\u00b7ges", "U\u00b7fer", "h\u00f6r\u00b7ten", "des", "Freu\u00b7den\u00b7gotts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Triumph, als allerobernd vom Indus her", "tokens": ["Tri\u00b7umph", ",", "als", "al\u00b7le\u00b7ro\u00b7bernd", "vom", "In\u00b7dus", "her"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADJD", "APPRART", "NE", "APZR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der junge Bacchus kam, mit heilgem", "tokens": ["Der", "jun\u00b7ge", "Bac\u00b7chus", "kam", ",", "mit", "heil\u00b7gem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weine vom Schlafe die V\u00f6lker weckend.", "tokens": ["Wei\u00b7ne", "vom", "Schla\u00b7fe", "die", "V\u00f6l\u00b7ker", "we\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und du, des Tages Engel! erweckst sie nicht,", "tokens": ["Und", "du", ",", "des", "Ta\u00b7ges", "En\u00b7gel", "!", "er\u00b7weckst", "sie", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "NE", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die jetzt noch schlafen? gib die Gesetze, gib", "tokens": ["Die", "jetzt", "noch", "schla\u00b7fen", "?", "gib", "die", "Ge\u00b7set\u00b7ze", ",", "gib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ADV", "VVINF", "$.", "VVIMP", "ART", "NN", "$,", "VVIMP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Uns Leben, siege, Meister, du nur", "tokens": ["Uns", "Le\u00b7ben", ",", "sie\u00b7ge", ",", "Meis\u00b7ter", ",", "du", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "VVFIN", "$,", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hast der Eroberung Recht, wie Bacchus.", "tokens": ["Hast", "der", "Er\u00b7o\u00b7be\u00b7rung", "Recht", ",", "wie", "Bac\u00b7chus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "PWAV", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Nicht, was wohl sonst des Menschen Geschick und Sorg", "tokens": ["Nicht", ",", "was", "wohl", "sonst", "des", "Men\u00b7schen", "Ge\u00b7schick", "und", "Sorg"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PRELS", "ADV", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Haus und unter offenem Himmel ist,", "tokens": ["Im", "Haus", "und", "un\u00b7ter", "of\u00b7fe\u00b7nem", "Him\u00b7mel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn edler, denn das Wild, der Mann sich", "tokens": ["Wenn", "ed\u00b7ler", ",", "denn", "das", "Wild", ",", "der", "Mann", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJA", "$,", "KON", "ART", "NN", "$,", "ART", "NN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wehret und n\u00e4hrt! denn es gilt ein anders,", "tokens": ["Weh\u00b7ret", "und", "n\u00e4hrt", "!", "denn", "es", "gilt", "ein", "an\u00b7ders", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$.", "KON", "PPER", "VVFIN", "ART", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Zu Sorg und Dienst den Dichtenden anvertraut!", "tokens": ["Zu", "Sorg", "und", "Dienst", "den", "Dich\u00b7ten\u00b7den", "an\u00b7ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der H\u00f6chste, der ists, dem wir geeignet sind,", "tokens": ["Der", "H\u00f6chs\u00b7te", ",", "der", "ists", ",", "dem", "wir", "ge\u00b7eig\u00b7net", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df n\u00e4her, immerneu besungen", "tokens": ["Da\u00df", "n\u00e4\u00b7her", ",", "im\u00b7mer\u00b7neu", "be\u00b7sun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJD", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn die befreundete Brust vernehme.", "tokens": ["Ihn", "die", "be\u00b7freun\u00b7de\u00b7te", "Brust", "ver\u00b7neh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und dennoch, o ihr Himmlischen all, und all", "tokens": ["Und", "den\u00b7noch", ",", "o", "ihr", "Himm\u00b7li\u00b7schen", "all", ",", "und", "all"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "FM", "PPOSAT", "NN", "PIAT", "$,", "KON", "PIAT"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Quellen und ihr Ufer und Hain' und H\u00f6hn,", "tokens": ["Ihr", "Quel\u00b7len", "und", "ihr", "U\u00b7fer", "und", "Hain'", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo wunderbar zuerst, als du die", "tokens": ["Wo", "wun\u00b7der\u00b7bar", "zu\u00b7erst", ",", "als", "du", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Locken ergriffen, und unverge\u00dflich", "tokens": ["Lo\u00b7cken", "er\u00b7grif\u00b7fen", ",", "und", "un\u00b7ver\u00b7ge\u00df\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "KON", "ADJD"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der unverhoffte Genius \u00fcber uns", "tokens": ["Der", "un\u00b7ver\u00b7hoff\u00b7te", "Ge\u00b7nius", "\u00fc\u00b7ber", "uns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6pferische, g\u00f6ttliche kam, da\u00df stumm", "tokens": ["Der", "sch\u00f6p\u00b7fe\u00b7ri\u00b7sche", ",", "g\u00f6tt\u00b7li\u00b7che", "kam", ",", "da\u00df", "stumm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "VVFIN", "$,", "KOUS", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Sinn uns ward und, wie vom", "tokens": ["Der", "Sinn", "uns", "ward", "und", ",", "wie", "vom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "VAFIN", "KON", "$,", "PWAV", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Strahle ger\u00fchrt, das Gebein erbebte,", "tokens": ["Strah\u00b7le", "ge\u00b7r\u00fchrt", ",", "das", "Ge\u00b7bein", "er\u00b7beb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ihr ruhelosen Taten in weiter Welt!", "tokens": ["Ihr", "ru\u00b7he\u00b7lo\u00b7sen", "Ta\u00b7ten", "in", "wei\u00b7ter", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr Schicksalstag', ihr rei\u00dfenden, wenn der Gott", "tokens": ["Ihr", "Schick\u00b7sals\u00b7tag'", ",", "ihr", "rei\u00b7\u00dfen\u00b7den", ",", "wenn", "der", "Gott"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stillsinnend lenkt, wohin zorntrunken", "tokens": ["Still\u00b7sin\u00b7nend", "lenkt", ",", "wo\u00b7hin", "zorn\u00b7trun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn die gigantischen Rosse bringen,", "tokens": ["Ihn", "die", "gi\u00b7gan\u00b7ti\u00b7schen", "Ros\u00b7se", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Euch sollten wir verschweigen, und wenn in uns", "tokens": ["Euch", "soll\u00b7ten", "wir", "ver\u00b7schwei\u00b7gen", ",", "und", "wenn", "in", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "KON", "KOUS", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom stetigstillen Jahre der Wohllaut t\u00f6nt,", "tokens": ["Vom", "ste\u00b7tigs\u00b7til\u00b7len", "Jah\u00b7re", "der", "Wohl\u00b7laut", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So sollt es klingen, gleich als h\u00e4tte", "tokens": ["So", "sollt", "es", "klin\u00b7gen", ",", "gleich", "als", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "ADV", "KOKOM", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mutig und m\u00fc\u00dfig ein Kind des Meisters", "tokens": ["Mu\u00b7tig", "und", "m\u00fc\u00b7\u00dfig", "ein", "Kind", "des", "Meis\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Geweihte, reine Saiten im Scherz ger\u00fchrt?", "tokens": ["Ge\u00b7weih\u00b7te", ",", "rei\u00b7ne", "Sai\u00b7ten", "im", "Scherz", "ge\u00b7r\u00fchrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und darum hast du, Dichter! des Orients", "tokens": ["Und", "da\u00b7rum", "hast", "du", ",", "Dich\u00b7ter", "!", "des", "O\u00b7rients"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER", "$,", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Propheten und den Griechensang und", "tokens": ["Pro\u00b7phe\u00b7ten", "und", "den", "Grie\u00b7chen\u00b7sang", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Neulich die Donner geh\u00f6rt, damit du", "tokens": ["Neu\u00b7lich", "die", "Don\u00b7ner", "ge\u00b7h\u00f6rt", ",", "da\u00b7mit", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Den Geist zu Diensten brauchst und die Gegenwart", "tokens": ["Den", "Geist", "zu", "Diens\u00b7ten", "brauchst", "und", "die", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Guten \u00fcbereilest, in Spott, und den Albernen", "tokens": ["Des", "Gu\u00b7ten", "\u00fc\u00b7be\u00b7rei\u00b7lest", ",", "in", "Spott", ",", "und", "den", "Al\u00b7ber\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verleugnest, herzlos, und zum Spiele", "tokens": ["Ver\u00b7leug\u00b7nest", ",", "herz\u00b7los", ",", "und", "zum", "Spie\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feil, wie gefangenes Wild, ihn treibest?", "tokens": ["Feil", ",", "wie", "ge\u00b7fan\u00b7ge\u00b7nes", "Wild", ",", "ihn", "trei\u00b7best", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Bis aufgereizt vom Stachel im Grimme der", "tokens": ["Bis", "auf\u00b7ge\u00b7reizt", "vom", "Sta\u00b7chel", "im", "Grim\u00b7me", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPRART", "NN", "APPRART", "NN", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Ursprungs sich erinnert und ruft, da\u00df selbst", "tokens": ["Des", "Ur\u00b7sprungs", "sich", "e\u00b7rin\u00b7nert", "und", "ruft", ",", "da\u00df", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Meister kommt, dann unter hei\u00dfen", "tokens": ["Der", "Meis\u00b7ter", "kommt", ",", "dann", "un\u00b7ter", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Todesgeschossen entseelt dich l\u00e4sset.", "tokens": ["To\u00b7des\u00b7ge\u00b7schos\u00b7sen", "ent\u00b7seelt", "dich", "l\u00e4s\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Zu lang ist alles G\u00f6ttliche dienstbar schon", "tokens": ["Zu", "lang", "ist", "al\u00b7les", "G\u00f6tt\u00b7li\u00b7che", "dienst\u00b7bar", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PIAT", "NN", "ADJD", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und alle Himmelskr\u00e4fte verscherzt, verbraucht", "tokens": ["Und", "al\u00b7le", "Him\u00b7mels\u00b7kr\u00e4f\u00b7te", "ver\u00b7scherzt", ",", "ver\u00b7braucht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die G\u00fctigen, zur Lust, danklos, ein", "tokens": ["Die", "G\u00fc\u00b7ti\u00b7gen", ",", "zur", "Lust", ",", "dank\u00b7los", ",", "ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,", "ADJD", "$,", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schlaues Geschlecht und zu kennen w\u00e4hnt es,", "tokens": ["Schlau\u00b7es", "Ge\u00b7schlecht", "und", "zu", "ken\u00b7nen", "w\u00e4hnt", "es", ","], "token_info