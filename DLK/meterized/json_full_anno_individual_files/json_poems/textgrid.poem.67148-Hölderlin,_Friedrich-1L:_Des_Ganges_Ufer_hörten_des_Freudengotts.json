: "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Guten \u00fcbereilest, in Spott, und den Albernen", "tokens": ["Des", "Gu\u00b7ten", "\u00fc\u00b7be\u00b7rei\u00b7lest", ",", "in", "Spott", ",", "und", "den", "Al\u00b7ber\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verleugnest, herzlos, und zum Spiele", "tokens": ["Ver\u00b7leug\u00b7nest", ",", "herz\u00b7los", ",", "und", "zum", "Spie\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feil, wie gefangenes Wild, ihn treibest?", "tokens": ["Feil", ",", "wie", "ge\u00b7fan\u00b7ge\u00b7nes", "Wild", ",", "ihn", "trei\u00b7best", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.27": {"line.1": {"text": "Bis aufgereizt vom Stachel im Grimme der", "tokens": ["Bis", "auf\u00b7ge\u00b7reizt", "vom", "Sta\u00b7chel", "im", "Grim\u00b7me", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPRART", "NN", "APPRART", "NN", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Ursprungs sich erinnert und ruft, da\u00df selbst", "tokens": ["Des", "Ur\u00b7sprungs", "sich", "e\u00b7rin\u00b7nert", "und", "ruft", ",", "da\u00df", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Meister kommt, dann unter hei\u00dfen", "tokens": ["Der", "Meis\u00b7ter", "kommt", ",", "dann", "un\u00b7ter", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Todesgeschossen entseelt dich l\u00e4sset.", "tokens": ["To\u00b7des\u00b7ge\u00b7schos\u00b7sen", "ent\u00b7seelt", "dich", "l\u00e4s\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.28": {"line.1": {"text": "Zu lang ist alles G\u00f6ttliche dienstbar schon", "tokens": ["Zu", "lang", "ist", "al\u00b7les", "G\u00f6tt\u00b7li\u00b7che", "dienst\u00b7bar", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PIAT", "NN", "ADJD", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und alle Himmelskr\u00e4fte verscherzt, verbraucht", "tokens": ["Und", "al\u00b7le", "Him\u00b7mels\u00b7kr\u00e4f\u00b7te", "ver\u00b7scherzt", ",", "ver\u00b7braucht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die G\u00fctigen, zur Lust, danklos, ein", "tokens": ["Die", "G\u00fc\u00b7ti\u00b7gen", ",", "zur", "Lust", ",", "dank\u00b7los", ",", "ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,", "ADJD", "$,", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schlaues Geschlecht und zu kennen w\u00e4hnt es,", "tokens": ["Schlau\u00b7es", "Ge\u00b7schlecht", "und", "zu", "ken\u00b7nen", "w\u00e4hnt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PTKZU", "VVINF", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.29": {"line.1": {"text": "Wenn ihnen der Erhabne den Acker baut,", "tokens": ["Wenn", "ih\u00b7nen", "der", "Er\u00b7hab\u00b7ne", "den", "A\u00b7cker", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Tagslicht und den Donnerer, und es sp\u00e4ht", "tokens": ["Das", "Tags\u00b7licht", "und", "den", "Don\u00b7ne\u00b7rer", ",", "und", "es", "sp\u00e4ht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Sehrohr wohl sie all und z\u00e4hlt und", "tokens": ["Das", "Seh\u00b7rohr", "wohl", "sie", "all", "und", "z\u00e4hlt", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPER", "PIAT", "KON", "VVFIN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nennet mit Namen des Himmels Sterne.", "tokens": ["Nen\u00b7net", "mit", "Na\u00b7men", "des", "Him\u00b7mels", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.30": {"line.1": {"text": "Der Vater aber decket mit heilger Nacht,", "tokens": ["Der", "Va\u00b7ter", "a\u00b7ber", "de\u00b7cket", "mit", "heil\u00b7ger", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Damit wir bleiben m\u00f6gen, die Augen zu.", "tokens": ["Da\u00b7mit", "wir", "blei\u00b7ben", "m\u00f6\u00b7gen", ",", "die", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht liebt er Wildes! Doch es zwinget", "tokens": ["Nicht", "liebt", "er", "Wil\u00b7des", "!", "Doch", "es", "zwin\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "NN", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmer die weite Gewalt den Himmel.", "tokens": ["Nim\u00b7mer", "die", "wei\u00b7te", "Ge\u00b7walt", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.31": {"line.1": {"text": "Noch ists auch gut, zu weise zu sein. Ihn kennt", "tokens": ["Noch", "ists", "auch", "gut", ",", "zu", "wei\u00b7se", "zu", "sein", ".", "Ihn", "kennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "PTKA", "ADJD", "PTKZU", "VAINF", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Dank. Doch nicht beh\u00e4lt er es leicht allein,", "tokens": ["Der", "Dank", ".", "Doch", "nicht", "be\u00b7h\u00e4lt", "er", "es", "leicht", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "PTKNEG", "VVFIN", "PPER", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gern gesellt, damit verstehn sie", "tokens": ["Und", "gern", "ge\u00b7sellt", ",", "da\u00b7mit", "ver\u00b7stehn", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "PAV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Helfen, zu anderen sich ein Dichter.", "tokens": ["Hel\u00b7fen", ",", "zu", "an\u00b7de\u00b7ren", "sich", "ein", "Dich\u00b7ter."], "token_info": ["word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "$,", "APPR", "PIS", "PRF", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.32": {"line.1": {"text": "Furchtlos bleibt aber, so er es mu\u00df, der Mann", "tokens": ["Furcht\u00b7los", "bleibt", "a\u00b7ber", ",", "so", "er", "es", "mu\u00df", ",", "der", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$,", "ADV", "PPER", "PPER", "VMFIN", "$,", "ART", "NN"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Einsam vor Gott, es sch\u00fctzet die Einfalt ihn,", "tokens": ["Ein\u00b7sam", "vor", "Gott", ",", "es", "sch\u00fct\u00b7zet", "die", "Ein\u00b7falt", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und keiner Waffen brauchts und keiner", "tokens": ["Und", "kei\u00b7ner", "Waf\u00b7fen", "brauchts", "und", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Listen, so lange, bis Gottes Fehl hilft.