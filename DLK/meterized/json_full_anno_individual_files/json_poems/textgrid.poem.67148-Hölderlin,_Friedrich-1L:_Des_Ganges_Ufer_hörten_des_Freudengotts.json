, "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn die befreundete Brust vernehme.", "tokens": ["Ihn", "die", "be\u00b7freun\u00b7de\u00b7te", "Brust", "ver\u00b7neh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "Und dennoch, o ihr Himmlischen all, und all", "tokens": ["Und", "den\u00b7noch", ",", "o", "ihr", "Himm\u00b7li\u00b7schen", "all", ",", "und", "all"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "FM", "PPOSAT", "NN", "PIAT", "$,", "KON", "PIAT"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Quellen und ihr Ufer und Hain' und H\u00f6hn,", "tokens": ["Ihr", "Quel\u00b7len", "und", "ihr", "U\u00b7fer", "und", "Hain'", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo wunderbar zuerst, als du die", "tokens": ["Wo", "wun\u00b7der\u00b7bar", "zu\u00b7erst", ",", "als", "du", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Locken ergriffen, und unverge\u00dflich", "tokens": ["Lo\u00b7cken", "er\u00b7grif\u00b7fen", ",", "und", "un\u00b7ver\u00b7ge\u00df\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "KON", "ADJD"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.22": {"line.1": {"text": "Der unverhoffte Genius \u00fcber uns", "tokens": ["Der", "un\u00b7ver\u00b7hoff\u00