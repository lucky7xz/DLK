{"textgrid.poem.66834": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Mutter, w\u00e4re das Leben", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Mutter, w\u00e4re das Leben", "tokens": ["O", "Mut\u00b7ter", ",", "w\u00e4\u00b7re", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie du so gut, so sch\u00f6n,", "tokens": ["Wie", "du", "so", "gut", ",", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit lichtem Mantel schweben", "tokens": ["Mit", "lich\u00b7tem", "Man\u00b7tel", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollt' ich durch Tiefen und H\u00f6hn.", "tokens": ["Wollt'", "ich", "durch", "Tie\u00b7fen", "und", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Auf meinem Haupte die Krone", "tokens": ["Auf", "mei\u00b7nem", "Haup\u00b7te", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von sonnengoldnem Kristall", "tokens": ["Von", "son\u00b7nen\u00b7gold\u00b7nem", "Kris\u00b7tall"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "S\u00e4ng' ich zu deinem Lohne", "tokens": ["S\u00e4ng'", "ich", "zu", "dei\u00b7nem", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lieder von silbernem Schall.", "tokens": ["Lie\u00b7der", "von", "sil\u00b7ber\u00b7nem", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Der Menschen bes\u00e4nftigt Gewimmel", "tokens": ["Der", "Men\u00b7schen", "be\u00b7s\u00e4nf\u00b7tigt", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Lauschte in frommer Ruh,", "tokens": ["Lauschte", "in", "from\u00b7mer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Droben aus blauem Himmel", "tokens": ["Dro\u00b7ben", "aus", "blau\u00b7em", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nickte mir Jesus zu.", "tokens": ["Nick\u00b7te", "mir", "Je\u00b7sus", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "O Mutter, w\u00e4re das Leben", "tokens": ["O", "Mut\u00b7ter", ",", "w\u00e4\u00b7re", "das", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So sch\u00f6n und gut wie du \u2013", "tokens": ["So", "sch\u00f6n", "und", "gut", "wie", "du", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir aber alle kleben", "tokens": ["Wir", "a\u00b7ber", "al\u00b7le", "kle\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Staube immerzu.", "tokens": ["Am", "Stau\u00b7be", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Lieben, ich mu\u00dfte singen,", "tokens": ["Ihr", "Lie\u00b7ben", ",", "ich", "mu\u00df\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was in das Herz euch stach,", "tokens": ["Was", "in", "das", "Herz", "euch", "stach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schmach mu\u00df der Dichter bringen", "tokens": ["Schmach", "mu\u00df", "der", "Dich\u00b7ter", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Schande, Schande der Schmach.", "tokens": ["Der", "Schan\u00b7de", ",", "Schan\u00b7de", "der", "Schmach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Mit sch\u00f6nen Worten kr\u00f6nen", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Wor\u00b7ten", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich, was rein und echt,", "tokens": ["Will", "ich", ",", "was", "rein", "und", "echt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Niedertracht zu h\u00f6hnen,", "tokens": ["Die", "Nie\u00b7der\u00b7tracht", "zu", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mir kein Wort zu schlecht.", "tokens": ["Ist", "mir", "kein", "Wort", "zu", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da\u00df zu des Drachen Schaden", "tokens": ["Da\u00df", "zu", "des", "Dra\u00b7chen", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich schirme h\u00fcrnene Haut,", "tokens": ["Mich", "schir\u00b7me", "h\u00fcr\u00b7ne\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mu\u00df ich im Bad mich baden,", "tokens": ["Mu\u00df", "ich", "im", "Bad", "mich", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NE", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Davor mir selber graut ...", "tokens": ["Da\u00b7vor", "mir", "sel\u00b7ber", "graut", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich mu\u00df durch Gassen gehen,", "tokens": ["Ich", "mu\u00df", "durch", "Gas\u00b7sen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die euer Fu\u00df nicht kennt,", "tokens": ["Die", "eu\u00b7er", "Fu\u00df", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Dinge mu\u00df ich sehen,", "tokens": ["Und", "Din\u00b7ge", "mu\u00df", "ich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die euer Mund nicht nennt.", "tokens": ["Die", "eu\u00b7er", "Mund", "nicht", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und alles mu\u00df ich sagen,", "tokens": ["Und", "al\u00b7les", "mu\u00df", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Sinn und Seele schaut,", "tokens": ["Was", "Sinn", "und", "See\u00b7le", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Namen einzig fragen", "tokens": ["Nach", "Na\u00b7men", "ein\u00b7zig", "fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wahrheit, meine Braut.", "tokens": ["Die", "Wahr\u00b7heit", ",", "mei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die hat ein leuchtend Auge,", "tokens": ["Die", "hat", "ein", "leuch\u00b7tend", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verkl\u00e4rt von tiefer Qual,", "tokens": ["Ver\u00b7kl\u00e4rt", "von", "tie\u00b7fer", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus seinem Urquell sauge", "tokens": ["Aus", "sei\u00b7nem", "Ur\u00b7quell", "sau\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich neuer Sch\u00f6nheit Strahl.", "tokens": ["Ich", "neu\u00b7er", "Sch\u00f6n\u00b7heit", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wenn ihr es s\u00e4ht, ihr reichtet", "tokens": ["Wenn", "ihr", "es", "s\u00e4ht", ",", "ihr", "reich\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure"