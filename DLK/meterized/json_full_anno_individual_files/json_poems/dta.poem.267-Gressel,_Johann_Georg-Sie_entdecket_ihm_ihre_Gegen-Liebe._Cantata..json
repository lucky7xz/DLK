{"dta.poem.267": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie entdecket ihm ihre Gegen-Liebe.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz/ du hast gesiegt/", "tokens": ["Mein", "Schatz", "/", "du", "hast", "ge\u00b7siegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Hertze ist dein eigen", "tokens": ["Mein", "Hert\u00b7ze", "ist", "dein", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn wo die Liebe kriegt/", "tokens": ["Denn", "wo", "die", "Lie\u00b7be", "kriegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da mu\u00df sich alles neigen.", "tokens": ["Da", "mu\u00df", "sich", "al\u00b7les", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch/ ja ich bin vergn\u00fcgt/", "tokens": ["Doch", "/", "ja", "ich", "bin", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Weil du gefangen", "tokens": ["Weil", "du", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da du als Sieger denckst zu prangen.", "tokens": ["Da", "du", "als", "Sie\u00b7ger", "denckst", "zu", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jedoch du hast gesiegt", "tokens": ["Je\u00b7doch", "du", "hast", "ge\u00b7siegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mein Hertze ist dein eigen;", "tokens": ["Mein", "Hert\u00b7ze", "ist", "dein", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Denn wo die Liebe kriegt", "tokens": ["Denn", "wo", "die", "Lie\u00b7be", "kriegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Da mu\u00df sich alles neigen.", "tokens": ["Da", "mu\u00df", "sich", "al\u00b7les", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch mein Verlust", "tokens": ["Doch", "mein", "Ver\u00b7lust"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wil meine Brust", "tokens": ["Wil", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als Sieger gr\u00fcssen;", "tokens": ["Als", "Sie\u00b7ger", "gr\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der vielen obgesiegt", "tokens": ["Der", "vie\u00b7len", "ob\u00b7ge\u00b7siegt"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu meinen F\u00fcssen liegt", "tokens": ["Zu", "mei\u00b7nen", "F\u00fcs\u00b7sen", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Demuth sie zu k\u00fcssen.", "tokens": ["In", "De\u00b7muth", "sie", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In der Liebe s\u00fcssen Kriegen", "tokens": ["In", "der", "Lie\u00b7be", "s\u00fcs\u00b7sen", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df der Sieger unten liegen", "tokens": ["Mu\u00df", "der", "Sie\u00b7ger", "un\u00b7ten", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Besiegte kriegt den Krantz/", "tokens": ["Die", "Be\u00b7sieg\u00b7te", "kriegt", "den", "Krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner kan von Vortheil sagen/", "tokens": ["Kei\u00b7ner", "kan", "von", "Vor\u00b7theil", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Man h\u00f6rt sonst auch keine Klagen", "tokens": ["Man", "h\u00f6rt", "sonst", "auch", "kei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als es stirbt der Freyheits-Glantz.", "tokens": ["Als", "es", "stirbt", "der", "Frey\u00b7heits\u00b7Glantz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}