{"dta.poem.8479": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auftrag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Die kleine Harfe hinter dem Altar auf,", "tokens": ["Die", "klei\u00b7ne", "Har\u00b7fe", "hin\u00b7ter", "dem", "Al\u00b7tar", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo an der Wand die Todtenkr\u00e4nze", "tokens": ["Wo", "an", "der", "Wand", "die", "Tod\u00b7ten\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Manches verstorbenen M\u00e4dchens schimmern.", "tokens": ["Man\u00b7ches", "ver\u00b7stor\u00b7be\u00b7nen", "M\u00e4d\u00b7chens", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Der K\u00fcster zeigt dann freundlich dem Reisenden", "tokens": ["Der", "K\u00fcs\u00b7ter", "zeigt", "dann", "freund\u00b7lich", "dem", "Rei\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die kleine Harfe, rauscht mit dem rothen Band,", "tokens": ["Die", "klei\u00b7ne", "Har\u00b7fe", ",", "rauscht", "mit", "dem", "ro\u00b7then", "Band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das, an der Harfe festgeschlungen,", "tokens": ["Das", ",", "an", "der", "Har\u00b7fe", "fest\u00b7ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den goldenen Saiten flattert.", "tokens": ["Un\u00b7ter", "den", "gol\u00b7de\u00b7nen", "Sai\u00b7ten", "flat\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Oft, sagt er staunend, t\u00f6nen im Abendroth", "tokens": ["Oft", ",", "sagt", "er", "stau\u00b7nend", ",", "t\u00f6\u00b7nen", "im", "A\u00b7ben\u00b7droth"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von selbst die Saiten, leise wie Bienenton;", "tokens": ["Von", "selbst", "die", "Sai\u00b7ten", ",", "lei\u00b7se", "wie", "Bie\u00b7nen\u00b7ton", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$,", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Kinder, auf dem Kirchhof spielend,", "tokens": ["Die", "Kin\u00b7der", ",", "auf", "dem", "Kirch\u00b7hof", "spie\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rtens, und sahn, wie die Kr\u00e4nze bebten.", "tokens": ["H\u00f6r\u00b7tens", ",", "und", "sahn", ",", "wie", "die", "Kr\u00e4n\u00b7ze", "beb\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}