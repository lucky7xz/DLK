, "Was", ",", "Bl\u00fc\u00b7cher", ",", "hab'", "ich", "recht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "PWS", "$,", "NE", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ein Held des Volkes, mehr als je, durchschritt' ich dies Geschlecht;", "tokens": ["Ein", "Held", "des", "Vol\u00b7kes", ",", "mehr", "als", "je", ",", "durch\u00b7schritt'", "ich", "dies", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PIAT", "KOKOM", "ADV", "$,", "VMFIN", "PPER", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ging' ich zur Ruh' einst, allezeit gesegnet und erfleht!\u00ab \u2013", "tokens": ["Ging'", "ich", "zur", "Ruh'", "einst", ",", "al\u00b7le\u00b7zeit", "ge\u00b7seg\u00b7net", "und", "er\u00b7fleht", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADV", "$,", "ADV", "VVPP", "KON", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die alten Herrn verneigten sich: \u00bbJa \u2013 ", "tokens": ["Die", "al\u00b7ten", "Herrn", "ver\u00b7neig\u00b7ten", "sich", ":", "\u00bb", "Ja", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$.", "$(", "PTKANT", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "So ging es j\u00fcngst im Himmel zu: Der Alte Fritz sprang auf", "tokens": ["So", "ging", "es", "j\u00fcngst", "im", "Him\u00b7mel", "zu", ":", "Der", "Al\u00b7te", "Fritz", "sprang", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$.", "ART", "NN", "NE", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und rieb die H\u00e4nde sich und schlug an seinen Degenknauf;", "tokens": ["Und", "rieb", "die", "H\u00e4n\u00b7de", "sich", "und", "schlug", "an", "sei\u00b7nen", "De\u00b7gen\u00b7knauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Er schritt im Himmel auf und ab und schaute grimmig drein", "tokens": ["Er", "schritt", "im", "Him\u00b7mel", "auf", "und", "ab", "und", "schau\u00b7te", "grim\u00b7mig", "drein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und trat dann vor den Bl\u00fccher hin und vor den Herrn von Stein.", "tokens": ["Und", "trat", "dann", "vor", "den", "Bl\u00fc\u00b7cher", "hin", "und", "vor", "den", "Herrn", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "KON", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.14": {"line.1": {"text": "Die haben mit dem Gneisenau alsbald sich eingestellt;", "tokens": ["Die", "ha\u00b7ben", "mit", "dem", "Gnei\u00b7se\u00b7nau", "als\u00b7bald", "sich", "ein\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Imgleichen kamen der Schwerin, der Scharnhorst und der Keith,", "tokens": ["Im\u00b7glei\u00b7chen", "ka\u00b7men", "der", "Schwe\u00b7rin", ",", "der", "Scharn\u00b7horst", "und", "der", "Keith", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und all die gro\u00dfen Preu\u00dfen sonst aus alt und neuer Zeit.", "tokens": ["Und", "all", "die", "gro\u00b7\u00dfen", "Preu\u00b7\u00dfen", "sonst", "aus", "alt", "und", "neu\u00b7er", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "ADV", "APPR", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.15": {"line.1": {"text": "Und als er sie beisammen sah, da rief er: \u00bbSchwerenot!", "tokens": ["Und", "als", "er", "sie", "bei\u00b7sam\u00b7men", "sah", ",", "da", "rief", "er", ":", "\u00bb", "Schwe\u00b7re\u00b7not", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVIZU", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die Sache geht mir durch den Kopf! Was Teufel bin ich tot!", "tokens": ["Die", "Sa\u00b7che", "geht", "mir", "durch", "den", "Kopf", "!", "Was", "Teu\u00b7fel", "bin", "ich", "tot", "!"], "token