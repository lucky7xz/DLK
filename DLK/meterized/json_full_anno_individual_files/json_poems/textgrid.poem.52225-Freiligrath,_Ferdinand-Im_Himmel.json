{"textgrid.poem.52225": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Im Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ging es j\u00fcngst im Himmel zu: Der Alte Fritz sprang auf", "tokens": ["So", "ging", "es", "j\u00fcngst", "im", "Him\u00b7mel", "zu", ":", "Der", "Al\u00b7te", "Fritz", "sprang", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$.", "ART", "NN", "NE", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und rieb die H\u00e4nde sich und schlug an seinen Degenknauf;", "tokens": ["Und", "rieb", "die", "H\u00e4n\u00b7de", "sich", "und", "schlug", "an", "sei\u00b7nen", "De\u00b7gen\u00b7knauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Er schritt im Himmel auf und ab und schaute grimmig drein", "tokens": ["Er", "schritt", "im", "Him\u00b7mel", "auf", "und", "ab", "und", "schau\u00b7te", "grim\u00b7mig", "drein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und trat dann vor den Bl\u00fccher hin und vor den Herrn von Stein.", "tokens": ["Und", "trat", "dann", "vor", "den", "Bl\u00fc\u00b7cher", "hin", "und", "vor", "den", "Herrn", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "KON", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Die haben mit dem Gneisenau alsbald sich eingestellt;", "tokens": ["Die", "ha\u00b7ben", "mit", "dem", "Gnei\u00b7se\u00b7nau", "als\u00b7bald", "sich", "ein\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Imgleichen kamen der Schwerin, der Scharnhorst und der Keith,", "tokens": ["Im\u00b7glei\u00b7chen", "ka\u00b7men", "der", "Schwe\u00b7rin", ",", "der", "Scharn\u00b7horst", "und", "der", "Keith", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und all die gro\u00dfen Preu\u00dfen sonst aus alt und neuer Zeit.", "tokens": ["Und", "all", "die", "gro\u00b7\u00dfen", "Preu\u00b7\u00dfen", "sonst", "aus", "alt", "und", "neu\u00b7er", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "ADV", "APPR", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Und als er sie beisammen sah, da rief er: \u00bbSchwerenot!", "tokens": ["Und", "als", "er", "sie", "bei\u00b7sam\u00b7men", "sah", ",", "da", "rief", "er", ":", "\u00bb", "Schwe\u00b7re\u00b7not", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVIZU", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die Sache geht mir durch den Kopf! Was Teufel bin ich tot!", "tokens": ["Die", "Sa\u00b7che", "geht", "mir", "durch", "den", "Kopf", "!", "Was", "Teu\u00b7fel", "bin", "ich", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PWS", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Was Teufel bin ich eben jetzt daheim nicht zu Berlin!", "tokens": ["Was", "Teu\u00b7fel", "bin", "ich", "e\u00b7ben", "jetzt", "da\u00b7heim", "nicht", "zu", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "'s w\u00e4r' wieder eine Zeit f\u00fcr mich! \u2013 Was \u2013 meint Er nicht, Schwerin?", "tokens": ["'", "s", "w\u00e4r", "'", "wie\u00b7der", "ei\u00b7ne", "Zeit", "f\u00fcr", "mich", "!", "\u2013", "Was", "\u2013", "meint", "Er", "nicht", ",", "Schwe\u00b7rin", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "ADV", "ART", "NN", "APPR", "PPER", "$.", "$(", "PWS", "$(", "VVFIN", "PPER", "PTKNEG", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wie wollt' ich sie ergreifen! Ha \u2013 nicht mehr als Autokrat!", "tokens": ["Wie", "wollt'", "ich", "sie", "er\u00b7grei\u00b7fen", "!", "Ha", "\u2013", "nicht", "mehr", "als", "Au\u00b7to\u00b7krat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "NE", "$(", "PTKNEG", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Nein, nein \u2013 ein ander S\u00e4kulum, ihr Herrn, ein ander Staat!", "tokens": ["Nein", ",", "nein", "\u2013", "ein", "an\u00b7der", "S\u00e4\u00b7ku\u00b7lum", ",", "ihr", "Herrn", ",", "ein", "an\u00b7der", "Staat", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$(", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Go\u00df ich doch selber aus ein Licht, zu flammend und zu klar,", "tokens": ["Go\u00df", "ich", "doch", "sel\u00b7ber", "aus", "ein", "Licht", ",", "zu", "flam\u00b7mend", "und", "zu", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Als da\u00df ich kehren k\u00f6nnte ganz derselbe, der ich war!", "tokens": ["Als", "da\u00df", "ich", "keh\u00b7ren", "k\u00f6nn\u00b7te", "ganz", "der\u00b7sel\u00b7be", ",", "der", "ich", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVINF", "VMFIN", "ADV", "PDAT", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Nein \u2013 was ich auch gewirkt, ihr Herrn, durch Beispiel und durch Wort,", "tokens": ["Nein", "\u2013", "was", "ich", "auch", "ge\u00b7wirkt", ",", "ihr", "Herrn", ",", "durch", "Bei\u00b7spiel", "und", "durch", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWS", "PPER", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Dazu die ganze gro\u00dfe Zeit von Dreizehn und so fort \u2013", "tokens": ["Da\u00b7zu", "die", "gan\u00b7ze", "gro\u00b7\u00dfe", "Zeit", "von", "Drei\u00b7zehn", "und", "so", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "ADJA", "NN", "APPR", "CARD", "KON", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ein Unterbau nur w\u00e4r' es jetzt (gewaltig zwar und breit!)", "tokens": ["Ein", "Un\u00b7ter\u00b7bau", "nur", "w\u00e4r'", "es", "jetzt", "(", "ge\u00b7wal\u00b7tig", "zwar", "und", "breit", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "ADV", "$(", "ADJD", "ADV", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Drauf ich erh\u00fcbe frischen Muts den Staat der neuen Zeit!", "tokens": ["Drauf", "ich", "er\u00b7h\u00fc\u00b7be", "fri\u00b7schen", "Muts", "den", "Staat", "der", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Der neuen Zeit, die andres will, als Eidbruch und Verrat!", "tokens": ["Der", "neu\u00b7en", "Zeit", ",", "die", "and\u00b7res", "will", ",", "als", "Eid\u00b7bruch", "und", "Ver\u00b7rat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VMFIN", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Der neuen Zeit, die andres will, als Lug und L\u00fcgensaat!", "tokens": ["Der", "neu\u00b7en", "Zeit", ",", "die", "and\u00b7res", "will", ",", "als", "Lug", "und", "L\u00fc\u00b7gen\u00b7saat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VMFIN", "$,", "KOUS", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Die endlich einmal mehr verlangt, als Schall und Rederei!", "tokens": ["Die", "end\u00b7lich", "ein\u00b7mal", "mehr", "ver\u00b7langt", ",", "als", "Schall", "und", "Re\u00b7de\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "VVPP", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die endlich einmal atmen will \u2013 aufatmen tief und frei!", "tokens": ["Die", "end\u00b7lich", "ein\u00b7mal", "at\u00b7men", "will", "\u2013", "auf\u00b7at\u00b7men", "tief", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "VMFIN", "$(", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Herr, dies betrogne deutsche Volk! \u2013 Und keiner, der es r\u00e4cht!", "tokens": ["Herr", ",", "dies", "be\u00b7trog\u00b7ne", "deut\u00b7sche", "Volk", "!", "\u2013", "Und", "kei\u00b7ner", ",", "der", "es", "r\u00e4cht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "ADJA", "ADJA", "NN", "$.", "$(", "KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und keiner, der ihm schaffen mag sein vorenthaltnes Recht!", "tokens": ["Und", "kei\u00b7ner", ",", "der", "ihm", "schaf\u00b7fen", "mag", "sein", "vor\u00b7ent\u00b7halt\u00b7nes", "Recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Der jeden Schwur, den man ihm brach, einfordert fest und k\u00fchn!", "tokens": ["Der", "je\u00b7den", "Schwur", ",", "den", "man", "ihm", "brach", ",", "ein\u00b7for\u00b7dert", "fest", "und", "k\u00fchn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der zornig mit dem Fu\u00dfe tritt auf Karlsbad und auf Wien!", "tokens": ["Der", "zor\u00b7nig", "mit", "dem", "Fu\u00b7\u00dfe", "tritt", "auf", "Karls\u00b7bad", "und", "auf", "Wi\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "APPR", "NE", "KON", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Ich t\u00e4t's! Einschl\u00fcg' ich mit der Faust dies Diplomatennetz!", "tokens": ["Ich", "t\u00e4t's", "!", "Ein\u00b7schl\u00fcg'", "ich", "mit", "der", "Faust", "dies", "Dip\u00b7lo\u00b7ma\u00b7ten\u00b7netz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$.", "NN", "PPER", "APPR", "ART", "NN", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "'reichsst\u00e4nde! \u00f6ffentlich Gericht! ein einig deutsch Gesetz!", "tokens": ["'reichs\u00b7st\u00e4n\u00b7de", "!", "\u00f6f\u00b7fent\u00b7lich", "Ge\u00b7richt", "!", "ein", "ei\u00b7nig", "deutsch", "Ge\u00b7setz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "$.", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und \u00fcberall das freie Wort!' \u2013 Bei Gott, so tr\u00e4t' ich hin!", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "das", "frei\u00b7e", "Wort", "!", "'", "\u2013", "Bei", "Gott", ",", "so", "tr\u00e4t", "'", "ich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$.", "$(", "$(", "APPR", "NN", "$,", "ADV", "ADJD", "$(", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Bei Gott dem Herrn, so schl\u00fcg' ich durch! \u2013 so wahr ich K\u00f6nig bin!", "tokens": ["Bei", "Gott", "dem", "Herrn", ",", "so", "schl\u00fcg'", "ich", "durch", "!", "\u2013", "so", "wahr", "ich", "K\u00f6\u00b7nig", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ADV", "ADJD", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "'s w\u00fcrd' eine Bombe sein! Gleichviel! Ging's auch ein Jahr lang kraus,", "tokens": ["'", "s", "w\u00fcrd", "'", "ei\u00b7ne", "Bom\u00b7be", "sein", "!", "Gleich\u00b7viel", "!", "Ging's", "auch", "ein", "Jahr", "lang", "kraus", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "ART", "NN", "VAINF", "$.", "NN", "$.", "NE", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich br\u00e4cht' es in die Richte schon, ich f\u00fchrt' es doch hinaus!", "tokens": ["Ich", "br\u00e4cht'", "es", "in", "die", "Rich\u00b7te", "schon", ",", "ich", "f\u00fchrt'", "es", "doch", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und z\u00f6g' ein Wetter auch heran, und w\u00fcrfe Keil auf Keil:", "tokens": ["Und", "z\u00f6g'", "ein", "Wet\u00b7ter", "auch", "he\u00b7ran", ",", "und", "w\u00fcr\u00b7fe", "Keil", "auf", "Keil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ein K\u00f6nig trotzt' ich K\u00f6nigen \u2013 zu meines Volkes Heil!", "tokens": ["Ein", "K\u00f6\u00b7nig", "trotzt'", "ich", "K\u00f6\u00b7ni\u00b7gen", "\u2013", "zu", "mei\u00b7nes", "Vol\u00b7kes", "Heil", "!"], "to