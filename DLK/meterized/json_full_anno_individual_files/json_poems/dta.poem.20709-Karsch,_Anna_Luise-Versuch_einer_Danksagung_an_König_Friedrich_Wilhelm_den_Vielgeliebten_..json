{"dta.poem.20709": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Versuch einer Danksagung  \n an  \n  K\u00f6nig Friedrich Wilhelm  \n den Vielgeliebten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Monarch und Sch\u00f6pfer eines Gl\u00fccks,                 ", "tokens": ["Mon\u00b7arch", "und", "Sch\u00f6p\u00b7fer", "ei\u00b7nes", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das meinem Alter Blumen streuet,", "tokens": ["Das", "mei\u00b7nem", "Al\u00b7ter", "Blu\u00b7men", "streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe nur im Ausdruck meines Blicks", "tokens": ["Ich", "ha\u00b7be", "nur", "im", "Aus\u00b7druck", "mei\u00b7nes", "Blicks"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Sprache, die kein W\u00f6rterbuch verleihet,", "tokens": ["Die", "Spra\u00b7che", ",", "die", "kein", "W\u00f6r\u00b7ter\u00b7buch", "ver\u00b7lei\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur Thr\u00e4nen hab\u2019 ich, statt des Tons,", "tokens": ["Nur", "Thr\u00e4\u00b7nen", "hab'", "ich", ",", "statt", "des", "Tons", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich Dir danken will, Dir Schutzgott auf der H\u00f6he", "tokens": ["Wenn", "ich", "Dir", "dan\u00b7ken", "will", ",", "Dir", "Schutz\u00b7gott", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des landesv\u00e4terlichen Throns \u2014", "tokens": ["Des", "lan\u00b7des\u00b7v\u00e4\u00b7ter\u00b7li\u00b7chen", "Throns"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchl\u2019s, da\u00df ich auf Rosen gehe,", "tokens": ["Ich", "f\u00fchl's", ",", "da\u00df", "ich", "auf", "Ro\u00b7sen", "ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Rosen schlummre leicht und s\u00fc\u00df,", "tokens": ["Auf", "Ro\u00b7sen", "schlumm\u00b7re", "leicht", "und", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seitdem Dein W\u00f6llner mir\u2019s verk\u00fcndet,", "tokens": ["Seit\u00b7dem", "Dein", "W\u00f6ll\u00b7ner", "mir's", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ihm sein K\u00f6nig hie\u00df:", "tokens": ["Was", "ihm", "sein", "K\u00f6\u00b7nig", "hie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Haus, ein Haus wird mir gegr\u00fcndet,", "tokens": ["Ein", "Haus", ",", "ein", "Haus", "wird", "mir", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird aufgebauet, wird geschm\u00fcckt,", "tokens": ["Wird", "auf\u00b7ge\u00b7bau\u00b7et", ",", "wird", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als w\u00e4r\u2019s ein Tempelchen der Musen \u2014", "tokens": ["Als", "w\u00e4r's", "ein", "Tem\u00b7pel\u00b7chen", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O wenn\u2019s mein Auge nun erblickt,", "tokens": ["O", "wenn's", "mein", "Au\u00b7ge", "nun", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dann wird mein abgelebter Busen", "tokens": ["Dann", "wird", "mein", "ab\u00b7ge\u00b7leb\u00b7ter", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zu enge f\u00fcr des Herzens Drang,", "tokens": ["Zu", "en\u00b7ge", "f\u00fcr", "des", "Her\u00b7zens", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es flammt bei dieser Augenweide", "tokens": ["Es", "flammt", "bei", "die\u00b7ser", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Vielleicht nur Tage lang,", "tokens": ["Viel\u00b7leicht", "nur", "Ta\u00b7ge", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wird wonnekrank", "tokens": ["Wird", "won\u00b7ne\u00b7krank"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Und stirbt den sch\u00f6nen Tod der Freude:", "tokens": ["Und", "stirbt", "den", "sch\u00f6\u00b7nen", "Tod", "der", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sein lezter Schlag ist Dank!", "tokens": ["Sein", "lez\u00b7ter", "Schlag", "ist", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}