{"textgrid.poem.43795": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die gro\u00dfen Herren machen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die gro\u00dfen Herren machen", "tokens": ["Die", "gro\u00b7\u00dfen", "Her\u00b7ren", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit mir wohl gro\u00dfen Staat,", "tokens": ["Mit", "mir", "wohl", "gro\u00b7\u00dfen", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Mancher, der mich tr\u00e4get,", "tokens": ["Und", "Man\u00b7cher", ",", "der", "mich", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denkt Wunder was er hat.", "tokens": ["Denkt", "Wun\u00b7der", "was", "er", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Nachts am blauen Himmel", "tokens": ["Des", "Nachts", "am", "blau\u00b7en", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auf dem Sand am Meer.", "tokens": ["Und", "auf", "dem", "Sand", "am", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Schnee, an mancher Blume", "tokens": ["Am", "Schnee", ",", "an", "man\u00b7cher", "Blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewundert man mich sehr.", "tokens": ["Be\u00b7wun\u00b7dert", "man", "mich", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der mich verlangt zu sehen,", "tokens": ["Der", "mich", "ver\u00b7langt", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der sieht mich nur durch mich,", "tokens": ["Der", "sieht", "mich", "nur", "durch", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kannst du mich nicht sehen,", "tokens": ["Und", "kannst", "du", "mich", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bin ich betr\u00fcbt um dich.", "tokens": ["Bin", "ich", "be\u00b7tr\u00fcbt", "um", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt rathe, wie ich hei\u00dfe.", "tokens": ["Jetzt", "ra\u00b7the", ",", "wie", "ich", "hei\u00b7\u00dfe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"],