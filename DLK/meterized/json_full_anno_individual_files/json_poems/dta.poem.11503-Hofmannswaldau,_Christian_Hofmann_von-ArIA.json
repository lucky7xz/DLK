{"dta.poem.11503": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "ArIA", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Werthes vergn\u00fcgen! umarme mich doch!", "tokens": ["Wert\u00b7hes", "ver\u00b7gn\u00fc\u00b7gen", "!", "um\u00b7ar\u00b7me", "mich", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Schlag der sorgen hartes joch,", "tokens": ["Schlag", "der", "sor\u00b7gen", "har\u00b7tes", "joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mein hertze sonst erdr\u00fccken,", "tokens": ["Die", "mein", "hert\u00b7ze", "sonst", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch zu tausend st\u00fccken!", "tokens": ["Doch", "zu", "tau\u00b7send", "st\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "W\u00fctende st\u00fcrme! bes\u00e4nfftiget euch!", "tokens": ["W\u00fc\u00b7ten\u00b7de", "st\u00fcr\u00b7me", "!", "be\u00b7s\u00e4nff\u00b7ti\u00b7get", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VVFIN", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "La\u00dft, auf wilder wellen streich,", "tokens": ["La\u00dft", ",", "auf", "wil\u00b7der", "wel\u00b7len", "streich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich bey stiller fluth und winden", "tokens": ["Mich", "bey", "stil\u00b7ler", "fluth", "und", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst den stillen hafen finden.", "tokens": ["Einst", "den", "stil\u00b7len", "ha\u00b7fen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sternen! begl\u00fcckt mein vern\u00fcnfftiges ziel!", "tokens": ["Ster\u00b7nen", "!", "be\u00b7gl\u00fcckt", "mein", "ver\u00b7n\u00fcnff\u00b7ti\u00b7ges", "ziel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Glaubt, ich w\u00fcnsche nicht zu viel;", "tokens": ["Glaubt", ",", "ich", "w\u00fcn\u00b7sche", "nicht", "zu", "viel", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich such\u2019 auf so viel leiden", "tokens": ["Denn", "ich", "such'", "auf", "so", "viel", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADV", "ADV", "VVINF"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Keinen port der freuden.", "tokens": ["Kei\u00b7nen", "port", "der", "freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "H\u00fcpffende l\u00fcste begleitet verdru\u00df:", "tokens": ["H\u00fcpf\u00b7fen\u00b7de", "l\u00fcs\u00b7te", "be\u00b7glei\u00b7tet", "ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Und ein allzus\u00fcsser ku\u00df,", "tokens": ["Und", "ein", "all\u00b7zu\u00b7s\u00fcs\u00b7ser", "ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sich seel und seele gatten,", "tokens": ["Da", "sich", "seel", "und", "see\u00b7le", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "KON", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflegt nur abzumatten.", "tokens": ["Pflegt", "nur", "ab\u00b7zu\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wahres vergn\u00fcgen! dich such\u2019 ich allein.", "tokens": ["Wah\u00b7res", "ver\u00b7gn\u00fc\u00b7gen", "!", "dich", "such'", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVINF", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie gl\u00fccklich w\u00fcrd ich seyn,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "w\u00fcrd", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00e4tt\u2019 ich nur, statt freud und schmertzen,", "tokens": ["H\u00e4tt'", "ich", "nur", ",", "statt", "freud", "und", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUI", "ADJD", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine ruh im hertzen!", "tokens": ["Dei\u00b7ne", "ruh", "im", "hert\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}