{"dta.poem.2240": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "A. Nachdem ich lange nachgesonnen, ist mein Ver-", "tokens": ["A.", "Nach\u00b7dem", "ich", "lan\u00b7ge", "nach\u00b7ge\u00b7son\u00b7nen", ",", "ist", "mein", "Ver"], "token_info": ["abbreviation", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "KOUS", "PPER", "ADV", "VVINF", "$,", "VAFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "langen ungemein,", "tokens": ["lan\u00b7gen", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich m\u00f6chte gerne von dir wissen, was eigentlich Ge-", "tokens": ["Ich", "m\u00f6ch\u00b7te", "ger\u00b7ne", "von", "dir", "wis\u00b7sen", ",", "was", "ei\u00b7gent\u00b7lich", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,", "PRELS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "danken seyn?", "tokens": ["dan\u00b7ken", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "B. Wenn man, was Farben eigentlich, von einem Blin-", "tokens": ["B.", "Wenn", "man", ",", "was", "Far\u00b7ben", "ei\u00b7gent\u00b7lich", ",", "von", "ei\u00b7nem", "Blin"], "token_info": ["abbreviation", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOUS", "PIS", "$,", "PWS", "NN", "ADV", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "den wollt erfragen,", "tokens": ["den", "wollt", "er\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "H\u00e4tt er gleich viel davon geh\u00f6rt, sollt er wohl was", "tokens": ["H\u00e4tt", "er", "gleich", "viel", "da\u00b7von", "ge\u00b7h\u00f6rt", ",", "sollt", "er", "wohl", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PAV", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "PWS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Begreiflichs sagen?", "tokens": ["Be\u00b7greif\u00b7lichs", "sa\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "So geht es mir mit meiner Antwort auf deine Fra-", "tokens": ["So", "geht", "es", "mir", "mit", "mei\u00b7ner", "Ant\u00b7wort", "auf", "dei\u00b7ne", "Fra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "gen eben auch.", "tokens": ["gen", "e\u00b7ben", "auch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Weil es jedoch bey den Gelehrten, die\u00df zu gestehen,", "tokens": ["Weil", "es", "je\u00b7doch", "bey", "den", "Ge\u00b7lehr\u00b7ten", ",", "die\u00df", "zu", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,", "PDS", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "nicht der Brauch;", "tokens": ["nicht", "der", "Brauch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "So dienet dieses dir zur Antwort, wor\u00fcber du dich", "tokens": ["So", "die\u00b7net", "die\u00b7ses", "dir", "zur", "Ant\u00b7wort", ",", "wo\u00b7r\u00fc\u00b7ber", "du", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "PPER", "APPRART", "NN", "$,", "PWAV", "PPER", "PRF"], "meter": "-+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.14": {"text": "nicht wirst zanken:", "tokens": ["nicht", "wirst", "zan\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Die Erd\u2019 ist Erde, Feuer Feur, und die Gedanken", "tokens": ["Die", "Erd'", "ist", "Er\u00b7de", ",", "Feu\u00b7er", "Feur", ",", "und", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "NN", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "sind Gedanken.", "tokens": ["sind", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}