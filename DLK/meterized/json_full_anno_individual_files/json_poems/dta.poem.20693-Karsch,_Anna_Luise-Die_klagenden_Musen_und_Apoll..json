{"dta.poem.20693": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  klagenden Musen und Apoll.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sah ich der Latona Sohn                 ", "tokens": ["J\u00fcngst", "sah", "ich", "der", "La\u00b7to\u00b7na", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch auf dem steilen Helicon", "tokens": ["Hoch", "auf", "dem", "stei\u00b7len", "He\u00b7li\u00b7con"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Musen vor ihm knieen.", "tokens": ["Und", "al\u00b7le", "Mu\u00b7sen", "vor", "ihm", "kni\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Sie baten: Sag uns, Pythius!", "tokens": ["Sie", "ba\u00b7ten", ":", "Sag", "uns", ",", "Py\u00b7thius", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "PPER", "$,", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Kennst Du die Nymphe dort an deines Berges Fu\u00df?", "tokens": ["Kennst", "Du", "die", "Nym\u00b7phe", "dort", "an", "dei\u00b7nes", "Ber\u00b7ges", "Fu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ward ihr deine Gunst verliehen?", "tokens": ["Wenn", "ward", "ihr", "dei\u00b7ne", "Gunst", "ver\u00b7lie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn gabst du ihren Schl\u00e4fen Glanz?", "tokens": ["Wenn", "gabst", "du", "ih\u00b7ren", "Schl\u00e4\u00b7fen", "Glanz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie prahlt mit einem Lorbeerkranz,", "tokens": ["Sie", "prahlt", "mit", "ei\u00b7nem", "Lor\u00b7beer\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den sie schon deiner Huld entrissen;", "tokens": ["Den", "sie", "schon", "dei\u00b7ner", "Huld", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Stolz erfrechet sich so gar,", "tokens": ["Ihr", "Stolz", "er\u00b7fre\u00b7chet", "sich", "so", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sagt es \u00f6ffentlich der weisen Menschenschaar:", "tokens": ["Und", "sagt", "es", "\u00f6f\u00b7fent\u00b7lich", "der", "wei\u00b7sen", "Men\u00b7schen\u00b7schaar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es k\u00e4men Musen sie zu k\u00fcssen.", "tokens": ["Es", "k\u00e4\u00b7men", "Mu\u00b7sen", "sie", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie leugts zu unsrer aller Hohn,", "tokens": ["Sie", "leugts", "zu", "uns\u00b7rer", "al\u00b7ler", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Urania wei\u00df nichts davon,", "tokens": ["U\u00b7ra\u00b7nia", "wei\u00df", "nichts", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Polymnia wird dir mit Schw\u00fcren", "tokens": ["Po\u00b7lym\u00b7nia", "wird", "dir", "mit", "Schw\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beim Styx betheuern, da\u00df sie nicht", "tokens": ["Beim", "Styx", "be\u00b7theu\u00b7ern", ",", "da\u00df", "sie", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Mund, der viel zu k\u00fchn, viel zu verwegen spricht,", "tokens": ["Den", "Mund", ",", "der", "viel", "zu", "k\u00fchn", ",", "viel", "zu", "ver\u00b7we\u00b7gen", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PTKA", "ADJD", "$,", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gew\u00fcrdiget hat anzur\u00fchren.", "tokens": ["Ge\u00b7w\u00fcr\u00b7di\u00b7get", "hat", "an\u00b7zu\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da l\u00e4chelte der Gott und sprach;", "tokens": ["Da", "l\u00e4\u00b7chel\u00b7te", "der", "Gott", "und", "sprach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht auf, Geliebten! eure Schmach", "tokens": ["Steht", "auf", ",", "Ge\u00b7lieb\u00b7ten", "!", "eu\u00b7re", "Schmach"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird ohne meinen Arm gerochen;", "tokens": ["Wird", "oh\u00b7ne", "mei\u00b7nen", "Arm", "ge\u00b7ro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze Welt, die f\u00fchlen kann,", "tokens": ["Die", "gan\u00b7ze", "Welt", ",", "die", "f\u00fch\u00b7len", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fein zu horchen wei\u00df, h\u00f6rts ihren T\u00f6nen an,", "tokens": ["Und", "fein", "zu", "hor\u00b7chen", "wei\u00df", ",", "h\u00f6rts", "ih\u00b7ren", "T\u00f6\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df nie mein Geist aus ihr gesprochen;", "tokens": ["Da\u00df", "nie", "mein", "Geist", "aus", "ihr", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df keine Muse sie gek\u00fc\u00dft,", "tokens": ["Da\u00df", "kei\u00b7ne", "Mu\u00b7se", "sie", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df sie so verblendet ist", "tokens": ["Und", "da\u00df", "sie", "so", "ver\u00b7blen\u00b7det", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Ixion, der einst entz\u00fccket", "tokens": ["Wie", "I\u00b7xion", ",", "der", "einst", "ent\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In seinen Arm die Wolke zog,", "tokens": ["In", "sei\u00b7nen", "Arm", "die", "Wol\u00b7ke", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dann geschw\u00e4tzig von der gro\u00dfen Juno log,", "tokens": ["Und", "dann", "ge\u00b7schw\u00e4t\u00b7zig", "von", "der", "gro\u00b7\u00dfen", "Ju\u00b7no", "log", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er h\u00e4tte sie ans Herz gedr\u00fccket.", "tokens": ["Er", "h\u00e4t\u00b7te", "sie", "ans", "Herz", "ge\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}