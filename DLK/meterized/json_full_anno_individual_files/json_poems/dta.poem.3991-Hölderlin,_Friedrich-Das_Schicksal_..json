{"dta.poem.3991": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Schicksal .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.85", "nl:0.14"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Als von des Friedens heil'gen Thalen,", "tokens": ["Als", "von", "des", "Frie\u00b7dens", "heil'\u00b7gen", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sich die Liebe Kr\u00e4nze wand,", "tokens": ["Wo", "sich", "die", "Lie\u00b7be", "Kr\u00e4n\u00b7ze", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hin\u00fcber zu den G\u00f6ttermahlen", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "zu", "den", "G\u00f6t\u00b7ter\u00b7mah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des goldnen Alters Zauber schwand,", "tokens": ["Des", "gold\u00b7nen", "Al\u00b7ters", "Zau\u00b7ber", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als nun des Schicksals eh'rne Rechte,", "tokens": ["Als", "nun", "des", "Schick\u00b7sals", "eh'r\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die gro\u00dfe Meisterin, die Noth", "tokens": ["Die", "gro\u00b7\u00dfe", "Meis\u00b7te\u00b7rin", ",", "die", "Noth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem \u00fcberm\u00fcthigen Geschlechte", "tokens": ["Dem", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hi\u00b7gen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Den langen, bittern Kampf gebot:", "tokens": ["Den", "lan\u00b7gen", ",", "bit\u00b7tern", "Kampf", "ge\u00b7bot", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sprang er aus der Mutter Wiege,", "tokens": ["Da", "sprang", "er", "aus", "der", "Mut\u00b7ter", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da fand er sie, die sch\u00f6ne Spur", "tokens": ["Da", "fand", "er", "sie", ",", "die", "sch\u00f6\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu seiner Tugend schwerem Siege,", "tokens": ["Zu", "sei\u00b7ner", "Tu\u00b7gend", "schwe\u00b7rem", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sohn der heiligen Natur;", "tokens": ["Der", "Sohn", "der", "hei\u00b7li\u00b7gen", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hohen Geister h\u00f6chste Gabe,", "tokens": ["Der", "ho\u00b7hen", "Geis\u00b7ter", "h\u00f6chs\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tugend L\u00f6wenkraft begann,", "tokens": ["Der", "Tu\u00b7gend", "L\u00f6\u00b7wen\u00b7kraft", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Siege, den ein G\u00f6tterknabe", "tokens": ["Im", "Sie\u00b7ge", ",", "den", "ein", "G\u00f6t\u00b7ter\u00b7kna\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Ungeheuern abgewann.", "tokens": ["Den", "Un\u00b7ge\u00b7heu\u00b7ern", "ab\u00b7ge\u00b7wann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es kann die Lust der goldnen Ernte", "tokens": ["Es", "kann", "die", "Lust", "der", "gold\u00b7nen", "Ern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sonnenbrande nur gedeih'n;", "tokens": ["Im", "Son\u00b7nen\u00b7bran\u00b7de", "nur", "ge\u00b7deih'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur in seinem Blute lernte", "tokens": ["Und", "nur", "in", "sei\u00b7nem", "Blu\u00b7te", "lern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00e4mpfer, frei und stolz zu seyn;", "tokens": ["Der", "K\u00e4mp\u00b7fer", ",", "frei", "und", "stolz", "zu", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Triumph! die Paradiese schwanden;", "tokens": ["Tri\u00b7umph", "!", "die", "Pa\u00b7ra\u00b7die\u00b7se", "schwan\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Flammen aus der Wolke Schoos,", "tokens": ["Wie", "Flam\u00b7men", "aus", "der", "Wol\u00b7ke", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Samen aus dem Chaos, wanden", "tokens": ["Wie", "Sa\u00b7men", "aus", "dem", "Chaos", ",", "wan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Aus St\u00fcrmen sich Heroen los.", "tokens": ["Aus", "St\u00fcr\u00b7men", "sich", "He\u00b7roen", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Noth ist jede Lust entsprossen,", "tokens": ["Der", "Noth", "ist", "je\u00b7de", "Lust", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und unter Schmerzen nur gedeiht", "tokens": ["Und", "un\u00b7ter", "Schmer\u00b7zen", "nur", "ge\u00b7deiht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Liebste, was mein Herz genossen,", "tokens": ["Das", "Liebs\u00b7te", ",", "was", "mein", "Herz", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der holde Reiz der Menschlichkeit;", "tokens": ["Der", "hol\u00b7de", "Reiz", "der", "Menschlich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So stieg, in tiefer Fluth erzogen,", "tokens": ["So", "stieg", ",", "in", "tie\u00b7fer", "Fluth", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin kein sterblich Auge sah,", "tokens": ["Wo\u00b7hin", "kein", "sterb\u00b7lich", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stilll\u00e4chelnd aus den schwarzen Wogen", "tokens": ["Still\u00b7l\u00e4\u00b7chelnd", "aus", "den", "schwar\u00b7zen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In stolzer Bl\u00fcthe Cypria.", "tokens": ["In", "stol\u00b7zer", "Bl\u00fc\u00b7the", "Cyp\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Durch Noth vereiniget, beschwuren,", "tokens": ["Durch", "Noth", "ver\u00b7ei\u00b7ni\u00b7get", ",", "be\u00b7schwu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Jugendtraume s\u00fc\u00df berauscht,", "tokens": ["Vom", "Ju\u00b7gend\u00b7trau\u00b7me", "s\u00fc\u00df", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Todesbund die Dioskuren,", "tokens": ["Den", "To\u00b7des\u00b7bund", "die", "Dios\u00b7ku\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Schwerdt und Lanze ward getauscht;", "tokens": ["Und", "Schwerdt", "und", "Lan\u00b7ze", "ward", "ge\u00b7tauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In ihres Herzens Jubel eilten", "tokens": ["In", "ih\u00b7res", "Her\u00b7zens", "Ju\u00b7bel", "eil\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie, wie ein Adlerpaar, zum Streit,", "tokens": ["Sie", ",", "wie", "ein", "Ad\u00b7ler\u00b7paar", ",", "zum", "Streit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie L\u00f6wen ihre Beute, theilten", "tokens": ["Wie", "L\u00f6\u00b7wen", "ih\u00b7re", "Beu\u00b7te", ",", "theil\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Liebenden Unsterblichkeit.", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Die Klagen lehrt die Noth verachten,", "tokens": ["Die", "Kla\u00b7gen", "lehrt", "die", "Noth", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besch\u00e4mt und ruhmlos l\u00e4\u00dft sie nicht", "tokens": ["Be\u00b7sch\u00e4mt", "und", "ruh\u00b7mlos", "l\u00e4\u00dft", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kraft der J\u00fcnglinge verschmachten,", "tokens": ["Die", "Kraft", "der", "J\u00fcng\u00b7lin\u00b7ge", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Giebt Muth der Brust, dem Geiste Licht;", "tokens": ["Giebt", "Muth", "der", "Brust", ",", "dem", "Geis\u00b7te", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Greise Faust verj\u00fcngt sie wieder;", "tokens": ["Der", "Grei\u00b7se", "Faust", "ver\u00b7j\u00fcngt", "sie", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie k\u00f6mmt wie Gottes Blitz heran,", "tokens": ["Sie", "k\u00f6mmt", "wie", "Got\u00b7tes", "Blitz", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und tr\u00fcmmert Felsenberge nieder,", "tokens": ["Und", "tr\u00fcm\u00b7mert", "Fel\u00b7sen\u00b7ber\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wallt auf Riesen ihre Bahn.", "tokens": ["Und", "wallt", "auf", "Rie\u00b7sen", "ih\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit ihrem heil'gen Wetterschlage,", "tokens": ["Mit", "ih\u00b7rem", "heil'\u00b7gen", "Wet\u00b7ter\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Unerbittlichkeit vollbringt", "tokens": ["Mit", "Un\u00b7er\u00b7bitt\u00b7lich\u00b7keit", "voll\u00b7bringt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Noth an Einem gro\u00dfen Tage,", "tokens": ["Die", "Noth", "an", "Ei\u00b7nem", "gro\u00b7\u00dfen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was kaum Jahrhunderten gelingt;", "tokens": ["Was", "kaum", "Jahr\u00b7hun\u00b7der\u00b7ten", "ge\u00b7lingt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVPP", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und wenn in ihren Ungewittern", "tokens": ["Und", "wenn", "in", "ih\u00b7ren", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst ein Elysium vergeht,", "tokens": ["Selbst", "ein", "E\u00b7ly\u00b7si\u00b7um", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Welten ihrem Donner zittern \u2014", "tokens": ["Und", "Wel\u00b7ten", "ih\u00b7rem", "Don\u00b7ner", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was gro\u00df und g\u00f6ttlich ist, besteht.", "tokens": ["Was", "gro\u00df", "und", "g\u00f6tt\u00b7lich", "ist", ",", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza