{"dta.poem.19580": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Geisterro\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Durch den dreigetheilten Bogen,", "tokens": ["Durch", "den", "drei\u00b7ge\u00b7theil\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Triumphes prangend Thor,", "tokens": ["Des", "Tri\u00b7um\u00b7phes", "pran\u00b7gend", "Thor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Durch die lauten Menschenwogen", "tokens": ["Durch", "die", "lau\u00b7ten", "Men\u00b7schen\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort zum Capitol empor", "tokens": ["Dort", "zum", "Ca\u00b7pi\u00b7tol", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lenkt den Tanz der wei\u00dfen Pferde", "tokens": ["Lenkt", "den", "Tanz", "der", "wei\u00b7\u00dfen", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "C\u00e4sar's l\u00e4ssige Geberde.", "tokens": ["C\u00e4\u00b7sa\u00b7r's", "l\u00e4s\u00b7si\u00b7ge", "Ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Hinter des Triumphes Wagen", "tokens": ["Hin\u00b7ter", "des", "Tri\u00b7um\u00b7phes", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Duldend oder grollend gehn", "tokens": ["Dul\u00b7dend", "o\u00b7der", "grol\u00b7lend", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberwundne. Ketten tragen", "tokens": ["Ue\u00b7ber\u00b7wund\u00b7ne", ".", "Ket\u00b7ten", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "C\u00e4sar's lebende Troph\u00e4'n.", "tokens": ["C\u00e4\u00b7sa\u00b7r's", "le\u00b7ben\u00b7de", "Tro\u00b7ph\u00e4'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["NE", "ADJA", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201edieser!\u201c h\u00f6hnt es im Gedr\u00e4nge,", "tokens": ["\u201e", "die\u00b7ser", "!", "\u201c", "h\u00f6hnt", "es", "im", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "$.", "$(", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201edieser Trotz'ge!\u201c zischt die Menge.", "tokens": ["\u201e", "die\u00b7ser", "Trotz'\u00b7ge", "!", "\u201c", "zischt", "die", "Men\u00b7ge", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unber\u00fchrt vom Hohn der Stunde,", "tokens": ["Un\u00b7be\u00b7r\u00fchrt", "vom", "Hohn", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starren, traumgef\u00fcllten Blicks,", "tokens": ["Star\u00b7ren", ",", "traum\u00b7ge\u00b7f\u00fcll\u00b7ten", "Blicks", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht, ein Singen auf dem Munde,", "tokens": ["Geht", ",", "ein", "Sin\u00b7gen", "auf", "dem", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhig Vexcingetorix \u2014", "tokens": ["Ru\u00b7hig", "Vex\u00b7cin\u00b7ge\u00b7to\u00b7rix"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Fremde Weise, fremde Worte,", "tokens": ["Frem\u00b7de", "Wei\u00b7se", ",", "frem\u00b7de", "Wor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Geist an fremdem Orte:", "tokens": ["Mit", "dem", "Geist", "an", "frem\u00b7dem", "Or\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ec\u00e4sar, blendend wei\u00dfe Rosse", "tokens": ["\u201e", "c\u00e4\u00b7sar", ",", "blen\u00b7dend", "wei\u00b7\u00dfe", "Ros\u00b7se"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "$,", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat Hispanien dir gebracht!", "tokens": ["Hat", "His\u00b7pa\u00b7ni\u00b7en", "dir", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ellid, edler Ahnen Sprosse,", "tokens": ["El\u00b7lid", ",", "ed\u00b7ler", "Ah\u00b7nen", "Spros\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkel ist er wie die Nacht \u2014", "tokens": ["Dun\u00b7kel", "ist", "er", "wie", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Schimmel, deine viere,", "tokens": ["Dei\u00b7ne", "Schim\u00b7mel", ",", "dei\u00b7ne", "vie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tauscht' ich nicht mit meinem Thiere ...", "tokens": ["Tauscht'", "ich", "nicht", "mit", "mei\u00b7nem", "Thie\u00b7re", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ellid hei\u00dft der wackre Jager", "tokens": ["El\u00b7lid", "hei\u00dft", "der", "wack\u00b7re", "Ja\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark von Wuchs und fest im Bug,", "tokens": ["Stark", "von", "Wuchs", "und", "fest", "im", "Bug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher mich ins R\u00f6merlager", "tokens": ["Wel\u00b7cher", "mich", "ins", "R\u00f6\u00b7mer\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gewalt'gen Spr\u00fcngen trug ...", "tokens": ["Mit", "ge\u00b7walt'\u00b7gen", "Spr\u00fcn\u00b7gen", "trug", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der zum Opfer ich gegeben", "tokens": ["Der", "zum", "Op\u00b7fer", "ich", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich f\u00fcr meines Volkes Leben!", "tokens": ["Mich", "f\u00fcr", "mei\u00b7nes", "Vol\u00b7kes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}