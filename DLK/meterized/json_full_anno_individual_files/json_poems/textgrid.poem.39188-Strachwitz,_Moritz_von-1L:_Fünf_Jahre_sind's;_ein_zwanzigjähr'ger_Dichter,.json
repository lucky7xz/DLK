{"textgrid.poem.39188": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcnf Jahre sind's; ein zwanzigj\u00e4hr'ger Dichter,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnf Jahre sind's; ein zwanzigj\u00e4hr'ger Dichter,", "tokens": ["F\u00fcnf", "Jah\u00b7re", "sin\u00b7d's", ";", "ein", "zwan\u00b7zig\u00b7j\u00e4hr'\u00b7ger", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sang ich hinaus mein jugendlichstes Z\u00fcrnen,", "tokens": ["Sang", "ich", "hin\u00b7aus", "mein", "ju\u00b7gend\u00b7lichs\u00b7tes", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APZR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war ein B\u00fcchlein, Ihr gestrengen Richter,", "tokens": ["Es", "war", "ein", "B\u00fcch\u00b7lein", ",", "Ihr", "ge\u00b7stren\u00b7gen", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie's keimend schie\u00dft aus g\u00e4renden Gehirnen.", "tokens": ["Wie's", "kei\u00b7mend", "schie\u00dft", "aus", "g\u00e4\u00b7ren\u00b7den", "Ge\u00b7hir\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr kostetet und schnittet wohl Gesichter,", "tokens": ["Ihr", "kos\u00b7te\u00b7tet", "und", "schnit\u00b7tet", "wohl", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn gar zu unreif waren jene Birnen,", "tokens": ["Denn", "gar", "zu", "un\u00b7reif", "wa\u00b7ren", "je\u00b7ne", "Bir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch schien Euch manche s\u00fc\u00df und keine faulig,", "tokens": ["Doch", "schien", "Euch", "man\u00b7che", "s\u00fc\u00df", "und", "kei\u00b7ne", "fau\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJD", "KON", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur Manchem war der \u00bbGraf\u00ab zu schwer verdaulich.", "tokens": ["Nur", "Man\u00b7chem", "war", "der", "\u00bb", "Graf", "\u00ab", "zu", "schwer", "ver\u00b7dau\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ART", "$(", "NE", "$(", "PTKA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbmir ist auf Erden wenig quer gegangen,\u00ab", "tokens": ["\u00bb", "mir", "ist", "auf", "Er\u00b7den", "we\u00b7nig", "quer", "ge\u00b7gan\u00b7gen", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "ADV", "ADJD", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sang ich damals! Kindisches Bezeigen!", "tokens": ["So", "sang", "ich", "da\u00b7mals", "!", "Kin\u00b7di\u00b7sches", "Be\u00b7zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn prahlerisch des Gl\u00fcckes Zinnen prangen,", "tokens": ["Wenn", "prah\u00b7le\u00b7risch", "des", "Gl\u00fc\u00b7ckes", "Zin\u00b7nen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann kommt der Sturm, sie in den Staub zu neigen,", "tokens": ["Dann", "kommt", "der", "Sturm", ",", "sie", "in", "den", "Staub", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So ward verh\u00f6hnt mein loderndes Verlangen,", "tokens": ["So", "ward", "ver\u00b7h\u00f6hnt", "mein", "lo\u00b7dern\u00b7des", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gest\u00fcrmt mein Himmel, der da hing voll Geigen.", "tokens": ["Ge\u00b7st\u00fcrmt", "mein", "Him\u00b7mel", ",", "der", "da", "hing", "voll", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und von der ganzen Saat aus jener Sph\u00e4re", "tokens": ["Und", "von", "der", "gan\u00b7zen", "Saat", "aus", "je\u00b7ner", "Sph\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ist dieses Buch die einz'ge volle \u00c4hre.", "tokens": ["Ist", "die\u00b7ses", "Buch", "die", "einz'\u00b7ge", "vol\u00b7le", "\u00c4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch f\u00fcrchtet Nichts! Es ist kein Buch der Tr\u00e4nen,", "tokens": ["Doch", "f\u00fcrch\u00b7tet", "Nichts", "!", "Es", "ist", "kein", "Buch", "der", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht jedes Herz ist gar so leicht zerrissen;", "tokens": ["Nicht", "je\u00b7des", "Herz", "ist", "gar", "so", "leicht", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn andre weinen, knirsch' ich mit den Z\u00e4hnen", "tokens": ["Wenn", "and\u00b7re", "wei\u00b7nen", ",", "knir\u00b7sch'", "ich", "mit", "den", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und habe so mein sch\u00e4rfstes Weh verbissen.", "tokens": ["Und", "ha\u00b7be", "so", "mein", "sch\u00e4rfs\u00b7tes", "Weh", "ver\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was braucht die Welt bei ", "tokens": ["Was", "braucht", "die", "Welt", "bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was braucht die Welt von jedem Schmerz zu wissen?", "tokens": ["Was", "braucht", "die", "Welt", "von", "je\u00b7dem", "Schmerz", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur Weiber heulen vor gesamtem Volke,", "tokens": ["Nur", "Wei\u00b7ber", "heu\u00b7len", "vor", "ge\u00b7sam\u00b7tem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die heit're Kunst ist keine Tr\u00e4nenwolke!", "tokens": ["Die", "heit'\u00b7re", "Kunst", "ist", "kei\u00b7ne", "Tr\u00e4\u00b7nen\u00b7wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich fasse gern mit einem k\u00fchnen Griffe", "tokens": ["Ich", "fas\u00b7se", "gern", "mit", "ei\u00b7nem", "k\u00fch\u00b7nen", "Grif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ernstes Heldenbild vergang'ner Tage;", "tokens": ["Ein", "erns\u00b7tes", "Hel\u00b7den\u00b7bild", "ver\u00b7gang'\u00b7ner", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es kennt mein Lied viel perlenreiche Riffe", "tokens": ["Es", "kennt", "mein", "Lied", "viel", "per\u00b7len\u00b7rei\u00b7che", "Rif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im unersch\u00f6pften Meeresgrund der Sage;", "tokens": ["Im", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Mee\u00b7res\u00b7grund", "der", "Sa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ins starke Nordland f\u00fchrt es Euch zu Schiffe", "tokens": ["Ins", "star\u00b7ke", "Nord\u00b7land", "f\u00fchrt", "es", "Euch", "zu", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Damit es Euch uralte Schlachten schlage,", "tokens": ["Da\u00b7mit", "es", "Euch", "ur\u00b7al\u00b7te", "Schlach\u00b7ten", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Eures Himmels jammervoller Leere,", "tokens": ["In", "Eu\u00b7res", "Him\u00b7mels", "jam\u00b7mer\u00b7vol\u00b7ler", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da zeigt es Euch den Stern gewes'ner Ehre.", "tokens": ["Da", "zeigt", "es", "Euch", "den", "Stern", "ge\u00b7wes'\u00b7ner", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Frei blaut auch mir des Geistes k\u00fchnste Ferne,", "tokens": ["Frei", "blaut", "auch", "mir", "des", "Geis\u00b7tes", "k\u00fchns\u00b7te", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch hab' ich nicht verlernt vor Gott zu beten,", "tokens": ["Doch", "hab'", "ich", "nicht", "ver\u00b7lernt", "vor", "Gott", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Frauenliebe sing' ich gar zu gerne,", "tokens": ["Von", "Frau\u00b7en\u00b7lie\u00b7be", "sing'", "ich", "gar", "zu", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum hab' ich nie mit F\u00fc\u00dfen sie getreten.", "tokens": ["Drum", "hab'", "ich", "nie", "mit", "F\u00fc\u00b7\u00dfen", "sie", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So kann ich nicht, wie Eure j\u00fcngsten Sterne,", "tokens": ["So", "kann", "ich", "nicht", ",", "wie", "Eu\u00b7re", "j\u00fcng\u00b7sten", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Zwitter vom Rou\u00e9 und vom Propheten,", "tokens": ["Die", "Zwit\u00b7ter", "vom", "Rou\u00e9", "und", "vom", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "KON", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den h\u00f6chsten Gott und dann mein Lieb bewitzeln,", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Gott", "und", "dann", "mein", "Lieb", "be\u00b7wit\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich mag Euch nicht mit solchem Schmutze kitzeln.", "tokens": ["Ich", "mag", "Euch", "nicht", "mit", "sol\u00b7chem", "Schmut\u00b7ze", "kit\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So ist mein Lied, o da\u00df es Euch erbaue,", "tokens": ["So", "ist", "mein", "Lied", ",", "o", "da\u00df", "es", "Euch", "er\u00b7bau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "FM", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag es vielleicht ein Freundesherz ersch\u00fcttern,", "tokens": ["Mag", "es", "viel\u00b7leicht", "ein", "Freun\u00b7des\u00b7herz", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mag es zu Dir, Du allersch\u00f6nste Fraue,", "tokens": ["Mag", "es", "zu", "Dir", ",", "Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als des Verbannten t\u00f6nend Heimweh zittern! \u2013", "tokens": ["Als", "des", "Ver\u00b7bann\u00b7ten", "t\u00f6\u00b7nend", "Heim\u00b7weh", "zit\u00b7tern", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Vaterland, dem bald der Himmel blaue,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "dem", "bald", "der", "Him\u00b7mel", "blau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O, l\u00e4chle mir aus ringenden Gewittern;", "tokens": ["O", ",", "l\u00e4ch\u00b7le", "mir", "aus", "rin\u00b7gen\u00b7den", "Ge\u00b7wit\u00b7tern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mein Vaterland, das M\u00e4nnerworte richtet,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "das", "M\u00e4n\u00b7ner\u00b7wor\u00b7te", "rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O, richte Du: Der Mann hat deutsch gedichtet! \u2013", "tokens": ["O", ",", "rich\u00b7te", "Du", ":", "Der", "Mann", "hat", "deutsch", "ge\u00b7dich\u00b7tet", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "F\u00fcnf Jahre sind's; ein zwanzigj\u00e4hr'ger Dichter,", "tokens": ["F\u00fcnf", "Jah\u00b7re", "sin\u00b7d's", ";", "ein", "zwan\u00b7zig\u00b7j\u00e4hr'\u00b7ger", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sang ich hinaus mein jugendlichstes Z\u00fcrnen,", "tokens": ["Sang", "ich", "hin\u00b7aus", "mein", "ju\u00b7gend\u00b7lichs\u00b7tes", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APZR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war ein B\u00fcchlein, Ihr gestrengen Richter,", "tokens": ["Es", "war", "ein", "B\u00fcch\u00b7lein", ",", "Ihr", "ge\u00b7stren\u00b7gen", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie's keimend schie\u00dft aus g\u00e4renden Gehirnen.", "tokens": ["Wie's", "kei\u00b7mend", "schie\u00dft", "aus", "g\u00e4\u00b7ren\u00b7den", "Ge\u00b7hir\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr kostetet und schnittet wohl Gesichter,", "tokens": ["Ihr", "kos\u00b7te\u00b7tet", "und", "schnit\u00b7tet", "wohl", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn gar zu unreif waren jene Birnen,", "tokens": ["Denn", "gar", "zu", "un\u00b7reif", "wa\u00b7ren", "je\u00b7ne", "Bir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch schien Euch manche s\u00fc\u00df und keine faulig,", "tokens": ["Doch", "schien", "Euch", "man\u00b7che", "s\u00fc\u00df", "und", "kei\u00b7ne", "fau\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJD", "KON", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur Manchem war der \u00bbGraf\u00ab zu schwer verdaulich.", "tokens": ["Nur", "Man\u00b7chem", "war", "der", "\u00bb", "Graf", "\u00ab", "zu", "schwer", "ver\u00b7dau\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ART", "$(", "NE", "$(", "PTKA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbmir ist auf Erden wenig quer gegangen,\u00ab", "tokens": ["\u00bb", "mir", "ist", "auf", "Er\u00b7den", "we\u00b7nig", "quer", "ge\u00b7gan\u00b7gen", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "ADV", "ADJD", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sang ich damals! Kindisches Bezeigen!", "tokens": ["So", "sang", "ich", "da\u00b7mals", "!", "Kin\u00b7di\u00b7sches", "Be\u00b7zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn prahlerisch des Gl\u00fcckes Zinnen prangen,", "tokens": ["Wenn", "prah\u00b7le\u00b7risch", "des", "Gl\u00fc\u00b7ckes", "Zin\u00b7nen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann kommt der Sturm, sie in den Staub zu neigen,", "tokens": ["Dann", "kommt", "der", "Sturm", ",", "sie", "in", "den", "Staub", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So ward verh\u00f6hnt mein loderndes Verlangen,", "tokens": ["So", "ward", "ver\u00b7h\u00f6hnt", "mein", "lo\u00b7dern\u00b7des", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gest\u00fcrmt mein Himmel, der da hing voll Geigen.", "tokens": ["Ge\u00b7st\u00fcrmt", "mein", "Him\u00b7mel", ",", "der", "da", "hing", "voll", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und von der ganzen Saat aus jener Sph\u00e4re", "tokens": ["Und", "von", "der", "gan\u00b7zen", "Saat", "aus", "je\u00b7ner", "Sph\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ist dieses Buch die einz'ge volle \u00c4hre.", "tokens": ["Ist", "die\u00b7ses", "Buch", "die", "einz'\u00b7ge", "vol\u00b7le", "\u00c4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch f\u00fcrchtet Nichts! Es ist kein Buch der Tr\u00e4nen,", "tokens": ["Doch", "f\u00fcrch\u00b7tet", "Nichts", "!", "Es", "ist", "kein", "Buch", "der", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht jedes Herz ist gar so leicht zerrissen;", "tokens": ["Nicht", "je\u00b7des", "Herz", "ist", "gar", "so", "leicht", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn andre weinen, knirsch' ich mit den Z\u00e4hnen", "tokens": ["Wenn", "and\u00b7re", "wei\u00b7nen", ",", "knir\u00b7sch'", "ich", "mit", "den", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und habe so mein sch\u00e4rfstes Weh verbissen.", "tokens": ["Und", "ha\u00b7be", "so", "mein", "sch\u00e4rfs\u00b7tes", "Weh", "ver\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was braucht die Welt bei ", "tokens": ["Was", "braucht", "die", "Welt", "bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was braucht die Welt von jedem Schmerz zu wissen?", "tokens": ["Was", "braucht", "die", "Welt", "von", "je\u00b7dem", "Schmerz", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur Weiber heulen vor gesamtem Volke,", "tokens": ["Nur", "Wei\u00b7ber", "heu\u00b7len", "vor", "ge\u00b7sam\u00b7tem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die heit're Kunst ist keine Tr\u00e4nenwolke!", "tokens": ["Die", "heit'\u00b7re", "Kunst", "ist", "kei\u00b7ne", "Tr\u00e4\u00b7nen\u00b7wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich fasse gern mit einem k\u00fchnen Griffe", "tokens": ["Ich", "fas\u00b7se", "gern", "mit", "ei\u00b7nem", "k\u00fch\u00b7nen", "Grif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein ernstes Heldenbild vergang'ner Tage;", "tokens": ["Ein", "erns\u00b7tes", "Hel\u00b7den\u00b7bild", "ver\u00b7gang'\u00b7ner", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es kennt mein Lied viel perlenreiche Riffe", "tokens": ["Es", "kennt", "mein", "Lied", "viel", "per\u00b7len\u00b7rei\u00b7che", "Rif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im unersch\u00f6pften Meeresgrund der Sage;", "tokens": ["Im", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Mee\u00b7res\u00b7grund", "der", "Sa\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ins starke Nordland f\u00fchrt es Euch zu Schiffe", "tokens": ["Ins", "star\u00b7ke", "Nord\u00b7land", "f\u00fchrt", "es", "Euch", "zu", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Damit es Euch uralte Schlachten schlage,", "tokens": ["Da\u00b7mit", "es", "Euch", "ur\u00b7al\u00b7te", "Schlach\u00b7ten", "schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Eures Himmels jammervoller Leere,", "tokens": ["In", "Eu\u00b7res", "Him\u00b7mels", "jam\u00b7mer\u00b7vol\u00b7ler", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da zeigt es Euch den Stern gewes'ner Ehre.", "tokens": ["Da", "zeigt", "es", "Euch", "den", "Stern", "ge\u00b7wes'\u00b7ner", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Frei blaut auch mir des Geistes k\u00fchnste Ferne,", "tokens": ["Frei", "blaut", "auch", "mir", "des", "Geis\u00b7tes", "k\u00fchns\u00b7te", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch hab' ich nicht verlernt vor Gott zu beten,", "tokens": ["Doch", "hab'", "ich", "nicht", "ver\u00b7lernt", "vor", "Gott", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Frauenliebe sing' ich gar zu gerne,", "tokens": ["Von", "Frau\u00b7en\u00b7lie\u00b7be", "sing'", "ich", "gar", "zu", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum hab' ich nie mit F\u00fc\u00dfen sie getreten.", "tokens": ["Drum", "hab'", "ich", "nie", "mit", "F\u00fc\u00b7\u00dfen", "sie", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So kann ich nicht, wie Eure j\u00fcngsten Sterne,", "tokens": ["So", "kann", "ich", "nicht", ",", "wie", "Eu\u00b7re", "j\u00fcng\u00b7sten", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Zwitter vom Rou\u00e9 und vom Propheten,", "tokens": ["Die", "Zwit\u00b7ter", "vom", "Rou\u00e9", "und", "vom", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "KON", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den h\u00f6chsten Gott und dann mein Lieb bewitzeln,", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Gott", "und", "dann", "mein", "Lieb", "be\u00b7wit\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich mag Euch nicht mit solchem Schmutze kitzeln.", "tokens": ["Ich", "mag", "Euch", "nicht", "mit", "sol\u00b7chem", "Schmut\u00b7ze", "kit\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "So ist mein Lied, o da\u00df es Euch erbaue,", "tokens": ["So", "ist", "mein", "Lied", ",", "o", "da\u00df", "es", "Euch", "er\u00b7bau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "FM", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag es vielleicht ein Freundesherz ersch\u00fcttern,", "tokens": ["Mag", "es", "viel\u00b7leicht", "ein", "Freun\u00b7des\u00b7herz", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mag es zu Dir, Du allersch\u00f6nste Fraue,", "tokens": ["Mag", "es", "zu", "Dir", ",", "Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als des Verbannten t\u00f6nend Heimweh zittern! \u2013", "tokens": ["Als", "des", "Ver\u00b7bann\u00b7ten", "t\u00f6\u00b7nend", "Heim\u00b7weh", "zit\u00b7tern", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Vaterland, dem bald der Himmel blaue,", "tokens": ["Mein", "Va\u00b7ter\