, "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier welket meine Tugend still verdrossen;", "tokens": ["Hier", "wel\u00b7ket", "mei\u00b7ne", "Tu\u00b7gend", "still", "ver\u00b7dros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erschlie\u00dfe mir des Lebens reichsten Hort,", "tokens": ["Er\u00b7schlie\u00b7\u00dfe", "mir", "des", "Le\u00b7bens", "reichs\u00b7ten", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lehr' mich kennen, was du schon genossen!", "tokens": ["Und", "lehr'", "mich", "ken\u00b7nen", ",", "was", "du", "schon", "ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "La\u00df mich umklammern deine starke Brust;", "tokens": ["La\u00df", "mich", "um\u00b7klam\u00b7mern", "dei\u00b7ne", "star\u00b7ke", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch dir ist ", "tokens": ["Auch", "dir", "ist"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich sprech' es aus, was du noch nicht gewu\u00dft:", "tokens": ["Ich", "sprech'", "es", "aus", ",", "was", "du", "noch", "nicht", "ge\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "wor