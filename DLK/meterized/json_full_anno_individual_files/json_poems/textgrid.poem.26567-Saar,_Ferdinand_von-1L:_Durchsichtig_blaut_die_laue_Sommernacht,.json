{"textgrid.poem.26567": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durchsichtig blaut die laue Sommernacht,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchsichtig blaut die laue Sommernacht,", "tokens": ["Durch\u00b7sich\u00b7tig", "blaut", "die", "lau\u00b7e", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit gl\u00fch'nder Wange kehr' ich vom Gelage;", "tokens": ["Mit", "gl\u00fch'n\u00b7der", "Wan\u00b7ge", "kehr'", "ich", "vom", "Ge\u00b7la\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist die Stunde, wo mein Herz erwacht,", "tokens": ["Das", "ist", "die", "Stun\u00b7de", ",", "wo", "mein", "Herz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das eingeschl\u00e4fert schw\u00fcle, bange Tage.", "tokens": ["Das", "ein\u00b7ge\u00b7schl\u00e4\u00b7fert", "schw\u00fc\u00b7le", ",", "ban\u00b7ge", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VVFIN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Fenster auf. Nun str\u00f6me, linde Luft!", "tokens": ["Die", "Fens\u00b7ter", "auf", ".", "Nun", "str\u00f6\u00b7me", ",", "lin\u00b7de", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie K\u00fchlung zuckt es aus der Sterne Flimmern;", "tokens": ["Wie", "K\u00fch\u00b7lung", "zuckt", "es", "aus", "der", "Ster\u00b7ne", "Flim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All meine Sinne baden sich in Duft \u2013", "tokens": ["All", "mei\u00b7ne", "Sin\u00b7ne", "ba\u00b7den", "sich", "in", "Duft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da seh' ich's wei\u00df mir gegen\u00fcber schimmern.", "tokens": ["Da", "seh'", "ich's", "wei\u00df", "mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie lehnt im Fenster. Antlitz, Arm und Hand", "tokens": ["Sie", "lehnt", "im", "Fens\u00b7ter", ".", "Ant\u00b7litz", ",", "Arm", "und", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Mondenlicht; dem Nacken wird zur H\u00fclle", "tokens": ["Wie", "Mon\u00b7den\u00b7licht", ";", "dem", "Na\u00b7cken", "wird", "zur", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$.", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das zartgewob'ne lichte Florgewand,", "tokens": ["Das", "zart\u00b7ge\u00b7wob'\u00b7ne", "lich\u00b7te", "Flor\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dr\u00fcber hin des Haares gold'ne F\u00fclle.", "tokens": ["Und", "dr\u00fc\u00b7ber", "hin", "des", "Haa\u00b7res", "gold'\u00b7ne", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die sie bei Tag mit stiller Sorgfalt pflegt,", "tokens": ["Die", "sie", "bei", "Tag", "mit", "stil\u00b7ler", "Sorg\u00b7falt", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie duften jetzt die Rosen und Violen!", "tokens": ["Wie", "duf\u00b7ten", "jetzt", "die", "Ro\u00b7sen", "und", "Vi\u00b7o\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Blumen Hauch zu mir her\u00fcber tr\u00e4gt", "tokens": ["Der", "Blu\u00b7men", "Hauch", "zu", "mir", "her\u00b7\u00fc\u00b7ber", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des M\u00e4dchens Seufzer, gl\u00fchend, unverhohlen:", "tokens": ["Des", "M\u00e4d\u00b7chens", "Seuf\u00b7zer", ",", "gl\u00fc\u00b7hend", ",", "un\u00b7ver\u00b7hoh\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du rascher Mann, der du so gierig trinkst", "tokens": ["Du", "ra\u00b7scher", "Mann", ",", "der", "du", "so", "gie\u00b7rig", "trinkst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus vollem Becher, d'rin die Freude sch\u00e4umet;", "tokens": ["Aus", "vol\u00b7lem", "Be\u00b7cher", ",", "d'\u00b7rin", "die", "Freu\u00b7de", "sch\u00e4u\u00b7met", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der du gebieterisch an's Herz dir winkst", "tokens": ["Der", "du", "ge\u00b7bie\u00b7te\u00b7risch", "an's", "Herz", "dir", "winkst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jedwede Lust, die dir zu nahen s\u00e4umet \u2013:", "tokens": ["Jed\u00b7we\u00b7de", "Lust", ",", "die", "dir", "zu", "na\u00b7hen", "s\u00e4u\u00b7met", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "VVFIN", "$(", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "O blicke nieder auf mein einsam Loos,", "tokens": ["O", "bli\u00b7cke", "nie\u00b7der", "auf", "mein", "ein\u00b7sam", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "ADJD", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In meine Seele blicke, wie sie trauert", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", "bli\u00b7cke", ",", "wie", "sie", "trau\u00b7ert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In der Entbehrung ewig kargem Schoo\u00df,", "tokens": ["In", "der", "Ent\u00b7beh\u00b7rung", "e\u00b7wig", "kar\u00b7gem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von ahnungsvollen W\u00fcnschen nur durchschauert.", "tokens": ["Von", "ah\u00b7nungs\u00b7vol\u00b7len", "W\u00fcn\u00b7schen", "nur", "durch\u00b7schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Erf\u00fclle sie! Du kannst es. Nimm mich ", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "sie", "!", "Du", "kannst", "es", ".", "Nimm", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VMFIN", "PPER", "$.", "NE", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier welket meine Tugend still verdrossen;", "tokens": ["Hier", "wel\u00b7ket", "mei\u00b7ne", "Tu\u00b7gend", "still", "ver\u00b7dros\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erschlie\u00dfe mir des Lebens reichsten Hort,", "tokens": ["Er\u00b7schlie\u00b7\u00dfe", "mir", "des", "Le\u00b7bens", "reichs\u00b7ten", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lehr' mich kennen, was du schon genossen!", "tokens": ["Und", "lehr'", "mich", "ken\u00b7nen", ",", "was", "du", "schon", "ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "La\u00df mich umklammern deine starke Brust;", "tokens": ["La\u00df", "mich", "um\u00b7klam\u00b7mern", "dei\u00b7ne", "star\u00b7ke", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch dir ist ", "tokens": ["Auch", "dir", "ist"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich sprech' es aus, was du noch nicht gewu\u00dft:", "tokens": ["Ich", "sprech'", "es", "aus", ",", "was", "du", "noch", "nicht", "ge\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich liebe dich mit grenzenlosem Lieben! \u2013", "tokens": ["Ich", "lie\u00b7be", "dich", "mit", "gren\u00b7zen\u00b7lo\u00b7sem", "Lie\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die Nachtigall im nahen Bauer schweigt,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "im", "na\u00b7hen", "Bau\u00b7er", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie fl\u00f6tete der sch\u00f6nsten aller Stunden;", "tokens": ["Sie", "fl\u00f6\u00b7te\u00b7te", "der", "sch\u00f6ns\u00b7ten", "al\u00b7ler", "Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Morgens Schauer k\u00fchl der Nacht entsteigt,", "tokens": ["Des", "Mor\u00b7gens", "Schau\u00b7er", "k\u00fchl", "der", "Nacht", "ent\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Fenster klirrt \u2013 die Holde ist entschwunden.", "tokens": ["Das", "Fens\u00b7ter", "klirrt", "\u2013", "die", "Hol\u00b7de", "ist", "ent\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Zur Unrast such' ich auf den dumpfen Pf\u00fchl,", "tokens": ["Zur", "Un\u00b7rast", "such'", "ich", "auf", "den", "dum\u00b7pfen", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei ihr verweilen wach noch die Gedanken;", "tokens": ["Bei", "ihr", "ver\u00b7wei\u00b7len", "wach", "noch", "die", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch endlich naht der Schlaf mir, bleiern, schw\u00fcl,", "tokens": ["Doch", "end\u00b7lich", "naht", "der", "Schlaf", "mir", ",", "blei\u00b7ern", ",", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und w\u00fcste Tr\u00e4ume meinen Geist umschwanken.", "tokens": ["Und", "w\u00fcs\u00b7te", "Tr\u00e4u\u00b7me", "mei\u00b7nen", "Geist", "um\u00b7schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Nun ist es Tag! Nun rasch an's Fenster hin!", "tokens": ["Nun", "ist", "es", "Tag", "!", "Nun", "rasch", "an's", "Fens\u00b7ter", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun ist sie bei den Blumen wohl zu schauen \u2013", "tokens": ["Nun", "ist", "sie", "bei", "den", "Blu\u00b7men", "wohl", "zu", "schau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon steht sie dort, als milde Spenderin", "tokens": ["Schon", "steht", "sie", "dort", ",", "als", "mil\u00b7de", "Spen\u00b7de\u00b7rin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die durst'gen Kelche sorglich zu bethauen.", "tokens": ["Die", "dur\u00b7st'\u00b7gen", "Kel\u00b7che", "sorg\u00b7lich", "zu", "be\u00b7thau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Doch wie sie mich gewahrt, der erst sich barg,", "tokens": ["Doch", "wie", "sie", "mich", "ge\u00b7wahrt", ",", "der", "erst", "sich", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVPP", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um sp\u00e4hend zu erhaschen ihre Miene:", "tokens": ["Um", "sp\u00e4\u00b7hend", "zu", "er\u00b7ha\u00b7schen", "ih\u00b7re", "Mie\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tritt sie zur\u00fcck mit Blicken scheu und karg \u2013", "tokens": ["Tritt", "sie", "zu\u00b7r\u00fcck", "mit", "Bli\u00b7cken", "scheu", "und", "karg", "\u2013"], "token_info": ["word", "word", "word", "word", "