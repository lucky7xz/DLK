{"textgrid.poem.54248": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geliebte Magdalis, Annehmlichste der Erden,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Magdalis, Annehmlichste der Erden,", "tokens": ["Ge\u00b7lieb\u00b7te", "Mag\u00b7da\u00b7lis", ",", "An\u00b7nehm\u00b7lichs\u00b7te", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Verschm\u00e4he nicht mein Blat, das dir die H\u00e4nde k\u00fcst.", "tokens": ["Ver\u00b7schm\u00e4\u00b7he", "nicht", "mein", "Blat", ",", "das", "dir", "die", "H\u00e4n\u00b7de", "k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df, da\u00df du nicht kanst entr\u00fcstet dr\u00fcber werden", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "du", "nicht", "kanst", "ent\u00b7r\u00fcs\u00b7tet", "dr\u00fc\u00b7ber", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "PAV", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil jeder, der dich sieht, nicht mehr sein eigen ist.", "tokens": ["Weil", "je\u00b7der", ",", "der", "dich", "sieht", ",", "nicht", "mehr", "sein", "ei\u00b7gen", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Betrachtet man an dir die Rosen-schwangern Wangen,", "tokens": ["Be\u00b7trach\u00b7tet", "man", "an", "dir", "die", "Ro\u00b7sen\u00b7schwan\u00b7gern", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Lippen Purpur-Schein, der Augen Majest\u00e4t,", "tokens": ["Der", "Lip\u00b7pen", "Pur\u00b7pur\u00b7Schein", ",", "der", "Au\u00b7gen", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird ein Rie\u00df und Held gefesselt und gefangen;", "tokens": ["So", "wird", "ein", "Rie\u00df", "und", "Held", "ge\u00b7fes\u00b7selt", "und", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was Wunder? wann es so dem schwachen Crito geht.", "tokens": ["Was", "Wun\u00b7der", "?", "wann", "es", "so", "dem", "schwa\u00b7chen", "Cri\u00b7to", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Hertze liegt so hart, als Fu\u00df und Hand, geschlossen,", "tokens": ["Mein", "Hert\u00b7ze", "liegt", "so", "hart", ",", "als", "Fu\u00df", "und", "Hand", ",", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mich dr\u00fcckt ein hartes Joch, die Freyheit ist dahin.", "tokens": ["Mich", "dr\u00fcckt", "ein", "har\u00b7tes", "Joch", ",", "die", "Frey\u00b7heit", "ist", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein Strahl ist gar zu tieff in Marck und Blut geschossen,", "tokens": ["Dein", "Strahl", "ist", "gar", "zu", "tieff", "in", "Marck", "und", "Blut", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich geh als wie in Traum, und wei\u00df nicht wo ich bin.", "tokens": ["Ich", "geh", "als", "wie", "in", "Traum", ",", "und", "wei\u00df", "nicht", "wo", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "APPR", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Wesen, das man mu\u00df gar unvergleichlich nennen,", "tokens": ["Dein", "We\u00b7sen", ",", "das", "man", "mu\u00df", "gar", "un\u00b7ver\u00b7gleich\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deine Seltenheit hat mich so starck ger\u00fchrt,", "tokens": ["Und", "dei\u00b7ne", "Sel\u00b7ten\u00b7heit", "hat", "mich", "so", "starck", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df man, ich kan es selbst nicht sonder Scham bekennen,", "tokens": ["Da\u00df", "man", ",", "ich", "kan", "es", "selbst", "nicht", "son\u00b7der", "Scham", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gar nichts mehr menschliches an deinen Knecht versp\u00fchrt.", "tokens": ["Gar", "nichts", "mehr", "menschli\u00b7ches", "an", "dei\u00b7nen", "Knecht", "ver\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Hast du nun, Engels-Kind, mir alles di\u00df entrissen,", "tokens": ["Hast", "du", "nun", ",", "En\u00b7gels\u00b7Kind", ",", "mir", "al\u00b7les", "di\u00df", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "NN", "$,", "PPER", "PIS", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was uns zum Menschen macht, so will ich auch nunmehr", "tokens": ["Was", "uns", "zum", "Men\u00b7schen", "macht", ",", "so", "will", "ich", "auch", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nichts mehr vom Uberrest, der mir verblieben, wissen;", "tokens": ["Nichts", "mehr", "vom", "Ub\u00b7er\u00b7rest", ",", "der", "mir", "ver\u00b7blie\u00b7ben", ",", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nimm alles vollends hin und gib mir nur Geh\u00f6r.", "tokens": ["Nimm", "al\u00b7les", "vol\u00b7lends", "hin", "und", "gib", "mir", "nur", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ADV", "PTKVZ", "KON", "VVIMP", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich schw\u00f6re, da\u00df mein Hertz von dir wird nimmer wancken,", "tokens": ["Ich", "schw\u00f6\u00b7re", ",", "da\u00df", "mein", "Hertz", "von", "dir", "wird", "nim\u00b7mer", "wan\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein Bild bleibt meiner Brust auf ewig eingepr\u00e4gt,", "tokens": ["Dein", "Bild", "bleibt", "mei\u00b7ner", "Brust", "auf", "e\u00b7wig", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn Crito opfert dir, Geist, Sinnen, und Gedancken,", "tokens": ["Denn", "Cri\u00b7to", "op\u00b7fert", "dir", ",", "Geist", ",", "Sin\u00b7nen", ",", "und", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "So lange bi\u00df man ihn zu den Erbla\u00dften legt.", "tokens": ["So", "lan\u00b7ge", "bi\u00df", "man", "ihn", "zu", "den", "Er\u00b7bla\u00df\u00b7ten", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Du kanst nicht, Magdalis, auf mein Beginnen fluchen,", "tokens": ["Du", "kanst", "nicht", ",", "Mag\u00b7da\u00b7lis", ",", "auf", "mein", "Be\u00b7gin\u00b7nen", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil selbiges von nichts, als deiner Sch\u00f6nheit stammt,", "tokens": ["Weil", "sel\u00b7bi\u00b7ges", "von", "nichts", ",", "als", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "stammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "PIS", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wirst vorhero wohl den Fehler untersuchen,", "tokens": ["Und", "wirst", "vor\u00b7he\u00b7ro", "wohl", "den", "Feh\u00b7ler", "un\u00b7ter\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Eh mich dein sch\u00f6ner Mund aus Ungedult verdammt.", "tokens": ["Eh", "mich", "dein", "sch\u00f6\u00b7ner", "Mund", "aus", "Un\u00b7ge\u00b7dult", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Geist mu\u00df zwischen Furcht und Hofnung leider! schweben,", "tokens": ["Mein", "Geist", "mu\u00df", "zwi\u00b7schen", "Furcht", "und", "Hof\u00b7nung", "lei\u00b7der", "!", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "ADV", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mein Engel, schreibe doch, ertheile mir Bericht,", "tokens": ["Mein", "En\u00b7gel", ",", "schrei\u00b7be", "doch", ",", "er\u00b7thei\u00b7le", "mir", "Be\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was ich mir Armen soll hierauf zur Antwort geben,", "tokens": ["Was", "ich", "mir", "Ar\u00b7men", "soll", "hier\u00b7auf", "zur", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "NN", "VMFIN", "PAV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und ob mir Magdalis ein gn\u00e4dig Urtheil spricht.", "tokens": ["Und", "ob", "mir", "Mag\u00b7da\u00b7lis", "ein", "gn\u00e4\u00b7dig", "Ur\u00b7theil", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Erschreck mich, Sch\u00f6nste, nicht durch widriges Bezeigen,", "tokens": ["Er\u00b7schreck", "mich", ",", "Sch\u00f6ns\u00b7te", ",", "nicht", "durch", "wid\u00b7ri\u00b7ges", "Be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn soltst du gegen mich etwan tyrannisch seyn,", "tokens": ["Denn", "soltst", "du", "ge\u00b7gen", "mich", "et\u00b7wan", "ty\u00b7ran\u00b7nisch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So wirst du deinen Knecht mehr als empfindlich beugen,", "tokens": ["So", "wirst", "du", "dei\u00b7nen", "Knecht", "mehr", "als", "emp\u00b7find\u00b7lich", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PIAT", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Du st\u00fcrtzest vor der Zeit mich in die Grufft hinein.", "tokens": ["Du", "st\u00fcrt\u00b7zest", "vor", "der", "Zeit", "mich", "in", "die", "Grufft", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Grausamkeit kan nicht so sch\u00f6ne Seelen kleiden,", "tokens": ["Die", "Grau\u00b7sam\u00b7keit", "kan", "nicht", "so", "sch\u00f6\u00b7ne", "See\u00b7len", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Gro\u00dfmuth heist vor sie die sch\u00f6nste Liberey.", "tokens": ["Die", "Gro\u00df\u00b7muth", "heist", "vor", "sie", "die", "sch\u00f6ns\u00b7te", "Li\u00b7be\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "La\u00df deinen Crito nicht so lange Marter leiden,", "tokens": ["La\u00df", "dei\u00b7nen", "Cri\u00b7to", "nicht", "so", "lan\u00b7ge", "Mar\u00b7ter", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ach! mache selbigen von Band und Fesseln frey.", "tokens": ["Ach", "!", "ma\u00b7che", "sel\u00b7bi\u00b7gen", "von", "Band", "und", "Fes\u00b7seln", "frey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es hat zwar die Natur dich, als ein Meister-St\u00fccke,", "tokens": ["Es", "hat", "zwar", "die", "Na\u00b7tur", "dich", ",", "als", "ein", "Meis\u00b7ter\u00b7St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Aus zarten Marmor-Stein und Elfenbein geschnitzt;", "tokens": ["Aus", "zar\u00b7ten", "Mar\u00b7mor\u00b7Stein", "und", "El\u00b7fen\u00b7bein", "ge\u00b7schnitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Doch glaub ich, da\u00df ein Hertz, und di\u00df zu meinen Gl\u00fccke,", "tokens": ["Doch", "glaub", "ich", ",", "da\u00df", "ein", "Hertz", ",", "und", "di\u00df", "zu", "mei\u00b7nen", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "KON", "PDS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "In deiner Schwahnen-Brust von zarten Fleische sitzt.", "tokens": ["In", "dei\u00b7ner", "Schwahnen\u00b7Brust", "von", "zar\u00b7ten", "Flei\u00b7sche", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Verwirrte Zeilen geht, schmiegt euch zu ihren F\u00fcssen,", "tokens": ["Ver\u00b7wirr\u00b7te", "Zei\u00b7len", "geht", ",", "schmiegt", "euch", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Um die sich Crito schon aus Demuth wind und flicht;", "tokens": ["Um", "die", "sich", "Cri\u00b7to", "schon", "aus", "De\u00b7muth", "wind", "und", "flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PRF", "NE", "ADV", "APPR", "NN", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Sehnsucht heisset mich, erlesner Engel, schliessen,", "tokens": ["Die", "Sehn\u00b7sucht", "heis\u00b7set", "mich", ",", "er\u00b7les\u00b7ner", "En\u00b7gel", ",", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ich schliesse zwar den Brief, doch meine Hoffnung nicht.", "tokens": ["Ich", "schlies\u00b7se", "zwar", "den", "Brief", ",", "doch", "mei\u00b7ne", "Hoff\u00b7nung", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geliebte Magdalis, Annehmlichste der Erden,", "tokens": ["Ge\u00b7lieb\u00b7te", "Mag\u00b7da\u00b7lis", ",", "An\u00b7nehm\u00b7lichs\u00b7te", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Verschm\u00e4he nicht mein Blat, das dir die H\u00e4nde k\u00fcst.", "tokens": ["Ver\u00b7schm\u00e4\u00b7he", "nicht", "mein", "Blat", ",", "das", "dir", "die", "H\u00e4n\u00b7de", "k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df, da\u00df du nicht kanst entr\u00fcstet dr\u00fcber werden", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "du", "nicht", "kanst", "ent\u00b7r\u00fcs\u00b7tet", "dr\u00fc\u00b7ber", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "PAV", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil jeder, der dich sieht, nicht mehr sein eigen ist.", "tokens": ["Weil", "je\u00b7der", ",", "der", "dich", "sieht", ",", "nicht", "mehr", "sein", "ei\u00b7gen", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Betrachtet man an dir die Rosen-schwangern Wangen,", "tokens": ["Be\u00b7trach\u00b7tet", "man", "an", "dir", "die", "Ro\u00b7sen\u00b7schwan\u00b7gern", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Lippen Purpur-Schein, der Augen Majest\u00e4t,", "tokens": ["Der", "Lip\u00b7pen", "Pur\u00b7pur\u00b7Schein", ",", "der", "Au\u00b7gen", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird ein Rie\u00df und Held gefesselt und gefangen;", "tokens": ["So", "wird", "ein", "Rie\u00df", "und", "Held", "ge\u00b7fes\u00b7selt", "und", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was Wunder? wann es so dem schwachen Crito geht.", "tokens": ["Was", "Wun\u00b7der", "?", "wann", "es", "so", "dem", "schwa\u00b7chen", "Cri\u00b7to", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Hertze liegt so hart, als Fu\u00df und Hand, geschlossen,", "tokens": ["Mein", "Hert\u00b7ze", "liegt", "so", "hart", ",", "als", "Fu\u00df", "und", "Hand", ",", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mich dr\u00fcckt ein hartes Joch, die Freyheit ist dahin.", "tokens": ["Mich", "dr\u00fcckt", "ein", "har\u00b7tes", "Joch", ",", "die", "Frey\u00b7heit", "ist", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein Strahl ist gar zu tieff in Marck und Blut geschossen,", "tokens": ["Dein", "Strahl", "ist", "gar", "zu", "tieff", "in", "Marck", "und", "Blut", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich geh als wie in Traum, und wei\u00df nicht wo ich bin.", "tokens": ["Ich", "geh", "als", "wie", "in", "Traum", ",", "und", "wei\u00df", "nicht", "wo", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "APPR", "NN", "$,", "KON", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Wesen, das man mu\u00df gar unvergleichlich nennen,", "tokens": ["Dein", "We\u00b7sen", ",", "das", "man", "mu\u00df", "gar", "un\u00b7ver\u00b7gleich\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deine Seltenheit hat mich so starck ger\u00fchrt,", "tokens": ["Und", "dei\u00b7ne", "Sel\u00b7ten\u00b7heit", "hat", "mich", "so", "starck", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df man, ich kan es selbst nicht sonder Scham bekennen,", "tokens": ["Da\u00df", "man", ",", "ich", "kan", "es", "selbst", "nicht", "son\u00b7der", "Scham", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gar nichts mehr menschliches an deinen Knecht versp\u00fchrt.", "tokens": ["Gar", "nichts", "mehr", "menschli\u00b7ches", "an", "dei\u00b7nen", "Knecht", "ver\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Hast du nun, Engels-Kind, mir alles di\u00df entrissen,", "tokens": ["Hast", "du", "nun", ",", "En\u00b7gels\u00b7Kind", ",", "mir", "al\u00b7les", "di\u00df", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "NN", "$,", "PPER", "PIS", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was uns zum Menschen macht, so will ich auch nunmehr", "tokens": ["Was", "uns", "zum", "Men\u00b7schen", "macht", ",", "so", "will", "ich", "auch", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nichts mehr vom Uberrest, der mir verblieben, wissen;", "tokens": ["Nichts", "mehr", "vom", "Ub\u00b7er\u00b7rest", ",", "der", "mir", "ver\u00b7blie\u00b7ben", ",", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nimm alles vollends hin und gib mir nur Geh\u00f6r.", "tokens": ["Nimm", "al\u00b7les", "vol\u00b7lends", "hin", "und", "gib", "mir", "nur", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ADV", "PTKVZ", "KON", "VVIMP", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich schw\u00f6re, da\u00df mein Hertz von dir wird nimmer wancken,", "tokens": ["Ich", "schw\u00f6\u00b7re", ",", "da\u00df", "mein", "Hertz", "von", "dir", "wird", "nim\u00b7mer", "wan\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein Bild bleibt meiner Brust auf ewig eingepr\u00e4gt,", "tokens": ["Dein", "Bild", "bleibt", "mei\u00b7ner", "Brust", "auf", "e\u00b7wig", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn Crito opfert dir, Geist, Sinnen, und Gedancken,", "tokens": ["Denn", "Cri\u00b7to", "op\u00b7fert", "dir", ",", "Geist", ",", "Sin\u00b7nen", ",", "und", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "So lange bi\u00df man ihn zu den Erbla\u00dften legt.", "tokens": ["So", "lan\u00b7ge", "bi\u00df", "man", "ihn", "zu", "den", "Er\u00b7bla\u00df\u00b7ten", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Du kanst nicht, Magdalis, auf mein Beginnen fluchen,", "tokens": ["Du", "kanst", "nicht", ",", "Mag\u00b7da\u00b7lis", ",", "auf", "mein", "Be\u00b7gin\u00b7nen", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil selbiges von nichts, als deiner Sch\u00f6nheit stammt,", "tokens": ["Weil", "sel\u00b7bi\u00b7ges", "von", "nichts", ",", "als", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "stammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "PIS", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wirst vorhero wohl den Fehler untersuchen,", "tokens": ["Und", "wirst", "vor\u00b7he\u00b7ro", "wohl", "den", "Feh\u00b7ler", "un\u00b7ter\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Eh mich dein sch\u00f6ner Mund aus Ungedult verdammt.", "tokens": ["Eh", "mich", "dein", "sch\u00f6\u00b7ner", "Mund", "aus", "Un\u00b7ge\u00b7dult", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Geist mu\u00df zwischen Furcht und Hofnung leider! schweben,", "tokens": ["Mein", "Geist", "mu\u00df", "zwi\u00b7schen", "Furcht", "und", "Hof\u00b7nung", "lei\u00b7der", "!", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "ADV", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mein Engel, schreibe doch, ertheile mir Bericht,", "tokens": ["Mein", "En\u00b7gel", ",", "schrei\u00b7be", "doch", ",", "er\u00b7thei\u00b7le", "mir", "Be\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was ich mir Armen soll hierauf zur Antwort geben,", "tokens": ["Was", "ich", "mir", "Ar\u00b7men", "soll", "hier\u00b7auf", "zur", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "NN", "VMFIN", "PAV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und ob mir Magdalis ein gn\u00e4dig Urtheil spricht.", "tokens": ["Und", "ob", "mir", "Mag\u00b7da\u00b7lis", "ein", "gn\u00e4\u00b7dig", "Ur\u00b7theil", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Erschreck mich, Sch\u00f6nste, nicht durch widriges Bezeigen,", "tokens": ["Er\u00b7schreck", "mich", ",", "Sch\u00f6ns\u00b7te", ",", "nicht", "durch", "wid\u00b7ri\u00b7ges", "Be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn soltst du gegen mich etwan tyrannisch seyn,", "tokens": ["Denn", "soltst", "du", "ge\u00b7gen", "mich", "et\u00b7wan", "ty\u00b7ran\u00b7nisch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So wirst du deinen Knecht mehr als empfindlich beugen,", "tokens": ["So", "wirst", "du", "dei\u00b7nen", "Knecht", "mehr", "als", "emp\u00b7find\u00b7lich", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PIAT", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Du st\u00fcrtzest vor der Zeit mich in die Grufft hinein.", "tokens": ["Du", "st\u