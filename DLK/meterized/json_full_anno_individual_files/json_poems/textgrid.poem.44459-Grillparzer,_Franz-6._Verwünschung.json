{"textgrid.poem.44459": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "6. Verw\u00fcnschung", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rst du so gut, als sch\u00f6n du bist vor vielen,", "tokens": ["W\u00e4rst", "du", "so", "gut", ",", "als", "sch\u00f6n", "du", "bist", "vor", "vie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Krone w\u00e4rst du dessen, was man sieht;", "tokens": ["Die", "Kro\u00b7ne", "w\u00e4rst", "du", "des\u00b7sen", ",", "was", "man", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So aber mu\u00dftest du mit Wort und Treue spielen,", "tokens": ["So", "a\u00b7ber", "mu\u00df\u00b7test", "du", "mit", "Wort", "und", "Treu\u00b7e", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und freun dich noch des Unheils, das geschieht.", "tokens": ["Und", "freun", "dich", "noch", "des", "Un\u00b7heils", ",", "das", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wenn auch! H\u00e4tte nicht ein Gott im Grimme", "tokens": ["Und", "wenn", "auch", "!", "H\u00e4t\u00b7te", "nicht", "ein", "Gott", "im", "Grim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "$.", "VVFIN", "PTKNEG", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bunt vermengt, was feindlich sonst und zwei,", "tokens": ["So", "bunt", "ver\u00b7mengt", ",", "was", "feind\u00b7lich", "sonst", "und", "zwei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PRELS", "ADJD", "ADV", "KON", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man lobte, wo du gut, und tadelte das Schlimme,", "tokens": ["Man", "lob\u00b7te", ",", "wo", "du", "gut", ",", "und", "ta\u00b7del\u00b7te", "das", "Schlim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu w\u00e4hlen dich, zu lassen st\u00fcnde frei.", "tokens": ["Zu", "w\u00e4h\u00b7len", "dich", ",", "zu", "las\u00b7sen", "st\u00fcn\u00b7de", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "$,", "PTKZU", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun aber l\u00f6scht des Trachtens b\u00f6se T\u00fccke", "tokens": ["Nun", "a\u00b7ber", "l\u00f6scht", "des", "Trach\u00b7tens", "b\u00f6\u00b7se", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV"