reicht ihr Haar nur glatt;", "tokens": ["Er", "streicht", "ihr", "Haar", "nur", "glatt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er l\u00e4\u00dft die Bl\u00e4tter nicht zittern,", "tokens": ["Er", "l\u00e4\u00dft", "die", "Bl\u00e4t\u00b7ter", "nicht", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Er k\u00fc\u00dft nur jedes Blatt.", "tokens": ["Er", "k\u00fc\u00dft", "nur", "je\u00b7des", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Blumen traumhaft schwanken", "tokens": ["Die", "Blu\u00b7men", "traum\u00b7haft", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und atmen wollustschwer,", "tokens": ["Und", "at\u00b7men", "wol\u00b7lust\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es flattern M\u00e4rchengedanken", "tokens": ["Es", "flat\u00b7tern", "M\u00e4r\u00b7chen\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVINF", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um ihre H\u00e4upter her.", "tokens": ["Um", "ih\u00b7re", "H\u00e4up\u00b7ter", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "D