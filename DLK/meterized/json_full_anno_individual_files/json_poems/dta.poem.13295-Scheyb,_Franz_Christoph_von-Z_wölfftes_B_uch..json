{"dta.poem.13295": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein weil sie den Krei\u00df noch nicht beysammen sah,", "tokens": ["Al\u00b7lein", "weil", "sie", "den", "Krei\u00df", "noch", "nicht", "bey\u00b7sam\u00b7men", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wars, da\u00df dieser Saz jezt noch umsonst geschah.", "tokens": ["So", "wars", ",", "da\u00df", "die\u00b7ser", "Saz", "jezt", "noch", "um\u00b7sonst", "ge\u00b7schah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PDAT", "NN", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es stund die ", "tokens": ["Es", "stund", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Als ob sie, wie es schien, um eine Sache streite;", "tokens": ["Als", "ob", "sie", ",", "wie", "es", "schien", ",", "um", "ei\u00b7ne", "Sa\u00b7che", "strei\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUI", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "165Wir beyde giengen hin; der Vorwiz triebe mich,", "tokens": ["bey\u00b7de", "gien\u00b7gen", "hin", ";", "der", "Vor\u00b7wiz", "trie\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Weil manche wiederum von ihrer Stelle wich.", "tokens": ["Weil", "man\u00b7che", "wie\u00b7de\u00b7rum", "von", "ih\u00b7rer", "Stel\u00b7le", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}