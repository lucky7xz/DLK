{"textgrid.poem.65209": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tanzlied", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem tiefen stillen Grund", "tokens": ["Aus", "dem", "tie\u00b7fen", "stil\u00b7len", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klingen die Schalmeien.", "tokens": ["Klin\u00b7gen", "die", "Schal\u00b7mei\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sie tanzen wohl auf gr\u00fcnem Rund", "tokens": ["Sie", "tan\u00b7zen", "wohl", "auf", "gr\u00fc\u00b7nem", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schatten der k\u00fchlen Maien.", "tokens": ["Im", "Schat\u00b7ten", "der", "k\u00fch\u00b7len", "Mai\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Alle Weisen kenn' ich ja,", "tokens": ["Al\u00b7le", "Wei\u00b7sen", "kenn'", "ich", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann sie pfeifen und singen;", "tokens": ["Kann", "sie", "pfei\u00b7fen", "und", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Schon ist es mir, als w\u00e4r' ich da,", "tokens": ["Schon", "ist", "es", "mir", ",", "als", "w\u00e4r'", "ich", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sie h\u00fcpfen und schweben und springen.", "tokens": ["Wo", "sie", "h\u00fcp\u00b7fen", "und", "schwe\u00b7ben", "und", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Meine Sohlen heben sich,", "tokens": ["Mei\u00b7ne", "Soh\u00b7len", "he\u00b7ben", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Herz wird munter.", "tokens": ["Und", "mein", "Herz", "wird", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, liebes Kind, und s\u00e4h' ich dich,", "tokens": ["Ach", ",", "lie\u00b7bes", "Kind", ",", "und", "s\u00e4h'", "ich", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich spr\u00e4nge von oben hinunter.", "tokens": ["Ich", "spr\u00e4n\u00b7ge", "von", "o\u00b7ben", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Wenn ein andrer Bursch dich dreht,", "tokens": ["Wenn", "ein", "an\u00b7drer", "Bursch", "dich", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dich nicht verdrehen!", "tokens": ["La\u00df", "dich", "nicht", "ver\u00b7dre\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dein K\u00f6pfchen, wenn das fest nicht steht,", "tokens": ["Dein", "K\u00f6pf\u00b7chen", ",", "wenn", "das", "fest", "nicht", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PDS", "VVFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie soll mein Wort denn stehen?", "tokens": ["Wie", "soll", "mein", "Wort", "denn", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn eine Nadel dir", "tokens": ["Und", "wenn", "ei\u00b7ne", "Na\u00b7del", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Abf\u00e4llt aus dem Mieder,", "tokens": ["Ab\u00b7f\u00e4llt", "aus", "dem", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Das giebt in's Herz zehn Stiche mir,", "tokens": ["Das", "giebt", "in's", "Herz",