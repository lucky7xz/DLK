{"textgrid.poem.32481": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf - - -", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbo k\u00e4m der gro\u00dfe Geist bald in dies rauhe Land,", "tokens": ["\u00bb", "o", "k\u00e4m", "der", "gro\u00b7\u00dfe", "Geist", "bald", "in", "dies", "rau\u00b7he", "Land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "ART", "ADJA", "NN", "ADV", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohin aus Frankreichs Rom mich Nasos Gl\u00fcck verbannt,", "tokens": ["Wo\u00b7hin", "aus", "Fran\u00b7kreichs", "Rom", "mich", "Na\u00b7sos", "Gl\u00fcck", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NE", "PPER", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00e4r doch einer hier, noch au\u00dfer mir zu finden,", "tokens": ["So", "w\u00e4r", "doch", "ei\u00b7ner", "hier", ",", "noch", "au\u00b7\u00dfer", "mir", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADV", "$,", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dessen Munde sich Geschmack und Witz verbinden.", "tokens": ["In", "des\u00b7sen", "Mun\u00b7de", "sich", "Ge\u00b7schmack", "und", "Witz", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm Voltair! \u2013 \u2013\u00ab A ** gnug! der Himmel h\u00f6rt dein Flehn.", "tokens": ["Komm", "Vol\u00b7tair", "!", "\u2013", "\u2013", "\u00ab", "A", "*", "*", "gnug", "!", "der", "Him\u00b7mel", "h\u00f6rt", "dein", "Flehn", "."], "token_info": ["word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "$(", "$(", "$(", "XY", "XY", "XY", "ADV", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Er k\u00f6mmt, und l\u00e4\u00dft sogleich des Geistes Proben sehn.", "tokens": ["Er", "k\u00f6mmt", ",", "und", "l\u00e4\u00dft", "sog\u00b7leich", "des", "Geis\u00b7tes", "Pro\u00b7ben", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u00bbwas? ruft er; A** hier? Wenn mich der K\u00f6nig liebt,", "tokens": ["\u00bb", "was", "?", "ruft", "er", ";", "A", "*", "*", "hier", "?", "Wenn", "mich", "der", "K\u00f6\u00b7nig", "liebt", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "VVFIN", "PPER", "$.", "XY", "XY", "XY", "ADV", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So wei\u00df ich, da\u00df er stracks dem Schurken Abschied gibt.\u00ab", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "er", "stracks", "dem", "Schur\u00b7ken", "Ab\u00b7schied", "gibt", ".", "\u00ab"], "token_in