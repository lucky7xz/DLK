{"textgrid.poem.37456": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geboren ward er ohne Wehen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geboren ward er ohne Wehen", "tokens": ["Ge\u00b7bo\u00b7ren", "ward", "er", "oh\u00b7ne", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Leuten, die mit Geld versehen.", "tokens": ["Bei", "Leu\u00b7ten", ",", "die", "mit", "Geld", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er schw\u00e4nzt die Schule, lernt nicht viel,", "tokens": ["Er", "schw\u00e4nzt", "die", "Schu\u00b7le", ",", "lernt", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat Gl\u00fcck bei Weibern und im Spiel,", "tokens": ["Hat", "Gl\u00fcck", "bei", "Wei\u00b7bern", "und", "im", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimmt eine Frau sich, eine sch\u00f6ne,", "tokens": ["Nimmt", "ei\u00b7ne", "Frau", "sich", ",", "ei\u00b7ne", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erzeugt mit ihr zwei kluge S\u00f6hne,", "tokens": ["Er\u00b7zeugt", "mit", "ihr", "zwei", "klu\u00b7ge", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "CARD", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Hat Appetit, kriegt einen Bauch,", "tokens": ["Hat", "Ap\u00b7pe\u00b7tit", ",", "kriegt", "ei\u00b7nen", "Bauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und einen Orden kriegt er auch,", "tokens": ["Und", "ei\u00b7nen", "Or\u00b7den", "kriegt", "er", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und stirbt, nachdem er aufgespeichert", "tokens": ["Und", "stirbt", ",", "nach\u00b7dem", "er", "auf\u00b7ge\u00b7spei\u00b7chert"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein paar Milli\u00f6nchen, hochbetagt;", "tokens": ["Ein", "paar", "Mil\u00b7li\u00b7\u00f6n\u00b7chen", ",", "hoch\u00b7be\u00b7tagt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Obgleich ein jeder wei\u00df und sagt:", "tokens": ["Ob\u00b7gleich", "ein", "je\u00b7der", "wei\u00df", "und", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er wa