{"textgrid.poem.53114": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wiltu nichts vom Breutgam h\u00f6ren,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiltu nichts vom Breutgam h\u00f6ren,", "tokens": ["Wil\u00b7tu", "nichts", "vom", "Breut\u00b7gam", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPRART", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnschest dir f\u00fcr ihm den Tod?", "tokens": ["W\u00fcn\u00b7schest", "dir", "f\u00fcr", "ihm", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass dich nicht, mein Kind, beth\u00f6ren,", "tokens": ["Lass", "dich", "nicht", ",", "mein", "Kind", ",", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Setz dich willig nicht in Noht!", "tokens": ["Setz", "dich", "wil\u00b7lig", "nicht", "in", "Noht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denck was dieses sey f\u00fcr Pein:", "tokens": ["Denck", "was", "die\u00b7ses", "sey", "f\u00fcr", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alt, vnd