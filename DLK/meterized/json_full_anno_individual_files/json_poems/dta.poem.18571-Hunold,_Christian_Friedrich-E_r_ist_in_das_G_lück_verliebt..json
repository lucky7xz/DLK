{"dta.poem.18571": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E r ist in das  G l\u00fcck verliebt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So soll ich mich niemahls vergn\u00fcget wissen?", "tokens": ["So", "soll", "ich", "mich", "nie\u00b7mahls", "ver\u00b7gn\u00fc\u00b7get", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Beliebtes Gl\u00fcck/ was fliehst du denn von mir?", "tokens": ["Be\u00b7lieb\u00b7tes", "Gl\u00fcck", "/", "was", "fliehst", "du", "denn", "von", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was l\u00e4st du mich nur in Gedancken k\u00fcssen/", "tokens": ["Was", "l\u00e4st", "du", "mich", "nur", "in", "Ge\u00b7dan\u00b7cken", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Geist \u00fcmarmt den Schatten nur von dir.", "tokens": ["Mein", "Geist", "\u00fcm\u00b7armt", "den", "Schat\u00b7ten", "nur", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach g\u00f6nne mir doch einst dein sch\u00f6nes Wesen", "tokens": ["Ach", "g\u00f6n\u00b7ne", "mir", "doch", "einst", "dein", "sch\u00f6\u00b7nes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und letze mich durch deine Gegenwart.", "tokens": ["Und", "let\u00b7ze", "mich", "durch", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich habe dich zur Liebsten auserlesen", "tokens": ["Ich", "ha\u00b7be", "dich", "zur", "Liebs\u00b7ten", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was spielest du mit denen Knecht so hart?", "tokens": ["Was", "spie\u00b7lest", "du", "mit", "de\u00b7nen", "Knecht", "so", "hart", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PRELS", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du weist ja wohl/ wie wir zuerst gestanden/", "tokens": ["Du", "weist", "ja", "wohl", "/", "wie", "wir", "zu\u00b7erst", "ge\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie mich dein Arm an deine Brust gedr\u00fcckt.", "tokens": ["Wie", "mich", "dein", "Arm", "an", "dei\u00b7ne", "Brust", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du lie\u00dfest mich an deinen Ufer landen/", "tokens": ["Du", "lie\u00b7\u00dfest", "mich", "an", "dei\u00b7nen", "U\u00b7fer", "lan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dein sch\u00f6ner Schoo\u00df hat Geist und Seel erquickt.", "tokens": ["Dein", "sch\u00f6\u00b7ner", "Schoo\u00df", "hat", "Geist", "und", "Seel", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie bist du mir denn nun so grausam worden?", "tokens": ["Wie", "bist", "du", "mir", "denn", "nun", "so", "grau\u00b7sam", "wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mein Kind du brichst die mir geschworne Treu!", "tokens": ["Mein", "Kind", "du", "brichst", "die", "mir", "ge\u00b7schwor\u00b7ne", "Treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ach dencke doch/ da\u00df in den Liebes-Orden", "tokens": ["Ach", "den\u00b7cke", "doch", "/", "da\u00df", "in", "den", "Lie\u00b7bes\u00b7Or\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "$(", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nur Unbestand das gr\u00f6ste Laster sey.", "tokens": ["Nur", "Un\u00b7be\u00b7stand", "das", "gr\u00f6s\u00b7te", "Las\u00b7ter", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So kehre dann nun wiederm zur\u00fccke/", "tokens": ["So", "keh\u00b7re", "dann", "nun", "wie\u00b7derm", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und \u00e4ndere den allzu harten Sinn:", "tokens": ["Und", "\u00e4n\u00b7de\u00b7re", "den", "all\u00b7zu", "har\u00b7ten", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergeht die Nacht durch heitre Sonnen Blicke/", "tokens": ["Ver\u00b7geht", "die", "Nacht", "durch", "heit\u00b7re", "Son\u00b7nen", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So stirbt mein Leid durch deine Gnade hin.", "tokens": ["So", "stirbt", "mein", "Leid", "durch", "dei\u00b7ne", "Gna\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es schwinden gleich die Ungl\u00fccks vollen Schatten", "tokens": ["Es", "schwin\u00b7den", "gleich", "die", "Un\u00b7gl\u00fccks", "vol\u00b7len", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So bald mich nur dein froher Morgen k\u00fcst:", "tokens": ["So", "bald", "mich", "nur", "dein", "fro\u00b7her", "Mor\u00b7gen", "k\u00fcst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sonst wird der Tod sich mit der Seelen gatten/", "tokens": ["Sonst", "wird", "der", "Tod", "sich", "mit", "der", "See\u00b7len", "gat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weil Ungel\u00fcck doch stetes Sterben ist.", "tokens": ["Weil", "Un\u00b7ge\u00b7l\u00fcck", "doch", "ste\u00b7tes", "Ster\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}