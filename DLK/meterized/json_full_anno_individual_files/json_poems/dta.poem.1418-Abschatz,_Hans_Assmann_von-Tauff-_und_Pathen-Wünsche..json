{"dta.poem.1418": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des Christenthumes Grund/", "tokens": ["Des", "Chris\u00b7ten\u00b7thu\u00b7mes", "Grund", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der heilgen Tauffe Bund", "tokens": ["Der", "heil\u00b7gen", "Tauf\u00b7fe", "Bund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heilt uns und macht gesund", "tokens": ["Heilt", "uns", "und", "macht", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was Erb-Schuld hat verwund/", "tokens": ["Was", "Er\u00b7bSchuld", "hat", "ver\u00b7wund", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie Christus selbst/ der Mund", "tokens": ["Wie", "Chris\u00b7tus", "selbst", "/", "der", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "ADV", "$(", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Warheit/ machet kund:", "tokens": ["Der", "War\u00b7heit", "/", "ma\u00b7chet", "kund", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den/ Pathe/ trittst du an/", "tokens": ["Den", "/", "Pa\u00b7the", "/", "trittst", "du", "an", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NE", "$(", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Damit dir solches auch zu statten kommen kan.", "tokens": ["Da\u00b7mit", "dir", "sol\u00b7ches", "auch", "zu", "stat\u00b7ten", "kom\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "PTKZU", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}