{"dta.poem.4233": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erndte-Feyer in Ritzeb\u00fcttel,  \n  1736 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit ehrerbietigstem Gem\u00fchte", "tokens": ["Mit", "ehr\u00b7er\u00b7bie\u00b7tigs\u00b7tem", "Ge\u00b7m\u00fch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Verehr\u2019 ich Deine Gnad\u2019 und G\u00fcte,", "tokens": ["Ver\u00b7ehr'", "ich", "Dei\u00b7ne", "Gnad'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O HErr, da\u00df Deine Vater-Hand", "tokens": ["O", "Herr", ",", "da\u00df", "Dei\u00b7ne", "Va\u00b7ter\u00b7Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Gl\u00fcck mir zugewandt,", "tokens": ["Ein", "neu\u00b7es", "Gl\u00fcck", "mir", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ich nicht gnug zu sch\u00e4tzen wei\u00df,", "tokens": ["So", "ich", "nicht", "gnug", "zu", "sch\u00e4t\u00b7zen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da Du mich hast, aus Lieb\u2019, erlesen,", "tokens": ["Da", "Du", "mich", "hast", ",", "aus", "Lieb'", ",", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu mehren Deines Namens Preis,", "tokens": ["Zu", "meh\u00b7ren", "Dei\u00b7nes", "Na\u00b7mens", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und da\u00df ich heut, auf Dein Gehei\u00df,", "tokens": ["Und", "da\u00df", "ich", "heut", ",", "auf", "Dein", "Ge\u00b7hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Durch Dich, Dein Werkzeug bin gewesen.", "tokens": ["Durch", "Dich", ",", "Dein", "Werk\u00b7zeug", "bin", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du hast mir gn\u00e4dig wollen g\u00f6nnen,", "tokens": ["Du", "hast", "mir", "gn\u00e4\u00b7dig", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df ich, zur frohen Wirklichkeit,", "tokens": ["Da\u00df", "ich", ",", "zur", "fro\u00b7hen", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das, was ich schon so lange Zeit", "tokens": ["Das", ",", "was", "ich", "schon", "so", "lan\u00b7ge", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gew\u00fcnschet habe, bringen k\u00f6nnen.", "tokens": ["Ge\u00b7w\u00fcn\u00b7schet", "ha\u00b7be", ",", "brin\u00b7gen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den mir von Dir geschenkten Stand,", "tokens": ["Den", "mir", "von", "Dir", "ge\u00b7schenk\u00b7ten", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und die damit verkn\u00fcpfte Macht,", "tokens": ["Und", "die", "da\u00b7mit", "ver\u00b7kn\u00fcpf\u00b7te", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab\u2019 ich mit Freuden angewandt,", "tokens": ["Hab'", "ich", "mit", "Freu\u00b7den", "an\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df diesen Tag ein ganzes Land,", "tokens": ["Da\u00df", "die\u00b7sen", "Tag", "ein", "gan\u00b7zes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So Deine Wunder ohn\u2019 Bedacht", "tokens": ["So", "Dei\u00b7ne", "Wun\u00b7der", "ohn'", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bishero, leider! angesehen,", "tokens": ["Bis\u00b7he\u00b7ro", ",", "lei\u00b7der", "!", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Dank und Lob itzt mu\u00df gestehen,", "tokens": ["Mit", "Dank", "und", "Lob", "itzt", "mu\u00df", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Deines grossen Namens Preise:", "tokens": ["Zu", "Dei\u00b7nes", "gros\u00b7sen", "Na\u00b7mens", "Prei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df Du es seyst, der Brodt und Speise,", "tokens": ["Da\u00df", "Du", "es", "seyst", ",", "der", "Brodt", "und", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf eine wunderbare Weise,", "tokens": ["Auf", "ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "In solchem Ueberflu\u00df f\u00fcr sie,", "tokens": ["In", "sol\u00b7chem", "Ue\u00b7berf\u00b7lu\u00df", "f\u00fcr", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Jmgleichen Futter f\u00fcr ihr Vieh,", "tokens": ["Jm\u00b7glei\u00b7chen", "Fut\u00b7ter", "f\u00fcr", "ihr", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus Wasser, Luft und Erde bringt;", "tokens": ["Aus", "Was\u00b7ser", ",", "Luft", "und", "Er\u00b7de", "bringt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df von dem Sonnen-Schein und Regen", "tokens": ["Da\u00df", "von", "dem", "Son\u00b7nen\u00b7Schein", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Saamen-Korn ein reicher Segen", "tokens": ["Jm", "Saa\u00b7men\u00b7Korn", "ein", "rei\u00b7cher", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur blo\u00df allein durch Dich entspringt.", "tokens": ["Nur", "blo\u00df", "al\u00b7lein", "durch", "Dich", "ent\u00b7springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab\u2019 ein Dank-Fest eingesetzet,", "tokens": ["Ich", "hab'", "ein", "Dank\u00b7Fest", "ein\u00b7ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das sich, mit Fug, ein Erndte-Fest,", "tokens": ["Das", "sich", ",", "mit", "Fug", ",", "ein", "Ernd\u00b7te\u00b7Fest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PRF", "$,", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Ritzeb\u00fcttel, nennen l\u00e4\u00dft,", "tokens": ["In", "Rit\u00b7ze\u00b7b\u00fct\u00b7tel", ",", "nen\u00b7nen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Woran sich Jung und Alt ergetzet.", "tokens": ["Wo\u00b7ran", "sich", "Jung", "und", "Alt", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Tag brach an, der hell und klar", "tokens": ["Der", "Tag", "brach", "an", ",", "der", "hell", "und", "klar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(ein g\u00fcnstig Himmels-Zeichen!) war.", "tokens": ["(", "ein", "g\u00fcns\u00b7tig", "Him\u00b7mels\u00b7Zei\u00b7chen", "!", ")", "war", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "$.", "$(", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man h\u00f6rte das Gel\u00e4ut\u2019 erschallen;", "tokens": ["Man", "h\u00f6r\u00b7te", "das", "Ge\u00b7l\u00e4ut'", "er\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man sah, auf diesen frohen Schall,", "tokens": ["Man", "sah", ",", "auf", "die\u00b7sen", "fro\u00b7hen", "Schall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Weg- und Stegen \u00fcberall,", "tokens": ["Auf", "Weg", "und", "Ste\u00b7gen", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Hause Gottes fr\u00f6hlich wallen", "tokens": ["Zum", "Hau\u00b7se", "Got\u00b7tes", "fr\u00f6h\u00b7lich", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Land-Leut\u2019 ausgeschm\u00fcckte Schaar,", "tokens": ["Der", "Lan\u00b7dLeut'", "aus\u00b7ge\u00b7schm\u00fcck\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Menge, die Beweglichkeit,", "tokens": ["Die", "Men\u00b7ge", ",", "die", "Be\u00b7weg\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die bunten Kleider in dem Gr\u00fcnen,", "tokens": ["Die", "bun\u00b7ten", "Klei\u00b7der", "in", "dem", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vom hellen Sonnen-Strahl beschienen,", "tokens": ["Vom", "hel\u00b7len", "Son\u00b7nen\u00b7Strahl", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "War lustig, angenehm und sch\u00f6n,", "tokens": ["War", "lus\u00b7tig", ",", "an\u00b7ge\u00b7nehm", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In mancher Farb\u2019 und Form, zu sehn.", "tokens": ["In", "man\u00b7cher", "Fa\u00b7rb'", "und", "Form", ",", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Hier f\u00fchrte sie ein schmaler Steg", "tokens": ["Hier", "f\u00fchr\u00b7te", "sie", "ein", "schma\u00b7ler", "Steg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Durch noch nicht abgem\u00e4hten Rocken,", "tokens": ["Durch", "noch", "nicht", "ab\u00b7ge\u00b7m\u00e4h\u00b7ten", "Ro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und dort ein allgemeiner Weg", "tokens": ["Und", "dort", "ein", "all\u00b7ge\u00b7mei\u00b7ner", "Weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Durch zierlich aufgeth\u00fcrmte Hocken.", "tokens": ["Durch", "zier\u00b7lich", "auf\u00b7ge\u00b7th\u00fcrm\u00b7te", "Ho\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Kirche war, in vielen Reihen,", "tokens": ["Die", "Kir\u00b7che", "war", ",", "in", "vie\u00b7len", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Mit frischen, gr\u00fcnen, jungen Mayen,", "tokens": ["Mit", "fri\u00b7schen", ",", "gr\u00fc\u00b7nen", ",", "jun\u00b7gen", "Ma\u00b7yen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Beh\u00e4g- und lieblich ausgezieret.", "tokens": ["Be\u00b7h\u00e4g", "und", "lieb\u00b7lich", "aus\u00b7ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Es konnt\u2019 an diesen netten B\u00fcschen", "tokens": ["Es", "konnt'", "an", "die\u00b7sen", "net\u00b7ten", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Nicht nur das Auge sich erfrischen;", "tokens": ["Nicht", "nur", "das", "Au\u00b7ge", "sich", "er\u00b7fri\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Es ward von ihrem s\u00fcssen Duft", "tokens": ["Es", "ward", "von", "ih\u00b7rem", "s\u00fcs\u00b7sen", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die ganz damit erf\u00fcllte Luft,", "tokens": ["Die", "ganz", "da\u00b7mit", "er\u00b7f\u00fcll\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und auch, durch sie, der Geist ger\u00fchret.", "tokens": ["Und", "auch", ",", "durch", "sie", ",", "der", "Geist", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da, durch das Laub, denn alles bunt", "tokens": ["Da", ",", "durch", "das", "Laub", ",", "denn", "al\u00b7les", "bunt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,", "KON", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und als im gr\u00fcnen Walde stund.", "tokens": ["Und", "als", "im", "gr\u00fc\u00b7nen", "Wal\u00b7de", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df neue nun wirkt, durch die Seltenheit,", "tokens": ["Die\u00df", "neu\u00b7e", "nun", "wirkt", ",", "durch", "die", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sowohl im Munde ihrer Lehrer,", "tokens": ["So\u00b7wohl", "im", "Mun\u00b7de", "ih\u00b7rer", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihren Text ", "tokens": ["Die", "ih\u00b7ren", "Text"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Als in den Ohren ihrer H\u00f6rer,", "tokens": ["Als", "in", "den", "Oh\u00b7ren", "ih\u00b7rer", "H\u00f6\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sich bey beyden Achtsamkeit,", "tokens": ["Da\u00df", "sich", "bey", "bey\u00b7den", "Acht\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Samt Lust und Andacht, noch vermehrt.", "tokens": ["Samt", "Lust", "und", "An\u00b7dacht", ",", "noch", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es wurde lieblich musiciret:", "tokens": ["Es", "wur\u00b7de", "lieb\u00b7lich", "mu\u00b7si\u00b7ci\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "\u201cdu ewiger Gnaden allm\u00e4chtiger Wille rc. ", "tokens": ["\u201c", "du", "e\u00b7wi\u00b7ger", "Gna\u00b7den", "all\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Wil\u00b7le", "rc", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "ADJA", "NN", "NE", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Da\u00df dieses nun so wohl gelungen,", "tokens": ["Da\u00df", "die\u00b7ses", "nun", "so", "wohl", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Daf\u00fcr sey, grosser Sch\u00f6pfer, Dir", "tokens": ["Da\u00b7f\u00fcr", "sey", ",", "gros\u00b7ser", "Sch\u00f6p\u00b7fer", ",", "Dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "VAFIN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von allen, sonderlich von mir,", "tokens": ["Von", "al\u00b7len", ",", "son\u00b7der\u00b7lich", "von", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lob, Ehre, Preis und Dank gesungen.", "tokens": ["Lob", ",", "Eh\u00b7re", ",", "Preis", "und", "Dank", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, la\u00df uns oft mit Lust dergleichen Feyr begehen!", "tokens": ["Ach", ",", "la\u00df", "uns", "oft", "mit", "Lust", "derg\u00b7lei\u00b7chen", "Feyr", "be\u00b7ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADV", "APPR", "NN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df uns uns oft von Dir gesegnet sehen!", "tokens": ["La\u00df", "uns", "uns", "oft", "von", "Dir", "ge\u00b7seg\u00b7net", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPR", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "La\u00df oft von uns ein Lied, das Dir gef\u00e4llt, erklingen,", "tokens": ["La\u00df", "oft", "von", "uns", "ein", "Lied", ",", "das", "Dir", "ge\u00b7f\u00e4llt", ",", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bis wir dereinsten dort ein ewigs Loblied singen!", "tokens": ["Bis", "wir", "de\u00b7reins\u00b7ten", "dort", "ein", "e\u00b7wigs", "Lob\u00b7lied", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}