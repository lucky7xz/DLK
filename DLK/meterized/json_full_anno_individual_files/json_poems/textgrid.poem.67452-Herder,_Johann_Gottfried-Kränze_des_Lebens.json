{"textgrid.poem.67452": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Kr\u00e4nze des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manche der Kr\u00e4nze sind's, die uns f\u00fcr jegliches Alter", "tokens": ["Man\u00b7che", "der", "Kr\u00e4n\u00b7ze", "sin\u00b7d's", ",", "die", "uns", "f\u00fcr", "jeg\u00b7li\u00b7ches", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und f\u00fcr jede Gefahr lohnend die Gottheit bestimmt.", "tokens": ["Und", "f\u00fcr", "je\u00b7de", "Ge\u00b7fahr", "loh\u00b7nend", "die", "Got\u00b7theit", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVPP", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Lieblich lockt, mit Veilchen im Haar und mit Rosen am Busen,", "tokens": ["Lieb\u00b7lich", "lockt", ",", "mit", "Veil\u00b7chen", "im", "Haar", "und", "mit", "Ro\u00b7sen", "am", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "NN", "APPRART", "NN", "KON", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Uns die Hora den Pfad fr\u00f6hlicher Jugend hinan,", "tokens": ["Uns", "die", "Ho\u00b7ra", "den", "Pfad", "fr\u00f6h\u00b7li\u00b7cher", "Ju\u00b7gend", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART"