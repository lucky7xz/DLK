{"dta.poem.9799": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff des  \n Marggraffen zu Baden Ludwig Wilhelms  \n gl\u00fcckliche landung in Engelland 1694.  \n G. C. K.  \n Teutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje? will mein atlas ietzt von seiner st\u00e4te weichen?", "tokens": ["Wie", "?", "will", "mein", "at\u00b7las", "ietzt", "von", "sei\u00b7ner", "st\u00e4\u00b7te", "wei\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt mich sein helden-arm nicht wie er vor gethan", "tokens": ["H\u00e4lt", "mich", "sein", "hel\u00b7den\u00b7arm", "nicht", "wie", "er", "vor", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "PWAV", "PPER", "APPR", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll dann mein schutz-gestirn schon seinen west erreichen/", "tokens": ["Soll", "dann", "mein", "schutz\u00b7ge\u00b7stirn", "schon", "sei\u00b7nen", "west", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh\u2019 als sein milder strahl mich recht erw\u00e4rmen kan?", "tokens": ["Eh'", "als", "sein", "mil\u00b7der", "strahl", "mich", "recht", "er\u00b7w\u00e4r\u00b7men", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verl\u00e4st mein tapfrer F\u00fcrst mich nun in meinen n\u00f6then/", "tokens": ["Ver\u00b7l\u00e4st", "mein", "tapf\u00b7rer", "F\u00fcrst", "mich", "nun", "in", "mei\u00b7nen", "n\u00f6\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die seine gegenwart nur erst gemindert hat?", "tokens": ["Die", "sei\u00b7ne", "ge\u00b7gen\u00b7wart", "nur", "erst", "ge\u00b7min\u00b7dert", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer wird der feinde macht am Rhein und Neckar t\u00f6dten/", "tokens": ["Wer", "wird", "der", "fein\u00b7de", "macht", "am", "Rhein", "und", "Ne\u00b7ckar", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVFIN", "APPRART", "NE", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann mir ein Fabius entzieht den klugen rath?", "tokens": ["Wann", "mir", "ein", "Fa\u00b7bi\u00b7us", "ent\u00b7zieht", "den", "klu\u00b7gen", "rath", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sein festgesetzter geist eilt zu den k\u00fchnen Britten/", "tokens": ["Sein", "fest\u00b7ge\u00b7setz\u00b7ter", "geist", "eilt", "zu", "den", "k\u00fch\u00b7nen", "Brit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kein sturm noch ungemach hemm\u2019t diesen grossen schlu\u00df/", "tokens": ["Kein", "sturm", "noch", "un\u00b7ge\u00b7mach", "hem\u00b7m't", "die\u00b7sen", "gros\u00b7sen", "schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "VVFIN", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Es scheint/ er hat nunmehr erd\u2019/ feuer/ meer bestritten/", "tokens": ["Es", "scheint", "/", "er", "hat", "nun\u00b7mehr", "erd'", "/", "feu\u00b7er", "/", "meer", "be\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$(", "NN", "$(", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil iedes element ihm dienstbar werden mu\u00df.", "tokens": ["Weil", "ie\u00b7des", "e\u00b7le\u00b7ment", "ihm", "dienst\u00b7bar", "wer\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr v\u00f6lcker Albions/ die muth und tugend zieret/", "tokens": ["Ihr", "v\u00f6l\u00b7cker", "Al\u00b7bions", "/", "die", "muth", "und", "tu\u00b7gend", "zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Nehmt meinen helden doch/ mein kleinod/ wol in acht/", "tokens": ["Nehmt", "mei\u00b7nen", "hel\u00b7den", "doch", "/", "mein", "klei\u00b7nod", "/", "wol", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$(", "PPOSAT", "NN", "$(", "ADV", "APPR", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wisset/ da\u00df mit sich er unsre hertzen f\u00fchret/", "tokens": ["Und", "wis\u00b7set", "/", "da\u00df", "mit", "sich", "er", "uns\u00b7re", "hert\u00b7zen", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "APPR", "PRF", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und vor der Christen heil noch unerm\u00fcdet wacht.", "tokens": ["Und", "vor", "der", "Chris\u00b7ten", "heil", "noch", "un\u00b7er\u00b7m\u00fc\u00b7det", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}