{"textgrid.poem.35707": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "31. Rundgesang f\u00fcr die Treuen des Zirkels", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir trinken, k\u00fchl umschattet,", "tokens": ["Wir", "trin\u00b7ken", ",", "k\u00fchl", "um\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Rebensaft;", "tokens": ["Den", "Re\u00b7ben\u00b7saft", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Seel' an Seele gattet", "tokens": ["Und", "Seel'", "an", "See\u00b7le", "gat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Magnetenkraft!", "tokens": ["Mag\u00b7ne\u00b7ten\u00b7kraft", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Rundum, wie Klett' an Klette,", "tokens": ["Run\u00b7dum", ",", "wie", "Klett'", "an", "Klet\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schlingt fest die Zauberkette!", "tokens": ["Schlingt", "fest", "die", "Zau\u00b7ber\u00b7ket\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach unterm Mond ist mancherlei,", "tokens": ["Ach", "un\u00b7term", "Mond", "ist", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPRART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wovon nichts tr\u00e4umt die Tr\u00e4umerei", "tokens": ["Wo\u00b7von", "nichts", "tr\u00e4umt", "die", "Tr\u00e4u\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Filosofei!", "tokens": ["Fi\u00b7lo\u00b7so\u00b7fei", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Magnetisch braust im Glase", "tokens": ["Mag\u00b7ne\u00b7tisch", "braust", "im", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wein, und perlt,", "tokens": ["Der", "Wein", ",", "und", "perlt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von schwindelnder Ekstase", "tokens": ["Von", "schwin\u00b7deln\u00b7der", "Eks\u00b7ta\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie umgequerlt!", "tokens": ["Wie", "um\u00b7ge\u00b7querlt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schl\u00fcrft ein; und s\u00fc\u00dfer Wirbel", "tokens": ["Schl\u00fcrft", "ein", ";", "und", "s\u00fc\u00b7\u00dfer", "Wir\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durchdr\u00f6hn' uns bis zur Zirbel!", "tokens": ["Durch\u00b7dr\u00f6hn'", "uns", "bis", "zur", "Zir\u00b7bel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach unterm Mond ist mancherlei,", "tokens": ["Ach", "un\u00b7term", "Mond", "ist", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPRART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wovon nichts tr\u00e4umt die Tr\u00e4umerei", "tokens": ["Wo\u00b7von", "nichts", "tr\u00e4umt", "die", "Tr\u00e4u\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Filosofei!", "tokens": ["Fi\u00b7lo\u00b7so\u00b7fei", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Das Wasser selbst macht trunken", "tokens": ["Das", "Was\u00b7ser", "selbst", "macht", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Seligkeit,", "tokens": ["Von", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hat Glaubenshand den Funken", "tokens": ["Hat", "Glau\u00b7bens\u00b7hand", "den", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hineingestreut;", "tokens": ["Hin\u00b7ein\u00b7ge\u00b7streut", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch weiht sie Wein, dann h\u00f6ht er", "tokens": ["Doch", "weiht", "sie", "Wein", ",", "dann", "h\u00f6ht", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Blitz den Geist zum \u00c4ther!", "tokens": ["Wie", "Blitz", "den", "Geist", "zum", "\u00c4\u00b7ther", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach unterm Mond ist mancherlei,", "tokens": ["Ach", "un\u00b7term", "Mond", "ist", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPRART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wovon nichts tr\u00e4umt die Tr\u00e4umerei", "tokens": ["Wo\u00b7von", "nichts", "tr\u00e4umt", "die", "Tr\u00e4u\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Filosofei!", "tokens": ["Fi\u00b7lo\u00b7so\u00b7fei", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Frech magst du schrein und l\u00e4stern,", "tokens": ["Frech", "magst", "du", "schrein", "und", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Atheist!", "tokens": ["Du", "A\u00b7theist", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Trotzt, Br\u00fcder, trotzt, ihr Schwestern,", "tokens": ["Trotzt", ",", "Br\u00fc\u00b7der", ",", "trotzt", ",", "ihr", "Schwes\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Antichrist!", "tokens": ["Dem", "An\u00b7ti\u00b7ch\u00b7rist", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wir hegen Lieb' und Glauben,", "tokens": ["Wir", "he\u00b7gen", "Lieb'", "und", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Einf\u00e4ltig gleich den Tauben!", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "gleich", "den", "Tau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach unterm Mond ist mancherlei,", "tokens": ["Ach", "un\u00b7term", "Mond", "ist", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPRART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wovon nichts tr\u00e4umt die Tr\u00e4umerei", "tokens": ["Wo\u00b7von", "nichts", "tr\u00e4umt", "die", "Tr\u00e4u\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Filosofei!", "tokens": ["Fi\u00b7lo\u00b7so\u00b7fei", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Schm\u00e4ht's immer hyperbolisch,", "tokens": ["Schm\u00e4ht's", "im\u00b7mer", "hy\u00b7per\u00b7bo\u00b7lisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Herrn, und klafft!", "tokens": ["Ihr", "Herrn", ",", "und", "klafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Uns d\u00fcnkt sie apostolisch,", "tokens": ["Uns", "d\u00fcnkt", "sie", "a\u00b7pos\u00b7to\u00b7lisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Wunderkraft!", "tokens": ["Die", "Wun\u00b7der\u00b7kraft", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wir sind, wie echte Beter,", "tokens": ["Wir", "sind", ",", "wie", "ech\u00b7te", "Be\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Demut Wunderth\u00e4ter!", "tokens": ["In", "De\u00b7mut", "Wun\u00b7der\u00b7th\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach unterm Mond ist mancherlei,", "tokens": ["Ach", "un\u00b7term", "Mond", "ist", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPRART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wovon nichts tr\u00e4umt die Tr\u00e4umerei", "tokens": ["Wo\u00b7von", "nichts", "tr\u00e4umt", "die", "Tr\u00e4u\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Filosofei!", "tokens": ["Fi\u00b7lo\u00b7so\u00b7fei", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ihr tr\u00e4umt; wir sehn in Klarheit!", "tokens": ["Ihr", "tr\u00e4umt", ";", "wir", "sehn", "in", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dank, Me\u00dfmer, dir!", "tokens": ["Dank", ",", "Me\u00df\u00b7mer", ",", "dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wir sehn mit Ga\u00dfner Wahrheit,", "tokens": ["Wir", "sehn", "mit", "Ga\u00df\u00b7ner", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und P\u00fcyseg\u00fcr!", "tokens": ["Und", "P\u00fcy\u00b7se\u00b7g\u00fcr", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wir traun auf deine Bude,", "tokens": ["Wir", "traun", "auf", "dei\u00b7ne", "Bu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Cagliostro, ew'ger Jude!", "tokens": ["Cag\u00b7li\u00b7ost\u00b7ro", ",", "ew'\u00b7ger", "Ju\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach unterm Mond ist