{"textgrid.poem.26193": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Dein wei\u00dfes Kleid war wie aus wei\u00dfem Stein", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein wei\u00dfes Kleid war wie aus wei\u00dfem Stein,", "tokens": ["Dein", "wei\u00b7\u00dfes", "Kleid", "war", "wie", "aus", "wei\u00b7\u00dfem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Metallene Augen legte der Mond in dein Gesicht hinein.", "tokens": ["Me\u00b7tal\u00b7le\u00b7ne", "Au\u00b7gen", "leg\u00b7te", "der", "Mond", "in", "dein", "Ge\u00b7sicht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und wenn dein Nacken sich schlank bewegte,", "tokens": ["Und", "wenn", "dein", "Na\u00b7cken", "sich", "schlank", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "War es, als ob der Mond mit H\u00e4nden an dir niedersank", "tokens": ["War", "es", ",", "als", "ob", "der", "Mond", "mit", "H\u00e4n\u00b7den", "an", "dir", "nie\u00b7der\u00b7sank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOKOM", "KOUS", "ART", "NN", "APPR", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Und sich um deine H\u00fcften sehnend legte.", "tokens": ["Und", "sich", "um", "dei\u00b7ne", "H\u00fcf\u00b7ten", "seh\u00b7nend", "leg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du wurdest wie ohne Gef\u00fchl, und wie aus Silber gegossen,", "tokens": ["Du", "wur\u00b7dest", "wie", "oh\u00b7ne", "Ge\u00b7f\u00fchl", ",", "und", "wie", "aus", "Sil\u00b7ber", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "APPR", "NN", "$,", "KON", "PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und lehntest dich k\u00fchl zur\u00fcck in den Mondschein, den gro\u00dfen.", "tokens": ["Und", "lehn\u00b7test", "dich", "k\u00fchl", "zu\u00b7r\u00fcck", "in", "den", "Mond\u00b7schein", ",", "den", "gro\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da ist mir eine Blutwelle steil in den Sch\u00e4del geschossen,", "tokens": ["Da", "ist", "mir", "ei\u00b7ne", "Blut\u00b7wel\u00b7le", "steil", "in", "den", "Sch\u00e4\u00b7del", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und ich h\u00e4tte gern, wie ein Narr, mein 