{"dta.poem.6192": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nun die Erde noch die Sonne n\u00e4her sp\u00fcret,", "tokens": ["Wenn", "nun", "die", "Er\u00b7de", "noch", "die", "Son\u00b7ne", "n\u00e4\u00b7her", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich mit reiffen Aehren zieret;", "tokens": ["Und", "sich", "mit", "reif\u00b7fen", "A\u00b7eh\u00b7ren", "zie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn Ceres, durch der langen Tage Gunst,", "tokens": ["Wenn", "Ce\u00b7res", ",", "durch", "der", "lan\u00b7gen", "Ta\u00b7ge", "Gunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den gelben Schatz der Furchen reiffen sieht;", "tokens": ["Den", "gel\u00b7ben", "Schatz", "der", "Fur\u00b7chen", "reif\u00b7fen", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}