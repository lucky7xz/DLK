{"dta.poem.7701": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich nahm ein frostig Buch und legt' es auf die Flammen:", "tokens": ["Ich", "nahm", "ein", "fros\u00b7tig", "Buch", "und", "legt'", "es", "auf", "die", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch dir im Tode soll noch Lebensglut entstammen.", "tokens": ["Auch", "dir", "im", "To\u00b7de", "soll", "noch", "Le\u00b7bens\u00b7glut", "ent\u00b7stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00e4tter kr\u00fcmmten sich, vom Finger anger\u00fchrt", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "kr\u00fcmm\u00b7ten", "sich", ",", "vom", "Fin\u00b7ger", "an\u00b7ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hei\u00dfer Hand, wie sie nicht leicht ein Leser f\u00fchrt.", "tokens": ["So", "hei\u00b7\u00dfer", "Hand", ",", "wie", "sie", "nicht", "leicht", "ein", "Le\u00b7ser", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAV", "PPER", "PTKNEG", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Umschlug die Flamme voll Begierde Blatt f\u00fcr Blatt,", "tokens": ["Um\u00b7schlug", "die", "Flam\u00b7me", "voll", "Be\u00b7gier\u00b7de", "Blatt", "f\u00fcr", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und las an meiner Statt daran sich satt und matt.", "tokens": ["Und", "las", "an", "mei\u00b7ner", "Statt", "da\u00b7ran", "sich", "satt", "und", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PAV", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann meditirend ist sie dr\u00fcber eingesunken,", "tokens": ["Dann", "me\u00b7di\u00b7ti\u00b7rend", "ist", "sie", "dr\u00fc\u00b7ber", "ein\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und so verloschen schnell bis auf den letzten Funken.", "tokens": ["Und", "so", "ver\u00b7lo\u00b7schen", "schnell", "bis", "auf", "den", "letz\u00b7ten", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Buch hat ihr gedient zu kurzer Unterhaltung,", "tokens": ["Das", "Buch", "hat", "ihr", "ge\u00b7dient", "zu", "kur\u00b7zer", "Un\u00b7ter\u00b7hal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seiner Art gem\u00e4\u00df geendet mit Erkaltung.", "tokens": ["Und", "sei\u00b7ner", "Art", "ge\u00b7m\u00e4\u00df", "ge\u00b7en\u00b7det", "mit", "Er\u00b7kal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}