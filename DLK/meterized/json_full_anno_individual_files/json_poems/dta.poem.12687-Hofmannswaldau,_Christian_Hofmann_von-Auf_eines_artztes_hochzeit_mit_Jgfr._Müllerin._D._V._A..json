{"dta.poem.12687": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eines artztes hochzeit mit  \n Jgfr. M\u00fcllerin.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der purpur, seine zier, becr\u00f6nt ihn bey der see.", "tokens": ["Der", "pur\u00b7pur", ",", "sei\u00b7ne", "zier", ",", "be\u00b7cr\u00f6nt", "ihn", "bey", "der", "see", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Venus sahe zu, die aus dem meer ist kommen,", "tokens": ["Die", "Ve\u00b7nus", "sa\u00b7he", "zu", ",", "die", "aus", "dem", "meer", "ist", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie sagte: weil er mir mein schnecken-blut genommen.", "tokens": ["Sie", "sag\u00b7te", ":", "weil", "er", "mir", "mein", "schne\u00b7cken\u00b7blut", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So will ich, da\u00df er stets zum wasser mahlen geh.", "tokens": ["So", "will", "ich", ",", "da\u00df", "er", "stets", "zum", "was\u00b7ser", "mah\u00b7len", "geh."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}