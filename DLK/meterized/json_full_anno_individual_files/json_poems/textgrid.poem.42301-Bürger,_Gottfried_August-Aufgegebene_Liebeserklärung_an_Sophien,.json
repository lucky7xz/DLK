{"textgrid.poem.42301": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Aufgegebene Liebeserkl\u00e4rung an Sophien,", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Herzen, wie am Geist, l\u00e4ngst dumpf, und stumpf, wie \u2013 ", "tokens": ["Am", "Her\u00b7zen", ",", "wie", "am", "Geist", ",", "l\u00e4ngst", "dumpf", ",", "und", "stumpf", ",", "wie", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "ADV", "ADJD", "$,", "KON", "ADJD", "$,", "PWAV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4hnt' ich \u2013 ein schlechtes Ziel! \u2013 vor Amors Pfeil mich \u2013 ", "tokens": ["W\u00e4hnt'", "ich", "\u2013", "ein", "schlech\u00b7tes", "Ziel", "!", "\u2013", "vor", "A\u00b7mors", "Pfeil", "mich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ART", "ADJA", "NN", "$.", "$(", "APPR", "NE", "NN", "PPER", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bekannt mit meinem Wert, an Leib und Seele \u2013", "tokens": ["Be\u00b7kannt", "mit", "mei\u00b7nem", "Wert", ",", "an", "Leib", "und", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fri\u00dft, dacht' ich, wie ich bin, mich weder Hund noch \u2013 ", "tokens": ["Fri\u00dft", ",", "dacht'", "ich", ",", "wie", "ich", "bin", ",", "mich", "we\u00b7der", "Hund", "noch", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,", "PPER", "KON", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich w\u00fcrgt' an Vers und Reim, als steckt' im Hals ein \u2013 ", "tokens": ["Ich", "w\u00fcrgt'", "an", "Vers", "und", "Reim", ",", "als", "steckt'", "im", "Hals", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "KOUS", "VVFIN", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und langsam schlich mein Witz, wie Aarons S\u00fcnden- \u2013 ", "tokens": ["Und", "lang\u00b7sam", "schlich", "mein", "Witz", ",", "wie", "Aa\u00b7rons", "S\u00fcn\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "NE", "TRUNC", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da, Fiekchen, tratst du auf, an Kraft ein Lebens- \u2013", "tokens": ["Da", ",", "Fiek\u00b7chen", ",", "tratst", "du", "auf", ",", "an", "Kraft", "ein", "Le\u00b7bens", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "ART", "TRUNC", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bewegtest zum Bimbam der Zunge tr\u00e4gen \u2013", "tokens": ["Be\u00b7weg\u00b7test", "zum", "Bim\u00b7bam", "der", "Zun\u00b7ge", "tr\u00e4\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun, d\u00e4ucht mir, komm' ich fa\u00dft von neuem in den \u2013 ", "tokens": ["Nun", ",", "d\u00e4ucht", "mir", ",", "komm'", "ich", "fa\u00dft", "von", "neu\u00b7em", "in", "den", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "APPR", "ART", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ganz fra\u00df vielleicht der Wurm mich nicht zur tauben \u2013 ", "tokens": ["Ganz", "fra\u00df", "viel\u00b7leicht", "der", "Wurm", "mich", "nicht", "zur", "tau\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKNEG", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ha! tr\u00e4nktest du mich nun mit deiner Liebe \u2013", "tokens": ["Ha", "!", "tr\u00e4nk\u00b7test", "du", "mich", "nun", "mit", "dei\u00b7ner", "Lie\u00b7be", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So lernt' ich dein Apport noch wie der j\u00fcngste \u2013", "tokens": ["So", "lernt'", "ich", "dein", "Ap\u00b7port", "noch", "wie", "der", "j\u00fcngs\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "KOKOM", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dir spr\u00e4ng' ich \u00fcbern Stock und tanzt' im bunten \u2013", "tokens": ["Dir", 