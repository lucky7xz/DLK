{"textgrid.poem.44575": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Jahr hat unser Weihnachtsfest geruht,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Jahr hat unser Weihnachtsfest geruht,", "tokens": ["Ein", "Jahr", "hat", "un\u00b7ser", "Weih\u00b7nachts\u00b7fest", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Christbaum konnte hier, wie heute stehen;", "tokens": ["Kein", "Christ\u00b7baum", "konn\u00b7te", "hier", ",", "wie", "heu\u00b7te", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "$,", "PWAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Jahr von Not, von Jammer, ja von Blut,", "tokens": ["Ein", "Jahr", "von", "Not", ",", "von", "Jam\u00b7mer", ",", "ja", "von", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ihr geh\u00f6rt und selbst zum Teil gesehen.", "tokens": ["Wie", "ihr", "ge\u00b7h\u00f6rt", "und", "selbst", "zum", "Teil", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Sturmwind ging durch alle L\u00e4nder wild,", "tokens": ["Ein", "Sturm\u00b7wind", "ging", "durch", "al\u00b7le", "L\u00e4n\u00b7der", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf alles, was da recht und fest, erbittert,", "tokens": ["Auf", "al\u00b7les", ",", "was", "da", "recht", "und", "fest", ",", "er\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "KON", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst unsre Stadt, sonst heitrer G\u00fcte Bild,", "tokens": ["Selbst", "uns\u00b7re", "Stadt", ",", "sonst", "hei\u00b7trer", "G\u00fc\u00b7te", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sah sich in ihrem alten Wert ersch\u00fcttert.", "tokens": ["Sah", "sich", "in", "ih\u00b7rem", "al\u00b7ten", "Wert", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das macht: nicht ihr seid Kinder nur allein,", "tokens": ["Das", "macht", ":", "nicht", "ihr", "seid", "Kin\u00b7der", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PTKNEG", "PPER", "VAFIN", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch alle Menschen, sie sind Gottes Kinder,", "tokens": ["Auch", "al\u00b7le", "Men\u00b7schen", ",", "sie", "sind", "Got\u00b7tes", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PPER", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie man euch ermahnt, die ihr noch klein,", "tokens": ["Und", "wie", "man", "euch", "er\u00b7mahnt", ",", "die", "ihr", "noch", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mahnt er jene \u00c4lteren nicht minder.", "tokens": ["So", "mahnt", "er", "je\u00b7ne", "\u00c4l\u00b7te\u00b7ren", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er hat in jedes Menschen Brust gesenkt", "tokens": ["Er", "hat", "in", "je\u00b7des", "Men\u00b7schen", "Brust", "ge\u00b7senkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Warnerstimmen, die das Rechte k\u00fcnden,", "tokens": ["Die", "War\u00b7ner\u00b7stim\u00b7men", ",", "die", "das", "Rech\u00b7te", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was der Mensch ersinnt und kl\u00fcgelnd denkt,", "tokens": ["Und", "was", "der", "Mensch", "er\u00b7sinnt", "und", "kl\u00fc\u00b7gelnd", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kann sie ersetzen nicht und nicht ergr\u00fcnden.", "tokens": ["Kann", "sie", "er\u00b7set\u00b7zen", "nicht", "und", "nicht", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Recht, es ist; das Gute will, was gut,", "tokens": ["Das", "Recht", ",", "es", "ist", ";", "das", "Gu\u00b7te", "will", ",", "was", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "$.", "ART", "NN", "VMFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Liebe lebt in jedes Menschen Herzen;", "tokens": ["Die", "Lie\u00b7be", "lebt", "in", "je\u00b7des", "Men\u00b7schen", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Dank erzeugt des Wohltuns edler Mut,", "tokens": ["Den", "Dank", "er\u00b7zeugt", "des", "Wohl\u00b7tuns", "ed\u00b7ler", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Mitleid f\u00fchlt, gleich eignen, fremde Schmerzen.", "tokens": ["Das", "Mit\u00b7leid", "f\u00fchlt", ",", "gleich", "eig\u00b7nen", ",", "frem\u00b7de", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Solang der Mensch nun aufhorcht fromm und still", "tokens": ["So\u00b7lang", "der", "Mensch", "nun", "auf\u00b7horcht", "fromm", "und", "still"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf jene Stimmen, die im Innern mahnen,", "tokens": ["Auf", "je\u00b7ne", "Stim\u00b7men", ",", "die", "im", "In\u00b7nern", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was er gl\u00e4ubig h\u00f6rt, vertrauend will,", "tokens": ["Und", "was", "er", "gl\u00e4u\u00b7big", "h\u00f6rt", ",", "ver\u00b7trau\u00b7end", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$,", "VVPP", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geht er einher auf Gottes lichten Bahnen.", "tokens": ["Geht", "er", "ein\u00b7her", "auf", "Got\u00b7tes", "lich\u00b7ten", "Bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Doch wird er stolz und l\u00e4rmt und spricht und schreibt,", "tokens": ["Doch", "wird", "er", "stolz", "und", "l\u00e4rmt", "und", "spricht", "und", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt nicht mehr auf die leisen Gottesstimmen,", "tokens": ["H\u00f6rt", "nicht", "mehr", "auf", "die", "lei\u00b7sen", "Got\u00b7tes\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann schweigen sie, vom L\u00e4rmen \u00fcbert\u00e4ubt,", "tokens": ["Dann", "schwei\u00b7gen", "sie", ",", "vom", "L\u00e4r\u00b7men", "\u00fc\u00b7bert\u00b7\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und machen Platz dem Schlechten und dem Schlimmen.", "tokens": ["Und", "ma\u00b7chen", "Platz", "dem", "Schlech\u00b7ten", "und", "dem", "Schlim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So wars mit uns im j\u00fcngst verflo\u00dfnen Jahr,", "tokens": ["So", "wars", "mit", "uns", "im", "j\u00fcngst", "ver\u00b7flo\u00df\u00b7nen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "APPRART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo unser Heil in wilden Wahnsinns H\u00e4nden,", "tokens": ["Wo", "un\u00b7ser", "Heil", "in", "wil\u00b7den", "Wahn\u00b7sinns", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo jedem nur der eigne D\u00fcnkel wahr,", "tokens": ["Wo", "je\u00b7dem", "nur", "der", "eig\u00b7ne", "D\u00fcn\u00b7kel", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleichlaut in allen Schichten, allen St\u00e4nden.", "tokens": ["Gleich\u00b7laut", "in", "al\u00b7len", "Schich\u00b7ten", ",", "al\u00b7len", "St\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ein einzger Stand fiel nicht vom Rechten ab,", "tokens": ["Ein", "einz\u00b7ger", "Stand", "fiel", "nicht", "vom", "Rech\u00b7ten", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ward nicht an sich und andern zum Verr\u00e4ter,", "tokens": ["Ward", "nicht", "an", "sich", "und", "an\u00b7dern", "zum", "Ver\u00b7r\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PRF", "KON", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war der Stand, der euch das Leben gab,", "tokens": ["Es", "war", "der", "Stand", ",", "der", "euch", "das", "Le\u00b7ben", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Stand, Soldatenkinder! eurer V\u00e4ter.", "tokens": ["Der", "Stand", ",", "Sol\u00b7da\u00b7ten\u00b7kin\u00b7der", "!", "eu\u00b7rer", "V\u00e4\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Es war das Heer. Die einzigen, die fest,", "tokens": ["Es", "war", "das", "Heer", ".", "Die", "ein\u00b7zi\u00b7gen", ",", "die", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Volk und Staat im Taumel fast vernichtet,", "tokens": ["Als", "Volk", "und", "Staat", "im", "Tau\u00b7mel", "fast", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil sie verlie\u00dfen nicht, was nicht verl\u00e4\u00dft,", "tokens": ["Weil", "sie", "ver\u00b7lie\u00b7\u00dfen", "nicht", ",", "was", "nicht", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Gottesstimme, die im Herzen richtet;", "tokens": ["Die", "Got\u00b7tes\u00b7stim\u00b7me", ",", "die", "im", "Her\u00b7zen", "rich\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Weil Ehrfurcht und Gehorsam und die Pflicht,", "tokens": ["Weil", "Ehr\u00b7furcht", "und", "Ge\u00b7hor\u00b7sam", "und", "die", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vertrauen in die Einsicht der Bew\u00e4hrten,", "tokens": ["Ver\u00b7trau\u00b7en", "in", "die", "Ein\u00b7sicht", "der", "Be\u00b7w\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Erstorben noch in ihrem Busen nicht,", "tokens": ["E\u00b7rstor\u00b7ben", "noch", "in", "ih\u00b7rem", "Bu\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allm\u00e4chtig durch die Eintracht der Gef\u00e4hrten.", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "durch", "die", "Ein\u00b7tracht", "der", "Ge\u00b7f\u00e4hr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.12": {"line.1": {"text": "So schritten sie auf rauhen Bahnen fort,", "tokens": ["So", "schrit\u00b7ten", "sie", "auf", "rau\u00b7hen", "Bah\u00b7nen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr 