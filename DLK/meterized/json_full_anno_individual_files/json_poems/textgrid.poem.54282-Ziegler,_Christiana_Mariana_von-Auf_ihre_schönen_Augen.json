{"textgrid.poem.54282": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre sch\u00f6nen Augen", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So offt ein K\u00fcnstler euch zu schiltern sich bem\u00fcht,", "tokens": ["So", "offt", "ein", "K\u00fcnst\u00b7ler", "euch", "zu", "schil\u00b7tern", "sich", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "PTKZU", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So trifft er euch doch nicht, wie man gar \u00f6ffters sieht.", "tokens": ["So", "trifft", "er", "euch", "doch", "nicht", ",", "wie", "man", "gar", "\u00f6ff\u00b7ters", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch ist nicht seiner Faust der Fehler beyzumessen,", "tokens": ["Doch", "ist", "nicht", "sei\u00b7ner", "Faust", "der", "Feh\u00b7ler", "bey\u00b7zu\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er die Aehnlichkeit von euch darbey vergessen,", "tokens": ["Wenn", "er", "die", "A\u00b7ehn\u00b7lich\u00b7keit", "von", "euch", "dar\u00b7bey", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Und lebt Apelles noch, so k\u00f6nt es nicht geschehn,", "tokens": ["Und", "lebt", "A\u00b7pel\u00b7les", "noch", ",", "so", "k\u00f6nt", "es", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn Adler k\u00f6nnen nur blo\u00df in die Sonne sehn.", "tokens": ["Denn", "Ad\u00b7ler", "k\u00f6n\u00b7nen", "nur", "blo\u00df", "in", "die", "Son\u00b7ne", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Strahl, der in euch sitzt, steht gar nicht abzureissen,", "tokens": ["Der", "Strahl", ",", "der", "in", "euch", "sitzt", ",", "steht", "gar", "nicht", "ab\u00b7zu\u00b7reis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PTKNEG", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Mahler m\u00fcste denn ein Halb-Gott w\u00fcrcklich heissen.", "tokens": ["Der", "Mah\u00b7ler", "m\u00fcs\u00b7te", "denn", "ein", "Halb\u00b7Gott", "w\u00fcrck\u00b7lich", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"t