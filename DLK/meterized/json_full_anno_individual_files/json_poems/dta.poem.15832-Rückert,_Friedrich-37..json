{"dta.poem.15832": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In diesen Tagen, da mir manch Gedicht gelungen,", "tokens": ["In", "die\u00b7sen", "Ta\u00b7gen", ",", "da", "mir", "manch", "Ge\u00b7dicht", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat sich ein eigenes Gef\u00fchl mir aufgedrungen:", "tokens": ["Hat", "sich", "ein", "ei\u00b7ge\u00b7nes", "Ge\u00b7f\u00fchl", "mir", "auf\u00b7ge\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df alles, was bisher ich machte, sei ein Spiel,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "bis\u00b7her", "ich", "mach\u00b7te", ",", "sei", "ein", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Vorspiel, dem bevor noch steh' ein andres Ziel;", "tokens": ["Ein", "Vor\u00b7spiel", ",", "dem", "be\u00b7vor", "noch", "steh'", "ein", "and\u00b7res", "Ziel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und alle Uebung, die ich spielend mir errang,", "tokens": ["Und", "al\u00b7le", "Ue\u00b7bung", ",", "die", "ich", "spie\u00b7lend", "mir", "er\u00b7rang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sollt' angewendet seyn auf diesen ernsten Gang.", "tokens": ["Sollt'", "an\u00b7ge\u00b7wen\u00b7det", "seyn", "auf", "die\u00b7sen", "erns\u00b7ten", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VAINF", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}