{"dta.poem.21668": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wir ritten singend hin im gr&#252;nen Walde,                 ", "tokens": ["Wir", "rit\u00b7ten", "sin\u00b7gend", "hin", "im", "gr", "&#252;", "nen", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schneeigen Spitzen nickten fern her\u00fcber", "tokens": ["Die", "schne\u00b7e\u00b7i\u00b7gen", "Spit\u00b7zen", "nick\u00b7ten", "fern", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Alpengl\u00fchn. Es d\u00e4mmert tr\u00fcb und tr\u00fcber.", "tokens": ["Im", "Al\u00b7pen\u00b7gl\u00fchn", ".", "Es", "d\u00e4m\u00b7mert", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom fernen F\u00f6hn erbebt die Bergeshalde.", "tokens": ["Vom", "fer\u00b7nen", "F\u00f6hn", "er\u00b7bebt", "die", "Ber\u00b7ges\u00b7hal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Espe schaudert, fl\u00fcsternd klagt das Blatt,", "tokens": ["Die", "Es\u00b7pe", "schau\u00b7dert", ",", "fl\u00fcs\u00b7ternd", "klagt", "das", "Blatt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Weide ahnt den Sturm, der sie zerknickt:", "tokens": ["Die", "Wei\u00b7de", "ahnt", "den", "Sturm", ",", "der", "sie", "zer\u00b7knickt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie senkt die Zweige siech und kummersatt.", "tokens": ["Sie", "senkt", "die", "Zwei\u00b7ge", "siech", "und", "kum\u00b7mer\u00b7satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Selbst die Cikade summt nur tr\u00e4g und matt:", "tokens": ["Selbst", "die", "Ci\u00b7ka\u00b7de", "summt", "nur", "tr\u00e4g", "und", "matt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.5": {"text": "Natur in sich zur\u00fcckeschrickt.", "tokens": ["Na\u00b7tur", "in", "sich", "zu\u00b7r\u00fc\u00b7cke\u00b7schrickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur einmal, Holde, hab\u2019 ich dich gesehen,", "tokens": ["Nur", "ein\u00b7mal", ",", "Hol\u00b7de", ",", "hab'", "ich", "dich", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch werde nimmer deinen Reiz vergessen.", "tokens": ["Doch", "wer\u00b7de", "nim\u00b7mer", "dei\u00b7nen", "Reiz", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verklungen ist der Sang. Ich mu\u00df durchmessen", "tokens": ["Ver\u00b7klun\u00b7gen", "ist", "der", "Sang", ".", "Ich", "mu\u00df", "durch\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den rauhen Wald des Lebens und es wehen", "tokens": ["Den", "rau\u00b7hen", "Wald", "des", "Le\u00b7bens", "und", "es", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Herbstbl\u00e4tter nun im Sturme um mich her.", "tokens": ["Herbst\u00b7bl\u00e4t\u00b7ter", "nun", "im", "Stur\u00b7me", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach, die Erinnerung als Alpengl\u00fchen", "tokens": ["Ach", ",", "die", "E\u00b7rin\u00b7ne\u00b7rung", "als", "Al\u00b7pen\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Flammt hinter mir in stiller Nacht nicht mehr.", "tokens": ["Flammt", "hin\u00b7ter", "mir", "in", "stil\u00b7ler", "Nacht", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des Herzens Flammen allgemach verspr\u00fchen", "tokens": ["Des", "Her\u00b7zens", "Flam\u00b7men", "all\u00b7ge\u00b7mach", "ver\u00b7spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein Bild nur leuchtet wie ein letzter Funken", "tokens": ["Dein", "Bild", "nur", "leuch\u00b7tet", "wie", "ein", "letz\u00b7ter", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von h\u00f6chster Alpe der Vergangenheit,", "tokens": ["Von", "h\u00f6chs\u00b7ter", "Al\u00b7pe", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Bis auch dies Alpengl\u00fchen matt versunken", "tokens": ["Bis", "auch", "dies", "Al\u00b7pen\u00b7gl\u00fc\u00b7hen", "matt", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PDS", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tief in den Schluchten der Vergessenheit.", "tokens": ["Tief", "in", "den", "Schluch\u00b7ten", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Gelassen reit\u2019 ich durch die Felsenflur \u2014", "tokens": ["Ge\u00b7las\u00b7sen", "reit'", "ich", "durch", "die", "Fel\u00b7sen\u00b7flur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bange nicht dem Sturm, wie die Natur.", "tokens": ["Ich", "ban\u00b7ge", "nicht", "dem", "Sturm", ",", "wie", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn auch kein Licht mein Auge mehr erblickt \u2014", "tokens": ["Wenn", "auch", "kein", "Licht", "mein", "Au\u00b7ge", "mehr", "er\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein todtes Herz vor Nichts erschrickt.", "tokens": ["Ein", "tod\u00b7tes", "Herz", "vor", "Nichts", "er\u00b7schrickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}