{"textgrid.poem.41942": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Entfaltet l\u00e4\u00dft die weite Flur sich sehn", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entfaltet l\u00e4\u00dft die weite Flur sich sehn", "tokens": ["Ent\u00b7fal\u00b7tet", "l\u00e4\u00dft", "die", "wei\u00b7te", "Flur", "sich", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "ART", "ADJA", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und zeigt mir fern den Weg, den ich gegangen,", "tokens": ["Und", "zeigt", "mir", "fern", "den", "Weg", ",", "den", "ich", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ahnungsvoll, mit dunkelm Arm umfangen,", "tokens": ["Doch", "ah\u00b7nungs\u00b7voll", ",", "mit", "dun\u00b7kelm", "Arm", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den k\u00fcnft'gen Pfad, die wald'gen Bergesh\u00f6hn.", "tokens": ["Den", "k\u00fcnft'\u00b7gen", "Pfad", ",", "die", "wald'\u00b7gen", "Ber\u00b7ges\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Luft ist blau und leichte Winde wehn,", "tokens": ["Die", "Luft", "ist", "blau", "und", "leich\u00b7te", "Win\u00b7de", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein frischer Duft ums\u00e4uselt meine Wangen,", "tokens": ["Ein", "fri\u00b7scher", "Duft", "um\u00b7s\u00e4u\u00b7selt", "mei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fort zieht es mich mit brennendem Verlangen,", "tokens": ["Fort", "zieht", "es", "mich", "mit", "bren\u00b7nen\u00b7dem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf neuer Bahn zum neuen Ziel zu gehn.", "tokens": ["Auf", "neu\u00b7er", "Bahn", "zum", "neu\u00b7en", "Ziel", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit Thr\u00e4nen h\u00e4ngt mein Auge jetzt am Sch\u00f6nen;", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "h\u00e4ngt", "mein", "Au\u00b7ge", "jetzt", "am", "Sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die s\u00fc\u00dfe Ruh' erregt mir wildes Sehnen;", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Ruh'", "er\u00b7regt", "mir", "wil\u00b7des", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was sonst ihn stillte, weckt mir jetzt den Schmerz.", "tokens": ["Was", "sonst", "ihn", "still\u00b7te", ",", "weckt", "mir", "jetzt", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum mu\u00df ich rasch in's Thal hernieder eilen;", "tokens": ["Drum", "mu\u00df", "ich", "rasch", "in's", "Thal", "her\u00b7nie\u00b7der", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag dort die Lust, mag dort der Kummer weilen;", "tokens": ["Mag", "dort", "die", "Lust", ",", "mag", "dort", "der", "Kum\u00b7mer", "wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$,", "VMFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur immer fort, du hartbedr\u00e4ngtes Herz!", "tokens": ["Nur", "im\u00b7mer", "fort", ",", "du", "hart\u00b7be\u00b7dr\u00e4ng\u00b7tes", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Entfaltet l\u00e4\u00dft die weite Flur sich sehn", "tokens": ["Ent\u00b7fal\u00b7tet", "l\u00e4\u00dft", "die", "wei\u00b7te", "Flur"