{"dta.poem.5819": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der bewegte C\u00f6rper nun", "tokens": ["Wenn", "der", "be\u00b7weg\u00b7te", "C\u00f6r\u00b7per", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Fahrt mehr Hinderni\u00df versp\u00fcret;", "tokens": ["In", "sei\u00b7ner", "Fahrt", "mehr", "Hin\u00b7der\u00b7ni\u00df", "ver\u00b7sp\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird er viel eher ruhn,", "tokens": ["Wird", "er", "viel", "e\u00b7her", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da nemlich sich sein starcker Trieb verlieret.", "tokens": ["Da", "nem\u00b7lich", "sich", "sein", "star\u00b7cker", "Trieb", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So wird aus einem St\u00fcck die Kugel in der Erden,", "tokens": ["So", "wird", "aus", "ei\u00b7nem", "St\u00fcck", "die", "Ku\u00b7gel", "in", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn sie dieselbe trifft, sehr schnell gehemmet werden,", "tokens": ["Wenn", "sie", "die\u00b7sel\u00b7be", "trifft", ",", "sehr", "schnell", "ge\u00b7hem\u00b7met", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die, wenn sie in der Lufft, die weich, ohn Einhalt fl\u00f6ge;", "tokens": ["Die", ",", "wenn", "sie", "in", "der", "Lufft", ",", "die", "weich", ",", "ohn", "Ein\u00b7halt", "fl\u00f6\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "KOUI", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von ihrem Flug die Strich viel weit- und l\u00e4nger z\u00f6ge.", "tokens": ["Von", "ih\u00b7rem", "Flug", "die", "Strich", "viel", "weit", "und", "l\u00e4n\u00b7ger", "z\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "TRUNC", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}