{"dta.poem.8341": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die kurze Lebens-Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist sehr kurz, wenn man\ndie Jahre z\u00e4hlt,", "tokens": ["Das", "Le\u00b7ben", "ist", "sehr", "kurz", ",", "wenn", "man", "die", "Jah\u00b7re", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unsers Sch\u00f6pfers Schlus,", "tokens": ["Die", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Schlus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bald verschwinden nicht, die", "tokens": ["Wie", "bald", "ver\u00b7schwin\u00b7den", "nicht", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PTKNEG", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schatten-gleichen Stunden,", "tokens": ["Schat\u00b7ten\u00b7glei\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die wenn man sie ermi\u00dft nur fl\u00fcchtige Secunden!", "tokens": ["Die", "wenn", "man", "sie", "er\u00b7mi\u00dft", "nur", "fl\u00fcch\u00b7ti\u00b7ge", "Se\u00b7cun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PIS", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch k\u00fcrzer aber wird die Zeit, wenn man erwegt,", "tokens": ["Noch", "k\u00fcr\u00b7zer", "a\u00b7ber", "wird", "die", "Zeit", ",", "wenn", "man", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie sie in Lebenslauf mit uns zu gehen pflegt:", "tokens": ["Wie", "sie", "in", "Le\u00b7bens\u00b7lauf", "mit", "uns", "zu", "ge\u00b7hen", "pflegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Kindheit Jahre fliehn, da man im Traume", "tokens": ["Der", "Kind\u00b7heit", "Jah\u00b7re", "fliehn", ",", "da", "man", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "KOUS", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Unempfindlichkeit uns stets zum Schlaf ein-", "tokens": ["Die", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", "uns", "stets", "zum", "Schlaf", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein Theil geht also ab, da man nicht einmahl", "tokens": ["Ein", "Theil", "geht", "al\u00b7so", "ab", ",", "da", "man", "nicht", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PIS", "PTKNEG", "ADV"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Warum man Odem sch\u00f6pft, was unsers ", "tokens": ["Wa\u00b7rum", "man", "O\u00b7dem", "sch\u00f6pft", ",", "was", "un\u00b7sers"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Jugend kommt heran, da wir zuerst aufwa-", "tokens": ["Die", "Ju\u00b7gend", "kommt", "he\u00b7ran", ",", "da", "wir", "zu\u00b7erst", "auf\u00b7wa"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.13": {"text": "In etwas uns bem\u00fchn ein k\u00fcnfftig Gl\u00fck zu machen,", "tokens": ["In", "et\u00b7was", "uns", "be\u00b7m\u00fchn", "ein", "k\u00fcnff\u00b7tig", "Gl\u00fck", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "VVFIN", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Zeit wird abgek\u00fcrzt, durch M\u00fc\u00dfigkeit und", "tokens": ["Die", "Zeit", "wird", "ab\u00b7ge\u00b7k\u00fcrzt", ",", "durch", "M\u00fc\u00b7\u00dfig\u00b7keit", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Durch kindisches Gew\u00e4sch, durch nichtiges Ge-", "tokens": ["Durch", "kin\u00b7di\u00b7sches", "Ge\u00b7w\u00e4sch", ",", "durch", "nich\u00b7ti\u00b7ges", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die Jahre kommen an, wo der Verstand gereiffet,", "tokens": ["Die", "Jah\u00b7re", "kom\u00b7men", "an", ",", "wo", "der", "Ver\u00b7stand", "ge\u00b7reif\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da man noch nichts gethan, und sie doch schon ge-", "tokens": ["Da", "man", "noch", "nichts", "ge\u00b7than", ",", "und", "sie", "doch", "schon", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PIS", "VVPP", "$,", "KON", "PPER", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da reget sich der Fleis, so lange man noch bl\u00fcht", "tokens": ["Da", "re\u00b7get", "sich", "der", "Fleis", ",", "so", "lan\u00b7ge", "man", "noch", "bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "ADV", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch wenn man abermahl die Schlaffeszeit ab-", "tokens": ["Doch", "wenn", "man", "a\u00b7ber\u00b7mahl", "die", "Schlaf\u00b7fes\u00b7zeit", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "So geht die Helffte ab, und was vor Arbeits-", "tokens": ["So", "geht", "die", "Helff\u00b7te", "ab", ",", "und", "was", "vor", "Ar\u00b7beits"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "PWS", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Sind nicht in M\u00fc\u00dfigkeit, in fauler Ruh ver-", "tokens": ["Sind", "nicht", "in", "M\u00fc\u00b7\u00dfig\u00b7keit", ",", "in", "fau\u00b7ler", "Ruh", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Zieht man die wieder ab; so ist die Lebenszeit,", "tokens": ["Zieht", "man", "die", "wie\u00b7der", "ab", ";", "so", "ist", "die", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Schon wiederum verk\u00fcrzt, die ", "tokens": ["Schon", "wie\u00b7de\u00b7rum", "ver\u00b7k\u00fcrzt", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVPP", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Des Alters Mattigkeit untauglich zum Gewerben,", "tokens": ["Des", "Al\u00b7ters", "Mat\u00b7tig\u00b7keit", "un\u00b7taug\u00b7lich", "zum", "Ge\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.25": {"text": "Lehrt da\u00df die Alten schon, eh sie noch w\u00fcrklich ster-", "tokens": ["Lehrt", "da\u00df", "die", "Al\u00b7ten", "schon", ",", "eh", "sie", "noch", "w\u00fcrk\u00b7lich", "ster"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJD", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Schon wie erstorben sind: Darum ist sonder Streit,", "tokens": ["Schon", "wie", "ers\u00b7tor\u00b7ben", "sind", ":", "Da\u00b7rum", "ist", "son\u00b7der", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVPP", "VAFIN", "$.", "PAV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Noch k\u00fcrzer als man meint, die kurze Lebens-Zeit:", "tokens": ["Noch", "k\u00fcr\u00b7zer", "als", "man", "meint", ",", "die", "kur\u00b7ze", "Le\u00b7bens\u00b7Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn man dies recht bedenkt, mu\u00df man die Zeit", "tokens": ["Wenn", "man", "dies", "recht", "be\u00b7denkt", ",", "mu\u00df", "man", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PDS", "ADJD", "VVFIN", "$,", "VMFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Weil Jahre, Monath, Tag und Stunden bald ver-", "tokens": ["Weil", "Jah\u00b7re", ",", "Mo\u00b7nath", ",", "Tag", "und", "Stun\u00b7den", "bald", "ver"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}