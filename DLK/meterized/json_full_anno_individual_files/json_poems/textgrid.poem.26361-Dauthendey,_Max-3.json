{"textgrid.poem.26361": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hohe graue Buchen ragen, tragen hohes graues Schweigen.", "tokens": ["Ho\u00b7he", "grau\u00b7e", "Bu\u00b7chen", "ra\u00b7gen", ",", "tra\u00b7gen", "ho\u00b7hes", "grau\u00b7es", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Letzte gelbe Abendstreifen legen feuergoldne Reifen um die d\u00e4mmerdunklen St\u00e4mme.", "tokens": ["Letz\u00b7te", "gel\u00b7be", "A\u00b7bend\u00b7strei\u00b7fen", "le\u00b7gen", "feu\u00b7er\u00b7gold\u00b7ne", "Rei\u00b7fen", "um", "die", "d\u00e4m\u00b7mer\u00b7dunk\u00b7len", "St\u00e4m\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hart von roten toten Bl\u00e4ttern starrt der Boden.", "tokens": ["Hart", "von", "ro\u00b7ten", "to\u00b7ten", "Bl\u00e4t\u00b7tern", "starrt", "der", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "In dem schattenfeuchten Wald leuchten Leiber, nackte Glieder,", "tokens": ["In", "dem", "schat\u00b7ten\u00b7feuch\u00b7ten", "Wald", "leuch\u00b7ten", "Lei\u00b7ber", ",", "nack\u00b7te", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Matt die M\u00e4nner, matt die Frauen liegen nieder, in die Bl\u00e4tter eingescharrt,", "tokens": ["Matt", "die", "M\u00e4n\u00b7ner", ",", "matt", "die", "Frau\u00b7en", "lie\u00b7gen", "nie\u00b7der", ",", "in", "die", "Bl\u00e4t\u00b7ter", "ein\u00b7ge\u00b7scharrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUI", "ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ausgesch\u00fcttet funkelt Haar rot und dunkel.", "tokens": ["Aus\u00b7ge\u00b7sch\u00fct\u00b7tet", "fun\u00b7kelt", "Haar", "rot", "und", "dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Braune m\u00fcde M\u00e4nnernacken schmiegen sich an Frauenbr\u00fcste.", "tokens": ["Brau\u00b7ne", "m\u00fc\u00b7de", "M\u00e4n\u00b7ner\u00b7na\u00b7cken", "schmie\u00b7gen", "sich", "an", "Frau\u00b7en\u00b7br\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "L\u00fcsteweich Frauenaugen. M\u00e4nner, grau, bleich von Fl\u00fcchen,", "tokens": ["L\u00fcs\u00b7te\u00b7weich", "Frau\u00b7en\u00b7au\u00b7gen", ".", "M\u00e4n\u00b7ner", ",", "grau", ",", "bleich", "von", "Fl\u00fc\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$,", "ADJD", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Falten in die Stirn gehauen, fest die Keulen in den F\u00e4usten,", "tokens": ["Fal\u00b7ten", "in", "die", "Stirn", "ge\u00b7hau\u00b7en", ",", "fest", "die", "Keu\u00b7len", "in", "den", "F\u00e4us\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Z\u00e4hne bei\u00dfen in das beulenschwere Holz. \u2013", "tokens": ["Z\u00e4h\u00b7ne", "bei\u00b7\u00dfen", "in", "das", "beu\u00b7len\u00b7schwe\u00b7re", "Holz", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Schwei\u00dfig kalt fallen Tropfen von den B\u00e4umen,", "tokens": ["Schwei\u00b7\u00dfig", "kalt", "fal\u00b7len", "Trop\u00b7fen", "von", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "M\u00e4nner, Frauen kauern lautlos,", "tokens": ["M\u00e4n\u00b7ner", ",", "Frau\u00b7en", "kau\u00b7ern", "laut\u00b7los", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Na\u00df von Schauern trieft das Gras.", "tokens": ["Na\u00df", "von", "Schau\u00b7ern", "trieft", "das", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den B\u00fcschen streichen Schritte,", "tokens": ["In", "den", "B\u00fc\u00b7schen", "strei\u00b7chen", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Knaben, Greise, M\u00e4gde, schlangenleise, suchen gierig nach den Schatten.", "tokens": ["Kna\u00b7ben", ",", "Grei\u00b7se", ",", "M\u00e4g\u00b7de", ",", "schlan\u00b7gen\u00b7lei\u00b7se", ",", "su\u00b7chen", "gie\u00b7rig", "nach", "den", "Schat\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Schatten lauern in den Buchen, hangen lang in dunkeln Gliedern", "tokens": ["Schat\u00b7ten", "lau\u00b7ern", "in", "den", "Bu\u00b7chen", ",", "han\u00b7gen", "lang", "in", "dun\u00b7keln", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Von den bangen Espenb\u00fcschen, prangen funkelnd auf den schwarzen Tollkirschstauden.", "tokens": ["Von", "den", "ban\u00b7gen", "Es\u00b7pen\u00b7b\u00fc\u00b7schen", ",", "pran\u00b7gen", "fun\u00b7kelnd", "auf", "den", "schwar\u00b7zen", "Toll\u00b7kirschs\u00b7tau\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Um die matten M\u00e4nner, Frauen kreisen dunkler stets die Schatten,", "tokens": ["Um", "die", "mat\u00b7ten", "M\u00e4n\u00b7ner", ",", "Frau\u00b7en", "krei\u00b7sen", "dunk\u00b7ler", "stets", "die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "NN", "VVFIN", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Schw\u00e4rzer funkeln alle B\u00e4ume, und mit schwarzer hoher Lohe", "tokens": ["Schw\u00e4r\u00b7zer", "fun\u00b7keln", "al\u00b7le", "B\u00e4u\u00b7me", ",", "und", "mit", "schwar\u00b7zer", "ho\u00b7her", "Lo\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,", "KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Droht die Nacht.", "tokens": ["Droht", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Alle Augen wachen d\u00fcster,", "tokens": ["Al\u00b7le", "Au\u00b7gen", "wa\u00b7chen", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stimmen fl\u00fcstern:", "tokens": ["Stim\u00b7men", "fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "\u00bbla\u00dft die Knaben, M\u00e4gde, Greise um die hohlen N\u00e4chte werben,", "tokens": ["\u00bb", "la\u00dft", "die", "Kna\u00b7ben", ",", "M\u00e4g\u00b7de", ",", "Grei\u00b7se", "um", "die", "hoh\u00b7len", "N\u00e4ch\u00b7te", "wer\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "$,", "NN", "$,", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wir, wir wollen mit den letzten tollen Kr\u00e4ften lieben, sterben,", "tokens": ["Wir", ",", "wir", "wol\u00b7len", "mit", "den", "letz\u00b7ten", "tol\u00b7len", "Kr\u00e4f\u00b7ten", "lie\u00b7ben", ",", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "F\u00fchlt, der Wald br\u00fctet hei\u00dfe Nachtviolen, h\u00fctet schwarze Tollkirschbeeren,", "tokens": ["F\u00fchlt", ",", "der", "Wald", "br\u00fc\u00b7tet", "hei\u00b7\u00dfe", "Nacht\u00b7vi\u00b7o\u00b7len", ",", "h\u00fc\u00b7tet", "schwar\u00b7ze", "Toll\u00b7kirschbee\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "ADJA", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-++-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "In den reifen dunklen S\u00e4ften w\u00fcten dunkle Liebeskr\u00e4fte,", "tokens": ["In", "den", "rei\u00b7fen", "dunk\u00b7len", "S\u00e4f\u00b7ten", "w\u00fc\u00b7ten", "dunk\u00b7le", "Lie\u00b7bes\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Greift die Bl\u00fcten, greift die Beeren.\u00ab", "tokens": ["Greift", "die", "Bl\u00fc\u00b7ten", ",", "greift", "die", "Bee\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einer fl\u00fcstert es dem andern,", "tokens": ["Ei\u00b7ner", "fl\u00fcs\u00b7tert", "es", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Finger greifen nach den reifen Tollkirschzweigen,", "tokens": ["Fin\u00b7ger", "grei\u00b7fen", "nach", "den", "rei\u00b7fen", "Toll\u00b7kirschzwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Finger brechen von den stechend s\u00fc\u00dfen Bl\u00fcten.", "tokens": ["Fin\u00b7ger", "bre\u00b7chen", "von", "den", "ste\u00b7chend", "s\u00fc\u00b7\u00dfen", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Von den tollen Beerens\u00e4ften funkeln schwarz die vollen Lippen,", "tokens": ["Von", "den", "tol\u00b7len", "Bee\u00b7ren\u00b7s\u00e4f\u00b7ten", "fun\u00b7keln", "schwarz", "die", "vol\u00b7len", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "A