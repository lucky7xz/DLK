{"textgrid.poem.56086": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da oben wird das Bild von einer Welt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da oben wird das Bild von einer Welt", "tokens": ["Da", "o\u00b7ben", "wird", "das", "Bild", "von", "ei\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus Blicken immerfort erneut und gilt.", "tokens": ["aus", "Bli\u00b7cken", "im\u00b7mer\u00b7fort", "er\u00b7neut", "und", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur manchmal, heimlich, kommt ein Ding und stellt", "tokens": ["Nur", "manch\u00b7mal", ",", "heim\u00b7lich", ",", "kommt", "ein", "Ding", "und", "stellt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADJD", "$,", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sich neben ihn, wenn er durch dieses Bild", "tokens": ["sich", "ne\u00b7ben", "ihn", ",", "wenn", "er", "durch", "die\u00b7ses", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "sich dr\u00e4ngt, ganz unten, anders, wie er ist;", "tokens": ["sich", "dr\u00e4ngt", ",", "ganz", "un\u00b7ten", ",", "an\u00b7ders", ",", "wie", "er", "ist", ";"], "token_info": ["w