{"dta.poem.10083": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schuldigkeit der Menschheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschheit ist nicht das, was sie zu seyn geden-", "tokens": ["Die", "Menschheit", "ist", "nicht", "das", ",", "was", "sie", "zu", "seyn", "ge\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PDS", "$,", "PRELS", "PPER", "PTKZU", "VAINF", "TRUNC"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "cket;", "tokens": ["cket", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wo sie, wie GOTT die Welt so wunderbar geziert,", "tokens": ["Wo", "sie", ",", "wie", "GoTT", "die", "Welt", "so", "wun\u00b7der\u00b7bar", "ge\u00b7ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PWAV", "NE", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht mit vergn\u00fcgter Seele sp\u00fchrt,", "tokens": ["Nicht", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "See\u00b7le", "sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch auf des Sch\u00f6pfers Werck mit Lust die Augen len-", "tokens": ["Noch", "auf", "des", "Sch\u00f6p\u00b7fers", "Werck", "mit", "Lust", "die", "Au\u00b7gen", "len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "APPR", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "cket.", "tokens": ["cket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}