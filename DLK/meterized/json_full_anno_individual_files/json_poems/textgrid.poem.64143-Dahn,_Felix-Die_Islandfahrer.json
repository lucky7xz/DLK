{"textgrid.poem.64143": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Islandfahrer", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbihr Segelbr\u00fcder, habt acht, habt acht!", "tokens": ["\u00bb", "ihr", "Se\u00b7gel\u00b7br\u00fc\u00b7der", ",", "habt", "acht", ",", "habt", "acht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VAFIN", "CARD", "$,", "VAFIN", "CARD", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "H\u00e4ngt \u00fcber den Schiffsrand Schilde:", "tokens": ["H\u00e4ngt", "\u00fc\u00b7ber", "den", "Schiffs\u00b7rand", "Schil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von b\u00f6sen Gewalten, von Riesen umwacht", "tokens": ["Von", "b\u00f6\u00b7sen", "Ge\u00b7wal\u00b7ten", ",", "von", "Rie\u00b7sen", "um\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sind Islands \u00f6de Gefilde.", "tokens": ["Sind", "Is\u00b7lands", "\u00f6\u00b7de", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich h\u00fcte den Bugspriet: und schw\u00f6mme daher", "tokens": ["Ich", "h\u00fc\u00b7te", "den", "Bug\u00b7spriet", ":", "und", "schw\u00f6m\u00b7me", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "VVFIN", "PAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Midhgardhwurm an den Nachen \u2013", "tokens": ["Der", "Midh\u00b7gard\u00b7hwurm", "an", "den", "Na\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ich durchhieb' ihm das Haupt! \u2013 Du, Eisbart Swer,", "tokens": ["Ich", "durch\u00b7hieb'", "ihm", "das", "Haupt", "!", "\u2013", "Du", ",", "Eis\u00b7bart", "Swer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "PPER", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit dem Speer sollst das Steuer bewachen.", "tokens": ["Mit", "dem", "Speer", "sollst", "das", "Steu\u00b7er", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und hebt sich die Haffrau aus kreiselndem Meer,", "tokens": ["Und", "hebt", "sich", "die", "Haf\u00b7frau", "aus", "krei\u00b7seln\u00b7dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Greift spritzend sie \u00fcber die Planken, \u2013", "tokens": ["Greift", "sprit\u00b7zend", "sie", "\u00fc\u00b7ber", "die", "Plan\u00b7ken", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "PPER", "APPR", "ART", "NN", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann wehrt mit den Schilden und bohre den Speer", "tokens": ["Dann", "wehrt", "mit", "den", "Schil\u00b7den", "und", "boh\u00b7re", "den", "Speer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr, Eisbart, tief in die Flanken.", "tokens": ["Ihr", ",", "Eis\u00b7bart", ",", "tief", "in", "die", "Flan\u00b7ken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch getrost nun, Genossen! Das Land ist nah:", "tokens": ["Doch", "ge\u00b7trost", "nun", ",", "Ge\u00b7nos\u00b7sen", "!", "Das", "Land", "ist", "nah", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Noch wenige Ruderschl\u00e4ge!", "tokens": ["Noch", "we\u00b7ni\u00b7ge", "Ru\u00b7der\u00b7schl\u00e4\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur meidet die dr\u00e4uende Klippe mir da,", "tokens": ["Nur", "mei\u00b7det", "die", "dr\u00e4u\u00b7en\u00b7de", "Klip\u00b7pe", "mir", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die umbrandete, zackige S\u00e4ge! \u2013", "tokens": ["Die", "um\u00b7bran\u00b7de\u00b7te", ",", "za\u00b7cki\u00b7ge", "S\u00e4\u00b7ge", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Seht, hart vor dem Bug uns der Balken schwimmt:", "tokens": ["Seht", ",", "hart", "vor", "dem", "Bug", "uns", "der", "Bal\u00b7ken", "schwimmt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein First einst im Hofe zu Leimath:", "tokens": ["Mein", "First", "einst", "im", "Ho\u00b7fe", "zu", "Lei\u00b7math", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wo er landet, empf\u00e4ngt uns, g\u00f6tterbestimmt,", "tokens": ["Wo", "er", "lan\u00b7det", ",", "emp\u00b7f\u00e4ngt", "uns", ",", "g\u00f6t\u00b7ter\u00b7be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die Scholle der neuen Heimat.", "tokens": ["Die", "Schol\u00b7le", "der", "neu\u00b7en", "Hei\u00b7mat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die alten Runen, geritzt vom Ahn,", "tokens": ["Die", "al\u00b7ten", "Ru\u00b7nen", ",", "ge\u00b7ritzt", "vom", "Ahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er tr\u00e4gt sie, die Odhalsmarken,", "tokens": ["Er", "tr\u00e4gt", "sie", ",", "die", "Od\u00b7hals\u00b7mar\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als Landnahmezeichen vorauf dem Kahn:", "tokens": ["Als", "Land\u00b7nah\u00b7me\u00b7zei\u00b7chen", "vor\u00b7auf", "dem", "Kahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn die Erde geh\u00f6rt dem Starken.", "tokens": ["Denn", "die", "Er\u00b7de", "ge\u00b7h\u00f6rt", "dem", "Star\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wo er antreibt, bau' ich des Freihofs Wehr", "tokens": ["Wo", "er", "an\u00b7treibt", ",", "bau'", "ich", "des", "Frei\u00b7hofs", "Wehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns aus Norges trotzigen Eichen:", "tokens": ["Uns", "aus", "Nor\u00b7ges", "trot\u00b7zi\u00b7gen", "Ei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "La\u00df sehn, ob \u00fcber das weite Meer", "tokens": ["La\u00df", "sehn", ",", "ob", "\u00fc\u00b7ber", "das", "wei\u00b7te", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "K\u00f6nig Haralds Arm wird reichen.", "tokens": ["K\u00f6\u00b7nig", "Ha\u00b7ralds", "Arm", "wird", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und den Giebel schm\u00fcck ich \u2013 Th\u00f4r gebeut's \u2013", "tokens": ["Und", "den", "Gie\u00b7bel", "schm\u00fcck", "ich", "\u2013", "Th\u00f4r", "ge\u00b7beut's", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$(", "NE", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit dem Hammer und mit zwei Lanzen:", "tokens": ["Mit", "dem", "Ham\u00b7mer", "und", "mit", "zwei", "Lan\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "CARD", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "La\u00df sehn, ob der Pfaff das Christenkreuz", "tokens": ["La\u00df", "sehn", ",", "ob", "der", "Pfaff", "das", "Chris\u00b7ten\u00b7kreuz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "ART", "NE", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird \u00fcber das Haupt uns pflanzen.", "tokens": ["Wird", "\u00fc\u00b7ber", "das", "Haupt", "uns", "pflan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.r