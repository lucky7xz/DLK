{"textgrid.poem.25187": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war ein Kind, mit gro\u00dfen Kinderaugen,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Kind, mit gro\u00dfen Kinderaugen,", "tokens": ["Ich", "war", "ein", "Kind", ",", "mit", "gro\u00b7\u00dfen", "Kin\u00b7der\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die nur zu tr\u00e4umerischem Schauen,", "tokens": ["Die", "nur", "zu", "tr\u00e4u\u00b7me\u00b7ri\u00b7schem", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht zum Berechnen und zum schlauen", "tokens": ["Nicht", "zum", "Be\u00b7rech\u00b7nen", "und", "zum", "schlau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "KON", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwerben taugen;", "tokens": ["Er\u00b7wer\u00b7ben", "tau\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In dumpfen Stuben bangte mir, ich scheute", "tokens": ["In", "dum\u00b7pfen", "Stu\u00b7ben", "bang\u00b7te", "mir", ",", "ich", "scheu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gespr\u00e4che n\u00fcchtern kluger Leute", "tokens": ["Ge\u00b7spr\u00e4\u00b7che", "n\u00fcch\u00b7tern", "klu\u00b7ger", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und stahl mich fort mit stiller Wonne", "tokens": ["Und", "stahl", "mich", "fort", "mit", "stil\u00b7ler", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Blumen, Gras und Sonne.", "tokens": ["Zu", "Blu\u00b7men", ",", "Gras", "und", "Son\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort sog ich Luft wie ein Befreiter, lauschte", "tokens": ["Dort", "sog", "ich", "Luft", "wie", "ein", "Be\u00b7frei\u00b7ter", ",", "lauschte"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KOKOM", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Bienen, Grillen, schwankendem Gestr\u00e4uch,", "tokens": ["Den", "Bie\u00b7nen", ",", "Gril\u00b7len", ",", "schwan\u00b7ken\u00b7dem", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das wogengleich im weichen Winde rauschte;", "tokens": ["Das", "wo\u00b7gen\u00b7gleich", "im", "wei\u00b7chen", "Win\u00b7de", "rauschte", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Staunen und Entz\u00fccken schaute", "tokens": ["Mit", "Stau\u00b7nen", "und", "Ent\u00b7z\u00fc\u00b7cken", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Aug' empor zu ihm, der tief und weithin blaute;", "tokens": ["Mein", "Aug'", "em\u00b7por", "zu", "ihm", ",", "der", "tief", "und", "weit\u00b7hin", "blau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "APPR", "PPER", "$,", "PRELS", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und der beth\u00f6rte Tr\u00e4umersinn", "tokens": ["Und", "der", "be\u00b7th\u00f6r\u00b7te", "Tr\u00e4u\u00b7mer\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schwamm mit dem wunderbaren,", "tokens": ["Schwamm", "mit", "dem", "wun\u00b7der\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wie Schneegebirge klaren", "tokens": ["Wie", "Schnee\u00b7ge\u00b7bir\u00b7ge", "kla\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gew\u00f6lke sanft dahin.", "tokens": ["Ge\u00b7w\u00f6l\u00b7ke", "sanft", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So wuchs ich auf; und allezeit getreu", "tokens": ["So", "wuchs", "ich", "auf", ";", "und", "al\u00b7le\u00b7zeit", "ge\u00b7treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Blieb meinem Aug das tr\u00e4umerische Schauen.", "tokens": ["Blieb", "mei\u00b7nem", "Aug", "das", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", "Schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ich bedachte nie: Der Schatz der Auen", "tokens": ["Doch", "ich", "be\u00b7dach\u00b7te", "nie", ":", "Der", "Schatz", "der", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind nicht die bunten Blumen, sondern Heu;", "tokens": ["Sind", "nicht", "die", "bun\u00b7ten", "Blu\u00b7men", ",", "son\u00b7dern", "Heu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was blau und rot im \u00c4hrenfelde bl\u00fcht,", "tokens": ["Was", "blau", "und", "rot", "im", "\u00c4h\u00b7ren\u00b7fel\u00b7de", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist nicht dem Bauch des Erntesackes hold;", "tokens": ["Ist", "nicht", "dem", "Bauch", "des", "Ern\u00b7te\u00b7sa\u00b7ckes", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und eines Dichters tr\u00e4umereich Gem\u00fct", "tokens": ["Und", "ei\u00b7nes", "Dich\u00b7ters", "tr\u00e4u\u00b7me\u00b7reich", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Tr\u00e4gt wenig K\u00f6rnchen irdisch Gold. \u2013", "tokens": ["Tr\u00e4gt", "we\u00b7nig", "K\u00f6rn\u00b7chen", "ir\u00b7disch", "Gold", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Nun stehn die \u00c4cker braun und stopplig nackt,", "tokens": ["Nun", "stehn", "die", "\u00c4\u00b7cker", "braun", "und", "stopp\u00b7lig", "nackt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschorne Wiesen werden bleich und bleicher,", "tokens": ["Ge\u00b7schor\u00b7ne", "Wie\u00b7sen", "wer\u00b7den", "bleich", "und", "blei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mir zum Spotte tanzt im fremden Speicher", "tokens": ["Und", "mir", "zum", "Spot\u00b7te", "tanzt", "im", "frem\u00b7den", "Spei\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der plumpe Flegel trocknen Erntetakt.", "tokens": ["Der", "plum\u00b7pe", "Fle\u00b7gel", "trock\u00b7nen", "Ern\u00b7te\u00b7takt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Am Dornstrauch sitz' ich, tr\u00fcbe wie der Himmel;", "tokens": ["Am", "Dorn\u00b7strauch", "sitz'", "ich", ",", "tr\u00fc\u00b7be", "wie", "der", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verwelkte Bl\u00e4tter zerrt ein rauher Wind,", "tokens": ["Ver\u00b7welk\u00b7te", "Bl\u00e4t\u00b7ter", "zerrt", "ein", "rau\u00b7her", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Scheucht m\u00fcrrisch fort das raschelnde Gewimmel;", "tokens": ["Scheucht", "m\u00fcr\u00b7risch", "fort", "das", "ra\u00b7scheln\u00b7de", "Ge\u00b7wim\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und tr\u00e4umend starr' ich nach ... ich dummes gro\u00dfes Kind!", "tokens": ["Und", "tr\u00e4u\u00b7mend", "starr'", "ich", "nach", "...", "ich", "dum\u00b7mes", "gro\u00b7\u00dfes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "$(", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Winter kommt; ich werde frieren, darben", "tokens": ["Der", "Win\u00b7ter", "kommt", ";", "ich", "wer\u00b7de", "frie\u00b7ren", ",", "dar\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "VVINF", "$,", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie die arme Maus im Stoppelwald", "tokens": ["Und", "wie", "die", "ar\u00b7me", "Maus", "im", "Stop\u00b7pel\u00b7wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich n\u00e4hren von dem Abfall fremder Garben;", "tokens": ["Mich", "n\u00e4h\u00b7ren", "von", "dem", "Ab\u00b7fall", "frem\u00b7der", "Gar\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht auch sterb' ich bald ...", "tokens": ["Viel\u00b7leicht", "auch", "sterb'", "ich", "bald", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mag sein! Doch schlie\u00df' ich ohne Reue", "tokens": ["Mag", "sein", "!", "Doch", "schlie\u00df'", "ich", "oh\u00b7ne", "Reu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "$.", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und segne dankbar meinen Tr\u00e4umerblick;", "tokens": ["Und", "seg\u00b7ne", "dank\u00b7bar", "mei\u00b7nen", "Tr\u00e4u\u00b7mer\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er lie\u00df 