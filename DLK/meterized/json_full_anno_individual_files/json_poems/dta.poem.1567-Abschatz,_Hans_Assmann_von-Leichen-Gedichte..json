{"dta.poem.1567": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leichen-Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Den keine Noth geschreckt/ kein Barbar \u00fcberwand/", "tokens": ["Den", "kei\u00b7ne", "Noth", "ge\u00b7schreckt", "/", "kein", "Bar\u00b7bar", "\u00fc\u00b7ber\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$(", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den hat der fr\u00fche Tod zur besten Zeit bezwungen;", "tokens": ["Den", "hat", "der", "fr\u00fc\u00b7he", "Tod", "zur", "bes\u00b7ten", "Zeit", "be\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch ihn nicht/ nur den Leib: die Seel in GOttes Hand/", "tokens": ["Doch", "ihn", "nicht", "/", "nur", "den", "Leib", ":", "die", "Seel", "in", "Got\u00b7tes", "Hand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "$(", "ADV", "ART", "NN", "$.", "ART", "NN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der unverblichne Ruhm schwebt auff viel tausend Zungen.", "tokens": ["Der", "un\u00b7ver\u00b7blich\u00b7ne", "Ruhm", "schwebt", "auff", "viel", "tau\u00b7send", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was wiltu Schlesien mehr Thr\u00e4nen lassen fliessen", "tokens": ["Was", "wil\u00b7tu", "Schle\u00b7si\u00b7en", "mehr", "Thr\u00e4\u00b7nen", "las\u00b7sen", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "NN", "PIAT", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Sachsen/ Grichenland/ zu seiner Asche giessen.", "tokens": ["Als", "Sach\u00b7sen", "/", "Gri\u00b7chen\u00b7land", "/", "zu", "sei\u00b7ner", "A\u00b7sche", "gies\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "NE", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}