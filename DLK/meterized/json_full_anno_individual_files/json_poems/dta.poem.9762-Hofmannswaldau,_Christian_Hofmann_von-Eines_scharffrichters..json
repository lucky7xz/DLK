{"dta.poem.9762": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines scharffrichters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der mensch- und hunde-feind/ des todes lieutenant/", "tokens": ["Der", "men\u00b7sch", "und", "hun\u00b7de\u00b7feind", "/", "des", "to\u00b7des", "lie\u00b7u\u00b7ten\u00b7ant", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$(", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der viel pflag vor der zeit dem tode zuzuf\u00fchren/", "tokens": ["Der", "viel", "pflag", "vor", "der", "zeit", "dem", "to\u00b7de", "zu\u00b7zu\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "NN", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Liegt hier: Jm fall sein grab von hunden wird erkant/", "tokens": ["Liegt", "hier", ":", "Jm", "fall", "sein", "grab", "von", "hun\u00b7den", "wird", "er\u00b7kant", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "APPRART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "VAFIN", "VVPP", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "So d\u00fcrfften sie es leicht mit einem denckmahl zieren.", "tokens": ["So", "d\u00fcrff\u00b7ten", "sie", "es", "leicht", "mit", "ei\u00b7nem", "denck\u00b7mahl", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "APPR", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}