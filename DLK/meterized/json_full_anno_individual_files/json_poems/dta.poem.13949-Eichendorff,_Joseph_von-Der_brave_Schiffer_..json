{"dta.poem.13949": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der brave Schiffer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Fahr' wohl! wie Adlerschwingen", "tokens": ["Fahr'", "wohl", "!", "wie", "Ad\u00b7ler\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "$.", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird in der Zeiten Schwung", "tokens": ["Wird", "in", "der", "Zei\u00b7ten", "Schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Ringen und Dein Singen", "tokens": ["Dein", "Rin\u00b7gen", "und", "Dein", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch deutsche Herzen klingen,", "tokens": ["Durch", "deut\u00b7sche", "Her\u00b7zen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bleibst Du ewig jung!", "tokens": ["So", "bleibst", "Du", "e\u00b7wig", "jung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}