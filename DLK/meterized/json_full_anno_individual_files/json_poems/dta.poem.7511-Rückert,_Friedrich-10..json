{"dta.poem.7511": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Eigenschaften gibts, die sich verschieden gatten", "tokens": ["Drei", "Ei\u00b7gen\u00b7schaf\u00b7ten", "gibts", ",", "die", "sich", "ver\u00b7schie\u00b7den", "gat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "$,", "PRELS", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dir und jedem Ding: Licht, Finsternis und Schatten.", "tokens": ["In", "dir", "und", "je\u00b7dem", "Ding", ":", "Licht", ",", "Fins\u00b7ter\u00b7nis", "und", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PIAT", "NN", "$.", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Urg\u00f6ttlich ist das Licht, ung\u00f6ttlich Finsternis,", "tokens": ["Ur\u00b7g\u00f6tt\u00b7lich", "ist", "das", "Licht", ",", "un\u00b7g\u00f6tt\u00b7lich", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zwischen beiden sind die Schatten ungewis.", "tokens": ["Und", "zwi\u00b7schen", "bei\u00b7den", "sind", "die", "Schat\u00b7ten", "un\u00b7ge\u00b7wis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Schatten suchen Theil am Licht, um zu entstehn,", "tokens": ["Die", "Schat\u00b7ten", "su\u00b7chen", "Theil", "am", "Licht", ",", "um", "zu", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch die Finsternis bestehn sie und vergehn.", "tokens": ["Und", "durch", "die", "Fins\u00b7ter\u00b7nis", "be\u00b7stehn", "sie", "und", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob sie in Finsternis vergehen, ob im Licht?", "tokens": ["Ob", "sie", "in", "Fins\u00b7ter\u00b7nis", "ver\u00b7ge\u00b7hen", ",", "ob", "im", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Kampf vergehen sie, den dis und jene ficht.", "tokens": ["Im", "Kampf", "ver\u00b7ge\u00b7hen", "sie", ",", "den", "dis", "und", "je\u00b7ne", "ficht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PDS", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Im Kampf, in welchem sie vergehn, entstehn sie immer,", "tokens": ["Im", "Kampf", ",", "in", "wel\u00b7chem", "sie", "ver\u00b7gehn", ",", "ent\u00b7stehn", "sie", "im\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.2": {"text": "Vers\u00f6hnen wollen sie den Kampf und k\u00f6nnens nimmer.", "tokens": ["Ver\u00b7s\u00f6h\u00b7nen", "wol\u00b7len", "sie", "den", "Kampf", "und", "k\u00f6n\u00b7nens", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "KON", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie legen, um den Kampf zu s\u00fchnen, sich dazwischen,", "tokens": ["Sie", "le\u00b7gen", ",", "um", "den", "Kampf", "zu", "s\u00fch\u00b7nen", ",", "sich", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,", "PRF", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00fcssen in den Kampf sich wider Willen mischen;", "tokens": ["Und", "m\u00fcs\u00b7sen", "in", "den", "Kampf", "sich", "wi\u00b7der", "Wil\u00b7len", "mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Alswie ein Brudervolk sich in zwei V\u00f6lker spaltet,", "tokens": ["Als\u00b7wie", "ein", "Bru\u00b7der\u00b7volk", "sich", "in", "zwei", "V\u00f6l\u00b7ker", "spal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn um die Krone Streit von zweien H\u00e4uptern waltet.", "tokens": ["Wenn", "um", "die", "Kro\u00b7ne", "Streit", "von", "zwei\u00b7en", "H\u00e4up\u00b7tern", "wal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Das ist der gro\u00dfe Kampf, der ringt durch die Natur,", "tokens": ["Das", "ist", "der", "gro\u00b7\u00dfe", "Kampf", ",", "der", "ringt", "durch", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles Gro\u00df' entringt sich diesem Kampfe nur.", "tokens": ["Und", "al\u00b7les", "Gro\u00df'", "ent\u00b7ringt", "sich", "die\u00b7sem", "Kamp\u00b7fe", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}