{"dta.poem.21365": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fahne st\u00e4ubte er gleichfalls ab,", "tokens": ["Die", "Fah\u00b7ne", "st\u00e4ub\u00b7te", "er", "gleich\u00b7falls", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und er sprach: \u201emein gr\u00f6\u00dfter Stolz ist,", "tokens": ["Und", "er", "sprach", ":", "\u201e", "mein", "gr\u00f6\u00df\u00b7ter", "Stolz", "ist", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df noch keine Motte die Seide zerfra\u00df,", "tokens": ["Da\u00df", "noch", "kei\u00b7ne", "Mot\u00b7te", "die", "Sei\u00b7de", "zer\u00b7fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und auch kein Wurm im Holz ist.\u201c", "tokens": ["Und", "auch", "kein", "Wurm", "im", "Holz", "ist", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPRART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als wir kamen in den Saal,", "tokens": ["Und", "als", "wir", "ka\u00b7men", "in", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo schlafend am Boden liegen", "tokens": ["Wo", "schla\u00b7fend", "am", "Bo\u00b7den", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Viel tausend Krieger, kampfbereit,", "tokens": ["Viel", "tau\u00b7send", "Krie\u00b7ger", ",", "kampf\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Alte sprach mit Vergn\u00fcgen:", "tokens": ["Der", "Al\u00b7te", "sprach", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201ehier m\u00fcssen wir leiser reden und gehn,", "tokens": ["\u201e", "hier", "m\u00fcs\u00b7sen", "wir", "lei\u00b7ser", "re\u00b7den", "und", "gehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Damit wir nicht wecken die Leute;", "tokens": ["Da\u00b7mit", "wir", "nicht", "we\u00b7cken", "die", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wieder verflossen sind hundert Jahr", "tokens": ["Wie\u00b7der", "ver\u00b7flos\u00b7sen", "sind", "hun\u00b7dert", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "CARD", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und L\u00f6hnungstag ist heute.\u201c", "tokens": ["Und", "L\u00f6h\u00b7nungs\u00b7tag", "ist", "heu\u00b7te", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und siehe! der Kaiser nahte sich sacht", "tokens": ["Und", "sie\u00b7he", "!", "der", "Kai\u00b7ser", "nah\u00b7te", "sich", "sacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ART", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den schlafenden Soldaten,", "tokens": ["Den", "schla\u00b7fen\u00b7den", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und steckte heimlich in die Tasch\u2019", "tokens": ["Und", "steck\u00b7te", "heim\u00b7lich", "in", "die", "Ta\u00b7sch'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedwedem einen Dukaten.", "tokens": ["Jed\u00b7we\u00b7dem", "ei\u00b7nen", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}