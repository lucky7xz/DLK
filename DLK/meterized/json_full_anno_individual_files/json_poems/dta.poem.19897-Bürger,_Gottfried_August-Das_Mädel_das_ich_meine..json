{"dta.poem.19897": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n  M\u00e4del das ich meine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "O was in tausend Liebespracht               ", "tokens": ["O", "was", "in", "tau\u00b7send", "Lie\u00b7be\u00b7spracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00e4del, das ich meine, lacht!", "tokens": ["Das", "M\u00e4\u00b7del", ",", "das", "ich", "mei\u00b7ne", ",", "lacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun sing, o Lied, und sag mir an!", "tokens": ["Nun", "sing", ",", "o", "Lied", ",", "und", "sag", "mir", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer hat das Wunder aufgethan:", "tokens": ["Wer", "hat", "das", "Wun\u00b7der", "auf\u00b7ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df so in tausend Liebespracht", "tokens": ["Da\u00df", "so", "in", "tau\u00b7send", "Lie\u00b7be\u00b7spracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das M\u00e4del, das ich meine, lacht?", "tokens": ["Das", "M\u00e4\u00b7del", ",", "das", "ich", "mei\u00b7ne", ",", "lacht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer hat, wie Paradieseswelt,", "tokens": ["Wer", "hat", ",", "wie", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des M\u00e4dels blaues Aug erhelt? \u2014", "tokens": ["Des", "M\u00e4\u00b7dels", "blau\u00b7es", "Aug", "er\u00b7helt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der liebe Gott! der hat\u2019s gethan,", "tokens": ["Der", "lie\u00b7be", "Gott", "!", "der", "hat's", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der\u2019s Firmament erleuchten kan;", "tokens": ["Der's", "Fir\u00b7ma\u00b7ment", "er\u00b7leuch\u00b7ten", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hat, wie Paradieseswelt,", "tokens": ["Der", "hat", ",", "wie", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des M\u00e4dels blaues Aug erhelt.", "tokens": ["Des", "M\u00e4\u00b7dels", "blau\u00b7es", "Aug", "er\u00b7helt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer hat das Rot auf Weis gemalt,", "tokens": ["Wer", "hat", "das", "Rot", "auf", "Weis", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das von des M\u00e4dels Wange stralt? \u2014", "tokens": ["Das", "von", "des", "M\u00e4\u00b7dels", "Wan\u00b7ge", "stralt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der liebe Gott! der hat\u2019s gethan,", "tokens": ["Der", "lie\u00b7be", "Gott", "!", "der", "hat's", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pfirsichbl\u00fcthe malen kan;", "tokens": ["Der", "Pfir\u00b7sich\u00b7bl\u00fc\u00b7the", "ma\u00b7len", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hat das Rot auf Weis gemalt,", "tokens": ["Der", "hat", "das", "Rot", "auf", "Weis", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das von des M\u00e4dels Wange stralt.", "tokens": ["Das", "von", "des", "M\u00e4\u00b7dels", "Wan\u00b7ge", "stralt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer schuf des M\u00e4dels Puvpurmund", "tokens": ["Wer", "schuf", "des", "M\u00e4\u00b7dels", "Puv\u00b7pur\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcrzig, s\u00fcs, und lieb und rund? \u2014", "tokens": ["So", "w\u00fcr\u00b7zig", ",", "s\u00fcs", ",", "und", "lieb", "und", "rund", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "NE", "$,", "KON", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der liebe Gott! der hat\u2019s gethan,", "tokens": ["Der", "lie\u00b7be", "Gott", "!", "der", "hat's", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nelk\u2019 und Erdbeer w\u00fcrzen kan;", "tokens": ["Der", "Nel\u00b7k'", "und", "Erd\u00b7beer", "w\u00fcr\u00b7zen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der schuf des M\u00e4dels Purpurmund", "tokens": ["Der", "schuf", "des", "M\u00e4\u00b7dels", "Pur\u00b7pur\u00b7mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00fcrzig, s\u00fcs, und lieb un