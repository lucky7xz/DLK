], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des M\u00e4dels seidne Locken wehn.", "tokens": ["Des", "M\u00e4\u00b7dels", "seid\u00b7ne", "Lo\u00b7cken", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer gab, zu Liebesred\u2019 und Sang,", "tokens": ["Wer", "gab", ",", "zu", "Lie\u00b7bes\u00b7red'", "und", "Sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem M\u00e4del holder Stimme Klang? \u2014", "tokens": ["Dem", "M\u00e4\u00b7del", "hol\u00b7der", "Stim\u00b7me", "Klang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der liebe, liebe Gott that dies,", "tokens": ["Der", "lie\u00b7be", ",", "lie\u00b7be", "Gott", "that", "dies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "NN", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nachtigallen fl\u00f6ten hies;", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "fl\u00f6\u00b7ten", "hies", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der gab, zu Liebesred\u2019 und Sang,", "tokens": ["Der", "gab", ",", "zu", "Lie\u00b7bes\u00b7red'", "und", "Sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambi