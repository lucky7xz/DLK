{"textgrid.poem.52478": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "60. Noahs Taube", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh' Noah seine Taube sandte,", "tokens": ["Eh'", "Noah", "sei\u00b7ne", "Tau\u00b7be", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Das Glaubensland, das unbekannte,", "tokens": ["Das", "Glau\u00b7bens\u00b7land", ",", "das", "un\u00b7be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu suchen durch das weite Meer,", "tokens": ["Zu", "su\u00b7chen", "durch", "das", "wei\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versucht' er es mit einem Raben", "tokens": ["Ver\u00b7sucht'", "er", "es", "mit", "ei\u00b7nem", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von anerkannten Sp\u00e4hergaben;", "tokens": ["Von", "an\u00b7er\u00b7kann\u00b7ten", "Sp\u00e4\u00b7her\u00b7ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch, dessen Flug sank tief und schwer. \u2013", "tokens": ["Doch", ",", "des\u00b7sen", "Flug", "sank", "tief", "und", "schwer", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "PRELAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbnun, wenn es dem Verstand des alten", "tokens": ["\u00bb", "nun", ",", "wenn", "es", "dem", "Ver\u00b7stand", "des", "al\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erfahr'nen Forschers nicht gelingt,", "tokens": ["Er\u00b7fahr'\u00b7nen", "For\u00b7schers", "nicht", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sich in der H\u00f6he zu erhalten,", "tokens": ["Sich", "in", "der", "H\u00f6\u00b7he", "zu", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wo erst der Blick zum Ziele dringt,", "tokens": ["Wo", "erst", "der", "Blick", "zum", "Zie\u00b7le", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So mag der k\u00fchne Falke fliegen!", "tokens": ["So", "mag", "der", "k\u00fch\u00b7ne", "Fal\u00b7ke", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sein Auge blicket scharf und hell;", "tokens": ["Sein", "Au\u00b7ge", "bli\u00b7cket", "scharf", "und", "hell", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er wei\u00df, wie Schein und Ahnung tr\u00fcgen;", "tokens": ["Er", "wei\u00df", ",", "wie", "Schein", "und", "Ah\u00b7nung", "tr\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sein Kopf ist der Erkenntnis Quell.\u00ab \u2013", "tokens": ["Sein", "Kopf", "ist", "der", "Er\u00b7kennt\u00b7nis", "Quell", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hoch schwang sich der in engen Kreisen,", "tokens": ["Hoch", "schwang", "sich", "der", "in", "en\u00b7gen", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Verglich, bezweifelt', dachte frei \u2013", "tokens": ["Ver\u00b7glich", ",", "be\u00b7zwei\u00b7felt'", ",", "dach\u00b7te", "frei", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und kehrte bald, um zu beweisen,", "tokens": ["Und", "kehr\u00b7te", "bald", ",", "um", "zu", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Land hoffen w\u00e4re Schw\u00e4rmerei.", "tokens": ["Land", "hof\u00b7fen", "w\u00e4\u00b7re", "Schw\u00e4r\u00b7me\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Kranich war nun angegangen,", "tokens": ["Der", "Kra\u00b7nich", "war", "nun", "an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Man hoffte auf sein Ahnungslicht;", "tokens": ["Man", "hoff\u00b7te", "auf", "sein", "Ah\u00b7nungs\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Durch Wolken trieb ihn das Verlangen,", "tokens": ["Durch", "Wol\u00b7ken", "trieb", "ihn", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Jedoch Gewi\u00dfheit bracht' er nicht.", "tokens": ["Je\u00b7doch", "Ge\u00b7wi\u00df\u00b7heit", "bracht'", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nun stieg die Not, und Thr\u00e4nen flossen,", "tokens": ["Nun", "stieg", "die", "Not", ",", "und", "Thr\u00e4\u00b7nen", "flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der Schwache fing zu zweifeln an;", "tokens": ["Der", "Schwa\u00b7che", "fing", "zu", "zwei\u00b7feln", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da stieg bescheiden, doch entschlossen,", "tokens": ["Da", "stieg", "be\u00b7schei\u00b7den", ",", "doch", "ent\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ein T\u00e4ubchen auf zur Himmelsbahn.", "tokens": ["Ein", "T\u00e4ub\u00b7chen", "auf", "zur", "Him\u00b7mels\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Es flog, zu retten die Genossen", "tokens": ["Es", "flog", ",", "zu", "ret\u00b7ten", "die", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Von Not und glaubenslosem Wahn;", "tokens": ["Von", "Not", "und", "glau\u00b7bens\u00b7lo\u00b7sem", "Wahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Es pr\u00fcft' und forscht' in d\u00fcstern Tagen,", "tokens": ["Es", "pr\u00fcft'", "und", "forscht'", "in", "d\u00fcs\u00b7tern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und sah dann Land im Morgenrot,", "tokens": ["Und", "sah", "dann", "Land", "im", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und jenen stillen \u00d6lbaum ragen,", "tokens": ["Und", "je\u00b7nen", "stil\u00b7len", "\u00d6l\u00b7baum", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der Zweige ew'gen Friedens bot. \u2013", "tokens": ["Der", "Zwei\u00b7ge", "ew'\u00b7gen", "Frie\u00b7dens", "bot", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die fromme Zeugin kehrte wieder,", "tokens": ["Die", "from\u00b7me", "Zeu\u00b7gin", "kehr\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Empfangen nun mit Jubelgru\u00df,", "tokens": ["Emp\u00b7fan\u00b7gen", "nun", "mit", "Ju\u00b7bel\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und legte still ihr Zweiglein nieder,", "tokens": ["Und", "leg\u00b7te", "still", "ihr", "Zwei\u00b7glein", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Mit Demut an des Altars Fu\u00df.", "tokens": ["Mit", "De\u00b7mut", "an", "des", "Al\u00b7tars", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Der Falke mit verbiss'nem Schnabel,", "tokens": ["Der", "Fal\u00b7ke", "mit", "ver\u00b7biss'\u00b7nem", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Von kaltem Wisserstolz gebl\u00e4ht,", "tokens": ["Von", "kal\u00b7tem", "Wis\u00b7ser\u00b7stolz", "ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Verh\u00f6hnt die Botschaft dreist als Fabel,", "tokens": ["Ver\u00b7h\u00f6hnt", "die", "Bot\u00b7schaft", "dreist", "als", "Fa\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Der Kranich seufzt, der Rabe schm\u00e4ht.", "tokens": ["Der", "Kra\u00b7nich", "seufzt", ",", "der", "Ra\u00b7be", "schm\u00e4ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Doch Noahs Blick gebeut zu schweigen", "tokens": ["Doch", "No\u00b7ahs", "Blick", "ge\u00b7beut", "zu", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und hei\u00dft das fromme T\u00e4ubchen nah'n:", "tokens": ["Und", "hei\u00dft", "das", "from\u00b7me", "T\u00e4ub\u00b7chen", "nah'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "\u00bbseht, Reine w\u00e4hlt der Herr zu Zeugen,", "tokens": ["\u00bb", "seht", ",", "Rei\u00b7ne", "w\u00e4hlt", "der", "Herr", "zu", "Zeu\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Wollt ihr das Land des Glaubens schau'n,", "tokens": ["Wollt", "ihr", "das", "Land", "des", "Glau\u00b7bens", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "So lernt Gehorsam und Vertrau'n!\u00ab", "tokens": ["So", "lernt", "Ge\u00b7hor\u00b7sam", "und", "Ver\u00b7trau'n", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Eh' Noah seine Taube sandte,", "tokens": ["Eh'", "Noah", "sei\u00b7ne", "Tau\u00b7be", "sand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Das Glaubensland, das unbekannte,", "tokens": ["Das", "Glau\u00b7bens\u00b7land", ",", "das", "un\u00b7be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu suchen durch das weite Meer,", "tokens": ["Zu", "su\u00b7chen", "durch", "das", "wei\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versucht' er es mit einem Raben", "tokens": ["Ver\u00b7sucht'", "er", "es", "mit", "ei\u00b7nem", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von anerkannten Sp\u00e4hergaben;", "tokens": ["Von", "an\u00b7er\u00b7kann\u00b7ten", "Sp\u00e4\u00b7her\u00b7ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch, dessen Flug sank tief und schwer. \u2013", "tokens": ["Doch", ",", "des\u00b7sen", "Flug", "sank", "tief", "und", "schwer", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "PRELAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbnun, wenn es dem Verstand des alten", "tokens": ["\u00bb", "nun", ",", "wenn", "es", "dem", "Ver\u00b7stand", "des", "al\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erfahr'nen Forschers nicht gelingt,", "tokens": ["Er\u00b7fahr'\u00b7nen", "For\u00b7schers", "nicht", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sich in der H\u00f6he zu erhalten,", "tokens": ["Sich", "in", "der", "H\u00f6\u00b7he", "zu", "er\u00b7ha