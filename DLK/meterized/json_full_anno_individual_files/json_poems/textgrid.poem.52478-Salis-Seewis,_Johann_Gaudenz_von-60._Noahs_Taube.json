,", "tokens": ["Durch", "Wol\u00b7ken", "trieb", "ihn", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Jedoch Gewi\u00dfheit bracht' er nicht.", "tokens": ["Je\u00b7doch", "Ge\u00b7wi\u00df\u00b7heit", "bracht'", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nun stieg die Not, und Thr\u00e4nen flossen,", "tokens": ["Nun", "stieg", "die", "Not", ",", "und", "Thr\u00e4\u00b7nen", "flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der Schwache fing zu zweifeln an;", "tokens": ["Der", "Schwa\u00b7che", "fing", "zu", "zwei\u00b7feln", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da stieg bescheiden, doch entschlossen,", "tokens": ["Da", "stieg", "be\u00b7schei\u00b7den", ",", "doch", "ent\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ein T\u00e4ubchen auf zur Himmelsbahn.", "tokens": ["Ein", "T\u00e4ub\u00b7chen", "auf", "zur", "Him\u00b7mels\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Es flog, zu rette