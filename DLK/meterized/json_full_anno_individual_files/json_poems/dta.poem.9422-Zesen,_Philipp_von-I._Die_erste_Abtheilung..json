{"dta.poem.9422": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Die erste Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da sich der K\u00f6nig herwante zu gehen!", "tokens": ["Da", "sich", "der", "K\u00f6\u00b7nig", "her\u00b7wan\u00b7te", "zu", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gaben die Narden den besten geruch/", "tokens": ["Ga\u00b7ben", "die", "Nar\u00b7den", "den", "bes\u00b7ten", "ge\u00b7ruch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Gleich wie der p\u00fcschel von Myrrhen mag stehen", "tokens": ["Gleich", "wie", "der", "p\u00fc\u00b7schel", "von", "Myr\u00b7rhen", "mag", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "VMFIN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zwischen den Br\u00fcsten/ nach unserem spruch/", "tokens": ["Zwi\u00b7schen", "den", "Br\u00fcs\u00b7ten", "/", "nach", "un\u00b7se\u00b7rem", "spruch", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "So stehet mein Leben/", "tokens": ["So", "ste\u00b7het", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mein Liebster auch eben/", "tokens": ["Mein", "Liebs\u00b7ter", "auch", "e\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "In v\u00f6lligem schein:", "tokens": ["In", "v\u00f6l\u00b7li\u00b7gem", "schein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Traube mu\u00df weichen/", "tokens": ["Die", "Trau\u00b7be", "mu\u00df", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Kan Jhme nicht gleichen/", "tokens": ["Kan", "Jh\u00b7me", "nicht", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Weil er viel s\u00fc\u00dfer als jrgend der Wein.", "tokens": ["Weil", "er", "viel", "s\u00fc\u00b7\u00dfer", "als", "jr\u00b7gend", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}