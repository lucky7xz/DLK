{"dta.poem.10450": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sa\u00dfen am Fischerhause,", "tokens": ["Wir", "sa\u00b7\u00dfen", "am", "Fi\u00b7scher\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und schauten nach der See;", "tokens": ["Und", "schau\u00b7ten", "nach", "der", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Abendnebel kamen,", "tokens": ["Die", "A\u00b7bend\u00b7ne\u00b7bel", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Und stiegen in die H\u00f6h'.", "tokens": ["Und", "stie\u00b7gen", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Leuchtthurm wurden die Lichter", "tokens": ["Im", "Leucht\u00b7thurm", "wur\u00b7den", "die", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Allm\u00e4hlig angesteckt,", "tokens": ["All\u00b7m\u00e4h\u00b7lig", "an\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in der weiten Ferne", "tokens": ["Und", "in", "der", "wei\u00b7ten", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ward noch ein Schiff entdeckt.", "tokens": ["Ward", "noch", "ein", "Schiff", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir sprachen von Sturm und Schiffbruch,", "tokens": ["Wir", "spra\u00b7chen", "von", "Sturm", "und", "Schiff\u00b7bruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Seemann, und wie er lebt,", "tokens": ["Vom", "See\u00b7mann", ",", "und", "wie", "er", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und zwischen Himmel und Wasser,", "tokens": ["Und", "zwi\u00b7schen", "Him\u00b7mel", "und", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Angst und Freude schwebt.", "tokens": ["Und", "Angst", "und", "Freu\u00b7de", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir sprachen von fernen K\u00fcsten,", "tokens": ["Wir", "spra\u00b7chen", "von", "fer\u00b7nen", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom S\u00fcden und vom Nord,", "tokens": ["Vom", "S\u00fc\u00b7den", "und", "vom", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von den seltsamen Menschen,", "tokens": ["Und", "von", "den", "selt\u00b7sa\u00b7men", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Und seltsamen Sitten dort.", "tokens": ["Und", "selt\u00b7sa\u00b7men", "Sit\u00b7ten", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Am Ganges duftet's und leuchtet's", "tokens": ["Am", "Gan\u00b7ges", "duf\u00b7tet's", "und", "leuch\u00b7tet's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "KON", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und Riesenb\u00e4ume bl\u00fch'n.", "tokens": ["Und", "Rie\u00b7sen\u00b7b\u00e4u\u00b7me", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00f6ne, stille Menschen", "tokens": ["Und", "sch\u00f6\u00b7ne", ",", "stil\u00b7le", "Men\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Lotosblumen knie'n.", "tokens": ["Vor", "Lo\u00b7tos\u00b7blu\u00b7men", "knie'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In Lappland sind schmutzige Leute,", "tokens": ["In", "Lap\u00b7pland", "sind", "schmut\u00b7zi\u00b7ge", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Plattk\u00f6pfig, breitm\u00e4ulig und klein;", "tokens": ["Platt\u00b7k\u00f6p\u00b7fig", ",", "breit\u00b7m\u00e4u\u00b7lig", "und", "klein", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie kauern um's Feuer, und backen", "tokens": ["Sie", "kau\u00b7ern", "um's", "Feu\u00b7er", ",", "und", "ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sich Fische, und qu\u00e4ken und schrei'n.", "tokens": ["Sich", "Fi\u00b7sche", ",", "und", "qu\u00e4\u00b7ken", "und", "schrei'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PRF", "NN", "$,", "KON", "VVINF", "KON", "VVFIN", "NE"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Die M\u00e4dchen horchten ernsthaft,", "tokens": ["Die", "M\u00e4d\u00b7chen", "horch\u00b7ten", "ernst\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und endlich sprach Niemand mehr;", "tokens": ["Und", "end\u00b7lich", "sprach", "Nie\u00b7mand", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Schiff war nicht mehr sichtbar,", "tokens": ["Das", "Schiff", "war", "nicht", "mehr", "sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es dunkelte gar zu sehr.", "tokens": ["Es", "dun\u00b7kel\u00b7te", "gar", "zu", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}