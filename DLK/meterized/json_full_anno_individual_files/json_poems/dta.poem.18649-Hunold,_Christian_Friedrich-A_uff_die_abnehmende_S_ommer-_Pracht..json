{"dta.poem.18649": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A uff die abnehmende  S ommer-  \n Pracht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Lebe mein beliebtes Leben/", "tokens": ["Le\u00b7be", "mein", "be\u00b7lieb\u00b7tes", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebe du mein Auffenthalt!", "tokens": ["Le\u00b7be", "du", "mein", "Auf\u00b7fent\u00b7halt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mir hat Vergn\u00fcgung geben/", "tokens": ["Was", "mir", "hat", "Ver\u00b7gn\u00fc\u00b7gung", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du angenehmer Wald/", "tokens": ["Bist", "du", "an\u00b7ge\u00b7neh\u00b7mer", "Wald", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihr bunt-bemahlten Wiesen/", "tokens": ["Und", "ihr", "bunt\u00b7be\u00b7mahl\u00b7ten", "Wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure Pracht sey stets gepriesen.", "tokens": ["Eu\u00b7re", "Pracht", "sey", "stets", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was vor h\u00f6chst-begl\u00fcckte Stunden/", "tokens": ["Was", "vor", "h\u00f6chst\u00b7be\u00b7gl\u00fcck\u00b7te", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Hab' ich sch\u00f6nes Wunderthal!", "tokens": ["Hab'", "ich", "sch\u00f6\u00b7nes", "Wun\u00b7der\u00b7thal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht in deinen Schoo\u00df gefunden/", "tokens": ["Nicht", "in", "dei\u00b7nen", "Schoo\u00df", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in den bebl\u00fcmten Saal?", "tokens": ["Und", "in", "den", "be\u00b7bl\u00fcm\u00b7ten", "Saal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Da\u00df du mir/ ich mu\u00df bekennen/", "tokens": ["Da\u00df", "du", "mir", "/", "ich", "mu\u00df", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$(", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unvergleichlich bist zu nennen.", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7lich", "bist", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ihr gr\u00fcnen Lorbeer-Reiser!", "tokens": ["Denn", "ihr", "gr\u00fc\u00b7nen", "Lor\u00b7beer\u00b7Rei\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was vergleicht sich eurer Pracht?", "tokens": ["Was", "ver\u00b7gleicht", "sich", "eu\u00b7rer", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil so mancher grosser K\u00e4yser", "tokens": ["Weil", "so", "man\u00b7cher", "gros\u00b7ser", "K\u00e4y\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch zu tragen werth geacht.", "tokens": ["Euch", "zu", "tra\u00b7gen", "werth", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaut wie ich durch eure Str\u00e4uche", "tokens": ["Schaut", "wie", "ich", "durch", "eu\u00b7re", "Str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich den K\u00f6nigen vergleiche.", "tokens": ["Mich", "den", "K\u00f6\u00b7ni\u00b7gen", "ver\u00b7glei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Purpur/ Wolle/ Sammt und Seide", "tokens": ["Pur\u00b7pur", "/", "Wol\u00b7le", "/", "Sammt", "und", "Sei\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NE", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist viel h\u00e4rter als der Klee/", "tokens": ["Ist", "viel", "h\u00e4r\u00b7ter", "als", "der", "Klee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich meine Glieder weide/", "tokens": ["Wo", "ich", "mei\u00b7ne", "Glie\u00b7der", "wei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich gantz entz\u00fcckend seh.", "tokens": ["Und", "mich", "gantz", "ent\u00b7z\u00fc\u00b7ckend", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend Blumen und Narcissen", "tokens": ["Tau\u00b7send", "Blu\u00b7men", "und", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Helffen meine Ruh vers\u00fcssen.", "tokens": ["Helf\u00b7fen", "mei\u00b7ne", "Ruh", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja der Klang der Nachtigallen", "tokens": ["Ja", "der", "Klang", "der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bindet meinen Geist und Sinn/", "tokens": ["Bin\u00b7det", "mei\u00b7nen", "Geist", "und", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es macht ihr beliebtes Lallen/", "tokens": ["Es", "macht", "ihr", "be\u00b7lieb\u00b7tes", "Lal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich aus mir selber bin/", "tokens": ["Da\u00df", "ich", "aus", "mir", "sel\u00b7ber", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die sch\u00f6ne Lust-Sirene", "tokens": ["Denn", "die", "sch\u00f6\u00b7ne", "Lust\u00b7Si\u00b7re\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00e4sselt mich durch ihr Geth\u00f6ne.", "tokens": ["F\u00e4s\u00b7selt", "mich", "durch", "ihr", "Ge\u00b7th\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Und ihr Silber-reichen Fl\u00fcsse!", "tokens": ["Und", "ihr", "Sil\u00b7ber\u00b7rei\u00b7chen", "Fl\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00f6hmt in voller Lieblichkeit.", "tokens": ["Str\u00f6hmt", "in", "vol\u00b7ler", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach wie viele Liebes-K\u00fcsse/", "tokens": ["Ach", "wie", "vie\u00b7le", "Lie\u00b7bes\u00b7K\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schick ich euch so manche Zeit.", "tokens": ["Schick", "ich", "euch", "so", "man\u00b7che", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich mag euer stilles Rauschen", "tokens": ["Ich", "mag", "eu\u00b7er", "stil\u00b7les", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gar mit meiner Lust vertauschen.", "tokens": ["Gar", "mit", "mei\u00b7ner", "Lust", "ver\u00b7tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Du Dianens Lust-Gefilde!", "tokens": ["Du", "Di\u00b7a\u00b7nens", "Lust\u00b7Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du begr\u00fcntes G\u00f6tter-Zelt!", "tokens": ["Du", "be\u00b7gr\u00fcn\u00b7tes", "G\u00f6t\u00b7ter\u00b7Zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo fast alles von den Wilde", "tokens": ["Wo", "fast", "al\u00b7les", "von", "den", "Wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In ihr Garn und Netze f\u00e4llt.", "tokens": ["In", "ihr", "Garn", "und", "Net\u00b7ze", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Echo kan nicht sch\u00f6ner singen/", "tokens": ["E\u00b7cho", "kan", "nicht", "sch\u00f6\u00b7ner", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als wenn ihre H\u00f6rner klingen.", "tokens": ["Als", "wenn", "ih\u00b7re", "H\u00f6r\u00b7ner", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch die Luft so mir dein Wesen", "tokens": ["Doch", "die", "Luft", "so", "mir", "dein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur mit Millionen zahlt/", "tokens": ["Nur", "mit", "Mil\u00b7lion\u00b7en", "zahlt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "L\u00e4st sich in den Sande lesen/", "tokens": ["L\u00e4st", "sich", "in", "den", "San\u00b7de", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ist als nur abgemahlt/", "tokens": ["Und", "ist", "als", "nur", "ab\u00b7ge\u00b7mahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ADV", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Weil vor Zephyrs sanfftes Blasen", "tokens": ["Weil", "vor", "Ze\u00b7phyrs", "sanff\u00b7tes", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Boreas begint zu rasen.", "tokens": ["Bo\u00b7re\u00b7as", "be\u00b7gint", "zu", "ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Chloris/ deine Purpur-Wangen/", "tokens": ["Chlo\u00b7ris", "/", "dei\u00b7ne", "Pur\u00b7pur\u00b7Wan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcssen mich zu guter Nacht/", "tokens": ["K\u00fcs\u00b7sen", "mich", "zu", "gu\u00b7ter", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du Zephyr/ mein Verlangen!", "tokens": ["Und", "du", "Ze\u00b7phyr", "/", "mein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast den Abschied schon gemacht.", "tokens": ["Hast", "den", "Ab\u00b7schied", "schon", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach gedenckt/ da\u00df mein Vergn\u00fcgen", "tokens": ["Ach", "ge\u00b7denckt", "/", "da\u00df", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit euch mu\u00df in Grabe liegen.", "tokens": ["Mit", "euch", "mu\u00df", "in", "Gra\u00b7be", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lebet denn ihr gr\u00fcnen W\u00e4lder!", "tokens": ["Le\u00b7bet", "denn", "ihr", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebe sch\u00f6nes Luft-Revier!", "tokens": ["Le\u00b7be", "sch\u00f6\u00b7nes", "Luft\u00b7Re\u00b7vier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auen/ Th\u00e4ler/ Berg und Felder/", "tokens": ["Au\u00b7en", "/", "Th\u00e4\u00b7ler", "/", "Berg", "und", "Fel\u00b7der", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du begl\u00fcckte Sommer-Zier!", "tokens": ["Du", "be\u00b7gl\u00fcck\u00b7te", "Som\u00b7mer\u00b7Zier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaut/ wie eure Pracht zerfliesset/", "tokens": ["Schaut", "/", "wie", "eu\u00b7re", "Pracht", "zer\u00b7flies\u00b7set", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein Paradie\u00df verschliesset.", "tokens": ["Und", "mein", "Pa\u00b7ra\u00b7die\u00df", "ver\u00b7schlies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}