{"dta.poem.15587": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht gnug ists, selber nicht zu hassen noch zu neiden;", "tokens": ["Nicht", "gnug", "ists", ",", "sel\u00b7ber", "nicht", "zu", "has\u00b7sen", "noch", "zu", "nei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "$,", "ADV", "PTKNEG", "PTKZU", "VVINF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du mu\u00dft den Neid, den Ha\u00df von andern auch vermeiden.", "tokens": ["Du", "mu\u00dft", "den", "Neid", ",", "den", "Ha\u00df", "von", "an\u00b7dern", "auch", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Hasses Blick ist Frost, des Neides Blick ist Glut;", "tokens": ["Des", "Has\u00b7ses", "Blick", "ist", "Frost", ",", "des", "Nei\u00b7des", "Blick", "ist", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Liebespflanze, dir ist Glut und Frost nicht gut.", "tokens": ["O", "Lie\u00b7bes\u00b7pflan\u00b7ze", ",", "dir", "ist", "Glut", "und", "Frost", "nicht", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "NN", "KON", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott geb' ein Pl\u00e4tzchen dir, wo rein du k\u00f6nntest sprossen,", "tokens": ["Gott", "geb'", "ein", "Pl\u00e4tz\u00b7chen", "dir", ",", "wo", "rein", "du", "k\u00f6nn\u00b7test", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Liebesstral besonnt, von Freundschaftsthau begossen;", "tokens": ["Von", "Lie\u00b7bes\u00b7stral", "be\u00b7sonnt", ",", "von", "Freund\u00b7schaftst\u00b7hau", "be\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo dich kein Blick erreicht, wo dich kein Hauch ber\u00fchrt,", "tokens": ["Wo", "dich", "kein", "Blick", "er\u00b7reicht", ",", "wo", "dich", "kein", "Hauch", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$,", "PWAV", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem nicht Geist geweckt, und Andacht wird gesch\u00fcrt.", "tokens": ["Von", "dem", "nicht", "Geist", "ge\u00b7weckt", ",", "und", "An\u00b7dacht", "wird", "ge\u00b7sch\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "NN", "VVPP", "$,", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}