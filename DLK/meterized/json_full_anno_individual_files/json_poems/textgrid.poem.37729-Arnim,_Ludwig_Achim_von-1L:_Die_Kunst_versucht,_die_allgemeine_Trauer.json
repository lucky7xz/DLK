{"textgrid.poem.37729": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Kunst versucht, die allgemeine Trauer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kunst versucht, die allgemeine Trauer", "tokens": ["Die", "Kunst", "ver\u00b7sucht", ",", "die", "all\u00b7ge\u00b7mei\u00b7ne", "Trau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch uns in Melodien zu verk\u00fcnden,", "tokens": ["Durch", "uns", "in", "Me\u00b7lo\u00b7dien", "zu", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NE", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir g\u00e4ben gern dem fl\u00fccht'gen Troste Dauer,", "tokens": ["Wir", "g\u00e4\u00b7ben", "gern", "dem", "fl\u00fccht'\u00b7gen", "Tros\u00b7te", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir m\u00f6chten edle Liebe ewig binden;", "tokens": ["Wir", "m\u00f6ch\u00b7ten", "ed\u00b7le", "Lie\u00b7be", "e\u00b7wig", "bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch hemmet unsern Sang ein ird'scher Schauer!", "tokens": ["Doch", "hem\u00b7met", "un\u00b7sern", "Sang", "ein", "ird'\u00b7scher", "Schau\u00b7er", "!"], "token_info": ["word", "word", "wor