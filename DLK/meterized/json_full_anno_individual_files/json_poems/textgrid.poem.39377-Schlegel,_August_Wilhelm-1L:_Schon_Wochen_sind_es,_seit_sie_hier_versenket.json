{"textgrid.poem.39377": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon Wochen sind es, seit sie hier versenket", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon Wochen sind es, seit sie hier versenket", "tokens": ["Schon", "Wo\u00b7chen", "sind", "es", ",", "seit", "sie", "hier", "ver\u00b7sen\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den s\u00fc\u00dfen Leib, von aller Huld umflo\u00dfen,", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Leib", ",", "von", "al\u00b7ler", "Huld", "um\u00b7flo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der das geliebte Wesen eingeschlo\u00dfen,", "tokens": ["Der", "das", "ge\u00b7lieb\u00b7te", "We\u00b7sen", "ein\u00b7ge\u00b7schlo\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dem umsonst mein Sehnen nun sich lenket.", "tokens": ["Zu", "dem", "um\u00b7sonst", "mein", "Seh\u00b7nen", "nun", "sich", "len\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PPOSAT", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Welk ist der Kranz, dem Grabe frisch geschenket,", "tokens": ["Welk", "ist", "der", "Kranz", ",", "dem", "Gra\u00b7be", "frisch", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht ein Halm dem H\u00fcgel noch entspro\u00dfen;", "tokens": ["Und", "nicht", "ein", "Halm", "dem", "H\u00fc\u00b7gel", "noch", "ent\u00b7spro\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne zielt mit gl\u00fchenden Gescho\u00dfen,", "tokens": ["Die", "Son\u00b7ne", "zielt", "mit", "gl\u00fc\u00b7hen\u00b7den", "Ge\u00b7scho\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch Thau noch Regen hat den Staub getr\u00e4nket.", "tokens": ["Noch", "Thau", "noch", "Re\u00b7gen", "hat", "den", "Staub", "ge\u00b7tr\u00e4n\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch werd' ich dazu nicht des Himmels brauchen.", "tokens": ["Auch", "werd'", "ich", "da\u00b7zu", "nicht", "des", "Him\u00b7mels", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kehr' dich nur weg, f\u00fchlloses Weltenauge!", "tokens": ["Kehr'", "dich", "nur", "weg", ",", "f\u00fchl\