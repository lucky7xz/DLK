{"textgrid.poem.24088": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du gef\u00fchlt hast, stammelnd, ungef\u00fcge,", "tokens": ["Was", "du", "ge\u00b7f\u00fchlt", "hast", ",", "stam\u00b7melnd", ",", "un\u00b7ge\u00b7f\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVPP", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich durft es dir und mir zum Kranze runden,", "tokens": ["Ich", "durft", "es", "dir", "und", "mir", "zum", "Kran\u00b7ze", "run\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast die Worte, ich den Sang gefunden, \u2013", "tokens": ["Du", "hast", "die", "Wor\u00b7te", ",", "ich", "den", "Sang", "ge\u00b7fun\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, da\u00df ich noch die sch\u00f6nen Kr\u00e4nze tr\u00fcge!", "tokens": ["Ach", ",", "da\u00df", "ich", "noch", "die", "sch\u00f6\u00b7nen", "Kr\u00e4n\u00b7ze", "tr\u00fc\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was waren das f\u00fcr Tage, was f\u00fcr Fl\u00fcge", "tokens": ["Was", "wa\u00b7ren", "das", "f\u00fcr", "Ta\u00b7ge", ",", "was", "f\u00fcr", "Fl\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Trostland Traum! Die Schwere \u00fcberwunden,", "tokens": ["Im", "Trost\u00b7land", "Traum", "!", "Die", "Schwe\u00b7re", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$.", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir beiden eingefl\u00fcgelig verbunden, \u2013", "tokens": ["Wir", "bei\u00b7den", "ein\u00b7ge\u00b7fl\u00fc\u00b7ge\u00b7lig", "ver\u00b7bun\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PIAT", "ADJD", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heute h\u00f6hnt die Leere g\u00e4hnend: L\u00fcge!", "tokens": ["Und", "heu\u00b7te", "h\u00f6hnt", "die", "Lee\u00b7re", "g\u00e4h\u00b7nend", ":", "L\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVPP", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df von mir auch diese Bl\u00e4tter geben;", "tokens": ["Ich", "mu\u00df", "von", "mir", "auch", "die\u00b7se", "Bl\u00e4t\u00b7ter", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sei kei