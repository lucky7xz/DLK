{"textgrid.poem.63827": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum letztenmal", "tokens": ["Zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word"], "pos": ["APPRART", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Meinen Lieblingspfad", "tokens": ["Mei\u00b7nen", "Lieb\u00b7lings\u00b7pfad"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Am Bergesabhang", "tokens": ["Am", "Ber\u00b7ges\u00b7ab\u00b7hang"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Durch Rebeng\u00e4rten,", "tokens": ["Durch", "Re\u00b7ben\u00b7g\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wo zartverschr\u00e4nktes Olivenlaub", "tokens": ["Wo", "zart\u00b7ver\u00b7schr\u00e4nk\u00b7tes", "O\u00b7li\u00b7ven\u00b7laub"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Unter den dunklen Lorbeerwipfeln", "tokens": ["Un\u00b7ter", "den", "dunk\u00b7len", "Lor\u00b7beer\u00b7wip\u00b7feln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Freundlichen Schatten streut,", "tokens": ["Freund\u00b7li\u00b7chen", "Schat\u00b7ten", "streut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wandl' ich dahin.", "tokens": ["Wandl'", "ich", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Still ist die Luft.", "tokens": ["Still", "ist", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nur ein lichtes W\u00f6lkchen", "tokens": ["Nur", "ein", "lich\u00b7tes", "W\u00f6lk\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Steht regungslos", "tokens": ["Steht", "re\u00b7gungs\u00b7los"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dr\u00fcben auf der wei\u00dfen Stirn", "tokens": ["Dr\u00fc\u00b7ben", "auf", "der", "wei\u00b7\u00dfen", "Stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Des greisen Monte Baldo,", "tokens": ["Des", "grei\u00b7sen", "Mon\u00b7te", "Bal\u00b7do", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die kleinen St\u00e4dtchen, gelagert", "tokens": ["Und", "die", "klei\u00b7nen", "St\u00e4dt\u00b7chen", ",", "ge\u00b7la\u00b7gert"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Zu F\u00fc\u00dfen des Alten", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "des", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wie artige Kinder,", "tokens": ["Wie", "ar\u00b7ti\u00b7ge", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Torri und Garda", "tokens": ["Tor\u00b7ri", "und", "Gar\u00b7da"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Mit San Vigilios Kap", "tokens": ["Mit", "San", "Vi\u00b7gi\u00b7li\u00b7os", "Kap"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Und s\u00fcdlicher Bardolino", "tokens": ["Und", "s\u00fcd\u00b7li\u00b7cher", "Bar\u00b7do\u00b7li\u00b7no"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Schimmern mit blanken H\u00e4uschen", "tokens": ["Schim\u00b7mern", "mit", "blan\u00b7ken", "H\u00e4usc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "\u00dcber der purpurnen Bl\u00e4ue des Sees", "tokens": ["\u00dc\u00b7ber", "der", "pur\u00b7pur\u00b7nen", "Bl\u00e4u\u00b7e", "des", "Sees"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Den nicht ein Windhauch kr\u00e4uselt.", "tokens": ["Den", "nicht", "ein", "Wind\u00b7hauch", "kr\u00e4u\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wonniger Frieden weitum.", "tokens": ["Won\u00b7ni\u00b7ger", "Frie\u00b7den", "wei\u00b7tum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Still atmet die Natur", "tokens": ["Still", "at\u00b7met", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Nacht entgegen,", "tokens": ["Der", "Nacht", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und dr\u00fcben im \u00c4ther h\u00e4ngt", "tokens": ["Und", "dr\u00fc\u00b7ben", "im", "\u00c4\u00b7ther", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Mondenscheibe,", "tokens": ["Die", "Mon\u00b7den\u00b7schei\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Eine Silberflocke,", "tokens": ["Ei\u00b7ne", "Sil\u00b7ber\u00b7flo\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie eine Bl\u00fcte des Fr\u00fchlingshimmels.", "tokens": ["Wie", "ei\u00b7ne", "Bl\u00fc\u00b7te", "des", "Fr\u00fch\u00b7lings\u00b7him\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort aber das Haus,", "tokens": ["Dort", "a\u00b7ber", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zu dem ich oft den Schritt gelenkt \u2013", "tokens": ["Zu", "dem", "ich", "oft", "den", "Schritt", "ge\u00b7lenkt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dem niederen Eingang", "tokens": ["Vor", "dem", "nie\u00b7de\u00b7ren", "Ein\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf verwitterter Treppenstufe", "tokens": ["Auf", "ver\u00b7wit\u00b7ter\u00b7ter", "Trep\u00b7pen\u00b7stu\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Die spielenden Kn\u00e4bchen", "tokens": ["Die", "spie\u00b7len\u00b7den", "Kn\u00e4b\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Verstummen, da sie mich sehn,", "tokens": ["Ver\u00b7stum\u00b7men", ",", "da", "sie", "mich", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das H\u00fcndchen belfert mich an,", "tokens": ["Das", "H\u00fcnd\u00b7chen", "bel\u00b7fert", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das magre, scharrende H\u00fchnervolk", "tokens": ["Das", "mag\u00b7re", ",", "schar\u00b7ren\u00b7de", "H\u00fch\u00b7ner\u00b7volk"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Stiebt auseinander,", "tokens": ["Stiebt", "aus\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.10": {"text": "Nur die Kaninchen fahren", "tokens": ["Nur", "die", "Ka\u00b7nin\u00b7chen", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sorglos fort, die Kr\u00e4uter zu rupfen", "tokens": ["Sorg\u00b7los", "fort", ",", "die", "Kr\u00e4u\u00b7ter", "zu", "rup\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Im hohen Gras.", "tokens": ["Im", "ho\u00b7hen", "Gras", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Und jetzt die junge Herrin der H\u00fctte.", "tokens": ["Und", "jetzt", "die", "jun\u00b7ge", "Her\u00b7rin", "der", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Der zweite Knabe", "tokens": ["Der", "zwei\u00b7te", "Kna\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Hat die blauen Augen der Mutter,", "tokens": ["Hat", "die", "blau\u00b7en", "Au\u00b7gen", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Der \u00e4ltre des Vaters Augen \u2013", "tokens": ["Der", "\u00e4l\u00b7tre", "des", "Va\u00b7ters", "Au\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Der mag wohl sitzen", "tokens": ["Der", "mag", "wohl", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Heut am Sonntag", "tokens": ["Heut", "am", "Sonn\u00b7tag"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "In der Schenke drunten,", "tokens": ["In", "der", "Schen\u00b7ke", "drun\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "\u00dcber deren T\u00fcr man liest:", "tokens": ["\u00dc\u00b7ber", "de\u00b7ren", "T\u00fcr", "man", "liest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "L\u00e4chelnd bietet das junge Weib", "tokens": ["L\u00e4\u00b7chelnd", "bie\u00b7tet", "das", "jun\u00b7ge", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mir guten Abend", "tokens": ["Mir", "gu\u00b7ten", "A\u00b7bend"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und steht, indem ich raste,", "tokens": ["Und", "steht", ",", "in\u00b7dem", "ich", "ras\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor mir, und wir plaudern", "tokens": ["Vor", "mir", ",", "und", "wir", "plau\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Von ihrem m\u00fchsamen Tagwerk,", "tokens": ["Von", "ih\u00b7rem", "m\u00fch\u00b7sa\u00b7men", "Tag\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ihrer Kindersorge,", "tokens": ["Ih\u00b7rer", "Kin\u00b7der\u00b7sor\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Die selten nur ihr erlaubt", "tokens": ["Die", "sel\u00b7ten", "nur", "ihr", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "PPER", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Den Gang zur Messe.", "tokens": ["Den", "Gang", "zur", "Mes\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Die h\u00f6re der Mann statt ihrer.", "tokens": ["Die", "h\u00f6\u00b7re", "der", "Mann", "statt", "ih\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Brav sei er und flei\u00dfig", "tokens": ["Brav", "sei", "er", "und", "flei\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "KON", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und halte sie gut.", "tokens": ["Und", "hal\u00b7te", "sie", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Und das junge Gesicht", "tokens": ["Und", "das", "jun\u00b7ge", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Von zarter Bl\u00e4sse,", "tokens": ["Von", "zar\u00b7ter", "Bl\u00e4s\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Indem die Augen", "tokens": ["In\u00b7dem", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ruhn auf den Kleinen,", "tokens": ["Ruhn", "auf", "den", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Strahlt von geheimem Stolz und Gl\u00fcck,", "tokens": ["Strahlt", "von", "ge\u00b7hei\u00b7mem", "Stolz", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie jener R\u00f6merin,", "tokens": ["Wie", "je\u00b7ner", "R\u00f6\u00b7me\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die ihre Kinder der Freundin zeigte", "tokens": ["Die", "ih\u00b7re", "Kin\u00b7der", "der", "Freun\u00b7din", "zeig\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": 