{"dta.poem.7587": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gib Acht, was suchst du denn mit deiner Arbeit Streben?", "tokens": ["Gib", "Acht", ",", "was", "suchst", "du", "denn", "mit", "dei\u00b7ner", "Ar\u00b7beit", "Stre\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es soll Befriedigung dir deiner W\u00fcnsche geben.", "tokens": ["Es", "soll", "Be\u00b7frie\u00b7di\u00b7gung", "dir", "dei\u00b7ner", "W\u00fcn\u00b7sche", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ist dein erster Wunsch? wol Gut und Eigenthum?", "tokens": ["Was", "ist", "dein", "ers\u00b7ter", "Wunsch", "?", "wol", "Gut", "und", "Ei\u00b7gen\u00b7thum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was dein anderer? vielleicht auch Ehr' und Ruhm?", "tokens": ["Und", "was", "dein", "an\u00b7de\u00b7rer", "?", "viel\u00b7leicht", "auch", "Ehr'", "und", "Ruhm", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "$.", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wann aber hat ein Mensch an Gut und Ruhm genung?", "tokens": ["Wann", "a\u00b7ber", "hat", "ein", "Mensch", "an", "Gut", "und", "Ruhm", "ge\u00b7nung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In beiden also suchst du nicht Befriedigung.", "tokens": ["In", "bei\u00b7den", "al\u00b7so", "suchst", "du", "nicht", "Be\u00b7frie\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So suchest du vielleicht dir selber zu gen\u00fcgen,", "tokens": ["So", "su\u00b7chest", "du", "viel\u00b7leicht", "dir", "sel\u00b7ber", "zu", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Werk nach deinem Sinn und deiner Kunst zu f\u00fcgen!", "tokens": ["Ein", "Werk", "nach", "dei\u00b7nem", "Sinn", "und", "dei\u00b7ner", "Kunst", "zu", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wann aber thatest du dir jemals selbst genug?", "tokens": ["Wann", "a\u00b7ber", "tha\u00b7test", "du", "dir", "je\u00b7mals", "selbst", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch die Befriedigung des Wunsches ist ein Trug.", "tokens": ["Auch", "die", "Be\u00b7frie\u00b7di\u00b7gung", "des", "Wun\u00b7sches", "ist", "ein", "Trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.6": {"line.1": {"text": "Und keine andre bleibt, als deine Lieb' und St\u00e4rke", "tokens": ["Und", "kei\u00b7ne", "and\u00b7re", "bleibt", ",", "als", "dei\u00b7ne", "Lieb'", "und", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PIS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu weihen treu dem dir von Gott vertrauten Werke.", "tokens": ["Zu", "wei\u00b7hen", "treu", "dem", "dir", "von", "Gott", "ver\u00b7trau\u00b7ten", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ART", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Thust du soviel du kannst, so thust du ihm genug,", "tokens": ["Thust", "du", "so\u00b7viel", "du", "kannst", ",", "so", "thust", "du", "ihm", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dis Gef\u00fchl allein gen\u00fcgt dir ohne Trug.", "tokens": ["Und", "dis", "Ge\u00b7f\u00fchl", "al\u00b7lein", "ge\u00b7n\u00fcgt", "dir", "oh\u00b7ne", "Trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dann kommen wol von selbst die G\u00fcter auch und Ehren;", "tokens": ["Dann", "kom\u00b7men", "wol", "von", "selbst", "die", "G\u00fc\u00b7ter", "auch", "und", "Eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADV", "ART", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn sie bleiben aus, so kannst du sie entbehren.", "tokens": ["Und", "wenn", "sie", "blei\u00b7ben", "aus", ",", "so", "kannst", "du", "sie", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}