, "pos": ["CARD", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sechs hat er geschossen,", "tokens": ["Sechs", "hat", "er", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sechs laufen davon.", "tokens": ["Sechs", "lau\u00b7fen", "da\u00b7von", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.44": {"line.1": {"text": "Der Hund th\u00e4t sie fangen,", "tokens": ["Der", "Hund", "th\u00e4t", "sie", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie fallen aufs Knie.", "tokens": ["Sie", "fal\u00b7len", "aufs", "Knie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Thresel thut bitten:", "tokens": ["Die", "Thre\u00b7sel", "thut", "bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "\u00bbdie thun dirs wohl nie!\u00ab", "tokens": ["\u00bb", "die", "thun", "dirs", "wohl", "nie", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PIS", "ADV", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.45": {"line.1": {"text": "\u00bbey Bayrischer Matthiesel", "tokens": ["\u00bb", "ey", "Bay\u00b7ri\u00b7scher", "Mat\u00b7thie\u00b7sel"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "NE", "NE", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Leben uns schenk,", "tokens": ["Das", "Le\u00b7ben", "uns", "schenk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wir tragen dir die Hirsche", "tokens": ["Wir", "tra\u00b7gen", "dir", "die", "Hir\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So weit du gedenkst.\u00ab", "tokens": ["So", "weit", "du", "ge\u00b7denkst", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.46": {"line.1": {"text": "Trotz J\u00e4ger auf Almen,", "tokens": ["Trotz", "J\u00e4\u00b7ger", "auf", "Al\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Merkt gr\u00fcn ist mein Huth,", "tokens": ["Merkt", "gr\u00fcn", "ist", "mein", "Huth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Drauf Schildhahnenfedern", "tokens": ["Drauf", "Schild\u00b7hah\u00b7nen\u00b7fe\u00b7dern"], "token_info": ["word", "word"], "pos": ["PAV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Gemsbart mit Blut.", "tokens": ["Und", "Gems\u00b7bart", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}