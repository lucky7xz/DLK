{"textgrid.poem.38055": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der bayrische Hiesel", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ey du mein liebe Thresel,", "tokens": ["Ey", "du", "mein", "lie\u00b7be", "Thre\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin nun wieder da,", "tokens": ["Ich", "bin", "nun", "wie\u00b7der", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Nacht sollst mich behalten,", "tokens": ["Zu", "Nacht", "sollst", "mich", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gelt schlag mirs nicht ab.", "tokens": ["Gelt", "schlag", "mirs", "nicht", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ey Bayrischer Matthiesel", "tokens": ["Ey", "Bay\u00b7ri\u00b7scher", "Mat\u00b7thie\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zieh aus deinen Rock,", "tokens": ["Zieh", "aus", "dei\u00b7nen", "Rock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sez dich ein Weil nieder,", "tokens": ["Sez", "dich", "ein", "Weil", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bis ich dir was koch.", "tokens": ["Bis", "ich", "dir", "was", "koch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ey du mein liebe Thresel,", "tokens": ["Ey", "du", "mein", "lie\u00b7be", "Thre\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es hungert mich nicht,", "tokens": ["Es", "hun\u00b7gert", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich bin gar weit gangen,", "tokens": ["Ich", "bin", "gar", "weit", "gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Darum bin ich m\u00fcd.", "tokens": ["Da\u00b7rum", "bin", "ich", "m\u00fcd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Warum bist du gangen", "tokens": ["Wa\u00b7rum", "bist", "du", "gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und bist allzu m\u00fcd?", "tokens": ["Und", "bist", "all\u00b7zu", "m\u00fcd", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Drey Hirsch hab ich schossen,", "tokens": ["Drey", "Hirsch", "hab", "ich", "schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die hab ich bey mir.", "tokens": ["Die", "hab", "ich", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ey sollt dich nicht hungern,", "tokens": ["Ey", "sollt", "dich", "nicht", "hun\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ey durstet dich nicht?", "tokens": ["Ey", "durs\u00b7tet", "dich", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Hund h\u00e4lt die Wache,", "tokens": ["Mein", "Hund", "h\u00e4lt", "die", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das best ihm zuricht.", "tokens": ["Das", "best", "ihm", "zu\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ey Bayrischer Matthiesel", "tokens": ["Ey", "Bay\u00b7ri\u00b7scher", "Mat\u00b7thie\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zieh aus deine Schuh,", "tokens": ["Zieh", "aus", "dei\u00b7ne", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Leg dich ein Weile nieder", "tokens": ["Leg", "dich", "ein", "Wei\u00b7le", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deck dich warm zu.", "tokens": ["Und", "deck", "dich", "warm", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ey du mein liebe Thresel,", "tokens": ["Ey", "du", "mein", "lie\u00b7be", "Thre\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allein kanns nicht seyn,", "tokens": ["Al\u00b7lein", "kanns", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wenn ich im Bette liege,", "tokens": ["Wenn", "ich", "im", "Bet\u00b7te", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Must auch bey mir seyn.", "tokens": ["Must", "auch", "bey", "mir", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn die Kuh ist gemolken,", "tokens": ["Wenn", "die", "Kuh", "ist", "ge\u00b7mol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Milch ist gesaiht,", "tokens": ["Die", "Milch", "ist", "ge\u00b7saiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So will ich schon kommen,", "tokens": ["So", "will", "ich", "schon", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da ist es noch Zeit.", "tokens": ["Da", "ist", "es", "noch", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Sie schliefen zusammen", "tokens": ["Sie", "schlie\u00b7fen", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die zeitlange Nacht,", "tokens": ["Die", "zeit\u00b7lan\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bis da\u00df manch sch\u00f6n Hirschlein", "tokens": ["Bis", "da\u00df", "manch", "sch\u00f6n", "Hirsc\u00b7hlein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PIAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Fenster rum grast.", "tokens": ["Am", "Fens\u00b7ter", "rum", "grast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "\u00bbey Thresel sollst aufstehn,", "tokens": ["\u00bb", "ey", "Thre\u00b7sel", "sollst", "auf\u00b7stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bring Krapfen heraus,", "tokens": ["Bring", "Krap\u00b7fen", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zw\u00f6lf J\u00e4ger sind draussen", "tokens": ["Zw\u00f6lf", "J\u00e4\u00b7ger", "sind", "draus\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Geschwind mach uns auf.\u00ab", "tokens": ["Ge\u00b7schwind", "mach", "uns", "auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Ey meine liebe J\u00e4ger", "tokens": ["Ey", "mei\u00b7ne", "lie\u00b7be", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch la\u00df ich nicht ein,", "tokens": ["Euch", "la\u00df", "ich", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich thu mich stets f\u00fcrchten", "tokens": ["Ich", "thu", "mich", "stets", "f\u00fcrch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und bin ganz allein.", "tokens": ["Und", "bin", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbey du mein liebe Thresel", "tokens": ["\u00bb", "ey", "du", "mein", "lie\u00b7be", "Thre\u00b7sel"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du f\u00fchrst uns nur blind,", "tokens": ["Du", "f\u00fchrst", "uns", "nur", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dein Bayrischer Matthiesel", "tokens": ["Dein", "Bay\u00b7ri\u00b7scher", "Mat\u00b7thie\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist auch bey dir drin.\u00ab", "tokens": ["Ist", "auch", "bey", "dir", "drin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Ey Bayrischer Matthiesel,", "tokens": ["Ey", "Bay\u00b7ri\u00b7scher", "Mat\u00b7thie\u00b7sel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du kunstreicher Kund,", "tokens": ["Du", "kuns\u00b7trei\u00b7cher", "Kund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zw\u00f6lf J\u00e4ger sind draussen", "tokens": ["Zw\u00f6lf", "J\u00e4\u00b7ger", "sind", "draus\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "VVINF"], "meter": 