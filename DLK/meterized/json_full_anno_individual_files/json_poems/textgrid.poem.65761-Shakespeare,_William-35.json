{"textgrid.poem.65761": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "35", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nicht mehr bang um das, was du getan.", "tokens": ["Sei", "nicht", "mehr", "bang", "um", "das", ",", "was", "du", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "APPR", "PDS", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Ros' ist dornig, Schlamm tr\u00fcbt silberhelle Quellen,", "tokens": ["Die", "Ros'", "ist", "dor\u00b7nig", ",", "Schlamm", "tr\u00fcbt", "sil\u00b7ber\u00b7hel\u00b7le", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wolk' und Verfinst'rung, Sonn' und Mondenbahn,", "tokens": ["Wol\u00b7k'", "und", "Ver\u00b7finst'\u00b7rung", ",", "Sonn'", "und", "Mon\u00b7den\u00b7bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die sch\u00f6nsten Knospen darf ein ekler Wurm entstellen.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Knos\u00b7pen", "darf", "ein", "ek\u00b7ler", "Wurm", "ent\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir fehlen 