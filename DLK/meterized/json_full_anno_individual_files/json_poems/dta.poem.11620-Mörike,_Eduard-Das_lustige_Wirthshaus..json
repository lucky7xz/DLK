{"dta.poem.11620": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Man lebet doch wie im Schlaraffenland hier,", "tokens": ["Man", "le\u00b7bet", "doch", "wie", "im", "Schla\u00b7raf\u00b7fen\u00b7land", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "KOKOM", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da schmauset man fr\u00fche wie spat;", "tokens": ["Da", "schmau\u00b7set", "man", "fr\u00fc\u00b7he", "wie", "spat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schon dreht sich der Boden vor Wonne mit mir,", "tokens": ["Schon", "dreht", "sich", "der", "Bo\u00b7den", "vor", "Won\u00b7ne", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Kaum da\u00df ich die Schwelle betrat!", "tokens": ["Kaum", "da\u00df", "ich", "die", "Schwel\u00b7le", "be\u00b7trat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Der Becher, ihr Herrn, wird nur gratis gef\u00fcllt:", "tokens": ["Der", "Be\u00b7cher", ",", "ihr", "Herrn", ",", "wird", "nur", "gra\u00b7tis", "ge\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Wirth ist kein knausiger Tropf,", "tokens": ["Der", "Wirth", "ist", "kein", "knau\u00b7si\u00b7ger", "Tropf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er f\u00fchrt den Hanswurst nicht vergeblich im Schild,", "tokens": ["Er", "f\u00fchrt", "den", "Hans\u00b7wurst", "nicht", "ver\u00b7geb\u00b7lich", "im", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Man wirft euch das Geld an den Kopf.", "tokens": ["Man", "wirft", "euch", "das", "Geld", "an", "den", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Der Alte, man sagt's, soll ein Zauberer seyn,", "tokens": ["Der", "Al\u00b7te", ",", "man", "sagt's", ",", "soll", "ein", "Zau\u00b7be\u00b7rer", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "VVFIN", "$,", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er l\u00e4chelt auch immer so schlau;", "tokens": ["Er", "l\u00e4\u00b7chelt", "auch", "im\u00b7mer", "so", "schlau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u2014 Und seht nur, was treten f\u00fcr Kerl da herein!", "tokens": ["Und", "seht", "nur", ",", "was", "tre\u00b7ten", "f\u00fcr", "Kerl", "da", "her\u00b7ein", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "$,", "PWS", "VVFIN", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die Eule, der Storch und der Pfau!", "tokens": ["Die", "Eu\u00b7le", ",", "der", "Storch", "und", "der", "Pfau", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Seht nur, wie manierlich die Racker sich drehn!", "tokens": ["Seht", "nur", ",", "wie", "ma\u00b7nier\u00b7lich", "die", "Ra\u00b7cker", "sich", "drehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Kratzf\u00fc\u00df'! Ei Wetter, so schlag!", "tokens": ["Die", "Kratz\u00b7f\u00fc\u00df'", "!", "Ei", "Wet\u00b7ter", ",", "so", "schlag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie nehmen sich St\u00fchle \u2014 das mu\u00df ich gestehn,", "tokens": ["Sie", "neh\u00b7men", "sich", "St\u00fch\u00b7le", "das", "mu\u00df", "ich", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "$(", "PDS", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So was sieht man nicht alle Tag.", "tokens": ["So", "was", "sieht", "man", "nicht", "al\u00b7le", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PIS", "PTKNEG", "PIAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Mein Alter am F\u00e4\u00dfchen, er zapfet den Wein", "tokens": ["Mein", "Al\u00b7ter", "am", "F\u00e4\u00df\u00b7chen", ",", "er", "zap\u00b7fet", "den", "Wein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und h\u00e4lt sich vor Lachen den Bauch;", "tokens": ["Und", "h\u00e4lt", "sich", "vor", "La\u00b7chen", "den", "Bauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Rebekke schenkt ihnen vom feurigsten ein", "tokens": ["Re\u00b7bek\u00b7ke", "schenkt", "ih\u00b7nen", "vom", "feu\u00b7rigs\u00b7ten", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "ART"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und zierlich kredenzt sie ihn auch.", "tokens": ["Und", "zier\u00b7lich", "kre\u00b7denzt", "sie", "ihn", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun sitzen sie steif wie Professorsleut' da,", "tokens": ["Nun", "sit\u00b7zen", "sie", "steif", "wie", "Pro\u00b7fes\u00b7sors\u00b7leut'", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und lassen das Glas unber\u00fchrt,", "tokens": ["Und", "las\u00b7sen", "das", "Glas", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir Herren vom Humpen sind ihnen zu nah':", "tokens": ["Wir", "Her\u00b7ren", "vom", "Hum\u00b7pen", "sind", "ih\u00b7nen", "zu", "nah'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "VAFIN", "PPER", "PTKZU", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Man hat sich leicht compromittirt.", "tokens": ["Man", "hat", "sich", "leicht", "com\u00b7pro\u00b7mit\u00b7tirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur ruhig, und kehrt euch noch gar nicht an sie!", "tokens": ["Nur", "ru\u00b7hig", ",", "und", "kehrt", "euch", "noch", "gar", "nicht", "an", "sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die f\u00fchren ihr M\u00fcthlein im Sack;", "tokens": ["Die", "f\u00fch\u00b7ren", "ihr", "M\u00fcth\u00b7lein", "im", "Sack", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es ist nur erlogene Pedanterie,", "tokens": ["Es", "ist", "nur", "er\u00b7lo\u00b7ge\u00b7ne", "Pe\u00b7dan\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie sind das versoffenste Pack.", "tokens": ["Sie", "sind", "das", "ver\u00b7sof\u00b7fens\u00b7te", "Pack", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Inzwischen, mein sch\u00f6nes, schwarzaugiges Kind,", "tokens": ["I\u00b7nzwi\u00b7schen", ",", "mein", "sch\u00f6\u00b7nes", ",", "schwarz\u00b7au\u00b7gi\u00b7ges", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Komm, sing' uns was Lustiges vor!", "tokens": ["Komm", ",", "sing'", "uns", "was", "Lus\u00b7ti\u00b7ges", "vor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "PIS", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}