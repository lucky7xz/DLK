{"dta.poem.17768": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "12.  Der geheime Hirsch und sein Bronn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Hirsch der laufft und sucht ein k\u00fchles Br\u00fcnnelein/", "tokens": ["Der", "Hirsch", "der", "laufft", "und", "sucht", "ein", "k\u00fch\u00b7les", "Br\u00fcn\u00b7nel\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit sein Hertz erquikt und ruhig m\u00f6ge seyn.", "tokens": ["Da\u00b7mit", "sein", "Hertz", "er\u00b7quikt", "und", "ru\u00b7hig", "m\u00f6\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVPP", "KON", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seele die GOtt liebt/ die eilet zu dem Bronnen/", "tokens": ["Die", "See\u00b7le", "die", "Gott", "liebt", "/", "die", "ei\u00b7let", "zu", "dem", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$(", "ART", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Au\u00df dem die s\u00fcsse Bach de\u00df Lebens kombt geronnen.", "tokens": ["Au\u00df", "dem", "die", "s\u00fcs\u00b7se", "Bach", "de\u00df", "Le\u00b7bens", "kombt", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Bronn ist JEsus Christ/ der un\u00df mit seinem quall", "tokens": ["Der", "Bronn", "ist", "Je\u00b7sus", "Christ", "/", "der", "un\u00df", "mit", "sei\u00b7nem", "quall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "NE", "$(", "ART", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm wahren Glauben tr\u00e4nkt/ und st\u00e4rkt f\u00fcr S\u00fcnden", "tokens": ["Jm", "wah\u00b7ren", "Glau\u00b7ben", "tr\u00e4nkt", "/", "und", "st\u00e4rkt", "f\u00fcr", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$(", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "fall.", "tokens": ["fall", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Bleibstu bey diesem quall/ und trinkst offt au\u00df dem Bron-", "tokens": ["Bleibs\u00b7tu", "bey", "die\u00b7sem", "quall", "/", "und", "trinkst", "offt", "au\u00df", "dem", "Bron"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "$(", "KON", "VVFIN", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "nen/", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.10": {"text": "So hastu meine Seel gantz Seeliglich gewonnen.", "tokens": ["So", "has\u00b7tu", "mei\u00b7ne", "Seel", "gantz", "See\u00b7lig\u00b7lich", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}