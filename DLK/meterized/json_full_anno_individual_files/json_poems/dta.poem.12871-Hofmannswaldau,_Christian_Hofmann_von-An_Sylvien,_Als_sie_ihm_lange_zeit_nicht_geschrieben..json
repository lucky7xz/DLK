{"dta.poem.12871": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien,  \n Als sie ihm lange zeit nicht geschrieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjll deine sch\u00f6ne faust, die sich doch sonst beflei\u00dft,", "tokens": ["Wjll", "dei\u00b7ne", "sch\u00f6\u00b7ne", "faust", ",", "die", "sich", "doch", "sonst", "be\u00b7flei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, was dein hertz befiehlt, getreulich auszurichten,", "tokens": ["Das", ",", "was", "dein", "hertz", "be\u00b7fiehlt", ",", "ge\u00b7treu\u00b7lich", "aus\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich gar nicht mehr durch ihre schrifft verpflichten?", "tokens": ["Mich", "gar", "nicht", "mehr", "durch", "ih\u00b7re", "schrifft", "ver\u00b7pflich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, Sylvia! ich wei\u00df wohl, was das hei\u00dft.", "tokens": ["Ach", ",", "Syl\u00b7via", "!", "ich", "wei\u00df", "wohl", ",", "was", "das", "hei\u00dft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "PPER", "VVFIN", "ADV", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mich deucht, ihr schweigen will auch ohne schrifft und blat", "tokens": ["Mich", "deucht", ",", "ihr", "schwei\u00b7gen", "will", "auch", "oh\u00b7ne", "schrifft", "und", "blat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "VMFIN", "ADV", "APPR", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch die best\u00e4ndigkeit mir diese nachricht sagen,", "tokens": ["Durch", "die", "be\u00b7st\u00e4n\u00b7dig\u00b7keit", "mir", "die\u00b7se", "nach\u00b7richt", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die meine furcht schon offt dem hertzen vorgetragen:", "tokens": ["Die", "mei\u00b7ne", "furcht", "schon", "offt", "dem", "hert\u00b7zen", "vor\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df Sylvia mich l\u00e4ngst vergessen hat.", "tokens": ["Da\u00df", "Syl\u00b7via", "mich", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}