{"textgrid.poem.63607": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt an mein Herz, kommt nah heran, ihr Lieben!", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt an mein Herz, kommt nah heran, ihr Lieben!", "tokens": ["Kommt", "an", "mein", "Herz", ",", "kommt", "nah", "he\u00b7ran", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Jammer, welch ein Raub an mir geschehn,", "tokens": ["Im", "Jam\u00b7mer", ",", "welch", "ein", "Raub", "an", "mir", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAT", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verga\u00df ich, welch ein Reichtum mir geblieben.", "tokens": ["Ver\u00b7ga\u00df", "ich", ",", "welch", "ein", "Reich\u00b7tum", "mir", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAT", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00dft nur des Sturmes erste Wut verwehn,", "tokens": ["La\u00dft", "nur", "des", "Stur\u00b7mes", "ers\u00b7te", "Wut", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann blickt des Himmels ausgeweinte Bl\u00e4ue", "tokens": ["Dann", "blickt", "des", "Him\u00b7mels", "aus\u00b7ge\u00b7wein\u00b7te", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gelassen durch, drin tausend Sterne stehn;", "tokens": ["Ge\u00b7las\u00b7sen", "durch", ",", "drin", "tau\u00b7send", "Ster\u00b7ne", "stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und man gedenkt der Lebenspflicht aufs neue,", "tokens": ["Und", "man", "ge\u00b7denkt", "der", "Le\u00b7bens\u00b7pflicht", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Gr\u00fcfte nicht die Seele zu vergraben,", "tokens": ["In", "Gr\u00fcf\u00b7te", "nicht", "die", "See\u00b7le", "zu", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In frevlem Unma\u00df sehnsuchtsvoller Treue.", "tokens": ["In", "frev\u00b7lem", "Un\u00b7ma\u00df", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sollt' ich der Pflicht zu lang vergessen haben,", "tokens": ["Sollt'", "ich", "der", "Pflicht", "zu", "lang", "ver\u00b7ges\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Vergebt mir's! Dieser Schlag war allzu herbe.", "tokens": ["Ver\u00b7gebt", "mir's", "!", "Die\u00b7ser", "Schlag", "war", "all\u00b7zu", "her\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "PDAT", "NN", "VAFIN", "PTKA", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zu herzlich hing mein Herz an diesem Knaben.", "tokens": ["Zu", "herz\u00b7lich", "hing", "mein", "Herz", "an", "die\u00b7sem", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nun sei die Menschheit meines Lieblings Erbe,", "tokens": ["Nun", "sei", "die", "Menschheit", "mei\u00b7nes", "Lieb\u00b7lings", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Auf da\u00df der Schatz, den ich f\u00fcr ihn gespart,", "tokens": ["Auf", "da\u00df", "der", "Schatz", ",", "den", "ich", "f\u00fcr", "ihn", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "An Liebeskraft nicht herrenlos verderbe.", "tokens": ["An", "Lie\u00b7bes\u00b7kraft", "nicht", "her\u00b7ren\u00b7los", "ver\u00b7der\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sie meinen, wer sich keiner Himmelfahrt,", "tokens": ["Sie", "mei\u00b7nen", ",", "wer", "sich", "kei\u00b7ner", "Him\u00b7mel\u00b7fahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Nicht froher Urst\u00e4nd' will getr\u00f6sten lassen,", "tokens": ["Nicht", "fro\u00b7her", "Ur\u00b7st\u00e4nd'", "will", "ge\u00b7tr\u00f6s\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VMFIN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Dem m\u00fcsse trostlos sein die Gegenwart.", "tokens": ["Dem", "m\u00fcs\u00b7se", "trost\u00b7los", "sein", "die", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VAINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dies bange Erdenzwielicht m\u00fcss' er hassen,", "tokens": ["Dies", "ban\u00b7ge", "Er\u00b7den\u00b7zwie\u00b7licht", "m\u00fcss'", "er", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Das nicht ein Strahl der Hoffnung je verkl\u00e4re,", "tokens": ["Das", "nicht", "ein", "Strahl", "der", "Hoff\u00b7nung", "je", "ver\u00b7kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und in Verzweiflung werd' er einst erblassen.", "tokens": ["Und", "in", "Ver\u00b7zwei\u00b7flung", "werd'", "er", "einst", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Kommt! Machen wir dem eignen Credo Ehre!", "tokens": ["Kommt", "!", "Ma\u00b7chen", "wir", "dem", "eig\u00b7nen", "Cre\u00b7do", "Eh\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Aufrechten Haupts, nicht trotzig, nicht verzagt,", "tokens": ["Auf\u00b7rech\u00b7ten", "Haupts", ",", "nicht", "trot\u00b7zig", ",", "nicht", "ver\u00b7zagt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Liebt, was da lieblich ist, ertragt das Schwere.", "tokens": ["Liebt", ",", "was", "da", "lieb\u00b7lich", "ist", ",", "er\u00b7tragt", "das", "Schwe\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "O sch\u00e4mt euch nicht, da\u00df ihr in Schmerzen klagt!", "tokens": ["O", "sch\u00e4mt", "euch", "nicht", ",", "da\u00df", "ihr", "in", "Schmer\u00b7zen", "klagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Es \u00e4chzt der Baum, wenn Ungewitter toben,", "tokens": ["Es", "\u00e4chzt", "der", "Baum", ",", "wenn", "Un\u00b7ge\u00b7wit\u00b7ter", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und kr\u00fcmmt den Wipfel, der so hoch geragt.", "tokens": ["Und", "kr\u00fcmmt", "den", "Wip\u00b7fel", ",", "der", "so", "hoch", "ge\u00b7ragt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dann wieder still von Sonnenglanz umwoben", "tokens": ["Dann", "wie\u00b7der", "still", "von", "Son\u00b7nen\u00b7glanz", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Trinkt er den \u00c4ther, reift der Ernte zu,", "tokens": ["Trinkt", "er", "den", "\u00c4\u00b7ther", ",", "reift", "der", "Ern\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.30": {"text": "Bis ihn die Axt zerspellt zu Scheit und Kloben.", "tokens": ["Bis", "ihn", "die", "Axt", "zer\u00b7spellt", "zu", "Scheit", "und", "Klo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "So, Kind der Erde, f\u00fcge dich auch du", "tokens": ["So", ",", "Kind", "der", "Er\u00b7de", ",", "f\u00fc\u00b7ge", "dich", "auch", "du"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und neide nicht hoff\u00e4rt'gen Himmelsp\u00e4chtern", "tokens": ["Und", "nei\u00b7de", "nicht", "hof\u00b7f\u00e4rt'\u00b7gen", "Him\u00b7mel\u00b7sp\u00e4ch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und S\u00e4ulenheil'gen ihre dumpfe Ruh.", "tokens": ["Und", "S\u00e4u\u00b7len\u00b7heil'\u00b7gen", "ih\u00b7re", "dump\u00b7fe", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Wag es, gleich all den atmenden Geschlechtern,", "tokens": ["Wag", "es", ",", "gleich", "all", "den", "at\u00b7men\u00b7den", "Ge\u00b7schlech\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.35": {"text": "Dein Herz zu h\u00e4ngen an dies kurze Sein,", "tokens": ["Dein", "Herz", "zu", "h\u00e4n\u00b7gen", "an", "dies", "kur\u00b7ze", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Die Welt zu lieben trotz den Weltver\u00e4chtern.", "tokens": ["Die", "Welt", "zu", "lie\u00b7ben", "trotz", "den", "Welt\u00b7ver\u00b7\u00e4ch\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Der Augenblick und dein Gem\u00fct sind dein,", "tokens": ["Der", "Au\u00b7gen\u00b7blick", "und", "dein", "Ge\u00b7m\u00fct", "sind", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Du Sterblicher; du sollst sie zum Gef\u00e4\u00dfe", "tokens": ["Du", "Sterb\u00b7li\u00b7cher", ";", "du", "sollst", "sie", "zum", "Ge\u00b7f\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "PPER", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Des edelsten, des ew'gen Inhalts weihn.", "tokens": ["Des", "e\u00b7dels\u00b7ten", ",", "des", "ew'\u00b7gen", "In\u00b7halts", "weihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.40": {"text": "Was jedem, der zu eigen es bes\u00e4\u00dfe,", "tokens": ["Was", "je\u00b7dem", ",", "der", "zu", "ei\u00b7gen", "es", "be\u00b7s\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "$,", "PRELS", "PTKA", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Das Leben tr\u00f6stlich macht, das schaff in dir", "tokens": ["Das", "Le\u00b7ben", "tr\u00f6st\u00b7lich", "macht", ",", "das", "schaff", "in", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und teil es mit, wo jemand sein verg\u00e4\u00dfe.", "tokens": ["Und", "teil", "es", "mit", ",", "wo", "je\u00b7mand", "sein", "ver\u00b7g\u00e4\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKVZ", "$,", "PWAV", "PIS", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "So hast du Ewigkeit und Himmel ", "tokens": ["So", "hast", "du", "E\u00b7wig\u00b7keit", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "So wirkst du in dir aus die echte Milde,", "tokens": ["So", "wirkst", "du", "in", "dir", "aus", "die", "ech\u00b7te", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Die rein von K\u00e4lte bleibt, wie von Begier.", "tokens": ["Die", "rein", "von", "K\u00e4l\u00b7te", "bleibt", ",", "wie", "von", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$,", "PWAV", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Es m\u00fcssen sich erfreun an deinem Bilde,", "tokens": ["Es", "m\u00fcs\u00b7sen", "sich", "er\u00b7freun", "an", "dei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Die d\u00fcrft'ger sind, als du, und alle Schwachen", "tokens": ["Die", "d\u00fcrft'\u00b7ger", "sind", ",", "als", "du", ",", "und", "al\u00b7le", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Beschirmst du treu mit deinem goldnen Schilde.", "tokens": ["Be\u00b7schirmst", "du", "treu", "mit", "dei\u00b7nem", "gold\u00b7nen", "Schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "La\u00df dann die Stolzen deiner Armut lachen,", "tokens": ["La\u00df", "dann", "die", "Stol\u00b7zen", "dei\u00b7ner", "Ar\u00b7mut", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Die ihren Schatz im Jenseits angelegt;", "tokens": ["Die", "ih\u00b7ren", "Schatz", "im", "Jen\u00b7seits", "an\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Du bist doch reich, um viele reich zu machen.", "tokens": ["Du", "bist", "doch", "reich", ",", "um", "vie\u00b7le", "reich", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUI", "PIAT", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "Du hast ein Herz, das frei und innig schl\u00e4gt,", "tokens": ["Du", "hast", "ein", "Herz", ",", "das", "frei", "und", "in\u00b7nig", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "Hast deine Sinne, voll dich zu erquicken,", "tokens": ["Hast", "dei\u00b7ne", "Sin\u00b7ne", ",", "voll", "dich", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Ein Fl\u00fcgelpaar, das dich zum Lichte tr\u00e4gt,", "tokens": ["Ein", "Fl\u00fc\u00b7gel\u00b7paar", ",", "das", "dich", "zum", "Lich\u00b7te", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.55": {"text": "Und Mut, dem Tod ins Angesicht zu blicken.", "tokens": ["Und", "Mut", ",", "dem", "Tod", "ins", "An\u00b7ge\u00b7sicht", "zu", "bli\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kommt an mein Herz, kommt nah heran, ihr Lieben!", "tokens": ["Kommt", "an", "mein", "Herz", ",", "kommt", "nah", "he\u00b7ran", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Jammer, welch ein Raub an mir geschehn,", "tokens": ["Im", "Jam\u00b7mer", ",", "welch", "ein", "Raub", "an", "mir", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAT", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verga\u00df ich, welch ein Reichtum mir geblieben.", "tokens": ["Ver\u00b7ga\u00df", "ich", ",", "welch", "ein", "Reich\u00b7tum", "mir", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAT", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00dft nur des Sturmes erste Wut verwehn,", "tokens": ["La\u00dft", "nur", "des", "Stur\u00b7mes", "ers\u00b7te", "Wut", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann blickt des Himmels ausgeweinte Bl\u00e4ue", "tokens": ["Dann", "blickt", "des", "Him\u00b7mels", "aus\u00b7ge\u00b7wein\u00b7te", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gelassen durch, drin tausend Sterne stehn;", "tokens": ["Ge\u00b7las\u00b7sen", "durch", ",", "drin", "tau\u00b7send", "Ster\u00b7ne", "stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und man gedenkt der Lebenspflicht aufs neue,", "tokens": ["Und", "man", "ge\u00b7denkt", "der", "Le\u00b7bens\u00b7pflicht", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Gr\u00fcfte nicht die Seele zu vergraben,", "tokens": ["In", "Gr\u00fcf\u00b7te", "nicht", "die", "See\u00b7le", "zu", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In frevlem Unma\u00df sehnsuchtsvoller Treue.", "tokens": ["In", "frev\u00b7lem", "Un\u00b7ma\u00df", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sollt' ich der Pflicht zu lang vergessen haben,", "tokens": ["Sollt'", "ich", "der", "Pflicht", "zu", "lang", "ver\u00b7ges\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Vergebt mir's! Dieser Schlag war allzu herbe.", "tokens": ["Ver\u00b7gebt", "mir's", "!", "Die\u00b7ser", "Schlag", "war", "all\u00b7zu", "her\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "PDAT", "NN", "VAFIN", "PTKA", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zu herzlich hing mein Herz an diesem Knaben.", "tokens": ["Zu", "herz\u00b7lich", "hing", "mein", "Herz", "an", "die\u00b7sem", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nun sei die Menschheit meines Lieblings Erbe,", "tokens": ["Nun", "sei", "die", "Menschheit", "mei\u00b7nes", "Lieb\u00b7lings", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Auf da\u00df der Schatz, den ich f\u00fcr ihn gespart,", "tokens": ["Auf", "da\u00df", "der", "Schatz", ",", "den", "ich", "f\u00fcr", "ihn", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "An Liebeskraft nicht herrenlos verderbe.", "tokens": ["An", "Lie\u00b7bes\u00b7kraft", "nicht", "her\u00b7ren\u00b7los", "ver\u00b7der\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sie meinen, wer sich keiner Himmelfahrt,", "tokens": ["Sie", "mei\u00b7nen", ",", "wer", "sich", "kei\u00b7ner", "Him\u00b7mel\u00b7fahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Nicht froher Urst\u00e4nd' will getr\u00f6sten lassen,", "tokens": ["Nicht", "fro\u00b7her", "Ur\u00b7st\u00e4nd'", "will", "ge\u00b7tr\u00f6s\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VMFIN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Dem m\u00fcsse trostlos sein die Gegenwart.", "tokens": ["Dem", "m\u00fcs\u00b7se", "trost\u00b7los", "sein", "die", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "VAINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dies bange Erdenzwielicht m\u00fcss' er hassen,", "tokens": ["Dies", "ban\u00b7ge", "Er\u00b7den\u00b7zwie\u00b7licht", "m\u00fcss'", "er", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Das nicht ein Strahl der Hoffnung je verkl\u00e4re,", "tokens": ["Das", "nicht", "ein", "Strahl", "der", "Hoff\u00b7nung", "je", "ver\u00b7kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und in Verzweiflung werd' er einst erblassen.", "tokens": ["Und", "in", "Ver\u00b7zwei\u00b7flung", "werd'", "er", "einst", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Kommt! Machen wir dem eignen Credo Ehre!", "tokens": ["Kommt", "!", "Ma\u00b7chen", "wir", "dem", "eig\u00b7nen", "Cre\u00b7do", "Eh\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Aufrechten Haupts, nicht trotzig, nicht verzagt,", "tokens": ["Auf\u00b7rech\u00b7ten", "Haupts", ",", "nicht", "trot\u00b7zig", ",", "nicht", "ver\u00b7zagt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Liebt, was da lieblich ist, ertragt das Schwere.", "tokens": ["Liebt", ",", "was", "da", "lieb\u00b7lich", "ist", ",", "er\u00b7tragt", "das", "Schwe\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "O sch\u00e4mt euch nicht, da\u00df ihr in Schmerzen klagt!", "tokens": ["O", "sch\u00e4mt", "euch", "nicht", ",", "da\u00df", "ihr", "in", "Schmer\u00b7zen", "klagt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Es \u00e4chzt der Baum, wenn Ungewitter toben,", "tokens": ["Es", "\u00e4chzt", "der", "Baum", ",", "wenn", "Un\u00b7ge\u00b7wit\u00b7ter", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und kr\u00fcmmt den Wipfel, der so hoch geragt.", "tokens": ["Und", "kr\u00fcmmt", "den", "Wip\u00b7fel", ",", "der", "so", "hoch", "ge\u00b7ragt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dann wieder still von Sonnenglanz umwoben", "tokens": ["Dann", "wie\u00b7der", "still", "von", "Son\u00b7nen\u00b7glanz", "um\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Trinkt er den \u00c4ther, reift der Ernte zu,", "tokens": ["Trinkt", "er", "den", "\u00c4\u00b7ther", ",", "reift", "der", "Ern\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.30": {"text": "Bis ihn die Axt zerspellt zu Scheit und Kloben.", "tokens": ["Bis", "ihn", "die", "Axt", "zer\u00b7spellt", "zu", "Scheit", "und", "Klo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "So, Kind der Erde, f\u00fcge dich auch du", "tokens": ["So", ",", "Kind", "der", "Er\u00b7de", ",", "f\u00fc\u00b7ge", "dich", "auch", "du"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und neide nicht hoff\u00e4rt'gen Himmelsp\u00e4chtern", "tokens": ["Und", "nei\u00b7de", "nicht", "hof\u00b7f\u00e4rt'\u00b7gen", "Him\u00b7mel\u00b7sp\u00e4ch\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJA", "NN"], "me