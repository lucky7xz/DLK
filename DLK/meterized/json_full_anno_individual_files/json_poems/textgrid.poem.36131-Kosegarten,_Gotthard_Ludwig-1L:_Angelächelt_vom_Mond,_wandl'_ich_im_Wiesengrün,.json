{"textgrid.poem.36131": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Angel\u00e4chelt vom Mond, wandl' ich im Wiesengr\u00fcn,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Angel\u00e4chelt vom Mond, wandl' ich im Wiesengr\u00fcn,", "tokens": ["An\u00b7ge\u00b7l\u00e4\u00b7chelt", "vom", "Mond", ",", "wandl'", "ich", "im", "Wie\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wo das fl\u00fcsternde Schilf, wo das bethaute Gras", "tokens": ["Wo", "das", "fl\u00fcs\u00b7tern\u00b7de", "Schilf", ",", "wo", "das", "be\u00b7thau\u00b7te", "Gras"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Frische labende D\u00fcfte", "tokens": ["Fri\u00b7sche", "la\u00b7ben\u00b7de", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In die k\u00fchlere Luft ergeust.", "tokens": ["In", "die", "k\u00fch\u00b7le\u00b7re", "Luft", "er\u00b7geust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O, wie ist mir so wohl! Wonne, die lauterste,", "tokens": ["O", ",", "wie", "ist", "mir", "so", "wohl", "!", "Won\u00b7ne", ",", "die", "lau\u00b7ters\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "wo