{"textgrid.poem.39974": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2. Wie der Probst gestraft wird", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Helfenberger Schlosse,", "tokens": ["Auf", "dem", "Hel\u00b7fen\u00b7ber\u00b7ger", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Thurgau's fettem Thal,", "tokens": ["In", "des", "Thur\u00b7gau's", "fet\u00b7tem", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt der Probst mit edlen Herren,", "tokens": ["Sitzt", "der", "Probst", "mit", "ed\u00b7len", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt beim roten Wein das Mahl.", "tokens": ["H\u00e4lt", "beim", "ro\u00b7ten", "Wein", "das", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber r\u00f6ter als der Wein", "tokens": ["A\u00b7ber", "r\u00f6\u00b7ter", "als", "der", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00e4ngt der Himmel an zu stralen,", "tokens": ["F\u00e4ngt", "der", "Him\u00b7mel", "an", "zu", "stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In den klaren Teichen sehn", "tokens": ["In", "den", "kla\u00b7ren", "Tei\u00b7chen", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie die dunkle Glut sich malen.", "tokens": ["Sie", "die", "dunk\u00b7le", "Glut", "sich", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bu\u00dfnang steht in d\u00fcstern Flammen,", "tokens": ["Bu\u00df\u00b7nang", "steht", "in", "d\u00fcs\u00b7tern", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ke\u00dfwyl's alter Thurm, er raucht,", "tokens": ["Ke\u00df\u00b7wyl's", "al\u00b7ter", "Thurm", ",", "er", "raucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Enn' und B\u00fcrglen gl\u00fchn zusammen,", "tokens": ["Enn'", "und", "B\u00fcrg\u00b7len", "gl\u00fchn", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eins vom andern angehaucht.", "tokens": ["Eins", "vom", "an\u00b7dern", "an\u00b7ge\u00b7haucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Qualm erf\u00fcllt das gr\u00fcne Thal,", "tokens": ["Qualm", "er\u00b7f\u00fcllt", "das", "gr\u00fc\u00b7ne", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer steigt die Flamme heller,", "tokens": ["Im\u00b7mer", "steigt", "die", "Flam\u00b7me", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Fliehen ruft ein Knecht:", "tokens": ["Und", "im", "Flie\u00b7hen", "ruft", "ein", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "\u00bbherr, ach Herr, die Appenzeller!\u00ab", "tokens": ["\u00bb", "herr", ",", "ach", "Herr", ",", "die", "Ap\u00b7pen\u00b7zel\u00b7ler", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "XY", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es hebt der Vogt von Schwendi", "tokens": ["Und", "es", "hebt", "der", "Vogt", "von", "Schwen\u00b7di"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bla\u00df und zitternd sich vom Mahl,", "tokens": ["Bla\u00df", "und", "zit\u00b7ternd", "sich", "vom", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Vogt der Abteszelle", "tokens": ["Und", "der", "Vogt", "der", "Ab\u00b7tes\u00b7zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzet fl\u00fcchtig in den Saal.", "tokens": ["St\u00fcr\u00b7zet", "fl\u00fcch\u00b7tig", "in", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Schlaf ward er gejagt", "tokens": ["Aus", "dem", "Schlaf", "ward", "er", "ge\u00b7jagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem ersten Morgenschimmer,", "tokens": ["Mit", "dem", "ers\u00b7ten", "Mor\u00b7gen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Hirte hinter ihm", "tokens": ["Und", "der", "Hir\u00b7te", "hin\u00b7ter", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPER"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Ri\u00df die Burg in Schutt und Tr\u00fcmmer.", "tokens": ["Ri\u00df", "die", "Burg", "in", "Schutt", "und", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oede wird es an den Tischen,", "tokens": ["O\u00b7e\u00b7de", "wird", "es", "an", "den", "Ti\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu den Waffen ruft der Probst,", "tokens": ["Zu", "den", "Waf\u00b7fen", "ruft", "der", "Probst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ihn warnt ein frommer Ritter:", "tokens": ["Doch", "ihn", "warnt", "ein", "from\u00b7mer", "Rit\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbherr! umsonst ist's, da\u00df du tobst.", "tokens": ["\u00bb", "herr", "!", "um\u00b7sonst", "ist's", ",", "da\u00df", "du", "tobst", "."], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADV", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als du Vater schlugst und Kind,", "tokens": ["Als", "du", "Va\u00b7ter", "schlugst", "und", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auf Menschen hetztest Hunde,", "tokens": ["Und", "auf", "Men\u00b7schen", "hetz\u00b7test", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Brannten deine Burgen schon,", "tokens": ["Brann\u00b7ten", "dei\u00b7ne", "Bur\u00b7gen", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "War gekommen deine Stunde!", "tokens": ["War", "ge\u00b7kom\u00b7men", "dei\u00b7ne", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lege g\u00fctlich dich zum Ziele;", "tokens": ["Le\u00b7ge", "g\u00fct\u00b7lich", "dich", "zum", "Zie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du thatst im Zornesmut,", "tokens": ["Was", "du", "thatst", "im", "Zor\u00b7nes\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00fc\u00dfe mit gelinden Worten,", "tokens": ["B\u00fc\u00b7\u00dfe", "mit", "ge\u00b7lin\u00b7den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kluge Reu' macht vieles gut!\u00ab", "tokens": ["Klu\u00b7ge", "Reu'", "macht", "vie\u00b7les", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "PIS", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zag und trotzig spricht der Probst:", "tokens": ["Zag", "und", "trot\u00b7zig", "spricht", "der", "Probst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbseht Ihr B\u00fcrger von Sankt Gallen?", "tokens": ["\u00bb", "seht", "Ihr", "B\u00fcr\u00b7ger", "von", "Sankt", "Gal\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "APPR", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit den Bauren handl' ich nicht;", "tokens": ["Mit", "den", "Bau\u00b7ren", "handl'", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "B\u00fcrger lass' ich mir gefallen.\u00ab", "tokens": ["B\u00fcr\u00b7ger", "lass'", "ich", "mir", "ge\u00b7fal\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und den Feinden vor der Veste", "tokens": ["Und", "den", "Fein\u00b7den", "vor", "der", "Ves\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut sich auf das alte Thor;", "tokens": ["Thut", "sich", "auf", "das", "al\u00b7te", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd'ge B\u00fcrger von Sankt Gallen", "tokens": ["W\u00fcrd'\u00b7ge", "B\u00fcr\u00b7ger", "von", "Sankt", "Gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringen ihr Begehren vor.", "tokens": ["Brin\u00b7gen", "ihr", "Be\u00b7geh\u00b7ren", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freundlich von dem roten Wein", "tokens": ["Freund\u00b7lich", "von", "dem", "ro\u00b7ten", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schenkt der Probst den ernsten G\u00e4sten;", "tokens": ["Schenkt", "der", "Probst", "den", "erns\u00b7ten", "G\u00e4s\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihnen, nur den Hirten nicht,", "tokens": ["Ih\u00b7nen", ",", "nur", "den", "Hir\u00b7ten", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Uebergiebt er seine Vesten.", "tokens": ["Ue\u00b7berg\u00b7iebt", "er", "sei\u00b7ne", "Ves\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch die schlichten Appenzeller", "tokens": ["Doch", "die", "schlich\u00b7ten", "Ap\u00b7pen\u00b7zel\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trauen ihrem Feinde nicht,", "tokens": ["Trau\u00b7en", "ih\u00b7rem", "Fein\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es gel\u00fcstet sie, zu schauen", "tokens": ["Es", "ge\u00b7l\u00fcs\u00b7tet", "sie", ",", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Gegners Angesicht.", "tokens": ["Ih\u00b7res", "Geg\u00b7ners", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der so vielen Leids gethan,", "tokens": ["Der", "so", "vie\u00b7len", "Leids", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selber wollen sie ihn h\u00f6ren;", "tokens": ["Sel\u00b7ber", "wol\u00b7len", "sie", "ihn", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kam aus seinem Mund der Eid,", "tokens": ["Kam", "aus", "sei\u00b7nem", "Mund", "der", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollen sie ihm Frieden schw\u00f6ren.", "tokens": ["Wol\u00b7len", "sie", "ihm", "Frie\u00b7den", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Als sie zornig die\u00df bedeutet,", "tokens": ["Als", "sie", "zor\u00b7nig", "die\u00df", "be\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut sich auf das alte Thor,", "tokens": ["Thut", "sich", "auf", "das", "al\u00b7te", "Thor", ","