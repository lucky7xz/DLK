["VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "War gekommen deine Stunde!", "tokens": ["War", "ge\u00b7kom\u00b7men", "dei\u00b7ne", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Lege g\u00fctlich dich zum Ziele;", "tokens": ["Le\u00b7ge", "g\u00fct\u00b7lich", "dich", "zum", "Zie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du thatst im Zornesmut,", "tokens": ["Was", "du", "thatst", "im", "Zor\u00b7nes\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00fc\u00dfe mit gelinden Worten,", "tokens": ["B\u00fc\u00b7\u00dfe", "mit", "ge\u00b7lin\u00b7den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kluge Reu' macht vieles gut!\u00ab", "tokens": ["Klu\u00b7ge", "Reu'", "macht", "vie\u00b7les", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "VVFIN", "PIS", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zag und trotzig spricht der Probst:", "tokens": ["Zag", "und", "trot\u00b7zig", "spricht", "der", "Probst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbseht Ihr B\u00fcrger von Sankt Gallen?", "tokens": [