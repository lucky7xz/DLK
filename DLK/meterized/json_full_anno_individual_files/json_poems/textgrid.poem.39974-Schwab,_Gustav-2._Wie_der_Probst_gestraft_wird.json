}, "stanza.9": {"line.1": {"text": "Und die Schar betr\u00fcbter Ritter", "tokens": ["Und", "die", "Schar", "be\u00b7tr\u00fcb\u00b7ter", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehet stille mit ihm aus.", "tokens": ["Zie\u00b7het", "stil\u00b7le", "mit", "ihm", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch der Hirte schwur ihm redlich,", "tokens": ["Auch", "der", "Hir\u00b7te", "schwur", "ihm", "red\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wandelt ohne Groll nach Haus.", "tokens": ["Wan\u00b7delt", "oh\u00b7ne", "Groll", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einsam, aufrecht steht die Burg", "tokens": ["Ein\u00b7sam", ",", "auf\u00b7recht", "steht", "die", "Burg"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwischen den verheerten Auen,", "tokens": ["Zwi\u00b7schen", "den", "ver\u00b7heer\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Darf, geschirmt von M\u00e4nnereid,", "tokens": ["Darf", ",", "ge\u00b7schirmt", "von", "M\u00e4n\u00b7ne\u00b7reid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"