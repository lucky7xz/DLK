{"dta.poem.13512": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Lindes Rauschen in den Wipfeln,", "tokens": ["Lin\u00b7des", "Rau\u00b7schen", "in", "den", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6glein, die ihr fernab fliegt,", "tokens": ["V\u00f6\u00b7glein", ",", "die", "ihr", "fern\u00b7ab", "fliegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Bronnen von den stillen Gipfeln,", "tokens": ["Bron\u00b7nen", "von", "den", "stil\u00b7len", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt, wo meine Heimath liegt?", "tokens": ["Sagt", ",", "wo", "mei\u00b7ne", "Hei\u00b7math", "liegt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heut' im Traum sah ich sie wieder,", "tokens": ["Heut'", "im", "Traum", "sah", "ich", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von allen Bergen ging", "tokens": ["Und", "von", "al\u00b7len", "Ber\u00b7gen", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solches Gr\u00fc\u00dfen zu mir nieder,", "tokens": ["Sol\u00b7ches", "Gr\u00fc\u00b7\u00dfen", "zu", "mir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich an zu weinen fing.", "tokens": ["Da\u00df", "ich", "an", "zu", "wei\u00b7nen", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, hier auf den fremden Gipfeln:", "tokens": ["Ach", ",", "hier", "auf", "den", "frem\u00b7den", "Gip\u00b7feln", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschen, Quellen, Fels und Baum,", "tokens": ["Men\u00b7schen", ",", "Quel\u00b7len", ",", "Fels", "und", "Baum", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirres Rauschen in den Wipfeln, \u2014", "tokens": ["Wir\u00b7res", "Rau\u00b7schen", "in", "den", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles ist mir wie ein Traum.", "tokens": ["Al\u00b7les", "ist", "mir", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}