{"dta.poem.2782": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Siebende Capitel Hiobs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat nicht der arme Mensch mit Feinden stets zu thun!", "tokens": ["Hat", "nicht", "der", "ar\u00b7me", "Mensch", "mit", "Fein\u00b7den", "stets", "zu", "thun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mu\u00df er doch im Zanck und argem Streite leben.", "tokens": ["Wie", "mu\u00df", "er", "doch", "im", "Zanck", "und", "ar\u00b7gem", "Strei\u00b7te", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPRART", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier hat ihn Fleisch und Blut verr\u00e4therisch umbgeben/", "tokens": ["Hier", "hat", "ihn", "Fleisch", "und", "Blut", "ver\u00b7r\u00e4\u00b7the\u00b7risch", "umb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort kan er vor dem Feind der Finstern\u00fc\u00df nicht ruhn.", "tokens": ["Dort", "kan", "er", "vor", "dem", "Feind", "der", "Fins\u00b7ter\u00b7n\u00fc\u00df", "nicht", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Welt tritt auch mit an und seine gantze Tage", "tokens": ["Die", "Welt", "tritt", "auch", "mit", "an", "und", "sei\u00b7ne", "gant\u00b7ze", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PTKVZ", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sind Tagel\u00f6hnern gleich/ nie ohne M\u00fch und Plage.", "tokens": ["Sind", "Ta\u00b7ge\u00b7l\u00f6h\u00b7nern", "gleich", "/", "nie", "oh\u00b7ne", "M\u00fch", "und", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$(", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie sehnlich w\u00fcnscht ein Knecht des Abends-Schatten Streiff/", "tokens": ["Wie", "sehn\u00b7lich", "w\u00fcnscht", "ein", "Knecht", "des", "A\u00b7bends\u00b7Schat\u00b7ten", "Streiff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Tagel\u00f6hner hofft der schweren Arbeit Ende.", "tokens": ["Ein", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ner", "hofft", "der", "schwe\u00b7ren", "Ar\u00b7beit", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie hab ich doch umbsonst die Wercke meiner H\u00e4nde/", "tokens": ["Wie", "hab", "ich", "doch", "um\u00b7bsonst", "die", "Wer\u00b7cke", "mei\u00b7ner", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nach Monaten erf\u00fcllt; elender N\u00e4chte Reiff", "tokens": ["Nach", "Mo\u00b7na\u00b7ten", "er\u00b7f\u00fcllt", ";", "e\u00b7len\u00b7der", "N\u00e4ch\u00b7te", "Reiff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$.", "ADJA", "NN", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Und rauhe Witterung vergebens ausgestanden/", "tokens": ["Und", "rau\u00b7he", "Wit\u00b7te\u00b7rung", "ver\u00b7ge\u00b7bens", "aus\u00b7ge\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil keine Linderung der Trangsal war vorhanden.", "tokens": ["Weil", "kei\u00b7ne", "Lin\u00b7de\u00b7rung", "der", "Tran\u00b7gsal", "war", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn ich mich erst gelegt/ so sprach ich schon bey mir:", "tokens": ["Wenn", "ich", "mich", "erst", "ge\u00b7legt", "/", "so", "sprach", "ich", "schon", "bey", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$(", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn wach ich wieder auff/ und geh aus meinem Bette?", "tokens": ["Wenn", "wach", "ich", "wie\u00b7der", "auff", "/", "und", "geh", "aus", "mei\u00b7nem", "Bet\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADV", "APPR", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dann rechnet ich genau/ wie viel ich Stunden h\u00e4tte/", "tokens": ["Dann", "rech\u00b7net", "ich", "ge\u00b7nau", "/", "wie", "viel", "ich", "Stun\u00b7den", "h\u00e4t\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "PWAV", "PIS", "PPER", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Eh als die Nacht einfiel; denn ich zum Scheusel schier", "tokens": ["Eh", "als", "die", "Nacht", "ein\u00b7fiel", ";", "denn", "ich", "zum", "Scheu\u00b7sel", "schier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN", "$.", "KON", "PPER", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bey lichtem Tage war/ bi\u00df Finstern\u00fc\u00df entstunde/", "tokens": ["Bey", "lich\u00b7tem", "Ta\u00b7ge", "war", "/", "bi\u00df", "Fins\u00b7ter\u00b7n\u00fc\u00df", "ent\u00b7stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$(", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und mich der bl\u00f6den Angst die Dunckelheit entbunde.", "tokens": ["Und", "mich", "der", "bl\u00f6\u00b7den", "Angst", "die", "Dun\u00b7ckel\u00b7heit", "ent\u00b7bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein Fleisch ist umb und umb durchfressen durch den Wurm/", "tokens": ["Mein", "Fleisch", "ist", "umb", "und", "umb", "durch\u00b7fres\u00b7sen", "durch", "den", "Wurm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "KON", "KOUI", "VVIZU", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und kothicher Gestanck klebt auf den d\u00fcrren Knochen.", "tokens": ["Und", "ko\u00b7thi\u00b7cher", "Ge\u00b7stanck", "klebt", "auf", "den", "d\u00fcr\u00b7ren", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Haut ist Eiter voll und schrumpffich eingekrochen;", "tokens": ["Die", "Haut", "ist", "Ei\u00b7ter", "voll", "und", "schrum\u00b7pf\u00b7fich", "ein\u00b7ge\u00b7kro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ADJD", "KON", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Ja ich bin gantz zernicht. O grauser Ungl\u00fccks-Sturm!", "tokens": ["Ja", "ich", "bin", "gantz", "zer\u00b7nicht", ".", "O", "grau\u00b7ser", "Un\u00b7gl\u00fccks\u00b7Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "ADV", "VVPP", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und meine Tage sind so schnell dahin geflogen/", "tokens": ["Und", "mei\u00b7ne", "Ta\u00b7ge", "sind", "so", "schnell", "da\u00b7hin", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als wie ein Weberspul wird pl\u00f6tzlich durchgezogen.", "tokens": ["Als", "wie", "ein", "We\u00b7ber\u00b7spul", "wird", "pl\u00f6tz\u00b7lich", "durch\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Weil da kein Halten war/ so giengen sie darvon.", "tokens": ["Weil", "da", "kein", "Hal\u00b7ten", "war", "/", "so", "gien\u00b7gen", "sie", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Gedencke/ da\u00df mein Rest des Lebens Wind ist worden/", "tokens": ["Ge\u00b7den\u00b7cke", "/", "da\u00df", "mein", "Rest", "des", "Le\u00b7bens", "Wind", "ist", "wor\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN", "VAFIN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und meine Augen nicht mehr kommen zu dem Orden", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "nicht", "mehr", "kom\u00b7men", "zu", "dem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der di\u00df was gut/ beschaut. Es sieht mich ohne Hohn", "tokens": ["Der", "di\u00df", "was", "gut", "/", "be\u00b7schaut", ".", "Es", "sieht", "mich", "oh\u00b7ne", "Hohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PDS", "PRELS", "ADJD", "$(", "VVPP", "$.", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kein lebend Auge mehr/ und kan mich auch nicht sehen:", "tokens": ["Kein", "le\u00b7bend", "Au\u00b7ge", "mehr", "/", "und", "kan", "mich", "auch", "nicht", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "ADV", "$(", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du aber siehest mich. Was wird mir noch geschehen.", "tokens": ["Du", "a\u00b7ber", "sie\u00b7hest", "mich", ".", "Was", "wird", "mir", "noch", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wolcke die vergeht und f\u00e4hret fl\u00fcchtig hin;", "tokens": ["Die", "Wol\u00b7cke", "die", "ver\u00b7geht", "und", "f\u00e4h\u00b7ret", "fl\u00fcch\u00b7tig", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wer in schwartzen Pful der tieffen H\u00f6llen st\u00fcrtzet/", "tokens": ["So", "wer", "in", "schwart\u00b7zen", "Pful", "der", "tief\u00b7fen", "H\u00f6l\u00b7len", "st\u00fcrt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Demselben wird der Weg zu uns herauff verk\u00fcrtzet;", "tokens": ["Dem\u00b7sel\u00b7ben", "wird", "der", "Weg", "zu", "uns", "her\u00b7auff", "ver\u00b7k\u00fcrt\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er kommt nicht wiederumb in Stell und Ort/ darinn", "tokens": ["Er", "kommt", "nicht", "wie\u00b7de\u00b7rumb", "in", "Stell", "und", "Ort", "/", "da\u00b7rinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$(", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er vor gewesen ist. Sein Hau\u00df wird ihn nicht kennen/", "tokens": ["Er", "vor", "ge\u00b7we\u00b7sen", "ist", ".", "Sein", "Hau\u00df", "wird", "ihn", "nicht", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "VAPP", "VAFIN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein Eigenthum nicht mehr wie vor/ Besitzer nennen.", "tokens": ["Sein", "Ei\u00b7gen\u00b7thum", "nicht", "mehr", "wie", "vor", "/", "Be\u00b7sit\u00b7zer", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "KOKOM", "APPR", "$(", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drumb wil ich meinem Mund nicht wehren/ weil er kan/", "tokens": ["Drumb", "wil", "ich", "mei\u00b7nem", "Mund", "nicht", "weh\u00b7ren", "/", "weil", "er", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$(", "KOUS", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die heisse Hertzens-Angst beweglich auszusagen/", "tokens": ["Die", "heis\u00b7se", "Hert\u00b7zens\u00b7Angst", "be\u00b7weg\u00b7lich", "aus\u00b7zu\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Seelen Jammer-Leyd dem HErren f\u00fcrzutragen.", "tokens": ["Der", "See\u00b7len", "Jam\u00b7mer\u00b7Leyd", "dem", "Her\u00b7ren", "f\u00fcr\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! bin ich denn ein Meer? Trag ich denn Schuppen an", "tokens": ["Ach", "!", "bin", "ich", "denn", "ein", "Meer", "?", "Trag", "ich", "denn", "Schup\u00b7pen", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "ART", "NN", "$.", "NN", "PPER", "ADV", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df du mich so verwahrst? Wenn ich bey mir gedachte", "tokens": ["Da\u00df", "du", "mich", "so", "ver\u00b7wahrst", "?", "Wenn", "ich", "bey", "mir", "ge\u00b7dach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$.", "KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Jm Bette Trost zu hol\u2019n; Wenn ich mein Lager machte", "tokens": ["Jm", "Bet\u00b7te", "Trost", "zu", "hol'n", ";", "Wenn", "ich", "mein", "La\u00b7ger", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "PTKZU", "VVINF", "$.", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zur Schmertz/ Erleichterung/ wenn ich mich selbst besprach/", "tokens": ["Zur", "Schmertz", "/", "Er\u00b7leich\u00b7te\u00b7rung", "/", "wenn", "ich", "mich", "selbst", "be\u00b7sprach", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "$(", "KOUS", "PPER", "PRF", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schreckst du mich die Nacht mit ungeheuren Tr\u00e4umen/", "tokens": ["So", "schreckst", "du", "mich", "die", "Nacht", "mit", "un\u00b7ge\u00b7heu\u00b7ren", "Tr\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df meine Seele w\u00fcnscht erhenckt zu seyn an B\u00e4umen/", "tokens": ["Da\u00df", "mei\u00b7ne", "See\u00b7le", "w\u00fcnscht", "er\u00b7henckt", "zu", "seyn", "an", "B\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "VVPP", "PTKZU", "VAINF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mein Gebein der Tod tr\u00fcg an der Ketten nach.", "tokens": ["Und", "mein", "Ge\u00b7bein", "der", "Tod", "tr\u00fcg", "an", "der", "Ket\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wil und kan nicht mehr/ erz\u00f6rnter Sch\u00f6pffer leben/", "tokens": ["Ich", "wil", "und", "kan", "nicht", "mehr", "/", "er\u00b7z\u00f6rn\u00b7ter", "Sch\u00f6pf\u00b7fer", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADV", "$(", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "H\u00f6r auff von mir/ h\u00f6r auff! wer kan dir widerstreben?", "tokens": ["H\u00f6r", "auff", "von", "mir", "/", "h\u00f6r", "auff", "!", "wer", "kan", "dir", "wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPER", "$(", "VVFIN", "PTKVZ", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Denn meine Tage sind vergebens hingewischt.", "tokens": ["Denn", "mei\u00b7ne", "Ta\u00b7ge", "sind", "ver\u00b7ge\u00b7bens", "hin\u00b7ge\u00b7wischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist ein Mensch/ O HErr/ da\u00df du ihn so gro\u00df achtest/", "tokens": ["Was", "ist", "ein", "Mensch", "/", "O", "Herr", "/", "da\u00df", "du", "ihn", "so", "gro\u00df", "ach\u00b7test", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$(", "NE", "NN", "$(", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und mit Bek\u00fcmmerung nach seinen Thaten trachtest/", "tokens": ["Und", "mit", "Be\u00b7k\u00fcm\u00b7me\u00b7rung", "nach", "sei\u00b7nen", "Tha\u00b7ten", "trach\u00b7test", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du suchst ihn t\u00e4glich heim/ sein Leyd wird stets erfrischt.", "tokens": ["Du", "suchst", "ihn", "t\u00e4g\u00b7lich", "heim", "/", "sein", "Leyd", "wird", "stets", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach warumb thust du dich doch nicht von meiner Seiten!", "tokens": ["Ach", "wa\u00b7rumb", "thust", "du", "dich", "doch", "nicht", "von", "mei\u00b7ner", "Sei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "L\u00e4\u00dft du denn gantz nicht ab/ und h\u00f6rst nicht auff zu streiten/", "tokens": ["L\u00e4\u00dft", "du", "denn", "gantz", "nicht", "ab", "/", "und", "h\u00f6rst", "nicht", "auff", "zu", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "PTKVZ", "$(", "KON", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Bi\u00df ich den Speichel schling? Hab ich ges\u00fcndigt je/", "tokens": ["Bi\u00df", "ich", "den", "Spei\u00b7chel", "schling", "?", "Hab", "ich", "ge\u00b7s\u00fcn\u00b7digt", "je", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "NN", "PPER", "VVPP", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was soll ich dir denn mehr/ du Menschen-H\u00fcter leisten?", "tokens": ["Was", "soll", "ich", "dir", "denn", "mehr", "/", "du", "Men\u00b7schen\u00b7H\u00fc\u00b7ter", "leis\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ADV", "ADV", "$(", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warumb verursachst du/ da\u00df ich auf dich am meisten", "tokens": ["Wa\u00b7rumb", "ver\u00b7ur\u00b7sachst", "du", "/", "da\u00df", "ich", "auf", "dich", "am", "meis\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "APPR", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sto\u00df/ und mir selbsten bin die Lastung meiner M\u00fch?", "tokens": ["Sto\u00df", "/", "und", "mir", "selbs\u00b7ten", "bin", "die", "Las\u00b7tung", "mei\u00b7ner", "M\u00fch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "PPER", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Warumb vergibst du mir nicht meine Missethaten/", "tokens": ["Wa\u00b7rumb", "ver\u00b7gibst", "du", "mir", "nicht", "mei\u00b7ne", "Mis\u00b7se\u00b7tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nimmst die S\u00fcnde weg in die ich bin gerathen?", "tokens": ["Und", "nimmst", "die", "S\u00fcn\u00b7de", "weg", "in", "die", "ich", "bin", "ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Denn nunmehr scharr ich mich tieff in die Erd hinein/", "tokens": ["Denn", "nun\u00b7mehr", "scharr", "ich", "mich", "tieff", "in", "die", "Erd", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da wil ich meine Ruh nach so viel Ungl\u00fcck hoffen/", "tokens": ["Da", "wil", "ich", "mei\u00b7ne", "Ruh", "nach", "so", "viel", "Un\u00b7gl\u00fcck", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df ist der beste Tausch so ich jemals getroffen/", "tokens": ["Di\u00df", "ist", "der", "bes\u00b7te", "Tausch", "so", "ich", "je\u00b7mals", "ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und wenn man mich denn sucht bey fr\u00fchem Morgenschein/", "tokens": ["Und", "wenn", "man", "mich", "denn", "sucht", "bey", "fr\u00fc\u00b7hem", "Mor\u00b7gen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird noch Stumpff noch Stiel auf dieser weiten Erden/", "tokens": ["So", "wird", "noch", "Stumpff", "noch", "Stiel", "auf", "die\u00b7ser", "wei\u00b7ten", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "ADV", "NN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von mir bedr\u00e4ngtem Mann gefunden k\u00f6nnen werden.", "tokens": ["Von", "mir", "be\u00b7dr\u00e4ng\u00b7tem", "Mann", "ge\u00b7fun\u00b7den", "k\u00f6n\u00b7nen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "VVPP", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}