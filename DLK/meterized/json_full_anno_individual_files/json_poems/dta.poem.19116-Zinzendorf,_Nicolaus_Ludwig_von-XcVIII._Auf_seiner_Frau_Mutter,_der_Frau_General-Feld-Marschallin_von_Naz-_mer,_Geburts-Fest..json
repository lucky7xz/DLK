{"dta.poem.19116": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XcVIII.  Auf seiner Frau Mutter, der Frau  \n General-Feld-Marschallin von Naz-  \n mer, Geburts-Fest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich diese Tage \u00fcber Nachricht \u00fcberkommen mu\u00df,", "tokens": ["Da", "ich", "die\u00b7se", "Ta\u00b7ge", "\u00fc\u00b7ber", "Nach\u00b7richt", "\u00fc\u00b7ber\u00b7kom\u00b7men", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df in dieser Monaths-Zeit Euer Gnaden aufgelebet,", "tokens": ["Da\u00df", "in", "die\u00b7ser", "Mo\u00b7naths\u00b7Zeit", "Eu\u00b7er", "Gna\u00b7den", "auf\u00b7ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ich ausgeschlossen bleibe vom Pers\u00f6hnlichen Genu\u00df:", "tokens": ["Und", "ich", "aus\u00b7ge\u00b7schlos\u00b7sen", "blei\u00b7be", "vom", "Per\u00b7s\u00f6hn\u00b7li\u00b7chen", "Ge\u00b7nu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ist es billig, da\u00df mein Hertz sich zum Thron der Krafft erhebet.", "tokens": ["Ist", "es", "bil\u00b7lig", ",", "da\u00df", "mein", "Hertz", "sich", "zum", "Thron", "der", "Krafft", "er\u00b7he\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Jesus, der getreue Heyland, mache ", "tokens": ["Je\u00b7sus", ",", "der", "ge\u00b7treu\u00b7e", "Hey\u00b7land", ",", "ma\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Wie die Tage der Geliebten, die er St\u00fcndlich benedeyt.", "tokens": ["Wie", "die", "Ta\u00b7ge", "der", "Ge\u00b7lieb\u00b7ten", ",", "die", "er", "St\u00fcnd\u00b7lich", "be\u00b7ne\u00b7deyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ich bezeuge vor dem HErrn: Meine Seel ist voll Verlangen", "tokens": ["Ich", "be\u00b7zeu\u00b7ge", "vor", "dem", "Herrn", ":", "Mei\u00b7ne", "Seel", "ist", "voll", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Jhre Gegend zu besuchen. M\u00f6cht ich nur ein einig mahl", "tokens": ["Ih\u00b7re", "Ge\u00b7gend", "zu", "be\u00b7su\u00b7chen", ".", "M\u00f6cht", "ich", "nur", "ein", "ei\u00b7nig", "mahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$.", "VMFIN", "PPER", "ADV", "ART", "ADJD", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Euer Gnaden Augen sehen: m\u00f6cht ich ihre Knie umfangen;", "tokens": ["Eu\u00b7er", "Gna\u00b7den", "Au\u00b7gen", "se\u00b7hen", ":", "m\u00f6cht", "ich", "ih\u00b7re", "Knie", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "M\u00f6chte die Erstaunungs-werthe, die sichtbare Gnaden-Wahl,", "tokens": ["M\u00f6ch\u00b7te", "die", "Er\u00b7stau\u00b7nungs\u00b7wert\u00b7he", ",", "die", "sicht\u00b7ba\u00b7re", "Gna\u00b7den\u00b7Wahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Uber mich, mein gantzes Hau\u00df und mein Volck, noch auf der", "tokens": ["U\u00b7ber", "mich", ",", "mein", "gant\u00b7zes", "Hau\u00df", "und", "mein", "Volck", ",", "noch", "auf", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,", "ADV", "APPR", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Von der theuresten Mama Freuden-voll gesehen werden!", "tokens": ["Von", "der", "theu\u00b7res\u00b7ten", "Ma\u00b7ma", "Freu\u00b7den\u00b7voll", "ge\u00b7se\u00b7hen", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVPP", "VAINF", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Hier ist Hertz und Feder Eins! m\u00f6cht ich ihren Hertzen n\u00e4her,", "tokens": ["Hier", "ist", "Hertz", "und", "Fe\u00b7der", "Eins", "!", "m\u00f6cht", "ich", "ih\u00b7ren", "Hert\u00b7zen", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Jhrer Liebe, ihrer Treue \u00fcberzeugt zu Dienste stehn,", "tokens": ["Ih\u00b7rer", "Lie\u00b7be", ",", "ih\u00b7rer", "Treu\u00b7e", "\u00fc\u00b7berz\u00b7eugt", "zu", "Diens\u00b7te", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00f6cht ich ihr zur Freude seyn! HErr, du weists, du klarer", "tokens": ["M\u00f6cht", "ich", "ihr", "zur", "Freu\u00b7de", "seyn", "!", "Herr", ",", "du", "weists", ",", "du", "kla\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPRART", "NN", "VAINF", "$.", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "ADJA"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "M\u00f6cht ich lauter solche Wege, die ihr Geist erkennet, gehn;", "tokens": ["M\u00f6cht", "ich", "lau\u00b7ter", "sol\u00b7che", "We\u00b7ge", ",", "die", "ihr", "Geist", "er\u00b7ken\u00b7net", ",", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Nimm dir meinen gantzen Willen, nimm dir meine Kr\u00e4ffte hin,", "tokens": ["Nimm", "dir", "mei\u00b7nen", "gant\u00b7zen", "Wil\u00b7len", ",", "nimm", "dir", "mei\u00b7ne", "Kr\u00e4ff\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und bereite um und an mir alles recht nach deinem Sinn!", "tokens": ["Und", "be\u00b7rei\u00b7te", "um", "und", "an", "mir", "al\u00b7les", "recht", "nach", "dei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "APPR", "PPER", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und die auserwehlte Frau, welche mich zur Welt gebohren,", "tokens": ["Und", "die", "au\u00b7ser\u00b7wehl\u00b7te", "Frau", ",", "wel\u00b7che", "mich", "zur", "Welt", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Deren Mutter ich gebrochen, und dir aufgeopffert bin,", "tokens": ["De\u00b7ren", "Mut\u00b7ter", "ich", "ge\u00b7bro\u00b7chen", ",", "und", "dir", "auf\u00b7ge\u00b7opf\u00b7fert", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVPP", "$,", "KON", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Bleibe dir zu deinem Prei\u00df und Belustigung erkohren,", "tokens": ["Blei\u00b7be", "dir", "zu", "dei\u00b7nem", "Prei\u00df", "und", "Be\u00b7lus\u00b7ti\u00b7gung", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Nimm sie, auserwehlter Heyland, deinem Hertzen zum Gewinn.", "tokens": ["Nimm", "sie", ",", "au\u00b7ser\u00b7wehl\u00b7ter", "Hey\u00b7land", ",", "dei\u00b7nem", "Hert\u00b7zen", "zum", "Ge\u00b7winn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}