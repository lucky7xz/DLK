{"textgrid.poem.46456": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ueber Wiesen, die am Stadtrand liegen,", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber Wiesen, die am Stadtrand liegen,", "tokens": ["Ue\u00b7ber", "Wie\u00b7sen", ",", "die", "am", "Stadt\u00b7rand", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Geh ich mit erfreuten Sinnen hin,", "tokens": ["Geh", "ich", "mit", "er\u00b7freu\u00b7ten", "Sin\u00b7nen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Drosseln schn\u00e4rren \u2013 Wolken fliegen \u2013", "tokens": ["Dros\u00b7seln", "schn\u00e4r\u00b7ren", "\u2013", "Wol\u00b7ken", "flie\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Sausewind rauschen, biegen", "tokens": ["Im", "Sau\u00b7se\u00b7wind", "rau\u00b7schen", ",", "bie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sich die Hecken \u2013 gr\u00fcne Gr\u00e4ser wogen, wiegen \u2013", "tokens": ["Sich", "die", "He\u00b7cken", "\u2013", "gr\u00fc\u00b7ne", "Gr\u00e4\u00b7ser", "wo\u00b7gen", ",", "wie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF",