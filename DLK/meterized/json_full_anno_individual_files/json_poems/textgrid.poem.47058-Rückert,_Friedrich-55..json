{"textgrid.poem.47058": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann still die Nacht auf dunkeln Pfaden schreitet,", "tokens": ["Wann", "still", "die", "Nacht", "auf", "dun\u00b7keln", "Pfa\u00b7den", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die unterm Mantel tr\u00e4gt die goldnen Sterne,", "tokens": ["Die", "un\u00b7term", "Man\u00b7tel", "tr\u00e4gt", "die", "gold\u00b7nen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und im Gew\u00f6lk gleich heimlicher Laterne", "tokens": ["Und", "im", "Ge\u00b7w\u00f6lk", "gleich", "heim\u00b7li\u00b7cher", "La\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mond sein wachsend Silberlicht bereitet;", "tokens": ["Der", "Mond", "sein", "wach\u00b7send", "Sil\u00b7ber\u00b7licht", "be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denk' ich, und meines Auges Thr\u00e4ne gleitet,", "tokens": ["Denk'", "ich", ",", "und", "mei\u00b7nes", "Au\u00b7ges", "Thr\u00e4\u00b7ne", "glei\u00b7tet", ","], "token_info": ["word", "word"