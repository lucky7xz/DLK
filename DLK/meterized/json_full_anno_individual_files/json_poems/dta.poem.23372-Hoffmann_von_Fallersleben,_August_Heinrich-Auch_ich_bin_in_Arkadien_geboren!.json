{"dta.poem.23372": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auch ich bin in Arkadien geboren!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur Europa hat Geschichte,", "tokens": ["Nur", "Eu\u00b7ro\u00b7pa", "hat", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat noch Sagen und Gedichte.", "tokens": ["Hat", "noch", "Sa\u00b7gen", "und", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprecht, in welchem Erdenwinkel", "tokens": ["Sprecht", ",", "in", "wel\u00b7chem", "Er\u00b7den\u00b7win\u00b7kel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebt es soviel Poesie?", "tokens": ["Giebt", "es", "so\u00b7viel", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Von Geschlechte zu Geschlechte", "tokens": ["Von", "Ge\u00b7schlech\u00b7te", "zu", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erben fort die V\u00f6lkerrechte,", "tokens": ["Er\u00b7ben", "fort", "die", "V\u00f6l\u00b7ker\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die V\u00f6lker und die Rechte,", "tokens": ["Und", "die", "V\u00f6l\u00b7ker", "und", "die", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles ist nur Poesie!", "tokens": ["Al\u00b7les", "ist", "nur", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alle F\u00f6derationen,", "tokens": ["Al\u00b7le", "F\u00f6\u00b7de\u00b7ra\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friedensschl\u00fcss' und Conventionen \u2014", "tokens": ["Frie\u00b7dens\u00b7schl\u00fcss'", "und", "Con\u00b7ven\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Fragt die ganze Weltgeschichte,", "tokens": ["Fragt", "die", "gan\u00b7ze", "Welt\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nicht Alles Poesie?", "tokens": ["Ist", "nicht", "Al\u00b7les", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und die herrlichsten Congresse", "tokens": ["Und", "die", "herr\u00b7lichs\u00b7ten", "Con\u00b7gres\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.2": {"text": "Nur aus reinem Volksinteresse \u2014", "tokens": ["Nur", "aus", "rei\u00b7nem", "Volks\u00b7in\u00b7ter\u00b7es\u00b7se"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ward nicht diese nackte Prosa", "tokens": ["Ward", "nicht", "die\u00b7se", "nack\u00b7te", "Pro\u00b7sa"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PDAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "L\u00e4ngst zur schmucken Poesie?", "tokens": ["L\u00e4ngst", "zur", "schmu\u00b7cken", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und die Proclamationen", "tokens": ["Und", "die", "Pro\u00b7cla\u00b7ma\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Constitutionen \u2014", "tokens": ["Und", "die", "Con\u00b7sti\u00b7tu\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War nicht Alles von dem Anfang", "tokens": ["War", "nicht", "Al\u00b7les", "von", "dem", "An\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Ende Poesie?", "tokens": ["Bis", "zum", "En\u00b7de", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und die f\u00fcrstlichen Versprechen,", "tokens": ["Und", "die", "f\u00fcrst\u00b7li\u00b7chen", "Ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abzuthun die Staatsgebrechen \u2014", "tokens": ["Ab\u00b7zu\u00b7thun", "die", "Staats\u00b7ge\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannten je die alten Heiden", "tokens": ["Kann\u00b7ten", "je", "die", "al\u00b7ten", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine solche Poesie?", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Unser Adel ohne Ende,", "tokens": ["Un\u00b7ser", "A\u00b7del", "oh\u00b7ne", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre R\u00e4ng' und unsre St\u00e4nde \u2014", "tokens": ["Uns\u00b7re", "R\u00e4ng'", "und", "uns\u00b7re", "St\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hatten wohl die Patriarchen", "tokens": ["Hat\u00b7ten", "wohl", "die", "Pat\u00b7ri\u00b7ar\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kindlichere Poesie?", "tokens": ["Kind\u00b7li\u00b7che\u00b7re", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Unser ganzes Sein und Leben,", "tokens": ["Un\u00b7ser", "gan\u00b7zes", "Sein", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Hoffen, unser Streben \u2014", "tokens": ["Un\u00b7ser", "Hof\u00b7fen", ",", "un\u00b7ser", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward nicht Alles, ist nicht Alles,", "tokens": ["Ward", "nicht", "Al\u00b7les", ",", "ist", "nicht", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIS", "$,", "VAFIN", "PTKNEG", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, Alles Poesie?", "tokens": ["Al\u00b7les", ",", "Al\u00b7les", "Poe\u00b7sie", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}