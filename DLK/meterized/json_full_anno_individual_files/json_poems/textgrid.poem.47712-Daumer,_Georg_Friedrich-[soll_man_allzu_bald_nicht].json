chaic.tri"}}, "stanza.6": {"line.1": {"text": "Sieh, da ist der Sch\u00f6ne,", "tokens": ["Sieh", ",", "da", "ist", "der", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Welchen ich begehre,", "tokens": ["Wel\u00b7chen", "ich", "be\u00b7geh\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit dem Garn zur Stelle,", "tokens": ["Mit", "dem", "Garn", "zur", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wirft es in die Welle,", "tokens": ["Wirft", "es", "in", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lauschet in die See,", "tokens": ["Lau\u00b7schet", "in", "die", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Meint, er hab' ein Hechtlein,", "tokens": ["Meint", ",", "er", "hab'", "ein", "Hecht\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Schw\u00e4rzlich anzuschauen,", "tokens": ["Schw\u00e4rz\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Doch es ist ein Wesen", "tokens": ["Doch", "es", "ist", "ein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Stammend aus der Frauen", "tokens": ["Stam\u00b7mend", "aus", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Zierlichem Geschlechtlein;", "tokens": ["Zier\u00b7li\u00b7chem", "Ge\u00b7schlecht\u00b7lein", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Eingekr\u00fcmt im Netzlein", "tokens": ["Ein\u00b7ge\u00b7kr\u00fcmt", "im", "Ne\u00b7tzlein"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Liegt ein Minnesch\u00e4tzlein,", "tokens": ["Liegt", "ein", "Min\u00b7ne\u00b7sch\u00e4tz\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Lugt mit hellen Augen", "tokens": ["Lugt", "mit", "hel\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Schelmisch in die H\u00f6h'.", "tokens": ["Schel\u00b7misch", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}