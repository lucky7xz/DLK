{"textgrid.poem.39688": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "3. Adonis Todtenfeyer", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehe! da\u00df der Gott auf Erden", "tokens": ["We\u00b7he", "!", "da\u00df", "der", "Gott", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterblich mu\u00dft gebohren werden!", "tokens": ["Sterb\u00b7lich", "mu\u00dft", "ge\u00b7boh\u00b7ren", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Dasein, alles Leben", "tokens": ["Al\u00b7les", "Da\u00b7sein", ",", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mit ihm dem Tod gegeben.", "tokens": ["Ist", "mit", "ihm", "dem", "Tod", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles wandelt und vergehet,", "tokens": ["Al\u00b7les", "wan\u00b7delt", "und", "ver\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Morgen sinkt was heute stehet;", "tokens": ["Mor\u00b7gen", "sinkt", "was", "heu\u00b7te", "ste\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PWS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was jetzt sch\u00f6n und herrlich steiget,", "tokens": ["Was", "jetzt", "sch\u00f6n", "und", "herr\u00b7lich", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bald sich hin zum Staube neiget;", "tokens": ["Bald", "sich", "hin", "zum", "Stau\u00b7be", "nei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dauer ist nicht zu erwerben,", "tokens": ["Dau\u00b7er", "ist", "nicht", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wandeln ist unsterblich Sterben.", "tokens": ["Wan\u00b7deln", "ist", "uns\u00b7terb\u00b7lich", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wehe! da\u00df der Gott auf Erden", "tokens": ["We\u00b7he", "!", "da\u00df", "der", "Gott", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sterblich mu\u00dft gebohren werden!", "tokens": ["Sterb\u00b7lich", "mu\u00dft", "ge\u00b7boh\u00b7ren", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Alle sind dem Tod verfallen,", "tokens": ["Al\u00b7le", "sind", "dem", "Tod", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sterben ist das Loos von allen.", "tokens": ["Ster\u00b7ben", "ist", "das", "Loos", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Viele doch sind die nicht wissen,", "tokens": ["Vie\u00b7le", "doch", "sind", "die", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "ART", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wie der Gott hat sterben m\u00fcssen;", "tokens": ["Wie", "der", "Gott", "hat", "ster\u00b7ben", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Blinde sind es, die nicht sehen,", "tokens": ["Blin\u00b7de", "sind", "es", ",", "die", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Nicht den tiefen Schmerz verstehen,", "tokens": ["Nicht", "den", "tie\u00b7fen", "Schmerz", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nicht der G\u00f6ttin Klag und Sehnen,", "tokens": ["Nicht", "der", "G\u00f6t\u00b7tin", "Klag", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ihre ungez\u00e4hlten Thr\u00e4nen,", "tokens": ["Ih\u00b7re", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Da\u00df der s\u00fc\u00dfe Leib des Sch\u00f6nen", "tokens": ["Da\u00df", "der", "s\u00fc\u00b7\u00dfe", "Leib", "des", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Mu\u00df dem kargen Tode fr\u00f6hnen.", "tokens": ["Mu\u00df", "dem", "kar\u00b7gen", "To\u00b7de", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft die Klage uns erneuern!", "tokens": ["La\u00dft", "die", "Kla\u00b7ge", "uns", "er\u00b7neu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rufet zu geheimen Feyern,", "tokens": ["Ru\u00b7fet", "zu", "ge\u00b7hei\u00b7men", "Fey\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Adonis heilig nennen,", "tokens": ["Die", "A\u00b7do\u00b7nis", "hei\u00b7lig", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Gottheit anerkennen,", "tokens": ["Sei\u00b7ne", "Got\u00b7theit", "an\u00b7er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die die Weihen sich erworben,", "tokens": ["Die", "die", "Wei\u00b7hen", "sich", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denen auch der Gott gestorben.", "tokens": ["De\u00b7nen", "auch", "der", "Gott", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Brecht die dunkle Anemone,", "tokens": ["Brecht", "die", "dunk\u00b7le", "A\u00b7ne\u00b7mo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie, die ihre Bl\u00e4tterkrone", "tokens": ["Sie", ",", "die", "ih\u00b7re", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinnend still herunter beuget,", "tokens": ["Sin\u00b7nend", "still", "her\u00b7un\u00b7ter", "beu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise sich zur Tiefe neiget,", "tokens": ["Lei\u00b7se", "sich", "zur", "Tie\u00b7fe", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Forschend ob der Gott auf Erden", "tokens": ["For\u00b7schend", "ob", "der", "Gott", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wieder soll gebohren werden!", "tokens": ["Wie\u00b7der", "soll", "ge\u00b7boh\u00b7ren", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Brechet Rosen; jede Blume", "tokens": ["Bre\u00b7chet", "Ro\u00b7sen", ";", "je\u00b7de", "Blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei verehrt im Heiligthume,", "tokens": ["Sei", "ver\u00b7ehrt", "im", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Forscht in ihren Kindermienen,", "tokens": ["Forscht", "in", "ih\u00b7ren", "Kin\u00b7der\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es schl\u00e4ft der Gott in ihnen;", "tokens": ["Denn", "es", "schl\u00e4ft", "der", "Gott", "in", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns ist er durch sie erstanden", "tokens": ["Uns", "ist", "er", "durch", "sie", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aus des dumpfen Grabes Banden.", "tokens": ["Aus", "des", "dum\u00b7pfen", "Gra\u00b7bes", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sie leis hervor sich dr\u00e4ngen,", "tokens": ["Wie", "sie", "leis", "her\u00b7vor", "sich", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und des H\u00fcgels Decke sprengen,", "tokens": ["Und", "des", "H\u00fc\u00b7gels", "De\u00b7cke", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ringet aus des Grabes Engen", "tokens": ["Rin\u00b7get", "aus", "des", "Gra\u00b7bes", "En\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sich empor verschlo\u00dfnes Leben;", "tokens": ["Sich", "em\u00b7por", "ver\u00b7schlo\u00df\u00b7nes", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Tod den Raub mu\u00df wiedergeben,", "tokens": ["Tod", "den", "Raub", "mu\u00df", "wie\u00b7der\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Leben wiederkehrt zum Leben.", "tokens": ["Le\u00b7ben", "wie\u00b7der\u00b7kehrt", "zum", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Also ist der Gott erstanden", "tokens": ["Al\u00b7so", "ist", "der", "Gott", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Aus des dumpfen Grabes Banden.", "tokens": ["Aus", "des", "dum\u00b7pfen", "Gra\u00b7bes", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wehe! da\u00df der Gott auf Erden", "tokens": ["We\u00b7he", "!", "da\u00df", "der", "Gott", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterblich mu\u00dft gebohren werden!", "tokens": ["Sterb\u00b7lich", "mu\u00dft", "ge\u00b7boh\u00b7ren", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Dasein, alles Leben", "tokens": ["Al\u00b7les", "Da\u00b7sein", ",", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mit ihm dem Tod gegeben.", "tokens": ["Ist", "mit", "ihm", "dem", "Tod", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "