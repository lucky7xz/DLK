{"dta.poem.2234": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie viele Menschen sind vergn\u00fcgt im Unvergn\u00fcgen,", "tokens": ["Wie", "vie\u00b7le", "Men\u00b7schen", "sind", "ver\u00b7gn\u00fcgt", "im", "Un\u00b7ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schelten, Schm\u00e4hlen,", "tokens": ["Schel\u00b7ten", ",", "Schm\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Jm Gram und Murren! Unvergn\u00fcgt hingegen, wenn", "tokens": ["Jm", "Gram", "und", "Mur\u00b7ren", "!", "Un\u00b7ver\u00b7gn\u00fcgt", "hin\u00b7ge\u00b7gen", ",", "wenn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "KON", "NN", "$.", "ADJD", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sie nicht befehlen,", "tokens": ["sie", "nicht", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nicht keifen und nicht klagen k\u00f6nnen! Wer leugnet, da\u00df", "tokens": ["Nicht", "kei\u00b7fen", "und", "nicht", "kla\u00b7gen", "k\u00f6n\u00b7nen", "!", "Wer", "leug\u00b7net", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "VMINF", "$.", "PWS", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "nicht die dem Schwein,", "tokens": ["nicht", "die", "dem", "Schwein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Das nur im Koth und Wust zufrieden, in ihrem Leben", "tokens": ["Das", "nur", "im", "Koth", "und", "Wust", "zu\u00b7frie\u00b7den", ",", "in", "ih\u00b7rem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN", "KON", "NN", "ADJD", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "\u00e4hnlich seyn?", "tokens": ["\u00e4hn\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}