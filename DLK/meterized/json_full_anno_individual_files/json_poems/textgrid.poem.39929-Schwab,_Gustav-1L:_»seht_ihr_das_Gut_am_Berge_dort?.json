{"textgrid.poem.39929": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbseht ihr das Gut am Berge dort?", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbseht ihr das Gut am Berge dort?", "tokens": ["\u00bb", "seht", "ihr", "das", "Gut", "am", "Ber\u00b7ge", "dort", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es gl\u00e4nzt wie gr\u00fcner Edelstein;", "tokens": ["Es", "gl\u00e4nzt", "wie", "gr\u00fc\u00b7ner", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "De\u00df Lehen soll's von heute sein,", "tokens": ["De\u00df", "Le\u00b7hen", "soll's", "von", "heu\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der zu mir spricht das liebste Wort!\u00ab", "tokens": ["Der", "zu", "mir", "spricht", "das", "liebs\u00b7te", "Wort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und alle Mannen, die zu Pferd", "tokens": ["Und", "al\u00b7le", "Man\u00b7nen", ",", "die", "zu", "Pferd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Grafen folgen in das Thal,", "tokens": ["Dem", "Gra\u00b7fen", "fol\u00b7gen", "in", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sprechen heben an zumal", "tokens": ["Zu", "spre\u00b7chen", "he\u00b7ben", "an", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein W\u00f6rtlein, das ihm d\u00e4uchte wert.", "tokens": ["Ein", "W\u00f6rt\u00b7lein", ",", "das", "ihm", "d\u00e4uch\u00b7te", "wert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da hei\u00dft er g\u00fctig, hei\u00dft er sch\u00f6n,", "tokens": ["Da", "hei\u00dft", "er", "g\u00fc\u00b7tig", ",", "hei\u00dft", "er", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein reicher Herr an Volk und Feld,", "tokens": ["Ein", "rei\u00b7cher", "Herr", "an", "Volk", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uralt von Stamm, ein starker Held,", "tokens": ["Ur\u00b7alt", "von", "Stamm", ",", "ein", "star\u00b7ker", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sein Schlo\u00df das herrlichste der H\u00f6hn.", "tokens": ["Sein", "Schlo\u00df", "das", "herr\u00b7lichs\u00b7te", "der", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So hallt sein Lob zum Rosseshuf;", "tokens": ["So", "hallt", "sein", "Lob", "zum", "Ros\u00b7ses\u00b7huf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da steht am Weg ein M\u00fctterlein,", "tokens": ["Da", "steht", "am", "Weg", "ein", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Stab gebeugt, vom Alter klein,", "tokens": ["Am", "Stab", "ge\u00b7beugt", ",", "vom", "Al\u00b7ter", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die l\u00e4\u00dft ert\u00f6nen ihren Ruf.", "tokens": ["Die", "l\u00e4\u00dft", "er\u00b7t\u00f6\u00b7nen", "ih\u00b7ren", "Ruf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbdu lieber Sohn! Gott gr\u00fc\u00dfe dich!\u00ab", "tokens": ["\u00bb", "du", "lie\u00b7ber", "Sohn", "!", "Gott", "gr\u00fc\u00b7\u00dfe", "dich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADV", "NN", "$.", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schallt ihr Gru\u00df am Pferd empor,", "tokens": ["So", "schallt", "ihr", "Gru\u00df", "am", "Pferd", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Graf neigt schmerzlich Aug' und Ohr,", "tokens": ["Der", "Graf", "neigt", "schmerz\u00b7lich", "Aug'", "und", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die stolzen Ritter wundern sich.", "tokens": ["Die", "stol\u00b7zen", "Rit\u00b7ter", "wun\u00b7dern", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbwas sprichst du f\u00fcr ein th\u00f6richt Wort?", "tokens": ["\u00bb", "was", "sprichst", "du", "f\u00fcr", "ein", "th\u00f6\u00b7richt", "Wort", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter liegt im Grabe mir,", "tokens": ["Die", "Mut\u00b7ter", "liegt", "im", "Gra\u00b7be", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie war der Edelt\u00f6chter Zier,", "tokens": ["Sie", "war", "der", "E\u00b7del\u00b7t\u00f6ch\u00b7ter", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da sprach das M\u00fctterlein: \u00bbJa, Sohn!", "tokens": ["Da", "sprach", "das", "M\u00fct\u00b7ter\u00b7lein", ":", "\u00bb", "Ja", ",", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "PTKANT", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man trug dich fort von ihrer Brust,", "tokens": ["Man", "trug", "dich", "fort", "von", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch eh du sp\u00fcrtest den Verlust,", "tokens": ["Doch", "eh", "du", "sp\u00fcr\u00b7test", "den", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lagst du an meinen Br\u00fcsten schon!", "tokens": ["Lagst", "du", "an", "mei\u00b7nen", "Br\u00fcs\u00b7ten", "schon", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dein Auge das so traurig schaut,", "tokens": ["Dein", "Au\u00b7ge", "das", "so", "trau\u00b7rig", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lachte hold an mir empor,", "tokens": ["Es", "lach\u00b7te", "hold", "an", "mir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es lauschte meinem Lied dein Ohr,", "tokens": ["Es", "lauschte", "mei\u00b7nem", "Lied", "dein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sanft war deiner Stimme Laut.", "tokens": ["Und", "sanft", "war", "dei\u00b7ner", "Stim\u00b7me", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "In deinen Adern flo\u00df mein Blut,", "tokens": ["In", "dei\u00b7nen", "A\u00b7dern", "flo\u00df", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich stahl die Milch dem eignen Sohn;", "tokens": ["Ich", "stahl", "die", "Milch", "dem", "eig\u00b7nen", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum la\u00df mir meinen Ammenlohn:", "tokens": ["Drum", "la\u00df", "mir", "mei\u00b7nen", "Am\u00b7men\u00b7lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich nenne \u203aSohn\u2039 dich wohlgemut!\u00ab", "tokens": ["Ich", "nen\u00b7ne", "\u203a", "Sohn", "\u2039", "dich", "wohl\u00b7ge\u00b7mut", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$(", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Im Nebel schwamm des Grafen Blick,", "tokens": ["Im", "Ne\u00b7bel", "schwamm", "des", "Gra\u00b7fen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und vor ihm schwankte Berg und Thal,", "tokens": ["Und", "vor", "ihm", "schwank\u00b7te", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ward sein Aug' ein Sonnenstral,", "tokens": ["Dann", "ward", "sein", "Aug'", "ein", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er bog vom Pferd sein stolz Genick.", "tokens": ["Er", "bog", "vom", "Pferd", "sein", "stolz", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die Rechte bot er dar der Frau,", "tokens": ["Die", "Rech\u00b7te", "bot", "er", "dar", "der", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Mund auf ihrer Lippe ruht,", "tokens": ["Sein", "Mund"