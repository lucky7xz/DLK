{"textgrid.poem.67381": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Bitten", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allm\u00e4cht'ge G\u00fcte, Vater aller Wesen,", "tokens": ["All\u00b7m\u00e4cht'\u00b7ge", "G\u00fc\u00b7te", ",", "Va\u00b7ter", "al\u00b7ler", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Herz, das sich in jedem Herzen reget,", "tokens": ["Du", "Herz", ",", "das", "sich", "in", "je\u00b7dem", "Her\u00b7zen", "re\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Mutterhand, die Alles hebt und tr\u00e4get", "tokens": ["Du", "Mut\u00b7ter\u00b7hand", ",", "die", "Al\u00b7les", "hebt", "und", "tr\u00e4\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich, auch mich zu Deinem Kind erlesen:", "tokens": ["Und", "mich", ",", "auch", "mich", "zu", "Dei\u00b7nem", "Kind", "er\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich knie vor Dir als Kind in stiller Demuth;", "tokens": ["Ich", "knie", "vor", "Dir", "als", "Kind", "in", "stil\u00b7ler", "De\u00b7muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOUS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du siehst ja innig, was ich will und denke,", "tokens": ["Du", "siehst", "ja", "in\u00b7nig", ",", "was", "ich", "will", "und", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWS", "PPER", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du lenkest selbst mein Herz, wenn ich es lenke,", "tokens": ["Du", "len\u00b7kest", "selbst", "mein", "Herz", ",", "wenn", "ich", "es", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und giebst mir selbst die Thr\u00e4ne s\u00fc\u00dfer Wehmuth.", "tokens": ["Und", "giebst", "mir", "selbst", "die", "Thr\u00e4\u00b7ne", "s\u00fc\u00b7\u00dfer", "Weh\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein tiefstes Dasein ist vor Dir enth\u00fcllet,", "tokens": ["Mein", "tiefs\u00b7tes", "Da\u00b7sein", "ist", "vor", "Dir", "ent\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Lebensbuch ist vor Dir aufgeschlagen,", "tokens": ["Mein", "Le\u00b7bens\u00b7buch", "ist", "vor", "Dir", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und manche W\u00fcnsche, die stumm in mir lagen,", "tokens": ["Und", "man\u00b7che", "W\u00fcn\u00b7sche", ",", "die", "stumm", "in", "mir", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eh ich sie wagete, hast Du erf\u00fcllet.", "tokens": ["Eh", "ich", "sie", "wa\u00b7ge\u00b7te", ",", "hast", "Du", "er\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was kann ich Dir, als was ich ganz bin, geben?", "tokens": ["Was", "kann", "ich", "Dir", ",", "als", "was", "ich", "ganz", "bin", ",", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "$,", "KOUS", "PRELS", "PPER", "ADV", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn, Freund, Du gabst mir, was ich bin und habe:", "tokens": ["Denn", ",", "Freund", ",", "Du", "gabst", "mir", ",", "was", "ich", "bin", "und", "ha\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Wunsch, Dir treu zu sein, ist Deine Gabe,", "tokens": ["Mein", "Wunsch", ",", "Dir", "treu", "zu", "sein", ",", "ist", "Dei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Licht ist Dein, mein Trost und all mein Leben.", "tokens": ["Mein", "Licht", "ist", "Dein", ",", "mein", "Trost", "und", "all", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "$,", "PPOSAT", "NN", "KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach, w\u00e4r' ich, wozu mich Dein Blick bestimmte,", "tokens": ["Ach", ",", "w\u00e4r'", "ich", ",", "wo\u00b7zu", "mich", "Dein", "Blick", "be\u00b7stimm\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sein zu sollen tief ich in mir f\u00fchle!", "tokens": ["Was", "sein", "zu", "sol\u00b7len", "tief", "ich", "in", "mir", "f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAINF", "PTKZU", "VVINF", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich irre noch, ich irre fern vom Ziele,", "tokens": ["Ich", "ir\u00b7re", "noch", ",", "ich", "ir\u00b7re", "fern", "vom", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mancher Funk' erlosch, der in mir glimmte.", "tokens": ["Und", "man\u00b7cher", "Funk'", "er\u00b7losch", ",", "der", "in", "mir", "glimm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Freund meines Lebens, reiche Deine H\u00e4nde!", "tokens": ["Freund", "mei\u00b7nes", "Le\u00b7bens", ",", "rei\u00b7che", "Dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O sei mir selbst der F\u00fchrer, der mich leite,", "tokens": ["O", "sei", "mir", "selbst", "der", "F\u00fch\u00b7rer", ",", "der", "mich", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Trieb, die Stimme, die mich stets begleite", "tokens": ["Der", "Trieb", ",", "die", "Stim\u00b7me", ",", "die", "mich", "stets", "be\u00b7glei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Fehler selbst zum Guten wende!", "tokens": ["Und", "mei\u00b7ne", "Feh\u00b7ler", "selbst", "zum", "Gu\u00b7ten", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nie will ich th\u00f6richt Dir mein Herz verh\u00fcllen,", "tokens": ["Nie", "will", "ich", "th\u00f6\u00b7richt", "Dir", "mein", "Herz", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie tobend k\u00fchn die Wahrheit von mir scheuchen.", "tokens": ["Nie", "to\u00b7bend", "k\u00fchn", "die", "Wahr\u00b7heit", "von", "mir", "scheu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Alles weicht, sollst Du nicht von mir weichen;", "tokens": ["Wenn", "Al\u00b7les", "weicht", ",", "sollst", "Du", "nicht", "von", "mir", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Du nur kannst und wirst mein Herz erf\u00fcllen.", "tokens": ["Denn", "Du", "nur", "kannst", "und", "wirst", "mein", "Herz", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VMFIN", "KON", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Du liebest mich und hast mich stets geliebet;", "tokens": ["Du", "lie\u00b7best", "mich", "und", "hast", "mich", "stets", "ge\u00b7lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn Dein sind meiner Jugend frohe Zeiten.", "tokens": ["Denn", "Dein", "sind", "mei\u00b7ner", "Ju\u00b7gend", "fro\u00b7he", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wirst mich lieben, in die Ewigkeiten", "tokens": ["Du", "wirst", "mich", "lie\u00b7ben", ",", "in", "die", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich lieben, Herr, wie oft ich Dich betr\u00fcbet.", "tokens": ["Mich", "lie\u00b7ben", ",", "Herr", ",", "wie", "oft", "ich", "Dich", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PWAV", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Gieb mir auf meinem kurzen Lebenswege", "tokens": ["Gieb", "mir", "auf", "mei\u00b7nem", "kur\u00b7zen", "Le\u00b7bens\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur t\u00e4glich reine Dankbarkeit und Freude,", "tokens": ["Nur", "t\u00e4g\u00b7lich", "rei\u00b7ne", "Dank\u00b7bar\u00b7keit", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und frohen Muth, wenn ich unschuldig leide,", "tokens": ["Und", "fro\u00b7hen", "Muth", ",", "wenn", "ich", "un\u00b7schul\u00b7dig", "lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und neuen Muth zu jedem rauhen Stege,", "tokens": ["Und", "neu\u00b7en", "Muth", "zu", "je\u00b7dem", "rau\u00b7hen", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und Glaub' und Lieb', die Alles \u00fcberwinden,", "tokens": ["Und", "Glaub'", "und", "Lieb'", ",", "die", "Al\u00b7les", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meiner ew'gen Hoffnung neue Fl\u00fcgel!", "tokens": ["Und", "mei\u00b7ner", "ew'\u00b7gen", "Hoff\u00b7nung", "neu\u00b7e", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich klimme ab und auf den Lebensh\u00fcgel,", "tokens": ["Ich", "klim\u00b7me", "ab", "und", "auf", "den", "Le\u00b7bens\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Dich, o Herr, wo mich ich werde finden.", "tokens": ["Wo", "Dich", ",", "o", "Herr", ",", "wo", "mich", "ich", "wer\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "FM", "NN", "$,", "PWAV", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und was ich mir, erfleh' ich auch den Meinen,", "tokens": ["Und", "was", "ich", "mir", ",", "er\u00b7fleh'", "ich", "auch", "den", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die nah und ferne, Herr, wie ich, hintreten,", "tokens": ["Die", "nah", "und", "fer\u00b7ne", ",", "Herr", ",", "wie", "ich", ",", "hin\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "$,", "NN", "$,", "PWAV", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Deine Knie sich schlie\u00dfen, in Gott beten;", "tokens": ["An", "Dei\u00b7ne", "Knie", "sich", "schlie\u00b7\u00dfen", ",", "in", "Gott", "be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erh\u00f6r uns, Herr! wir sind, wir sind die Deinen!", "tokens": ["Er\u00b7h\u00f6r", "uns", ",", "Herr", "!", "wir", "sind", ",", "wir", "sind", "die", "Dei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$.", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Allm\u00e4cht'ge G\u00fcte, Vater aller Wesen,", "tokens": ["All\u00b7m\u00e4cht'\u00b7ge", "G\u00fc\u00b7te", ",", "Va\u00b7ter", "al\u00b7ler", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Herz, das sich in jedem Herzen reget,", "tokens": ["Du", "Herz", ",", "das", "sich", "in", "je\u00b7dem", "Her\u00b7zen", "re\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Mutterhand, die Alles hebt und tr\u00e4get", "tokens": ["Du", "Mut\u00b7ter\u00b7hand", ",", "die", "Al\u00b7les", "hebt", "und", "tr\u00e4\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich, auch mich zu Deinem Kind erlesen:", "tokens": ["Und", "mich", ",", "auch", "mich", "zu", "Dei\u00b7nem", "Kind", "er\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Ich knie vor Dir als Kind in stiller Demuth;", "tokens": ["Ich", "knie", "vor", "Dir", "als", "Kind", "in", "stil\u00b7ler", "De\u00b7muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOUS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du siehst ja innig, was ich will und denke,", "tokens": ["Du", "siehst", "ja", "in\u00b7nig", ",", "was", "ich", "will", "und", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWS", "PPER", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du lenkest selbst mein Herz, wenn ich es lenke,", "tokens": ["Du", "len\u00b7kest", "selbst", "mein", "Herz", ",", "wenn", "ich", "es", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und giebst mir selbst die Thr\u00e4ne s\u00fc\u00dfer Wehmuth.", "tokens": ["Und", "giebst", "mir", "selbst", "die", "Thr\u00e4\u00b7ne", "s\u00fc\u00b7\u00dfer", "Weh\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Mein tiefstes Dasein ist vor Dir enth\u00fcllet,", "tokens": ["Mein", "tiefs\u00b7tes", "Da\u00b7sein", "ist", "vor", "Dir", "ent\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Lebensbuch ist vor Dir aufgeschlagen,", "tokens": ["Mein", "Le\u00b7bens\u00b7buch", "ist", "vor", "Dir", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und manche W\u00fcnsche, die stumm in mir lagen,", "tokens": ["Und", "man\u00b7che", "W\u00fcn\u00b7sche", ",", "die", "stumm", "in", "mir", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eh ich sie wagete, hast Du erf\u00fcllet.", "tokens": ["Eh", "ich", "sie", "wa\u00b7ge\u00b7te", ",", "hast", "Du", "er\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Was kann ich Dir, als was ich ganz bin, geben?", "tokens": ["Was", "kann", "ich", "Dir", ",", "als", "was", "ich", "ganz", "bin", ",", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "$,", "KOUS", "PRELS", "PPER", "ADV", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn, Freund, Du gabst mir, was ich bin und habe:", "tokens": ["Denn", ",", "Freund", ",", "Du", "gabst", "mir", ",", "was", "ich", "bin", "und", "ha\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Wunsch, Dir treu zu sein, ist Deine Gabe,", "tokens": ["Mein", "Wunsch", ",", "Dir", "treu", "zu", "sein", ",", "ist", "Dei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Licht ist Dein, mein Trost und all mein Leben.", "tokens": ["Mein", "Licht", "ist", "Dein", ",", "mein", "Trost", "und", "all", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "$,", "PPOSAT", "NN", "KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Ach, w\u00e4r' ich, wozu mich Dein Blick bestimmte,", "tokens": ["Ach", ",", "w\u00e4r'", "ich", ",", "wo\u00b7zu", "mich", "Dein", "Blick", "be\u00b7stimm\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sein zu sollen tief ich in mir f\u00fchle!", "tokens": ["Was", "sein", "zu", "sol\u00b7len", "tief", "ich", "in", "mir", "f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAINF", "PTKZU", "VVINF", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich irre noch, ich irre fern vom Ziele,", "tokens": ["Ich", "ir\u00b7re", "noch", ",", "ich", "ir\u00b7re", "fern", "vom", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mancher Funk' erlosch, der in mir glimmte.", "tokens": ["Und", "man\u00b7cher", "Funk'", "er\u00b7losch", ",", "der", "in", "mir", "glimm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Freund meines Lebens, reiche Deine H\u00e4nde!", "tokens": ["Freund", "mei\u00b7nes", "Le\u00b7bens", ",", "rei\u00b7che", "Dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O sei mir selbst der F\u00fchrer, der mich leite,", "tokens": ["O", "sei", "mir", "selbst", "der", "F\u00fch\u00b7rer", ",", "der", "mich", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Trieb, die Stimme, die mich stets begleite", "tokens": ["Der", "Trieb", ",", "die", "Stim\u00b7me", ",", "die", "mich", "stets", "be\u00b7glei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meine Fehler selbst zum Guten wende!", "tokens": ["Und", "mei\u00b7ne", "Feh\u00b7ler", "selbst", "zum", "Gu\u00b7ten", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Nie will ich th\u00f6richt Dir mein Herz verh\u00fcllen,", "tokens": ["Nie", "will", "ich", "th\u00f6\u00b7richt", "Dir", "mein", "Herz", "ver\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie tobend k\u00fchn die Wahrheit von mir scheuchen.", "tokens": ["Nie", "to\u00b7bend", "k\u00fchn", "die", "Wahr\u00b7heit", "von", "mir", "scheu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Alles weicht, sollst Du nicht von mir weichen;", "tokens": ["Wenn", "Al\u00b7les", "weicht", ",", "sollst", "Du", "nicht", "von", "mir", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Du nur kannst und wirst mein Herz erf\u00fcllen.", "tokens": ["Denn", "Du", "nur", "kannst", "und", "wirst", "mein", "Herz", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VMFIN", "KON", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Du liebest mich und hast mich stets geliebet;", "tokens": ["Du", "lie\u00b7best", "mich", "und", "hast", "mich", "stets", "ge\u00b7lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn Dein sind meiner Jugend frohe Zeiten.", "tokens": ["Denn", "Dein", "sind", "mei\u00b7ner", "Ju\u00b7gend", "fro\u00b7he", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wirst mich lieben, in die Ewigkeiten", "tokens": ["Du", "wirst", "mich", "lie\u00b7ben", ",", "in", "die", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich lieben, Herr, wie oft ich Dich betr\u00fcbet.", "tokens": ["Mich", "lie\u00b7ben", ",", "Herr", ",", "wie", "oft", "ich", "Dich", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PWAV", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "Gieb mir auf meinem kurzen Lebenswege", "tokens": ["Gieb", "mir", "auf", "mei\u00b7nem", "kur\u00b7zen", "Le\u00b7bens\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur t\u00e4glich reine Dankbarkeit und Freude,", "tokens": ["Nur", "t\u00e4g\u00b7lich", "rei\u00b7ne", "Dank\u00b7bar\u00b7keit", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und frohen Muth, wenn ich unschuldig leide,", "tokens": ["Und", "fro\u00b7hen", "Muth", ",", "wenn", "ich", "un\u00b7schul\u00b7dig", "lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und neuen Muth zu jedem rauhen Stege,", "tokens": ["Und", "neu\u00b7en", "Muth", "zu", "je\u00b7dem", "rau\u00b7hen", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "Und Glaub' und Lieb', die Alles \u00fcberwinden,", "tokens": ["Und", "Glaub'", "und", "Lieb'", ",", "die", "Al\u00b7les", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meiner ew'gen Hoffnung neue Fl\u00fcgel!", "tokens": ["Und", "mei\u00b7ner", "ew'\u00b7gen", "Hoff\u00b7nung", "neu\u00b7e", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich klimme ab und auf den Lebensh\u00fcgel,", "tokens": ["Ich", "klim\u00b7me", "ab", "und", "auf", "den", "Le\u00b7bens\u00b7h\u00fc\u00b7ge