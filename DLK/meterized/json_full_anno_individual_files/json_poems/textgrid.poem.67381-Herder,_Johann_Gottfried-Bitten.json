d, wir sind die Deinen!", "tokens": ["Er\u00b7h\u00f6r", "uns", ",", "Herr", "!", "wir", "sind", ",", "wir", "sind", "die", "Dei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$.", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}