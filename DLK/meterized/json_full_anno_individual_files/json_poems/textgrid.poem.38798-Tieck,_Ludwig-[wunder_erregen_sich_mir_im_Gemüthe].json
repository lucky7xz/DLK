{"textgrid.poem.38798": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[wunder erregen sich mir im Gem\u00fcthe]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wunder erregen sich mir im Gem\u00fcthe,", "tokens": ["Wun\u00b7der", "er\u00b7re\u00b7gen", "sich", "mir", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Herz bl\u00fcht auf licht in Gesangesf\u00fclle,", "tokens": ["Das", "Herz", "bl\u00fcht", "auf", "licht", "in", "Ge\u00b7san\u00b7ges\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Ein innig Sehnen schwebt in sanfter Stille,", "tokens": ["Ein", "in\u00b7nig", "Seh\u00b7nen", "schwebt", "in", "sanf\u00b7ter", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Denken, Gef\u00fchl, Ahndung in s\u00fc\u00dfer Bl\u00fcthe.", "tokens": ["Den\u00b7ken", ",", "Ge\u00b7f\u00fchl", ",", "Ahn\u00b7dung", "in", "s\u00fc\u00b7\u00dfer", "Bl\u00fc\u00b7the", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--++--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Blumen schaun mich an mit linder G\u00fcte,", "tokens": ["Die", "Blu\u00b7me