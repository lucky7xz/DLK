{"dta.poem.9076": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Gr\u00e4ben und grauen Hecken,", "tokens": ["Zwi\u00b7schen", "Gr\u00e4\u00b7ben", "und", "grau\u00b7en", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "den Rockkragen hoch, die H\u00e4nde in den Taschen,", "tokens": ["den", "Rock\u00b7kra\u00b7gen", "hoch", ",", "die", "H\u00e4n\u00b7de", "in", "den", "Ta\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "schlendre ich durch den fr\u00fchen M\u00e4rzmorgen.", "tokens": ["schlend\u00b7re", "ich", "durch", "den", "fr\u00fc\u00b7hen", "M\u00e4rz\u00b7mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Falbes Gras, blinkende Lachen und schwarzes Brachland,", "tokens": ["Fal\u00b7bes", "Gras", ",", "blin\u00b7ken\u00b7de", "La\u00b7chen", "und", "schwar\u00b7zes", "Brach\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "so weit ich sehn kann.", "tokens": ["so", "weit", "ich", "sehn", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dazwischen,", "tokens": ["Da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "mitten in den weissen Horizont hinein,", "tokens": ["mit\u00b7ten", "in", "den", "weis\u00b7sen", "Ho\u00b7ri\u00b7zont", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "wie erstarrt,", "tokens": ["wie", "er\u00b7starrt", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "eine Weidenreihe.", "tokens": ["ei\u00b7ne", "Wei\u00b7den\u00b7rei\u00b7he", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ich bleibe stehn.", "tokens": ["Ich", "blei\u00b7be", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nirgends ein Laut. Noch nirgends Leben.", "tokens": ["Nir\u00b7gends", "ein", "Laut", ".", "Noch", "nir\u00b7gends", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "ADV", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nur die Luft und die Landschaft.", "tokens": ["Nur", "die", "Luft", "und", "die", "Land\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Und sonnenlos, wie den Himmel, f\u00fchl ich mein Herz!", "tokens": ["Und", "son\u00b7nen\u00b7los", ",", "wie", "den", "Him\u00b7mel", ",", "f\u00fchl", "ich", "mein", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Pl\u00f6tzlich ein Klang.", "tokens": ["Pl\u00f6tz\u00b7lich", "ein", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Ich starre in die Wolken.", "tokens": ["Ich", "star\u00b7re", "in", "die", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ueber mir,", "tokens": ["Ue\u00b7ber", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "jubelnd,", "tokens": ["ju\u00b7belnd", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "durch immer heller werdendes Licht,", "tokens": ["durch", "im\u00b7mer", "hel\u00b7ler", "wer\u00b7den\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "die erste Lerche!", "tokens": ["die", "ers\u00b7te", "Ler\u00b7che", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}