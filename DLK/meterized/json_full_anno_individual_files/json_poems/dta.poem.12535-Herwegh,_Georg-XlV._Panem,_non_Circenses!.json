{"dta.poem.12535": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlV.  \n  Panem, non Circenses!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ebrod!\u201c so rufet das Volk, und ihr? ihr gebet ihm", "tokens": ["\u201e", "brod", "!", "\u201c", "so", "ru\u00b7fet", "das", "Volk", ",", "und", "ihr", "?", "ihr", "ge\u00b7bet", "ihm"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sagt mir, Pfaffen, doch an: hei\u00dft ihr das christlich", "tokens": ["Sagt", "mir", ",", "Pfaf\u00b7fen", ",", "doch", "an", ":", "hei\u00dft", "ihr", "das", "christ\u00b7lich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "PTKVZ", "$.", "VVFIN", "PPER", "ART", "ADJD"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "gedacht?", "tokens": ["ge\u00b7dacht", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "\u201ebrod!\u201c so rufet das Volk: da forschen und suchen", "tokens": ["\u201e", "brod", "!", "\u201c", "so", "ru\u00b7fet", "das", "Volk", ":", "da", "for\u00b7schen", "und", "su\u00b7chen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVINF", "KON", "VVINF"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "die Weisen,", "tokens": ["die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Suchen nur wieder den Stein, de\u00df uns so wenig", "tokens": ["Su\u00b7chen", "nur", "wie\u00b7der", "den", "Stein", ",", "de\u00df", "uns", "so", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "$,", "ART", "PPER", "ADV", "PIS"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "gebricht.", "tokens": ["ge\u00b7bricht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "\u201ebrod!\u201c so rufet das Volk, und die Herrschenden", "tokens": ["\u201e", "brod", "!", "\u201c", "so", "ru\u00b7fet", "das", "Volk", ",", "und", "die", "Herr\u00b7schen\u00b7den"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NN", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "treten zusammen,", "tokens": ["tre\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und rings fliegen daher wiederum Steine \u2014 zum", "tokens": ["Und", "rings", "flie\u00b7gen", "da\u00b7her", "wie\u00b7de\u00b7rum", "Stei\u00b7ne", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PAV", "ADV", "NN", "$(", "APPRART"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dom.", "tokens": ["Dom", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}