ure": "trochaic.penta"}, "line.2": {"text": "Werden ihm die Namen dr\u00e4u'n,", "tokens": ["Wer\u00b7den", "ihm", "die", "Na\u00b7men", "dr\u00e4u'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Schaudernd wird an seine Lieb' er denken,", "tokens": ["Schau\u00b7dernd", "wird", "an", "sei\u00b7ne", "Lieb'", "er", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ger\u00fchrt dich nicht entweih'n.", "tokens": ["Und", "ge\u00b7r\u00fchrt", "dich", "nicht", "ent\u00b7weih'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVPP", "PPER", "PTKNEG", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}