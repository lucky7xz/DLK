{"textgrid.poem.56427": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu sp\u00e4t. Die Griechen schlachten sie wie Schafe.", "tokens": ["Zu", "sp\u00e4t", ".", "Die", "Grie\u00b7chen", "schlach\u00b7ten", "sie", "wie", "Scha\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$.", "ART", "NN", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Wasser springt aus den geborstnen Schl\u00e4uchen,", "tokens": ["Wie", "Was\u00b7ser", "springt", "aus", "den", "ge\u00b7borst\u00b7nen", "Schl\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So st\u00fcrzt ihr Blut aus Hals und Kopf und B\u00e4uchen.", "tokens": ["So", "st\u00fcrzt", "ihr", "Blut", "aus", "Hals", "und", "Kopf", "und", "B\u00e4u\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sinken reihenweis zum letzten Schlafe.", "tokens": ["Sie", "sin\u00b7ken", "rei\u00b7hen\u00b7weis", "zum", "letz\u00b7ten", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Griechen steigt das Blut bis zu den Knien.", "tokens": ["Den", "Grie\u