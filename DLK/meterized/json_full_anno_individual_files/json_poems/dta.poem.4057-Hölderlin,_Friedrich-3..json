{"dta.poem.4057": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Licht der Liebe! scheinest du denn auch Todten, du", "tokens": ["Licht", "der", "Lie\u00b7be", "!", "schei\u00b7nest", "du", "denn", "auch", "Tod\u00b7ten", ",", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "PPER", "ADV", "ADV", "NN", "$,", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "goldnes!", "tokens": ["gold\u00b7nes", "!"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Bilder aus hellerer Zeit leuchtet ihr mir in die", "tokens": ["Bil\u00b7der", "aus", "hel\u00b7le\u00b7rer", "Zeit", "leuch\u00b7tet", "ihr", "mir", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART"], "meter": "+--+--++--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nacht?", "tokens": ["Nacht", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Liebliche G\u00e4rten, seyd, ihr abendr\u00f6thlichen Berge,", "tokens": ["Lieb\u00b7li\u00b7che", "G\u00e4r\u00b7ten", ",", "seyd", ",", "ihr", "a\u00b7bend\u00b7r\u00f6th\u00b7li\u00b7chen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Seyd willkommen, und ihr, schweigende Pfade", "tokens": ["Seyd", "will\u00b7kom\u00b7men", ",", "und", "ihr", ",", "schwei\u00b7gen\u00b7de", "Pfa\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "KON", "PPER", "$,", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "des Hains,", "tokens": ["des", "Hains", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Zeugen himmlischen Gl\u00fccks, und ihr, hochschauende", "tokens": ["Zeu\u00b7gen", "himm\u00b7li\u00b7schen", "Gl\u00fccks", ",", "und", "ihr", ",", "hoch\u00b7schau\u00b7en\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ADJA", "NN", "$,", "KON", "PPER", "$,", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Sterne,", "tokens": ["Ster\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Die mir damals oft segnende Blicke geg\u00f6nnt!", "tokens": ["Die", "mir", "da\u00b7mals", "oft", "seg\u00b7nen\u00b7de", "Bli\u00b7cke", "ge\u00b7g\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.11": {"text": "Euch, ihr Liebenden, auch, ihr sch\u00f6nen Kinder des", "tokens": ["Euch", ",", "ihr", "Lie\u00b7ben\u00b7den", ",", "auch", ",", "ihr", "sch\u00f6\u00b7nen", "Kin\u00b7der", "des"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$,", "PPOSAT", "ADJA", "NN", "ART"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Maitags,", "tokens": ["Mai\u00b7tags", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Stille Rosen und euch, Lilien, nenn' ich noch oft!", "tokens": ["Stil\u00b7le", "Ro\u00b7sen", "und", "euch", ",", "Li\u00b7li\u00b7en", ",", "nenn'", "ich", "noch", "oft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "PPER", "$,", "NE", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "Ihr Vertrauten! ihr Lebenden all' einst nahe dem", "tokens": ["Ihr", "Ver\u00b7trau\u00b7ten", "!", "ihr", "Le\u00b7ben\u00b7den", "all'", "einst", "na\u00b7he", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "PIS", "ADV", "APPR", "ART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Herzen,", "tokens": ["Her\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Einst wahrhaftiger, einst heller und sch\u00f6ner ge-", "tokens": ["Einst", "wahr\u00b7haf\u00b7ti\u00b7ger", ",", "einst", "hel\u00b7ler", "und", "sch\u00f6\u00b7ner", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJA", "TRUNC"], "meter": "-+----+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "sehn.", "tokens": ["sehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Wohl gehn Fr\u00fchlinge fort, ein Jahr verdr\u00e4nget", "tokens": ["Wohl", "gehn", "Fr\u00fch\u00b7lin\u00b7ge", "fort", ",", "ein", "Jahr", "ver\u00b7dr\u00e4n\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "das andre,", "tokens": ["das", "and\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Wechselnd und streitend, so tost droben vor\u00fcber", "tokens": ["Wech\u00b7selnd", "und", "strei\u00b7tend", ",", "so", "tost", "dro\u00b7ben", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADV", "VVFIN", "ADV", "ADV"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.21": {"text": "die Zeit", "tokens": ["die", "Zeit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Ueber sterblichem Haupt, doch nicht vor seligen Augen,", "tokens": ["Ue\u00b7ber", "sterb\u00b7li\u00b7chem", "Haupt", ",", "doch", "nicht", "vor", "se\u00b7li\u00b7gen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Und den Liebenden ist anderes Leben geschenkt.", "tokens": ["Und", "den", "Lie\u00b7ben\u00b7den", "ist", "an\u00b7de\u00b7res", "Le\u00b7ben", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Denn sie alle, die Tag' und Jahre der Sterne,", "tokens": ["Denn", "sie", "al\u00b7le", ",", "die", "Tag'", "und", "Jah\u00b7re", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "$,", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "sie waren", "tokens": ["sie", "wa\u00b7ren"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Diotima! um uns innig und ewig vereint.", "tokens": ["Dio\u00b7ti\u00b7ma", "!", "um", "uns", "in\u00b7nig", "und", "e\u00b7wig", "ver\u00b7eint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}