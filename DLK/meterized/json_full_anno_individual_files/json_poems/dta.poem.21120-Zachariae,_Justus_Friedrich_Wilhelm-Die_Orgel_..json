{"dta.poem.21120": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Orgel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re den rauschenden Wind in der stillerwartenden\nOrgel,", "tokens": ["H\u00f6\u00b7re", "den", "rau\u00b7schen\u00b7den", "Wind", "in", "der", "stil\u00b7ler\u00b7war\u00b7ten\u00b7den", "Or\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die er bereitet zum hohen Gesang!", "tokens": ["Die", "er", "be\u00b7rei\u00b7tet", "zum", "ho\u00b7hen", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Folge mir, werthester Freund, bis unter die schauern-", "tokens": ["Fol\u00b7ge", "mir", ",", "wert\u00b7hes\u00b7ter", "Freund", ",", "bis", "un\u00b7ter", "die", "schau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "TRUNC"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Heilige ganz dich der frommen Musik!", "tokens": ["Hei\u00b7li\u00b7ge", "ganz", "dich", "der", "from\u00b7men", "Mu\u00b7sik", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Himmel! ihr Jubel hebt an. Die hohen harmonischen", "tokens": ["Him\u00b7mel", "!", "ihr", "Ju\u00b7bel", "hebt", "an", ".", "Die", "ho\u00b7hen", "har\u00b7mo\u00b7ni\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "ADJA"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Brausen zu unserm erstaunenden Ohr.", "tokens": ["Brau\u00b7sen", "zu", "un\u00b7serm", "er\u00b7stau\u00b7nen\u00b7den", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Kraft von dem Himmel hebt mich! So klangen die Hal-", "tokens": ["Kraft", "von", "dem", "Him\u00b7mel", "hebt", "mich", "!", "So", "klan\u00b7gen", "die", "Ha\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Von der Trommeten festlichem Schall.", "tokens": ["Von", "der", "Trom\u00b7me\u00b7ten", "fest\u00b7li\u00b7chem", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Unter mir dr\u00f6net der Grund, und einsame Gr\u00e4ber er-", "tokens": ["Un\u00b7ter", "mir", "dr\u00f6\u00b7net", "der", "Grund", ",", "und", "ein\u00b7sa\u00b7me", "Gr\u00e4\u00b7ber", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "ADJA", "NN", "TRUNC"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von dem belebenden Schalle begr\u00fc\u00dft.", "tokens": ["Von", "dem", "be\u00b7le\u00b7ben\u00b7den", "Schal\u00b7le", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Also, doch m\u00e4chtiger noch, wird sie der Engel begr\u00fc\u00dfen,", "tokens": ["Al\u00b7so", ",", "doch", "m\u00e4ch\u00b7ti\u00b7ger", "noch", ",", "wird", "sie", "der", "En\u00b7gel", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJA", "ADV", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Mit der Posaune des letzten Gerichts.", "tokens": ["Mit", "der", "Po\u00b7sau\u00b7ne", "des", "letz\u00b7ten", "Ge\u00b7richts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn nun der Richter erscheint auf einer verblendenden", "tokens": ["Wenn", "nun", "der", "Rich\u00b7ter", "er\u00b7scheint", "auf", "ei\u00b7ner", "ver\u00b7blen\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und in dem Felde der Todten es rauscht;", "tokens": ["Und", "in", "dem", "Fel\u00b7de", "der", "Tod\u00b7ten", "es", "rauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn das belebte Gebein nun, seinem Erwecker gehor-", "tokens": ["Wenn", "das", "be\u00b7leb\u00b7te", "Ge\u00b7bein", "nun", ",", "sei\u00b7nem", "Er\u00b7we\u00b7cker", "ge\u00b7hor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "$,", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stimmen der starken Posaune vernimmt.", "tokens": ["Stim\u00b7men", "der", "star\u00b7ken", "Po\u00b7sau\u00b7ne", "ver\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dann der Richter der Welt die Heiligen um sich", "tokens": ["Und", "dann", "der", "Rich\u00b7ter", "der", "Welt", "die", "Hei\u00b7li\u00b7gen", "um", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "APPR", "PRF"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Oder Verworfne zum Feuer verst\u00f6\u00dft;", "tokens": ["O\u00b7der", "Ver\u00b7worf\u00b7ne", "zum", "Feu\u00b7er", "ver\u00b7st\u00f6\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und auf ihr Antlitz alsdann die Thronen und Cherubim", "tokens": ["Und", "auf", "ihr", "Ant\u00b7litz", "als\u00b7dann", "die", "Thro\u00b7nen", "und", "Che\u00b7ru\u00b7bim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "KON", "NE"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vor dem Allm\u00e4chtgen in Ehrfurcht gebeugt.", "tokens": ["Vor", "dem", "All\u00b7m\u00e4cht\u00b7gen", "in", "Ehr\u00b7furcht", "ge\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-++--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Eben so t\u00f6net der Schall durch jubilirende R\u00f6hren,", "tokens": ["E\u00b7ben", "so", "t\u00f6\u00b7net", "der", "Schall", "durch", "ju\u00b7bi\u00b7li\u00b7ren\u00b7de", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Seele, was hebt dich zum Himmel empor?", "tokens": ["See\u00b7le", ",", "was", "hebt", "dich", "zum", "Him\u00b7mel", "em\u00b7por", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Bist du nicht durch die Gewalt der hohen harmoni-", "tokens": ["Bist", "du", "nicht", "durch", "die", "Ge\u00b7walt", "der", "ho\u00b7hen", "har\u00b7mo\u00b7ni"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Unter die Ch\u00f6re der Engel verz\u00fcckt?", "tokens": ["Un\u00b7ter", "die", "Ch\u00f6\u00b7re", "der", "En\u00b7gel", "ver\u00b7z\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}