{"textgrid.poem.43921": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Versteht ihr auch, ihr sanften H\u00e4nde,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Versteht ihr auch, ihr sanften H\u00e4nde,", "tokens": ["Ver\u00b7steht", "ihr", "auch", ",", "ihr", "sanf\u00b7ten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum euch mein Verlangen dr\u00fcckt?", "tokens": ["Wa\u00b7rum", "euch", "mein", "Ver\u00b7lan\u00b7gen", "dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freyheit, merck ich, geht zum Ende", "tokens": ["Die", "Frey\u00b7heit", ",", "merck", "ich", ",", "geht", "zum", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird mir mit Gewalt entr\u00fcckt;", "tokens": ["Und", "wird", "mir", "mit", "Ge\u00b7walt", "ent\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich such und denck euch zu bewegen,", "tokens": ["Ich", "such", "und", "denck", "euch", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVIMP", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir st\u00e4rckre Fe\u00dfel anzulegen.", "tokens": ["Mir", "st\u00e4r\u00b7ck\u00b7re", "Fe\u00b7\u00dfel", "an\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVIZU", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, fragt nur eurer Sch\u00f6nen Herze,", "tokens": ["Ach", ",", "fragt", "nur", "eu\u00b7rer", "Sch\u00f6\u00b7nen", "Her\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem ihr Blut und Feuer kriegt;", "tokens": ["Von", "dem", "ihr", "Blut", "und", "Feu\u00b7er", "kriegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es weis vielleicht von diesem Schmerze,", "tokens": ["Es", "weis", "viel\u00b7leicht", "von", "die\u00b7sem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den mir ihr Auge zugef\u00fcgt,", "tokens": ["Den", "mir", "ihr", "Au\u00b7ge", "zu\u00b7ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Auge, de\u00dfen Glut und Lachen", "tokens": ["Ihr", "Au\u00b7ge", ",", "de\u00b7\u00dfen", "Glut", "und", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir gr\u00f6\u00dfre Pein als Hofnung machen.", "tokens": ["Mir", "gr\u00f6\u00df\u00b7re", "Pein", "als", "Hof\u00b7nung", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und h\u00e4tt ich auch noch sonst zu hofen,", "tokens": ["Und", "h\u00e4tt", "ich", "auch", "noch", "sonst", "zu", "ho\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wehrt es mir die kurze Zeit,", "tokens": ["So", "wehrt", "es", "mir", "die", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es steht kein Weg zum Umgang ofen;", "tokens": ["Es", "steht", "kein", "Weg", "zum", "Um\u00b7gang", "o\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, seelige Gelegenheit,", "tokens": ["Komm", ",", "see\u00b7li\u00b7ge", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schaffe, da\u00df ich zeigen k\u00f6nne,", "tokens": ["Und", "schaf\u00b7fe", ",", "da\u00df", "ich", "zei\u00b7gen", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie zart und rein mein Herze brenne.", "tokens": ["Wie", "zart", "und", "rein", "mein", "Her\u00b7ze", "bren\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich weis, die artige Rosette", "tokens": ["Ich", "weis", ",", "die", "ar\u00b7ti\u00b7ge", "Ro\u00b7set\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erkl\u00e4rte sich vor meine Treu,", "tokens": ["Er\u00b7kl\u00e4r\u00b7te", "sich", "vor", "mei\u00b7ne", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wofern sie erst gepr\u00fcfet h\u00e4tte,", "tokens": ["Wo\u00b7fern", "sie", "erst", "ge\u00b7pr\u00fc\u00b7fet", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gleich ihr mein Gem\u00fcthe sey,", "tokens": ["Wie", "gleich", "ihr", "mein", "Ge\u00b7m\u00fc\u00b7the", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn sie aus Erfahrung w\u00fcste,", "tokens": ["Und", "wenn", "sie", "aus", "Er\u00b7fah\u00b7rung", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was manch Verliebter dulden m\u00fcste.", "tokens": ["Was", "manch", "Ver\u00b7lieb\u00b7ter", "dul\u00b7den", "m\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich bin mit mancher umgegangen,", "tokens": ["Ich", "bin", "mit", "man\u00b7cher", "um\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die noch wohl liebensw\u00fcrdig w\u00e4r,", "tokens": ["Die", "noch", "wohl", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis jezo blieb ich ungefangen;", "tokens": ["Bis", "je\u00b7zo", "blieb", "ich", "un\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du, sch\u00f6nes Kind, kommst ohngefehr", "tokens": ["Du", ",", "sch\u00f6\u00b7nes", "Kind", ",", "kommst", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und r\u00fchrst mich gleich zum ersten Mahle", "tokens": ["Und", "r\u00fchrst", "mich", "gleich", "zum", "ers\u00b7ten", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch nur mit einem holden Strahle.", "tokens": ["Auch", "nur", "mit", "ei\u00b7nem", "hol\u00b7den", "Strah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die kurze Lust der Abendstunde", "tokens": ["Die", "kur\u00b7ze", "Lust", "der", "A\u00b7bends\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehrte diese Leidenschaft,", "tokens": ["Ver\u00b7mehr\u00b7te", "die\u00b7se", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da nahm ein Ku\u00df von sch\u00f6nem Munde", "tokens": ["Da", "nahm", "ein", "Ku\u00df", "von", "sch\u00f6\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herze v\u00f6llig in Verhaft;", "tokens": ["Das", "Her\u00b7ze", "v\u00f6l\u00b7lig", "in", "Ver\u00b7haft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es hies zwar nur im Scherz und Spielen,", "tokens": ["Es", "hies", "zwar", "nur", "im", "Scherz", "und", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allein ich kan es anders f\u00fchlen.", "tokens": ["Al\u00b7lein", "ich", "kan", "es", "an\u00b7ders", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dein Bildn\u00fc\u00df kam darauf im Schlummer", "tokens": ["Dein", "Bild\u00b7n\u00fc\u00df", "kam", "da\u00b7rauf", "im", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem tr\u00e4umenden Ged\u00e4chtn\u00fc\u00df ein,", "tokens": ["Dem", "tr\u00e4u\u00b7men\u00b7den", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich deucht, ich klagte dir den Kummer,", "tokens": ["Mich", "deucht", ",", "ich", "klag\u00b7te", "dir", "den", "Kum\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du schienest nicht erz\u00fcrnt zu seyn;", "tokens": ["Du", "schie\u00b7nest", "nicht", "er\u00b7z\u00fcrnt", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da gab mir der verhaste Morgen", "tokens": ["Da", "gab", "mir", "der", "ver\u00b7has\u00b7te", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor falsche Wollust wahre Sorgen.", "tokens": ["Vor", "fal\u00b7sche", "Wol\u00b7lust", "wah\u00b7re", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dies alles ist wohl nicht vergebens,", "tokens": ["Dies", "al\u00b7les", "ist", "wohl", "nicht", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel paart oft wunderlich;", "tokens": ["Der", "Him\u00b7mel", "paart", "oft", "wun\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Troste des betr\u00fcbten Lebens", "tokens": ["Zum", "Tros\u00b7te", "des", "be\u00b7tr\u00fcb\u00b7ten", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Begehrt ich sonst kein Kind als dich;", "tokens": ["Be\u00b7gehrt", "ich", "sonst", "kein", "Kind", "als", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe k\u00f6nte Mittel zeigen", "tokens": ["Die", "Lie\u00b7be", "k\u00f6n\u00b7te", "Mit\u00b7tel", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und heute \u2013 doch ich mu\u00df nur schweigen.", "tokens": ["Und", "heu\u00b7te", "\u2013", "doch", "ich", "mu\u00df", "nur", "schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "KON", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Versteht ihr auch, ihr sanften H\u00e4nde,", "tokens": ["Ver\u00b7steht", "ihr", "auch", ",", "ihr", "sanf\u00b7ten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum euch mein Verlangen dr\u00fcckt?", "tokens": ["Wa\u00b7rum", "euch", "mein", "Ver\u00b7lan\u00b7gen", "dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freyheit, merck ich, geht zum Ende", "tokens": ["Die", "Frey\u00b7heit", ",", "merck", "ich", ",", "geht", "zum", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird mir mit Gewalt entr\u00fcckt;", "tokens": ["Und", "wird", "mir", "mit", "Ge\u00b7walt", "ent\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich such und denck euch zu bewegen,", "tokens": ["Ich", "such", "und", "denck", "euch", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVIMP", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir st\u00e4rckre Fe\u00dfel anzulegen.", "tokens": ["Mir", "st\u00e4r\u00b7ck\u00b7re", "Fe\u00b7\u00dfel", "an\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVIZU", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Ach, fragt nur eurer Sch\u00f6nen Herze,", "tokens": ["Ach", ",", "fragt", "nur", "eu\u00b7rer", "Sch\u00f6\u00b7nen", "Her\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von dem ihr Blut und Feuer kriegt;", "tokens": ["Von", "dem", "ihr", "Blut", "und", "Feu\u00b7er", "kriegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es weis vielleicht von diesem Schmerze,", "tokens": ["Es", "weis", "viel\u00b7leicht", "von", "die\u00b7sem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den mir ihr Auge zugef\u00fcgt,", "tokens": ["Den", "mir", "ihr", "Au\u00b7ge", "zu\u00b7ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Auge, de\u00dfen Glut und Lachen", "tokens": ["Ihr", "Au\u00b7ge", ",", "de\u00b7\u00dfen", "Glut", "und", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir gr\u00f6\u00dfre Pein als Hofnung machen.", "tokens": ["Mir", "gr\u00f6\u00df\u00b7re", "Pein", "als", "Hof\u00b7nung", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und h\u00e4tt ich auch noch sonst zu hofen,", "tokens": ["Und", "h\u00e4tt", "ich", "auch", "noch", "sonst", "zu", "ho\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wehrt es mir die kurze Zeit,", "tokens": ["So", "wehrt", "es", "mir", "die", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es steht kein Weg zum Umgang ofen;", "tokens": ["Es", "steht", "kein", "Weg", "zum", "Um\u00b7gang", "o\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, seelige Gelegenheit,", "tokens": ["Komm", ",", "see\u00b7li\u00b7ge", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schaffe, da\u00df ich zeigen k\u00f6nne,", "tokens": ["Und", "schaf\u00b7fe", ",", "da\u00df", "ich", "zei\u00b7gen", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON",