{"dta.poem.3338": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Weil Phyllis untreu ist, will Damon sich erstechen:", "tokens": ["Weil", "Phyl\u00b7lis", "un\u00b7treu", "ist", ",", "will", "Da\u00b7mon", "sich", "er\u00b7ste\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "VAFIN", "$,", "VMFIN", "NE", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch will er kl\u00fcglich erst mit seinem Weine sprechen.", "tokens": ["Doch", "will", "er", "kl\u00fcg\u00b7lich", "erst", "mit", "sei\u00b7nem", "Wei\u00b7ne", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein kl\u00fcgrer Wein giebt ihm den Rath,", "tokens": ["Sein", "kl\u00fcg\u00b7rer", "Wein", "giebt", "ihm", "den", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er soll durch eine gleiche That", "tokens": ["Er", "soll", "durch", "ei\u00b7ne", "glei\u00b7che", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich an der Ungetreuen r\u00e4chen:", "tokens": ["Sich", "an", "der", "Un\u00b7ge\u00b7treu\u00b7en", "r\u00e4\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er thuts, und lebt noch itzt: gewi\u00df ein guter Rath!", "tokens": ["Er", "thuts", ",", "und", "lebt", "noch", "itzt", ":", "ge\u00b7wi\u00df", "ein", "gu\u00b7ter", "Rath", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ADV", "$.", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Liebesgott braucht sein Gefieder,", "tokens": ["Der", "Lie\u00b7bes\u00b7gott", "braucht", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Als Amor, als der Gott der Lust:", "tokens": ["Als", "A\u00b7mor", ",", "als", "der", "Gott", "der", "Lust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Freude flieht; er sucht sie wieder;", "tokens": ["Die", "Freu\u00b7de", "flieht", ";", "er", "sucht", "sie", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und findet sie auf andrer Sch\u00f6nen Brust.", "tokens": ["Und", "fin\u00b7det", "sie", "auf", "an\u00b7drer", "Sch\u00f6\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Sch\u00f6nen alte Strenge fliehet:", "tokens": ["Der", "Sch\u00f6\u00b7nen", "al\u00b7te", "Stren\u00b7ge", "flie\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie sind ja Fleisch, wie ieder siehet,", "tokens": ["Sie", "sind", "ja", "Fleisch", ",", "wie", "ie\u00b7der", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das sch\u00f6nste Fleisch, nicht harter Stein.", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Fleisch", ",", "nicht", "har\u00b7ter", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Man gebe mir die gr\u00f6\u00dfte Spr\u00f6de,", "tokens": ["Man", "ge\u00b7be", "mir", "die", "gr\u00f6\u00df\u00b7te", "Spr\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch in der D\u00e4mmrung und allein:", "tokens": ["Doch", "in", "der", "D\u00e4mm\u00b7rung", "und", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sie soll nicht lange spr\u00f6de seyn.", "tokens": ["Sie", "soll", "nicht", "lan\u00b7ge", "spr\u00f6\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Man weis, wir Gnomen sind nicht bl\u00f6de:", "tokens": ["Man", "weis", ",", "wir", "Gno\u00b7men", "sind", "nicht", "bl\u00f6\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$,", "PPER", "NN", "VAFIN", "PTKNEG", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wer muthig st\u00fcrmt, nimmt alles ein.", "tokens": ["Wer", "mut\u00b7hig", "st\u00fcrmt", ",", "nimmt", "al\u00b7les", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}