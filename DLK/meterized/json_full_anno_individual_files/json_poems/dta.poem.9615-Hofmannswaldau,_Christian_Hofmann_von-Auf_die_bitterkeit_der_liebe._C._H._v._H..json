{"dta.poem.9615": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die bitterkeit der liebe.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die nacht Egyptiens/ des Aetna wildes feuer/", "tokens": ["Die", "nacht", "E\u00b7gyp\u00b7tiens", "/", "des", "A\u00b7et\u00b7na", "wil\u00b7des", "feu\u00b7er", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das w\u00fcten von der see/ der w\u00fcsten ungeheuer/", "tokens": ["Das", "w\u00fc\u00b7ten", "von", "der", "see", "/", "der", "w\u00fcs\u00b7ten", "un\u00b7ge\u00b7heu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des drachen gelbes gift/ der Garamanten sand/", "tokens": ["Des", "dra\u00b7chen", "gel\u00b7bes", "gift", "/", "der", "Ga\u00b7ra\u00b7man\u00b7ten", "sand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des neuen Zembles ey\u00df/ der h\u00f6llen heisser brand/", "tokens": ["Des", "neu\u00b7en", "Zem\u00b7bles", "ey\u00df", "/", "der", "h\u00f6l\u00b7len", "heis\u00b7ser", "brand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Scythen haupt-gefahr/ der donner-berge grausen/", "tokens": ["Der", "Scyt\u00b7hen", "haup\u00b7tge\u00b7fahr", "/", "der", "don\u00b7ner\u00b7ber\u00b7ge", "grau\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Caucasus verdru\u00df/ des norden kaltes sausen/", "tokens": ["Des", "Cau\u00b7ca\u00b7sus", "ver\u00b7dru\u00df", "/", "des", "nor\u00b7den", "kal\u00b7tes", "sau\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist nur ein schattenwerck und bild derselben qual/", "tokens": ["Ist", "nur", "ein", "schat\u00b7ten\u00b7werck", "und", "bild", "der\u00b7sel\u00b7ben", "qual", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "KON", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Damit die Venus hat gezieret ihren saal.", "tokens": ["Da\u00b7mit", "die", "Ve\u00b7nus", "hat", "ge\u00b7zie\u00b7ret", "ih\u00b7ren", "saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}