{"dta.poem.12741": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf  * * *  \n F. E. W.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje Curtius zu pferd ist h\u00f6llen-werts geritten;", "tokens": ["Wie", "Cur\u00b7tius", "zu", "pferd", "ist", "h\u00f6l\u00b7len\u00b7werts", "ge\u00b7rit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So sprengst du ebenfalls zu pferde da hinein,", "tokens": ["So", "sprengst", "du", "e\u00b7ben\u00b7falls", "zu", "pfer\u00b7de", "da", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem ein geiles weib hierzu der gaul mu\u00df seyn,", "tokens": ["In\u00b7dem", "ein", "gei\u00b7les", "weib", "hier\u00b7zu", "der", "gaul", "mu\u00df", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PAV", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der eilt der h\u00f6llen zu mit h\u00f6chstgeschwinden schritten.", "tokens": ["Der", "eilt", "der", "h\u00f6l\u00b7len", "zu", "mit", "h\u00f6chst\u00b7ge\u00b7schwin\u00b7den", "schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "VVINF", "PTKZU", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "al