{"dta.poem.16267": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist bei diesem Spiel des Lebens zu gewinnen?", "tokens": ["Was", "ist", "bei", "die\u00b7sem", "Spiel", "des", "Le\u00b7bens", "zu", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer's nicht verlieren will, der sollt' es nicht beginnen.", "tokens": ["Wer's", "nicht", "ver\u00b7lie\u00b7ren", "will", ",", "der", "sollt'", "es", "nicht", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn zum Verlieren nur ist ein Gewinn der Lust,", "tokens": ["Denn", "zum", "Ver\u00b7lie\u00b7ren", "nur", "ist", "ein", "Ge\u00b7winn", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zu gewinnen ist nichts sicher als Verlust.", "tokens": ["Und", "zu", "ge\u00b7win\u00b7nen", "ist", "nichts", "si\u00b7cher", "als", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VAFIN", "PIS", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dich schmerzt, was du verlorst, dich, was du nicht gewannst,", "tokens": ["Dich", "schmerzt", ",", "was", "du", "ver\u00b7lorst", ",", "dich", ",", "was", "du", "nicht", "ge\u00b7wannst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "$,", "PWS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am meisten schmerzt dich, was du noch verlieren kannst.", "tokens": ["Am", "meis\u00b7ten", "schmerzt", "dich", ",", "was", "du", "noch", "ver\u00b7lie\u00b7ren", "kannst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und alles hast du, wenn du hast den Muth gewonnen,", "tokens": ["Und", "al\u00b7les", "hast", "du", ",", "wenn", "du", "hast", "den", "Muth", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es auszuspielen weil es einmal ist begonnen.", "tokens": ["Es", "aus\u00b7zu\u00b7spie\u00b7len", "weil", "es", "ein\u00b7mal", "ist", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du siehst, es w\u00e4re fast der Muth mir selbst abhanden", "tokens": ["Du", "siehst", ",", "es", "w\u00e4\u00b7re", "fast", "der", "Muth", "mir", "selbst", "ab\u00b7han\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gekommen, als einmal mir schlimm die Karten standen.", "tokens": ["Ge\u00b7kom\u00b7men", ",", "als", "ein\u00b7mal", "mir", "schlimm", "die", "Kar\u00b7ten", "stan\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Doch hab' ich mich bedacht und dieses Lied ersonnen:", "tokens": ["Doch", "hab'", "ich", "mich", "be\u00b7dacht", "und", "die\u00b7ses", "Lied", "er\u00b7son\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVPP", "KON", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was auch verloren sei, die Lieder sind gewonnen.", "tokens": ["Was", "auch", "ver\u00b7lo\u00b7ren", "sei", ",", "die", "Lie\u00b7der", "sind", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}