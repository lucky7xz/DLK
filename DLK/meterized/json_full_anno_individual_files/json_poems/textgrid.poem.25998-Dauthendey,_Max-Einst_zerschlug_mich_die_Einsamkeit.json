{"textgrid.poem.25998": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Einst zerschlug mich die Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst zerschlug mich die Einsamkeit wie dumm Holz Scheit um Scheit,", "tokens": ["Einst", "zer\u00b7schlug", "mich", "die", "Ein\u00b7sam\u00b7keit", "wie", "dumm", "Holz", "Scheit", "um", "Scheit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KOKOM", "ADJD", "NN", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+++-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Unter deiner Hand wurden die Wunden ein Traum,", "tokens": ["Un\u00b7ter", "dei\u00b7ner", "Hand", "wur\u00b7den", "die", "Wun\u00b7den", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Im gesunden Baum singen mit jungem Flaum deine V\u00f6gel.", "tokens": ["Im", "ge\u00b7sun\u00b7den", "Baum", "sin\u00b7gen", "mit", "jun\u00b7gem", "Flaum", "dei\u00b7ne", "V\u00f6\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+---+-+--+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Herz hat das Wort \u00bbWeh\u00ab sterben gemacht,", "tokens": ["Dein", "Herz", "hat", "das", "Wort", "\u00bb", "Weh", "\u00ab", "ster\u00b7ben", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "NN", "$(", "VVINF", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du hast warme \u00c4hren auf die Felder gestellt,", "tokens": ["Du", "hast", "war\u00b7me", "\u00c4h\u00b7ren", "auf", "die", "Fel\u00b7der", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Du wirst s\u00fc\u00dfe Trauben bescheren", "tokens": ["Du", "wirst", "s\u00fc\u00b7\u00dfe", "Trau\u00b7ben", "be\u00b7sche\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und endlich den Schnee, der den Winter erhellt.", "tokens": ["Und", "end\u00b7lich", "den", "Schnee", ",", "der", "den", "Win\u00b7ter", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Das Jahr w\u00e4chst freundlich aus deinem Scho\u00df,", "tokens": ["Das", "Jahr", "w\u00e4chst", "freund\u00b7lich", "aus", "dei\u00b7nem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich sehe staunend zu, wie reich du bist,", "tokens": ["Ich", "se\u00b7he", "stau\u00b7nend", "zu", ",", "wie", "reich", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VAFI