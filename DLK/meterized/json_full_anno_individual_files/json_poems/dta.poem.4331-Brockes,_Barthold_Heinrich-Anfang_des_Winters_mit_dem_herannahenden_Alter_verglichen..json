{"dta.poem.4331": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anfang des Winters  \n mit dem  \n herannahenden Alter verglichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Winter, wenn ich, mit Vergn\u00fcgen,", "tokens": ["Jm", "Win\u00b7ter", ",", "wenn", "ich", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den noch nicht dick gefallnen Schnee", "tokens": ["Den", "noch", "nicht", "dick", "ge\u00b7fall\u00b7nen", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PTKNEG", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur d\u00fcnn auf flachen Aeckern liegen,", "tokens": ["Nur", "d\u00fcnn", "auf", "fla\u00b7chen", "A\u00b7e\u00b7ckern", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie decken und nicht decken seh,", "tokens": ["Sie", "de\u00b7cken", "und", "nicht", "de\u00b7cken", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Indem der Furchen Tiefen nur", "tokens": ["In\u00b7dem", "der", "Fur\u00b7chen", "Tie\u00b7fen", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Schaum der Luft die weisse Spur", "tokens": ["Vom", "Schaum", "der", "Luft", "die", "weis\u00b7se", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns, in geraden Strichen, zeigen,", "tokens": ["Uns", ",", "in", "ge\u00b7ra\u00b7den", "Stri\u00b7chen", ",", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und da\u00df wir die erhabnen H\u00f6h\u2019n,", "tokens": ["Und", "da\u00df", "wir", "die", "er\u00b7hab\u00b7nen", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als ob sie aus den Tiefen steigen,", "tokens": ["Als", "ob", "sie", "aus", "den", "Tie\u00b7fen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In einem braunen Schmuck noch seh\u2019n;", "tokens": ["In", "ei\u00b7nem", "brau\u00b7nen", "Schmuck", "noch", "seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So kommt der ganze Acker mir,", "tokens": ["So", "kommt", "der", "gan\u00b7ze", "A\u00b7cker", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In einer sprenklich-grauen Zier,", "tokens": ["In", "ei\u00b7ner", "sprenk\u00b7lich\u00b7grau\u00b7en", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als wie bey uns ein sprenklicht Haar,", "tokens": ["Als", "wie", "bey", "uns", "ein", "sprenk\u00b7licht", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PPER", "PTKVZ", "VVFIN", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.14": {"text": "Bey noch nicht vollem Alter, f\u00fcr.", "tokens": ["Bey", "noch", "nicht", "vol\u00b7lem", "Al\u00b7ter", ",", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "PTKNEG", "ADJA", "NN", "$,", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Man wird nur einzeln hier und dar", "tokens": ["Man", "wird", "nur", "ein\u00b7zeln", "hier", "und", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein gl\u00e4nzend Silber-Haar gewahr,", "tokens": ["Ein", "gl\u00e4n\u00b7zend", "Sil\u00b7ber\u00b7Haar", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das noch die braunen unterdr\u00fccken.", "tokens": ["Das", "noch", "die", "brau\u00b7nen", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ob sie nun gleich die Scheitel schm\u00fccken,", "tokens": ["Ob", "sie", "nun", "gleich", "die", "Schei\u00b7tel", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ist doch in der noch seltnen Pracht", "tokens": ["Ist", "doch", "in", "der", "noch", "selt\u00b7nen", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "(wie hier des Winters kalte Nacht)", "tokens": ["(", "wie", "hier", "des", "Win\u00b7ters", "kal\u00b7te", "Nacht", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Das nahe Alter zu erblicken.", "tokens": ["Das", "na\u00b7he", "Al\u00b7ter", "zu", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Doch schrecket beydes mein Gesicht,", "tokens": ["Doch", "schre\u00b7cket", "bey\u00b7des", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da ich auf meines Scheitels H\u00f6he", "tokens": ["Da", "ich", "auf", "mei\u00b7nes", "Schei\u00b7tels", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Auch schon dergleichen Schimmer sehe,", "tokens": ["Auch", "schon", "derg\u00b7lei\u00b7chen", "Schim\u00b7mer", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Mich, durch die Furcht vors K\u00fcnft\u2019ge, nicht.", "tokens": ["Mich", ",", "durch", "die", "Furcht", "vors", "K\u00fcnft'\u00b7ge", ",", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich seh, GOtt Lob! auch hier die Spur", "tokens": ["Ich", "seh", ",", "Gott", "Lob", "!", "auch", "hier", "die", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "NN", "$.", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der weisen Ordnung der Natur,", "tokens": ["Der", "wei\u00b7sen", "Ord\u00b7nung", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denke, wenn die Felder wei\u00df,", "tokens": ["Und", "den\u00b7ke", ",", "wenn", "die", "Fel\u00b7der", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser Haupt-Schmuck v\u00f6llig grei\u00df,", "tokens": ["Und", "un\u00b7ser", "Haup\u00b7tSchmuck", "v\u00f6l\u00b7lig", "grei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df wir dem Lenzen n\u00e4her kommen,", "tokens": ["Da\u00df", "wir", "dem", "Len\u00b7zen", "n\u00e4\u00b7her", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier einem, der ver\u00e4nderlich,", "tokens": ["Hier", "ei\u00b7nem", ",", "der", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort aber einem, welcher sich", "tokens": ["Dort", "a\u00b7ber", "ei\u00b7nem", ",", "wel\u00b7cher", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht \u00e4ndert, der uns nie entnommen.", "tokens": ["Nicht", "\u00e4n\u00b7dert", ",", "der", "uns", "nie", "ent\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich seh inde\u00df, so lang ich kann,", "tokens": ["Ich", "seh", "in\u00b7de\u00df", ",", "so", "lang", "ich", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des fr\u00fchen Winters sanftes Prangen,", "tokens": ["Des", "fr\u00fc\u00b7hen", "Win\u00b7ters", "sanf\u00b7tes", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das sich nur eben angefangen,", "tokens": ["Das", "sich", "nur", "e\u00b7ben", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als einen holden Vorwurf an:", "tokens": ["Als", "ei\u00b7nen", "hol\u00b7den", "Vor\u00b7wurf", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergn\u00fcge mich nicht nur allein", "tokens": ["Ver\u00b7gn\u00fc\u00b7ge", "mich", "nicht", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An seinem Silber-gleichen Schein,", "tokens": ["An", "sei\u00b7nem", "Sil\u00b7ber\u00b7glei\u00b7chen", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich denk auch, da\u00df sein gleiches Bild,", "tokens": ["Ich", "denk", "auch", ",", "da\u00df", "sein", "glei\u00b7ches", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So mir bereits mein Haupt erf\u00fcllt,", "tokens": ["So", "mir", "be\u00b7reits", "mein", "Haupt", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mir, wenn ich nur vern\u00fcnftig lebe,", "tokens": ["Mir", ",", "wenn", "ich", "nur", "ver\u00b7n\u00fcnf\u00b7tig", "le\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mehr Anmuht noch, als Schrecken, gebe.", "tokens": ["Mehr", "An\u00b7muht", "noch", ",", "als", "Schre\u00b7cken", ",", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ich dort GOttes Ordnung sehe", "tokens": ["Da", "ich", "dort", "Got\u00b7tes", "Ord\u00b7nung", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jm frohen Wechsel unsrer Zeit;", "tokens": ["Jm", "fro\u00b7hen", "Wech\u00b7sel", "uns\u00b7rer", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So seh ich hier der Ewigkeit", "tokens": ["So", "seh", "ich", "hier", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begl\u00fcckten Fr\u00fchling, in der N\u00e4he,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ten", "Fr\u00fch\u00b7ling", ",", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den unsers Sch\u00f6pfers Liebe fest,", "tokens": ["Den", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Lie\u00b7be", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach meiner kurzen Tage Rest,", "tokens": ["Nach", "mei\u00b7ner", "kur\u00b7zen", "Ta\u00b7ge", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Voll Zuversicht mich hoffen l\u00e4\u00dft.", "tokens": ["Voll", "Zu\u00b7ver\u00b7sicht", "mich", "hof\u00b7fen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich kr\u00e4nkt dabey kein Zweifel mehr,", "tokens": ["Mich", "kr\u00e4nkt", "da\u00b7bey", "kein", "Zwei\u00b7fel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da GOttes Ordnung feste stehet:", "tokens": ["Da", "Got\u00b7tes", "Ord\u00b7nung", "fes\u00b7te", "ste\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Denn hier so wohl, als dorten, gehet", "tokens": ["Denn", "hier", "so", "wohl", ",", "als", "dor\u00b7ten", ",", "ge\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "KOUS", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Winter vor dem Fr\u00fchling her.", "tokens": ["Der", "Win\u00b7ter", "vor", "dem", "Fr\u00fch\u00b7ling", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}