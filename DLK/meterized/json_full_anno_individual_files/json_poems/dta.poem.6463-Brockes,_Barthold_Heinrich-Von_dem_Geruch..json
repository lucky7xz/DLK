{"dta.poem.6463": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der unersch\u00f6pffliche Gernch, den Ambra heget,", "tokens": ["Der", "un\u00b7er\u00b7sch\u00f6pf\u00b7fli\u00b7che", "Gernch", ",", "den", "A\u00b7mbra", "he\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So wie der Museus auch, macht, da\u00df man wol erweget,", "tokens": ["So", "wie", "der", "Mu\u00b7seus", "auch", ",", "macht", ",", "da\u00df", "man", "wol", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "$,", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf welche Weise sie so kleine C\u00f6rperlein,", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sie", "so", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In solcher Zeiten L\u00e4nge", "tokens": ["In", "sol\u00b7cher", "Zei\u00b7ten", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hervor zu bringen f\u00e4hig seyn,", "tokens": ["Her\u00b7vor", "zu", "brin\u00b7gen", "f\u00e4\u00b7hig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zwar in solcher Menge,", "tokens": ["Und", "zwar", "in", "sol\u00b7cher", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die in der Lufft verbreitet, umgef\u00fchret,", "tokens": ["Die", "in", "der", "Lufft", "ver\u00b7brei\u00b7tet", ",", "um\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und durch so sanften Druck Gehirn und Nase r\u00fchret.", "tokens": ["Und", "durch", "so", "sanf\u00b7ten", "Druck", "Ge\u00b7hirn", "und", "Na\u00b7se", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man wundert sich,", "tokens": ["Man", "wun\u00b7dert", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Da\u00df ihre Krafft sich endlich nicht verlieret.", "tokens": ["Da\u00df", "ih\u00b7re", "Krafft", "sich", "end\u00b7lich", "nicht", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Allein, man darf nur dieses dencken,", "tokens": ["Al\u00b7lein", ",", "man", "darf", "nur", "die\u00b7ses", "den\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PIS", "VMFIN", "ADV", "PDAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df die bewegte Theil, als unver\u00e4nderlich,", "tokens": ["Da\u00df", "die", "be\u00b7weg\u00b7te", "Theil", ",", "als", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ob sie sich gleich von ihrer Massa trennen,", "tokens": ["Ob", "sie", "sich", "gleich", "von", "ih\u00b7rer", "Mas\u00b7sa", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sich um sie in der R\u00fcnde lencken.", "tokens": ["Sich", "um", "sie", "in", "der", "R\u00fcn\u00b7de", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Damit wir dieses nun, als m\u00f6glich, fassen k\u00f6nnen,", "tokens": ["Da\u00b7mit", "wir", "die\u00b7ses", "nun", ",", "als", "m\u00f6g\u00b7lich", ",", "fas\u00b7sen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "$,", "KOUS", "ADJD", "$,", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie solch ein Ausflu\u00df immer w\u00e4hre,", "tokens": ["Wie", "solch", "ein", "Aus\u00b7flu\u00df", "im\u00b7mer", "w\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ohn da\u00df ihn jemahls was vermindre, noch zerst\u00f6hre", "tokens": ["Ohn", "da\u00df", "ihn", "je\u00b7mahls", "was", "ver\u00b7mind\u00b7re", ",", "noch", "zer\u00b7st\u00f6h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "ADV", "PWS", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von der empfindlichen und holden S\u00fcssigkeit;", "tokens": ["Von", "der", "emp\u00b7find\u00b7li\u00b7chen", "und", "hol\u00b7den", "S\u00fcs\u00b7sig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So dencke man nur dies dabey,", "tokens": ["So", "den\u00b7cke", "man", "nur", "dies", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PDS", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df die Materie ohn Ende theilbar sey,", "tokens": ["Da\u00df", "die", "Ma\u00b7te\u00b7rie", "ohn", "En\u00b7de", "theil\u00b7bar", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "Und da\u00df ein Theil, wenns auch nicht sichtbar ist,", "tokens": ["Und", "da\u00df", "ein", "Theil", ",", "wenns", "auch", "nicht", "sicht\u00b7bar", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "KOUS", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Sich dennoch immer trenne,", "tokens": ["Sich", "den\u00b7noch", "im\u00b7mer", "tren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und immer kleiner werden k\u00f6nne.", "tokens": ["Und", "im\u00b7mer", "klei\u00b7ner", "wer\u00b7den", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ja dieser Gegenwurff, wenn man ihn recht ermisst,", "tokens": ["Ja", "die\u00b7ser", "Ge\u00b7gen\u00b7wurff", ",", "wenn", "man", "ihn", "recht", "er\u00b7misst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "NN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Beweiset auf das neu,", "tokens": ["Be\u00b7wei\u00b7set", "auf", "das", "neu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}