{"dta.poem.13172": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch deine Gegenwart, begl\u00fccktes Kronen-Haupt!", "tokens": ["Durch", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", ",", "be\u00b7gl\u00fcck\u00b7tes", "Kro\u00b7nen\u00b7Haupt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sey meiner Cither Klang hier darzuthun erlaubt:", "tokens": ["Sey", "mei\u00b7ner", "Cit\u00b7her", "Klang", "hier", "dar\u00b7zu\u00b7thun", "er\u00b7laubt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df das, was er besingt, kein Staats-Gedicht zu nennen,", "tokens": ["Da\u00df", "das", ",", "was", "er", "be\u00b7singt", ",", "kein", "Staats\u00b7Ge\u00b7dicht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wovor die sp\u00e4te Welt es etwann wurd\u2019 erkennen:", "tokens": ["Wo\u00b7vor", "die", "sp\u00e4\u00b7te", "Welt", "es", "et\u00b7wann", "wurd'", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measu