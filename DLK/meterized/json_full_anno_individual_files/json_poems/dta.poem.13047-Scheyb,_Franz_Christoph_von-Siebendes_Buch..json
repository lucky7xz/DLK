{"dta.poem.13047": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich gehe weiter fort: hat eurer Tugend Macht", "tokens": ["\u201e", "ich", "ge\u00b7he", "wei\u00b7ter", "fort", ":", "hat", "eu\u00b7rer", "Tu\u00b7gend", "Macht"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PTKVZ", "$.", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edas Werck allein gef\u00fchrt und fast zum Ziel gebracht;", "tokens": ["\u201e", "das", "Werck", "al\u00b7lein", "ge\u00b7f\u00fchrt", "und", "fast", "zum", "Ziel", "ge\u00b7bracht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VVPP", "KON", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "395\u201dWas seynd vor Wirckungen aus dieser M\u00fch entsprossen,", "tokens": ["\"", "Was", "seynd", "vor", "Wir\u00b7ckun\u00b7gen", "aus", "die\u00b7ser", "M\u00fch", "ent\u00b7spros\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "APPR", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u201edie diese Friedens-Kriegs- und Staats- und Lands-Genossen", "tokens": ["\u201e", "die", "die\u00b7se", "Frie\u00b7dens\u00b7Kriegs", "und", "Staats", "und", "Lands\u00b7Ge\u00b7nos\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "PDAT", "TRUNC", "KON", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eallhier so Tag als Nacht, im Feld das ganze Jahr", "tokens": ["\u201e", "all\u00b7hier", "so", "Tag", "als", "Nacht", ",", "im", "Feld", "das", "gan\u00b7ze", "Jahr"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "NN", "KOUS", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ein Frost, in Hiz, in Durst, in Hunger, in Gefahr", "tokens": ["\u201e", "in", "Frost", ",", "in", "Hiz", ",", "in", "Durst", ",", "in", "Hun\u00b7ger", ",", "in", "Ge\u00b7fahr"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "APPR", "NE", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201emit Unverdrossenheit und Eifer angewendet?", "tokens": ["\u201e", "mit", "Un\u00b7ver\u00b7dros\u00b7sen\u00b7heit", "und", "Ei\u00b7fer", "an\u00b7ge\u00b7wen\u00b7det", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "400\u201dW\u00e4r dieses, sagt es frey, nicht unfruchtbar verschwendet?", "tokens": ["\"", "W\u00e4r", "die\u00b7ses", ",", "sagt", "es", "frey", ",", "nicht", "un\u00b7frucht\u00b7bar", "ver\u00b7schwen\u00b7det", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "$,", "VVFIN", "PPER", "ADJD", "$,", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewer wiederspricht mir die\u00df mit Recht und gutem Fug?", "tokens": ["\u201e", "wer", "wie\u00b7der\u00b7spricht", "mir", "die\u00df", "mit", "Recht", "und", "gu\u00b7tem", "Fug", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PDS", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ehat man nicht \u00fcberall Beweisth\u00fcmer genug?", "tokens": ["\u201e", "hat", "man", "nicht", "\u00fc\u00b7be\u00b7rall", "Be\u00b7weist\u00b7h\u00fc\u00b7mer", "ge\u00b7nug", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "PTKNEG", "ADV", "NN", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "\u201ezeigt nicht fast jedes Werck die Zeichen und die Spuhren,", "tokens": ["\u201e", "zeigt", "nicht", "fast", "je\u00b7des", "Werck", "die", "Zei\u00b7chen", "und", "die", "Spuh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201edurch die wir den Erfolg so grosser M\u00fch erfuhren?", "tokens": ["\u201e", "durch", "die", "wir", "den", "Er\u00b7folg", "so", "gros\u00b7ser", "M\u00fch", "er\u00b7fuh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "405\u201dMit einem Wort: es ist kein Kriegs-kein Friedens-Held,", "tokens": ["\"", "Mit", "ei\u00b7nem", "Wort", ":", "es", "ist", "kein", "Kriegs\u00b7\u00b7kein", "Frie\u00b7dens\u00b7Held", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201evon dessen Emsigkeit, Verdienst und Ruhm ich meld,", "tokens": ["\u201e", "von", "des\u00b7sen", "Em\u00b7sig\u00b7keit", ",", "Ver\u00b7dienst", "und", "Ruhm", "ich", "meld", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELAT", "NN", "$,", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eder nicht um diesen Stein mit euch soll streiten k\u00f6nnen,", "tokens": ["\u201e", "der", "nicht", "um", "die\u00b7sen", "Stein", "mit", "euch", "soll", "strei\u00b7ten", "k\u00f6n\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PTKNEG", "APPR", "PDAT", "NN", "APPR", "PPER", "VMFIN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201ewir k\u00f6nnten jeden fast desselben w\u00fcrdig nennen.", "tokens": ["\u201e", "wir", "k\u00f6nn\u00b7ten", "je\u00b7den", "fast", "des\u00b7sel\u00b7ben", "w\u00fcr\u00b7dig", "nen\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PIS", "ADV", "PDS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201ewer aber wurd am Schlu\u00df der Auserw\u00e4hlte seyn?", "tokens": ["\u201e", "wer", "a\u00b7ber", "wurd", "am", "Schlu\u00df", "der", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", "seyn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "410\u201dWem g\u00e4bet ihr das Recht zu diesem Ehren-Stein?", "tokens": ["\"", "Wem", "g\u00e4\u00b7bet", "ihr", "das", "Recht", "zu", "die\u00b7sem", "Eh\u00b7ren\u00b7Stein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201ewer wei\u00df es? und wer kann derselben Thaten z\u00e4hlen?", "tokens": ["\u201e", "wer", "wei\u00df", "es", "?", "und", "wer", "kann", "der\u00b7sel\u00b7ben", "Tha\u00b7ten", "z\u00e4h\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "KON", "PWS", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201ewie wurdet ihr sodann den Treflichsten erw\u00e4hlen?", "tokens": ["\u201e", "wie", "wur\u00b7det", "ihr", "so\u00b7dann", "den", "Tref\u00b7lichs\u00b7ten", "er\u00b7w\u00e4h\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}}}}