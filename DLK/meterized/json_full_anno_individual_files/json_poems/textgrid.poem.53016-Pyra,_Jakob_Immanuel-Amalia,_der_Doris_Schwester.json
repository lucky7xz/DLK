{"textgrid.poem.53016": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Amalia, der Doris Schwester", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist es. Ja! an ihrer Seite", "tokens": ["Sie", "ist", "es", ".", "Ja", "!", "an", "ih\u00b7rer", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$.", "PTKANT", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sitzt unsre Doris, neben ihr.", "tokens": ["Sitzt", "uns\u00b7re", "Do\u00b7ris", ",", "ne\u00b7ben", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "APPR", "PPOSAT", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "So stellet ich sie mir noch heute,", "tokens": ["So", "stel\u00b7let", "ich", "sie", "mir", "noch", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So angenehm, so artig f\u00fcr.", "tokens": ["So", "an\u00b7ge\u00b7nehm", ",", "so", "ar\u00b7tig", "f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist ihr sch\u00f6nes Angesichte,", "tokens": ["So", "ist", "ihr", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So tr\u00e4gt sie Hand und Brust und Leib.", "tokens": ["So", "tr\u00e4gt", "sie", "Hand", "und", "Brust", "und", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O Muse, sieh das muntre Weib,", "tokens": ["O", "Mu\u00b7se", ",", "sieh", "das", "mun\u00b7tre", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und freu dich, auf des Geistes Fr\u00fcchte.", "tokens": ["Und", "freu", "dich", ",", "auf", "des", "Geis\u00b7tes", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jezt winckt mir schon die holde Doris,", "tokens": ["Jezt", "winckt", "mir", "schon", "die", "hol\u00b7de", "Do\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihr rollend Fahrzeug k\u00f6mmt uns nah.", "tokens": ["Ihr", "rol\u00b7lend", "Fahr\u00b7zeug", "k\u00f6mmt", "uns", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willkommen unsre andre Chloris,", "tokens": ["Will\u00b7kom\u00b7men", "uns\u00b7re", "and\u00b7re", "Chlo\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Willkommen o Amalia!", "tokens": ["Will\u00b7kom\u00b7men", "o", "A\u00b7ma\u00b7lia", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "FM", "NE", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Die Chloris, die der Fr\u00fchling f\u00fchret,", "tokens": ["Die", "Chlo\u00b7ris", ",", "die", "der", "Fr\u00fch\u00b7ling", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat ihre Sch\u00e4tze angewandt", "tokens": ["Hat", "ih\u00b7re", "Sch\u00e4t\u00b7ze", "an\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schon mit blumenvoller Hand", "tokens": ["Und", "schon", "mit", "blu\u00b7men\u00b7vol\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Wiesen f\u00fcr dich ausgezieret.", "tokens": ["Die", "Wie\u00b7sen", "f\u00fcr", "dich", "aus\u00b7ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Fr\u00fch hab ich selber zugeh\u00f6ret,", "tokens": ["Fr\u00fch", "hab", "ich", "sel\u00b7ber", "zu\u00b7ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie sich das Chor der Nymphen \u00fcbt,", "tokens": ["Wie", "sich", "das", "Chor", "der", "Nym\u00b7phen", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stets einander singend lehret,", "tokens": ["Und", "stets", "ein\u00b7an\u00b7der", "sin\u00b7gend", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie man die prei\u00dft, die jeder liebt.", "tokens": ["Wie", "man", "die", "prei\u00dft", ",", "die", "je\u00b7der", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald sah ich eine sich entfernen,", "tokens": ["Bald", "sah", "ich", "ei\u00b7ne", "sich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dort von einer Nachtigall", "tokens": ["Und", "dort", "von", "ei\u00b7ner", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den k\u00fcnstlich ungezwungnen Schall,", "tokens": ["Den", "k\u00fcnst\u00b7lich", "un\u00b7ge\u00b7zwung\u00b7nen", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Manier und Harmonien lernen.", "tokens": ["Ma\u00b7nier", "und", "Har\u00b7mo\u00b7ni\u00b7en", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da andre dort vereinigt singen,", "tokens": ["Da", "and\u00b7re", "dort", "ver\u00b7ei\u00b7nigt", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So l\u00e4\u00dft die Echo durch den Wald", "tokens": ["So", "l\u00e4\u00dft", "die", "E\u00b7cho", "durch", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Amalia zuletzt erklingen,", "tokens": ["A\u00b7ma\u00b7lia", "zu\u00b7letzt", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es durch Feld und Wiesen schallt.", "tokens": ["Da\u00df", "es", "durch", "Feld", "und", "Wie\u00b7sen", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die gantze Gegend ist erreget,", "tokens": ["Die", "gant\u00b7ze", "Ge\u00b7gend", "ist", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jedweder Sch\u00e4fer eilt zum Tantz,", "tokens": ["Jed\u00b7we\u00b7der", "Sch\u00e4\u00b7fer", "eilt", "zum", "Tantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Geschm\u00fcckt mit einem jungen Krantz,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "ei\u00b7nem", "jun\u00b7gen", "Krantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wie man sonst an Festen pfleget.", "tokens": ["So", "wie", "man", "sonst", "an", "Fes\u00b7ten", "pfle\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du wirst dich auch nicht wundern k\u00f6nnen,", "tokens": ["Du", "wirst", "dich", "auch", "nicht", "wun\u00b7dern", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihnen alles die\u00df bewu\u00dft;", "tokens": ["Da\u00df", "ih\u00b7nen", "al\u00b7les", "die\u00df", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PDS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie h\u00f6rten dich fast t\u00e4glich nennen,", "tokens": ["Sie", "h\u00f6r\u00b7ten", "dich", "fast", "t\u00e4g\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sprachen stets von dir mit Lust.", "tokens": ["Wir", "spra\u00b7chen", "stets", "von", "dir", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, la\u00df es dir bey uns gefallen,", "tokens": ["Komm", ",", "la\u00df", "es", "dir", "bey", "uns", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Nimm alles selbst in Augenschein.", "tokens": ["Nimm", "al\u00b7les", "selbst", "in", "Au\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und denn la\u00df durch den kleinen Hain", "tokens": ["Und", "denn", "la\u00df", "durch", "den", "klei\u00b7nen", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KON", "VVIMP", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Auch deine sanfte Fl\u00f6te schallen.", "tokens": ["Auch", "dei\u00b7ne", "sanf\u00b7te", "Fl\u00f6\u00b7te", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie ist es. Ja! an ihrer Seite", "tokens": ["Sie", "ist", "es", ".", "Ja", "!", "an", "ih\u00b7rer", "Sei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$.", "PTKANT", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sitzt unsre Doris, neben ihr.", "tokens": ["Sitzt", "uns\u00b7re", "Do\u00b7ris", ",", "ne\u00b7ben", "ihr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM