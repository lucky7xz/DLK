{"textgrid.poem.42389": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft muss ich denken: Wie mein Haar ergraut!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft muss ich denken: Wie mein Haar ergraut!", "tokens": ["Oft", "muss", "ich", "den\u00b7ken", ":", "Wie", "mein", "Haar", "er\u00b7graut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind denn noch immer bl\u00fchend meine Wangen?", "tokens": ["Sind", "denn", "noch", "im\u00b7mer", "bl\u00fc\u00b7hend", "mei\u00b7ne", "Wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie wenn ein Wanderer nach r\u00fcckw\u00e4rts schaut", "tokens": ["Wie", "wenn", "ein", "Wan\u00b7de\u00b7rer", "nach", "r\u00fcck\u00b7w\u00e4rts", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und zu sich spricht: Wie bin ich weit gegangen!", "tokens": ["Und", "zu", "sich", "spricht", ":", "Wie", "bin", "ich", "weit", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann dr\u00e4ngt inbr\u00fcnstiger noch mein Gef\u00fchl", "tokens": ["Dann", "dr\u00e4ngt", "in\u00b7br\u00fcns\u00b7ti\u00b7ger", "noch", "mein", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich zu dem Heute, das noch nicht entschwebte,", "tokens": ["Sich", "zu", "dem", "Heu\u00b7te", ",", "das", "noch", "nicht", "ent\u00b7schweb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der Vergangenheit enttaucht so k\u00fchl,", "tokens": ["Und", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "ent\u00b7taucht", "so", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ehedem so schmerzlich ich durchlebte.", "tokens": ["Was", "e\u00b7he\u00b7dem", "so", "schmerz\u00b7lich", "ich", "durch\u00b7leb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So kommt ein Freund, den du verlorst, vielleicht", "tokens": ["So", "kommt", "ein", "Freund", ",", "den", "du", "ver\u00b7lorst", ",", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von ungef\u00e4hr dir \u00fcbern Weg nach Jahren,", "tokens": ["Von", "un\u00b7ge\u00b7f\u00e4hr", "dir", "\u00fc\u00b7bern", "Weg", "nach", "Jah\u00b7ren", ",