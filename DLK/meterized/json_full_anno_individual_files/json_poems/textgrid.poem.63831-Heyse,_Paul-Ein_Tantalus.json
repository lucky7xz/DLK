{"textgrid.poem.63831": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Ein Tantalus", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nino ist todt. Heut in der Fr\u00fche fand", "tokens": ["Ni\u00b7no", "ist", "todt", ".", "Heut", "in", "der", "Fr\u00fc\u00b7he", "fand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADJD", "$.", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Herr auf d\u00fcrft'gem Lager ihn entseelt,", "tokens": ["Sein", "Herr", "auf", "d\u00fcrft'\u00b7gem", "La\u00b7ger", "ihn", "ent\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Miene sanft, wie eines Schlafenden,", "tokens": ["Die", "Mie\u00b7ne", "sanft", ",", "wie", "ei\u00b7nes", "Schla\u00b7fen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Doch alles R\u00fctteln, aller Zuruf war", "tokens": ["Doch", "al\u00b7les", "R\u00fct\u00b7teln", ",", "al\u00b7ler", "Zu\u00b7ruf", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Umsonst. Zu seiner Tagesfrone stand", "tokens": ["Um\u00b7sonst", ".", "Zu", "sei\u00b7ner", "Ta\u00b7ges\u00b7fro\u00b7ne", "stand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er nicht mehr auf.", "tokens": ["Er", "nicht", "mehr", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Undank, der Lohn der Welt,", "tokens": ["Un\u00b7dank", ",", "der", "Lohn", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Auch dir, du frommer Knecht, ward er zu Teil.", "tokens": ["Auch", "dir", ",", "du", "from\u00b7mer", "Knecht", ",", "ward", "er", "zu", "Teil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn von den Deinen, denen Jahre lang", "tokens": ["Denn", "von", "den", "Dei\u00b7nen", ",", "de\u00b7nen", "Jah\u00b7re", "lang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Du treu gedient, ward eine Tr\u00e4ne kaum", "tokens": ["Du", "treu", "ge\u00b7dient", ",", "ward", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVPP", "$,", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dir nachgeweint, kein Grabgesang ert\u00f6nt:", "tokens": ["Dir", "nach\u00b7ge\u00b7weint", ",", "kein", "Grab\u00b7ge\u00b7sang", "er\u00b7t\u00f6nt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In steinigem Acker wirst du eingescharrt", "tokens": ["In", "stei\u00b7ni\u00b7gem", "A\u00b7cker", "wirst", "du", "ein\u00b7ge\u00b7scharrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und morgen schon vergessen.", "tokens": ["Und", "mor\u00b7gen", "schon", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ich nur widme", "tokens": ["Ich", "nur", "wid\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Bewegt dir einen Nachruf. Denn du warst", "tokens": ["Be\u00b7wegt", "dir", "ei\u00b7nen", "Nach\u00b7ruf", ".", "Denn", "du", "warst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Zwar nur ein Pferd, doch gut und sanft und wohl", "tokens": ["Zwar", "nur", "ein", "Pferd", ",", "doch", "gut", "und", "sanft", "und", "wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Auch h\u00fcbsch in deiner Jugend, bis die Last", "tokens": ["Auch", "h\u00fcbsch", "in", "dei\u00b7ner", "Ju\u00b7gend", ",", "bis", "die", "Last"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Arbeit dir das glatte Fell verdarb", "tokens": ["Der", "Ar\u00b7beit", "dir", "das", "glat\u00b7te", "Fell", "ver\u00b7darb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und dir der R\u00fccken einsank. Dies zwar ist", "tokens": ["Und", "dir", "der", "R\u00fc\u00b7cken", "ein\u00b7sank", ".", "Dies", "zwar", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "PTKVZ", "$.", "PDS", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Gemeines Pferdelos. Dich aber traf", "tokens": ["Ge\u00b7mei\u00b7nes", "Pfer\u00b7de\u00b7los", ".", "Dich", "a\u00b7ber", "traf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ein schlimmres, denn du warst ein Tantalus.", "tokens": ["Ein", "schlimm\u00b7res", ",", "denn", "du", "warst", "ein", "Tan\u00b7ta\u00b7lus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Dein Herr, der Fruttajuol, ein wackrer Mann,", "tokens": ["Dein", "Herr", ",", "der", "Frut\u00b7ta\u00b7juol", ",", "ein", "wack\u00b7rer", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Doch ahnungslos f\u00fcr Pferdeseelenschmerz,", "tokens": ["Doch", "ah\u00b7nungs\u00b7los", "f\u00fcr", "Pfer\u00b7de\u00b7see\u00b7len\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "An jedem fr\u00fchen Morgen spannt' er dich", "tokens": ["An", "je\u00b7dem", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "spannt'", "er", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Vor seinen Karren, drauf in K\u00f6rben frisch", "tokens": ["Vor", "sei\u00b7nen", "Kar\u00b7ren", ",", "drauf", "in", "K\u00f6r\u00b7ben", "frisch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PAV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ein mancherlei Gem\u00fcse lag, Salat,", "tokens": ["Ein", "man\u00b7cher\u00b7lei", "Ge\u00b7m\u00fc\u00b7se", "lag", ",", "Sa\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Kohl, Artischocken, Petersilie, auch", "tokens": ["Kohl", ",", "Ar\u00b7ti\u00b7scho\u00b7cken", ",", "Pe\u00b7ter\u00b7si\u00b7lie", ",", "auch"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Spinat und w\u00fcrz'ger Fenchel und was sonst", "tokens": ["Spi\u00b7nat", "und", "w\u00fcrz'\u00b7ger", "Fen\u00b7chel", "und", "was", "sonst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN", "KON", "PWS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Gott in Italiens G\u00e4rten wachsen l\u00e4\u00dft.", "tokens": ["Gott", "in", "I\u00b7ta\u00b7li\u00b7ens", "G\u00e4r\u00b7ten", "wach\u00b7sen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.30": {"text": "Dann hui! und omm! und Peitschenknall, auch wohl", "tokens": ["Dann", "hui", "!", "und", "omm", "!", "und", "Peit\u00b7schen\u00b7knall", ",", "auch", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NE", "$.", "KON", "ADJD", "$.", "KON", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ein Peitschenhieb, und auf der Stra\u00dfe fort", "tokens": ["Ein", "Peit\u00b7schen\u00b7hieb", ",", "und", "auf", "der", "Stra\u00b7\u00dfe", "fort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Zogst du die gr\u00fcne Ware f\u00fcr den Tisch", "tokens": ["Zogst", "du", "die", "gr\u00fc\u00b7ne", "Wa\u00b7re", "f\u00fcr", "den", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Der Villen, hieltst vor jeder T\u00fcre still", "tokens": ["Der", "Vil\u00b7len", ",", "hieltst", "vor", "je\u00b7der", "T\u00fc\u00b7re", "still"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und h\u00f6rtest hinter dir die K\u00f6chin feilschen", "tokens": ["Und", "h\u00f6r\u00b7test", "hin\u00b7ter", "dir", "die", "K\u00f6\u00b7chin", "feil\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Mit deinem Herrn um Leckerbissen, die", "tokens": ["Mit", "dei\u00b7nem", "Herrn", "um", "Le\u00b7cker\u00b7bis\u00b7sen", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Du nie gekostet. Wenn die Hausfrau dir,", "tokens": ["Du", "nie", "ge\u00b7kos\u00b7tet", ".", "Wenn", "die", "Haus\u00b7frau", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$.", "KOUS", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Mitleidig gegen jegliches Getier,", "tokens": ["Mit\u00b7lei\u00b7dig", "ge\u00b7gen", "jeg\u00b7li\u00b7ches", "Ge\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Ein Wei\u00dfbrot spendet' und zuweilen auch", "tokens": ["Ein", "Wei\u00df\u00b7brot", "spen\u00b7det'", "und", "zu\u00b7wei\u00b7len", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Ein St\u00fccklein Zucker, dankbar nahmst du's hin", "tokens": ["Ein", "St\u00fcck\u00b7lein", "Zu\u00b7cker", ",", "dank\u00b7bar", "nahmst", "du's", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "VVFIN", "PIS", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und seufzend doch \u2013 ach, nur ein Tropfen war's", "tokens": ["Und", "seuf\u00b7zend", "doch", "\u2013", "ach", ",", "nur", "ein", "Trop\u00b7fen", "wa\u00b7r's"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "$(", "XY", "$,", "ADV", "ART", "NN", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.41": {"text": "Auf hei\u00dfen Stein! Wie h\u00e4ttst du erst geschwelgt", "tokens": ["Auf", "hei\u00b7\u00dfen", "Stein", "!", "Wie", "h\u00e4ttst", "du", "erst", "ge\u00b7schwelgt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Im saft'gen Gr\u00fcnzeug, dem du vorgespannt!", "tokens": ["Im", "saft'\u00b7gen", "Gr\u00fcn\u00b7zeug", ",", "dem", "du", "vor\u00b7ge\u00b7spannt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und wieder hui! und omm! und weiter ging's", "tokens": ["Und", "wie\u00b7der", "hui", "!", "und", "omm", "!", "und", "wei\u00b7ter", "ging's"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "NE", "$.", "KON", "ADJD", "$.", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Den Leidensweg. ", "tokens": ["Den", "Lei\u00b7dens\u00b7weg", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "Dein vorwurfsvoller Blick.", "tokens": ["Dein", "vor\u00b7wurfs\u00b7vol\u00b7ler", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Nun ruhst du aus", "tokens": ["Nun", "ruhst", "du", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "Von ungestillter Sehnsucht, und ein andrer,", "tokens": ["Von", "un\u00b7ge\u00b7still\u00b7ter", "Sehn\u00b7sucht", ",", "und", "ein", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Nicht braun wie du und auch so mager nicht,", "tokens": ["Nicht", "braun", "wie", "du", "und", "auch", "so", "ma\u00b7ger", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KOKOM", "PPER", "KON", "ADV", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Ein muntrer Scheck zieht den Gem\u00fcsekarren,", "tokens": ["Ein", "mun\u00b7trer", "Scheck", "zieht", "den", "Ge\u00b7m\u00fc\u00b7se\u00b7kar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Noch ganz vergn\u00fcgt. Ich aber seh' voraus,", "tokens": ["Noch", "ganz", "ver\u00b7gn\u00fcgt", ".", "Ich", "a\u00b7ber", "seh'", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$.", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "Was seiner harrt, und seufze mitleidsvoll:", "tokens": ["Was", "sei\u00b7ner", "harrt", ",", "und", "seuf\u00b7ze", "mit\u00b7le\u00b7