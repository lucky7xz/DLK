{"textgrid.poem.39729": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Schlittenlied", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter muntrer Gl\u00f6cklein Schallen", "tokens": ["Un\u00b7ter", "mun\u00b7trer", "Gl\u00f6c\u00b7klein", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Raschelt's wie ein Elfenzug,", "tokens": ["Ra\u00b7schelt's", "wie", "ein", "El\u00b7fen\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig drein die Peitschen knallen,", "tokens": ["Freu\u00b7dig", "drein", "die", "Peit\u00b7schen", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles schwindet hin im Flug:", "tokens": ["Al\u00b7les", "schwin\u00b7det", "hin", "im", "Flug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosse, Reiter, in der Mitten", "tokens": ["Ros\u00b7se", ",", "Rei\u00b7ter", ",", "in", "der", "Mit\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mutig die besonnten Schlitten,", "tokens": ["Mu\u00b7tig", "die", "be\u00b7sonn\u00b7ten", "Schlit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die in Sammt und Pelz geh\u00fcllt", "tokens": ["Die", "in", "Sammt", "und", "Pelz", "ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Niedlich Feenvolk erf\u00fcllt.", "tokens": ["Nied\u00b7lich", "Feen\u00b7volk", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Kaum begonnen hat die Wonne;", "tokens": ["Kaum", "be\u00b7gon\u00b7nen", "hat", "die", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist schon wieder alles aus?", "tokens": ["Ist", "schon", "wie\u00b7der", "al\u00b7les", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg aus Duft und Schnee und Sonne", "tokens": ["Weg", "aus", "Duft", "und", "Schnee", "und", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen wir ins dumpfe Haus?", "tokens": ["Sol\u00b7len", "wir", "ins", "dump\u00b7fe", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch es \u00f6ffnen sich die Th\u00fcren", "tokens": ["Doch", "es", "\u00f6ff\u00b7nen", "sich", "die", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter lust'gem Musiciren;", "tokens": ["Un\u00b7ter", "lust'\u00b7gem", "Mu\u00b7si\u00b7ci\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Freundlich steht zu Tanz und Mahl", "tokens": ["Freund\u00b7lich", "steht", "zu", "Tanz", "und", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aufgeschm\u00fcckt der kleine Saal.", "tokens": ["Auf\u00b7ge\u00b7schm\u00fcckt", "der", "klei\u00b7ne", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eilig streift die Winterh\u00fclle", "tokens": ["Ei\u00b7lig", "streift", "die", "Win\u00b7ter\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes sch\u00f6ne Kind von sich,", "tokens": ["Je\u00b7des", "sch\u00f6\u00b7ne", "Kind", "von", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmuck und hell, in s\u00fc\u00dfer F\u00fclle,", "tokens": ["Schmuck", "und", "hell", ",", "in", "s\u00fc\u00b7\u00dfer", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchten alle sommerlich;", "tokens": ["Leuch\u00b7ten", "al\u00b7le", "som\u00b7mer\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wissen mit den stillen Blicken", "tokens": ["Wis\u00b7sen", "mit", "den", "stil\u00b7len", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! so lieblich zu begl\u00fccken,", "tokens": ["Ach", "!", "so", "lieb\u00b7lich", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Holde Rede klingt darein \u2013", "tokens": ["Hol\u00b7de", "Re\u00b7de", "klingt", "da\u00b7rein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann es wohl noch Winter sein?", "tokens": ["Kann", "es", "wohl", "noch", "Win\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie sich's tanzt so freudig heute,", "tokens": ["Wie", "sich's", "tanzt", "so", "freu\u00b7dig", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich's noch besser schmaust und singt!", "tokens": ["Sich's", "noch", "bes\u00b7ser", "schmaust", "und", "singt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn, die Freundlichen zur Seite,", "tokens": ["Wenn", ",", "die", "Freund\u00b7li\u00b7chen", "zur", "Sei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glas mit Glas zusammenklingt;", "tokens": ["Glas", "mit", "Glas", "zu\u00b7sam\u00b7men\u00b7klingt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn, was Keiner wagt zu sagen,", "tokens": ["Wenn", ",", "was", "Kei\u00b7ner", "wagt", "zu", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder darf zu singen wagen;", "tokens": ["Je\u00b7der", "darf", "zu", "sin\u00b7gen", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Rauscht das Lied, und gl\u00fcht der Wein \u2013", "tokens": ["Rauscht", "das", "Lied", ",", "und", "gl\u00fcht", "der", "Wein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann es wohl noch Winter sein?", "tokens": ["Kann", "es", "wohl", "noch", "Win\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Drau\u00dfen spielet licht und leise", "tokens": ["Drau\u00b7\u00dfen", "spie\u00b7let", "licht", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Schnee der Mondenschein;", "tokens": ["Mit", "dem", "Schnee", "der", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromm beschickt man sich zur Reise,", "tokens": ["Fromm", "be\u00b7schickt", "man", "sich", "zur", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegt im hellen Traum herein,", "tokens": ["Fliegt", "im", "hel\u00b7len", "Traum", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirft sich tr\u00e4umend hin aufs Bette,", "tokens": ["Wirft", "sich", "tr\u00e4u\u00b7mend", "hin", "aufs", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und um jede Schlummerst\u00e4tte", "tokens": ["Und", "um", "je\u00b7de", "Schlum\u00b7mer\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wogt im Schlafe Tanz und Sang", "tokens": ["Wogt", "im", "Schla\u00b7fe", "Tanz", "und", "Sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch die ganze Nacht entlang.", "tokens": ["Noch", "die", "gan\u00b7ze", "Nacht", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer, zur Hand die treue Leier,", "tokens": ["Wer", ",", "zur", "Hand", "die", "treu\u00b7e", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dieses kleine Lied erdacht,", "tokens": ["Die\u00b7ses", "klei\u00b7ne", "Lied", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Preist zum letzten Mal die Feier", "tokens": ["Preist", "zum", "letz\u00b7ten", "Mal", "die", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solcher sch\u00f6nen Winternacht:", "tokens": ["Sol\u00b7cher", "sch\u00f6\u00b7nen", "Win\u00b7ter\u00b7nacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die Flocken wieder fl\u00fcstern,", "tokens": ["Wann", "die", "Flo\u00b7cken", "wie\u00b7der", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohnt er unter den Philistern;", "tokens": ["Wohnt", "er", "un\u00b7ter", "den", "Phi\u00b7lis\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fahrt kehrt wieder, Sang und Klang \u2013", "tokens": ["Fahrt", "kehrt", "wie\u00b7der", ",", "Sang", "und", "Klang", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch vergessen ist er lang!", "tokens": ["Doch", "ver\u00b7ges\u00b7sen", "ist", "er", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unter muntrer Gl\u00f6cklein Schallen", "tokens": ["Un\u00b7ter", "mun\u00b7trer", "Gl\u00f6c\u00b7klein", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Raschelt's wie ein Elfenzug,", "tokens": ["Ra\u00b7schelt's", "wie", "ein", "El\u00b7fen\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig drein die Peitschen knallen,", "tokens": ["Freu\u00b7dig", "drein", "die", "Peit\u00b7schen", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles schwindet hin im Flug:", "tokens": ["Al\u00b7les", "schwin\u00b7det", "hin", "im", "Flug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measu