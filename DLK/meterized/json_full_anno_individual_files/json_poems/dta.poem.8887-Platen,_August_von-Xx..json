{"dta.poem.8887": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die tausendfach sich ineinander schlingen,", "tokens": ["Die", "tau\u00b7send\u00b7fach", "sich", "in\u00b7ein\u00b7an\u00b7der", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie wird hindurchzugehn mir je gelingen?", "tokens": ["Wie", "wird", "hin\u00b7durch\u00b7zu\u00b7gehn", "mir", "je", "ge\u00b7lin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie werd' ich je dies gro\u00dfe R\u00e4thsel fassen?", "tokens": ["Wie", "werd'", "ich", "je", "dies", "gro\u00b7\u00dfe", "R\u00e4th\u00b7sel", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ersteigend erst des Markusthurms Terrassen,", "tokens": ["Er\u00b7stei\u00b7gend", "erst", "des", "Mar\u00b7kust\u00b7hurms", "Ter\u00b7ras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vermag ich vorw\u00e4rts mit dem Blick zu dringen,", "tokens": ["Ver\u00b7mag", "ich", "vor\u00b7w\u00e4rts", "mit", "dem", "Blick", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus den Wundern, welche mich umringen,", "tokens": ["Und", "aus", "den", "Wun\u00b7dern", ",", "wel\u00b7che", "mich", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entsteht ein Bild, es theilen sich die Massen.", "tokens": ["Ent\u00b7steht", "ein", "Bild", ",", "es", "thei\u00b7len", "sich", "die", "Mas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich gr\u00fc\u00dfe dort den Ocean, den blauen,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dort", "den", "O\u00b7cean", ",", "den", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hier die Alpen, die im weiten Bogen", "tokens": ["Und", "hier", "die", "Al\u00b7pen", ",", "die", "im", "wei\u00b7ten", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf die Laguneninseln niederschauen.", "tokens": ["Auf", "die", "La\u00b7gu\u00b7nen\u00b7in\u00b7seln", "nie\u00b7der\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sieh! da kam ein muth'ges Volk gezogen,", "tokens": ["Und", "sieh", "!", "da", "kam", "ein", "muth'\u00b7ges", "Volk", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pall\u00e4ste sich und Tempel sich zu bauen", "tokens": ["Pal\u00b7l\u00e4s\u00b7te", "sich", "und", "Tem\u00b7pel", "sich", "zu", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "KON", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Eichenpf\u00e4hle mitten in die Wogen.", "tokens": ["Auf", "Ei\u00b7chen\u00b7pf\u00e4h\u00b7le", "mit\u00b7ten", "in", "die", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}