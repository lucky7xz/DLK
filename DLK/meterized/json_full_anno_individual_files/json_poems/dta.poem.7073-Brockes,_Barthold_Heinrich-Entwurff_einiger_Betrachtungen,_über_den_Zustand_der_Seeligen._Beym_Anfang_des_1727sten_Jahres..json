{"dta.poem.7073": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wird sie von den Herrlichkeiten", "tokens": ["Wie", "wird", "sie", "von", "den", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wunderw\u00fcrdigen Vollkommenheiten", "tokens": ["Und", "wun\u00b7der\u00b7w\u00fcr\u00b7di\u00b7gen", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sch\u00f6pffung nicht seyn eingenommen!", "tokens": ["Der", "Sch\u00f6pf\u00b7fung", "nicht", "seyn", "ein\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit welcher Lust, mit welchem Freuden-Schein", "tokens": ["Mit", "wel\u00b7cher", "Lust", ",", "mit", "wel\u00b7chem", "Freu\u00b7den\u00b7Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird das Erl\u00f6sungs-Werck ihr innerstes nicht r\u00fchren,", "tokens": ["Wird", "das", "Er\u00b7l\u00f6\u00b7sungs\u00b7\u00b7Werck", "ihr", "in\u00b7ners\u00b7tes", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wenn sie viel deutlicher, als hier auf Erden,", "tokens": ["Wenn", "sie", "viel", "deut\u00b7li\u00b7cher", ",", "als", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "KOUS", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Desselben Gr\u00f6sse wird verm\u00f6gend seyn zu sp\u00fchren!", "tokens": ["Des\u00b7sel\u00b7ben", "Gr\u00f6s\u00b7se", "wird", "ver\u00b7m\u00f6\u00b7gend", "seyn", "zu", "sp\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "VAINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von welchem Wunder ich, dieweil ich es nicht fasse,", "tokens": ["Von", "wel\u00b7chem", "Wun\u00b7der", "ich", ",", "die\u00b7weil", "ich", "es", "nicht", "fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Voll Ehrfurcht, schweig, und von desselben Tieffe", "tokens": ["Voll", "Ehr\u00b7furcht", ",", "schweig", ",", "und", "von", "des\u00b7sel\u00b7ben", "Tief\u00b7fe"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "VVFIN", "$,", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die GOtts-Gelehrten singen lasse.", "tokens": ["Die", "Gotts\u00b7Ge\u00b7lehr\u00b7ten", "sin\u00b7gen", "las\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit welchem Glantz, mit welchem Wunder-Licht", "tokens": ["Mit", "wel\u00b7chem", "Glantz", ",", "mit", "wel\u00b7chem", "Wun\u00b7der\u00b7Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der G\u00f6ttlichen Versehung, wird sie nicht", "tokens": ["Der", "G\u00f6tt\u00b7li\u00b7chen", "Ver\u00b7se\u00b7hung", ",", "wird", "sie", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erleuchtet, angef\u00fcllt und angestrahlet werden!", "tokens": ["Er\u00b7leuch\u00b7tet", ",", "an\u00b7ge\u00b7f\u00fcllt", "und", "an\u00b7ge\u00b7strah\u00b7let", "wer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}