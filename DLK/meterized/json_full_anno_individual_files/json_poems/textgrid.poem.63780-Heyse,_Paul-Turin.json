{"textgrid.poem.63780": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Turin", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df still und einsam, wie ein schlichter Held,", "tokens": ["Gro\u00df", "still", "und", "ein\u00b7sam", ",", "wie", "ein", "schlich\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der, wenn er k\u00fchn bestanden schwere Proben,", "tokens": ["Der", ",", "wenn", "er", "k\u00fchn", "be\u00b7stan\u00b7den", "schwe\u00b7re", "Pro\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit k\u00fcmmerlichem Dank beiseit geschoben,", "tokens": ["Mit", "k\u00fcm\u00b7mer\u00b7li\u00b7chem", "Dank", "bei\u00b7seit", "ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich stolz zur\u00fcckzieht vom Ger\u00e4usch der Welt,", "tokens": ["Sich", "stolz", "zu\u00b7r\u00fcck\u00b7zieht", "vom", "Ge\u00b7r\u00e4usch", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So ruhst du. Deine Gassenadern schwellt", "tokens": ["So", "ruhst", "du", ".", "Dei\u00b7ne", "Gas\u00b7se\u00b7na\u00b7dern", "schwellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein frisches Lebensblut mit muntrem Toben.", "tokens": ["Kein", "fri\u00b7sches", "Le\u00b7bens\u00b7blut", "mit", "mun\u00b7trem", "To\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ernst blickt hernieder die Superga droben,", "tokens": ["Ernst", "blickt", "her\u00b7nie\u00b7der", "die", "Su\u00b7per\u00b7ga", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo deine F\u00fcrsten sich die Gruft bestellt.", "tokens": ["Wo", "dei\u00b7ne", "F\u00fcrs\u00b7ten", "sich", "die", "Gruft", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stumm und ver\u00f6det ragt dein K\u00f6nigsschlo\u00df,", "tokens": ["Stumm", "und", "ver\u00b7\u00f6\u00b7det", "ragt", "dein", "K\u00f6\u00b7nigs\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Adlernest, aus dem zu Kampf und Siege", "tokens": ["Das", "Ad\u00b7ler\u00b7nest", ",", "aus", "dem", "zu", "Kampf", "und", "Sie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "APPR", "NN", "KON", "NN"], "me