{"textgrid.poem.55975": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die n\u00e4chste Flut verwischt den Weg im Watt,", "tokens": ["Die", "n\u00e4chs\u00b7te", "Flut", "ver\u00b7wischt", "den", "Weg", "im", "Watt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und alles wird auf allen Seiten gleich;", "tokens": ["und", "al\u00b7les", "wird", "auf", "al\u00b7len", "Sei\u00b7ten", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die kleine Insel drau\u00dfen aber hat", "tokens": ["die", "klei\u00b7ne", "In\u00b7sel", "drau\u00b7\u00dfen", "a\u00b7ber", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die Augen zu; verwirrend kreist der Deich", "tokens": ["die", "Au\u00b7gen", "zu", ";", "ver\u00b7wir\u00b7rend", "kreist", "der", "Deich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "um ihre Wohner, die in einen Schlaf", "tokens": ["um", "ih\u00b7re", "Woh\u00b7ner", ",", "die", "in", "ei\u00b7nen", "Schlaf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "geboren werden, drin sie viele Welten", "tokens": ["ge\u00b7bo\u00b7ren", "wer\u00b7den", ",", "drin", "sie", "vie\u00b7le", "Wel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "$,", "ADV", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "verwechseln, schweigend; denn sie reden selten,", "tokens": ["ver\u00b7wech\u00b7seln", ",", "schwei\u00b7gend", ";", "denn", "sie", "re\u00b7den", "sel\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "ADJD", "$.", "KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und jeder Satz ist wie ein Epitaph", "tokens": ["und", "je\u00b7der", "Satz", "ist", "wie", "ein", "E\u00b7pi\u00b7taph"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "f\u00fcr etwas Angeschwemmtes, Unbekanntes,", "tokens": ["f\u00fcr", "et\u00b7was", "An\u00b7ge\u00b7schwemm\u00b7tes", ",", "Un\u00b7be\u00b7kann\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das unerkl\u00e4rt zu ihnen kommt und bleibt.", "tokens": ["das", "un\u00b7er\u00b7kl\u00e4rt", "zu", "ih\u00b7nen", "kommt", "und", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und so ist alles was ihr Blick beschreibt", "tokens": ["Und", "so", "ist", "al\u00b7les", "was", "ihr", "Blick", "be\u00b7schreibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PWS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "von Kindheit an: nicht auf sie Angewandtes,", "tokens": ["von", "Kind\u00b7heit", "an", ":", "nicht", "auf", "sie", "An\u00b7ge\u00b7wand\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$.", "PTKNEG", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu Gro\u00dfes, R\u00fccksichtsloses, Hergesandtes,", "tokens": ["zu", "Gro\u00b7\u00dfes", ",", "R\u00fcck\u00b7sichts\u00b7lo\u00b7ses", ",", "Her\u00b7ge\u00b7sand\u00b7tes", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das ihre Einsamkeit noch \u00fcbertreibt.", "tokens": ["das", "ih\u00b7re", "Ein\u00b7sam\u00b7keit", "noch", "\u00fc\u00b7bert\u00b7reibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die n\u00e4chste Flut verwischt den Weg im Watt,", "tokens": ["Die", "n\u00e4chs\u00b7te", "Flut", "ver\u00b7wischt", "den", "Weg", "im", "Watt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und alles wird auf allen Seiten gleich;", "tokens": ["und", "al\u00b7les", "wird", "auf", "al\u00b7len", "Sei\u00b7ten", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "