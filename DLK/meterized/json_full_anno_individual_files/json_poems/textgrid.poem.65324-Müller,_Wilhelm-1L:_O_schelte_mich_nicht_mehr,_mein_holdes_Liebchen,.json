{"textgrid.poem.65324": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O schelte mich nicht mehr, mein holdes Liebchen,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schelte mich nicht mehr, mein holdes Liebchen,", "tokens": ["O", "schel\u00b7te", "mich", "nicht", "mehr", ",", "mein", "hol\u00b7des", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich dir sage: Deiner Wangen Gr\u00fcbchen", "tokens": ["Wenn", "ich", "dir", "sa\u00b7ge", ":", "Dei\u00b7ner", "Wan\u00b7gen", "Gr\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind wie zwei rothe Rosen mir erschienen.", "tokens": ["Sind", "wie", "zwei", "ro\u00b7the", "Ro\u00b7sen", "mir", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "CARD", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Siehst du die Bienen nicht sie oft umflattern,", "tokens": ["Siehst", "du", "die", "Bie\u00b7nen", "nicht", "sie", "oft", "um\u00b7flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob sie Honigseim in ihnen wittern?", "tokens": ["Als", "ob", "sie"