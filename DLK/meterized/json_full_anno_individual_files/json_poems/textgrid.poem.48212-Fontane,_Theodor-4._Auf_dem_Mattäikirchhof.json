{"textgrid.poem.48212": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf dem Matt\u00e4ikirchhof", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alltags mit den Offiziellen", "tokens": ["All\u00b7tags", "mit", "den", "Of\u00b7fi\u00b7zi\u00b7el\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wei\u00df ich mich immer gut zu stellen,", "tokens": ["Wei\u00df", "ich", "mich", "im\u00b7mer", "gut", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aber feiertags was Fremdes sie haben,", "tokens": ["A\u00b7ber", "fei\u00b7er\u00b7tags", "was", "Frem\u00b7des", "sie", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Besonders, wenn sie wen begraben,", "tokens": ["Be\u00b7son\u00b7ders", ",", "wenn", "sie", "wen", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann treten sie (dr\u00fcber ist kaum zu streiten)", "tokens": ["Dann", "tre\u00b7ten", "sie", "(", "dr\u00fc\u00b7ber", "ist", "kaum", "zu", "strei\u00b7ten", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PAV", "VAFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mit einem Mal in die Feierlichkeiten.", "tokens": ["Mit", "ei\u00b7nem", "Mal", "in", "die", "Fei\u00b7er\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Man ist nicht Null, nicht geradezu Luft,", "tokens": ["Man", "ist", "nicht", "Null", ",", "nicht", "ge\u00b7ra\u00b7de\u00b7zu", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "ADV", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Aber es g\u00e4hnt doch eine Kluft,", "tokens": ["A\u00b7ber", "es", "g\u00e4hnt", "doch", "ei\u00b7ne", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und das ist die Kunst, die Meisterschaft eben,", "tokens": ["Und", "das", "ist", "die", "Kunst", ",", "die", "Meis\u00b7ter\u00b7schaft", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dieser Kluft das rechte Ma\u00df zu geben.", "tokens": ["Die\u00b7ser", "Kluft", "das", "rech\u00b7te", "Ma\u00df", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht zu breit und nicht zu schmal,", "tokens": ["Nicht", "zu", "breit", "und", "nicht", "zu", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich fl\u00fcchtig begegnen, ein-, zwei-, dreimal,", "tokens": ["Sich", "fl\u00fcch\u00b7tig", "be\u00b7geg\u00b7nen", ",", "ein", ",", "zwei", ",", "drei\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$,", "TRUNC", "$,", "TRUNC", "$,", "ADV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und verbietet sich solch Vor\u00fcberschieben,", "tokens": ["Und", "ver\u00b7bie\u00b7tet", "sich", "solch", "Vor\u00b7\u00fc\u00b7bersc\u00b7hie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Dann ist der Gespr\u00e4chsgang vorgeschrieben:", "tokens": ["Dann", "ist", "der", "Ge\u00b7spr\u00e4chs\u00b7gang", "vor\u00b7ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "\u00bbanheimelnder Kirchhof ... beinah ein Garten ...", "tokens": ["\u00bb", "an\u00b7hei\u00b7meln\u00b7der", "Kirch\u00b7hof", "...", "bei\u00b7nah", "ein", "Gar\u00b7ten", "..."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$(", "ADV", "ART", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Der Prediger l\u00e4\u00dft heute lange warten ...\u00ab", "tokens": ["Der", "Pre\u00b7di\u00b7ger", "l\u00e4\u00dft", "heu\u00b7te", "lan\u00b7ge", "war\u00b7ten", "...", "\u00ab"], "to