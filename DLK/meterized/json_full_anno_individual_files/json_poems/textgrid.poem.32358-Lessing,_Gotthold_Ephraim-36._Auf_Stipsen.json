{"textgrid.poem.32358": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "36. Auf Stipsen", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stips ist, trotz einem Edelmann,", "tokens": ["Stips", "ist", ",", "trotz", "ei\u00b7nem", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Dummkopf und ein braver Degen;", "tokens": ["Ein", "Dumm\u00b7kopf", "und", "ein", "bra\u00b7ver", "De\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Borgt, wie ein frecher Edelmann;", "tokens": ["Borgt", ",", "wie", "ein", "fre\u00b7cher", "E\u00b7del\u00b7mann", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zahlt, wie ein Edelmann, mit Schl\u00e4gen;", "tokens": ["Zahlt", ",", "wie", "ein", "E\u00b7del\u00b7mann", ",", "mit", "Schl\u00e4\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verprasset sein und anderer Verm\u00f6gen,", "tokens": ["Ver\u00b7pras\u00b7set", "sein", "und", "an\u00b7de\u00b7rer", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.