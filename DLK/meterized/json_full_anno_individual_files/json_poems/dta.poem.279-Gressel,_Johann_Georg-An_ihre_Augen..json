{"dta.poem.279": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An ihre Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Qu\u00e4hlt mich nur ihr holden Augen/", "tokens": ["Qu\u00e4hlt", "mich", "nur", "ihr", "hol\u00b7den", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verkehret euch in Blitz/", "tokens": ["Und", "ver\u00b7keh\u00b7ret", "euch", "in", "Blitz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich will gern die Flammen saugen", "tokens": ["Ich", "will", "gern", "die", "Flam\u00b7men", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sterb ich gleich vor Liebes-Hitz/", "tokens": ["Sterb", "ich", "gleich", "vor", "Lie\u00b7bes\u00b7Hitz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Strahlet/ blitzet/ und verbrennet", "tokens": ["Strah\u00b7let", "/", "blit\u00b7zet", "/", "und", "ver\u00b7bren\u00b7net"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles ist euch gern geg\u00f6nnet.", "tokens": ["Al\u00b7les", "ist", "euch", "gern", "ge\u00b7g\u00f6n\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mu\u00df ich gleich im Feur verderben/", "tokens": ["Mu\u00df", "ich", "gleich", "im", "Feur", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt mir doch der Ruhm davon/", "tokens": ["Bleibt", "mir", "doch", "der", "Ruhm", "da\u00b7von", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich G\u00f6ttlich k\u00f6nnen sterben/", "tokens": ["Da\u00df", "ich", "G\u00f6tt\u00b7lich", "k\u00f6n\u00b7nen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein ander ", "tokens": ["Wie", "ein", "an\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Welchem Blitz und Donner-Strahlen", "tokens": ["Wel\u00b7chem", "Blitz", "und", "Don\u00b7ner\u00b7Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcssen seinen Stoltz bezahlen.", "tokens": ["M\u00fcs\u00b7sen", "sei\u00b7nen", "Stoltz", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sterb\u2019 ich denn durch eure Flammen", "tokens": ["Sterb'", "ich", "denn", "durch", "eu\u00b7re", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So geschieht mir eben recht/", "tokens": ["So", "ge\u00b7schieht", "mir", "e\u00b7ben", "recht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blicke/ die vom Himmel stammen", "tokens": ["Bli\u00b7cke", "/", "die", "vom", "Him\u00b7mel", "stam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "ART", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6ren nicht vor einem Knecht/", "tokens": ["H\u00f6\u00b7ren", "nicht", "vor", "ei\u00b7nem", "Knecht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum will ich auch willig leiden/", "tokens": ["Drum", "will", "ich", "auch", "wil\u00b7lig", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sich Leib und Seele scheiden.", "tokens": ["Da\u00df", "sich", "Leib", "und", "See\u00b7le", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}