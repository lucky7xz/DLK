{"textgrid.poem.41682": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft deucht es mich, da\u00df mein Blut mir entflieht,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft deucht es mich, da\u00df mein Blut mir entflieht,", "tokens": ["Oft", "deucht", "es", "mich", ",", "da\u00df", "mein", "Blut", "mir", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ein Springbrunn mit seltsam schluchzendem Lied,", "tokens": ["Wie", "ein", "Spring\u00b7brunn", "mit", "selt\u00b7sam", "schluch\u00b7zen\u00b7dem", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wohl h\u00f6r ich, wie es str\u00f6mt, dumpf murmelnd Stund an Stunde,", "tokens": ["Wohl", "h\u00f6r", "ich", ",", "wie", "es", "str\u00f6mt", ",", "dumpf", "mur\u00b7melnd", "Stund", "an", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch taste ich umsonst und finde keine Wunde.", "tokens": ["Doch", "tas\u00b7te", "ich", "um\u00b7sonst", "und", "fin\u00b7de", "kei\u00b7ne", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch die Stadt rinnts wie durch umfriedet Gebiet,", "tokens": ["Durch", "die", "Stadt", "rinnts", "wie", "durch", "um\u00b7frie\u00b7det", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "KOKOM", "APPR", "VVFIN", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und das Pflaster gleicht Inseln, die es umzieht.", "tokens": ["Und", "das", "Pflas\u00b7ter", "gleicht", "In\u00b7seln", ",", "die", "es", "um\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Jedwede Kreatur trinkt es mit durstgem Munde,", "tokens": ["Jed\u00b7we\u00b7de", "Kre\u00b7a\u00b7tur", "trinkt", "es", "mit", "durst\u00b7gem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+---+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Es taucht in tiefes Rot die ungeheure Runde.", "tokens": ["Es", "taucht", "in", "tie\u00b7fes", "Rot", "die", "un\u00b7ge\u00b7heu\u00b7re", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verzweifelt fleht' ich an der Weine Zaubermacht,", "tokens": ["Ver\u00b7zwei\u00b7felt", "fleht'", "ich", "an", "der", "Wei\u00b7ne", "Zau\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nur ein Tag mir frei von diesem Graun erscheine.", "tokens": ["Da\u00df", "nur", "ein", "Tag", "mir", "frei", "von", "die\u00b7sem", "Graun", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch das Ohr wird fein, das Auge klar vom Weine.", "tokens": ["Je\u00b7doch", "das", "Ohr", "wird", "fein", ",", "das", "Au\u00b7ge", "klar", "vom", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Lieben suchte ich vergessensdunkle Nacht;", "tokens": ["Im", "Lie\u00b7ben", "such\u00b7te", "ich", "ver\u00b7ges\u00b7sens\u00b7dunk\u00b7le", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch scheint mir, sich im Rausch der Liebe zu versenken,", "tokens": ["Doch", "scheint", "mir", ",", "sich", "im", "Rausch", "der", "Lie\u00b7be", "zu", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRF", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3"