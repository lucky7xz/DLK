{"textgrid.poem.36028": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[soll mir Jesus liebevoll sich zeigen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll mir Jesus liebevoll sich zeigen,", "tokens": ["Soll", "mir", "Je\u00b7sus", "lie\u00b7be\u00b7voll", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mu\u00df ich \u00fcber Berg und Ufer steigen.", "tokens": ["Mu\u00df", "ich", "\u00fc\u00b7ber", "Berg", "und", "U\u00b7fer", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Blumenpfl\u00fccken mu\u00df ich mir versagen,", "tokens": ["Blu\u00b7men\u00b7pfl\u00fc\u00b7cken", "mu\u00df", "ich", "mir", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Darf auch nicht vor wilden Tieren zagen.", "tokens": ["Darf", "auch", "nicht", "vor", "wil\u00b7den", "Tie\u00b7ren", "za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Durch die Starken, welche mich bestreiten,", "tokens": ["Durch", "die", "Star\u00b7ken", ",", "wel\u00b7che", "mich", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Selbst durch meine Grenzen mu\u00df ich schreiten.", "tokens": ["Selbst", "durch", "mei\u00b7ne", "Gren\u00b7zen", "mu\u00df", "ich", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn wir handeln, dulden, beten, schweigen,", "tokens": ["Wenn", "wir", "han\u00b7deln", ",", "dul\u00b7den", ",", "be\u00b7ten", ",", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dann wird Jesu Klarheit uns sich zeigen.", "tokens": ["Dann", "wird", "Je\u00b7su", "Klar\u00b7heit", "uns", "sich", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Soll mir Jesus liebevoll sich zeigen,", "tokens": ["Soll", "mir", "Je\u00b7sus", "lie\u00b7be\u00b7voll", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mu\u00df ich \u00fcber Berg und Ufer steigen.", "tokens": ["Mu\u00df", "ich", "\u00fc\u00b7ber", "Berg", "und", "U\u00b7fer", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure"