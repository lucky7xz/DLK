{"dta.poem.13578": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wehmuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wir gr\u00fc\u00dfen Land und Sterne", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", "Land", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit wunderbarem Klang,", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rem", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer uns sp\u00fcrt von Ferne,", "tokens": ["Und", "wer", "uns", "sp\u00fcrt", "von", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem wird so wohl und bang.", "tokens": ["Dem", "wird", "so", "wohl", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}