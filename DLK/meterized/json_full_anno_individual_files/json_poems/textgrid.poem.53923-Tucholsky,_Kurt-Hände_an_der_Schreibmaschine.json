{"textgrid.poem.53923": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "H\u00e4nde an der Schreibmaschine", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Schrift kann niemand lesen,", "tokens": ["Mei\u00b7ne", "Schrift", "kann", "nie\u00b7mand", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nicht mal ich. Nur noch Chinesen", "tokens": ["nicht", "mal", "ich", ".", "Nur", "noch", "Chi\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPER", "$.", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "pinseln wichtig.", "tokens": ["pin\u00b7seln", "wich\u00b7tig", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ich will kein solch Pinseler bleiben.", "tokens": ["Ich", "will", "kein", "solch", "Pin\u00b7se\u00b7ler", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Mit acht Fingern la\u00dft mich schreiben!", "tokens": ["Mit", "acht", "Fin\u00b7gern", "la\u00dft", "mich", "schrei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber richtig!", "tokens": ["A\u00b7ber", "rich\u00b7tig", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hebel rauscht, und Gl\u00f6ckchen klingt,", "tokens": ["He\u00b7bel", "rauscht", ",", "und", "Gl\u00f6ck\u00b7chen", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Schreibmaschine singt:", "tokens": ["und", "die", "Schreib\u00b7ma\u00b7schi\u00b7ne", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "sel. Nachfolger", "tokens": ["sel", ".", "Nach\u00b7fol\u00b7ger"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$.", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "W\u00fcrzburg an der W\u00fcrze", "tokens": ["W\u00fcrz\u00b7burg", "an", "der", "W\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u00dcbung kommt so mit den Jahren.", "tokens": ["\u00dc\u00b7bung", "kommt", "so", "mit", "den", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich schalte wie beim Fahren", "tokens": ["Und", "ich", "schal\u00b7te", "wie", "beim", "Fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dritten Gang ein.", "tokens": ["drit\u00b7ten", "Gang", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hoppla, Kurve! Achtung, Liebe!", "tokens": ["Hopp\u00b7la", ",", "Kur\u00b7ve", "!", "Ach\u00b7tung", ",", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich schalte wie beim Fahren", "tokens": ["Und", "ich", "schal\u00b7te", "wie", "beim", "Fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "jeden Klang ein.", "tokens": ["je\u00b7den", "Klang", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hebel rauscht, und Gl\u00f6ckchen zirpt,", "tokens": ["He\u00b7bel", "rauscht", ",", "und", "Gl\u00f6ck\u00b7chen", "zirpt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Schreibmaschine wirbt:", "tokens": ["und", "die", "Schreib\u00b7ma\u00b7schi\u00b7ne", "wirbt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tausend Finger laufen eilig", "tokens": ["Tau\u00b7send", "Fin\u00b7ger", "lau\u00b7fen", "ei\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "amtlich, dienstlich, polizeilich", "tokens": ["amt\u00b7lich", ",", "dienst\u00b7lich", ",", "po\u00b7li\u00b7zei\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "auf den Tasten.", "tokens": ["auf", "den", "Tas\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Aufgebote f\u00fcr die Heirat,", "tokens": ["Auf\u00b7ge\u00b7bo\u00b7te", "f\u00fcr", "die", "Hei\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "das Gesuch beim Polizeirat,", "tokens": ["das", "Ge\u00b7such", "beim", "Po\u00b7li\u00b7zei\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steuerlasten.", "tokens": ["Steu\u00b7er\u00b7las\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hebel schnattern, Walze steht,", "tokens": ["He\u00b7bel", "schnat\u00b7tern", ",", "Wal\u00b7ze", "steht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Schreibmaschine fleht:", "tokens": ["und", "die", "Schreib\u00b7ma\u00b7schi\u00b7ne", "fleht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "An den Herrn", "tokens": ["An", "den", "Herrn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": " regierungspr\u00e4sidenten", "tokens": ["re\u00b7gie\u00b7rungs\u00b7pr\u00e4\u00b7si\u00b7den\u00b7ten"], "token_info": ["word"], "pos": ["ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "zu erlassen, wovon ich mir eine wesentliche", "tokens": ["zu", "er\u00b7las\u00b7sen", ",", "wo\u00b7von", "ich", "mir", "ei\u00b7ne", "we\u00b7sent\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.7": {"line.1": {"text": "Unser Leben, eingefangen,", "tokens": ["Un\u00b7ser", "Le\u00b7ben", ",", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ist durch dich hindurchgegangen,", "tokens": ["ist", "durch", "dich", "hin\u00b7durch\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Guillotine!", "tokens": ["Guil\u00b7lo\u00b7ti\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Unsere Freuden, unsere Sorgen,", "tokens": ["Un\u00b7se\u00b7re", "Freu\u00b7den", ",", "un\u00b7se\u00b7re", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "gestern, heute, \u00fcbermorgen \u2013", "tokens": ["ge\u00b7stern", ",", "heu\u00b7te", ",", "\u00fc\u00b7ber\u00b7mor\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "o Maschine!", "tokens": ["o", "Ma\u00b7schi\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hebel wirbeln, Wagen knackt,", "tokens": ["He\u00b7bel", "wir\u00b7beln", ",", "Wa\u00b7gen", "knackt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Schreibmaschine tackt:", "tokens": ["und", "die", "Schreib\u00b7ma\u00b7schi\u00b7ne", "tackt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Alles wei\u00dft du, Maschine, immer stehst du startbereit!", "tokens": ["Al\u00b7les", "wei\u00dft", "du", ",", "Ma\u00b7schi\u00b7ne", ",", "im\u00b7mer", "stehst", "du", "start\u00b7be\u00b7reit", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "---+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "In dir ist unser Beruf, unser Leben und unsre ganze Zeit.", "tokens": ["In", "dir", "ist", "un\u00b7ser", "Be\u00b7ruf", ",", "un\u00b7ser", "Le\u00b7ben", "und", "uns\u00b7re", "gan\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sogar auf Reisen kommst du mit, praktisch und gut verpackt,", "tokens": ["So\u00b7gar", "auf", "Rei\u00b7sen", "kommst", "du", "mit", ",", "prak\u00b7tisch", "und", "gut", "ver\u00b7packt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+---+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "bis eines Tages zum letzten Male dein Hebel knackt.", "tokens": ["bis", "ei\u00b7nes", "Ta\u00b7ges", "zum", "letz\u00b7ten", "Ma\u00b7le", "dein", "He\u00b7bel", "knackt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Millionen Konzerte steigen t\u00e4glich auf aus Stahl und Papier.", "tokens": ["Mil\u00b7lion\u00b7en", "Kon\u00b7zer\u00b7te", "stei\u00b7gen", "t\u00e4g\u00b7lich", "auf", "aus", "Stahl", "und", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADJD", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Was w\u00e4ren wir ohne dich, du Gesch\u00e4ftsklavier \u2013!", "tokens": ["Was", "w\u00e4\u00b7ren", "wir", "oh\u00b7ne", "dich", ",", "du", "Ge\u00b7sch\u00e4fts\u00b7kla\u00b7vier", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "$,", "PPER", "NN", "$(", "$."], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Meine Schrift kann niemand lesen,", "tokens": ["Mei\u00b7ne", "Schrift", "kann", "nie\u00b7mand", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nicht mal ich. Nur noch Chinesen", "tokens": ["nicht", "mal", "ich", ".", "Nur", "noch", "Chi\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPER", "$.", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "pinseln wichtig.", "tokens": ["pin\u00b7seln", "wich\u00b7tig", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ich will kein solch Pinseler bleiben.", "tokens": ["Ich", "will", "kein", "solch", "Pin\u00b7se\u00b7ler", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Mit acht Fingern la\u00dft mich schreiben!", "tokens": ["Mit", "acht", "Fin\u00b7gern", "la\u00dft", "mich", "schrei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber richtig!", "tokens": ["A\u00b7ber", "rich\u00b7tig", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hebel rauscht, und Gl\u00f6ckchen klingt,", "tokens": ["He\u00b7bel", "rauscht", ",", "und", "Gl\u00f6ck\u00b7chen", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Schreibmaschine singt:", "tokens": ["und", "die", "Schreib\u00b7ma\u00b7schi\u00b7ne", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "sel. Nachfolger", "tokens": ["sel", ".", "Nach\u00b7fol\u00b7ger"], "token_info": ["word", "punct", "word"], "pos": ["A