in\u00b7durch\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Guillotine!", "tokens": ["Guil\u00b7lo\u00b7ti\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Unsere Freuden, unsere Sorgen,", "tokens": ["Un\u00b7se\u00b7re", "Freu\u00b7den", ",", "un\u00b7se\u00b7re", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "gestern, heute, \u00fcbermorgen \u2013", "tokens": ["ge\u00b7stern", ",", "heu\u00b7te", ",", "\u00fc\u00b7ber\u00b7mor\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "o Maschine!", "tokens": ["o", "Ma\u00b7schi\u00b7ne", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hebel wirbeln, Wagen knackt,", "tokens": ["He\u00b7bel", "wir\u00b7beln", ",", "Wa\u00b7gen", "knackt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Schreibmaschine tackt:", "tokens": ["und", "die", "Schreib\u00b7ma\u00b7schi\u00b7ne", "tackt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Alles wei\u00dft du, Maschine, immer stehst du startbereit!", "tokens": ["Al\u00b7les", "wei\u00dft", "du", ",", "Ma\u00b7schi\u00b7ne", ",", "im\u0