"+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Hebel rauscht, und Gl\u00f6ckchen zirpt,", "tokens": ["He\u00b7bel", "rauscht", ",", "und", "Gl\u00f6ck\u00b7chen", "zirpt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Schreibmaschine wirbt:", "tokens": ["und", "die", "Schreib\u00b7ma\u00b7schi\u00b7ne", "wirbt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tausend Finger laufen eilig", "tokens": ["Tau\u00b7send", "Fin\u00b7ger", "lau\u00b7fen", "ei\u00b7lig"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "amtlich, dienstlich, polizeilich", "tokens": ["amt\u00b7lich", ",", "dienst\u00b7lich", ",", "po\u00b7li\u00b7zei\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "auf den Tasten.", "tokens": ["auf", "den", "Tas\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Aufgebote f\u00fcr die Heirat,", "tokens": ["Auf\u00b7ge\u00b7bo\u00b7te", "f\u00fcr", "die", "Hei\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "das Gesuch beim Polizeirat,", "tokens": ["das", "Ge\u00b7such", "beim", "Po\u00b7li\u00b7zei\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "troch