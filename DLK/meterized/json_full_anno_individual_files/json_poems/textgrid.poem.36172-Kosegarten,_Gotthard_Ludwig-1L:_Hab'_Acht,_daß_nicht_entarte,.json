{"textgrid.poem.36172": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hab' Acht, da\u00df nicht entarte,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab' Acht, da\u00df nicht entarte,", "tokens": ["Hab'", "Acht", ",", "da\u00df", "nicht", "ent\u00b7ar\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "$,", "KOUS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Anfangs \u00e4cht und fein!", "tokens": ["Was", "An\u00b7fangs", "\u00e4cht", "und", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar leicht verdirbt das Zarte,", "tokens": ["Gar", "leicht", "ver\u00b7dirbt", "das", "Zar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und unrein wird, was rein.", "tokens": ["Und", "un\u00b7rein", "wird", ",", "was", "rein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl edel ist's, zu k\u00e4mpfen", "tokens": ["Wohl", "e\u00b7del", "ist's", ",", "zu", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Freiheit, Recht und Pflicht.", "tokens": ["F\u00fcr", "Frei\u00b7heit", ",", "Recht", "und", "Pflicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl sch\u00f6n, den Feind zu d\u00e4mpfen,", "tokens": ["Wohl", "sch\u00f6n", ",", "den", "Feind", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Treu und Glauben bricht.", "tokens": ["Der", "Treu", "und", "Glau\u00b7ben", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ist heilig nun die Sache,", "tokens": ["Ist", "hei\u00b7lig", "nun", "die", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kampf gerecht und gut;", "tokens": ["Der", "Kampf", "ge\u00b7recht", "und", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So schnaube nicht nach Rache,", "tokens": ["So", "schnau\u00b7be", "nicht", "nach", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So durste nicht nach Blut!", "tokens": ["So", "durs\u00b7te", "nicht", "nach", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dem Keuler la\u00df das Schnauben;", "tokens": ["Dem", "Keu\u00b7ler", "la\u00df", "das", "Schnau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blut schl\u00fcrfe der Vampyr!", "tokens": ["Blut", "schl\u00fcr\u00b7fe", "der", "Vam\u00b7pyr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Zu lieben und zu glauben,", "tokens": ["Zu", "lie\u00b7ben", "und", "zu", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Mensch, geziemet dir.", "tokens": ["O", "Mensch", ",", "ge\u00b7zie\u00b7met", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl sch\u00f6n steht es, zu eifern", "tokens": ["Wohl", "sch\u00f6n", "steht", "es", ",", "zu", "ei\u00b7fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "F\u00fcr Bruderwohl und Weh;", "tokens": ["F\u00fcr", "Bru\u00b7der\u00b7wohl", "und", "Weh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch knirschen, sch\u00e4umen, geifern", "tokens": ["Doch", "knir\u00b7schen", ",", "sch\u00e4u\u00b7men", ",", "gei\u00b7fern"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thut nur der Rasende.", "tokens": ["Thut", "nur", "der", "Ra\u00b7sen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Willst du die Welt erretten,", "tokens": ["Willst", "du", "die", "Welt", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von schn\u00f6der Knechtschaft Fluch;", "tokens": ["Von", "schn\u00f6\u00b7der", "Knecht\u00b7schaft", "Fluch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So brich zuvor die Ketten,", "tokens": ["So", "brich", "zu\u00b7vor", "die", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "D'rinn dich die S\u00fcnde schlug!", "tokens": ["D'\u00b7rinn", "dich", "die", "S\u00fcn\u00b7de", "schlug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Willst du das Volk erl\u00f6sen", "tokens": ["Willst", "du", "das", "Volk", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von herber Tyrannei;", "tokens": ["Von", "her\u00b7ber", "Ty\u00b7ran\u00b7nei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So steur' in dir dem B\u00f6sen,", "tokens": ["So", "steur'", "in", "dir", "dem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und werd' erst selber frei.", "tokens": ["Und", "werd'", "erst", "sel\u00b7ber", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Willst du durch Schrift und Rede", "tokens": ["Willst", "du", "durch", "Schrift", "und", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Vaterland erneu'n;", "tokens": ["Das", "Va\u00b7ter\u00b7land", "er\u00b7neu'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ziemt es nicht, so schn\u00f6de", "tokens": ["So", "ziemt", "es", "nicht", ",", "so", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Ausland Weihrauch streu'n!", "tokens": ["Dem", "Aus\u00b7land", "Weih\u00b7rauch", "streu'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zumeist meint sich der Fremde,", "tokens": ["Zu\u00b7meist", "meint", "sich", "der", "Frem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zun\u00e4chst Den, der ihm traut;", "tokens": ["Zu\u00b7n\u00e4chst", "Den", ",", "der", "ihm", "traut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wohl n\u00e4her als das Hemde", "tokens": ["Wohl", "n\u00e4\u00b7her", "als", "das", "Hem\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist ihm die eig'ne Haut.", "tokens": ["Ist", "ihm", "die", "eig'\u00b7ne", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vertrau' der eig'nen Rechte!", "tokens": ["Ver\u00b7trau'", "der", "eig'\u00b7nen", "Rech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schau auf den eig'nen Stern!", "tokens": ["Schau", "auf", "den", "eig'\u00b7nen", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was frommt der Tausch dem Knechte,", "tokens": ["Was", "frommt", "der", "Tausch", "dem", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tauscht er nur blo\u00df den Herrn?", "tokens": ["Tauscht", "er", "nur", "blo\u00df", "den", "Herrn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Sey klug, gerecht und g\u00fctig,", "tokens": ["Sey", "klug", ",", "ge\u00b7recht", "und", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Besonnen, kalt und klar;", "tokens": ["Be\u00b7son\u00b7nen", ",", "kalt", "und", "klar", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht frech noch \u00fcberm\u00fcthig,", "tokens": ["Nicht", "frech", "noch", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aufrichtig, offen, wahr!", "tokens": ["Auf\u00b7rich\u00b7tig", ",", "of\u00b7fen", ",", "wahr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dem B\u00f6sen steur' und wehre;", "tokens": ["Dem", "B\u00f6\u00b7sen", "steur'", "und", "weh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Frevel bleibe fern!", "tokens": ["Vom", "Fre\u00b7vel", "blei\u00b7be", "fern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gieb Ehre, dem die Ehre", "tokens": ["Gieb", "Eh\u00b7re", ",", "dem", "die", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geb\u00fchret, Gott dem Herrn!", "tokens": ["Ge\u00b7b\u00fch\u00b7ret", ",", "Gott", "dem", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Bist du also gesonnen,", "tokens": ["Bist", "du", "al\u00b7so", "ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "So zeuch, zeuch hin mit Gott!", "tokens": ["So", "zeuch", ",", "zeuch", "hin", "mit", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "VVIMP", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kampf ist schon entbronnen;", "tokens": ["Der", "Kampf", "ist", "schon", "ent\u00b7bron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieg gilt es oder Tod!", "tokens": ["Sieg", "gilt", "es", "o\u00b7der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Hab' Acht, da\u00df nicht entarte,", "tokens": ["Hab'", "Acht", ",", "da\u00df", "nicht", "ent\u00b7ar\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "$,", "KOUS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Anfangs \u00e4cht und fein!", "tokens": ["Was", "An\u00b7fangs", "\u00e4cht", "und", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar leicht verdirbt das Zarte,", "tokens": ["Gar", "leicht", "ver\u00b7dirbt", "das", "Zar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und unrein wird, was rein.", "tokens": ["Und", "un\u00b7rein", "wird", ",", "was", "rein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wohl edel ist's, zu k\u00e4mpfen", "tokens": ["Wohl", "e\u00b7del", "ist's", ",", "zu", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr Freiheit, Recht und Pflicht.", "tokens": ["F\u00fcr", "Frei\u00b7heit", ",", "Recht", "und", "Pflicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl sch\u00f6n, den Feind zu d\u00e4mpfen,", "tokens": ["Wohl", "sch\u00f6n", ",", "den", "Feind", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Treu und Glauben bricht.", "tokens": ["Der", "Treu", "und", "Glau\u00b7ben", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ist heilig nun die Sache,", "tokens": ["Ist", 