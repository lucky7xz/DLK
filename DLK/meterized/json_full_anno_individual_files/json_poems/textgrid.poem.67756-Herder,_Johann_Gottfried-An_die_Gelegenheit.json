{"textgrid.poem.67756": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An die Gelegenheit", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singt die G\u00f6ttin des Gl\u00fccks, singet die K\u00f6nigin", "tokens": ["Singt", "die", "G\u00f6t\u00b7tin", "des", "Gl\u00fccks", ",", "sin\u00b7get", "die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Selbst der Schicksale! singt sie, die dem D\u00fcrftigen", "tokens": ["Selbst", "der", "Schick\u00b7sa\u00b7le", "!", "singt", "sie", ",", "die", "dem", "D\u00fcrf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Manna, goldene Kronen", "tokens": ["Man\u00b7na", ",", "gol\u00b7de\u00b7ne", "Kro\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wirft als Bote der G\u00f6tter herab,", "tokens": ["Wirft", "als", "Bo\u00b7te", "der", "G\u00f6t\u00b7ter", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wirft der Sterblichen Loos, sendet der Freuden Heer,", "tokens": ["Wirft", "der", "Sterb\u00b7li\u00b7chen", "Loos", ",", "sen\u00b7det", "der", "Freu\u00b7den", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Leichter Schatten des Gl\u00fccks und des noch schnelleren", "tokens": ["Leich\u00b7ter", "Schat\u00b7ten", "des", "Gl\u00fccks", "und", "des", "noch", "schnel\u00b7le\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "KON", "ART", "ADV", "VVINF"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Ungl\u00fccks, und der entschlossne", "tokens": ["Un\u00b7gl\u00fccks", ",", "und", "der", "ent\u00b7schloss\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Weise haschet und greifet sein Loos!", "tokens": ["Wei\u00b7se", "ha\u00b7schet", "und", "grei\u00b7fet", "sein", "Loos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Greift nach Freuden und Ruhm; haschet er aber blind", "tokens": ["Greift", "nach", "Freu\u00b7den", "und", "Ruhm", ";", "ha\u00b7schet", "er", "a\u00b7ber", "blind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$.", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Schwarzumw\u00f6lkte Gefahr oder ein schimmernd Nichts,", "tokens": ["Schwar\u00b7zum\u00b7w\u00f6lk\u00b7te", "Ge\u00b7fahr", "o\u00b7der", "ein", "schim\u00b7mernd", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADJD", "PIS", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "L\u00e4chelnd giebt's er den Winden", "tokens": ["L\u00e4\u00b7chelnd", "giebt's", "er", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Und greifet kl\u00fcger und fasset es fest,", "tokens": ["Und", "grei\u00b7fet", "kl\u00fc\u00b7ger", "und", "fas\u00b7set", "es", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Was er haschte. Und sie schwindet im Flug so schnell,", "tokens": ["Was", "er", "haschte", ".", "Und", "sie", "schwin\u00b7det", "im", "Flug", "so", "schnell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Pl\u00f6tzlich, wie der Gedank, edel zu sein, erstirbt,", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "wie", "der", "Ge\u00b7dank", ",", "e\u00b7del", "zu", "sein", ",", "er\u00b7stirbt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Und drauf keimende Reue", "tokens": ["Und", "drauf", "kei\u00b7men\u00b7de", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PAV", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Schnell zum innersten Herzen gr\u00e4bt.", "tokens": ["Schnell", "zum", "in\u00b7ners\u00b7ten", "Her\u00b7zen", "gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und drauf schwarzer Verdacht Flammen der Rache speit,", "tokens": ["Und", "drauf", "schwar\u00b7zer", "Ver\u00b7dacht", "Flam\u00b7men", "der", "Ra\u00b7che", "speit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJA", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und doch leider zu sp\u00e4t seufzet die Seele nach,", "tokens": ["Und", "doch", "lei\u00b7der", "zu", "sp\u00e4t", "seuf\u00b7zet", "die", "See\u00b7le", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKA", "ADJD", "VVFIN", "ART", "NN", "APPR", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Nach der fl\u00fcchtigen G\u00f6ttin,", "tokens": ["Nach", "der", "fl\u00fcch\u00b7ti\u00b7gen", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.20": {"text": "Die die Reue der Thoren verlacht.", "tokens": ["Die", "die", "Reu\u00b7e", "der", "Tho\u00b7ren", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Ein zweischneidiges Schwert w\u00fchlet sich in die Brust,", "tokens": ["Ein", "zwei\u00b7schnei\u00b7di\u00b7ges", "Schwert", "w\u00fch\u00b7let", "sich", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Wenn arbeitend der Geist unter der Last erliegt;", "tokens": ["Wenn", "ar\u00b7bei\u00b7tend", "der", "Geist", "un\u00b7ter", "der", "Last", "er\u00b7liegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+---+---+-+", "measure": "dactylic.init"}, "line.23": {"text": "Vor Dir hingen des Schicksals", "tokens": ["Vor", "Dir", "hin\u00b7gen", "des", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.24": {"text": "Ehrne Schalen, das Leben und Tod!", "tokens": ["Ehr\u00b7ne", "Scha\u00b7len", ",", "das", "Le\u00b7ben", "und", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Tod ist Dein! Du ergriffst nicht der Gelegenheit", "tokens": ["Tod", "ist", "Dein", "!", "Du", "er\u00b7griffst", "nicht", "der", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "$.", "PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.26": {"text": "Schnell zu fassende Stirn; ewig, auf ewig floh,", "tokens": ["Schnell", "zu", "fas\u00b7sen\u00b7de", "Stirn", ";", "e\u00b7wig", ",", "auf", "e\u00b7wig", "floh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$.", "ADJD", "$,", "APPR", "ADJD", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Die auf Seufzern der Reue,", "tokens": ["Die", "auf", "Seuf\u00b7zern", "der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Als auf schnelleren Fl\u00fcgeln, entweicht.", "tokens": ["Als", "auf", "schnel\u00b7le\u00b7ren", "Fl\u00fc\u00b7geln", ",", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.29": {"text": "Wer sie suchet, umsonst! denn sie verbirgt sich Dir;", "tokens": ["Wer", "sie", "su\u00b7chet", ",", "um\u00b7sonst", "!", "denn", "sie", "ver\u00b7birgt", "sich", "Dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "$.", "KON", "PPER", "VVFIN", "PRF", "PPER", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Wer sie fliehet, umsonst! denn sie verfolgt Dich nie;", "tokens": ["Wer", "sie", "flie\u00b7het", ",", "um\u00b7sonst", "!", "denn", "sie", "ver\u00b7folgt", "Dich", "nie", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Aber wer sie belauschet,", "tokens": ["A\u00b7ber", "wer", "sie", "be\u00b7lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.32": {"text": "Tr\u00e4gt olympische Kr\u00e4nze davon.", "tokens": ["Tr\u00e4gt", "o\u00b7lym\u00b7pi\u00b7sche", "Kr\u00e4n\u00b7ze", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Und sie gie\u00dfet das Horn gr\u00fcnender F\u00fcll' auf ihn,", "tokens": ["Und", "sie", "gie\u00b7\u00dfet", "das", "Horn", "gr\u00fc\u00b7nen\u00b7der", "F\u00fcll'", "auf", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.34": {"text": "Und sein duftendes Haupt pranget Unsterblichkeit;", "tokens": ["Und", "sein", "duf\u00b7ten\u00b7des", "Haupt", "pran\u00b7get", "U\u00b7nsterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.35": {"text": "Rosen sprosset ihr Tritt auf,", "tokens": ["Ro\u00b7sen", "spros\u00b7set", "ihr", "Tritt", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wo der Fliehnde die Erde ber\u00fchrt;", "tokens": ["Wo", "der", "Fliehn\u00b7de", "die", "Er\u00b7de", "be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.37": {"text": "Die im Felde des Ruhms schwer zu erreichenden", "tokens": ["Die", "im", "Fel\u00b7de", "des", "Ruhms", "schwer", "zu", "er\u00b7rei\u00b7chen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.38": {"text": "Ew'gen Lorbeer zum Ziel gl\u00e4nzender Thaten hebt", "tokens": ["Ew'\u00b7gen", "Lor\u00b7beer", "zum", "Ziel", "gl\u00e4n\u00b7zen\u00b7der", "Tha\u00b7ten", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.39": {"text": "Und auf trennenden Wegen", "tokens": ["Und", "auf", "tren\u00b7nen\u00b7den", "We\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.40": {"text": "Sich dem w\u00e4hlenden J\u00fcngling zeigt.", "tokens": ["Sich", "dem", "w\u00e4h\u00b7len\u00b7den", "J\u00fcng\u00b7ling", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Singt die G\u00f6ttin des Gl\u00fccks, singet die K\u00f6nigin", "tokens": ["Singt", "die", "G\u00f6t\u00b7tin", "des", "Gl\u00fccks", ",", "sin\u00b7get", "die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Selbst der Schicksale! singt sie, die dem D\u00fcrftigen", "tokens": ["Selbst", "der", "Schick\u00b7sa\u00b7le", "!", "singt", "sie", ",", "die", "dem", "D\u00fcrf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Manna, goldene Kronen", "tokens": ["Man\u00b7na", ",", "gol\u00b7de\u00b7ne", "Kro\u00b7nen"],