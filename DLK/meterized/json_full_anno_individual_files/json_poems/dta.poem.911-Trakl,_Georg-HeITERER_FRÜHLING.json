{"dta.poem.911": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "HeITERER FR\u00dcHLING", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Am Bach, der durch das gelbe Brachfeld flie\u00dft,", "tokens": ["Am", "Bach", ",", "der", "durch", "das", "gel\u00b7be", "Brach\u00b7feld", "flie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zieht noch das d\u00fcrre Rohr vom vorigen Jahr.", "tokens": ["Zieht", "noch", "das", "d\u00fcr\u00b7re", "Rohr", "vom", "vo\u00b7ri\u00b7gen", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Durchs Graue gleiten Kl\u00e4nge wunderbar,", "tokens": ["Durchs", "Grau\u00b7e", "glei\u00b7ten", "Kl\u00e4n\u00b7ge", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor\u00fcberweht ein Hauch von warmem Mist.", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7weht", "ein", "Hauch", "von", "war\u00b7mem", "Mist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An Weiden baumeln K\u00e4tzchen sacht im Wind,", "tokens": ["An", "Wei\u00b7den", "bau\u00b7meln", "K\u00e4tz\u00b7chen", "sacht", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein traurig Lied singt tr\u00e4umend ein Soldat.", "tokens": ["Sein", "trau\u00b7rig", "Lied", "singt", "tr\u00e4u\u00b7mend", "ein", "Sol\u00b7dat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wiesenstreifen saust verweht und matt,", "tokens": ["Ein", "Wie\u00b7sen\u00b7strei\u00b7fen", "saust", "ver\u00b7weht", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Kind steht in Konturen weich und lind.", "tokens": ["Ein", "Kind", "steht", "in", "Kon\u00b7tu\u00b7ren", "weich", "und", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Birken dort, der schwarze Dornenstrauch,", "tokens": ["Die", "Bir\u00b7ken", "dort", ",", "der", "schwar\u00b7ze", "Dor\u00b7nen\u00b7strauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch fliehn im Rauch Gestalten aufgel\u00f6st.", "tokens": ["Auch", "fliehn", "im", "Rauch", "Ge\u00b7stal\u00b7ten", "auf\u00b7ge\u00b7l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hell Gr\u00fcnes bl\u00fcht und anderes verwest", "tokens": ["Hell", "Gr\u00fc\u00b7nes", "bl\u00fcht", "und", "an\u00b7de\u00b7res", "ver\u00b7west"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Kr\u00f6ten schliefen durch den jungen Lauch.", "tokens": ["Und", "Kr\u00f6\u00b7ten", "schlie\u00b7fen", "durch", "den", "jun\u00b7gen", "Lauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dich lieb ich treu du derbe W\u00e4scherin.", "tokens": ["Dich", "lieb", "ich", "treu", "du", "der\u00b7be", "W\u00e4\u00b7sche\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch tr\u00e4gt die Flut des Himmels goldene Last.", "tokens": ["Noch", "tr\u00e4gt", "die", "Flut", "des", "Him\u00b7mels", "gol\u00b7de\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ein Fischlein blitzt vor\u00fcber und verbla\u00dft;", "tokens": ["Ein", "Fisc\u00b7hlein", "blitzt", "vor\u00b7\u00fc\u00b7ber", "und", "ver\u00b7bla\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein w\u00e4chsern Antlitz flie\u00dft durch Erlen hin.", "tokens": ["Ein", "w\u00e4ch\u00b7sern", "Ant\u00b7litz", "flie\u00dft", "durch", "Er\u00b7len", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In G\u00e4rten sinken Glocken lang und leis", "tokens": ["In", "G\u00e4r\u00b7ten", "sin\u00b7ken", "Glo\u00b7cken", "lang", "und", "leis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein kleiner Vogel tr\u00e4llert wie verr\u00fcckt.", "tokens": ["Ein", "klei\u00b7ner", "Vo\u00b7gel", "tr\u00e4l\u00b7lert", "wie", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das sanfte Korn schwillt leise und verz\u00fcckt", "tokens": ["Das", "sanf\u00b7te", "Korn", "schwillt", "lei\u00b7se", "und", "ver\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Bienen sammeln noch mit ernstem Flei\u00df.", "tokens": ["Und", "Bie\u00b7nen", "sam\u00b7meln", "noch", "mit", "erns\u00b7tem", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Komm Liebe nun zum m\u00fcden Arbeitsmann!", "tokens": ["Komm", "Lie\u00b7be", "nun", "zum", "m\u00fc\u00b7den", "Ar\u00b7beits\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In seine H\u00fctte f\u00e4llt ein lauer Strahl.", "tokens": ["In", "sei\u00b7ne", "H\u00fct\u00b7te", "f\u00e4llt", "ein", "lau\u00b7er", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wald str\u00f6mt durch den Abend herb und fahl", "tokens": ["Der", "Wald", "str\u00f6mt", "durch", "den", "A\u00b7bend", "herb", "und", "fahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Knospen knistern heiter dann und wann.", "tokens": ["Und", "Knos\u00b7pen", "knis\u00b7tern", "hei\u00b7ter", "dann", "und", "wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "ADV", "KON", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie scheint doch alles Werdende so krank!", "tokens": ["Wie", "scheint", "doch", "al\u00b7les", "Wer\u00b7den\u00b7de", "so", "krank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.2": {"text": "Ein Fieberhauch um einen Weiler kreist;", "tokens": ["Ein", "Fie\u00b7ber\u00b7hauch", "um", "ei\u00b7nen", "Wei\u00b7ler", "kreist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch aus Gezweigen winkt ein sanfter Geist", "tokens": ["Doch", "aus", "Ge\u00b7zwei\u00b7gen", "winkt", "ein", "sanf\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00f6ffnet das Gem\u00fcte weit und bang.", "tokens": ["Und", "\u00f6ff\u00b7net", "das", "Ge\u00b7m\u00fc\u00b7te", "weit", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein bl\u00fchender Ergu\u00df verrinnt sehr sacht", "tokens": ["Ein", "bl\u00fc\u00b7hen\u00b7der", "Er\u00b7gu\u00df", "ver\u00b7rinnt", "sehr", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Ungebornes pflegt der eignen Ruh.", "tokens": ["Und", "Un\u00b7ge\u00b7bor\u00b7nes", "pflegt", "der", "eig\u00b7nen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebenden bl\u00fchn ihren Sternen zu", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "bl\u00fchn", "ih\u00b7ren", "Ster\u00b7nen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und s\u00fc\u00dfer flie\u00dft ihr Odem durch die Nacht.", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "flie\u00dft", "ihr", "O\u00b7dem", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "So schmerzlich gut und wahrhaft ist, was lebt;", "tokens": ["So", "schmerz\u00b7lich", "gut", "und", "wahr\u00b7haft", "ist", ",", "was", "lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "ADV", "VAFIN", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leise r\u00fchrt dich an ein alter Stein:", "tokens": ["Und", "lei\u00b7se", "r\u00fchrt", "dich", "an", "ein", "al\u00b7ter", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wahrlich! Ich werde immer bei euch sein.", "tokens": ["Wahr\u00b7lich", "!", "Ich", "wer\u00b7de", "im\u00b7mer", "bei", "euch", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "O Mund! der durch die Silberweide bebt.", "tokens": ["O", "Mund", "!", "der", "durch", "die", "Sil\u00b7ber\u00b7wei\u00b7de", "bebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}