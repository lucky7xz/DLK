{"textgrid.poem.49486": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An seinen Freund Damon", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebter, gl\u00e4ube mir, ein Mensch ist gl\u00fccklich dran,", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "gl\u00e4u\u00b7be", "mir", ",", "ein", "Mensch", "ist", "gl\u00fcck\u00b7lich", "dran", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in dem treuen Schoos von Freunden ruhen kann,", "tokens": ["Der", "in", "dem", "treu\u00b7en", "Schoos", "von", "Freun\u00b7den", "ru\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mit vereintem Fleis nach Kunst und Weisheit streben,", "tokens": ["Die", "mit", "ver\u00b7ein\u00b7tem", "Fleis", "nach", "Kunst", "und", "Weis\u00b7heit", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auch der Menschlichkeit, was ihr geb\u00fchret, geben.", "tokens": ["Und", "auch", "der", "Menschlich\u00b7keit", ",", "was", "ihr", "ge\u00b7b\u00fch\u00b7ret", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein solcher ist gewis der weisen Henne Sohn;", "tokens": ["Ein", "sol\u00b7cher", "ist", "ge\u00b7wis", "der", "wei\u00b7sen", "Hen\u00b7ne", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sonne sieht auf ihn von ihrem goldnen Thron", "tokens": ["Die", "Son\u00b7ne", "sieht", "auf", "ihn", "von", "ih\u00b7rem", "gold\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit Neid und Groll herab, und w\u00fcnscht sich solche Stunden,", "tokens": ["Mit", "Neid", "und", "Groll", "her\u00b7ab", ",", "und", "w\u00fcnscht", "sich", "sol\u00b7che", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,", "KON", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie zwischen mir und dir in reiner Lust verschwunden.", "tokens": ["Wie", "zwi\u00b7schen", "mir", "und", "dir", "in", "rei\u00b7ner", "Lust", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "KON", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem H\u00f6chsten sey gedanckt, der, wenn ichs sagen darf,", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "sey", "ge\u00b7danckt", ",", "der", ",", "wenn", "ichs", "sa\u00b7gen", "darf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PRELS", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mich aus besondrer Huld in deine Armen warf;", "tokens": ["Mich", "aus", "be\u00b7sond\u00b7rer", "Huld", "in", "dei\u00b7ne", "Ar\u00b7men", "warf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und als ich dich einmahl in seinem Tempel schaute,", "tokens": ["Und", "als", "ich", "dich", "ein\u00b7mahl", "in", "sei\u00b7nem", "Tem\u00b7pel", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mich auch so gleich mit dir vor seinem Altar traute.", "tokens": ["Mich", "auch", "so", "gleich", "mit", "dir", "vor", "sei\u00b7nem", "Al\u00b7tar", "trau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Seit diesem kam mirs vor bey aller Sklaverey,", "tokens": ["Seit", "die\u00b7sem", "kam", "mirs", "vor", "bey", "al\u00b7ler", "Skla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "NE", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ob ich sorgenlos, und ungebunden sey;", "tokens": ["Als", "ob", "ich", "sor\u00b7gen\u00b7los", ",", "und", "un\u00b7ge\u00b7bun\u00b7den", "sey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich gla\u00fcbte, wenn ich dich aus s\u00fcsser Liebe hertzte,", "tokens": ["Ich", "gla\u00fcb\u00b7te", ",", "wenn", "ich", "dich", "aus", "s\u00fcs\u00b7ser", "Lie\u00b7be", "hertz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Euryalus zu seyn, der mit dem Nisus schertzte,", "tokens": ["Eu\u00b7ry\u00b7a\u00b7lus", "zu", "seyn", ",", "der", "mit", "dem", "Ni\u00b7sus", "schertz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VAINF", "$,", "PRELS", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sah ich dich zu mir mit holden Schritten gehn,", "tokens": ["Und", "sah", "ich", "dich", "zu", "mir", "mit", "hol\u00b7den", "Schrit\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So meynt ich, wie im Traum, die Musen selbst zu sehn.", "tokens": ["So", "meynt", "ich", ",", "wie", "im", "Traum", ",", "die", "Mu\u00b7sen", "selbst", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O da\u00df der Himmel mir das hohe Gl\u00fccke g\u00f6nnte!", "tokens": ["O", "da\u00df", "der", "Him\u00b7mel", "mir", "das", "ho\u00b7he", "Gl\u00fc\u00b7cke", "g\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich zun\u00e4chst bey dir auf ewig wohnen k\u00f6nnte;", "tokens": ["Da\u00df", "ich", "zu\u00b7n\u00e4chst", "bey", "dir", "auf", "e\u00b7wig", "woh\u00b7nen", "k\u00f6nn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "APPR", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ich diese Stirn, den Thron der Redlichkeit,", "tokens": ["Und", "da\u00df", "ich", "die\u00b7se", "Stirn", ",", "den", "Thron", "der", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies holde Augenpaar, das so viel Anmuth streut,", "tokens": ["Dies", "hol\u00b7de", "Au\u00b7gen\u00b7paar", ",", "das", "so", "viel", "An\u00b7muth", "streut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und diesen s\u00fcssen Mund, der oft mein Hertz bewegte,", "tokens": ["Und", "die\u00b7sen", "s\u00fcs\u00b7sen", "Mund", ",", "der", "oft", "mein", "Hertz", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den langen Tag hindurch beschaun und h\u00f6ren m\u00f6gte!", "tokens": ["Den", "lan\u00b7gen", "Tag", "hin\u00b7durch", "be\u00b7schaun", "und", "h\u00f6\u00b7ren", "m\u00f6g\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn eine Nachtigall die nackte Brut verl\u00e4st,", "tokens": ["Wenn", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "die", "nack\u00b7te", "Brut", "ver\u00b7l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schreyt und zwitzert sie in dem einsamen Nest,", "tokens": ["So", "schreyt", "und", "zwit\u00b7zert", "sie", "in", "dem", "ein\u00b7sa\u00b7men", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht sie ringsumher auf die begr\u00fcnten Auen,", "tokens": ["Man", "sieht", "sie", "rings\u00b7um\u00b7her", "auf", "die", "be\u00b7gr\u00fcn\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die H\u00e4lsgen ausgestreckt, mit ofnen Schn\u00e4beln, schauen,", "tokens": ["Die", "H\u00e4ls\u00b7gen", "aus\u00b7ge\u00b7streckt", ",", "mit", "of\u00b7nen", "Schn\u00e4\u00b7beln", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bis aus der nahen Saat der Mutter Stimm erklingt,", "tokens": ["Bis", "aus", "der", "na\u00b7hen", "Saat", "der", "Mut\u00b7ter", "Stimm", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die schon geflogen k\u00f6mmt, und frisches Futter bringt:", "tokens": ["Die", "schon", "ge\u00b7flo\u00b7gen", "k\u00f6mmt", ",", "und", "fri\u00b7sches", "Fut\u00b7ter", "bringt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sehn ich mich nach dir. Ich mu\u00df es endlich wagen,", "tokens": ["So", "sehn", "ich", "mich", "nach", "dir", ".", "Ich", "mu\u00df", "es", "end\u00b7lich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir, was ich leiden mu\u00df, mein werther Freund, zu sagen.", "tokens": ["Dir", ",", "was", "ich", "lei\u00b7den", "mu\u00df", ",", "mein", "wert\u00b7her", "Freund", ",", "zu", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Hertze wallet schon aufs neue zu dir hin,", "tokens": ["Mein", "Hert\u00b7ze", "wal\u00b7let", "schon", "aufs", "neu\u00b7e", "zu", "dir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "ADV", "APPRART", "ADJA", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn ich nur einen Tag von dir geschieden bin;", "tokens": ["Wenn", "ich", "nur", "ei\u00b7nen", "Tag", "von", "dir", "ge\u00b7schie\u00b7den", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schau ich nicht stets dein Bild leibhaftig vor mir schweben,", "tokens": ["Schau", "ich", "nicht", "stets", "dein", "Bild", "leib\u00b7haf\u00b7tig", "vor", "mir", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So fehlt mir alle Lust in diesem Land zu leben;", "tokens": ["So", "fehlt", "mir", "al\u00b7le", "Lust", "in", "die\u00b7sem", "Land", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Tages holder Strahl ist mir alsdann verhast,", "tokens": ["Des", "Ta\u00b7ges", "hol\u00b7der", "Strahl", "ist", "mir", "als\u00b7dann", "ver\u00b7hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und jede Sommernacht die allerschwerste Last.", "tokens": ["Und", "je\u00b7de", "Som\u00b7mer\u00b7nacht", "die", "al\u00b7ler\u00b7schwers\u00b7te", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach! bist du dann mein Freund, suchst du mein Gl\u00fcck zu bauen,", "tokens": ["Ach", "!", "bist", "du", "dann", "mein", "Freund", ",", "suchst", "du", "mein", "Gl\u00fcck", "zu", "bau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So la\u00df dich jeden Tag bey deinem Freunde schauen;", "tokens": ["So", "la\u00df", "dich", "je\u00b7den", "Tag", "bey", "dei\u00b7nem", "Freun\u00b7de", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn du bey mir nicht bist, so leb ich als verbannt;", "tokens": ["Wenn", "du", "bey", "mir", "nicht", "bist", ",", "so", "leb", "ich", "als", "ver\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo du, Geliebter, bist, da ist mein Vaterland.", "tokens": ["Wo", "du", ",", "Ge\u00b7lieb\u00b7ter", ",", "bist", ",", "da", "ist", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "VAFIN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Geliebter, gl\u00e4ube mir, ein Mensch ist gl\u00fccklich dran,", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "gl\u00e4u\u00b7be", "mir", ",", "ein", "Mensch", "ist", "gl\u00fcck\u00b7lich", "dran", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in dem treuen Schoos von Freunden ruhen kann,", "tokens": ["Der", "in", "dem", "treu\u00b7en", "Schoos", "von", "Freun\u00b7den", "ru\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mit vereintem Fleis nach Kunst und Weisheit streben,", "tokens": ["Die", "mit", "ver\u00b7ein\u00b7tem", "Fleis", "nach", "Kunst", "und", "Weis\u00b7heit", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auch der Menschlichkeit, was ihr geb\u00fchret, geben.", "tokens": ["Und", "auch", "der", "Menschlich\u00b7keit", ",", "was", "ihr", "ge\u00b7b\u00fch\u00b7ret", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein solcher ist gewis der weisen Henne Sohn;", "tokens": ["Ein", "sol\u00b7cher", "ist", "ge\u00b7wis", "der", "wei\u00b7sen", "Hen\u00b7ne", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Sonne sieht auf ihn von ihrem goldnen Thron", "tokens": ["Die", "Son\u00b7ne", "sieht", "auf", "ihn", "von", "ih\u00b7rem", "gold\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit Neid und Groll herab, und w\u00fcnscht sich solche Stunden,", "tokens": ["Mit", "Neid", "und", "Groll", "her\u00b7ab", ",", "und", "w\u00fcnscht", "sich", "sol\u00b7che", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,", "KON", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie zwischen mir und dir in reiner Lust verschwunden.", "tokens": ["Wie", "zwi\u00b7schen", "mir", "und", "dir", "in", "rei\u00b7ner", "Lust", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "KON", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem H\u00f6chsten sey gedanckt, der, wenn ichs sagen darf,", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "sey", "ge\u00b7danckt", ",", "der", ",", "wenn", "ichs", "sa\u00b7gen", "darf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PRELS", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mich aus besondrer Huld in deine Armen warf;", "tokens": ["Mich", "aus", "be\u00b7sond\u00b7rer", "Huld", "in", "dei\u00b7ne", "Ar\u00b7men", "warf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und als ich dich einmahl in seinem Tempel schaute,", "tokens": ["Und", "als", "ich", "dich", "ein\u00b7mahl", "in", "sei\u00b7nem", "Tem\u00b7pel", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mich auch so gleich mit dir vor seinem Altar traute.", "tokens": ["Mich", "auch", "so", "gleich", "mit", "dir", "vor", "sei\u00b7nem", "Al\u00b7tar", "trau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Seit diesem kam mirs vor bey aller Sklaverey,", "tokens": ["Seit", "die\u00b7sem", "kam", "mirs", "vor", "bey", "al\u00b7ler", "Skla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "NE", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ob ich sorgenlos, und ungebunden sey;", "tokens": ["Als", "ob", "ich", "sor\u00b7gen\u00b7los", ",", "und", "un\u00b7ge\u00b7bun\u00b7den", "sey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich gla\u00fcbte, wenn ich dich aus s\u00fcsser Liebe hertzte,", "tokens": ["Ich", "gla\u00fcb\u00b7te", ",", "wenn", "ich", "dich", "aus", "s\u00fcs\u00b7ser", "Lie\u00b7be", "hertz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Euryalus zu seyn, der mit dem Nisus schertzte,", "tokens": ["Eu\u00b7ry\u00b7a\u00b7lus", "zu", "seyn", ",", "der", "mit", "dem", "Ni\u00b7sus", "schertz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VAINF", "$,", "PRELS", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sah ich dich zu mir mit holden Schritten gehn,", "tokens": ["Und", "sah", "ich", "dich", "zu", "mir", "mit", "hol\u00b7den", "Schrit\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So meynt ich, wie im Traum, die Musen selbst zu sehn.", "tokens": ["So", "meynt", "ich", ",", "wie", "im", "Traum", ",", "die", "Mu\u00b7sen", "selbst", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "O da\u00df der Himmel mir das hohe Gl\u00fccke g\u00f6nnte!", "tokens": ["O", "da\u00df", "der", "Him\u00b7mel", "mir", "das", "ho\u00b7he", "Gl\u00fc\u00b7cke", "g\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich zun\u00e4chst bey dir auf ewig wohnen k\u00f6nnte;", "tokens": ["Da\u00df", "ich", "zu\u00b7n\u00e4chst", "bey", "dir", "auf", "e\u00b7wig", "woh\u00b7nen", "k\u00f6nn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "APPR", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ich diese Stirn, den Thron der Redlichkeit,", "tokens": ["Und", "da\u00df", "ich", "die\u00b7se", "Stirn", ",", "den", "Thron", "der", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies holde Augenpaar, das so viel Anmuth streut,", "tokens": ["Dies", "hol\u00b7de", "Au\u00b7gen\u00b7paar", ",", "das", "so", "viel", "An\u00b7muth", "streut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und diesen s\u00fcssen Mund, der oft mein Hertz bewegte,", "tokens": ["Und", "die\u00b7sen", "s\u00fcs\u00b7sen", "Mund", ",", "der", "oft", "mein", "Hertz", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den langen Tag hindurch beschaun und h\u00f6ren m\u00f6gte!", "tokens": ["Den", "lan\u00b7gen", "Tag", "hin\u00b7durch", "be\u00b7schaun", "und", "h\u00f6\u00b7ren", "m\u00f6g\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wenn eine Nachtigall die nackte Brut verl\u00e4st,", "tokens": ["Wenn", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "die", "nack\u00b7te", "Brut", "ver\u00b7l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schreyt und zwitzert sie in dem einsamen Nest,", "tokens": ["So", "schreyt", "und", "zwit\u00b7zert", "sie", "in", "dem", "ein\u00b7sa\u00b7men", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sieht sie ringsumher auf die begr\u00fcnten Auen,", "tokens": ["Man", "sieht", "sie", "rings\u00b7um\u00b7her", "auf", "die", "be\u00b7gr\u00fcn\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die H\u00e4lsgen ausgestreckt, mit ofnen Schn\u00e4beln, schauen,", "tokens": ["Die", "H\u00e4ls\u00b7gen", "aus\u00b7ge\u00b7streckt", ",", "mit", "of\u00b7nen", "Schn\u00e4\u00b7beln", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bis aus der nahen Saat der Mutter Stimm erklingt,", "tokens": ["Bis", "aus", "der", "na\u00b7hen", "Saat", "der", "Mut\u00b7ter", "Stimm", "er\u00b7klingt", ","], "token_info": ["word", 