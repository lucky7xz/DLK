{"dta.poem.4078": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hyperions Schicksalslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr wandelt droben im Licht", "tokens": ["Ihr", "wan\u00b7delt", "dro\u00b7ben", "im", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auf weichem Boden, selige Genien!", "tokens": ["Auf", "wei\u00b7chem", "Bo\u00b7den", ",", "se\u00b7li\u00b7ge", "Ge\u00b7ni\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gl\u00e4nzende G\u00f6tterl\u00fcfte", "tokens": ["Gl\u00e4n\u00b7zen\u00b7de", "G\u00f6t\u00b7ter\u00b7l\u00fcf\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "R\u00fchren euch leicht,", "tokens": ["R\u00fch\u00b7ren", "euch", "leicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie die Finger der K\u00fcnstlerin", "tokens": ["Wie", "die", "Fin\u00b7ger", "der", "K\u00fcnst\u00b7le\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Heilige Saiten.", "tokens": ["Hei\u00b7li\u00b7ge", "Sai\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schicksallos, wie der schlafende", "tokens": ["Schick\u00b7sal\u00b7los", ",", "wie", "der", "schla\u00b7fen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "S\u00e4ugling, athmen die Himmlischen;", "tokens": ["S\u00e4ug\u00b7ling", ",", "ath\u00b7men", "die", "Himm\u00b7li\u00b7schen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Keusch bewahrt", "tokens": ["Keusch", "be\u00b7wahrt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "In bescheidener Knospe,", "tokens": ["In", "be\u00b7schei\u00b7de\u00b7ner", "Knos\u00b7pe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Bl\u00fchet ewig", "tokens": ["Bl\u00fc\u00b7het", "e\u00b7wig"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ihnen der Geist,", "tokens": ["Ih\u00b7nen", "der", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und die seligen Augen", "tokens": ["Und", "die", "se\u00b7li\u00b7gen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Blicken in stiller", "tokens": ["Bli\u00b7cken", "in", "stil\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ewiger Klarheit.", "tokens": ["E\u00b7wi\u00b7ger", "Klar\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch uns ist gegeben,", "tokens": ["Doch", "uns", "ist", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf keiner St\u00e4tte zu ruh'n,", "tokens": ["Auf", "kei\u00b7ner", "St\u00e4t\u00b7te", "zu", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es schwinden, es fallen", "tokens": ["Es", "schwin\u00b7den", ",", "es", "fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die leidenden Menschen", "tokens": ["Die", "lei\u00b7den\u00b7den", "Men\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Blindlings von einer", "tokens": ["Blind\u00b7lings", "von", "ei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Stunde zur andern,", "tokens": ["Stun\u00b7de", "zur", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wie Wasser von Klippe", "tokens": ["Wie", "Was\u00b7ser", "von", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zu Klippe geworfen,", "tokens": ["Zu", "Klip\u00b7pe", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Jahrlang in's Ungewisse hinab.", "tokens": ["Jahr\u00b7lang", "in's", "Un\u00b7ge\u00b7wis\u00b7se", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}