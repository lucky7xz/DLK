{"textgrid.poem.65816": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "90", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hasse mich denn, wann du willst; gleich nun,", "tokens": ["So", "has\u00b7se", "mich", "denn", ",", "wann", "du", "willst", ";", "gleich", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt, da die Welt mich kreuzt in meinen Taten:", "tokens": ["Jetzt", ",", "da", "die", "Welt", "mich", "kreuzt", "in", "mei\u00b7nen", "Ta\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hilf dem Verdru\u00df des Gl\u00fccks mir weh zu tun,", "tokens": ["Hilf", "dem", "Ver\u00b7dru\u00df", "des", "Gl\u00fccks", "mir", "weh", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und triff nicht sp\u00e4t mich mit verspartem Schaden.", "tokens": ["Und", "triff", "nicht", "sp\u00e4t", "mich", "mit", "ver\u00b7spar\u00b7tem", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach komm, wenn ich