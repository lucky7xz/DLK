 ["Hat", "un\u00b7ser", "Herz", "durch\u00b7gl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und innig uns durchdrungen.", "tokens": ["Und", "in\u00b7nig", "uns", "durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da trieb es uns hinaus,", "tokens": ["Da", "trieb", "es", "uns", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu wandern durch das Leben,", "tokens": ["Zu", "wan\u00b7dern", "durch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Ruh, den Hof, das Haus", "tokens": ["Die", "Ruh", ",", "den", "Hof", ",", "das", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Alles dran zu geben.", "tokens": ["Und", "Al\u00b7les", "dran", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Uns riefen von dem Herrn", "tokens": ["Uns", "rie\u00b7fen", "von", "dem", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Sagen und die Kunden,", "tokens": ["Die", "Sa\u00b7gen", "und", "die", "Kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir folgten seinem Stern,", "tokens": ["Wir", "folg\u00b7ten", "sei\u00b7nem", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bis wir ihn selbst gefunden.", "tokens": ["Bis", "wir", "ihn", "selbst", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Maria, s\u00fc\u00dfes Bild,", "tokens": ["Ma\u00b7ria", ",", "s\u00fc\u00b7\u00dfes", "Bild", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wir k\u00f6nnen's nie vergessen,", "tokens": ["Wir", "k\u00f6n\u00b7nen's", "nie", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie du, so fromm und mild,", "tokens": ["Wie", "du", ",", "so", "fromm", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Kripplein bist gesessen.", "tokens": ["Am", "Krip\u00b7plein", "bist", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das folgt uns wie ein Traum", "tokens": ["Das", "folgt", "uns", "wie", "ein", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach K\u00f6llen an dem Rheine,", "tokens": ["Nach", "K\u00f6l\u00b7len", "an", "dem", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcllt unsern Grabesraum", "tokens": ["F\u00fcllt", "un\u00b7sern", "Gra\u00b7bes\u00b7raum"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem hellen Scheine.", "tokens": ["Mit", "sei\u00b7nem", "hel\u00b7len", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wenn ein holdes Kind", "tokens": ["Und", "wenn", "ein", "hol\u00b7des", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach unserm Grabe ziehet,", "tokens": ["Nach", "un\u00b7serm", "Gra\u00b7be", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn treu und stillgesinnt", "tokens": ["Wenn", "treu", "und", "still\u00b7ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich Muttersorge m\u00fchet,", "tokens": ["Sich", "Mut\u00b7ter\u00b7sor\u00b7ge", "m\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dann f\u00fchlen wir die Lust", "tokens": ["Dann", "f\u00fch\u00b7len", "wir", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus alter Zeit sich regen,", "tokens": ["Aus", "al\u00b7ter", "Zeit", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es zieht in manche Brust", "tokens": ["Es", "zieht", "in", "man\u00b7che", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus unserm Grab der Segen.", "tokens": ["Aus", "un\u00b7serm", "Grab", "der", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Myrrhen Bitterkeit \u2013", "tokens": ["Der", "Myr\u00b7rhen", "Bit\u00b7ter\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Man kennt sie wol im Leben,", "tokens": ["Man", "kennt", "sie", "wol", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sollen dr\u00fcber weit", "tokens": ["Doch", "sol\u00b7len", "dr\u00fc\u00b7ber", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PAV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Weihrauchswolken schweben.", "tokens": ["Die", "Weih\u00b7rauchs\u00b7wol\u00b7ken", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das Gold, es ist die Treu", "tokens": ["Das", "Gold", ",", "es", "ist", "die", "Treu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Leben, wie im Sterben:", "tokens": ["Im", "Le\u00b7ben", ",", "wie", "im", "Ster\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Solch edle Spezerei", "tokens": ["Solch", "ed\u00b7le", "Spe\u00b7ze\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kann Jeder hier erwerben.", "tokens": ["Kann", "Je\u00b7der", "hier", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auf stillen Felsenh\u00f6h'n", "tokens": ["Auf", "stil\u00b7len", "Fel\u00b7sen\u00b7h\u00f6h'n"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir standen viele N\u00e4chte,", "tokens": ["Wir", "stan\u00b7den", "vie\u00b7le", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort nach dem Licht zu seh'n", "tokens": ["Dort", "nach", "dem", "Licht", "zu", "seh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU"