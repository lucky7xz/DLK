, "hol\u00b7des", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach unserm Grabe ziehet,", "tokens": ["Nach", "un\u00b7serm", "Gra\u00b7be", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn treu und stillgesinnt", "tokens": ["Wenn", "treu", "und", "still\u00b7ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich Muttersorge m\u00fchet,", "tokens": ["Sich", "Mut\u00b7ter\u00b7sor\u00b7ge", "m\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Dann f\u00fchlen wir die Lust", "tokens": ["Dann", "f\u00fch\u00b7len", "wir", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Aus alter Zeit sich regen,", "tokens": ["Aus", "al\u00b7ter", "Zeit", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es zieht in manche Brust", "tokens": ["Es", "zieht", "in", "man\u00b7che", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus unserm Grab der Segen.", "tokens": ["Aus", "un\u00b7serm", "Grab", "der", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Der Myrrhen Bitterkeit \u2013", "tokens": ["Der", "Myr\u00b7rhen", "Bit\u00b7ter\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Man kennt sie wol im Leben,", "tokens": ["Man", "kennt", "sie", "wol", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sollen dr\u00fcber weit", "tokens": ["Doch", "sol\u00b7len", "dr\u00fc\u00b7ber", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PAV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Weihrauchswolken schweben.", "tokens": ["Die", "Weih\u00b7rauchs\u00b7wol\u00b7ken", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Das Gold, es ist die Treu", "tokens": ["Das", "Gold", ",", "es", "ist", "die", "Treu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Leben, wie im Sterben:", "tokens": ["Im", "Le\u00b7ben", ",", "wie", "im", "Ster\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Solch edle Spezerei", "tokens": ["Solch", "ed\u00b7le", "Spe\u00b7ze\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kann Jeder hier erwerben.", "tokens": ["Kann", "Je\u00b7der", "hier", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}