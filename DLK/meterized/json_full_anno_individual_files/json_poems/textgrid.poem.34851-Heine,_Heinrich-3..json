"pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In dem Hain erwachen Lieder,", "tokens": ["In", "dem", "Hain", "er\u00b7wa\u00b7chen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Oreade spricht:", "tokens": ["Und", "die", "O\u00b7rea\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "\u203adeine Blumen kehren wieder,", "tokens": ["\u203a", "dei\u00b7ne", "Blu\u00b7men", "keh\u00b7ren", "wie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Deine Tochter kehret nicht.\u2039", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", "keh\u00b7ret", "nicht", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach wie lang ist's, da\u00df ich walle", "tokens": ["Ach", "wie", "lang", "ist's", ",", "da\u00df", "ich", "wal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchend durch der Erde Flur!", "tokens": ["Su\u00b7chend", "durch", "der", "Er\u00b7de", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Titan, deine Strahlen alle", "tokens": ["Ti\u00b