{"textgrid.poem.34851": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hrend solcherlei Beschwerde", "tokens": ["W\u00e4h\u00b7rend", "sol\u00b7cher\u00b7lei", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Unterwelt sich h\u00e4uft,", "tokens": ["In", "der", "Un\u00b7ter\u00b7welt", "sich", "h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jammert Ceres auf der Erde.", "tokens": ["Jam\u00b7mert", "Ce\u00b7res", "auf", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verr\u00fcckte G\u00f6ttin l\u00e4uft,", "tokens": ["Die", "ver\u00b7r\u00fcck\u00b7te", "G\u00f6t\u00b7tin", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Haube, ohne Kragen,", "tokens": ["Oh\u00b7ne", "Hau\u00b7be", ",", "oh\u00b7ne", "Kra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlotterbusig durch das Land,", "tokens": ["Schlot\u00b7ter\u00b7bu\u00b7sig", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deklamierend jene Klagen,", "tokens": ["De\u00b7kla\u00b7mie\u00b7rend", "je\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die euch allen wohlbekannt:", "tokens": ["Die", "euch", "al\u00b7len", "wohl\u00b7be\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbist der holde Lenz erschienen?", "tokens": ["\u00bb", "ist", "der", "hol\u00b7de", "Lenz", "er\u00b7schie\u00b7nen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Erde sich verj\u00fcngt?", "tokens": ["Hat", "die", "Er\u00b7de", "sich", "ver\u00b7j\u00fcngt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die besonnten H\u00fcgel gr\u00fcnen,", "tokens": ["Die", "be\u00b7sonn\u00b7ten", "H\u00fc\u00b7gel", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Eises Rinde springt.", "tokens": ["Und", "des", "Ei\u00b7ses", "Rin\u00b7de", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der Str\u00f6me blauem Spiegel", "tokens": ["Aus", "der", "Str\u00f6\u00b7me", "blau\u00b7em", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Lacht der unbew\u00f6lkte Zeus,", "tokens": ["Lacht", "der", "un\u00b7be\u00b7w\u00f6lk\u00b7te", "Zeus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Milder wehen Zephirs Fl\u00fcgel,", "tokens": ["Mil\u00b7der", "we\u00b7hen", "Ze\u00b7phirs", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Augen treibt das junge Reis.", "tokens": ["Au\u00b7gen", "treibt", "das", "jun\u00b7ge", "Reis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In dem Hain erwachen Lieder,", "tokens": ["In", "dem", "Hain", "er\u00b7wa\u00b7chen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Oreade spricht:", "tokens": ["Und", "die", "O\u00b7rea\u00b7de", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "\u203adeine Blumen kehren wieder,", "tokens": ["\u203a", "dei\u00b7ne", "Blu\u00b7men", "keh\u00b7ren", "wie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Deine Tochter kehret nicht.\u2039", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", "keh\u00b7ret", "nicht", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach wie lang ist's, da\u00df ich walle", "tokens": ["Ach", "wie", "lang", "ist's", ",", "da\u00df", "ich", "wal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchend durch der Erde Flur!", "tokens": ["Su\u00b7chend", "durch", "der", "Er\u00b7de", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Titan, deine Strahlen alle", "tokens": ["Ti\u00b7tan", ",", "dei\u00b7ne", "Strah\u00b7len", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sandt ich nach der teuren Spur!", "tokens": ["Sandt", "ich", "nach", "der", "teu\u00b7ren", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner hat mir noch verk\u00fcndet", "tokens": ["Kei\u00b7ner", "hat", "mir", "noch", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem lieben Angesicht,", "tokens": ["Von", "dem", "lie\u00b7ben", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Tag, der alles findet,", "tokens": ["Und", "der", "Tag", ",", "der", "al\u00b7les", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die Verlorne fand er nicht.", "tokens": ["Die", "Ver\u00b7lor\u00b7ne", "fand", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hast du, Zeus, sie mir entrissen?", "tokens": ["Hast", "du", ",", "Zeus", ",", "sie", "mir", "ent\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NE", "$,", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hat, von ihrem Reiz ger\u00fchrt,", "tokens": ["Hat", ",", "von", "ih\u00b7rem", "Reiz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Zu des Orkus schwarzen Fl\u00fcssen", "tokens": ["Zu", "des", "Or\u00b7kus", "schwar\u00b7zen", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Pluto sie hinabgef\u00fchrt?", "tokens": ["Plu\u00b7to", "sie", "hin\u00b7ab\u00b7ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer wird nach dem d\u00fcstern Strande", "tokens": ["Wer", "wird", "nach", "dem", "d\u00fcs\u00b7tern", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meines Grames Bote sein?", "tokens": ["Mei\u00b7nes", "Gra\u00b7mes", "Bo\u00b7te", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig st\u00f6\u00dft der Kahn vom Lande,", "tokens": ["E\u00b7wig", "st\u00f6\u00dft", "der", "Kahn", "vom", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch nur Schatten nimmt er ein.", "tokens": ["Doch", "nur", "Schat\u00b7ten", "nimmt", "er", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedem sel'gen Aug' verschlossen", "tokens": ["Je\u00b7dem", "sel'\u00b7gen", "Aug'", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bleibt das n\u00e4chtliche Gefild',", "tokens": ["Bleibt", "das", "n\u00e4cht\u00b7li\u00b7che", "Ge\u00b7fild'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und solang der Styx geflossen,", "tokens": ["Und", "so\u00b7lang", "der", "Styx", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trug er kein lebendig Bild.", "tokens": ["Trug", "er", "kein", "le\u00b7ben\u00b7dig", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nieder f\u00fchren tausend Steige,", "tokens": ["Nie\u00b7der", "f\u00fch\u00b7ren", "tau\u00b7send", "Stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Keiner f\u00fchrt zum Tag zur\u00fcck;", "tokens": ["Kei\u00b7ner", "f\u00fchrt", "zum", "Tag", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ihre Tr\u00e4ne bringt kein Zeuge", "tokens": ["Ih\u00b7re", "Tr\u00e4\u00b7ne", "bringt", "kein", "Zeu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Vor der bangen Mutter Blick.\u00ab", "tokens": ["Vor", "der", "ban\u00b7gen", "Mut\u00b7ter", "Blick", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "W\u00e4hrend solcherlei Beschwerde", "tokens": ["W\u00e4h\u00b7rend", "sol\u00b7cher\u00b7lei", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Unterwelt sich h\u00e4uft,", "tokens": ["In", "der", "Un\u00b7ter\u00b7welt", "sich", "h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"},