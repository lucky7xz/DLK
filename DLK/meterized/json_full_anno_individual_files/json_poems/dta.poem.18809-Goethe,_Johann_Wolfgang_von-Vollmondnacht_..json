{"dta.poem.18809": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Vollmondnacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Herrinn! sag was heisst das Fl\u00fcstern?", "tokens": ["Her\u00b7rinn", "!", "sag", "was", "heisst", "das", "Fl\u00fcs\u00b7tern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was bewegt dir leis\u2019 die Lippen?", "tokens": ["Was", "be\u00b7wegt", "dir", "leis'", "die", "Lip\u00b7pen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lispelst immer vor dich hin,", "tokens": ["Lis\u00b7pelst", "im\u00b7mer", "vor", "dich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieblicher als Weines Nippen!", "tokens": ["Lieb\u00b7li\u00b7cher", "als", "Wei\u00b7nes", "Nip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denkst du deinen Mundgeschwistern", "tokens": ["Denkst", "du", "dei\u00b7nen", "Mund\u00b7ge\u00b7schwis\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch ein P\u00e4rchen herzuziehn?", "tokens": ["Noch", "ein", "P\u00e4r\u00b7chen", "her\u00b7zu\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will k\u00fcssen! K\u00fcssen! sagt\u2019 ich.", "tokens": ["Ich", "will", "k\u00fcs\u00b7sen", "!", "K\u00fcs\u00b7sen", "!", "sagt'", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schau! Im zweifelhaften Dunkel", "tokens": ["Schau", "!", "Im", "zwei\u00b7fel\u00b7haf\u00b7ten", "Dun\u00b7kel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fchen bl\u00fchend alle Zweige,", "tokens": ["Gl\u00fc\u00b7hen", "bl\u00fc\u00b7hend", "al\u00b7le", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nieder spielet Stern auf Stern,", "tokens": ["Nie\u00b7der", "spie\u00b7let", "Stern", "auf", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, smaragden, durchs Gestr\u00e4uche", "tokens": ["Und", ",", "sma\u00b7rag\u00b7den", ",", "durchs", "Ge\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Tausendf\u00e4ltiger Karfunkel;", "tokens": ["Tau\u00b7send\u00b7f\u00e4l\u00b7ti\u00b7ger", "Kar\u00b7fun\u00b7kel", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch dein Geist ist allem fern.", "tokens": ["Doch", "dein", "Geist", "ist", "al\u00b7lem", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will k\u00fcssen! K\u00fcssen! sagt\u2019 ich.", "tokens": ["Ich", "will", "k\u00fcs\u00b7sen", "!", "K\u00fcs\u00b7sen", "!", "sagt'", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Geliebter, fern, erprobet", "tokens": ["Dein", "Ge\u00b7lieb\u00b7ter", ",", "fern", ",", "er\u00b7pro\u00b7bet"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleicherweis im Sauers\u00fcssen,", "tokens": ["Glei\u00b7cher\u00b7weis", "im", "Sau\u00b7er\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlt ein ungl\u00fccksel\u2019ges Gl\u00fcck.", "tokens": ["F\u00fchlt", "ein", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ges", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch im Vollmond zu begr\u00fcssen", "tokens": ["Euch", "im", "Voll\u00b7mond", "zu", "be\u00b7gr\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Habt ihr heilig angelobet,", "tokens": ["Habt", "ihr", "hei\u00b7lig", "an\u00b7ge\u00b7lo\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses ist der Augenblick.", "tokens": ["Die\u00b7ses", "ist", "der", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will k\u00fcssen! K\u00fcssen! sag\u2019 ich.", "tokens": ["Ich", "will", "k\u00fcs\u00b7sen", "!", "K\u00fcs\u00b7sen", "!", "sag'", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}