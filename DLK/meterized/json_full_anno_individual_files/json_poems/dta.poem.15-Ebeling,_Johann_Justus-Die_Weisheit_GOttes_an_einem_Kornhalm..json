{"dta.poem.15": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Weisheit GOttes  \n an einem Kornhalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man saget, da\u00df ein Atheiste, Julius C\u00e4sar Vaninus. den man\nschon lange so genannt,", "tokens": ["Man", "sa\u00b7get", ",", "da\u00df", "ein", "A\u00b7theis\u00b7te", ",", "Ju\u00b7lius", "C\u00e4\u00b7sar", "Va\u00b7ni\u00b7nus", ".", "den", "man", "schon", "lan\u00b7ge", "so", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ART", "ADJA", "$,", "NE", "NE", "NE", "$.", "ART", "PIS", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--++--+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Vor seinem scharfen Hals-Gerichte, aus", "tokens": ["Vor", "sei\u00b7nem", "schar\u00b7fen", "Hals\u00b7Ge\u00b7rich\u00b7te", ",", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seinen Nichter zeigen wollen: Man k\u00f6nn aus", "tokens": ["Und", "sei\u00b7nen", "Nich\u00b7ter", "zei\u00b7gen", "wol\u00b7len", ":", "Man", "k\u00f6nn", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VMINF", "$.", "PIS", "VMFIN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es sei ein ", "tokens": ["Es", "sei", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "---", "measure": "unknown.measure.zero"}, "line.5": {"text": "Was er vielleicht sich zu erretten, gesagt, ist doch", "tokens": ["Was", "er", "viel\u00b7leicht", "sich", "zu", "er\u00b7ret\u00b7ten", ",", "ge\u00b7sagt", ",", "ist", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ADV", "PRF", "PTKZU", "VVINF", "$,", "VVPP", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So viele Halmen auf den Felde, so viele Zeugen", "tokens": ["So", "vie\u00b7le", "Hal\u00b7men", "auf", "den", "Fel\u00b7de", ",", "so", "vie\u00b7le", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Von seiner GOttheit ewgen Wesen, da uns der", "tokens": ["Von", "sei\u00b7ner", "Got\u00b7theit", "ew\u00b7gen", "We\u00b7sen", ",", "da", "uns", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Des Sch\u00f6pfers Dasein, Macht und G\u00fcte, und", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Da\u00b7sein", ",", "Macht", "und", "G\u00fc\u00b7te", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Er w\u00e4chset aus dem Saamenkorne, der ihn in sei-", "tokens": ["Er", "w\u00e4ch\u00b7set", "aus", "dem", "Saa\u00b7men\u00b7kor\u00b7ne", ",", "der", "ihn", "in", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Woraus er denn in feuchten Schoo\u00dfe der Erde, durch", "tokens": ["Wo\u00b7raus", "er", "denn", "in", "feuch\u00b7ten", "Schoo\u00b7\u00dfe", "der", "Er\u00b7de", ",", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Von der verborgnen Frucht gen\u00e4hret, sich ausein-", "tokens": ["Von", "der", "ver\u00b7borg\u00b7nen", "Frucht", "ge\u00b7n\u00e4h\u00b7ret", ",", "sich", "aus\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und durch des Wachsthums rege S\u00e4fte, bis zur", "tokens": ["Und", "durch", "des", "Wach\u00b7sthums", "re\u00b7ge", "S\u00e4f\u00b7te", ",", "bis", "zur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,", "KOUS", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn man dies mit Vernunft erweget; so zeiget sich", "tokens": ["Wenn", "man", "dies", "mit", "Ver\u00b7nunft", "er\u00b7we\u00b7get", ";", "so", "zei\u00b7get", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PDS", "APPR", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Weil weder Sonne, Luft und Erde, ihn von sich", "tokens": ["Weil", "we\u00b7der", "Son\u00b7ne", ",", "Luft", "und", "Er\u00b7de", ",", "ihn", "von", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KON", "NN", "$,", "NN", "KON", "NN", "$,", "PPER", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer solte, wenn wirs nicht gesehen, woll dies als", "tokens": ["Wer", "sol\u00b7te", ",", "wenn", "wirs", "nicht", "ge\u00b7se\u00b7hen", ",", "woll", "dies", "als"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "$,", "KOUS", "PIS", "PTKNEG", "VVPP", "$,", "VMFIN", "PDS", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es k\u00f6nne ein so kleiner Saame, aus sich so langen", "tokens": ["Es", "k\u00f6n\u00b7ne", "ein", "so", "klei\u00b7ner", "Saa\u00b7me", ",", "aus", "sich", "so", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADV", "ADJA", "NN", "$,", "APPR", "PRF", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Man sieht des Allerh\u00f6chsten Wunder, wenn man", "tokens": ["Man", "sieht", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Wun\u00b7der", ",", "wenn", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wie er sich auf dem schwanken Fusse, so steif zu", "tokens": ["Wie", "er", "sich", "auf", "dem", "schwan\u00b7ken", "Fus\u00b7se", ",", "so", "steif", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "PTKZU"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Wie k\u00fcnstlich er ist ausgeh\u00f6let; wie sich dadurch", "tokens": ["Wie", "k\u00fcnst\u00b7lich", "er", "ist", "aus\u00b7ge\u00b7h\u00f6\u00b7let", ";", "wie", "sich", "da\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$.", "PWAV", "PRF", "PAV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Die durch der Wurzeln hohle Spizzen, zwar un-", "tokens": ["Die", "durch", "der", "Wur\u00b7zeln", "hoh\u00b7le", "Spiz\u00b7zen", ",", "zwar", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "In seiner R\u00f6hre circuliren, bis da\u00df die Aehre draus", "tokens": ["In", "sei\u00b7ner", "R\u00f6h\u00b7re", "cir\u00b7cu\u00b7li\u00b7ren", ",", "bis", "da\u00df", "die", "A\u00b7eh\u00b7re", "draus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "KON", "KOUS", "ART", "NN", "PAV"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Die sich mit allen ihren Theilen, gleich einen Kopf", "tokens": ["Die", "sich", "mit", "al\u00b7len", "ih\u00b7ren", "Thei\u00b7len", ",", "gleich", "ei\u00b7nen", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PIAT", "PPOSAT", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Hier \u00e4usert sich vor unsern Augen, ein Allmachts-", "tokens": ["Hier", "\u00e4u\u00b7sert", "sich", "vor", "un\u00b7sern", "Au\u00b7gen", ",", "ein", "All\u00b7machts"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Wenn man vern\u00fcnftig \u00fcberdenket, wie er die Aeh-", "tokens": ["Wenn", "man", "ver\u00b7n\u00fcnf\u00b7tig", "\u00fc\u00b7ber\u00b7den\u00b7ket", ",", "wie", "er", "die", "A\u00b7eh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "PWAV", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "Die von der K\u00f6rner Last beschweret, da\u00df dadurch", "tokens": ["Die", "von", "der", "K\u00f6r\u00b7ner", "Last", "be\u00b7schwe\u00b7ret", ",", "da\u00df", "da\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NE", "VVFIN", "$,", "KOUS", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil ihn die Aehre durchs Gewichte der vielen K\u00f6r-", "tokens": ["Weil", "ihn", "die", "A\u00b7eh\u00b7re", "durchs", "Ge\u00b7wich\u00b7te", "der", "vie\u00b7len", "K\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "ART", "PIAT", "NN"], "meter": "-+----+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Der Sch\u00f6pfer hat aus weisen Gr\u00fcnden, den Halm", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "hat", "aus", "wei\u00b7sen", "Gr\u00fcn\u00b7den", ",", "den", "Halm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "Damit er, wenn er wird beschweret, dennoch kan", "tokens": ["Da\u00b7mit", "er", ",", "wenn", "er", "wird", "be\u00b7schwe\u00b7ret", ",", "den\u00b7noch", "kan"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Knoten sind wie starke Bande, die sich jemehr", "tokens": ["Die", "Kno\u00b7ten", "sind", "wie", "star\u00b7ke", "Ban\u00b7de", ",", "die", "sich", "je\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ADJA", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Jemehr die Aehr zu Boden lenket, mit ihren har-", "tokens": ["Je\u00b7mehr", "die", "A\u00b7ehr", "zu", "Bo\u00b7den", "len\u00b7ket", ",", "mit", "ih\u00b7ren", "ha\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "+---+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Die Knoten sizzen an der Stelle, wo sonst bei dr\u00fck-", "tokens": ["Die", "Kno\u00b7ten", "siz\u00b7zen", "an", "der", "Stel\u00b7le", ",", "wo", "sonst", "bei", "dr\u00fc\u00b7k"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+--++-+", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Ein Rohr von solcher Halmen L\u00e4nge, am ersten", "tokens": ["Ein", "Rohr", "von", "sol\u00b7cher", "Hal\u00b7men", "L\u00e4n\u00b7ge", ",", "am", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "NN", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Dies lehret uns ein weiser Meister, der alles reiflich", "tokens": ["Dies", "leh\u00b7ret", "uns", "ein", "wei\u00b7ser", "Meis\u00b7ter", ",", "der", "al\u00b7les", "reif\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Hat hie es k\u00fcnstlich abgemessen als er die Halmen", "tokens": ["Hat", "hie", "es", "k\u00fcnst\u00b7lich", "ab\u00b7ge\u00b7mes\u00b7sen", "als", "er", "die", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "ADJD", "VVPP", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Die Knoten sind so eingerichtet, da\u00df sie der starken", "tokens": ["Die", "Kno\u00b7ten", "sind", "so", "ein\u00b7ge\u00b7rich\u00b7tet", ",", "da\u00df", "sie", "der", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Und dem gepre\u00dften Hauch der L\u00fcfte, der sie an-", "tokens": ["Und", "dem", "ge\u00b7pre\u00df\u00b7ten", "Hauch", "der", "L\u00fcf\u00b7te", ",", "der", "sie", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Halmen k\u00f6nnen sich doch beugen, die dem em-", "tokens": ["Die", "Hal\u00b7men", "k\u00f6n\u00b7nen", "sich", "doch", "beu\u00b7gen", ",", "die", "dem", "em"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,", "PRELS", "ART", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.38": {"text": "Sonst br\u00e4che sie der Winde Blasen, dem sie nicht", "tokens": ["Sonst", "br\u00e4\u00b7che", "sie", "der", "Win\u00b7de", "Bla\u00b7sen", ",", "dem", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,", "PRELS", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie wallen wie der Wind sich drehet, und wenn", "tokens": ["Sie", "wal\u00b7len", "wie", "der", "Wind", "sich", "dre\u00b7het", ",", "und", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "KOKOM", "ART", "NN", "PRF", "VVFIN", "$,", "KON", "KOUS"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.40": {"text": "So sieht man wie der volle Akker, von seinen Fr\u00fcch-", "tokens": ["So", "sieht", "man", "wie", "der", "vol\u00b7le", "Ak\u00b7ker", ",", "von", "sei\u00b7nen", "Fr\u00fcch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Es richtet sich der Halmen Menge, allm\u00e4hlig wie-", "tokens": ["Es", "rich\u00b7tet", "sich", "der", "Hal\u00b7men", "Men\u00b7ge", ",", "all\u00b7m\u00e4h\u00b7lig", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN", "$,", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Dabei wir denn von neuen wieder, der Knoten", "tokens": ["Da\u00b7bei", "wir", "denn", "von", "neu\u00b7en", "wie\u00b7der", ",", "der", "Kno\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "PPER", "ADV", "APPR", "ADJA", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Wie weislich hat es ", "tokens": ["Wie", "weis\u00b7lich", "hat", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.44": {"text": "Denn dadurch wird dem Feldbesizzer, ein gr\u00f6\u00dfrer", "tokens": ["Denn", "da\u00b7durch", "wird", "dem", "Feld\u00b7be\u00b7siz\u00b7zer", ",", "ein", "gr\u00f6\u00df\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Weil auf dem abgeme\u00dfnen Akker, so viele tausen-", "tokens": ["Weil", "auf", "dem", "ab\u00b7ge\u00b7me\u00df\u00b7nen", "Ak\u00b7ker", ",", "so", "vie\u00b7le", "tau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "Die sich in ihren engen Grenzen, recht dichte, in", "tokens": ["Die", "sich", "in", "ih\u00b7ren", "en\u00b7gen", "Gren\u00b7zen", ",", "recht", "dich\u00b7te", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "ADJA", "$,", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.47": {"text": "Und gleichsam eine Wand formiren. O! welch ein", "tokens": ["Und", "gleich\u00b7sam", "ei\u00b7ne", "Wand", 