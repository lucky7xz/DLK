{"textgrid.poem.48239": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Verzeiht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzeiht den Anekdotenkram", "tokens": ["Ver\u00b7zeiht", "den", "A\u00b7nek\u00b7do\u00b7ten\u00b7kram"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df niemals ich einen \u00bbAnlauf\u00ab nahm,", "tokens": ["Und", "da\u00df", "nie\u00b7mals", "ich", "ei\u00b7nen", "\u00bb", "An\u00b7lauf", "\u00ab", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "ART", "$(", "NN", "$(", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auch niemals mit den G\u00f6ttern grollte,", "tokens": ["Auch", "nie\u00b7mals", "mit", "den", "G\u00f6t\u00b7tern", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mal den Staat verbessern wollte,", "tokens": ["Nicht", "mal", "den", "Staat", "ver\u00b7bes\u00b7sern", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht mal mit \u00bbsexuellen Problemen\u00ab", "tokens": ["Nicht", "mal", "mit", "\u00bb", "se\u00b7xu\u00b7el\u00b7len", "Prob\u00b7le\u00b7men", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "$(", "ADJA", "NN", "$("], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gelegenheit nahm mich zu benehmen.", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "nahm", "mich", "zu", "be\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Man mu\u00df nur wollen, was man kann,", "tokens": ["Man", "mu\u00df", "nur", "wol\u00b7len", ",", "was", "man", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VMFIN", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir w\u00fcrde der Weitsprung nicht gelingen,", "tokens": ["Mir", "w\u00fcr\u00b7de", "der", "Weit\u00b7sprung", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So blieb ich denn bei den n\u00e4heren Dingen,", "tokens": ["So", "blieb", "ich", "denn", "bei", "den", "n\u00e4\u00b7he\u00b7ren", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Drei Schritt blo\u00df \u2013 \u2013 ich wei\u00df, es ist nicht viel,", "tokens": ["Drei", "Schritt", "blo\u00df", "\u2013", "\u2013", "ich", "wei\u00df", ",", "es", "ist", "nicht", "viel", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$(", "$(", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5