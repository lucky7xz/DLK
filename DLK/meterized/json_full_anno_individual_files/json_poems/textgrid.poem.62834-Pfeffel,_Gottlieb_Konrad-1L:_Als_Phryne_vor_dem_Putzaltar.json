{"textgrid.poem.62834": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Phryne vor dem Putzaltar", "genre": "verse", "period": "N.A.", "pub_year": 1760, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Phryne vor dem Putzaltar", "tokens": ["Als", "Phry\u00b7ne", "vor", "dem", "Put\u00b7zal\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr balsamirtes schwarzes Haar", "tokens": ["Ihr", "bal\u00b7sa\u00b7mir\u00b7tes", "schwar\u00b7zes", "Haar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit grauem Modepuder schm\u00fcckte,", "tokens": ["Mit", "grau\u00b7em", "Mo\u00b7de\u00b7pu\u00b7der", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit dem Anstand einer Braut", "tokens": ["Und", "mit", "dem", "An\u00b7stand", "ei\u00b7ner", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf ihre zarte Rosenhaut", "tokens": ["Auf", "ih\u00b7re", "zar\u00b7te", "Ro\u00b7sen\u00b7haut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein ganzes Weltsystem von schwarzen Pflastern dr\u00fcckte,", "tokens": ["Ein", "gan\u00b7zes", "Welt\u00b7sys\u00b7tem", "von", "schwar\u00b7zen", "Pflas\u00b7tern", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "So trat Urania, die gute Fey,", "tokens": ["So", "trat", "U\u00b7ra\u00b7nia", ",", "die", "gu\u00b7te", "Fey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Die sie zur Taufe hob, unangeklopft herbey", "tokens": ["Die", "sie", "zur", "Tau\u00b7fe", "hob", ",", "un\u00b7an\u00b7ge\u00b7klopft", "her\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und sprach, mit Majest\u00e4t in jeder Miene,", "tokens": ["Und", "sprach", ",", "mit", "Ma\u00b7jes\u00b7t\u00e4t", "in", "je\u00b7der", "Mie\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit Ernst in jedem Ton, zur armen blassen Phryne:", "tokens": ["Mit", "Ernst", "in", "je\u00b7dem", "Ton", ",", "zur", "ar\u00b7men", "blas\u00b7sen", "Phry\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PIAT", "NN", "$,", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Noch ehe du das Licht der Unterwelt erblickt,", "tokens": ["Noch", "e\u00b7he", "du", "das", "Licht", "der", "Un\u00b7ter\u00b7welt", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hab ich dich mit dem Reiz des Himmels ausgeschm\u00fcckt:", "tokens": ["Hab", "ich", "dich", "mit", "dem", "Reiz", "des", "Him\u00b7mels", "aus\u00b7ge\u00b7schm\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und du Wahnsinnige! du wagst mit eignen H\u00e4nden", "tokens": ["Und", "du", "Wahn\u00b7sin\u00b7ni\u00b7ge", "!", "du", "wagst", "mit", "eig\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das Denkmal meiner Huld zu sch\u00e4nden?", "tokens": ["Das", "Denk\u00b7mal", "mei\u00b7ner", "Huld", "zu", "sch\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Itzt f\u00e4hrt sie mit dem Zauberstab", "tokens": ["Itzt", "f\u00e4hrt", "sie", "mit", "dem", "Zau\u00b7ber\u00b7stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Erz\u00fcrnt an Phrynens kalte Stirne;", "tokens": ["Er\u00b7z\u00fcrnt", "an", "Phry\u00b7nens", "kal\u00b7te", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wohlan, galante Modedirne,", "tokens": ["Wo\u00b7hlan", ",", "ga\u00b7lan\u00b7te", "Mo\u00b7de\u00b7dir\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Behalte stets den Reiz, den deine Kunst dir gab.", "tokens": ["Be\u00b7hal\u00b7te", "stets", "den", "Reiz", ",", "den", "dei\u00b7ne", "Kunst", "dir", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So sprach die Fey, und nun war sie verschwunden.", "tokens": ["So", "sprach", "die", "Fey", ",", "und", "nun", "war", "sie", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die gute Phryne sa\u00df allein,", "tokens": ["Die", "gu\u00b7te", "Phry\u00b7ne", "sa\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Gleich einem unbeseelten Stein,", "tokens": ["Gleich", "ei\u00b7nem", "un\u00b7be\u00b7seel\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Den halben Vormittag im Armstuhl angebunden.", "tokens": ["Den", "hal\u00b7ben", "Vor\u00b7mit\u00b7tag", "im", "Arm\u00b7stuhl", "an\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Kaum kam sie zu sich selbst zur\u00fcck,", "tokens": ["Kaum", "kam", "sie", "zu", "sich", "selbst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So blieb ihr erster schwacher Blick", "tokens": ["So", "blieb", "ihr", "ers\u00b7ter", "schwa\u00b7cher", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Auf dem getreuen Spiegel kleben.", "tokens": ["Auf", "dem", "ge\u00b7treu\u00b7en", "Spie\u00b7gel", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Was seh ich? Himmel! ist es wahr?", "tokens": ["Was", "seh", "ich", "?", "Him\u00b7mel", "!", "ist", "es", "wahr", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "$.", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich eine Tiegerhaut? ich graues Haar?", "tokens": ["Ich", "ei\u00b7ne", "Tie\u00b7ger\u00b7haut", "?", "ich", "grau\u00b7es", "Haar", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die Schmach kann ich nicht \u00fcberleben!", "tokens": ["Die", "Schmach", "kann", "ich", "nicht", "\u00fc\u00b7berl\u00b7e\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "So ruft sie, rauft sich voller Wuth", "tokens": ["So", "ruft", "sie", ",", "rauft", "sich", "vol\u00b7ler", "Wuth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Die Locken aus, und w\u00e4scht sich bis aufs Blut.", "tokens": ["Die", "Lo\u00b7cken", "aus", ",", "und", "w\u00e4scht", "sich", "bis", "aufs", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Umsonst, es will ihr nicht gelingen", "tokens": ["Um\u00b7sonst", ",", "es", "will", "ihr", "nicht", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Die schwarzen Flecken wegzubringen:", "tokens": ["Die", "schwar\u00b7zen", "Fle\u00b7cken", "weg\u00b7zu\u00b7brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und ach, sie starb, wie man leicht denken kann,", "tokens": ["Und", "ach", ",", "sie", "starb", ",", "wie", "man", "leicht", "den\u00b7ken", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "$,", "PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "\u2013 Ihr M\u00e4dchen, zittert \u2013 ohne Mann.", "tokens": ["\u2013", "Ihr", "M\u00e4d\u00b7chen", ",", "zit\u00b7tert", "\u2013", "oh\u00b7ne", "Mann", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als Phryne vor dem Putzaltar", "tokens": ["Als", "Phry\u00b7ne", "vor", "dem", "Put\u00b7zal\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr balsamirtes schwarzes Haar", "tokens": ["Ihr", "bal\u00b7sa\u00b7mir\u00b7tes", "schwar\u00b7zes", "Haar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit grauem Modepuder schm\u00fcckte,", "tokens": ["Mit", "grau\u00b7em", "Mo\u00b7de\u00b7pu\u00b7der", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit dem Anstand einer Braut", "tokens": ["Und", "mit", "dem", "An\u00b7stand", "ei\u00b7ner", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf ihre zarte Rosenhaut", "tokens": ["Auf", "ih\u00b7re", "zar\u00b7te", "Ro\u00b7sen\u00b7haut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein ganzes Weltsystem von schwarzen Pflastern dr\u00fcckte,", "tokens": ["Ein", "gan\u00b7zes", "Welt\u00b7sys\u00b7tem", "von", "schwar\u00b7zen", "Pflas\u0