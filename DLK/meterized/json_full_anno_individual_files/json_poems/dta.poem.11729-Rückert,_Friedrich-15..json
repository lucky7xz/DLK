{"dta.poem.11729": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Baumes Bl\u00fct' erfreut, des Baumes Schatten beut", "tokens": ["Des", "Bau\u00b7mes", "Bl\u00fct'", "er\u00b7freut", ",", "des", "Bau\u00b7mes", "Schat\u00b7ten", "beut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Dach dir, und ein Mahl die Frucht, die er verstreut.", "tokens": ["Ein", "Dach", "dir", ",", "und", "ein", "Mahl", "die", "Frucht", ",", "die", "er", "ver\u00b7streut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "KON", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was brauchst du noch? ein Kleid? nimm es von seinem Bast;", "tokens": ["Was", "brauchst", "du", "noch", "?", "ein", "Kleid", "?", "nimm", "es", "von", "sei\u00b7nem", "Bast", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "$.", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mach' auch ein Buch daraus, wenn du es n\u00f6thig hast.", "tokens": ["Mach'", "auch", "ein", "Buch", "da\u00b7raus", ",", "wenn", "du", "es", "n\u00f6\u00b7thig", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und brauchst du dann ein Grab, er wird dich auch begraben,", "tokens": ["Und", "brauchst", "du", "dann", "ein", "Grab", ",", "er", "wird", "dich", "auch", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag Ruh im k\u00fchlen Grund, mag Feuertod dich laben.", "tokens": ["Mag", "Ruh", "im", "k\u00fch\u00b7len", "Grund", ",", "mag", "Feu\u00b7er\u00b7tod", "dich", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPRART", "ADJA", "NN", "$,", "VMFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Scheiterhaufen baut er hier, und dort den Sarg,", "tokens": ["Den", "Schei\u00b7ter\u00b7hau\u00b7fen", "baut", "er", "hier", ",", "und", "dort", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis deinen Rest im Schirm er seiner Wurzeln barg.", "tokens": ["Bis", "dei\u00b7nen", "Rest", "im", "Schirm", "er", "sei\u00b7ner", "Wur\u00b7zeln", "barg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was keucht durch fernen Raum der Hunger fremden Brodes,", "tokens": ["Was", "keucht", "durch", "fer\u00b7nen", "Raum", "der", "Hun\u00b7ger", "frem\u00b7den", "Bro\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dich begn\u00fcgt ein Baum des Lebens und des Todes?", "tokens": ["Wenn", "dich", "be\u00b7gn\u00fcgt", "ein", "Baum", "des", "Le\u00b7bens", "und", "des", "To\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Als Vogel schwinge sich dein Geist, vom Leib geschieden,", "tokens": ["Als", "Vo\u00b7gel", "schwin\u00b7ge", "sich", "dein", "Geist", ",", "vom", "Leib", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem h\u00f6chsten Wipfel zu, der nicht mehr ist hienieden;", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "Wip\u00b7fel", "zu", ",", "der", "nicht", "mehr", "ist", "hien\u00b7ie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "PTKNEG", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und singe von dem Baum des Todes und des Lebens", "tokens": ["Und", "sin\u00b7ge", "von", "dem", "Baum", "des", "To\u00b7des", "und", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herab zum Erdenraum den Frieden nicht vergebens.", "tokens": ["Her\u00b7ab", "zum", "Er\u00b7den\u00b7raum", "den", "Frie\u00b7den", "nicht", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}