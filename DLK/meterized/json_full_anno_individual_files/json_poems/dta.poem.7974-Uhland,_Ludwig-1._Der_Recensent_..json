{"dta.poem.7974": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.  Der Recensent .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste! du hast mir befohlen,", "tokens": ["Sch\u00f6ns\u00b7te", "!", "du", "hast", "mir", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Thema zu glossiren;", "tokens": ["Die\u00b7ses", "The\u00b7ma", "zu", "glos\u00b7si\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Doch ich sag\u2019 es unverhohlen:", "tokens": ["Doch", "ich", "sag'", "es", "un\u00b7ver\u00b7hoh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses hei\u00dft die Zeit verlieren,", "tokens": ["Die\u00b7ses", "hei\u00dft", "die", "Zeit", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich sitze wie auf Kohlen.", "tokens": ["Und", "ich", "sit\u00b7ze", "wie", "auf", "Koh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebtet ihr nicht, stolze Sch\u00f6nen!", "tokens": ["Lieb\u00b7tet", "ihr", "nicht", ",", "stol\u00b7ze", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Selbst die Logik zu verh\u00f6hnen,", "tokens": ["Selbst", "die", "Lo\u00b7gik", "zu", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcrd\u2019 ich zu beweisen wagen,", "tokens": ["W\u00fcrd'", "ich", "zu", "be\u00b7wei\u00b7sen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df es Unsinn ist, zu sagen:", "tokens": ["Da\u00df", "es", "Un\u00b7sinn", "ist", ",", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "S&#252;sse Liebe denkt in T\u00f6nen.", "tokens": ["S", "&#252;", "sse", "Lie\u00b7be", "denkt", "in", "T\u00f6\u00b7nen", "."], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$(", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar versteh\u2019 ich wohl das Schema", "tokens": ["Zwar", "ver\u00b7steh'", "ich", "wohl", "das", "Sche\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dieser abgeschmackten Glossen,", "tokens": ["Die\u00b7ser", "ab\u00b7ge\u00b7schmack\u00b7ten", "Glos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber solch verzwicktes Thema,", "tokens": ["A\u00b7ber", "solch", "ver\u00b7zwick\u00b7tes", "The\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Solche r\u00e4thselhafte Possen", "tokens": ["Sol\u00b7che", "r\u00e4th\u00b7sel\u00b7haf\u00b7te", "Pos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind ein gordisches Problema.", "tokens": ["Sind", "ein", "gor\u00b7di\u00b7sches", "Prob\u00b7le\u00b7ma", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dennoch macht\u2019 ich dir, mein Stern!", "tokens": ["Den\u00b7noch", "macht'", "ich", "dir", ",", "mein", "Stern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Diese Freude gar zu gern.", "tokens": ["Die\u00b7se", "Freu\u00b7de", "gar", "zu", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "PTKA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hoffnunglos reib\u2019 ich die H\u00e4nde,", "tokens": ["Hoff\u00b7nun\u00b7glos", "reib'", "ich", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nimmer bring\u2019 ich es zu Ende,", "tokens": ["Nim\u00b7mer", "bring'", "ich", "es", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn Gedanken stehn zu fern.", "tokens": ["Denn", "Ge\u00b7dan\u00b7ken", "stehn", "zu", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df, mein Kind! die span\u2019sche Mode,", "tokens": ["La\u00df", ",", "mein", "Kind", "!", "die", "span'\u00b7sche", "Mo\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die fremden Triolette,", "tokens": ["La\u00df", "die", "frem\u00b7den", "Tri\u00b7o\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die w\u00e4lsche Klangmethode", "tokens": ["La\u00df", "die", "w\u00e4l\u00b7sche", "Klang\u00b7me\u00b7tho\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Kanzonen und Sonette,", "tokens": ["Der", "Kan\u00b7zo\u00b7nen", "und", "So\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleib bei deiner sapph\u2019schen Ode!", "tokens": ["Bleib", "bei", "dei\u00b7ner", "sap\u00b7ph'\u00b7schen", "O\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Bleib der Aftermuse fern", "tokens": ["Bleib", "der", "Af\u00b7ter\u00b7mu\u00b7se", "fern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der romantisch s\u00fc\u00dfen Herrn!", "tokens": ["Der", "ro\u00b7man\u00b7tisch", "s\u00fc\u00b7\u00dfen", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Duftig schwebeln, luftig t\u00e4nzeln", "tokens": ["Duf\u00b7tig", "schwe\u00b7beln", ",", "luf\u00b7tig", "t\u00e4n\u00b7zeln"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur in Reimchen, Asson\u00e4nzeln,", "tokens": ["Nur", "in", "Reim\u00b7chen", ",", "As\u00b7so\u00b7n\u00e4n\u00b7zeln", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur in T\u00f6nen mag sie gern.", "tokens": ["Nur", "in", "T\u00f6\u00b7nen", "mag", "sie", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht in T\u00f6nen solcher Glossen", "tokens": ["Nicht", "in", "T\u00f6\u00b7nen", "sol\u00b7cher", "Glos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann die Poesie sich zeigen;", "tokens": ["Kann", "die", "Poe\u00b7sie", "sich", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "In antiken Verskolossen", "tokens": ["In", "an\u00b7ti\u00b7ken", "Ver\u00b7sko\u00b7los\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Stampft sie besser ihren Reigen", "tokens": ["Stampft", "sie", "bes\u00b7ser", "ih\u00b7ren", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Spondeen und Molossen.", "tokens": ["Mit", "Spon\u00b7de\u00b7en", "und", "Mo\u00b7los\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nur im Hammerschlag und Dr\u00f6hnen", "tokens": ["Nur", "im", "Ham\u00b7mer\u00b7schlag", "und", "Dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deutschhellenischer Kam\u00f6nen", "tokens": ["Deutschhel\u00b7le\u00b7ni\u00b7scher", "Ka\u00b7m\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Kann sie selbst die alten, kranken,", "tokens": ["Kann", "sie", "selbst", "die", "al\u00b7ten", ",", "kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Allerh\u00e4\u00dflichsten Gedanken,", "tokens": ["Al\u00b7ler\u00b7h\u00e4\u00df\u00b7lichs\u00b7ten", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.10": {"text": "Alles, was sie will, versch\u00f6nen.", "tokens": ["Al\u00b7les", ",", "was", "sie", "will", ",", "ver\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VMFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}