{"dta.poem.21774": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Was frag' ich nach den Trauer-fahnen/", "tokens": ["Was", "frag'", "ich", "nach", "den", "Trau\u00b7e\u00b7rfah\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was nach den Wapen vieler Ahnen/", "tokens": ["was", "nach", "den", "Wa\u00b7pen", "vie\u00b7ler", "Ah\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und ob mich denn ein Marmor ziert:", "tokens": ["und", "ob", "mich", "denn", "ein", "Mar\u00b7mor", "ziert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn einsten zu den blassen Schaaren", "tokens": ["Wenn", "eins\u00b7ten", "zu", "den", "blas\u00b7sen", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "mein Geist ist \u00fcbern Flu\u00df gefahren", "tokens": ["mein", "Geist", "ist", "\u00fc\u00b7bern", "Flu\u00df", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wor uns der Ehre Sucht nicht r\u00fchrt.", "tokens": ["wor", "uns", "der", "Eh\u00b7re", "Sucht", "nicht", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es mag mich wer da will beklagen/", "tokens": ["Es", "mag", "mich", "wer", "da", "will", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PWS", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mag sauer sehn/ und Leide tragen;", "tokens": ["mag", "sau\u00b7er", "sehn", "/", "und", "Lei\u00b7de", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$(", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "ich achte nicht de\u00df P\u00f6fels Spiel-", "tokens": ["ich", "ach\u00b7te", "nicht", "de\u00df", "P\u00f6\u00b7fels", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hin Filidor/ nur hingestorben/", "tokens": ["Hin", "Fi\u00b7li\u00b7dor", "/", "nur", "hin\u00b7ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "bleibt nur dein Nachruhm unverdorben", "tokens": ["bleibt", "nur", "dein", "Nach\u00b7ruhm", "un\u00b7ver\u00b7dor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "bey Rosilis/ der Reime Ziel.", "tokens": ["bey", "Ro\u00b7si\u00b7lis", "/", "der", "Rei\u00b7me", "Ziel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df/ es werden deine Zeilen", "tokens": ["Ich", "wei\u00df", "/", "es", "wer\u00b7den", "dei\u00b7ne", "Zei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird dich lesen Tag und Nacht/", "tokens": ["Sie", "wird", "dich", "le\u00b7sen", "Tag", "und", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "auff unser beyder Brunst erdacht.", "tokens": ["auff", "un\u00b7ser", "bey\u00b7der", "Brunst", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}