{"dta.poem.21425": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Stuhl ist zu Aachen, auf welchem er", "tokens": ["\u201e", "der", "Stuhl", "ist", "zu", "Aa\u00b7chen", ",", "auf", "wel\u00b7chem", "er"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "APPR", "NE", "$,", "APPR", "PRELS", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am Tage der Kr\u00f6nung ruhte;", "tokens": ["Am", "Ta\u00b7ge", "der", "Kr\u00f6\u00b7nung", "ruh\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Stuhl worauf er sa\u00df in der Nacht,", "tokens": ["Den", "Stuhl", "wo\u00b7rauf", "er", "sa\u00df", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den erbte die Mutter, die gute.", "tokens": ["Den", "erb\u00b7te", "die", "Mut\u00b7ter", ",", "die", "gu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u201edie Mutter hinterlie\u00df ihn mir,", "tokens": ["\u201e", "die", "Mut\u00b7ter", "hin\u00b7ter\u00b7lie\u00df", "ihn", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein M\u00f6bel von scheinlosem Aeu\u00dfern,", "tokens": ["Ein", "M\u00f6\u00b7bel", "von", "schein\u00b7lo\u00b7sem", "A\u00b7e\u00b7u\u00b7\u00dfern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch b\u00f6te mir Rothschild all\u2019 sein Geld,", "tokens": ["Doch", "b\u00f6\u00b7te", "mir", "Roth\u00b7schild", "all'", "sein", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich w\u00fcrde ihn nicht ver\u00e4u\u00dfern.", "tokens": ["Ich", "w\u00fcr\u00b7de", "ihn", "nicht", "ver\u00b7\u00e4u\u00b7\u00dfern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201esiehst du, dort in dem Winkel steht", "tokens": ["\u201e", "siehst", "du", ",", "dort", "in", "dem", "Win\u00b7kel", "steht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ein alter Sessel, zerrissen", "tokens": ["Ein", "al\u00b7ter", "Ses\u00b7sel", ",", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Leder der Lehne, von Mottenfra\u00df", "tokens": ["Das", "Le\u00b7der", "der", "Leh\u00b7ne", ",", "von", "Mot\u00b7ten\u00b7fra\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zernagt das Polsterkissen.", "tokens": ["Zer\u00b7nagt", "das", "Pols\u00b7ter\u00b7kis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201edoch gehe hin und hebe auf", "tokens": ["\u201e", "doch", "ge\u00b7he", "hin", "und", "he\u00b7be", "auf"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Das Kissen von dem Sessel,", "tokens": ["Das", "Kis\u00b7sen", "von", "dem", "Ses\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du schaust eine runde Oeffnung dann,", "tokens": ["Du", "schaust", "ei\u00b7ne", "run\u00b7de", "Oeff\u00b7nung", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Darunter einen Kessel \u2013", "tokens": ["Da\u00b7run\u00b7ter", "ei\u00b7nen", "Kes\u00b7sel", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}