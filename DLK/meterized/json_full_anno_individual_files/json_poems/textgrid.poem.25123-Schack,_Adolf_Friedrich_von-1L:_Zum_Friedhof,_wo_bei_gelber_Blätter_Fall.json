{"textgrid.poem.25123": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Friedhof, wo bei gelber Bl\u00e4tter Fall", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Friedhof, wo bei gelber Bl\u00e4tter Fall", "tokens": ["Zum", "Fried\u00b7hof", ",", "wo", "bei", "gel\u00b7ber", "Bl\u00e4t\u00b7ter", "Fall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Matt im Novemberlicht die Kreuze gl\u00e4nzen,", "tokens": ["Matt", "im", "No\u00b7vem\u00b7ber\u00b7licht", "die", "Kreu\u00b7ze", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nun str\u00f6mt das Volk, bei Trauerglockenschall", "tokens": ["Nun", "str\u00f6mt", "das", "Volk", ",", "bei", "Trau\u00b7er\u00b7glo\u00b7cken\u00b7schall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geliebte Gr\u00e4ber zu bekr\u00e4nzen.", "tokens": ["Ge\u00b7lieb\u00b7te", "Gr\u00e4\u00b7ber", "zu", "be\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "War je der Jahre, die gewesen sind,", "tokens": ["War", "je", "der", "Jah\u00b7re", ",", "die", "ge\u00b7we\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00f6rdrisch eins wie dies? Mehr Hoffen", "tokens": ["So", "m\u00f6rd\u00b7risch", "eins", "wie", "dies", "?", "Mehr", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PIS", "KOKOM", "PDS", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat es, als Bl\u00e4tter der Novemberwind,", "tokens": ["Hat", "es", ",", "als", "Bl\u00e4t\u00b7ter", "der", "No\u00b7vem\u00b7ber\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit gift'gem Todespfeil getroffen.", "tokens": ["Mit", "gift'\u00b7gem", "To\u00b7des\u00b7pfeil", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie viele schleppten matt und todeswund", "tokens": ["Wie", "vie\u00b7le", "schlepp\u00b7ten", "matt", "und", "to\u00b7des\u00b7wund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Frankreichs blutged\u00fcngten St\u00e4tten", "tokens": ["Von", "Fran\u00b7kreichs", "blut\u00b7ge\u00b7d\u00fcng\u00b7ten", "St\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Glieder heim, nur um auf deutschem Grund", "tokens": ["Die", "Glie\u00b7der", "heim", ",", "nur", "um", "auf", "deut\u00b7schem", "Grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur letzten Ruhe sich zu betten!", "tokens": ["Zur", "letz\u00b7ten", "Ru\u00b7he", "sich", "zu", "bet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und neidenswert noch ihr, die in den Scho\u00df", "tokens": ["Und", "nei\u00b7dens\u00b7wert", "noch", "ihr", ",", "die", "in", "den", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Heimat ihr gesenkt die Euern!", "tokens": ["Der", "Hei\u00b7mat", "ihr", "ge\u00b7senkt", "die", "Eu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie manche Mutter sehnt sich schlummerlos", "tokens": ["Wie", "man\u00b7che", "Mut\u00b7ter", "sehnt", "sich", "schlum\u00b7mer\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur nach der Asche ihrer Teuern!", "tokens": ["Nur", "nach", "der", "A\u00b7sche", "ih\u00b7rer", "Teu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Abend kommt; im Kreise um sie her", "tokens": ["Der", "A\u00b7bend", "kommt", ";", "im", "Krei\u00b7se", "um", "sie", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPRART", "NN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versammelt hat sie ihre Lieben;", "tokens": ["Ver\u00b7sam\u00b7melt", "hat", "sie", "ih\u00b7re", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch stumm blickt sie, das Auge thr\u00e4nenschwer,", "tokens": ["Doch", "stumm", "blickt", "sie", ",", "das", "Au\u00b7ge", "thr\u00e4\u00b7nen\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf einen Platz, der leer geblieben.", "tokens": ["Auf", "ei\u00b7nen", "Platz", ",", "der", "leer", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie hofft umsonst je von des Sohnes Hand", "tokens": ["Sie", "hofft", "um\u00b7sonst", "je", "von", "des", "Soh\u00b7nes", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch werde regen sich die Klinke,", "tokens": ["Noch", "wer\u00b7de", "re\u00b7gen", "sich", "die", "Klin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergebens, da\u00df, gekehrt ins Vaterland,", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "da\u00df", ",", "ge\u00b7kehrt", "ins", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er an das Mutterherz ihr sinke.", "tokens": ["Er", "an", "das", "Mut\u00b7ter\u00b7herz", "ihr", "sin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf ferner Heide streiten nun vielleicht", "tokens": ["Auf", "fer\u00b7ner", "Hei\u00b7de", "strei\u00b7ten", "nun", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NE", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um seine Leiche sich die Raben;", "tokens": ["Um", "sei\u00b7ne", "Lei\u00b7che", "sich", "die", "Ra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wind, der kalt durch die Vogesen streicht,", "tokens": ["Der", "Wind", ",", "der", "kalt", "durch", "die", "Vo\u00b7ge\u00b7sen", "streicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat sie vielleicht in Schnee begraben.", "tokens": ["Hat", "sie", "viel\u00b7leicht", "in", "Schnee", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch nein, nicht so! Verstumme, Grabgel\u00e4ut,", "tokens": ["Doch", "nein", ",", "nicht", "so", "!", "Ver\u00b7stum\u00b7me", ",", "Grab\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PTKNEG", "ADV", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hemmt, ihr M\u00fctter, Br\u00fcder, Schwestern,", "tokens": ["Und", "hemmt", ",", "ihr", "M\u00fct\u00b7ter", ",", "Br\u00fc\u00b7der", ",", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Klagelaut! Verga\u00df das kleine Heut", "tokens": ["Den", "Kla\u00b7ge\u00b7laut", "!", "Ver\u00b7ga\u00df", "das", "klei\u00b7ne", "Heut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So schnell schon das gewalt'ge Gestern?", "tokens": ["So", "schnell", "schon", "das", "ge\u00b7walt'\u00b7ge", "Ge\u00b7stern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Denkt, wie, als w\u00e4r' ein Himmel aufgethan,", "tokens": ["Denkt", ",", "wie", ",", "als", "w\u00e4r'", "ein", "Him\u00b7mel", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "$,", "KOKOM", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4chelnd zum Vollglanz unsrer Siege,", "tokens": ["L\u00e4\u00b7chelnd", "zum", "Voll\u00b7glanz", "uns\u00b7rer", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Empor vom Sterbebett die Greise sahn,", "tokens": ["Em\u00b7por", "vom", "Ster\u00b7be\u00b7bett", "die", "Grei\u00b7se", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die S\u00e4uglinge aus ihrer Wiege!", "tokens": ["Die", "S\u00e4ug\u00b7lin\u00b7ge", "aus", "ih\u00b7rer", "Wie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Da warfen stolz, dem Heldentod geweiht,", "tokens": ["Da", "war\u00b7fen", "stolz", ",", "dem", "Hel\u00b7den\u00b7tod", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich jener heil'gen Schar von Theben,", "tokens": ["Gleich", "je\u00b7ner", "heil'\u00b7gen", "Schar", "von", "The\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Euren hin ihr niedres Staubeskleid,", "tokens": ["Die", "Eu\u00b7ren", "hin", "ihr", "nie\u00b7dres", "Stau\u00b7bes\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um in Unsterblichkeit zu leben;", "tokens": ["Um", "in", "U\u00b7nsterb\u00b7lich\u00b7keit", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Gl\u00fcckselig sie, die, w\u00e4hrend sie der Sieg", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "sie", ",", "die", ",", "w\u00e4h\u00b7rend", "sie", "der", "Sieg"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "$,", "PRELS", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Umrauschte aus des Banners Falten,", "tokens": ["Um\u00b7rauschte", "aus", "des", "Ban\u00b7ners", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der deutschen Sonne, welche glorreich stieg,", "tokens": ["Der", "deut\u00b7schen", "Son\u00b7ne", ",", "wel\u00b7che", "glor\u00b7reich", "stieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins Antlitz schau'nd, nach jenseits wallten.", "tokens": ["Ins", "Ant\u00b7litz", "schau'nd", ",", "nach", "jen\u00b7seits", "wall\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Betr\u00fcgt sie denn durch Klagen, wie zum Hohn,", "tokens": ["Be\u00b7tr\u00fcgt", "sie", "denn", "durch", "Kla\u00b7gen", ",", "wie", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht um den Ruhm, ihr teures Erbe;", "tokens": ["Nicht", "um", "den", "Ruhm", ",", "ihr", "teu\u00b7res", "Er\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, bleibt euch nach den andern noch ein Sohn,", "tokens": ["Nein", ",", "bleibt", "euch", "nach", "den", "an\u00b7dern", "noch", "ein", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lehrt ihn, da\u00df wie sie er sterbe!", "tokens": ["So", "lehrt", "ihn", ",", "da\u00df", "wie", "sie", "er", "ster\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hinweg mit Seufzern und dem wei\u00dfen Kranz,", "tokens": ["Hin\u00b7weg", "mit", "Seuf\u00b7zern", "und", "dem", "wei\u00b7\u00dfen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Threnodien und Trauerschleiern!", "tokens": ["Mit", "Thre\u00b7no\u00b7di\u00b7en", "und", "Trau\u00b7er\u00b7schlei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In jedem Auge Freudenthr\u00e4nenglanz,", "tokens": ["In", "je\u00b7dem", "Au\u00b7ge", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll Deutschland seine Toten feiern!", "tokens": ["Soll", "Deutschland", "sei\u00b7ne", "To\u00b7ten", "fei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Zum Friedhof, wo bei gelber Bl\u00e4tter Fall", "tokens": ["Zum", "Fried\u00b7hof", ",", "wo", "bei", "gel\u00b7ber", "Bl\u00e4t\u00b7ter", "Fall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Matt im Novemberlicht die Kreuze gl\u00e4nzen,", "tokens": ["Matt", "im", "No\u00b7vem\u00b7ber\u00b7licht", "die", "Kreu\u00b7ze", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nun str\u00f6mt das Volk, bei Trauerglockenschall", "tokens": ["Nun", "str\u00f6mt", "das", "Volk", ",", "bei", "Trau\u00b7er\u00b7glo\u00b7cken\u00b7schall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geliebte Gr\u00e4ber zu bekr\u00e4nzen.", "tokens": ["Ge\u00b7lieb\u00b7te", "Gr\u00e4\u00b7ber", "zu", "be\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "War je der Jahre, die gewesen sind,", "tokens": ["War", "je", "der", "Jah\u00b7re", ",", "die", "ge\u00b7we\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00f6rdrisch eins wie dies? Mehr Hoffen", "tokens": ["So", "m\u00f6rd\u00b7risch", "eins", "wie", "dies", "?", "Mehr", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PIS", "KOKOM", "PDS", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat es, als Bl\u00e4tter der Novemberwind,", "tokens": ["Hat", "es", ",", "als", "Bl\u00e4t\u00b7ter", "der", "No\u00b7vem\u00b7ber\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit gift'gem Todespfeil getroffen.", "tokens": ["Mit", "gift'\u00b7gem", "To\u00b7des\u00b7pfeil", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Wie viele schleppten matt und todeswund", "tokens": ["Wie", "vie\u00b7le", "schlepp\u00b7ten", "matt", "und", "to\u00b7des\u00b7wund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Frankreichs blutged\u00fcngten St\u00e4tten", "tokens": ["Von", "Fran\u00b7kreichs", "blut\u00b7ge\u00b7d\u00fcng\u00b7ten", "St\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Glieder heim, nur um auf deutschem Grund", "tokens": ["Die", "Glie\u00b7der", "heim", ",", "nur", "um", "auf", "deut\u00b7schem", "Grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur letzten Ruhe sich zu betten!", "tokens": ["Zur", "letz\u00b7ten", "Ru\u00b7he", "sich", "zu", "bet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und neidenswert noch ihr, die in den Scho\u00df", "tokens": ["Und", "nei\u00b7dens\u00b7wert", "noch", "ihr", ",", "die", "in", "den", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Heimat ihr gesenkt die Euern!", "tokens": ["Der", "Hei\u00b7mat", "ihr", "ge\u00b7senkt", "die", "Eu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie manche Mutter sehnt sich schlummerlos", "tokens": ["Wie", "man\u00b7che", "Mut\u00b7ter", "sehnt", "sich", "schlum\u00b7mer\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur nach der Asche ihrer Teuern!", "tokens": ["Nur", "nach", "der", "A\u00b7sche", "ih\u00b7rer", "Teu\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Der Abend kommt; im Kreise um sie her", "tokens": ["Der", "A\u00b7bend", "kommt", ";", "im", "Krei\u00b7se", "um", "sie", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPRART", "NN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versammelt hat sie ihre Lieben;", "tokens": ["Ver\u00b7sam\u00b7melt", "hat", "sie", "ih\u00b7re", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch stumm blickt sie, das Auge thr\u00e4nenschwer,", "tokens": ["Doch", "stumm", "blickt", "sie", ",", "das", "Au\u00b7ge", "thr\u00e4\u00b7nen\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf einen Platz, der leer geblieben.", "tokens": ["Auf", "ei\u00b7nen", "Platz", ",", "der", "leer", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Sie hofft umsonst je von des Sohnes Hand", "tokens": ["Sie", "hofft", "um\u00b7sonst", "je", "von", "des", "Soh\u00b7nes", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch werde regen sich die Klinke,", "tokens": ["Noch", "wer\u00b7de", "re\u00b7gen", "sich", "die", "Klin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergebens, da\u00df, gekehrt ins Vaterland,", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "da\u00df", ",", "ge\u00b7kehrt", "ins", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er an das Mutterherz ihr sinke.", "tokens": ["Er", "an", "das", "Mut\u00b7ter\u00b7herz", "ihr", "sin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Auf ferner Heide streiten nun vielleicht", "tokens": ["Auf", "fer\u00b7ner", "Hei\u00b7de", "strei\u00b7ten", "nun", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NE", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um seine Leiche sich die Raben;", "tokens": ["Um", "sei\u00b7ne", "Lei\u00b7che", "sich", "die", "Ra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wind, der kalt durch die Vogesen streicht,", "tokens": ["Der", "Wind", ",", "der", "kalt", "durch", "die", "Vo\u00b7ge\u00b7sen", "streicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat sie vielleicht in Schnee begraben.", "tokens": ["Hat", "sie", "viel\u00b7leicht", "in", "Schnee", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Doch nein, nicht so! Verstumme, Grabgel\u00e4ut,", "tokens": ["Doch", "nein", ",", "nicht", "so", "!", "Ver\u00b7stum\u00b7me", ",", "Grab\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PTKNEG", "ADV", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hemmt, ihr M\u00fctter, Br\u00fcder, Schwestern,", "tokens": ["Und", "hemmt", ",", "ihr", "M\u00fct\u00b7ter", ",", "Br\u00fc\u00b7der", ",", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Klagelaut! Verga\u00df das kleine Heut", "tokens": ["Den", "Kla\u00b7ge\u00b7laut", "!", "Ver\u00b7ga\u00df", "das", "klei\u00b7ne", "Heut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So schnell schon das gewalt'ge Gestern?", "tokens": ["So", "schnell", "schon", "das", "ge\u00b7walt'\u00b7ge", "Ge\u00b7stern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.22": {"line.1": {"text": "Denkt, wie, als w\u00e4r' ein Himmel aufgethan,", "tokens": ["Denkt", ",", "wie", ",", "als", "w\u00e4r'", "ein", "Him\u00b7mel", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "$,", "KOKOM", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4chelnd zum Vollglanz unsrer Siege,", "tokens": ["L\u00e4\u00b7chelnd", "zum", "Voll\u00b7glanz", "uns\u00b7rer", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Empor vom Sterbebett die Greise sahn,", "tokens": ["Em\u00b7por", "vom", "Ster\u00b7be\u00b7bett", "die", "Grei\u00b7se", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die S\u00e4uglinge aus ihrer Wiege!", "tokens": ["Die", "S\u00e4ug\u00b7lin\u00b7ge", "aus", "ih\u00b7rer", "Wie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.23": {"line.1": {"text": "Da warfen stolz, dem Heldentod geweiht,", "tokens": ["Da", "war\u00b7fen", "stolz", ",", "dem", "Hel\u00b7den\u00b7tod", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich jener heil'gen Schar von Theben,", "tokens": ["Gleich", "je\u00b7ner", "heil'\u00b7gen", "Schar", "von", "The\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Euren hin ihr niedres Staubeskleid,", "tokens": ["Die", "Eu\u00b7ren", "hin", "ihr", "nie\u00b7dres", "Stau\u00b7bes\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um in Unsterblichkeit zu leben;", "tokens": ["Um", "in", "U\u00b7nsterb\u00b7lich\u00b7keit", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Gl\u00fcckselig sie, die, w\u00e4hrend sie der Sieg", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "sie", ",", "die", ",", "w\u00e4h\u00b7rend", "sie", "der", "Sieg"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "$,", "PRELS", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Umrauschte aus des Banners Falten,", "tokens": ["Um\u00b7rauschte", "aus", "des", "Ban\u00b7ners", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der deutschen Sonne, welche glorreich stieg,", "tokens": ["Der", "deut\u00b7schen", "Son\u00b7ne", ",", "wel\u00b7che", "glor\u00b7reich", "stieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ins Antlitz schau'nd, nach jenseits wallten.", "tokens": ["Ins", "Ant\u00b7litz", "schau'nd", ",", "nach", "jen\u00b7seits", "wall\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Betr\u00fcgt sie denn durch Klagen, wie zum Hohn,", "tokens": ["Be\u00b7tr\u00fcgt", "sie", "denn", "durch", "Kla\u00b7gen", ",", "wie", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht um den Ruhm, ihr teures Erbe;", "tokens": ["Nicht", "um", "den", "Ruhm", ",", "ihr", "teu\u00b7res", "Er\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, bleibt euch nach den andern noch ein Sohn,", "tokens": ["Nein", ",", "bleibt", "euch", "nach", "den", "an\u00b7dern", "noch", "ein", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lehrt ihn, da\u00df wie sie er sterbe!", "tokens": ["So", "lehrt", "ihn", ",", "da\u00df", "wie", "sie", "er", "ster\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Hinweg mit Seufzern und dem wei\u00dfen Kranz,", "tokens": ["Hin\u00b7weg", "mit", "Seuf\u00b7zern", "und", "dem", "wei\u00b7\u00dfen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Threnodien und Trauerschleiern!", "tokens": ["Mit", "Thre\u00b7no\u00b7di\u00b7en", "und", "Trau\u00b7er\u00b7schlei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In jedem Auge Freudenthr\u00e4nenglanz,", "tokens": ["In", "je\u00b7dem", "Au\u00b7ge", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll Deutschland seine Toten feiern!", "tokens": ["Soll", "Deutschland", "sei\u00b7ne", "To\u00b7ten", "fei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}