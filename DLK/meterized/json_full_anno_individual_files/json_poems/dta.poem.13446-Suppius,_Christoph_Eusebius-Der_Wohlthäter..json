{"dta.poem.13446": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Wohlth\u00e4ter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe sie im pr\u00e4chtigen Geleite,", "tokens": ["Ich", "sa\u00b7he", "sie", "im", "pr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das F\u00fcllhorn trug sie an der linken Seite,", "tokens": ["Das", "F\u00fcll\u00b7horn", "trug", "sie", "an", "der", "lin\u00b7ken", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie huldreich! denn sie glich an zartem Triebe", "tokens": ["Wie", "huld\u00b7reich", "!", "denn", "sie", "glich", "an", "zar\u00b7tem", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von denen Sternen stieg die G\u00f6ttin nieder;", "tokens": ["Von", "de\u00b7nen", "Ster\u00b7nen", "stieg", "die", "G\u00f6t\u00b7tin", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jhr, dieser Menschenfreundin! sing ich Lieder,", "tokens": ["Ihr", ",", "die\u00b7ser", "Men\u00b7schen\u00b7freun\u00b7din", "!", "sing", "ich", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDAT", "NN", "$.", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der man bereits dort in erlauchten Ch\u00f6ren", "tokens": ["Der", "man", "be\u00b7reits", "dort", "in", "er\u00b7lauch\u00b7ten", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Mildigkeit! die G \u2012 \u2012 zu mir leitet,", "tokens": ["O", "Mil\u00b7dig\u00b7keit", "!", "die", "G", "\u2012", "\u2012", "zu", "mir", "lei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NE", "$(", "$(", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die in mein Thal zum erstenmahle schreitet,", "tokens": ["Die", "in", "mein", "Thal", "zum", "ers\u00b7ten\u00b7mah\u00b7le", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Boreas zugleich im Arm genommen,", "tokens": ["Von", "Bo\u00b7re\u00b7as", "zu\u00b7gleich", "im", "Arm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Warum so selten sichtbar auf der Erden?", "tokens": ["Wa\u00b7rum", "so", "sel\u00b7ten", "sicht\u00b7bar", "auf", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kannst du denn nicht allhier einheimisch werden?", "tokens": ["Kannst", "du", "denn", "nicht", "all\u00b7hier", "ein\u00b7hei\u00b7misch", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In deiner Macht beruht der Niedern Gl\u00fccke,", "tokens": ["In", "dei\u00b7ner", "Macht", "be\u00b7ruht", "der", "Nie\u00b7dern", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zufriedenheit sieht nicht nach vollen H\u00e4nden,", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "sieht", "nicht", "nach", "vol\u00b7len", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob du dich n\u00e4herst, G\u00fcther auszuspenden,", "tokens": ["Ob", "du", "dich", "n\u00e4\u00b7herst", ",", "G\u00fc\u00b7ther", "aus\u00b7zu\u00b7spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Eigennutz allein in ihrer H\u00fclle", "tokens": ["Der", "Ei\u00b7gen\u00b7nutz", "al\u00b7lein", "in", "ih\u00b7rer", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zwar du hast Feinde, welche dich verbannen,", "tokens": ["Zwar", "du", "hast", "Fein\u00b7de", ",", "wel\u00b7che", "dich", "ver\u00b7ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erbost jagt dich der d\u00fcrre Geiz von dannen,", "tokens": ["Er\u00b7bost", "jagt", "dich", "der", "d\u00fcr\u00b7re", "Geiz", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der nichts empfangen soll; schon will dein Scheiden", "tokens": ["Der", "nichts", "emp\u00b7fan\u00b7gen", "soll", ";", "schon", "will", "dein", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$.", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du fliehst! doch W \u2012 \u2012 ruft, er wird erh\u00f6ret,", "tokens": ["Du", "fliehst", "!", "doch", "W", "\u2012", "\u2012", "ruft", ",", "er", "wird", "er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "NE", "$(", "$(", "VVFIN", "$,", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In seinem Herzen bist du eingekehret,", "tokens": ["In", "sei\u00b7nem", "Her\u00b7zen", "bist", "du", "ein\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das gro\u00df und edel ist, und auserlesen", "tokens": ["Das", "gro\u00df", "und", "e\u00b7del", "ist", ",", "und", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mit liebreichsegnenden und holden Blicken", "tokens": ["Mit", "lieb\u00b7reich\u00b7seg\u00b7nen\u00b7den", "und", "hol\u00b7den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df mich dein Reiz aus selbigen entz\u00fccken,", "tokens": ["Mu\u00df", "mich", "dein", "Reiz", "aus", "sel\u00b7bi\u00b7gen", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Kummer sieht dorther sich \u00fcbersch\u00fcttet,", "tokens": ["Mein", "Kum\u00b7mer", "sieht", "dor\u00b7ther", "sich", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Eh er sich f\u00fchlt, f\u00e4llt schon dein Abendregen,", "tokens": ["Eh", "er", "sich", "f\u00fchlt", ",", "f\u00e4llt", "schon", "dein", "A\u00b7bend\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und der mich nie erblickt, eilt mir entgegen,", "tokens": ["Und", "der", "mich", "nie", "er\u00b7blickt", ",", "eilt", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "VVPP", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er auf mein empfindlich Herze s\u00e4et,", "tokens": ["Da\u00df", "er", "auf", "mein", "emp\u00b7find\u00b7lich", "Her\u00b7ze", "s\u00e4et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Der Kranke, welcher sich auf Elend bettet,", "tokens": ["Der", "Kran\u00b7ke", ",", "wel\u00b7cher", "sich", "auf", "E\u00b7lend", "bet\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht sich von seiner Liebeshand errettet.", "tokens": ["Sieht", "sich", "von", "sei\u00b7ner", "Lie\u00b7bes\u00b7hand", "er\u00b7ret\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast ihn, der nach deinen Trieben handelt,", "tokens": ["Du", "hast", "ihn", ",", "der", "nach", "dei\u00b7nen", "Trie\u00b7ben", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "O! la\u00df ihn auch die wahre Wohllust schmecken,", "tokens": ["O", "!", "la\u00df", "ihn", "auch", "die", "wah\u00b7re", "Wohl\u00b7lust", "schme\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die solch ein heimlich Wohlthun kann erwecken,", "tokens": ["Die", "solch", "ein", "heim\u00b7lich", "Wohl\u00b7thun", "kann", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ART", "ADJD", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und welche Seelen, die am Staube kleben,", "tokens": ["Und", "wel\u00b7che", "See\u00b7len", ",", "die", "am", "Stau\u00b7be", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ach! w\u00e4re nicht die Tugend so erhaben,", "tokens": ["Ach", "!", "w\u00e4\u00b7re", "nicht", "die", "Tu\u00b7gend", "so", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PTKNEG", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch ein Verzeichni\u00df unger\u00fchmter Gaben", "tokens": ["Welch", "ein", "Ver\u00b7zeich\u00b7ni\u00df", "un\u00b7ge\u00b7r\u00fchm\u00b7ter", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erblickte sie auf manchen Brand-Alt\u00e4ren,", "tokens": ["Er\u00b7blick\u00b7te", "sie", "auf", "man\u00b7chen", "Bran\u00b7dAl\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Die Dankbarkeit w\u00fcrd\u2019 ihr Gel\u00fcbde zahlen,", "tokens": ["Die", "Dank\u00b7bar\u00b7keit", "w\u00fcrd'", "ihr", "Ge\u00b7l\u00fcb\u00b7de", "zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jhn w\u00fcrde sie der Nachwelt kenntlich malen,", "tokens": ["Jhn", "w\u00fcr\u00b7de", "sie", "der", "Nach\u00b7welt", "kennt\u00b7lich", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den allbereits der Erdkreis nicht verkennet,", "tokens": ["Den", "all\u00b7be\u00b7reits", "der", "Erd\u00b7kreis", "nicht", "ver\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "O G\u00f6ttin! la\u00df ihn mich in dir besingen,", "tokens": ["O", "G\u00f6t\u00b7tin", "!", "la\u00df", "ihn", "mich", "in", "dir", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVIMP", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und will vor Regung mir kein Lied gelingen,", "tokens": ["Und", "will", "vor", "Re\u00b7gung", "mir", "kein", "Lied", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df seiner w\u00fcrdig ist, erhitzt kein Feuer", "tokens": ["Da\u00df", "sei\u00b7ner", "w\u00fcr\u00b7dig", "ist", ",", "er\u00b7hitzt", "kein", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "VAFIN", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Die nicht so rein gestimmt, wie seine Saiten,", "tokens": ["Die", "nicht", "so", "rein", "ge\u00b7stimmt", ",", "wie", "sei\u00b7ne", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADJD", "VVPP", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df der Zukunft ihm ein Lob bereiten,", "tokens": ["So", "la\u00df", "der", "Zu\u00b7kunft", "ihm", "ein", "Lob", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihn mit dir zugleich, o Preis der Erden!", "tokens": ["Und", "ihn", "mit", "dir", "zu\u00b7gleich", ",", "o", "Preis", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "ADV", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}