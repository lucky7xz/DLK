{"dta.poem.23368": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Serenate  \n  unter den Fenstergittern des Kuhschnappelschen  \n Landtags.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So mancher macht sich breit,", "tokens": ["So", "man\u00b7cher", "macht", "sich", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Will Sprech- und Pre\u00dffreiheit,", "tokens": ["Will", "Sprech", "und", "Pre\u00df\u00b7frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "TRUNC", "KON", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und thut dann auf einmal", "tokens": ["Und", "thut", "dann", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Entsetzlich liberal.", "tokens": ["Ent\u00b7setz\u00b7lich", "li\u00b7be\u00b7ral", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gebt ihm ein B\u00e4ndelein", "tokens": ["Gebt", "ihm", "ein", "B\u00e4n\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und Titel obendrein,", "tokens": ["Und", "Ti\u00b7tel", "o\u00b7ben\u00b7drein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da ist der Kerl gleich stumm", "tokens": ["Da", "ist", "der", "Kerl", "gleich", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und ganz entsetzlich dumm \u2014", "tokens": ["Und", "ganz", "ent\u00b7setz\u00b7lich", "dumm"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Stockfisch! o Stockfisch!", "tokens": ["O", "Stock\u00b7fisch", "!", "o", "Stock\u00b7fisch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "So mancher denkt und spricht:", "tokens": ["So", "man\u00b7cher", "denkt", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir brauchen sie ja nicht", "tokens": ["Wir", "brau\u00b7chen", "sie", "ja", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Constitution,", "tokens": ["Die", "Con\u00b7sti\u00b7tu\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir sind ja gl\u00fccklich schon.", "tokens": ["Wir", "sind", "ja", "gl\u00fcck\u00b7lich", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er denkt an sich allein,", "tokens": ["Er", "denkt", "an", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Uns f\u00e4llt dabei nur ein:", "tokens": ["Uns", "f\u00e4llt", "da\u00b7bei", "nur", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Freund, sieh dich besser um!", "tokens": ["Freund", ",", "sieh", "dich", "bes\u00b7ser", "um", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du bist entsetzlich dumm \u2014", "tokens": ["Du", "bist", "ent\u00b7setz\u00b7lich", "dumm"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Stockfisch! o Stockfisch!", "tokens": ["O", "Stock\u00b7fisch", "!", "o", "Stock\u00b7fisch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}