{"textgrid.poem.66280": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Urquell aller Seligkeiten,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urquell aller Seligkeiten,", "tokens": ["Ur\u00b7quell", "al\u00b7ler", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in Str\u00f6men sich verbreiten", "tokens": ["Die", "in", "Str\u00f6\u00b7men", "sich", "ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der Sch\u00f6pfungen Gebiet;", "tokens": ["Durch", "der", "Sch\u00f6p\u00b7fun\u00b7gen", "Ge\u00b7biet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vater, h\u00f6r' mein flehend Lied!", "tokens": ["Va\u00b7ter", ",", "h\u00f6r'", "mein", "fle\u00b7hend", "Lied", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht um G\u00fcter dieser Erde,", "tokens": ["Nicht", "um", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des erhabnern Geists Beschwerde,", "tokens": ["Des", "er\u00b7hab\u00b7nern", "Geists", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht um Goldstaub, der verweht;", "tokens": ["Nicht", "um", "Gold\u00b7staub", ",", "der", "ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht um Ehre, die vergeht;", "tokens": ["Nicht", "um", "Eh\u00b7re", ",", "die", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht um Blasen, Kinderpuppen,", "tokens": ["Nicht", "um", "Bla\u00b7sen", ",", "Kin\u00b7der\u00b7pup\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Schlang' mit goldnen Schuppen,", "tokens": ["Um", "die", "Schlang'", "mit", "gold\u00b7nen", "Schup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Weltlust komm' ich nicht,", "tokens": ["Um", "die", "Welt\u00b7lust", "komm'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater, vor dein Angesicht.", "tokens": ["Va\u00b7ter", ",", "vor", "dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht um frische Lebensfluthen,", "tokens": ["Nicht", "um", "fri\u00b7sche", "Le\u00b7bens\u00b7flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie in Thr\u00e4nen wegzubluten;", "tokens": ["Sie", "in", "Thr\u00e4\u00b7nen", "weg\u00b7zu\u00b7blu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht um l\u00e4ngres Leben fleht", "tokens": ["Nicht", "um", "l\u00e4ng\u00b7res", "Le\u00b7ben", "fleht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich mein klagendes Gebet.", "tokens": ["Dich", "mein", "kla\u00b7gen\u00b7des", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Nicht nach Freiheit will ich schreien.", "tokens": ["Nicht", "nach", "Frei\u00b7heit", "will", "ich", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel w\u00fcrden mir's verzeihen,", "tokens": ["En\u00b7gel", "w\u00fcr\u00b7den", "mir's", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich mit gebognem Knie", "tokens": ["Wenn", "ich", "mit", "ge\u00b7bog\u00b7nem", "Knie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer Freiheit! Freiheit! schrie.", "tokens": ["Im\u00b7mer", "Frei\u00b7heit", "!", "Frei\u00b7heit", "!", "schrie", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$.", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weg mit G\u00fctern dieser Erde!", "tokens": ["Weg", "mit", "G\u00fc\u00b7tern", "die\u00b7ser", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00fcter von dem h\u00f6chsten Werthe,", "tokens": ["G\u00fc\u00b7ter", "von", "dem", "h\u00f6chs\u00b7ten", "Wert\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Himmel selber preist,", "tokens": ["Die", "der", "Him\u00b7mel", "sel\u00b7ber", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche G\u00fcter sucht mein Geist.", "tokens": ["Sol\u00b7che", "G\u00fc\u00b7ter", "sucht", "mein", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sch\u00e4tze, welche nie verst\u00e4uben,", "tokens": ["Sch\u00e4t\u00b7ze", ",", "wel\u00b7che", "nie", "ver\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tugenden, die ewig bleiben,", "tokens": ["Tu\u00b7gen\u00b7den", ",", "die", "e\u00b7wig", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Thaten, eines Christen werth,", "tokens": ["Tha\u00b7ten", ",", "ei\u00b7nes", "Chris\u00b7ten", "werth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind es, die mein Herz begehrt.", "tokens": ["Sind", "es", ",", "die", "mein", "Herz", "be\u00b7gehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Geber aller guten Gaben!", "tokens": ["Ge\u00b7ber", "al\u00b7ler", "gu\u00b7ten", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Festen Glauben m\u00f6cht' ich haben;", "tokens": ["Fes\u00b7ten", "Glau\u00b7ben", "m\u00f6cht'", "ich", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Meerfels unbewegt,", "tokens": ["Wie", "ein", "Meer\u00b7fels", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn an ihn die Woge schl\u00e4gt;", "tokens": ["Wenn", "an", "ihn", "die", "Wo\u00b7ge", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.9": {"line.1": {"text": "Lieb', aus deinem Herzen stammend,", "tokens": ["Lieb'", ",", "aus", "dei\u00b7nem", "Her\u00b7zen", "stam\u00b7mend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer rein und immer flammend,", "tokens": ["Im\u00b7mer", "rein", "und", "im\u00b7mer", "flam\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe, die dem Feind verzeiht", "tokens": ["Lie\u00b7be", ",", "die", "dem", "Feind", "ver\u00b7zeiht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem Freund das Leben weiht;", "tokens": ["Und", "dem", "Freund", "das", "Le\u00b7ben", "weiht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hoffnung, die mit hohem Haupte,", "tokens": ["Hoff\u00b7nung", ",", "die", "mit", "ho\u00b7hem", "Haup\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Welt ihr Alles raubte,", "tokens": ["Wenn", "die", "Welt", "ihr", "Al\u00b7les", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinblickt, wo sie wonnevoll", "tokens": ["Hin\u00b7blickt", ",", "wo", "sie", "won\u00b7ne\u00b7voll"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "PPER", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alles wieder finden soll.", "tokens": ["Al\u00b7les", "wie\u00b7der", "fin\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hohen Muth im Kampf des Christen", "tokens": ["Ho\u00b7hen", "Muth", "im", "Kampf", "des", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit des Erdenstaubes L\u00fcsten;", "tokens": ["Mit", "des", "Er\u00b7dens\u00b7tau\u00b7bes", "L\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieg dem Geist! und wenn er siegt,", "tokens": ["Sieg", "dem", "Geist", "!", "und", "wenn", "er", "siegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Demuth, die im Staub sich schmiegt;", "tokens": ["De\u00b7muth", ",", "die", "im", "Staub", "sich", "schmiegt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Duldung, alle Lebensplagen", "tokens": ["Dul\u00b7dung", ",", "al\u00b7le", "Le\u00b7ben\u00b7spla\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Gelassenheit zu tragen;", "tokens": ["Mit", "Ge\u00b7las\u00b7sen\u00b7heit", "zu", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stilles Harren, bis der Tod", "tokens": ["Stil\u00b7les", "Har\u00b7ren", ",", "bis", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich erl\u00f6st auf dein Gebot;", "tokens": ["Mich", "er\u00b7l\u00f6st", "auf", "dein", "Ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und dann Christenmuth im Sterben.", "tokens": ["Und", "dann", "Chris\u00b7ten\u00b7muth", "im", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann die Lippen sich entf\u00e4rben,", "tokens": ["Wann", "die", "Lip\u00b7pen", "sich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Seufzer noch zu dir:", "tokens": ["Ei\u00b7nen", "Seuf\u00b7zer", "noch", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jesu, nimm den Geist von mir!", "tokens": ["Je\u00b7su", ",", "nimm", "den", "Geist", "von", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Willst du Herr von meinem Leben,", "tokens": ["Willst", "du", "Herr", "von", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Seligkeit mir geben;", "tokens": ["Die\u00b7se", "Se\u00b7lig\u00b7keit", "mir", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So wird meine Kerkernacht", "tokens": ["So", "wird", "mei\u00b7ne", "Ker\u00b7ker\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir zum Paradies gemacht.", "tokens": ["Mir", "zum", "Pa\u00b7ra\u00b7dies", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Immer will ich beten, ringen,", "tokens": ["Im\u00b7mer", "will", "ich", "be\u00b7ten", ",", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Banden, danken, singen;", "tokens": ["In", "den", "Ban\u00b7den", ",", "dan\u00b7ken", ",", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Harren, bis es dir gef\u00e4llt,", "tokens": ["Har\u00b7ren", ",", "bis", "es", "dir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zu holen aus der Welt.", "tokens": ["Mich", "zu", "ho\u00b7len", "aus", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Seele, gib dich nun zufrieden:", "tokens": ["See\u00b7le", ",", "gib", "dich", "nun", "zu\u00b7frie\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesus kennt und st\u00e4rkt dich M\u00fcden;", "tokens": ["Je\u00b7sus", "kennt", "und", "st\u00e4rkt", "dich", "M\u00fc\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur vergi\u00df nie sein Gebot:", "tokens": ["Nur", "ver\u00b7gi\u00df", "nie", "sein", "Ge\u00b7bot", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei getreu bis in den Tod.", "tokens": ["Sei", "ge\u00b7treu", "bis", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Urquell aller Seligkeiten,", "tokens": ["Ur\u00b7quell", "al\u00b7ler", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in Str\u00f6men sich verbreiten", "tokens": ["Die", "in", "Str\u00f6\u00b7men", "sich", "ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der Sch\u00f6pfungen Gebiet;", "tokens": ["Durch", "der", "Sch\u00f6p\u00b7fun\u00b7gen", "Ge\u00b7biet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Vater, h\u00f6r' mein flehend Lied!", "tokens": ["Va\u00b7ter", ",", "h\u00f6r'", "mein", "fle\u00b7hend", "Lied", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Nicht um G\u00fcter dieser Erde,", "tokens": ["Nicht", "um", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des erhabnern Geists Beschwerde,", "tokens": ["Des", "er\u00b7hab\u00b7nern", "Geists", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht um Goldstaub, der verweht;", "tokens": ["Nicht", "um", "Gold\u00b7staub", ",", "der", "ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht um Ehre, die vergeht;", "tokens": ["Nicht", "um", "Eh\u00b7re", ",", "die", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Nicht um Blasen, Kinderpuppen,", "tokens": ["Nicht", "um", "Bla\u00b7sen", ",", "Kin\u00b7der\u00b7pup\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Schlang' mit goldnen Schuppen,", "tokens": ["Um", "die", "Schlang'", "mit", "gold\u00b7nen", "Schup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Weltlust komm' ich nicht,", "tokens": ["Um", "die", "Welt\u00b7lust", "komm'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater, vor dein Angesicht.", "tokens": ["Va\u00b7ter", ",", "vor", "dein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Nicht um frische Lebensfluthen,", "tokens": ["Nicht", "um", "fri\u00b7sche", "Le\u00b7bens\u00b7flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie in Thr\u00e