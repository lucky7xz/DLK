{"dta.poem.7440": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Zahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr im Abends\u00e4useln schon", "tokens": ["Die", "ihr", "im", "A\u00b7bend\u00b7s\u00e4u\u00b7seln", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herren Spur gewahrt,", "tokens": ["Des", "Her\u00b7ren", "Spur", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und denen er im Kr\u00e4useln schon", "tokens": ["Und", "de\u00b7nen", "er", "im", "Kr\u00e4u\u00b7seln", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELS", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der See sich offenbart \u2014", "tokens": ["Der", "See", "sich", "of\u00b7fen\u00b7bart"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O freut euch eurer Loose,", "tokens": ["O", "freut", "euch", "eu\u00b7rer", "Loo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und dankt und la\u00dft mich gehn!", "tokens": ["Und", "dankt", "und", "la\u00dft", "mich", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im wilden Sturmgetose,", "tokens": ["Im", "wil\u00b7den", "Sturm\u00b7ge\u00b7to\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Feuer nur, wie Mose,", "tokens": ["Im", "Feu\u00b7er", "nur", ",", "wie", "Mo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mag ich den Herren sehn!", "tokens": ["Mag", "ich", "den", "Her\u00b7ren", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So Einer gl\u00fccklich, sonn' er sich", "tokens": ["So", "Ei\u00b7ner", "gl\u00fcck\u00b7lich", ",", "sonn'", "er", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "ADJD", "$,", "KON", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Frieden vor dem Haus;", "tokens": ["In", "Frie\u00b7den", "vor", "dem", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Sinai Gebraus.", "tokens": ["Des", "Si\u00b7nai", "Ge\u00b7braus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich f\u00fchl's durch alle Nerven,", "tokens": ["Ich", "f\u00fchl's", "durch", "al\u00b7le", "Ner\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch alle Adern spr\u00fchn:", "tokens": ["Durch", "al\u00b7le", "A\u00b7dern", "spr\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich m\u00f6chte Speere werfen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "Spee\u00b7re", "wer\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich m\u00f6chte Klingen sch\u00e4rfen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "Klin\u00b7gen", "sch\u00e4r\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und thatlos nicht vergl\u00fchn.", "tokens": ["Und", "that\u00b7los", "nicht", "ver\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht mehr an Blumenh\u00fcgeln m\u00f6cht'", "tokens": ["Nicht", "mehr", "an", "Blu\u00b7men\u00b7h\u00fc\u00b7geln", "m\u00f6cht'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich liegen auf der Wacht,", "tokens": ["Ich", "lie\u00b7gen", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In eines Streithengsts B\u00fcgeln m\u00f6cht'", "tokens": ["In", "ei\u00b7nes", "Strei\u00b7thengsts", "B\u00fc\u00b7geln", "m\u00f6cht'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wiegen mich zur Schlacht,", "tokens": ["Ich", "wie\u00b7gen", "mich", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht mehr im Mondschein wandeln,", "tokens": ["Nicht", "mehr", "im", "Mond\u00b7schein", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht l\u00e4nger schreiben mehr,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "schrei\u00b7ben", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich m\u00f6cht' nun einmal sandeln,", "tokens": ["Ich", "m\u00f6cht'", "nun", "ein\u00b7mal", "san\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich m\u00f6cht' nun einmal handeln \u2014", "tokens": ["Ich", "m\u00f6cht'", "nun", "ein\u00b7mal", "han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Auf! bringt mir Fahnen her!", "tokens": ["Auf", "!", "bringt", "mir", "Fah\u00b7nen", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft endlich das Geleier sein", "tokens": ["La\u00dft", "end\u00b7lich", "das", "Ge\u00b7lei\u00b7er", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00fchrt die Trommel nur!", "tokens": ["Und", "r\u00fchrt", "die", "Trom\u00b7mel", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Deutsche mu\u00df erst freier sein,", "tokens": ["Der", "Deut\u00b7sche", "mu\u00df", "erst", "frei\u00b7er", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann sei er Troubadour.", "tokens": ["Dann", "sei", "er", "Trou\u00b7ba\u00b7dour", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Freiheitsfeuertranke", "tokens": ["Im", "Frei\u00b7heits\u00b7feu\u00b7er\u00b7tran\u00b7ke"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Werd' unser Reich erfrischt,", "tokens": ["Werd'", "un\u00b7ser", "Reich", "er\u00b7frischt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr ewiger Gedanke", "tokens": ["Ihr", "e\u00b7wi\u00b7ger", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fchr' unser Schwert, das blanke,", "tokens": ["F\u00fchr'", "un\u00b7ser", "Schwert", ",", "das", "blan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wenn's in die Feinde zischt!", "tokens": ["Wenn's", "in", "die", "Fein\u00b7de", "zischt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}