{"dta.poem.23633": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mi\u00dfverst\u00e4ndniss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201esinge wem Gesang gegeben,\u201c", "tokens": ["\u201e", "sin\u00b7ge", "wem", "Ge\u00b7sang", "ge\u00b7ge\u00b7ben", ",", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach zur Vogelschaar der Aar,", "tokens": ["Sprach", "zur", "Vo\u00b7gel\u00b7schaar", "der", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201edas ist Freude das ist Leben!\u201c \u2014", "tokens": ["\u201e", "das", "ist", "Freu\u00b7de", "das", "ist", "Le\u00b7ben", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "NN", "PDS", "VAFIN", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es sang die Vogelschaar.", "tokens": ["Und", "es", "sang", "die", "Vo\u00b7gel\u00b7schaar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es wurde bunt die Heide,", "tokens": ["Und", "es", "wur\u00b7de", "bunt", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcn der Wald und gr\u00fcn das Feld,", "tokens": ["Gr\u00fcn", "der", "Wald", "und", "gr\u00fcn", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus ihrem Winterleide", "tokens": ["Und", "aus", "ih\u00b7rem", "Win\u00b7ter\u00b7lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trat verj\u00fcngt hervor die Welt.", "tokens": ["Trat", "ver\u00b7j\u00fcngt", "her\u00b7vor", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das war Freude, das war Leben", "tokens": ["Das", "war", "Freu\u00b7de", ",", "das", "war", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "$,", "PDS", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Wald und auf der Flur,", "tokens": ["In", "dem", "Wald", "und", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die S\u00e4nger waren eben", "tokens": ["Denn", "die", "S\u00e4n\u00b7ger", "wa\u00b7ren", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauter gute S\u00e4nger nur.", "tokens": ["Lau\u00b7ter", "gu\u00b7te", "S\u00e4n\u00b7ger", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch es kamen stolze Namen,", "tokens": ["Doch", "es", "ka\u00b7men", "stol\u00b7ze", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiedehopf und K\u00f6niglein,", "tokens": ["Wie\u00b7de\u00b7hopf", "und", "K\u00f6\u00b7nig\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfau, Fasan und Truthahn kamen,", "tokens": ["Pfau", ",", "Fa\u00b7san", "und", "Trut\u00b7hahn", "ka\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mischten ihren Jubel ein.", "tokens": ["Mischten", "ih\u00b7ren", "Ju\u00b7bel", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und es wurde bleich die Heide,", "tokens": ["Und", "es", "wur\u00b7de", "bleich", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Falbe wurde Wald und Feld,", "tokens": ["Fal\u00b7be", "wur\u00b7de", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in ihrem Winterleide", "tokens": ["Und", "in", "ih\u00b7rem", "Win\u00b7ter\u00b7lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lag nun wiederum die Welt.", "tokens": ["Lag", "nun", "wie\u00b7de\u00b7rum", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}