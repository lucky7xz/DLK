{"textgrid.poem.51152": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4. Wenn schlanke Lilien wandelten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn schlanke Lilien wandelten, vom Weste leis geschwungen,", "tokens": ["Wenn", "schlan\u00b7ke", "Li\u00b7li\u00b7en", "wan\u00b7del\u00b7ten", ",", "vom", "Wes\u00b7te", "leis", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "W\u00e4r doch ein Gang, wie deiner ist, nicht gleicherweis gelungen!", "tokens": ["W\u00e4r", "doch", "ein", "Gang", ",", "wie", "dei\u00b7ner", "ist", ",", "nicht", "glei\u00b7cher\u00b7weis", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "PPOSAT", "VAFIN", "$,", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wohin du gehst, da ist nicht Gram, da ebnet sich der Pfad.", "tokens": ["Wo\u00b7hin", "du", "gehst", ",", "da", "ist", "nicht", "Gram", ",", "da", "eb\u00b7net", "sich", "der", "Pfad", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "So dacht ich, als vom Garten her dein Schritt mir leis erklungen.", "tokens": ["So", "dacht", "ich", ",", "als", "vom", "Gar\u00b7