unct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Doch wenn sie einmal nur freundlich gelacht,", "tokens": ["Doch", "wenn", "sie", "ein\u00b7mal", "nur", "freund\u00b7lich", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verschm\u00e4ht um sie, was unsterblich macht,", "tokens": ["Ver\u00b7schm\u00e4ht", "um", "sie", ",", "was", "uns\u00b7terb\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den goldnen G\u00f6tterpokal.", "tokens": ["Den", "gold\u00b7nen", "G\u00f6t\u00b7ter\u00b7po\u00b7kal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Doch tr\u00e4gt der Himmel des Auges Schmerz", "tokens": ["Doch", "tr\u00e4gt", "der", "Him\u00b7mel", "des", "Au\u00b7ges", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Blick,", "tokens": ["Im", "Blick", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Gleich fliehet alles ins tiefe Herz", "tokens": ["Gleich", "flie\u00b7het", "al\u00b7les", "ins", "tie\u00b7fe", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zur\u00fcck.", "t