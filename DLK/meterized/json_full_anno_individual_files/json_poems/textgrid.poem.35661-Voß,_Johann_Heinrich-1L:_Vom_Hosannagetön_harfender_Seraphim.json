{"textgrid.poem.35661": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Hosannaget\u00f6n harfender Seraphim", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Hosannaget\u00f6n harfender Seraphim", "tokens": ["Vom", "Ho\u00b7san\u00b7na\u00b7ge\u00b7t\u00f6n", "har\u00b7fen\u00b7der", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Schlich, ihr freundliches Kind im Arm,", "tokens": ["Schlich", ",", "ihr", "freund\u00b7li\u00b7ches", "Kind", "im", "Arm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Deine Schwester sich oft sinnig zur Palmennacht", "tokens": ["Dei\u00b7ne", "Schwes\u00b7ter", "sich", "oft", "sin\u00b7nig", "zur", "Pal\u00b7men\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "ADJD", "APPRART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Am krystallenen Wasserfall.", "tokens": ["Am", "krys\u00b7tal\u00b7le\u00b7nen", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Denn nur Kunde des Grams brachte der Engel mit,", "tokens": ["Denn", "nur", "Kun\u00b7de", "des", "Grams", "brach\u00b7te", "der", "En\u00b7gel", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "(einst ihr Genius, jetzt ihr Freund,)", "tokens": ["(", "einst", "ihr", "Ge\u00b7nius", ",", "jetzt", "ihr", "Freund", ",", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Von dem heimischen Stern, wo ihr Gewand von Staub,", "tokens": ["Von", "dem", "hei\u00b7mi\u00b7schen", "Stern", ",", "wo", "ihr", "Ge\u00b7wand", "von", "Staub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unter Thr\u00e4nen und Schluchzen, sank.", "tokens": ["Un\u00b7ter", "Thr\u00e4\u00b7nen", "und", "Schluch\u00b7zen", ",", "sank", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Aber eilendes Flugs kehrte von seiner Fahrt", "tokens": ["A\u00b7ber", "ei\u00b7len\u00b7des", "Flugs", "kehr\u00b7te", "von", "sei\u00b7ner", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Freudestrahlend der Seraph jetzt:", "tokens": ["Freu\u00b7des\u00b7trah\u00b7lend", "der", "Se\u00b7raph", "jetzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "\u00bbfriede! Friede mit dir! Gott hat den Donnersturm", "tokens": ["\u00bb", "frie\u00b7de", "!", "Frie\u00b7de", "mit", "dir", "!", "Gott", "hat", "den", "Don\u00b7ner\u00b7sturm"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NN", "$.", "NN", "APPR", "PPER", "$.", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Umgeschaffen zu leisem Wehn.", "tokens": ["Um\u00b7ge\u00b7schaf\u00b7fen", "zu", "lei\u00b7sem", "Wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Seltner weinet und still deiner Verla\u00dfnen Schmerz", "tokens": ["Selt\u00b7ner", "wei\u00b7net", "und", "still", "dei\u00b7ner", "Ver\u00b7la\u00df\u00b7nen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KON", "ADJD", "PPOSAT", "NN", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Um dein abendlich Espengrab;", "tokens": ["Um", "dein", "a\u00b7bend\u00b7lich", "Es\u00b7pen\u00b7grab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und es w\u00e4lzet nicht mehr laute Verzweifelung", "tokens": ["Und", "es", "w\u00e4l\u00b7zet", "nicht", "mehr", "lau\u00b7te", "Ver\u00b7zwei\u00b7fe\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "VVFIN", "NN"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deinen Mann durch das Graun der Nacht.", "tokens": ["Dei\u00b7nen", "Mann", "durch", "das", "Graun", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Deine Schwester im Arm, sah ich den Gl\u00fccklichen", "tokens": ["Dei\u00b7ne", "Schwes\u00b7ter", "im", "Arm", ",", "sah", "ich", "den", "Gl\u00fcck\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mondumd\u00e4mmert. Sie fl\u00fcsterten,", "tokens": ["Mon\u00b7dum\u00b7d\u00e4m\u00b7mert", ".", "Sie", "fl\u00fcs\u00b7ter\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Nannten dich, und gemach blinkte der Thr\u00e4nentau,", "tokens": ["Nann\u00b7ten", "dich", ",", "und", "ge\u00b7mach", "blink\u00b7te", "der", "Thr\u00e4\u00b7nen\u00b7tau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-++-+---+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Doch auf l\u00e4chelnde Wangen, hin.", "tokens": ["Doch", "auf", "l\u00e4\u00b7cheln\u00b7de", "Wan\u00b7gen", ",", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Und er gr\u00fc\u00dfte sie Braut!\u00ab \u2013 Wonneverkl\u00e4rung steigt", "tokens": ["Und", "er", "gr\u00fc\u00df\u00b7te", "sie", "Braut", "!", "\u00ab", "\u2013", "Won\u00b7ne\u00b7ver\u00b7kl\u00e4\u00b7rung", "steigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "$.", "$(", "$(", "NN", "VVFIN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr ins Antlitz; sie herzt ihr Kind,", "tokens": ["Ihr", "ins", "Ant\u00b7litz", ";", "sie", "herzt", "ihr", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hebt zum sonnigen Thron Gottes ihr Aug', und kniet", "tokens": ["Hebt", "zum", "son\u00b7ni\u00b7gen", "Thron", "Got\u00b7tes", "ihr", "Aug'", ",", "und", "kniet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "NN", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "H\u00e4ndefaltend: die Kleine staunt.", "tokens": ["H\u00e4n\u00b7de\u00b7fal\u00b7tend", ":", "die", "Klei\u00b7ne", "staunt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "\u00bbhalleluja! du hast Gro\u00dfes an mir gethan,", "tokens": ["\u00bb", "hal\u00b7le\u00b7lu\u00b7ja", "!", "du", "hast", "Gro\u00b7\u00dfes", "an", "mir", "ge\u00b7than", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "PPER", "VAFIN", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Gott Jehovah! dir jauchzt mein Dank!", "tokens": ["Gott", "Je\u00b7ho\u00b7vah", "!", "dir", "jauchzt", "mein", "Dank", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Alle Thr\u00e4nen, auch die, welche die Liebe weint,", "tokens": ["Al\u00b7le", "Thr\u00e4\u00b7nen", ",", "auch", "die", ",", "wel\u00b7che", "die", "Lie\u00b7be", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Trocknest du der Vollendeten!", "tokens": ["Trock\u00b7nest", "du", "der", "Voll\u00b7en\u00b7de\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Freude schenkest du ihm, den ich im Kummer lie\u00df,", "tokens": ["Freu\u00b7de", "schen\u00b7kest", "du", "ihm", ",", "den", "ich", "im", "Kum\u00b7mer", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Meinem Teuren, f\u00fcr den ich starb!", "tokens": ["Mei\u00b7nem", "Teu\u00b7ren", ",", "f\u00fcr", "den", "ich", "starb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gleiches Herzens mit mir schufst du Elisa, Gott!", "tokens": ["Glei\u00b7ches", "Her\u00b7zens", "mit", "mir", "schufst", "du", "E\u00b7li\u00b7sa", ",", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "PPER", "NE", "$,", "NN", "$."], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Lieben wird sie, wie ich, den Mann!", "tokens": ["Lie\u00b7ben", "wird", "sie", ",", "wie", "ich", ",", "den", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "PPER", "$,", "PWAV", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Halleluja! \u2013 Doch du, Seraph, zerrei\u00df den Kranz,", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "\u2013", "Doch", "du", ",", "Se\u00b7raph", ",", "zer\u00b7rei\u00df", "den", "Kranz", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "$(", "KON", "PPER", "$,", "NE", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Den ich meinen Geliebten flocht!", "tokens": ["Den", "ich", "mei\u00b7nen", "Ge\u00b7lieb\u00b7ten", "flocht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Froher Enkel Geschlecht werden sie schaun, bevor,", "tokens": ["Fro\u00b7her", "En\u00b7kel", "Ge\u00b7schlecht", "wer\u00b7den", "sie", "schaun", ",", "be\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Hand in Hand, sie der Bote bringt!\u00ab", "tokens": ["Hand", "in", "Hand", ",", "sie", "der", "Bo\u00b7te", "bringt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Vom Hosannaget\u00f6n harfender Seraphim", "tokens": ["Vom", "Ho\u0