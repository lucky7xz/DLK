{"textgrid.poem.37542": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Undankbare", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Menschen namens Meier", "tokens": ["Ei\u00b7nen", "Men\u00b7schen", "na\u00b7mens", "Mei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schubst man aus des Hauses Tor,", "tokens": ["Schubst", "man", "aus", "des", "Hau\u00b7ses", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man spricht, betrunken sei er;", "tokens": ["Und", "man", "spricht", ",", "be\u00b7trun\u00b7ken", "sei", "er", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber kam's ihm nicht so vor.", "tokens": ["Sel\u00b7ber", "kam's", "ihm", "nicht", "so", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Grade auf des Weges Mitte,", "tokens": ["Gra\u00b7de", "auf", "des", "We\u00b7ges", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisch mit spitzem Kies belegt,", "tokens": ["Frisch", "mit", "spit\u00b7zem", "Kies", "be\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat er sich im Schl\u00fcrferschritte", "tokens": ["Hat", "er", "sich", "im", "Schl\u00fcr\u00b7fer\u00b7schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Knickebeinig fortbewegt.", "tokens": ["Kni\u00b7cke\u00b7bei\u00b7nig", "fort\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich will es Meier scheinen,", "tokens": ["Pl\u00f6tz\u00b7lich", "will", "es", "Mei\u00b7er", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als wenn sich die Stra\u00dfe hebt,", "tokens": ["Als", "wenn", "sich", "die", "Stra\u00b7\u00dfe", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "So da\u00df er mit seinen Beinen", "tokens": ["So", "da\u00df", "er", "mit", "sei\u00b7nen", "Bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Demgem\u00e4\u00df nach oben strebt.", "tokens": ["Dem\u00b7ge\u00b7m\u00e4\u00df", "nach", "o\u00b7ben", "strebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber T\u00e4uschung ist es leider.", "tokens": ["A\u00b7ber", "T\u00e4u\u00b7schung", "ist", "es", "lei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meier f\u00e4llt auf seinen Bauch,", "tokens": ["Mei\u00b7er", "f\u00e4llt", "auf", "sei\u00b7nen", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirkt zerst\u00f6rend auf die Kleider", "tokens": ["Wirkt", "zer\u00b7st\u00f6\u00b7rend", "auf", "die", "Klei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf die Zigarre auch.", "tokens": ["Und", "auf", "die", "Zi\u00b7gar\u00b7re", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Schnell sucht er sich aufzurappeln.", "tokens": ["Schnell", "sucht", "er", "sich", "auf\u00b7zu\u00b7rap\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weh, jetzt wird die Stra\u00dfe krumm,", "tokens": ["Weh", ",", "jetzt", "wird", "die", "Stra\u00b7\u00dfe", "krumm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und es drehn sich alle Pappeln,", "tokens": ["Und", "es", "drehn", "sich", "al\u00b7le", "Pap\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "CARD", "PRF", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auch Meier dreht es um.", "tokens": ["Und", "auch", "Mei\u00b7er", "dreht", "es", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Knacks, er f\u00e4llt auf seine Taschen,", "tokens": ["Knacks", ",", "er", "f\u00e4llt", "auf", "sei\u00b7ne", "Ta\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Worin er mit Vorbedacht", "tokens": ["Wo\u00b7rin", "er", "mit", "Vor\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch zwei wohlgef\u00fcllte Flaschen", "tokens": ["Noch", "zwei", "wohl\u00b7ge\u00b7f\u00fcll\u00b7te", "Fla\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "CARD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Klug verwahrt und mitgebracht.", "tokens": ["Klug", "ver\u00b7wahrt", "und", "mit\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hilfsbed\u00fcrftig voller Schmerzen", "tokens": ["Hilfs\u00b7be\u00b7d\u00fcrf\u00b7tig", "vol\u00b7ler", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt er da in Glas und Kies,", "tokens": ["Sitzt", "er", "da", "in", "Glas", "und", "Kies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ein Herr mit gutem Herzen", "tokens": ["Doch", "ein", "Herr", "mit", "gu\u00b7tem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam vorbei und merkte dies.", "tokens": ["Kam", "vor\u00b7bei", "und", "merk\u00b7te", "dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PDS", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Voller Mitleid und Erbarmen", "tokens": ["Vol\u00b7ler", "Mit\u00b7leid", "und", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sieht er, wie es Meiern geht,", "tokens": ["Sieht", "er", ",", "wie", "es", "Mei\u00b7ern", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt ihn auf in seinen Armen,", "tokens": ["Hebt", "ihn", "auf", "in", "sei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis er wieder grade steht.", "tokens": ["Bis", "er", "wie\u00b7der", "gra\u00b7de", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Puff! Da trifft ein h\u00f6chst geschwinder", "tokens": ["Puff", "!", "Da", "trifft", "ein", "h\u00f6chst", "ge\u00b7schwin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlag von Meiern seiner Hand", "tokens": ["Schlag", "von", "Mei\u00b7ern", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Fremden Prachtzylinder,", "tokens": ["Auf", "des", "Frem\u00b7den", "Pracht\u00b7zy\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Mann im Dunkeln stand.", "tokens": ["Da\u00df", "der", "Mann", "im", "Dun\u00b7keln", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ohne H\u00f6ren, ohne Sehen", "tokens": ["Oh\u00b7ne", "H\u00f6\u00b7ren", ",", "oh\u00b7ne", "Se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht der Gute sinnend da;", "tokens": ["Steht", "der", "Gu\u00b7te", "sin\u00b7nend", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er fragt, wie das geschehen,", "tokens": ["Und", "er", "fragt", ",", "wie", "das", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "w