{"textgrid.poem.46224": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber die zusamenkunft K\u00f6nig Jacobs zu Gro\u00df Britannien und Graf Ernsten von Mansfeld", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, die sterngucker frech, des bl\u00f6den volks einfalt", "tokens": ["Ja", ",", "die", "stern\u00b7gu\u00b7cker", "frech", ",", "des", "bl\u00f6\u00b7den", "volks", "ein\u00b7falt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADJD", "ADJD", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu schrecken, tr\u00e4uen uns mit neuen straf und kriegen,", "tokens": ["zu", "schre\u00b7cken", ",", "tr\u00e4u\u00b7en", "uns", "mit", "neu\u00b7en", "straf", "und", "krie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ADJA", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dieweil sich dises jahr Saturnus alt und kalt", "tokens": ["die\u00b7weil", "sich", "di\u00b7ses", "jahr", "Sa\u00b7tur\u00b7nus", "alt", "und", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PDAT", "NN", "NE", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und Mars voll blut und mut zusamen, seltsam, f\u00fcgen:", "tokens": ["und", "Mars", "voll", "blut", "und", "mut", "zu\u00b7sa\u00b7men", ",", "selt\u00b7sam", ",", "f\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "ADJD", "NN", "KON", "NN", "PTKVZ", "$,", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich, steigend nicht so hoch, glaub, uns solt der gewalt,", "tokens": ["Ich", ",", "stei\u00b7gend", "nicht", "so", "hoch", ",", "glaub", ",", "uns", "solt", "der", "ge\u00b7walt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "PTKNEG", "ADV", "ADJD", "$,", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "den diser erden stern ausgie\u00dfen, wol vern\u00fcgen,", "tokens": ["den", "di\u00b7ser", "er\u00b7den", "stern", "aus\u00b7gie\u00b7\u00dfen", ",", "wol", "ver\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVINF", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "als deren gegenschein und w\u00fcrkungen gestalt", "tokens": ["als", "de\u00b7ren", "ge\u00b7gen\u00b7schein", "und", "w\u00fcr\u00b7kun\u00b7gen", "ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "PTKVZ", "KON", "VAFIN", "VVPP"], "meter": "-----+-++--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "das volk verblinden kan, die weisen nicht betriegen.", "tokens": ["das", "volk", "ver\u00b7blin\u00b7den", "kan", ",", "die", "wei\u00b7sen", "nicht", "be\u00b7trie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum alsbald ich sah mit wunder und mit lust", "tokens": ["Da\u00b7rum", "als\u00b7bald", "ich", "sah", "mit", "wun\u00b7der", "und", "mit", "lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "der weisheit und manheit gestirn einander gr\u00fc\u00dfen,", "tokens": ["der", "weis\u00b7heit", "und", "man\u00b7heit", "ge\u00b7stirn", "ein\u00b7an\u00b7der", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "als Mansfelds herz gewan des k\u00f6nig Jacobs brust:", "tokens": ["als", "Mans\u00b7felds", "herz", "ge\u00b7wan", "des", "k\u00f6\u00b7nig", "Ja\u00b7cobs", "brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADV", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der s\u00fc\u00dfe glanz, sprach ich, den dise stern ausgie\u00dfen,", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "glanz", ",", "sprach", "ich", ",", "den", "di\u00b7se", "stern", "aus\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "kan niemand, dan den feind, dem sie allein verlust,", "tokens": ["kan", "nie\u00b7mand", ",", "dan", "den", "feind", ",", "dem", "sie", "al\u00b7lein", "ver\u00b7lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "wor