{"dta.poem.7434": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Auf dem Berge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da w\u00e4ren sie, der Erde h\u00f6chste Spitzen!", "tokens": ["Da", "w\u00e4\u00b7ren", "sie", ",", "der", "Er\u00b7de", "h\u00f6chs\u00b7te", "Spit\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wo ist der, der einst an sie geglaubt?", "tokens": ["Doch", "wo", "ist", "der", ",", "der", "einst", "an", "sie", "ge\u00b7glaubt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "$,", "PRELS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Auge sieht die Sonne n\u00e4her blitzen,", "tokens": ["Das", "Au\u00b7ge", "sieht", "die", "Son\u00b7ne", "n\u00e4\u00b7her", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch arm und sonnenlos ist dieses Haubt.", "tokens": ["Doch", "arm", "und", "son\u00b7nen\u00b7los", "ist", "die\u00b7ses", "Haubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sehe die granitnen S\u00e4ulen ragen,", "tokens": ["Ich", "se\u00b7he", "die", "gra\u00b7nit\u00b7nen", "S\u00e4u\u00b7len", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und endlos w\u00f6lbt das Blau sich dr\u00fcber hin;", "tokens": ["Und", "end\u00b7los", "w\u00f6lbt", "das", "Blau", "sich", "dr\u00fc\u00b7ber", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PRF", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch will das Herz mir tief beklommen schlagen,", "tokens": ["Doch", "will", "das", "Herz", "mir", "tief", "be\u00b7klom\u00b7men", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie unter einem K\u00f6nigsbaldachin.", "tokens": ["Wie", "un\u00b7ter", "ei\u00b7nem", "K\u00f6\u00b7nigs\u00b7bal\u00b7da\u00b7ch\u00b7in", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier wollte ich als frommer Parse beten,", "tokens": ["Hier", "woll\u00b7te", "ich", "als", "from\u00b7mer", "Par\u00b7se", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier singen nach der Sterne reinem Takt,", "tokens": ["Hier", "sin\u00b7gen", "nach", "der", "Ster\u00b7ne", "rei\u00b7nem", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier mit der Donnerstimme des Profeten", "tokens": ["Hier", "mit", "der", "Don\u00b7ner\u00b7stim\u00b7me", "des", "Pro\u00b7fe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gotttrunken jauchzen in den Katarakt.", "tokens": ["Gott\u00b7trun\u00b7ken", "jauch\u00b7zen", "in", "den", "Ka\u00b7ta\u00b7rakt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich wollte \u2014 ja, ich habe mich vermessen \u2014", "tokens": ["Ich", "woll\u00b7te", "ja", ",", "ich", "ha\u00b7be", "mich", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "PTKANT", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In diesen Bergen suchen mir mein Gl\u00fcck;", "tokens": ["In", "die\u00b7sen", "Ber\u00b7gen", "su\u00b7chen", "mir", "mein", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollte, ach! und konnte nicht vergessen", "tokens": ["Ich", "woll\u00b7te", ",", "ach", "!", "und", "konn\u00b7te", "nicht", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "ITJ", "$.", "KON", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Welt, die ich im Thale lie\u00df zur\u00fcck.", "tokens": ["Die", "Welt", ",", "die", "ich", "im", "Tha\u00b7le", "lie\u00df", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O wie verlangt mich nach dem Staub der Strassen,", "tokens": ["O", "wie", "ver\u00b7langt", "mich", "nach", "dem", "Staub", "der", "Stras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Druck, der Not da unten allzumal!", "tokens": ["Dem", "Druck", ",", "der", "Not", "da", "un\u00b7ten", "all\u00b7zu\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie nach den Feinden selbst, die ich verlassen,", "tokens": ["Wie", "nach", "den", "Fein\u00b7den", "selbst", ",", "die", "ich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nach der Menschheit vollster, tiefster Qual!", "tokens": ["Und", "nach", "der", "Menschheit", "volls\u00b7ter", ",", "tiefs\u00b7ter", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihr gl\u00e4nzt umsonst, ihr Purpurwolkenstreifen,", "tokens": ["Ihr", "gl\u00e4nzt", "um\u00b7sonst", ",", "ihr", "Pur\u00b7pur\u00b7wol\u00b7ken\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ladet mich gleich sel'gen Engeln ein;", "tokens": ["Und", "la\u00b7det", "mich", "gleich", "sel'\u00b7gen", "En\u00b7geln", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann den Himmel hier mit H\u00e4nden greifen,", "tokens": ["Ich", "kann", "den", "Him\u00b7mel", "hier", "mit", "H\u00e4n\u00b7den", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00f6cht' doch lieber auf der Erde sein.", "tokens": ["Und", "m\u00f6cht'", "doch", "lie\u00b7ber", "auf", "der", "Er\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}