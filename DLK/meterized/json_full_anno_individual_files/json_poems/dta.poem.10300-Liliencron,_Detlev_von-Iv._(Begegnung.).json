{"dta.poem.10300": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n (Begegnung.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Halt, M\u00e4dchen, balt! und sieh dich um geschwind,", "tokens": ["Halt", ",", "M\u00e4d\u00b7chen", ",", "balt", "!", "und", "sieh", "dich", "um", "ge\u00b7schwind", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "$.", "KON", "VVIMP", "PPER", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel Schiffe schaukeln westw\u00e4rts durch die Wellen,", "tokens": ["Viel", "Schif\u00b7fe", "schau\u00b7keln", "west\u00b7w\u00e4rts", "durch", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel hundert bugumspritzte Sturmgesellen,", "tokens": ["Viel", "hun\u00b7dert", "bu\u00b7gum\u00b7spritz\u00b7te", "Sturm\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hengist und Horst befahlen Weg und Wind.", "tokens": ["Hen\u00b7gist", "und", "Horst", "be\u00b7fah\u00b7len", "Weg", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du lachst mich aus und zeigst dich v\u00f6llig blind,", "tokens": ["Du", "lachst", "mich", "aus", "und", "zeigst", "dich", "v\u00f6l\u00b7lig", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00f6gen aneinander sie zerschellen.", "tokens": ["So", "m\u00f6\u00b7gen", "an\u00b7ein\u00b7an\u00b7der", "sie", "zer\u00b7schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier aber blitzen Fliegen und Libellen,", "tokens": ["Hier", "a\u00b7ber", "blit\u00b7zen", "Flie\u00b7gen", "und", "Li\u00b7bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzieh ein St\u00fcndchen, frisches Friesenkind.", "tokens": ["Ver\u00b7zieh", "ein", "St\u00fcnd\u00b7chen", ",", "fri\u00b7sches", "Frie\u00b7sen\u00b7kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch uns hat heut der Juni eingewiegt,", "tokens": ["Auch", "uns", "hat", "heut", "der", "Ju\u00b7ni", "ein\u00b7ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Schmetterlinge selbst, die Gauklerbande,", "tokens": ["Und", "Schmet\u00b7ter\u00b7lin\u00b7ge", "selbst", ",", "die", "Gauk\u00b7ler\u00b7ban\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind durch die Fr\u00fchlingsst\u00fcrme nicht besiegt.", "tokens": ["Sind", "durch", "die", "Fr\u00fch\u00b7lings\u00b7st\u00fcr\u00b7me", "nicht", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch hier ein Sommertag, an diesem Strande,", "tokens": ["Auch", "hier", "ein", "Som\u00b7mer\u00b7tag", ",", "an", "die\u00b7sem", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo alles schwirrt und flirrt und flitzt und fliegt,", "tokens": ["Wo", "al\u00b7les", "schwirrt", "und", "flirrt", "und", "flitzt", "und", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Freude flimmert selbst der Stein im Sande.", "tokens": ["Aus", "Freu\u00b7de", "flim\u00b7mert", "selbst", "der", "Stein", "im", "San\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}