{"dta.poem.23557": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die orthodoxen Royalisten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was, Erdens\u00f6hne, wollt ihr doch von Gottessohne?", "tokens": ["Was", ",", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", ",", "wollt", "ihr", "doch", "von", "Got\u00b7tes\u00b7soh\u00b7ne", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VMFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr setzt ihn auf und setzt ihn ab von seinem Throne.", "tokens": ["Ihr", "setzt", "ihn", "auf", "und", "setzt", "ihn", "ab", "von", "sei\u00b7nem", "Thro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er l\u00e4sst euch ruhig schreiben, disputiern und schrei'n,", "tokens": ["Er", "l\u00e4sst", "euch", "ru\u00b7hig", "schrei\u00b7ben", ",", "dis\u00b7pu\u00b7tiern", "und", "schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr wisset wohl, Er f\u00fchrt euch nicht zur Frohnfest ein.", "tokens": ["Ihr", "wis\u00b7set", "wohl", ",", "Er", "f\u00fchrt", "euch", "nicht", "zur", "Frohn\u00b7fest", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und vor den Erdenherrschern kriechet ihr im Staube!", "tokens": ["Und", "vor", "den", "Er\u00b7den\u00b7herr\u00b7schern", "krie\u00b7chet", "ihr", "im", "Stau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie unersch\u00fctterlich ist da doch euer Glaube!", "tokens": ["Wie", "un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7lich", "ist", "da", "doch", "eu\u00b7er", "Glau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr macht von jedem Zweifel eure Herzen frei,", "tokens": ["Ihr", "macht", "von", "je\u00b7dem", "Zwei\u00b7fel", "eu\u00b7re", "Her\u00b7zen", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sobald ihr wittert nur Censur und Polizei.", "tokens": ["So\u00b7bald", "ihr", "wit\u00b7tert", "nur", "Cen\u00b7sur", "und", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}