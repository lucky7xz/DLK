{"dta.poem.13385": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Weil doch die Berge reden k\u00f6nnen,", "tokens": ["Weil", "doch", "die", "Ber\u00b7ge", "re\u00b7den", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "so werden Sie ein Wort verg\u00f6nnen;", "tokens": ["so", "wer\u00b7den", "Sie", "ein", "Wort", "ver\u00b7g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie leben!", "tokens": ["Sie", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Doch", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "so zeigt die\u00df Blatt, wie treu ichs meyne", "tokens": ["so", "zeigt", "die\u00df", "Blatt", ",", "wie", "treu", "ichs", "mey\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "NN", "$,", "PWAV", "ADJD", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "am Tage der Gebuhrt,", "tokens": ["am", "Ta\u00b7ge", "der", "Ge\u00b7buhrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "man nennet mich", "tokens": ["man", "nen\u00b7net", "mich"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}}}}