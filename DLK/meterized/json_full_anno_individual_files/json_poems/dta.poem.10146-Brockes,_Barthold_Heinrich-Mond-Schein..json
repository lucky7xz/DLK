{"dta.poem.10146": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mond-Schein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Mond-Scheins Schein im Wasser scheinet", "tokens": ["Des", "Mon\u00b7dScheins", "Schein", "im", "Was\u00b7ser", "schei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Demjenigen, der ihn allein,", "tokens": ["Dem\u00b7je\u00b7ni\u00b7gen", ",", "der", "ihn", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht gen Himmel schaut,", "tokens": ["Und", "nicht", "gen", "Him\u00b7mel", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein wirckliches Original zu seyn:", "tokens": ["Ein", "wir\u00b7ck\u00b7li\u00b7ches", "O\u00b7rig\u00b7i\u00b7nal", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Er ist es aber nicht.", "tokens": ["Er", "ist", "es", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf gleiche Weise scheint des Mondes wircklichs Licht", "tokens": ["Auf", "glei\u00b7che", "Wei\u00b7se", "scheint", "des", "Mon\u00b7des", "wir\u00b7ck\u00b7lichs", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Dem, der nicht weiter sieht, ein wesentlicher Schein:", "tokens": ["Dem", ",", "der", "nicht", "wei\u00b7ter", "sieht", ",", "ein", "we\u00b7sent\u00b7li\u00b7cher", "Schein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist es aber nicht.", "tokens": ["Er", "ist", "es", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer wei\u00df, ob auch so gar der Sonnen heller Strahl", "tokens": ["Wer", "wei\u00df", ",", "ob", "auch", "so", "gar", "der", "Son\u00b7nen", "hel\u00b7ler", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht abermahl", "tokens": ["Nicht", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein ander Urbild hat, das uns noch unbekannt?", "tokens": ["Ein", "an\u00b7der", "Ur\u00b7bild", "hat", ",", "das", "uns", "noch", "un\u00b7be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ja wircklich scheint und kommet mir", "tokens": ["Ja", "wir\u00b7ck\u00b7lich", "scheint", "und", "kom\u00b7met", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADJD", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jhr heller Lebens-Brand,", "tokens": ["Ihr", "hel\u00b7ler", "Le\u00b7bens\u00b7Brand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der aller Creatur Lust, Leben, W\u00e4rm\u2019 und Wonne,", "tokens": ["Der", "al\u00b7ler", "Crea\u00b7tur", "Lust", ",", "Le\u00b7ben", ",", "W\u00e4rm'", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$,", "NN", "$,", "VVIMP", "KON", "NN", "$,"], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Wenn ich es recht erweg, nicht anders f\u00fcr,", "tokens": ["Wenn", "ich", "es", "recht", "er\u00b7weg", ",", "nicht", "an\u00b7ders", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "PTKVZ", "$,", "PTKNEG", "ADV", "APPR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als w\u00e4r der Sch\u00f6pfer selbst der Sonnen Sonne.", "tokens": ["Als", "w\u00e4r", "der", "Sch\u00f6p\u00b7fer", "selbst", "der", "Son\u00b7nen", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}