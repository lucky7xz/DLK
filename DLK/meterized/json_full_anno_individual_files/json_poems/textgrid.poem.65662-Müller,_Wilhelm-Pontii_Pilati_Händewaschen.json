{"textgrid.poem.65662": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Pontii Pilati H\u00e4ndewaschen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O bringet doch Weihwasser her! Vom besten mu\u00df es sein;", "tokens": ["O", "brin\u00b7get", "doch", "Weih\u00b7was\u00b7ser", "her", "!", "Vom", "bes\u00b7ten", "mu\u00df", "es", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "PTKVZ", "$.", "APPRART", "ADJA", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-++--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Holt es aus Rom! Das r\u00f6mische, das w\u00e4scht ja Alles rein.", "tokens": ["Holt", "es", "aus", "Rom", "!", "Das", "r\u00f6\u00b7mi\u00b7sche", ",", "das", "w\u00e4scht", "ja", "Al\u00b7les", "rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$.", "ART", "ADJA", "$,", "PDS", "VVFIN", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Pilatus, wasche deine Hand und wasche deinen Mund!", "tokens": ["Pi\u00b7la\u00b7tus", ",", "wa\u00b7sche", "dei\u00b7ne", "Hand", "und", "wa\u00b7sche", "dei\u00b7nen", "Mund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die Hand, sie ist von Tinte schwarz, der Mund vom Gifte wund.", "tokens": ["Die", "Hand", ",", "sie", "ist", "von", "Tin\u00b7te", "schwarz", ",", "der", "Mund", "vom", "Gif\u00b7te", "wund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "APPR", "NE", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Nun wasch' und sprich: Ich habe nicht gestochen und gehaun,", "tokens": ["Nun", "wasch'", "und", "sprich", ":", "Ich", "ha\u00b7be", "nicht", "ge\u00b7sto\u00b7chen", "und", "ge\u00b7haun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "An meinen beiden H\u00e4nden ist kein Tropfen Blut zu schaun;", "tokens": ["An", "mei\u00b7nen", "bei\u00b7den", "H\u00e4n\u00b7den", "ist", "kein", "Trop\u00b7fen", "Blut", "zu", "schaun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "VAFIN", "PIAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Nur Tint' und Geifer klebt mir an, damit hab' ich befleckt", "tokens": ["Nur", "Tint'", "und", "Gei\u00b7fer", "klebt", "mir", "an", ",", "da\u00b7mit", "hab'", "ich", "be\u00b7fleckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PAV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Was heilig, hoch, rein, stark und frei, was M\u00e4nnerseelen weckt", "tokens": ["Was", "hei\u00b7lig", ",", "hoch", ",", "rein", ",", "stark", "und", "frei", ",", "was", "M\u00e4n\u00b7ner\u00b7see\u00b7len", "weckt"], "token_info": ["word",