{"textgrid.poem.67444": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Venus, die dem Amor die Fl\u00fcgel nimmt", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter der Liebe, Du hast dem Sohn die Fl\u00fcgel geraubet,", "tokens": ["Mut\u00b7ter", "der", "Lie\u00b7be", ",", "Du", "hast", "dem", "Sohn", "die", "Fl\u00fc\u00b7gel", "ge\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und nun weint er und fleht um ein phantastisches Gl\u00fcck.", "tokens": ["Und", "nun", "weint", "er", "und", "fleht", "um", "ein", "phan\u00b7tas\u00b7ti\u00b7sches", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gieb, o gieb es ihm wieder! Erzwungen-best\u00e4ndige Liebe", "tokens": ["Gieb", ",", "o", "gieb", "es", "ihm", "wie\u00b7der", "!", "Er\u00b7zwun\u00b7gen\u00b7be\u00b7st\u00e4n\u00b7di\u00b7ge", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "FM", "VVIMP", "PPER", "PPER", "ADV", "$.", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Qu\u00e4lt die Geliebte mehr, als sie den Liebenden qu\u00e4lt.", "tokens": ["Qu\u00e4lt", "die", "Ge\u00b7lieb\u00b7te", "mehr", ",", "als", "sie", "den", "Lie\u00b7ben\u00b7den", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "wo