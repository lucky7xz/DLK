{"textgrid.poem.48491": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. Als er zu Schaden am Auge kommen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchl', ach gar zu spat, o Amor, deine Rach'.", "tokens": ["Ich", "f\u00fchl'", ",", "ach", "gar", "zu", "spat", ",", "o", "A\u00b7mor", ",", "dei\u00b7ne", "Rach'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "ADV", "PTKA", "ADJD", "$,", "FM", "FM", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halt' an, du gro\u00dfes Kind, halt' an und tu gemach!", "tokens": ["Halt'", "an", ",", "du", "gro\u00b7\u00dfes", "Kind", ",", "halt'", "an", "und", "tu", "ge\u00b7mach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "KON", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mu\u00df bekennen itzt, ich triebe fast nur Possen", "tokens": ["Ich", "mu\u00df", "be\u00b7ken\u00b7nen", "itzt", ",", "ich", "trie\u00b7be", "fast", "nur", "Pos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "aus deiner Bogen Kunst und t\u00f6tenden Geschossen.", "tokens": ["aus", "dei\u00b7ner", "Bo\u00b7gen", "Kunst", "und", "t\u00f6\u00b7ten\u00b7den", "Ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun siehst du dir an mir gar viel ein ander Ziel.", "tokens": ["Nun", "siehst", "du", "dir", "an", "mir", "gar", "viel", "ein", "an\u00b7der", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ich des Herzens Zweck getroffen haben will,", "tokens": ["Da", "ich", "des", "Her\u00b7zens", "Zweck", "ge\u00b7trof\u00b7fen", "ha\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da wendest du das Rohr, verletzest mein Gesichte.", "tokens": ["da", "wen\u00b7dest", "du", "das", "Rohr", ",", "ver\u00b7let\u00b7zest", "mein", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach, R\u00e4cher meiner Schuld, wie scharf ist dein Gerichte!", "tokens": ["Ach", ",", "R\u00e4\u00b7cher", "mei\u00b7ner", "Schuld", ",", "wie", "scharf", "ist", "dein", "Ge\u00b7rich\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor kunt' und wolt' ich nicht, itzt kan ich sie nicht sehn,", "tokens": ["Vor", "kunt'", "und", "wolt'", "ich", "nicht", ",", "itzt", "kan", "ich", "sie", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "KON", "VMFIN", "PPER", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wie gern' ich immer will. Ist mir nicht recht geschehn?", "tokens": ["wie", "gern'", "ich", "im\u00b7mer", "will", ".", "Ist", "mir", "nicht", "recht", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VMFIN", "$.", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchl', ach gar zu spat, o Amor, deine Rach'.", "tokens": ["Ich", "f\u00fchl'", ",", "ach", "gar", "zu", "spat", ",", "o", "A\u00b7mor", ",", "dei\u00b7ne", "Rach'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "ADV", "PTKA", "ADJD", "$,", "FM", "FM", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halt' an, du gro\u00dfes Kind, halt' an und tu gemach!", "tokens": ["Halt'", "an", ",", "du", "gro\u00b7\u00dfes", "Kind", ",", "halt'", "an", "und", "tu", "ge\u00b7mach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "KON", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mu\u00df b