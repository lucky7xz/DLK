{"dta.poem.8866": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Epilog.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ein heitres Leben lebet:", "tokens": ["Wer", "ein", "heit\u00b7res", "Le\u00b7ben", "le\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Manches ist ihm ausgeblieben,", "tokens": ["Man\u00b7ches", "ist", "ihm", "aus\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er hoffet, doch er strebet,", "tokens": ["Doch", "er", "hof\u00b7fet", ",", "doch", "er", "stre\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch er h\u00f6rt nicht auf zu lieben!", "tokens": ["Doch", "er", "h\u00f6rt", "nicht", "auf", "zu", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn kein Schiffer soll verzagen,", "tokens": ["Denn", "kein", "Schif\u00b7fer", "soll", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat ihn auch die Flut betrogen:", "tokens": ["Hat", "ihn", "auch", "die", "Flut", "be\u00b7tro\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was er will, das mu\u00df er wagen,", "tokens": ["Was", "er", "will", ",", "das", "mu\u00df", "er", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er g\u00f6nnt sein Schiff den Wogen,", "tokens": ["Und", "er", "g\u00f6nnt", "sein", "Schiff", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er wei\u00df, sie werden tragen.", "tokens": ["Und", "er", "wei\u00df", ",", "sie", "wer\u00b7den", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was am H\u00f6chsten oft erhoben,", "tokens": ["Was", "am", "H\u00f6chs\u00b7ten", "oft", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockt am K\u00fchnsten die Verwegnen,", "tokens": ["Lockt", "am", "K\u00fchns\u00b7ten", "die", "Ver\u00b7weg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sich das Versagte loben,", "tokens": ["Die", "sich", "das", "Ver\u00b7sag\u00b7te", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie m\u00fcssen ihm begegnen,", "tokens": ["Und", "sie", "m\u00fcs\u00b7sen", "ihm", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie m\u00fcssen es erproben!", "tokens": ["Und", "sie", "m\u00fcs\u00b7sen", "es", "er\u00b7pro\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ihr suchet ohne Wanken", "tokens": ["Wenn", "ihr", "su\u00b7chet", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was das Leben kann erfrischen,", "tokens": ["Was", "das", "Le\u00b7ben", "kann", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleiben jung euch die Gedanken;", "tokens": ["Blei\u00b7ben", "jung", "euch", "die", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil sie ewig jung nur zwischen", "tokens": ["Weil", "sie", "e\u00b7wig", "jung", "nur", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffen und Erf\u00fcllen schwanken.", "tokens": ["Hof\u00b7fen", "und", "Er\u00b7f\u00fcl\u00b7len", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00f6gt ihr diesen Sinn bewahren,", "tokens": ["M\u00f6gt", "ihr", "die\u00b7sen", "Sinn", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr stille W\u00fcnsche traget,", "tokens": ["Die", "ihr", "stil\u00b7le", "W\u00fcn\u00b7sche", "tra\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz Beschwerden, trotz Gefahren:", "tokens": ["Trotz", "Be\u00b7schwer\u00b7den", ",", "trotz", "Ge\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn das Leben was versaget,", "tokens": ["Wenn", "das", "Le\u00b7ben", "was", "ver\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PWS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fc\u00dft ihr's fr\u00fch genug erfahren!", "tokens": ["M\u00fc\u00dft", "ih\u00b7r's", "fr\u00fch", "ge\u00b7nug", "er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was uns Der und Jener zeiget,", "tokens": ["Was", "uns", "Der", "und", "Je\u00b7ner", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "KON", "PDAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft uns dem das Ohr verstopfen,", "tokens": ["La\u00dft", "uns", "dem", "das", "Ohr", "ver\u00b7stop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis das Herz im Busen schweiget;", "tokens": ["Bis", "das", "Herz", "im", "Bu\u00b7sen", "schwei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn beginnt das Herz zu klopfen,", "tokens": ["Denn", "be\u00b7ginnt", "das", "Herz", "zu", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00df es wohl, wohin sich's neiget!", "tokens": ["Wei\u00df", "es", "wohl", ",", "wo\u00b7hin", "sich's", "nei\u00b7get", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}