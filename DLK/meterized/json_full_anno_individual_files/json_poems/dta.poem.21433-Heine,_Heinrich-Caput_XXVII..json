{"dta.poem.21433": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im letzten Capitel hab\u2019 ich versucht", "tokens": ["Im", "letz\u00b7ten", "Ca\u00b7pi\u00b7tel", "hab'", "ich", "ver\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein bischen nachzuahmen", "tokens": ["Ein", "bi\u00b7schen", "nach\u00b7zu\u00b7ah\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den Schlu\u00df der \u201eV\u00f6gel\u201c, die sind gewi\u00df", "tokens": ["Den", "Schlu\u00df", "der", "\u201e", "V\u00f6\u00b7gel", "\u201c", ",", "die", "sind", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "$(", "NN", "$(", "$,", "PRELS", "VAFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das beste von Vaters Dramen.", "tokens": ["Das", "bes\u00b7te", "von", "Va\u00b7ters", "Dra\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die \u201eFr\u00f6sche\u201c sind auch vortrefflich. Man giebt", "tokens": ["Die", "\u201e", "Fr\u00f6\u00b7sche", "\u201c", "sind", "auch", "vor\u00b7treff\u00b7lich", ".", "Man", "giebt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$(", "NN", "$(", "VAFIN", "ADV", "ADJD", "$.", "PIS", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In deutscher Uebersetzung", "tokens": ["In", "deut\u00b7scher", "Ue\u00b7ber\u00b7set\u00b7zung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie jetzt auf der B\u00fchne von Berlin,", "tokens": ["Sie", "jetzt", "auf", "der", "B\u00fch\u00b7ne", "von", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--++", "measure": "prosodiakos"}, "line.4": {"text": "Zu k\u00f6niglicher Ergetzung.", "tokens": ["Zu", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Er\u00b7get\u00b7zung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig liebt das St\u00fcck. Das zeugt", "tokens": ["Der", "K\u00f6\u00b7nig", "liebt", "das", "St\u00fcck", ".", "Das", "zeugt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von gutem antiquen Geschmacke;", "tokens": ["Von", "gu\u00b7tem", "an\u00b7ti\u00b7quen", "Ge\u00b7schma\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Alten am\u00fcsirte weit mehr", "tokens": ["Den", "Al\u00b7ten", "am\u00b7\u00fc\u00b7sir\u00b7te", "weit", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Modernes Froschgequacke.", "tokens": ["Mo\u00b7der\u00b7nes", "Froschge\u00b7qua\u00b7cke", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Der K\u00f6nig liebt das St\u00fcck. Jedoch", "tokens": ["Der", "K\u00f6\u00b7nig", "liebt", "das", "St\u00fcck", ".", "Je\u00b7doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r\u2019 noch der Autor am Leben,", "tokens": ["W\u00e4r'", "noch", "der", "Au\u00b7tor", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ich riethe ihm nicht sich in Person", "tokens": ["Ich", "rie\u00b7the", "ihm", "nicht", "sich", "in", "Per\u00b7son"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PRF", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach Preu\u00dfen zu begeben.", "tokens": ["Nach", "Preu\u00b7\u00dfen", "zu", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}