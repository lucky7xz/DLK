{"dta.poem.4189": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das bl\u00fchende Korn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich, mein bl\u00fchendes Getrayde,", "tokens": ["Als", "ich", ",", "mein", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7tray\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Ruhm des Sch\u00f6pfers, anzusehn,", "tokens": ["Zum", "Ruhm", "des", "Sch\u00f6p\u00b7fers", ",", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einer innerlichen Freude,", "tokens": ["Mit", "ei\u00b7ner", "in\u00b7ner\u00b7li\u00b7chen", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschlossen hatt\u2019 aufs Feld zu gehn;", "tokens": ["Be\u00b7schlos\u00b7sen", "hatt'", "aufs", "Feld", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "(wovon ich j\u00fcngst, erstaunt, bemerkt,", "tokens": ["(", "wo\u00b7von", "ich", "j\u00fcngst", ",", "er\u00b7staunt", ",", "be\u00b7merkt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ADV", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df es, durch W\u00e4rm\u2019 und Thau gest\u00e4rkt,", "tokens": ["Da\u00df", "es", ",", "durch", "W\u00e4rm'", "und", "Thau", "ge\u00b7st\u00e4rkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einer einzgen Nacht so gar", "tokens": ["In", "ei\u00b7ner", "einz\u00b7gen", "Nacht", "so", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcnf Finger breit gewachsen war)", "tokens": ["F\u00fcnf", "Fin\u00b7ger", "breit", "ge\u00b7wach\u00b7sen", "war", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sah ich den Rocken wundersch\u00f6n,", "tokens": ["Sah", "ich", "den", "Ro\u00b7cken", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In Segen-reicher Zierde, stehn.", "tokens": ["In", "Se\u00b7gen\u00b7rei\u00b7cher", "Zier\u00b7de", ",", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Halmen H\u00f6h\u2019 auf sieben Fu\u00df,", "tokens": ["Der", "Hal\u00b7men", "H\u00f6h'", "auf", "sie\u00b7ben", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die jedermann bewundern mu\u00df,", "tokens": ["Die", "je\u00b7der\u00b7mann", "be\u00b7wun\u00b7dern", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Dichtigkeit der Aehren Menge,", "tokens": ["Die", "Dich\u00b7tig\u00b7keit", "der", "A\u00b7eh\u00b7ren", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und \u00fcber acht Zoll langen L\u00e4nge,", "tokens": ["Und", "\u00fc\u00b7ber", "acht", "Zoll", "lan\u00b7gen", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Die theils mit Purpur, theils im Gr\u00fcnen,", "tokens": ["Die", "theils", "mit", "Pur\u00b7pur", ",", "theils", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Recht Seladon gef\u00e4rbet schienen,", "tokens": ["Recht", "Se\u00b7la\u00b7don", "ge\u00b7f\u00e4r\u00b7bet", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und deren Schimmer-reiche Schaar,", "tokens": ["Und", "de\u00b7ren", "Schim\u00b7mer\u00b7rei\u00b7che", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In wandelbarem Glanz, so gar,", "tokens": ["In", "wan\u00b7del\u00b7ba\u00b7rem", "Glanz", ",", "so", "gar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Tauben-H\u00e4lsen \u00e4hnlich war,", "tokens": ["Den", "Tau\u00b7ben\u00b7H\u00e4lsen", "\u00e4hn\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "War noch mit einer neuen Pracht,", "tokens": ["War", "noch", "mit", "ei\u00b7ner", "neu\u00b7en", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die ich nicht, sonder Lust, erblicket,", "tokens": ["Die", "ich", "nicht", ",", "son\u00b7der", "Lust", ",", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "$,", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Bluhmen nemlich ausgeschm\u00fccket.", "tokens": ["Mit", "Bluh\u00b7men", "nem\u00b7lich", "aus\u00b7ge\u00b7schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die, da sie sonst nicht sonders gro\u00df,", "tokens": ["Die", ",", "da", "sie", "sonst", "nicht", "son\u00b7ders", "gro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und fast Figur- und Farben-los;", "tokens": ["Und", "fast", "Fi\u00b7gur", "und", "Fa\u00b7rben\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hier Form- und Farben deutlich zeigten.", "tokens": ["Hier", "For\u00b7m", "und", "Far\u00b7ben", "deut\u00b7lich", "zeig\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Ein l\u00e4nglich Bl\u00fchmchen, gelblich gr\u00fcn,", "tokens": ["Ein", "l\u00e4ng\u00b7lich", "Bl\u00fch\u00b7mchen", ",", "gelb\u00b7lich", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Hing \u00fcberall, so, da\u00df es schien,", "tokens": ["Hing", "\u00fc\u00b7be\u00b7rall", ",", "so", ",", "da\u00df", "es", "schien", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da sich die meisten Aehren beugten,", "tokens": ["Da", "sich", "die", "meis\u00b7ten", "A\u00b7eh\u00b7ren", "beug\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Sie, durch die Meng\u2019, herabzuziehn.", "tokens": ["Sie", ",", "durch", "die", "Meng'", ",", "her\u00b7ab\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es hing die ungez\u00e4hlte Schaar", "tokens": ["Es", "hing", "die", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An einem F\u00e4dchen wie ein Haar,", "tokens": ["An", "ei\u00b7nem", "F\u00e4d\u00b7chen", "wie", "ein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch sie, wenn die L\u00fcfte k\u00fchlten,", "tokens": ["Wo\u00b7durch", "sie", ",", "wenn", "die", "L\u00fcf\u00b7te", "k\u00fchl\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In stetiger Bewegung spielten", "tokens": ["In", "ste\u00b7ti\u00b7ger", "Be\u00b7we\u00b7gung", "spiel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der gelben Bl\u00fchmchen stetes Regen,", "tokens": ["Der", "gel\u00b7ben", "Bl\u00fch\u00b7mchen", "ste\u00b7tes", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr sanft- und zitterndes Bewegen", "tokens": ["Ihr", "sanft", "und", "zit\u00b7tern\u00b7des", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "War, da sie von dem bl\u00e4ulich Gr\u00fcnen", "tokens": ["War", ",", "da", "sie", "von", "dem", "bl\u00e4u\u00b7lich", "Gr\u00fc\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Aehren ganz verschieden schienen,", "tokens": ["Der", "A\u00b7eh\u00b7ren", "ganz", "ver\u00b7schie\u00b7den", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Noch mehr von denen, deren Rand", "tokens": ["Noch", "mehr", "von", "de\u00b7nen", ",", "de\u00b7ren", "Rand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PRELS", "$,", "PRELAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Man purpur-roht gef\u00e4rbet fand,", "tokens": ["Man", "pur\u00b7pur\u00b7roht", "ge\u00b7f\u00e4r\u00b7bet", "fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In ihrem bunt- und sanften Spiel,", "tokens": ["In", "ih\u00b7rem", "bunt", "und", "sanf\u00b7ten", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Aug\u2019 ein angenehmes Ziel.", "tokens": ["Dem", "Aug'", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Aehren ", "tokens": ["Der", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "(aus denen, wie wir einst gesehen,", "tokens": ["(", "aus", "de\u00b7nen", ",", "wie", "wir", "einst", "ge\u00b7se\u00b7hen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Aehten ganz allein bestehen,)", "tokens": ["Die", "A\u00b7eh\u00b7ten", "ganz", "al\u00b7lein", "be\u00b7ste\u00b7hen", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wodurch sie denn, verwunderlich,", "tokens": ["Wo\u00b7durch", "sie", "denn", ",", "ver\u00b7wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergr\u00f6ssert anzuscheinen fingen.", "tokens": ["Ver\u00b7gr\u00f6s\u00b7sert", "an\u00b7zu\u00b7schei\u00b7nen", "fin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schienen, durch die Bluhmen, auch,", "tokens": ["Sie", "schie\u00b7nen", ",", "durch", "die", "Bluh\u00b7men", ",", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die sie fast \u00fcberall umringen,", "tokens": ["Die", "sie", "fast", "\u00fc\u00b7be\u00b7rall", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und allenthalben abwerts hingen,", "tokens": ["Und", "al\u00b7len\u00b7thal\u00b7ben", "ab\u00b7werts", "hin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Statt ihrer vor\u2019gen Gl\u00e4tte, rauch.", "tokens": ["Statt", "ih\u00b7rer", "vor'\u00b7gen", "Gl\u00e4t\u00b7te", ",", "rauch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So bald ein sanfter Wind nun gehet,", "tokens": ["So", "bald", "ein", "sanf\u00b7ter", "Wind", "nun", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und \u00fcber diese Fl\u00e4chen wehet,", "tokens": ["Und", "\u00fc\u00b7ber", "die\u00b7se", "Fl\u00e4\u00b7chen", "we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bewegt sich alles: Es entstehet,", "tokens": ["Be\u00b7wegt", "sich", "al\u00b7les", ":", "Es", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Auf dieser anfangs ebnen Flur,", "tokens": ["Auf", "die\u00b7ser", "an\u00b7fangs", "eb\u00b7nen", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vom regen Wasser, die Figur.", "tokens": ["Vom", "re\u00b7gen", "Was\u00b7ser", ",", "die", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Es scheinen hin und wieder Wellen", "tokens": ["Es", "schei\u00b7nen", "hin", "und", "wie\u00b7der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sich zu erh\u00f6hen und zu schwellen,", "tokens": ["Sich", "zu", "er\u00b7h\u00f6\u00b7hen", "und", "zu", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zugleich an unterschiednen Stellen", "tokens": ["Zu\u00b7gleich", "an", "un\u00b7ter\u00b7schied\u00b7nen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sich wieder niederwerts zu lenken,", "tokens": ["Sich", "wie\u00b7der", "nie\u00b7der\u00b7werts", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und allgem\u00e4hlich sich zu senken,", "tokens": ["Und", "all\u00b7ge\u00b7m\u00e4h\u00b7lich", "sich", "zu", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wiederum sich zu erh\u00f6hen.", "tokens": ["Bald", "wie\u00b7de\u00b7rum", "sich", "zu", "er\u00b7h\u00f6\u0