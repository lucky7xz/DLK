{"dta.poem.21466": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fieberglut .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Durch meine Adern", "tokens": ["Durch", "mei\u00b7ne", "A\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Rast Fieberglut!", "tokens": ["Rast", "Fie\u00b7berg\u00b7lut", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In meinem kranken", "tokens": ["In", "mei\u00b7nem", "kran\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ausged\u00f6rrten Hirne", "tokens": ["Aus\u00b7ge\u00b7d\u00f6rr\u00b7ten", "Hir\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lodert des Wahnsinns", "tokens": ["Lo\u00b7dert", "des", "Wahn\u00b7sinns"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Flamme empor!", "tokens": ["Flam\u00b7me", "em\u00b7por", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Aus Nacht und Graus,", "tokens": ["Aus", "Nacht", "und", "Graus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Aus wilder Verzweiflung", "tokens": ["Aus", "wil\u00b7der", "Ver\u00b7zwei\u00b7flung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Schreit meine Seele", "tokens": ["Schreit", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Nach dir, nach dir", "tokens": ["Nach", "dir", ",", "nach", "dir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Du s\u00fc\u00dfe", "tokens": ["Du", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Ewigverlorene Geliebte!", "tokens": ["E\u00b7wig\u00b7ver\u00b7lo\u00b7re\u00b7ne", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "In hei\u00dfen Thr\u00e4nen", "tokens": ["In", "hei\u00b7\u00dfen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Quillt das Opfer", "tokens": ["Quillt", "das", "Op\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Unendlicher Sehnsucht.", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "O da\u00df ich vergessen k\u00f6nnte! . .", "tokens": ["O", "da\u00df", "ich", "ver\u00b7ges\u00b7sen", "k\u00f6nn\u00b7te", "!", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "KOUS", "PPER", "VVINF", "VMFIN", "$.", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ert\u00f6dten der Erinnerung", "tokens": ["Er\u00b7t\u00f6d\u00b7ten", "der", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.18": {"text": "Vielholden M\u00e4hrchenduft!", "tokens": ["Viel\u00b7hol\u00b7den", "M\u00e4hr\u00b7chen\u00b7duft", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "In tollem Sinnentaumel", "tokens": ["In", "tol\u00b7lem", "Sin\u00b7nen\u00b7tau\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Bachantisch schwelgen,", "tokens": ["Ba\u00b7ch\u00b7an\u00b7tisch", "schwel\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Hinsterben in den Wonnen", "tokens": ["Hins\u00b7ter\u00b7ben", "in", "den", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Rauschseliger Liebe! \u2026", "tokens": ["Rauschse\u00b7li\u00b7ger", "Lie\u00b7be", "!", "\u2026"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Wie sch\u00f6n schien die Welt", "tokens": ["Wie", "sch\u00f6n", "schien", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "Dem Auge des Gl\u00fccklichen!", "tokens": ["Dem", "Au\u00b7ge", "des", "Gl\u00fcck\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.25": {"text": "Ein schimmernder Bl\u00fcthenhag", "tokens": ["Ein", "schim\u00b7mern\u00b7der", "Bl\u00fc\u00b7then\u00b7hag"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "S\u00fc\u00df umwoben und durchzittert", "tokens": ["S\u00fc\u00df", "um\u00b7wo\u00b7ben", "und", "durch\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Von Duft und Schall", "tokens": ["Von", "Duft", "und", "Schall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Und nun \u2014", "tokens": ["Und", "nun"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "In Nacht getaucht", "tokens": ["In", "Nacht", "ge\u00b7taucht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Ist der Lichtkreis der Sonne,", "tokens": ["Ist", "der", "Licht\u00b7kreis", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.31": {"text": "Zum Schmerz wird jeder Athemzug", "tokens": ["Zum", "Schmerz", "wird", "je\u00b7der", "A\u00b7them\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Der leidgepre\u00dften Brust,", "tokens": ["Der", "leid\u00b7ge\u00b7pre\u00df\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Immer wieder w\u00fchl\u2019 ich,", "tokens": ["Im\u00b7mer", "wie\u00b7der", "w\u00fchl'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.34": {"text": "O wolll\u00fcstige Selbstqual!", "tokens": ["O", "woll\u00b7l\u00fcs\u00b7ti\u00b7ge", "Selbst\u00b7qual", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "In meiner Wunde,", "tokens": ["In", "mei\u00b7ner", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Der nie verharschenden \u2026", "tokens": ["Der", "nie", "ver\u00b7har\u00b7schen\u00b7den", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}