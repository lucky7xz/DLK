{"dta.poem.15751": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Wahrheit lebenswerth war einmal nur das Leben,", "tokens": ["In", "Wahr\u00b7heit", "le\u00b7bens\u00b7werth", "war", "ein\u00b7mal", "nur", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sch\u00f6ne Menschheit war des Menschen h\u00f6chstes Streben.", "tokens": ["Als", "sch\u00f6\u00b7ne", "Menschheit", "war", "des", "Men\u00b7schen", "h\u00f6chs\u00b7tes", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "An Seel' und Leib gesund sind durchaus nur die Griechen,", "tokens": ["An", "Seel'", "und", "Leib", "ge\u00b7sund", "sind", "durc\u00b7haus", "nur", "die", "Grie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dagegen unsre Welt ein gro\u00dfes Haus der Siechen.", "tokens": ["Da\u00b7ge\u00b7gen", "uns\u00b7re", "Welt", "ein", "gro\u00b7\u00dfes", "Haus", "der", "Sie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+