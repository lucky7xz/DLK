{"dta.poem.9170": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n Er ist fromm/ aber wenn er schl\u00e4fft.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich meiner Rosilis", "tokens": ["Als", "ich", "mei\u00b7ner", "Ro\u00b7si\u00b7lis"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Neulich an die sch\u00fcrtze grieffe/", "tokens": ["Neu\u00b7lich", "an", "die", "sch\u00fcrt\u00b7ze", "grief\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagte sie mir gar gewi\u00df/", "tokens": ["Sag\u00b7te", "sie", "mir", "gar", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich w\u00e4r fromm/ doch wann ich schlieffe;", "tokens": ["Ich", "w\u00e4r", "fromm", "/", "doch", "wann", "ich", "schlief\u00b7fe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Sonsten w\u00e4r ich in der haut", "tokens": ["Sons\u00b7ten", "w\u00e4r", "ich", "in", "der", "haut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein rechtschaffen b\u00f6ses kraut.", "tokens": ["Ein", "recht\u00b7schaf\u00b7fen", "b\u00f6\u00b7ses", "kraut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ja mein liebgen/ fieng ich an/", "tokens": ["Ja", "mein", "lieb\u00b7gen", "/", "fi\u00b7eng", "ich", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "VVFIN", "$(", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ich gesteh es/ wenn ich wache/", "tokens": ["Ich", "ge\u00b7steh", "es", "/", "wenn", "ich", "wa\u00b7che", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich es nicht lassen kan:", "tokens": ["Da\u00df", "ich", "es", "nicht", "las\u00b7sen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es ist so eine sache/", "tokens": ["Doch", "es", "ist", "so", "ei\u00b7ne", "sa\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stelle deine sch\u00f6nheit ein/", "tokens": ["Stel\u00b7le", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So will ich nicht lose seyn.", "tokens": ["So", "will", "ich", "nicht", "lo\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "3. Uber dieses bin ich doch", "tokens": ["U\u00b7ber", "die\u00b7ses", "bin", "ich", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem schlaffe fromm und stille/", "tokens": ["In", "dem", "schlaf\u00b7fe", "fromm", "und", "stil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADJD", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum mein engel/ ist es noch", "tokens": ["Drum", "mein", "en\u00b7gel", "/", "ist", "es", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "$(", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein und mein beliebter wille/", "tokens": ["Dein", "und", "mein", "be\u00b7lieb\u00b7ter", "wil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Suchst du die gewogenheit", "tokens": ["Suchst", "du", "die", "ge\u00b7wo\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Blo\u00df in meiner fr\u00f6mmigkeit.", "tokens": ["Blo\u00df", "in", "mei\u00b7ner", "fr\u00f6m\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ey so schlaff einmahl bey mir:", "tokens": ["Ey", "so", "schlaff", "ein\u00b7mahl", "bey", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonsten mu\u00df ich es gestehen/", "tokens": ["Sons\u00b7ten", "mu\u00df", "ich", "es", "ge\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich keinmal kan zu dir", "tokens": ["Da\u00df", "ich", "kein\u00b7mal", "kan", "zu", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fromm und eingezogen gehen:", "tokens": ["Fromm", "und", "ein\u00b7ge\u00b7zo\u00b7gen", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich fromm seyn/ meine zier/", "tokens": ["Soll", "ich", "fromm", "seyn", "/", "mei\u00b7ne", "zier", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ey so schlaf einmal bey mir.", "tokens": ["Ey", "so", "schlaf", "ein\u00b7mal", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}