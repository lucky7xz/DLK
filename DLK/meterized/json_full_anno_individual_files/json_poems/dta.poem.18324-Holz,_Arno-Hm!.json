{"dta.poem.18324": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Hm!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben habe keinen Zweck;", "tokens": ["Das", "Le\u00b7ben", "ha\u00b7be", "kei\u00b7nen", "Zweck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man sieht's, sie haben nie gegessen", "tokens": ["Man", "sieht's", ",", "sie", "ha\u00b7ben", "nie", "ge\u00b7ges\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fasanenstiz und Schnepfendreck.", "tokens": ["Fa\u00b7sa\u00b7nen\u00b7stiz", "und", "Schnep\u00b7fen\u00b7dreck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}}}}