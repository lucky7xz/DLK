{"textgrid.poem.55131": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Prometheus", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bedecke deinen Himmel, Zeus,", "tokens": ["Be\u00b7de\u00b7cke", "dei\u00b7nen", "Him\u00b7mel", ",", "Zeus", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wolkendunst,", "tokens": ["Mit", "Wol\u00b7ken\u00b7dunst", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und \u00fcbe, dem Knaben gleich,", "tokens": ["Und", "\u00fc\u00b7be", ",", "dem", "Kna\u00b7ben", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Disteln k\u00f6pft,", "tokens": ["Der", "Dis\u00b7teln", "k\u00f6pft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "An Eichen dich und Bergesh\u00f6hn;", "tokens": ["An", "Ei\u00b7chen", "dich", "und", "Ber\u00b7ges\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00dft mir meine Erde", "tokens": ["Mu\u00dft", "mir", "mei\u00b7ne", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Doch lassen stehn", "tokens": ["Doch", "las\u00b7sen", "stehn"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVINF", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und meine H\u00fctte, die du nicht gebaut,", "tokens": ["Und", "mei\u00b7ne", "H\u00fct\u00b7te", ",", "die", "du", "nicht", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und meinen Herd,", "tokens": ["Und", "mei\u00b7nen", "Herd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Um dessen Glut", "tokens": ["Um", "des\u00b7sen", "Glut"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Du mich beneidest.", "tokens": ["Du", "mich", "be\u00b7nei\u00b7dest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich kenne nichts \u00c4rmeres", "tokens": ["Ich", "ken\u00b7ne", "nichts", "\u00c4r\u00b7me\u00b7res"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Unter der Sonn als euch, G\u00f6tter!", "tokens": ["Un\u00b7ter", "der", "Sonn", "als", "euch", ",", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOUS", "PPER", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Ihr n\u00e4hret k\u00fcmmerlich", "tokens": ["Ihr", "n\u00e4h\u00b7ret", "k\u00fcm\u00b7mer\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Opfersteuern", "tokens": ["Von", "Op\u00b7fer\u00b7steu\u00b7ern"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und Gebetshauch", "tokens": ["Und", "Ge\u00b7bets\u00b7hauch"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Eure Majest\u00e4t", "tokens": ["Eu\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und darbtet, w\u00e4ren", "tokens": ["Und", "darb\u00b7tet", ",", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "PAV", "$,", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Nicht Kinder und Bettler", "tokens": ["Nicht", "Kin\u00b7der", "und", "Bett\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Hoffnungsvolle Toren.", "tokens": ["Hoff\u00b7nungs\u00b7vol\u00b7le", "To\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Da ich ein Kind war,", "tokens": ["Da", "ich", "ein", "Kind", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nicht wu\u00dfte, wo aus noch ein,", "tokens": ["Nicht", "wu\u00df\u00b7te", ",", "wo", "aus", "noch", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWAV", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kehrt ich mein verirrtes Auge", "tokens": ["Kehrt", "ich", "mein", "ver\u00b7irr\u00b7tes", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zur Sonne, als wenn dr\u00fcber w\u00e4r", "tokens": ["Zur", "Son\u00b7ne", ",", "als", "wenn", "dr\u00fc\u00b7ber", "w\u00e4r"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOKOM", "KOUS", "PAV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Ohr, zu h\u00f6ren meine Klage,", "tokens": ["Ein", "Ohr", ",", "zu", "h\u00f6\u00b7ren", "mei\u00b7ne", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Herz wie meins,", "tokens": ["Ein", "Herz", "wie", "meins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sich des Bedr\u00e4ngten zu erbarmen.", "tokens": ["Sich", "des", "Be\u00b7dr\u00e4ng\u00b7ten", "zu", "er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer half mir", "tokens": ["Wer", "half", "mir"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wider der Titanen \u00dcbermut?", "tokens": ["Wi\u00b7der", "der", "Ti\u00b7ta\u00b7nen", "\u00dc\u00b7ber\u00b7mut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer rettete vom Tode mich,", "tokens": ["Wer", "ret\u00b7te\u00b7te", "vom", "To\u00b7de", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sklaverei?", "tokens": ["Von", "Skla\u00b7ve\u00b7rei", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hast du nicht alles selbst vollendet,", "tokens": ["Hast", "du", "nicht", "al\u00b7les", "selbst", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Heilig gl\u00fchend Herz?", "tokens": ["Hei\u00b7lig", "gl\u00fc\u00b7hend", "Herz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und gl\u00fchtest jung und gut,", "tokens": ["Und", "gl\u00fch\u00b7test", "jung", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Betrogen, Rettungsdank", "tokens": ["Be\u00b7tro\u00b7gen", ",", "Ret\u00b7tungs\u00b7dank"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dem Schlafenden da droben?", "tokens": ["Dem", "Schla\u00b7fen\u00b7den", "da", "dro\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich dich ehren? Wof\u00fcr?", "tokens": ["Ich", "dich", "eh\u00b7ren", "?", "Wo\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hast du die Schmerzen gelindert", "tokens": ["Hast", "du", "die", "Schmer\u00b7zen", "ge\u00b7lin\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Je des Beladenen?", "tokens": ["Je", "des", "Be\u00b7la\u00b7de\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Hast du die Tr\u00e4nen gestillet", "tokens": ["Hast", "du", "die", "Tr\u00e4\u00b7nen", "ge\u00b7stil\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Je des Ge\u00e4ngsteten?", "tokens": ["Je", "des", "Ge\u00b7\u00e4ngs\u00b7te\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Hat nicht mich zum Manne geschmiedet", "tokens": ["Hat", "nicht", "mich", "zum", "Man\u00b7ne", "ge\u00b7schmie\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "PPER", "APPRART", "NN", "VVPP"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Die allm\u00e4chtige Zeit", "tokens": ["Die", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und das ewige Schicksal,", "tokens": ["Und", "das", "e\u00b7wi\u00b7ge", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.9": {"text": "Meine Herrn und deine?", "tokens": ["Mei\u00b7ne", "Herrn", "und", "dei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "W\u00e4hntest du etwa,", "tokens": ["W\u00e4hn\u00b7test", "du", "et\u00b7wa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ich sollte das Leben hassen,", "tokens": ["Ich", "soll\u00b7te", "das", "Le\u00b7ben", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In W\u00fcsten fliehen,", "tokens": ["In", "W\u00fcs\u00b7ten", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Weil nicht", "tokens": ["Weil", "nicht"], "token_info": ["word", "word"], "pos": ["KOUS", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "alle Bl\u00fctentr\u00e4ume reiften?", "tokens": ["al\u00b7le", "Bl\u00fc\u00b7ten\u00b7tr\u00e4u\u00b7me", "reif\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier sitz ich, forme Menschen", "tokens": ["Hier", "sitz", "ich", ",", "for\u00b7me", "Men\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach meinem Bilde,", "tokens": ["Nach", "mei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein Geschlecht, das mir gleich sei,", "tokens": ["Ein", "Ge\u00b7schlecht", ",", "das", "mir", "gleich", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu leiden, zu weinen,", "tokens": ["Zu", "lei\u00b7den", ",", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu genie\u00dfen und zu freuen sich,", "tokens": ["Zu", "ge\u00b7nie\u00b7\u00dfen", "und", "zu", "freu\u00b7en", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und dein nicht zu achten,", "tokens": ["Und", "dein", "nicht", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie ich!", "tokens": ["Wie", "ich", "!"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Bedecke deinen Himmel, Zeus,", "tokens": ["Be\u00b7de\u00b7cke", "dei\u00b7nen", "Him\u00b7mel", ",", "Zeus", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wolkendunst,", "tokens": ["Mit", "Wol\u00b7ken\u00b7dunst", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und \u00fcbe, dem Knaben gleic