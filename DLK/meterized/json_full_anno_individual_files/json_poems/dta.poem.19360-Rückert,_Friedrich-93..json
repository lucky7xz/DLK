{"dta.poem.19360": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder hat sein Recht, um sich in sich zu r\u00fcnden;", "tokens": ["Ein", "je\u00b7der", "hat", "sein", "Recht", ",", "um", "sich", "in", "sich", "zu", "r\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPOSAT", "NN", "$,", "KOUI", "PRF", "APPR", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch was die Einzlen trennt, das soll sie auch verb\u00fcnden.", "tokens": ["Doch", "was", "die", "Einz\u00b7len", "trennt", ",", "das", "soll", "sie", "auch", "ver\u00b7b\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn nur auf den Beding ist dein, was dein du nennst,", "tokens": ["Denn", "nur", "auf", "den", "Be\u00b7ding", "ist", "dein", ",", "was", "dein", "du", "nennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "$,", "PRELS", "PPOSAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du hinwider auch als mein das Mein' erkennst.", "tokens": ["Wenn", "du", "hin\u00b7wi\u00b7der", "auch", "als", "mein", "das", "Mein'", "er\u00b7kennst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "KOKOM", "PPOSAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nimmst du Meines mir, ists nicht genug, da\u00df du", "tokens": ["Doch", "nimmst", "du", "Mei\u00b7nes", "mir", ",", "ists", "nicht", "ge\u00b7nug", ",", "da\u00df", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "PPER", "$,", "VAFIN", "PTKNEG", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wiedergibst, du mu\u00dft verlieren Deins dazu.", "tokens": ["Es", "wie\u00b7der\u00b7gibst", ",", "du", "mu\u00dft", "ver\u00b7lie\u00b7ren", "Deins", "da\u00b7zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das ist die Strafe, die du selbst dir zuerkannt;", "tokens": ["Das", "ist", "die", "Stra\u00b7fe", ",", "die", "du", "selbst", "dir", "zu\u00b7er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein eignes Thun hat sich auf dich zur\u00fcck gewandt.", "tokens": ["Dein", "eig\u00b7nes", "Thun", "hat", "sich", "auf", "dich", "zu\u00b7r\u00fcck", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "APPR", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Das ganze Recht ist dis, da\u00df du dem andren nicht", "tokens": ["Das", "gan\u00b7ze", "Recht", "ist", "dis", ",", "da\u00df", "du", "dem", "an\u00b7dren", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDS", "$,", "KOUS", "PPER", "ART", "ADJA", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das thust, was du nicht willst, da\u00df dir von ihm geschicht.", "tokens": ["Das", "thust", ",", "was", "du", "nicht", "willst", ",", "da\u00df", "dir", "von", "ihm", "ge\u00b7schicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In diesem seid ihr gleich, und frei, wenn ihr verst\u00e4ndig", "tokens": ["In", "die\u00b7sem", "seid", "ihr", "gleich", ",", "und", "frei", ",", "wenn", "ihr", "ver\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "ADV", "$,", "KON", "ADJD", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Rechtes Unterschied erkennet als nothwendig.", "tokens": ["Des", "Rech\u00b7tes", "Un\u00b7ter\u00b7schied", "er\u00b7ken\u00b7net", "als", "noth\u00b7wen\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}