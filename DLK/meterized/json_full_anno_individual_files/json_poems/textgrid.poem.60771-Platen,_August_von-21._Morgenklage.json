{"textgrid.poem.60771": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "21. Morgenklage", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von bebender Wimper tropft der Nacht Z\u00e4hre mir,", "tokens": ["Von", "be\u00b7ben\u00b7der", "Wim\u00b7per", "tropft", "der", "Nacht", "Z\u00e4h\u00b7re", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "PPER", "$,"], "meter": "-+--+-+-++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Indes den ersehnten Tag verhei\u00dft Hahnenruf:", "tokens": ["In\u00b7des", "den", "er\u00b7sehn\u00b7ten", "Tag", "ver\u00b7hei\u00dft", "Hah\u00b7nen\u00b7ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wach auf, o betr\u00fcbte Seele,", "tokens": ["Wach", "auf", ",", "o", "be\u00b7tr\u00fcb\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlie\u00df einen Bund mit Gott!", "tokens": ["Schlie\u00df", "ei\u00b7nen", "Bund", "mit", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schw\u00f6re den sch\u00f6nen Schwur, getreu stets zu sein", "tokens": ["Ich", "schw\u00f6\u00b7re", "den", "sch\u00f6\u00b7nen", "Schwur", ",", "ge\u00b7treu", "stets", "zu", "sein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VVFIN", "$,", "ADJD", "ADV", "PTKZU", "VAINF"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem hohen Gesetz und will, in Andacht vertieft,", "tokens": ["Dem", "ho\u00b7hen", "Ge\u00b7setz", "und", "will", ",", "in", "An\u00b7dacht", "ver\u00b7tieft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VMFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Voll Priestergef\u00fchl verwalten", "tokens": ["Voll", "Pries\u00b7ter\u00b7ge\u00b7f\u00fchl", "ver\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein gro\u00df Prophetenamt.", "tokens": ["Dein", "gro\u00df", "Pro\u00b7phe\u00b7ten\u00b7amt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du aber, ein einzigmal vom Geist nimm die Last!", "tokens": ["Du", "a\u00b7ber", ",", "ein", "ein\u00b7zig\u00b7mal", "vom", "Geist", "nimm", "die", "Last", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "ADV", "APPRART", "NN", "VVIMP", "ART", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Liebe wie au\u00dfer mir, an Gleichwarmer Brust,", "tokens": ["Von", "Lie\u00b7be", "wie", "au\u00b7\u00dfer", "mir", ",", "an", "Gleich\u00b7war\u00b7mer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "APPR", "PPER", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+--+--++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "La\u00df fr\u00f6hlich und selbstvergessen", "tokens": ["La\u00df", "fr\u00f6h\u00b7lich", "und", "selbst\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "KON", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mich f\u00fchlen, Mensch zu sein!", "tokens": ["Mich", "f\u00fch\u00b7len", ",", "Mensch", "zu", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vergebens! Die Hand erstarrt, da voll stolzen Frosts", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "Die", "Hand", "er\u00b7starrt", ",", "da", "voll", "stol\u00b7zen", "Frosts"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "NN", "VVPP", "$,", "KOUS", "ADJD", "ADJA", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach irdischer Frucht sie greift! Es seufzt unter dir,", "tokens": ["Nach", "ir\u00b7di\u00b7scher", "Frucht", "sie", "greift", "!", "Es", "seufzt", "un\u00b7ter", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schwerm\u00fctige Wucht, Gedanke,", "tokens": ["Schwer\u00b7m\u00fc\u00b7ti\u00b7ge", "Wucht", ",", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Nacken tiefgebeugt!", "tokens": ["Mein", "Na\u00b7cken", "tief\u00b7ge\u00b7beugt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Umnebelt den Blick die Welt, so la\u00df, keusches Licht,", "tokens": ["Um\u00b7ne\u00b7belt", "den", "Blick", "die", "Welt", ",", "so", "la\u00df", ",", "keu\u00b7sches", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In reinere L\u00fcfte mich emporschwebend gehn!", "tokens": ["In", "rei\u00b7ne\u00b7re", "L\u00fcf\u00b7te", "mich", "em\u00b7por\u00b7schwe\u00b7bend", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wer aber hienieden setzte", "tokens": ["Wer", "a\u00b7ber", "hien\u00b7ie\u00b7den", "setz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf Wolken je den Fu\u00df?", "tokens": ["Auf", "Wol\u00b7ken", "je", "den", "Fu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O seliger Mann, wofern gelebt Einer, der", "tokens": ["O", "se\u00b7li\u00b7ger", "Mann", ",", "wo\u00b7fern", "ge\u00b7lebt", "Ei\u00b7ner", ",", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVPP", "PIS", "$,", "PRELS"], "meter": "+---+-+-++-+", "measure": "dactylic.init"}, "line.2": {"text": "In Ruhe die Nacht verbringt, und jedweden Tag,", "tokens": ["In", "Ru\u00b7he"