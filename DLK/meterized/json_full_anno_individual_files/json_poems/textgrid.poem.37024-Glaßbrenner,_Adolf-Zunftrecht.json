{"textgrid.poem.37024": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Zunftrecht", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein launisch-wildes Wetter!", "tokens": ["Welch", "ein", "lau\u00b7nischwil\u00b7des", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Aber, sieh! es warf dazwischen", "tokens": ["A\u00b7ber", ",", "sieh", "!", "es", "warf", "da\u00b7zwi\u00b7schen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVIMP", "$.", "PPER", "VVFIN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Auge, unsre Sonne,", "tokens": ["Got\u00b7tes", "Au\u00b7ge", ",", "uns\u00b7re", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen wonnig milden Blick und \u2013", "tokens": ["Ei\u00b7nen", "won\u00b7nig", "mil\u00b7den", "Blick", "und", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "KON", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und aus einer rothen Wolke", "tokens": ["Und", "aus", "ei\u00b7ner", "ro\u00b7then", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hagelte es rasselnd, prasselnd:", "tokens": ["Ha\u00b7gel\u00b7te", "es", "ras\u00b7selnd", ",", "pras\u00b7selnd", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdank Dir, Himmel! Dank, Gott ", "tokens": ["\u00bb", "dank", "Dir", ",", "Him\u00b7mel", "!", "Dank", ",", "Gott"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["$(", "APPR", "PPER", "$,", "NN", "$.", "NN", "$,", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "(welcher nebenbei dem Amt des", "tokens": ["(", "wel\u00b7cher", "ne\u00b7ben\u00b7bei", "dem", "Amt", "des"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "APPR", "ART", "NN", "ART"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wind- und Wettermachens vorstand).", "tokens": ["Win\u00b7d", "und", "Wet\u00b7ter\u00b7ma\u00b7chens", "vor\u00b7stand", ")", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["TRUNC", "KON", "NN", "VVFIN", "$(", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00bbdank Dir, g\u00fct'ger Gott der Ordnung!\u00ab", "tokens": ["\u00bb", "dank", "Dir", ",", "g\u00fct'\u00b7ger", "Gott", "der", "Ord\u00b7nung", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rief das Volk, begierig sammelnd", "tokens": ["Rief", "das", "Volk", ",", "be\u00b7gie\u00b7rig", "sam\u00b7melnd"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Geschirren und in K\u00f6rben,", "tokens": ["In", "Ge\u00b7schir\u00b7ren", "und", "in", "K\u00f6r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was dies meteorologisch-", "tokens": ["Was", "dies", "me\u00b7te\u00b7o\u00b7ro\u00b7lo\u00b7gisch"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PDS", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ungesetzliche Ereigni\u00df", "tokens": ["Un\u00b7ge\u00b7setz\u00b7li\u00b7che", "Er\u00b7eig\u00b7ni\u00df"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Seiner Armuth freundlich darbot.", "tokens": ["Sei\u00b7ner", "Ar\u00b7muth", "freund\u00b7lich", "dar\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Aber schon am selben Abend", "tokens": ["A\u00b7ber", "schon", "am", "sel\u00b7ben", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrmten alle Zuckerb\u00e4cker,", "tokens": ["St\u00fcrm\u00b7ten", "al\u00b7le", "Zu\u00b7cker\u00b7b\u00e4\u00b7cker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem\u00fcthigst petitionirend,", "tokens": ["De\u00b7m\u00fct\u00b7higst", "pe\u00b7ti\u00b7ti\u00b7o\u00b7ni\u00b7rend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In das Schlo\u00df des Ober-Mufti's,", "tokens": ["In", "das", "Schlo\u00df", "des", "O\u00b7ber\u00b7Muf\u00b7ti's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und nach dreimal drei\u00dfig Wochen", "tokens": ["Und", "nach", "drei\u00b7mal", "drei\u00b7\u00dfig", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "(da\u00df sich die Beh\u00f6rden hierorts", "tokens": ["(", "da\u00df", "sich", "die", "Be\u00b7h\u00f6r\u00b7den", "hier\u00b7orts"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PRF", "ART", "NN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Keine Zeit zur ruh'gen Pr\u00fcfung", "tokens": ["Kei\u00b7ne", "Zeit", "zur", "ruh'\u00b7gen", "Pr\u00fc\u00b7fung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eingegangner Bitten lassen,", "tokens": ["Ein\u00b7ge\u00b7gang\u00b7ner", "Bit\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Fand ich immer unverzeihlich!", "tokens": ["Fand", "ich", "im\u00b7mer", "un\u00b7ver\u00b7zeih\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hab' es aber nie ge\u00e4u\u00dfert.)", "tokens": ["Hab'", "es", "a\u00b7ber", "nie", "ge\u00b7\u00e4u\u00b7\u00dfert", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kam ein muftiger Erla\u00df schon", "tokens": ["Kam", "ein", "muf\u00b7ti\u00b7ger", "Er\u00b7la\u00df", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "ADV"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "An des Reiches Zuckerb\u00e4cker,", "tokens": ["An", "des", "Rei\u00b7ches", "Zu\u00b7cker\u00b7b\u00e4\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00f6rtlich also lautend:", "tokens": ["W\u00f6rt\u00b7lich", "al\u00b7so", "lau\u00b7tend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00bbschafe!", "tokens": ["\u00bb", "scha\u00b7fe", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Nach gewissenhafter Sitzung", "tokens": ["Nach", "ge\u00b7wis\u00b7sen\u00b7haf\u00b7ter", "Sit\u00b7zung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines zopf'gen Mufti-Rathes", "tokens": ["Mei\u00b7nes", "zopf'\u00b7gen", "Muf\u00b7ti\u00b7Rat\u00b7hes"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nach dessen Anerkennung", "tokens": ["Und", "nach", "des\u00b7sen", "An\u00b7er\u00b7ken\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Gerechtigkeit der Klage,", "tokens": ["Der", "Ge\u00b7rech\u00b7tig\u00b7keit", "der", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welche ihr Mir \u00fcbergeben,", "tokens": ["Wel\u00b7che", "ihr", "Mir", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Habe Ich dem Standbild ", "tokens": ["Ha\u00b7be", "Ich", "dem", "Stand\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "In das linke Ohr gerufen,", "tokens": ["In", "das", "lin\u00b7ke", "Ohr", "ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df die Zuckerb\u00e4ckerei sei", "tokens": ["Da\u00df", "die", "Zu\u00b7cker\u00b7b\u00e4\u00b7cke\u00b7rei", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Euer gutes, alt-ehrw\u00fcrd'ges", "tokens": ["Eu\u00b7er", "gu\u00b7tes", ",", "al\u00b7tehr\u00b7w\u00fcrd'\u00b7ges"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Monopol und Zunftrecht, ", "tokens": ["Mo\u00b7no\u00b7pol", "und", "Zunf\u00b7trecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Er, der gro\u00dfe Gott der G\u00f6tter,", "tokens": ["Er", ",", "der", "gro\u00b7\u00dfe", "Gott", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Gn\u00e4digst m\u00f6ge anbefehlen:", "tokens": ["Gn\u00e4\u00b7digst", "m\u00f6\u00b7ge", "an\u00b7be\u00b7feh\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Niemals wieder Zuckererbsen", "tokens": ["Nie\u00b7mals", "wie\u00b7der", "Zu\u00b7cker\u00b7erb\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Auf das Volk herabzuhageln,", "tokens": ["Auf", "das", "Volk", "her\u00b7ab\u00b7zu\u00b7ha\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und dadurch den Zuckerb\u00e4ckern,", "tokens": ["Und", "da\u00b7durch", "den", "Zu\u00b7cker\u00b7b\u00e4\u00b7ckern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Welche concessionirt sind,", "tokens": ["Wel\u00b7che", "con\u00b7ces\u00b7si\u00b7o\u00b7nirt", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "VVPP", "VAFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.21": {"text": "Frechlichst in's Gewerb' zu pfuschen,", "tokens": ["Frech\u00b7lichst", "in's", "Ge\u00b7werb'", "zu", "pfu\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und \u2013 da ", "tokens": ["Und", "\u2013", "da"], "token_info": ["word", "punct", "word"], "pos": ["KON", "$(", "KOUS"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Feiner, s\u00fc\u00dfer als die ihren", "tokens": ["Fei\u00b7ner", ",", "s\u00fc\u00b7\u00dfer", "als", "die", "ih\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KOKOM", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und herabgefallen gratis \u2013", "tokens": ["Und", "her\u00b7ab\u00b7ge\u00b7fal\u00b7len", "gra\u00b7tis", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Sie, die guten Zuckerb\u00e4cker", "tokens": ["Sie", ",", "die", "gu\u00b7ten", "Zu\u00b7cker\u00b7b\u00e4\u00b7cker"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "So an Nahrung, wie an Ehre", "tokens": ["So", "an", "Nah\u00b7rung", ",", "wie", "an", "Eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Zu besch\u00e4d'gen und verletzen:", "tokens": ["Zu", "be\u00b7sch\u00e4d'\u00b7gen", "und", "ver\u00b7let\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Widerigenfalls gen\u00f6thigt", "tokens": ["Wi\u00b7de\u00b7ri\u00b7gen\u00b7falls", "ge\u00b7n\u00f6t\u00b7higt"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Ich und fest entschlossen w\u00e4re", "tokens": ["Ich", "und", "fest", "ent\u00b7schlos\u00b7sen", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "ADJD", "VVPP", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn zwar, rief Ich z\u00fcrnend weiter", "tokens": ["Denn", "zwar", ",", "rief", "Ich", "z\u00fcr\u00b7nend", "wei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "VVPP", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das linke Ohr Gott ", "tokens": ["In", "das", "lin\u00b7ke", "Ohr", "Gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist Herr ", "tokens": ["Ist", "Herr"], "token_info": ["word", "word"], "pos": ["VAFIN", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und wer ehrte mehr als Mufti's", "tokens": ["Und", "wer", "ehr\u00b7te", "mehr", "als", "Muf\u00b7ti's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PIS", "KOKOM", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier die alte feste Ordnung?", "tokens": ["Hier", "die", "al\u00b7te", "fes\u00b7te", "Ord\u00b7nung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber das ist ", "tokens": ["A\u00b7ber", "das", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDS", "VAFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Pl\u00f6tziglich sich zu erfrechen", "tokens": ["Pl\u00f6t\u00b7zig\u00b7lich", "sich", "zu", "er\u00b7fre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das Gewohnte durchzubrechen,", "tokens": ["Das", "Ge\u00b7wohn\u00b7te", "durch\u00b7zu\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und zum Vortheil u