"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und wer ehrte mehr als Mufti's", "tokens": ["Und", "wer", "ehr\u00b7te", "mehr", "als", "Muf\u00b7ti's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PIS", "KOKOM", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier die alte feste Ordnung?", "tokens": ["Hier", "die", "al\u00b7te", "fes\u00b7te", "Ord\u00b7nung", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber das ist ", "tokens": ["A\u00b7ber", "das", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDS", "VAFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Pl\u00f6tziglich sich zu erfrechen", "tokens": ["Pl\u00f6t\u00b7zig\u00b7lich", "sich", "zu", "er\u00b7fre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das Gewohnte durchzubrechen,", "tokens": ["Das", "Ge\u00b7wohn\u00b7te", "durch\u00b7zu\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und zum Vortheil und Vergn\u00fcgen", "tokens": ["Und", "zum", "Vor\u00b7theil", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der gesammten Volkesmasse,", "tokens": ["Der", "ge\u00b7samm\u00b7ten", "Vol\u00b7kes\u00b7mas\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Welche ewig gier und leckern:", 