{"textgrid.poem.57393": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00d6fn', o Teutona's Genius, dem Deutschen die Augen,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00d6fn', o Teutona's Genius, dem Deutschen die Augen,", "tokens": ["\u00d6fn'", ",", "o", "Teu\u00b7to\u00b7na's", "Ge\u00b7nius", ",", "dem", "Deut\u00b7schen", "die", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NE", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+---+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dass er sehe den Gr\u00e4nzungskreis,", "tokens": ["Dass", "er", "se\u00b7he", "den", "Gr\u00e4n\u00b7zungs\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Den du machtest f\u00fcr ihre beneidete Bildsamkeit, liebend", "tokens": ["Den", "du", "mach\u00b7test", "f\u00fcr", "ih\u00b7re", "be\u00b7nei\u00b7de\u00b7te", "Bild\u00b7sam\u00b7keit", ",", "lie\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJD"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "In den gemessenen Raum sie riefst.", "tokens": ["In", "den", "ge\u00b7mes\u00b7se\u00b7nen", "Raum", "sie", "riefst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Weise massest du ihn; und mit Seele schaute dein Blick hin,", "tokens": ["Wei\u00b7se", "mas\u00b7sest", "du", "ihn", ";", "und", "mit", "See\u00b7le", "schau\u00b7te", "dein", "Blick", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$.", "KON", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Da du f\u00fchrtest den goldnen Stab.", "tokens": ["Da", "du", "f\u00fchr\u00b7test", "den", "gold\u00b7nen", "Stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "H\u00f6rst du mein Flehen; so feyr' ich dir ein Fest an dem Tage,", "tokens": ["H\u00f6rst", "du", "mein", "Fle\u00b7hen", ";", "so", "fey\u00b7r'", "ich", "dir", "ein", "Fest", "an", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Welcher den Deutschen Luthern gab;", "tokens": ["Wel\u00b7cher", "den", "Deut\u00b7schen", "Lu\u00b7thern", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Lasse des Auslands Sprachen vor dir in T\u00e4nzen vor\u00fcber", "tokens": ["Las\u00b7se", "des", "Aus\u00b7lands", "Spra\u00b7chen", "vor", "dir", "in", "T\u00e4n\u00b7zen", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "APPR", "PPER", "APPR", "NN", "PTKVZ"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Muthiger fliegen, oder gehn,", "tokens": ["Mut\u00b7hi\u00b7ger", "flie\u00b7gen", ",", "o\u00b7der", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Wenn ihr Leben sich senkt. Sie tanzen dann alle mir ihrer", "tokens": ["Wenn", "ihr", "Le\u00b7ben", "sich", "senkt", ".", "Sie", "tan\u00b7zen", "dann", "al\u00b7le", "mir", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PIS", "PPER", "PPOSAT"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Haltung und Mine, mit ihrem Schwung,", "tokens": ["Hal\u00b7tung", "und", "Mi\u00b7ne", ",", "mit", "ih\u00b7rem", "Schwung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Ingles, und Hesperide, und Galliett', und Teutona's", "tokens": ["Ing\u00b7les", ",", "und", "Hes\u00b7pe\u00b7ri\u00b7de", ",", "und", "Gal\u00b7li\u00b7ett'", ",", "und", "Teu\u00b7to\u00b7na's"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "KON", "NE", "$,", "KON", "NN", "$,", "KON", "NE"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.14": {"text": "Schwestern, mit ihr gleichaltes Stams.", "tokens": ["Schwes\u00b7tern", ",", "mit", "ihr", "gleich\u00b7al\u00b7tes", "Stams", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Fl\u00f6ten ert\u00f6nen! Ich seh den Genius schon in der Halle", "tokens": ["Fl\u00f6\u00b7ten", "er\u00b7t\u00f6\u00b7nen", "!", "Ich", "seh", "den", "Ge\u00b7nius", "schon", "in", "der", "Hal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.16": {"text": "Stehn, und der T\u00e4nzerinnen Flug", "tokens": ["Stehn", ",", "und", "der", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "Flug"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Mit dem Blicke begleiten. Den Sinn des Blickes besp\u00e4het", "tokens": ["Mit", "dem", "Bli\u00b7cke", "be\u00b7glei\u00b7ten", ".", "Den", "Sinn", "des", "Bli\u00b7ckes", "be\u00b7sp\u00e4\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Manche gewendete T\u00e4nzerin.", "tokens": ["Man\u00b7che", "ge\u00b7wen\u00b7de\u00b7te", "T\u00e4n\u00b7ze\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Denn sie haben geh\u00f6rt von Teutona's str\u00f6menden F\u00fclle,", "tokens": ["Denn", "sie", "ha\u00b7ben", "ge\u00b7h\u00f6rt", "von", "Teu\u00b7to\u00b7na's", "str\u00f6\u00b7men\u00b7den", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Und der lebenden tiefen Kraft,", "tokens": ["Und", "der", "le\u00b7ben\u00b7den", "tie\u00b7fen", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Die sie beseelet, geh\u00f6rt von dem Gr\u00e4nzungskreis', und dass weit sich", "tokens": ["Die", "sie", "be\u00b7see\u00b7let", ",", "ge\u00b7h\u00f6rt", "von", "dem", "Gr\u00e4n\u00b7zungs\u00b7kreis'", ",", "und", "dass", "weit", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "KOUS", "ADJD", "PRF"], "meter": "-+-+--+--+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Schwingen d\u00fcrfe die Bildsamkeit!", "tokens": ["Schwin\u00b7gen", "d\u00fcr\u00b7fe", "die", "Bild\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "Aber sie h\u00f6rten es halb nur; und vor dem bemerkenden f\u00fchren", "tokens": ["A\u00b7ber", "sie", "h\u00f6r\u00b7ten", "es", "halb", "nur", ";", "und", "vor", "dem", "be\u00b7mer\u00b7ken\u00b7den", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ADV", "$.", "KON", "APPR", "ART", "ADJA", "VVINF"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Sie mit Stolze den Reigen fort!", "tokens": ["Sie", "mit", "Stol\u00b7ze", "den", "Rei\u00b7gen", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Guter Genius, edler, ich weiss, wenn du l\u00e4chelst, und weiss es,", "tokens": ["Gu\u00b7ter", "Ge\u00b7nius", ",", "ed\u00b7ler", ",", "ich", "weiss", ",", "wenn", "du", "l\u00e4\u00b7chelst", ",", "und", "weiss", "es", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+---+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Wenn auf der Stirne du Tadel w\u00f6lkst:", "tokens": ["Wenn", "auf", "der", "Stir\u00b7ne", "du", "Ta\u00b7del", "w\u00f6lkst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Aber ich schweige. Die Zeit thut einst des bemerkenden Spruch kund;", "tokens": ["A\u00b7ber", "ich", "schwei\u00b7ge", ".", "Die", "Zeit", "thut", "einst", "des", "be\u00b7mer\u00b7ken\u00b7den", "Spruch", "kund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.28": {"text": "Und dann schwindet, was Blendung war.", "tokens": ["Und", "dann", "schwin\u00b7det", ",", "was", "Blen\u00b7dung", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "\u00d6fn', o Teutona's Genius, dem Deutschen die Augen,", "tokens": ["\u00d6fn'", ",", "o", "Teu\u00b7to\u00b7na's", "Ge\u00b7nius", ",", "dem", "Deut\u00b