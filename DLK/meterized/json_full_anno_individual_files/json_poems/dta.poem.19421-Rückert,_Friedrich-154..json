{"dta.poem.19421": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "154.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil' an den Gr\u00e4bern nur, und pflanze Rosenhecken!", "tokens": ["Weil'", "an", "den", "Gr\u00e4\u00b7bern", "nur", ",", "und", "pflan\u00b7ze", "Ro\u00b7sen\u00b7he\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So denkst du an den Tod, und er wird dich nicht schrecken. \u2014", "tokens": ["So", "denkst", "du", "an", "den", "Tod", ",", "und", "er", "wird", "dich", "nicht", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn dir ein lieber Freund hinweg gestorben ist,", "tokens": ["Wenn", "dir", "ein", "lie\u00b7ber", "Freund", "hin\u00b7weg", "ge\u00b7stor\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APZR", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denk: eine Tagereis' ist dieses Lebens Frist.", "tokens": ["Denk", ":", "ei\u00b7ne", "Ta\u00b7ge\u00b7reis'", "ist", "die\u00b7ses", "Le\u00b7bens", "Frist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ART", "NN", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun, dein Gef\u00e4hrte gieng ein Streckchen nur voraus,", "tokens": ["Nun", ",", "dein", "Ge\u00b7f\u00e4hr\u00b7te", "gieng", "ein", "Streck\u00b7chen", "nur", "vo\u00b7raus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um so fr\u00fcher ist er angelangt zu Haus.", "tokens": ["Und", "um", "so", "fr\u00fc\u00b7her", "ist", "er", "an\u00b7ge\u00b7langt", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "ADV", "ADJD", "VAFIN", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was klagest du, da\u00df ihn die Herberg' aufgenommen?", "tokens": ["Was", "kla\u00b7gest", "du", ",", "da\u00df", "ihn", "die", "Her\u00b7ber\u00b7g'", "auf\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Geh nur des Wegs getrost! Bald bist du nachgekommen.", "tokens": ["Geh", "nur", "des", "Wegs", "ge\u00b7trost", "!", "Bald", "bist", "du", "nach\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVPP", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}