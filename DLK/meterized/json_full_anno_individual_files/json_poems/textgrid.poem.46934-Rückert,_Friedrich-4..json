{"textgrid.poem.46934": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die ihr klebt an eurem Werkger\u00fcste,", "tokens": ["Ihr", ",", "die", "ihr", "klebt", "an", "eu\u00b7rem", "Werk\u00b7ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um Holz und Stein nach eurem Ma\u00df zu hauen,", "tokens": ["Um", "Holz", "und", "Stein", "nach", "eu\u00b7rem", "Ma\u00df", "zu", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit nur jeder lass' ein Werklein schauen,", "tokens": ["Da\u00b7mit", "nur", "je\u00b7der", "lass'", "ein", "Wer\u00b7klein", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich jeder nur als kleiner Sch\u00f6pfer br\u00fcste!", "tokens": ["Sich", "je\u00b7der", "nur", "als", "klei\u00b7ner", "Sch\u00f6p\u00b7fer", "br\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "ADV", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wann lasset ihr das th\u00f6richte Gel\u00fcste,", "tokens": ["Wann", "las\u00b7set", "ihr", "das", "th\u00f6\u00b7rich\u00b7t