{"textgrid.poem.33777": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Der ewige Spieler", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei T\u00fcrme noch, Wall und Graben,", "tokens": ["Zwei", "T\u00fcr\u00b7me", "noch", ",", "Wall", "und", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "NE", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So ragt eine Burg im Forst,", "tokens": ["So", "ragt", "ei\u00b7ne", "Burg", "im", "Forst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf ihrer Zinne haben", "tokens": ["Auf", "ih\u00b7rer", "Zin\u00b7ne", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Falken einen Horst,", "tokens": ["Die", "Fal\u00b7ken", "ei\u00b7nen", "Horst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und unten auf d\u00fcstern Schwellen", "tokens": ["Und", "un\u00b7ten", "auf", "d\u00fcs\u00b7tern", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der ewige Spieler haust,", "tokens": ["Der", "e\u00b7wi\u00b7ge", "Spie\u00b7ler", "haust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und mit ihm seine Gesellen,", "tokens": ["Und", "mit", "ihm", "sei\u00b7ne", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Karten in der Faust.", "tokens": ["Die", "Kar\u00b7ten", "in", "der", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit klirrendem Sporn, die rote", "tokens": ["Mit", "klir\u00b7ren\u00b7dem", "Sporn", ",", "die", "ro\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hahnfeder auf dem Hut,", "tokens": ["Hahn\u00b7fe\u00b7der", "auf", "dem", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So sitzen sie da, drei Tote,", "tokens": ["So", "sit\u00b7zen", "sie", "da", ",", "drei", "To\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "CARD", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Beim Schein der H\u00f6llenglut.", "tokens": ["Beim", "Schein", "der", "H\u00f6l\u00b7len\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So haben sie sonst im Leben", "tokens": ["So", "ha\u00b7ben", "sie", "sonst", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Beisammen gesessen sp\u00e4t,", "tokens": ["Bei\u00b7sam\u00b7men", "ge\u00b7ses\u00b7sen", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Karten gemischt und gegeben,", "tokens": ["Die", "Kar\u00b7ten", "ge\u00b7mischt", "und", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Noch fr\u00fch wenn der Hahn gekr\u00e4ht.", "tokens": ["Noch", "fr\u00fch", "wenn", "der", "Hahn", "ge\u00b7kr\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie haben die Kircht\u00fcr' erbrochen,", "tokens": ["Sie", "ha\u00b7ben", "die", "Kircht\u00fcr'", "er\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Darin gespielt, und dort", "tokens": ["Da\u00b7rin", "ge\u00b7spielt", ",", "und", "dort"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PAV", "VVPP", "$,", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat Einer den Andern erstochen,", "tokens": ["Hat", "Ei\u00b7ner", "den", "An\u00b7dern", "er\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und immer noch spielen sie fort.", "tokens": ["Und", "im\u00b7mer", "noch", "spie\u00b7len", "sie", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Die W\u00fcrfel und die Karten", "tokens": ["Die", "W\u00fcr\u00b7fel", "und", "die", "Kar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind h\u00e4rter als der Stein", "tokens": ["Sind", "h\u00e4r\u00b7ter", "als", "der", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und graben tiefe Scharten", "tokens": ["Und", "gra\u00b7ben", "tie\u00b7fe", "Schar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Tisch und Felsen ein.", "tokens": ["Dem", "Tisch", "und", "Fel\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da geht's an ein Trumpfen und Stechen", "tokens": ["Da", "geht's", "an", "ein", "Trump\u00b7fen", "und", "Ste\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit K\u00f6nig und Ober und As,", "tokens": ["Mit", "K\u00f6\u00b7nig", "und", "O\u00b7ber", "und", "As", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nur Fl\u00fcche die Lippen sprechen,", "tokens": ["Nur", "Fl\u00fc\u00b7che", "die", "Lip\u00b7pen", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Herzen nur gl\u00fchenden Ha\u00df.", "tokens": ["Die", "Her\u00b7zen", "nur", "gl\u00fc\u00b7hen\u00b7den", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Und tiefer brennen die Kerzen,", "tokens": ["Und", "tie\u00b7fer", "bren\u00b7nen", "die", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der ewige Spieler lacht:", "tokens": ["Der", "e\u00b7wi\u00b7ge", "Spie\u00b7ler", "lacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbes waren doch immer Herzen,", "tokens": ["\u00bb", "es", "wa\u00b7ren", "doch", "im\u00b7mer", "Her\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Womit ich Gl\u00fcck gemacht.", "tokens": ["Wo\u00b7mit", "ich", "Gl\u00fcck", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Herzen, die kleinen Toren,", "tokens": ["Die", "Her\u00b7zen", ",", "die", "klei\u00b7nen", "To\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Betrog ich allezeit,", "tokens": ["Be\u00b7trog", "ich", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt hab' ich alle verloren,", "tokens": ["Jetzt", "hab'", "ich", "al\u00b7le", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dazu die Seligkeit.\u00ab", "tokens": ["Da\u00b7zu", "die", "Se\u00b7lig\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PAV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drauf wirft er in lodernde Flammen", "tokens": ["Drauf", "wirft", "er", "in", "lo\u00b7dern\u00b7de", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Karten mit einem Fluch,", "tokens": ["Die", "Kar\u00b7ten", "mit", "ei\u00b7nem", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und \u00fcber ihm schl\u00e4gt zusammen", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", "schl\u00e4gt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PTKVZ"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein brennendes Leichentuch.", "tokens": ["Ein", "bren\u00b7nen\u00b7des", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "word", "w