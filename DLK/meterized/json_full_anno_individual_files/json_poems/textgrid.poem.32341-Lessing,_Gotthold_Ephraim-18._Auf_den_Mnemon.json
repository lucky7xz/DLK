{"textgrid.poem.32341": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "18. Auf den Mnemon", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Mnemon nicht ein seltner Mann!", "tokens": ["Ist", "Mne\u00b7mon", "nicht", "ein", "selt\u00b7ner", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie weit er sich zur\u00fcck erinnern kann!", "tokens": ["Wie", "weit", "er", "sich", "zu\u00b7r\u00fcck", "e\u00b7rin\u00b7nern", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "PTKVZ", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis an die ersten Kinderpossen:", "tokens": ["Bis", "an", "die", "ers\u00b7ten", "Kin\u00b7der\u00b7pos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel er V\u00f6gel abgeschossen,", "tokens": ["Wie", "viel", "er", "V\u00f6\u00b7gel", "ab\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie manches M\u00e4dchen er begossen;", "tokens": ["Wie", "man\u00b7ches", "M\u00e4d\u00b7chen", "er", "be\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis an das G\u00e4ngelband, bis an die Ammenbrust,", "tokens": ["Bis", "an", "das", "G\u00e4n\u00b7gel\u00b7band", ",", "bis", "an", "die", "Am\u00b7men\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist, was er litt und tat, ihm alles noch bewu\u00dft.", "tokens": ["Ist", ",", "was", "er", "litt", "und", "tat", ",", "ihm", "al\u00b7les", "noch", "be\u00b7wu\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zwar alles glaub' ich nicht; ich glaub' indessen,", "tokens": ["Zwar", "al\u00b7les", "glaub'", "ich", "nicht", ";", "ich", "glaub'", "in\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Zeit ist ihm noch unvergessen,", "tokens": ["Die", "Zeit", "ist", "ihm", "noch", "un\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als seine Mutter Dorilis", "tokens": ["Als", "sei\u00b7ne", "Mut\u00b7ter", "Do\u00b7ri\u00b7lis"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Noch nicht nach seinem Vater hie\u00df.", "tokens": ["Noch", "nicht", "nach", "sei\u00b7nem", "Va\u00b7ter", "