er": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Lust gelockt, mit Zorn erschr\u00f6ckt;", "tokens": ["Mit", "Lust", "ge\u00b7lockt", ",", "mit", "Zorn", "er\u00b7schr\u00f6ckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wer die Kr\u00e4fte jenes Lebens", "tokens": ["Denn", "wer", "die", "Kr\u00e4f\u00b7te", "je\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich in deiner Liebe schmeckt,", "tokens": ["Wie", "ich", "in", "dei\u00b7ner", "Lie\u00b7be", "schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hat sein unverg\u00e4nglich Heil.", "tokens": ["Der", "hat", "sein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was will ich mehr? Der Herr mein Theil.", "tokens": ["Was", "will", "ich", "mehr", "?", "Der", "Herr", "mein", "Theil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}