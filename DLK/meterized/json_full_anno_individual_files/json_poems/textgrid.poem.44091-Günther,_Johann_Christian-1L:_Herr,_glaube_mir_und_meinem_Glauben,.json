{"textgrid.poem.44091": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, glaube mir und meinem Glauben,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, glaube mir und meinem Glauben,", "tokens": ["Herr", ",", "glau\u00b7be", "mir", "und", "mei\u00b7nem", "Glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dich allein verehrt und kennt", "tokens": ["Der", "dich", "al\u00b7lein", "ver\u00b7ehrt", "und", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wider Satans Zorn und Schnauben", "tokens": ["Und", "wi\u00b7der", "Sa\u00b7tans", "Zorn", "und", "Schnau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deine Kraft im Stillen brennt:", "tokens": ["Durch", "dei\u00b7ne", "Kraft", "im", "Stil\u00b7len", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist, was in der gro\u00dfen Welt", "tokens": ["Du", "bist", ",", "was", "in", "der", "gro\u00b7\u00dfen", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir einzig und allein gef\u00e4llt.", "tokens": ["Mir", "ein\u00b7zig", "und", "al\u00b7lein", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich seh die Wercke deiner H\u00e4nde,", "tokens": ["Ich", "seh", "die", "Wer\u00b7cke", "dei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind vollkommen, sch\u00f6n und gut.", "tokens": ["Sie", "sind", "voll\u00b7kom\u00b7men", ",", "sch\u00f6n", "und", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ordnung all- und jeder St\u00e4nde", "tokens": ["Die", "Ord\u00b7nung", "all", "und", "je\u00b7der", "St\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "TRUNC", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beweist, was deine Vorsicht thut,", "tokens": ["Be\u00b7weist", ",", "was", "dei\u00b7ne", "Vor\u00b7sicht", "thut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch welche warlich nichts geschieht,", "tokens": ["Durch", "wel\u00b7che", "war\u00b7lich", "nichts", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als was uns Lust und Vortheil zieht.", "tokens": ["Als", "was", "uns", "Lust", "und", "Vor\u00b7theil", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast die Erde fest gegr\u00fcndet", "tokens": ["Du", "hast", "die", "Er\u00b7de", "fest", "ge\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lauter Wunder dargestellt,", "tokens": ["Und", "lau\u00b7ter", "Wun\u00b7der", "dar\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So da\u00df sich auch kein St\u00e4ubchen findet,", "tokens": ["So", "da\u00df", "sich", "auch", "kein", "St\u00e4ub\u00b7chen", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus welchem nicht dein Bild erhellt,", "tokens": ["Aus", "wel\u00b7chem", "nicht", "dein", "Bild", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Bild von deiner G\u00fct und Macht,", "tokens": ["Ein", "Bild", "von", "dei\u00b7ner", "G\u00fct", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nichts umsonst hervorgebracht.", "tokens": ["Die", "nichts", "um\u00b7sonst", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Himmel hat noch gr\u00f6\u00dfre Sch\u00e4ze", "tokens": ["Dein", "Him\u00b7mel", "hat", "noch", "gr\u00f6\u00df\u00b7re", "Sch\u00e4\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Wollust sonder Eitelkeit,", "tokens": ["Und", "Wol\u00b7lust", "son\u00b7der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die die Erl\u00f6sung vom Geseze", "tokens": ["Die", "die", "Er\u00b7l\u00f6\u00b7sung", "vom", "Ge\u00b7se\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Glauben schon in Hofnung beuth.", "tokens": ["Dem", "Glau\u00b7ben", "schon", "in", "Hof\u00b7nung", "beuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O unausprechlicher Genu\u00df", "tokens": ["O", "un\u00b7aus\u00b7prech\u00b7li\u00b7cher", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Freuden und von \u00dcberflu\u00df!", "tokens": ["Von", "Freu\u00b7den", "und", "von", "\u00dc\u00b7berf\u00b7lu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch zeigt der Himmel und die Erde", "tokens": ["Doch", "zeigt", "der", "Him\u00b7mel", "und", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir keine Lust so reizend an,", "tokens": ["Mir", "kei\u00b7ne", "Lust", "so", "rei\u00b7zend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wenn ich dir vereinigt werde", "tokens": ["Als", "wenn", "ich", "dir", "ver\u00b7ei\u00b7nigt", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und deiner Huld genie\u00dfen kan,", "tokens": ["Und", "dei\u00b7ner", "Huld", "ge\u00b7nie\u00b7\u00dfen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil da, wo du, o Gott, nicht bist,", "tokens": ["Weil", "da", ",", "wo", "du", ",", "o", "Gott", ",", "nicht", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "PWAV", "PPER", "$,", "FM", "NN", "$,", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel selbst kein Himmel ist.", "tokens": ["Der", "Him\u00b7mel", "selbst", "kein", "Him\u00b7mel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Versto\u00df mich, wenn du wilst, zur H\u00f6llen;", "tokens": ["Ver\u00b7sto\u00df", "mich", ",", "wenn", "du", "wilst", ",", "zur", "H\u00f6l\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe, so mich dir vermehlt,", "tokens": ["Die", "Lie\u00b7be", ",", "so", "mich", "dir", "ver\u00b7mehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vers\u00fc\u00dft des Feuers Schmerzensquellen,", "tokens": ["Ver\u00b7s\u00fc\u00dft", "des", "Feu\u00b7ers", "Schmer\u00b7zens\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das blos den b\u00f6sen Willen qu\u00e4lt;", "tokens": ["Das", "blos", "den", "b\u00f6\u00b7sen", "Wil\u00b7len", "qu\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erlaubstu mir dein Angesicht,", "tokens": ["Er\u00b7laubs\u00b7tu", "mir", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So f\u00fchl ich die Verdammn\u00fc\u00df nicht.", "tokens": ["So", "f\u00fchl", "ich", "die", "Ver\u00b7damm\u00b7n\u00fc\u00df", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Rache Bliz schl\u00e4gt ins Gewi\u00dfen,", "tokens": ["Der", "Ra\u00b7che", "Bliz", "schl\u00e4gt", "ins", "Ge\u00b7wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die S\u00fcnden \u00fcberdecken mich,", "tokens": ["Die", "S\u00fcn\u00b7den", "\u00fc\u00b7berd\u00b7e\u00b7cken", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele schreyt von stummen Bi\u00dfen", "tokens": ["Die", "See\u00b7le", "schreyt", "von", "stum\u00b7men", "Bi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00e4mpft bald vor, bald wider sich,", "tokens": ["Und", "k\u00e4mpft", "bald", "vor", ",", "bald", "wi\u00b7der", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mund verdorrt, die Zunge klebt,", "tokens": ["Der", "Mund", "ver\u00b7dorrt", ",", "die", "Zun\u00b7ge", "klebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil Marck und Bein vor Schr\u00f6cken bebt.", "tokens": ["Weil", "Marck", "und", "Bein", "vor", "Schr\u00f6\u00b7cken", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dies nennt ein Weltkind H\u00f6llenschmerzen,", "tokens": ["Dies", "nennt", "ein", "Welt\u00b7kind", "H\u00f6l\u00b7len\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich aber nenn es Seeligkeit;", "tokens": ["Ich", "a\u00b7ber", "nenn", "es", "See\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn du, o Herr, gew\u00e4hrst dem Herzen", "tokens": ["Denn", "du", ",", "o", "Herr", ",", "ge\u00b7w\u00e4hrst", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die z\u00e4rtlichste Gela\u00dfenheit", "tokens": ["Die", "z\u00e4rt\u00b7lichs\u00b7te", "Ge\u00b7la\u00b7\u00dfen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wirfst der innerlichen Ruh", "tokens": ["Und", "wirfst", "der", "in\u00b7ner\u00b7li\u00b7chen", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Qual und Trost viel Nachdruck zu.", "tokens": ["Durch", "Qual", "und", "Trost", "viel", "Nach\u00b7druck", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Vom Gl\u00fccke werd ich nur vergebens", "tokens": ["Vom", "Gl\u00fc\u00b7cke", "werd", "ich", "nur", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Lust gelockt, mit Zorn erschr\u00f6ckt;", "tokens": ["Mit", "Lust", "ge\u00b7lockt", ",", "mit", "Zorn", "er\u00b7schr\u00f6ckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wer die Kr\u00e4fte jenes Lebens", "tokens": ["Denn", "wer", "die", "Kr\u00e4f\u00b7te", "je\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich in deiner Liebe schmeckt,", "tokens": ["Wie", "ich", "in", "dei\u00b7ner", "Lie\u00b7be", "schmeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der hat sein unverg\u00e4nglich Heil.", "tokens": ["Der", "hat", "sein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was will ich mehr? Der Herr mein Theil.", "tokens": ["Was", "will", "ich", "mehr", "?", "Der", "Herr", "mein", "Theil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "