{"textgrid.poem.35838": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war in fr\u00fcher Stille", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war in fr\u00fcher Stille", "tokens": ["Es", "war", "in", "fr\u00fc\u00b7her", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beim ersten Lerchenlied,", "tokens": ["Beim", "ers\u00b7ten", "Ler\u00b7chen\u00b7lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als mir der ew'ge Wille", "tokens": ["Als", "mir", "der", "ew'\u00b7ge", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Wanderstab beschied.", "tokens": ["Den", "Wan\u00b7der\u00b7stab", "be\u00b7schied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sprang auf jungem Rasen,", "tokens": ["Ich", "sprang", "auf", "jun\u00b7gem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Wiesenblumen bl\u00fchn,", "tokens": ["Wo", "Wie\u00b7sen\u00b7blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo zarte L\u00e4mmlein grasen,", "tokens": ["Wo", "zar\u00b7te", "L\u00e4mm\u00b7lein", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein frohes M\u00e4gdlein, hin.", "tokens": ["Ein", "fro\u00b7hes", "M\u00e4gd\u00b7lein", ",", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es hatte Gottes G\u00fcte", "tokens": ["Es", "hat\u00b7te", "Got\u00b7tes", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir Kindessinn verliehn;", "tokens": ["Mir", "Kin\u00b7des\u00b7sinn", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah aus jeder Bl\u00fcthe", "tokens": ["Ich", "sah", "aus", "je\u00b7der", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ew'ge Liebe bl\u00fchn.", "tokens": ["Die", "ew'\u00b7ge", "Lie\u00b7be", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und B\u00e4um' und Bl\u00fcthenranken", "tokens": ["Und", "B\u00e4um'", "und", "Bl\u00fc\u00b7then\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erf\u00fcllten mich mit Lust,", "tokens": ["Er\u00b7f\u00fcll\u00b7ten", "mich", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Jauchzen nur und Danken", "tokens": ["Und", "Jauch\u00b7zen", "nur", "und", "Dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewegte mir die Brust. \u2013", "tokens": ["Be\u00b7weg\u00b7te", "mir", "die", "Brust", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch gift'ge Dunstgebilde", "tokens": ["Doch", "gift'\u00b7ge", "Dunst\u00b7ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In falscher Farben Schein,", "tokens": ["In", "fal\u00b7scher", "Far\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie bargen Seine Milde", "tokens": ["Sie", "bar\u00b7gen", "Sei\u00b7ne", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu bald den Blicken mein.", "tokens": ["Zu", "bald", "den", "Bli\u00b7cken", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich lauschte eitler Thoren", "tokens": ["Ich", "lauschte", "eit\u00b7ler", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verlockendem Gesang,", "tokens": ["Ver\u00b7lo\u00b7cken\u00b7dem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der den get\u00e4uschten Ohren", "tokens": ["Der", "den", "ge\u00b7t\u00e4uschten", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie Weisheitslehre klang.", "tokens": ["Wie", "Weis\u00b7heits\u00b7leh\u00b7re", "klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Des Stolzes b\u00f6ses W\u00e4hnen", "tokens": ["Des", "Stol\u00b7zes", "b\u00f6\u00b7ses", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umstrickte mir den Sinn:", "tokens": ["Um\u00b7strick\u00b7te", "mir", "den", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es trieb mich rastlos Sehnen", "tokens": ["Es", "trieb", "mich", "rast\u00b7los", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu schn\u00f6dem Wissen hin.", "tokens": ["Zu", "schn\u00f6\u00b7dem", "Wis\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da wich der Einfalt Taube,", "tokens": ["Da", "wich", "der", "Ein\u00b7falt", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ich so treulos war:", "tokens": ["Der", "ich", "so", "treu\u00b7los", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es floh der fromme Glaube", "tokens": ["Es", "floh", "der", "from\u00b7me", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lie\u00df mich trostesbar.", "tokens": ["Und", "lie\u00df", "mich", "tros\u00b7tes\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und weiter ging ich immer", "tokens": ["Und", "wei\u00b7ter", "ging", "ich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und suchte reines Licht", "tokens": ["Und", "such\u00b7te", "rei\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und folgte falschem Schimmer", "tokens": ["Und", "folg\u00b7te", "fal\u00b7schem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sah die Sonne nicht.", "tokens": ["Und", "sah", "die", "Son\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Bis fernes Glockenklingen", "tokens": ["Bis", "fer\u00b7nes", "Glo\u00b7cken\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meine Ohren schlug", "tokens": ["An", "mei\u00b7ne", "Oh\u00b7ren", "schlug"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und himmelreines Singen", "tokens": ["Und", "him\u00b7mel\u00b7rei\u00b7nes", "Sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Luft her\u00fcbertrug.", "tokens": ["Die", "Luft", "her\u00b7\u00fc\u00b7bert\u00b7rug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich wahrt' auf Felsenh\u00f6he", "tokens": ["Ich", "wahrt'", "auf", "Fel\u00b7sen\u00b7h\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kirche, ernst und alt;", "tokens": ["Die", "Kir\u00b7che", ",", "ernst", "und", "alt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es zog in ihre N\u00e4he", "tokens": ["Es", "zog", "in", "ih\u00b7re", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich heilige Gewalt.", "tokens": ["Mich", "hei\u00b7li\u00b7ge", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "In ihrer Mitte prangen", "tokens": ["In", "ih\u00b7rer", "Mit\u00b7te", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich der Sonne Schein \u2013", "tokens": ["Sah", "ich", "der", "Son\u00b7ne", "Schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bin ich eingegangen", "tokens": ["Da", "bin", "ich", "ein\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum treu'sten Vater mein.", "tokens": ["Zum", "treu'\u00b7sten", "Va\u00b7ter", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und hab' in hei\u00dfer Aschen", "tokens": ["Und", "hab'", "in", "hei\u00b7\u00dfer", "A\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und herber Thr\u00e4nenfluth", "tokens": ["Und", "her\u00b7ber", "Thr\u00e4\u00b7nen\u00b7fluth"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Pilgerkleid gewaschen", "tokens": ["Mein", "Pil\u00b7ger\u00b7kleid", "ge\u00b7wa\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und selig ausgeruht.", "tokens": ["Und", "se\u00b7lig", "aus\u00b7ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da war vom ew'gen Tode", "tokens": ["Da", "war", "vom", "ew'\u00b7gen", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Seele r\u00fcckgekehrt", "tokens": ["Die", "See\u00b7le", "r\u00fcck\u00b7ge\u00b7kehrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ward mit heil'gem Brode", "tokens": ["Und", "ward", "mit", "heil'\u00b7gem", "Bro\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am reinen Tisch gen\u00e4hrt.", "tokens": ["Am", "rei\u00b7nen", "Tisch", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und in der Kirche Garten,", "tokens": ["Und", "in", "der", "Kir\u00b7che", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf gr\u00fcnem Rasengrund", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Ra\u00b7sen\u00b7grund"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollt' ich der L\u00e4mmlein warten", "tokens": ["Sollt'", "ich", "der", "L\u00e4mm\u00b7lein", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl bis zur Abendstund'.", "tokens": ["Wohl", "bis", "zur", "A\u00b7bend\u00b7stund'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Es war in Mittagsstille,", "tokens": ["Es", "war", "in", "Mit\u00b7tags\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann hei\u00df die Sonne gl\u00fcht,", "tokens": ["Wann", "hei\u00df", "die", "Son\u00b7ne", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als mir der Liebe Wille", "tokens": ["Als", "mir", "der", "Lie\u00b7be", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Hirtenstab beschied.", "tokens": ["Den", "Hir\u00b7ten\u00b7stab", "be\u00b7schied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ich that auf duft'ge Matten,", "tokens": ["Ich", "that", "auf", "duft'\u00b7ge", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo klare B\u00e4chlein spr\u00fchn,", "tokens": ["Wo", "kla\u00b7re", "B\u00e4ch\u00b7lein", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo hohe Palmen schatten,", "tokens": ["Wo", "ho\u00b7he", "Pal\u00b7men", "schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4