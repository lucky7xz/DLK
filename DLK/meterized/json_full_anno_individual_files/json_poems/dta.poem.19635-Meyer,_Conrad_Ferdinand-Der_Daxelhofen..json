{"dta.poem.19635": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Daxelhofen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Den Hauptmann Daxelhofen", "tokens": ["Den", "Haupt\u00b7mann", "Da\u00b7xel\u00b7ho\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bestaunten in der Stadt Paris", "tokens": ["Be\u00b7staun\u00b7ten", "in", "der", "Stadt", "Pa\u00b7ris"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kinder und die Zofen", "tokens": ["Die", "Kin\u00b7der", "und", "die", "Zo\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um seines blonden Bartes Vlie\u00df \u2014", "tokens": ["Um", "sei\u00b7nes", "blon\u00b7den", "Bar\u00b7tes", "Vlie\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Prinz Cond", "tokens": ["Prinz", "Cond"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Der Hauptmann Daxelhofen auch,", "tokens": ["Der", "Haupt\u00b7mann", "Da\u00b7xel\u00b7ho\u00b7fen", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da fuhr am Bord der Schelde", "tokens": ["Da", "fuhr", "am", "Bord", "der", "Schel\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Blitz und quoll der Pulverrauch.", "tokens": ["Der", "Blitz", "und", "quoll", "der", "Pul\u00b7ver\u00b7rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Lilienbanner hoben", "tokens": ["Die", "Li\u00b7li\u00b7en\u00b7ban\u00b7ner", "ho\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich sachte weg aus Niederland", "tokens": ["Sich", "sach\u00b7te", "weg", "aus", "Nie\u00b7der\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schoben sich und schoben", "tokens": ["Und", "scho\u00b7ben", "sich", "und", "scho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tout doucement zum Rheinesstrand.", "tokens": ["Tout", "dou\u00b7ce\u00b7ment", "zum", "Rhei\u00b7nes\u00b7strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eherr Prinz, welch k\u00f6stlich D\u00fcften!", "tokens": ["\u201e", "herr", "Prinz", ",", "welch", "k\u00f6st\u00b7lich", "D\u00fcf\u00b7ten", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So duftet nur am Rhein der Wein!", "tokens": ["So", "duf\u00b7tet", "nur", "am", "Rhein", "der", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dort der Thurm in L\u00fcften,", "tokens": ["Und", "dort", "der", "Thurm", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Herr Prinz, das ist doch Mainz am Rhein?", "tokens": ["Herr", "Prinz", ",", "das", "ist", "doch", "Mainz", "am", "Rhein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PDS", "VAFIN", "ADV", "NE", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In meinem Pakt geschrieben", "tokens": ["In", "mei\u00b7nem", "Pakt", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht: Ewig nimmer gegens Reich!", "tokens": ["Steht", ":", "E\u00b7wig", "nim\u00b7mer", "ge\u00b7gens", "Reich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So steht's und ist geblieben", "tokens": ["So", "steht's", "und", "ist", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bleibt sich unverbr\u00fcchlich gleich!", "tokens": ["Und", "bleibt", "sich", "un\u00b7ver\u00b7br\u00fcch\u00b7lich", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin von Schwabenstamme,", "tokens": ["Ich", "bin", "von", "Schwa\u00b7ben\u00b7stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bin auch ein Eidgenosse gut,", "tokens": ["Bin", "auch", "ein", "Eid\u00b7ge\u00b7nos\u00b7se", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df mich Gott verdamme,", "tokens": ["Und", "da\u00df", "mich", "Gott", "ver\u00b7dam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergie\u00df ich Deutscher deutsches Blut!", "tokens": ["Ver\u00b7gie\u00df", "ich", "Deut\u00b7scher", "deut\u00b7sches", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In Mainz als Feind zu r\u00fccken", "tokens": ["In", "Mainz", "als", "Feind", "zu", "r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KOUS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rei\u00dft mich kein H\u00f6llenteufel fort,", "tokens": ["Rei\u00dft", "mich", "kein", "H\u00f6l\u00b7len\u00b7teu\u00b7fel", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Betret' ich dort die Br\u00fccken,", "tokens": ["Be\u00b7tret'", "ich", "dort", "die", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sei mir Hand und Schlund verdorrt!", "tokens": ["So", "sei", "mir", "Hand", "und", "Schlund", "ver\u00b7dorrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht d\u00fcrft' ich mich bezechen", "tokens": ["Nicht", "d\u00fcrft'", "ich", "mich", "be\u00b7ze\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit frommen Christenleuten mehr!", "tokens": ["Mit", "from\u00b7men", "Chris\u00b7ten\u00b7leu\u00b7ten", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Waffen lieber brechen,", "tokens": ["Mein", "Waf\u00b7fen", "lie\u00b7ber", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als brechen Eid und Mannesehr!\u201c", "tokens": ["Als", "bre\u00b7chen", "Eid", "und", "Man\u00b7ne\u00b7sehr", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201ela, la\u201c, kirrt Cond", "tokens": ["\u201e", "la", ",", "la", "\u201c", ",", "kirrt", "Cond"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["$(", "FM", "$,", "FM", "$(", "$,", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dient Ihr um Doppel-Tripellohn.\u201c", "tokens": ["Dient", "Ihr", "um", "Dop\u00b7pel\u00b7Tri\u00b7pel\u00b7lohn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da bricht vorm Knie der Berner", "tokens": ["Da", "bricht", "vorm", "Knie", "der", "Ber\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In St\u00fccke krachend sein Sponton,", "tokens": ["In", "St\u00fc\u00b7cke", "kra\u00b7chend", "sein", "Spon\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem Prinzen wirft zu F\u00fc\u00dfen", "tokens": ["Dem", "Prin\u00b7zen", "wirft", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die beiden Tr\u00fcmmer er und spricht:", "tokens": ["Die", "bei\u00b7den", "Tr\u00fcm\u00b7mer", "er", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eden K\u00f6nig la\u00df ich gr\u00fc\u00dfen,", "tokens": ["\u201e", "den", "K\u00f6\u00b7nig", "la\u00df", "ich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das deutsche Reich befehd' ich nicht!\u201c", "tokens": ["Das", "deut\u00b7sche", "Reich", "be\u00b7fehd'", "ich", "nicht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}