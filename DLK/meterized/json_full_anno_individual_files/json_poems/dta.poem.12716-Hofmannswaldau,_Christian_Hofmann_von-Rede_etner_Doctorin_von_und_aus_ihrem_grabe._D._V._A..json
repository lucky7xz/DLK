{"dta.poem.12716": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Rede etner Doctorin von und aus  \n ihrem grabe.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer steht mein eigner sarg bey meines liebsten schrein:", "tokens": ["Hjer", "steht", "mein", "eig\u00b7ner", "sarg", "bey", "mei\u00b7nes", "liebs\u00b7ten", "schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir trotzen selbst den tod: der himmel paart die seelen,", "tokens": ["Wir", "trot\u00b7zen", "selbst", "den", "tod", ":", "der", "him\u00b7mel", "paart", "die", "see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hier ist leib bey leib in einer grabes-h\u00f6len:", "tokens": ["Und", "hier", "ist", "leib", "bey", "leib", "in", "ei\u00b7ner", "gra\u00b7bes\u00b7h\u00f6len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "APPR", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df auch unsre grufft ein ehe-bette seyn.", "tokens": ["So", "mu\u00df", "auch", "uns\u00b7re", "grufft", "ein", "e\u00b7he\u00b7bet\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "po