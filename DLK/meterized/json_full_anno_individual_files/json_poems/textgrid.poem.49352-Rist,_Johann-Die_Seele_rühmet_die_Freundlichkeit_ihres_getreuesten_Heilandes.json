{"textgrid.poem.49352": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Die Seele r\u00fchmet die Freundlichkeit ihres getreuesten Heilandes", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O freundlicher, o s\u00fc\u00dfer,", "tokens": ["O", "freund\u00b7li\u00b7cher", ",", "o", "s\u00fc\u00b7\u00dfer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O theurer Jesu Christ,", "tokens": ["O", "theu\u00b7rer", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Held, du S\u00fcndenb\u00fc\u00dfer,", "tokens": ["Du", "Held", ",", "du", "S\u00fcn\u00b7den\u00b7b\u00fc\u00b7\u00dfer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du so g\u00fctig bist,", "tokens": ["Da\u00df", "du", "so", "g\u00fc\u00b7tig", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das ist im Leidensorden", "tokens": ["Das", "ist", "im", "Lei\u00b7den\u00b7sor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir kl\u00e4rlich kund geworden;", "tokens": ["Mir", "kl\u00e4r\u00b7lich", "kund", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hoch hast du mich geliebt,", "tokens": ["Hoch", "hast", "du", "mich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als ich war hoch betr\u00fcbt!", "tokens": ["Als", "ich", "war", "hoch", "be\u00b7tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wie herlich hat erquicket", "tokens": ["Wie", "her\u00b7lich", "hat", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Dein Trost mein mattes Herz,", "tokens": ["Dein", "Trost", "mein", "mat\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Als solches hat ersticket", "tokens": ["Als", "sol\u00b7ches", "hat", "er\u00b7sti\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VAFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ein mehr denn Todesschmerz!", "tokens": ["Ein", "mehr", "denn", "To\u00b7des\u00b7schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie wol hat deine G\u00fcte", "tokens": ["Wie", "wol", "hat", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Befriedigt mein Gem\u00fcte,", "tokens": ["Be\u00b7frie\u00b7digt", "mein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Da\u00df st\u00fcndlich ich daran", "tokens": ["Da\u00df", "st\u00fcnd\u00b7lich", "ich", "da\u00b7ran"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Mit Lust gedenken kan!", "tokens": ["Mit", "Lust", "ge\u00b7den\u00b7ken", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Je mehr ichs nun betrachte,", "tokens": ["Je", "mehr", "ichs", "nun", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Je freundlicher du bist,", "tokens": ["Je", "freund\u00b7li\u00b7cher", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Je h\u00f6her ich die\u00df achte,", "tokens": ["Je", "h\u00f6\u00b7her", "ich", "die\u00df", "ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PDS", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Je mehr zur jeden Frist", "tokens": ["Je", "mehr", "zur", "je\u00b7den", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Empfind' ich deine Liebe;", "tokens": ["Emp\u00b7find'", "ich", "dei\u00b7ne", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Hilf, da\u00df auch ich mich \u00fcbe,", "tokens": ["Hilf", ",", "da\u00df", "auch", "ich", "mich", "\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "So fest zu lieben dich,", "tokens": ["So", "fest", "zu", "lie\u00b7ben", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Wie du, Herr, liebest mich!", "tokens": ["Wie", "du", ",", "Herr", ",", "lie\u00b7best", "mich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wie herlich sind die Gaben,", "tokens": ["Wie", "her\u00b7lich", "sind", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Die du bereitest mir;", "tokens": ["Die", "du", "be\u00b7rei\u00b7test", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wie gern wolt' ich dich laben,", "tokens": ["Wie", "gern", "wolt'", "ich", "dich", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "O treuer Gott, bei dir!", "tokens": ["O", "treu\u00b7er", "Gott", ",", "bei", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Hab' Acht auf meine Thr\u00e4nen,", "tokens": ["Hab'", "Acht", "auf", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Sie zeugen, da\u00df mein Sehnen,", "tokens": ["Sie", "zeu\u00b7gen", ",", "da\u00df", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Mein W\u00fcnschen, mein Geschrei", "tokens": ["Mein", "W\u00fcn\u00b7schen", ",", "mein", "Ge\u00b7schrei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Zu dir gerichtet sei.", "tokens": ["Zu", "dir", "ge\u00b7rich\u00b7tet", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Immittelst da\u00df ich wohnen", "tokens": ["Im\u00b7mit\u00b7telst", "da\u00df", "ich", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Mu\u00df in dem Leibe noch,", "tokens": ["Mu\u00df", "in", "dem", "Lei\u00b7be", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.35": {"text": "Den zwar noch selten schonen", "tokens": ["Den", "zwar", "noch", "sel\u00b7ten", "scho\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Der Tod wil w\u00fcrgen; doch", "tokens": ["Der", "Tod", "wil", "w\u00fcr\u00b7gen", ";", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "So stillet all mein Leiden", "tokens": ["So", "stil\u00b7let", "all", "mein", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Die Hoffnung solcher Freuden,", "tokens": ["Die", "Hoff\u00b7nung", "sol\u00b7cher", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Worauf ich Tag und Nacht", "tokens": ["Wo\u00b7rauf", "ich", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Bin inniglich bedacht.", "tokens": ["Bin", "in\u00b7nig\u00b7lich", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.41": {"text": "Wenn werd' ich zu dir kommen,", "tokens": ["Wenn", "werd'", "ich", "zu", "dir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Mein Helfer, der du mir", "tokens": ["Mein", "Hel\u00b7fer", ",", "der", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Das Herz so gar genommen,", "tokens": ["Das", "Herz", "so", "gar", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Da\u00df ich verschmachte schier,", "tokens": ["Da\u00df", "ich", "ver\u00b7schmach\u00b7te", "schier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Eh' ich auf mein Vertrauen", "tokens": ["Eh'", "ich", "auf", "mein", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Dein' Herlichkeit kan schauen;", "tokens": ["Dein'", "Her\u00b7lich\u00b7keit", "kan", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Ach Herr, wenn wird's geschehn,", "tokens": ["Ach", "Herr", ",", "wenn", "wird's", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Da\u00df ich f\u00fcr dir sol stehn?", "tokens": ["Da\u00df", "ich", "f\u00fcr", "dir", "sol", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Herr, la\u00df mich allzeit munter", "tokens": ["Herr", ",", "la\u00df", "mich", "all\u00b7zeit", "mun\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Zu deinem Lobe sein;", "tokens": ["Zu", "dei\u00b7nem", "Lo\u00b7be", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Send' eiligst doch herunter", "tokens": ["Send'", "ei\u00b7ligst", "doch", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APZR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Des Geistes Kraft allein,", "tokens": ["Des", "Geis\u00b7tes", "Kraft", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Da\u00df ich mit s\u00fc\u00dfen Weisen", "tokens": ["Da\u00df", "ich", "mit", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Dich m\u00f6g' ohn Ende preisen,", "tokens": ["Dich", "m\u00f6g'", "ohn", "En\u00b7de", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Denn du thust f\u00fcr und f\u00fcr", "tokens": ["Denn", "du", "thust", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.56": {"text": "Sehr gro\u00dfe Ding' an mir.", "tokens": ["Sehr", "gro\u00b7\u00dfe", "Ding'", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "La\u00df mich mein Herz erheben", "tokens": ["La\u00df", "mich", "mein", "Herz", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Von diesem Erdenklo\u00df,", "tokens": ["Von", "die\u00b7sem", "Er\u00b7den\u00b7klo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Auf da\u00df ich m\u00fcge leben", "tokens": ["Auf", "da\u00df", "ich", "m\u00fc\u00b7ge", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Bei dir, und hier nur blo\u00df", "tokens": ["Bei", "dir", ",", "und", "hier", "nur", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.61": {"text": "Dasselbe vollenbringen,", "tokens": ["Das\u00b7sel\u00b7be", "vol\u00b7len\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Was du f\u00fcr allen Dingen", "tokens": ["Was", "du", "f\u00fcr", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Zu thun mir auferlegt,", "tokens": ["Zu", "thun", "mir", "auf\u00b7er\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.64": {"text": "Das Fried' im Herzen hegt.", "tokens": ["Das", "Fried'", "im", "Her\u00b7zen", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.65": {"text": "O freundlicher, o sch\u00f6ner,", "tokens": ["O", "freund\u00b7li\u00b7cher", ",", "o", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "FM", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "O s\u00fc\u00dfer Jesu Christ,", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.67": {"text": "O Heiland, o Vers\u00f6hner,", "tokens": ["O", "Hei\u00b7land", ",", "o", "Ver\u00b7s\u00f6h\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Der du so lieblich bist,", "tokens": ["Der", "du", "so", "lieb\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.69": {"text": "Da\u00df es kein Mensch kan fassen,", "tokens": ["Da\u00df", "es", "kein", "Mensch", "kan", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "Hilf, da\u00df, wenn ich mu\u00df lassen", "tokens": ["Hilf", ",", "da\u00df", ",", "wenn", "ich", "mu\u00df", "las\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "$,", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Die\u00df Haus voll Angst und Pein,", "tokens": ["Die\u00df", "Haus", "voll", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.72": {"text": "Ich schnell bei dir mag sein.", "tokens": ["Ich", "schnell", "bei", "dir", "mag", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O freundlicher, o s\u00fc\u00dfer,", "tokens": ["O", "freund\u00b7li\u00b7cher", ",", "o", "s\u00fc\u00b7\u00dfer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O theurer Jesu Christ,", "tokens": ["O", "theu\u00b7rer", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Held, du S\u00fcndenb\u00fc\u00dfer,", "tokens": ["Du", "Held", ",", "du", "S\u00fcn\u00b7den\u00b7b\u00fc\u00b7\u00dfer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du so g\u00fctig bist,", "tokens": ["Da\u00df", "du", "so", "g\u00fc\u00b7tig", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das ist im Leidensorden", "tokens": ["Das", "ist", "im", "Lei\u00b7den\u00b7sor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir kl\u00e4rlich kund geworden;", "tokens": ["Mir", "kl\u00e4r\u00b7lich", "kund", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hoch hast du mich geliebt,", "tokens": ["Hoch", "hast", "du", "mich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als ich war hoch betr\u00fcbt!", "tokens": ["Als", "ich", "war", "hoch", "be\u00b7tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wie herlich hat erquicket", "tokens": ["Wie", "her\u00b7lich", "hat", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Dein Trost mein mattes Herz,", "tokens": ["Dein", "Trost", "mein", "mat\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Als solches hat ersticket", "tokens": ["Als", "sol\u00b7ches", "hat", "er\u00b7sti\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VAFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ein mehr denn Todesschmerz!", "tokens": ["Ein", "mehr", "denn", "To\u00b7des\u00b7schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie wol hat deine G\u00fcte", "tokens": ["Wie", "wol", "hat", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Befriedigt mein Gem\u00fcte,", "tokens": ["Be\u00b7frie\u00b7digt", "mein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Da\u00df st\u00fcndlich ich daran", "tokens": ["Da\u00df", "st\u00fcnd\u00b7lich", "ich", "da\u00b7ran"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Mit Lust gedenken kan!", "tokens": ["Mit", "Lust", "ge\u00b7den\u00b7ken", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Je mehr ichs nun betrachte,", "tokens": ["Je", "mehr", "ichs", "nun", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Je freundlicher du bist,", "tokens": ["Je", "freund\u00b7li\u00b7cher", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Je h\u00f6her ich die\u00df achte,", "tokens": ["Je", "h\u00f6\u00b7her", "ich", "die\u00df", "ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PDS", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Je mehr zur jeden Frist", "tokens": ["Je", "mehr", "zur", "je\u00b7den", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Empfind' ich deine Liebe;", "tokens": ["Emp\u00b7find'", "ich", "dei\u00b7ne", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Hilf, da\u00df auch ich mich \u00fcbe,", "tokens": ["Hilf", ",", "da\u00df", "auch", "ich", "mich", "\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "So fest zu lieben dich,", "tokens": ["So", "fest", "zu", "lie\u00b7ben", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Wie du, Herr, liebest mich!", "tokens": ["Wie", "du", ",", "Herr", ",", "lie\u00b7best", "mich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wie herlich sind die Gaben,", "tokens": ["Wie", "her\u00b7lich", "sind", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Die du bereitest mir;", "tokens": ["Die", "du", "be\u00b7rei\u00b7test", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wie gern wolt' ich dich laben,", "tokens": ["Wie", "gern", "wolt'", "ich", "dich", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "O treuer Gott, bei dir!", "tokens": ["O", "treu\u00b7er", "Gott", ",", "bei", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Hab' Acht auf meine Thr\u00e4nen,", "tokens": ["Hab'", "Acht", "auf", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Sie zeugen, da\u00df mein Sehnen,", "tokens": ["Sie", "zeu\u00b7gen", ",", "da\u00df", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Mein W\u00fcnschen, mein Geschrei", "tokens": ["Mein", "W\u00fcn\u00b7schen", ",", "mein", "Ge\u00b7schrei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Zu dir gerichtet sei.", "tokens": ["Zu", "dir", "ge\u00b7rich\u00b7tet", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Immittelst da\u00df ich wohnen", "tokens": ["Im\u00b7mit\u00b7telst", "da\u00df", "ich", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Mu\u00df in dem Leibe noch,", "tokens": ["Mu\u00df", "in", "dem", "Lei\u00b7be", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.35": {"text": "Den zwar noch selten schonen", "tokens": ["Den", "zwar", "noch", "sel\u00b7ten", "scho\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Der Tod wil w\u00fcrgen; doch", "tokens": ["Der", "Tod", "wil", "w\u00fcr\u00b7gen", ";", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "So stillet all mein Leiden", "tokens": ["So", "stil\u00b7let", "all", "mein", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Die Hoffnung solcher Freuden,", "tokens": ["Die", "Hoff\u00b7nung", "sol\u00b7cher", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Worauf ich Tag und Nacht", "tokens": ["Wo\u00b7rauf", "ich", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Bin inniglich bedacht.", "tokens": ["Bin", "in\u00b7nig\u00b7lich", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.41": {"text": "Wenn werd' ich zu dir kommen,", "tokens": ["Wenn", "werd'", "ich", "zu", "dir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Mein Helfer, der du mir", "tokens": ["Mein", "Hel\u00b7fer", ",", "der", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Das Herz so gar genommen,", "tokens": ["Das", "Herz", "so", "gar", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Da\u00df ich verschmachte schier,", "tokens": ["Da\u00df", "ich", "ver\u00b7schmach\u00b7te", "schier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Eh' ich auf mein Vertrauen", "tokens": ["Eh'", "ich", "auf", "mein", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Dein' Herlichkeit kan schauen;", "tokens": ["Dein'", "Her\u00b7lich\u00b7keit", "kan", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Ach Herr, wenn wird's geschehn,", "tokens": ["Ach", "Herr", ",", "wenn", "wird's", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.48": {"text": "Da\u00df ich f\u00fcr dir sol stehn?", "tokens": ["Da\u00df", "ich", "f\u00fcr", "dir", "sol", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Herr, la\u00df mich allzeit munter", "tokens": ["Herr", ",", "la\u00df", "mich", "all\u00b7zeit", "mun\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Zu deinem Lobe sein;", "tokens": ["Zu", "dei\u00b7nem", "Lo\u00b7be", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Send' eiligst doch herunter", "tokens": ["Send'", "ei\u00b7ligst", "doch", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APZR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Des Geistes Kraft allein,", "tokens": ["Des", "Geis\u00b7tes", "Kraft", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Da\u00df ich mit s\u00fc\u00dfen Weisen", "tokens": ["Da\u00df", "ich", "mit", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Dich m\u00f6g' ohn Ende preisen,", "tokens": ["Dich", "m\u00f6g'", "ohn", "En\u00b7de", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Denn du thust f\u00fcr und f\u00fcr", "tokens": ["Denn", "du", "thust", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.56": {"text": "Sehr gro\u00dfe Ding' an mir.", "tokens": ["Sehr", "gro\u00b7\u00dfe", "Ding'", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "La\u00df mich mein Herz erheben", "tokens": ["La\u00df", "mich", "mein", "Herz", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Von diesem Erdenklo\u00df,", "tokens": ["Von", "die\u00b7sem", "Er\u00b7den\u00b7klo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Auf da\u00df ich m\u00fcge leben", "tokens": ["Auf", "da\u00df", "ich", "m\u00fc\u00b7ge", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Bei dir, und hier nur blo\u00df", "tokens": ["Bei", "dir", ",", "und", "hier", "nur", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.61": {"text": "Dasselbe vollenbringen,", "tokens": ["Das\u00b7sel\u00b7be", "vol\u00b7len\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Was du f\u00fcr allen Dingen", "tokens": ["Was", "du", "f\u00fcr", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Zu thun mir auferlegt,", "tokens": ["Zu", "thun", "mir", "auf\u00b7er\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.64": {"text": "Das Fried' im Herzen hegt.", "tokens": ["Das", "Fried'", "im", "Her\u00b7zen", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.65": {"text": "O freundlicher, o sch\u00f6ner,", "tokens": ["O", "freund\u00b7li\u00b7cher", ",", "o", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "FM", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "O s\u00fc\u00dfer Jesu Christ,", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.67": {"text": "O Heiland, o Vers\u00f6hner,", "tokens": ["O", "Hei\u00b7land", ",", "o", "Ver\u00b7s\u00f6h\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Der du so lieblich bist,", "tokens": ["Der", "du", "so", "lieb\u00b7lich", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.69": {"text": "Da\u00df es kein Mensch kan fassen,", "tokens": ["Da\u00df", "es", "kein", "Mensch", "kan", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "Hilf, da\u00df, wenn ich mu\u00df lassen", "tokens": ["Hilf", ",", "da\u00df", ",", "wenn", "ich", "mu\u00df", "las\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "$,", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Die\u00df Haus voll Angst und Pein,", "tokens": ["Die\u00df", "Haus", "voll", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.72": {"text": "Ich schnell bei dir mag sein.", "tokens": ["Ich", "schnell", "bei", "dir", "mag", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}