 "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sie dulden stets ohne Schuld,", "tokens": ["Sie", "dul\u00b7den", "stets", "oh\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den Leiden sie vertrauen,", "tokens": ["Den", "Lei\u00b7den", "sie", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie naher Himmelshuld.", "tokens": ["Wie", "na\u00b7her", "Him\u00b7mels\u00b7huld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Also sie ist mir blieben,", "tokens": ["Al\u00b7so", "sie", "ist", "mir", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Nie sch\u00f6ner ich je sie schau,", "tokens": ["Nie", "sch\u00f6\u00b7ner", "ich", "je", "sie", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PPER", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kein Zeichen konnt' betr\u00fcben", "tokens": ["Kein", "Zei\u00b7chen", "konnt'", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die unschuldvolle Frau;", "tokens": ["Die", "un\u00b7schuld\u00b7vol\u00b7le", "Frau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "W\u00e4r' noch ein Mensch auf Erden,", "tokens": ["W\u00e4r'", "noch", "ein", "Mensch", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So mahlt' ich ihr Gesicht,", "tokens": ["So", "mahlt'", "ich", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch es soll keinem werden,", "tokens": ["Doch", "es", "soll", "kei\u00b7nem", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Weidenast mir bricht.", "tokens": ["Mein", "Wei\u00b7de\u00b7nast", "mir", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}