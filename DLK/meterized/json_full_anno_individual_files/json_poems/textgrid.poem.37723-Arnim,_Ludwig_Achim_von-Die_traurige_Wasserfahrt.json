{"textgrid.poem.37723": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die traurige Wasserfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sa\u00df ich wie jetzt auf der Weide", "tokens": ["Sa\u00df", "ich", "wie", "jetzt", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl \u00fcber den blanken Teich,", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "blan\u00b7ken", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo ich die Pfeifen schneide,", "tokens": ["Wo", "ich", "die", "Pfei\u00b7fen", "schnei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lang' ich Rohr erreich;", "tokens": ["So", "lang'", "ich", "Rohr", "er\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Abend mit kaltem Fl\u00fcgel", "tokens": ["A\u00b7bend", "mit", "kal\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Das letzte Rohr mir bricht,", "tokens": ["Das", "letz\u00b7te", "Rohr", "mir", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bestreicht den Himmelsspiegel", "tokens": ["Be\u00b7streicht", "den", "Him\u00b7mels\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und auch mein Angesicht.", "tokens": ["Und", "auch", "mein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mitten aus Nebelh\u00fclle", "tokens": ["Mit\u00b7ten", "aus", "Ne\u00b7bel\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Drang damals ein goldnes Schiff,", "tokens": ["Drang", "da\u00b7mals", "ein", "gold\u00b7nes", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Rings lastet fl\u00fcsternde Stille,", "tokens": ["Rings", "las\u00b7tet", "fl\u00fcs\u00b7tern\u00b7de", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf Wiesen ein Schnarrvogel pfiff,", "tokens": ["Auf", "Wie\u00b7sen", "ein", "Schnarr\u00b7vo\u00b7gel", "pfiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wollte doch nichts gelingen,", "tokens": ["Woll\u00b7te", "doch", "nichts", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Im Schiffe sie blieben so still,", "tokens": ["Im", "Schif\u00b7fe", "sie", "blie\u00b7ben", "so", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Drei V\u00f6lker drinn sa\u00dfen zum Singen,", "tokens": ["Drei", "V\u00f6l\u00b7ker", "drinn", "sa\u00b7\u00dfen", "zum", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Doch jedes was anderes will.", "tokens": ["Doch", "je\u00b7des", "was", "an\u00b7de\u00b7res", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PWS", "PIS", "VMFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Gr\u00fcn war das Volk, das eine,", "tokens": ["Gr\u00fcn", "war", "das", "Volk", ",", "das", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das andere war schon ganz roth,", "tokens": ["Das", "an\u00b7de\u00b7re", "war", "schon", "ganz", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und blau, so ging das meine,", "tokens": ["Und", "blau", ",", "so", "ging", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "VVFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beschirmten der K\u00f6nigin Noth;", "tokens": ["Be\u00b7schirm\u00b7ten", "der", "K\u00f6\u00b7ni\u00b7gin", "Noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Seufzten so tief die Ruder,", "tokens": ["Seufz\u00b7ten", "so", "tief", "die", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sang droben ein Kranichzug;", "tokens": ["Sang", "dro\u00b7ben", "ein", "Kra\u00b7nich\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bbkomm mit, du armer Bruder,", "tokens": ["\u00bb", "komm", "mit", ",", "du", "ar\u00b7mer", "Bru\u00b7der", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hier bleiben schon genug.\u00ab", "tokens": ["Hier", "blei\u00b7ben", "schon", "ge\u00b7nug", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Stritten sich jene noch immer,", "tokens": ["Strit\u00b7ten", "sich", "je\u00b7ne", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PDS", "ADV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die himmlische K\u00f6nigin,", "tokens": ["Die", "himm\u00b7li\u00b7sche", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sah fr\u00f6hlich in die Schimmer", "tokens": ["Sah", "fr\u00f6h\u00b7lich", "in", "die", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sah nicht auf Zeichen hin,", "tokens": ["Und", "sah", "nicht", "auf", "Zei\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sah ich's doch an dem Gr\u00fcnen,", "tokens": ["Sah", "ich's", "doch", "an", "dem", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es schwankt und knickt wie Rohr,", "tokens": ["Es", "schwankt", "und", "knickt", "wie", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Roth war auch verschienen,", "tokens": ["Das", "Roth", "war", "auch", "ver\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Himmelsbl\u00e4u hielt vor.", "tokens": ["Die", "Him\u00b7mels\u00b7bl\u00e4u", "hielt", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Also vom bunten Bogen", "tokens": ["Al\u00b7so", "vom", "bun\u00b7ten", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Die K\u00f6nigin sa\u00df umringt,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "sa\u00df", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie Regen kommt durchgezogen,", "tokens": ["Wie", "Re\u00b7gen", "kommt", "durch\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Nachen im Dunkel singt:", "tokens": ["Ein", "Na\u00b7chen", "im", "Dun\u00b7kel", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "\u00bbdumpfig und schwermuthvoll Gel\u00e4ute,", "tokens": ["\u00bb", "dump\u00b7fig", "und", "schwer\u00b7muth\u00b7voll", "Ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Du einzige letzte Gab',", "tokens": ["Du", "ein\u00b7zi\u00b7ge", "letz\u00b7te", "Gab'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dein warten junge Br\u00e4ute,", "tokens": ["Dein", "war\u00b7ten", "jun\u00b7ge", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Br\u00e4utigam hat kein Grab.\u00ab", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "hat", "kein", "Grab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NE", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Keiner ihn wagt zu st\u00f6ren,", "tokens": ["Kei\u00b7ner", "ihn", "wagt", "zu", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Doch wird es da allen kalt,", "tokens": ["Doch", "wird", "es", "da", "al\u00b7len", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur sie kann ruhig h\u00f6ren,", "tokens": ["Nur", "sie", "kann", "ru\u00b7hig", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was traurig sch\u00f6n erschallt;", "tokens": ["Was", "trau\u00b7rig", "sch\u00f6n", "er\u00b7schallt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Himmlisches Loos der Frauen,", "tokens": ["Himm\u00b7li\u00b7sches", "Loos", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sie dulden stets ohne Schuld,", "tokens": ["Sie", "dul\u00b7den", "stets", "oh\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den Leiden sie vertrauen,", "tokens": ["Den", "Lei\u00b7den", "sie", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie naher Himmelshuld.", "tokens": ["Wie", "na\u00b7her", "Him\u00b7mels\u00b7huld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Also sie ist mir blieben,", "tokens": ["Al\u00b7so", "sie", "ist", "mir", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Nie sch\u00f6ner ich je sie schau,", "tokens": ["Nie", "sch\u00f6\u00b7ner", "ich", "je", "sie", "schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PPER", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kein Zeichen konnt' betr\u00fcben", "tokens": ["Kein", "Zei\u00b7chen", "konnt'", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die unschuldvolle Frau;", "tokens": ["Die", "un\u00b7schuld\u00b7vol\u00b7le", "Frau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "W\u00e4r' noch ein Mensch auf Erden,", "tokens": ["W\u00e4r'", "noch", "ein", "Mensch", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So mahlt' ich ihr Gesicht,", "tokens": ["So", "mahlt'", "ich", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch es soll keinem werden,", "tokens": ["Doch", "es", "soll", "kei\u00b7nem", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Weidenast mir bricht.", "tokens": ["Mein", "Wei\u00b7de\u00b7nast", "mir", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sa\u00df ich wie jetzt auf der Weide", "tokens": ["Sa\u00df", "ich", "wie", "jetzt", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl \u00fcber den blanken Teich,", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "blan\u00b7ken", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo ich die Pfeifen schneide,", "tokens": ["Wo", "ich", "die", "Pfei\u00b7fen", "schnei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lang' ich Rohr erreich;", "tokens": ["So", "lang'", "ich", "Rohr", "er\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Abend mit kaltem Fl\u00fcgel", "tokens": ["A\u00b7bend", "mit",