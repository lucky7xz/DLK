{"dta.poem.10845": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Eine  \n gefrorne Fensterscheibe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die spielende Natur zeigt auch zur\nWinterszeit,", "tokens": ["Die", "spie\u00b7len\u00b7de", "Na\u00b7tur", "zeigt", "auch", "zur", "Win\u00b7ter\u00b7szeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vornemlich Morgens fr\u00fch, was", "tokens": ["Vor\u00b7nem\u00b7lich", "Mor\u00b7gens", "fr\u00fch", ",", "was"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "PWS"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor Formen und Figur, die Augen zu erg\u00f6zzen,", "tokens": ["Vor", "For\u00b7men", "und", "Fi\u00b7gur", ",", "die", "Au\u00b7gen", "zu", "er\u00b7g\u00f6z\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Kan selbsten im Gemach, kan in die Fenster sezzen.", "tokens": ["Kan", "selbs\u00b7ten", "im", "Ge\u00b7mach", ",", "kan", "in", "die", "Fens\u00b7ter", "sez\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "$,", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie mahlt bei dunkler Nacht, die Fensterscheiben", "tokens": ["Sie", "mahlt", "bei", "dunk\u00b7ler", "Nacht", ",", "die", "Fens\u00b7ter\u00b7schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Darin die Einbildung fast alles finden kan,", "tokens": ["Da\u00b7rin", "die", "Ein\u00b7bil\u00b7dung", "fast", "al\u00b7les", "fin\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Was man nur sehen will. Man sindet daran W\u00e4l-", "tokens": ["Was", "man", "nur", "se\u00b7hen", "will", ".", "Man", "sin\u00b7det", "da\u00b7ran", "W\u00e4l"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "ADV", "VVINF", "VMFIN", "$.", "PIS", "VVFIN", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Oft ein verwirrt Geb\u00fcsch, oft reich besaamte Fel-", "tokens": ["Oft", "ein", "ver\u00b7wirrt", "Ge\u00b7b\u00fcsch", ",", "oft", "reich", "be\u00b7saam\u00b7te", "Fel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "NN", "$,", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Oft St\u00e4dte, Schl\u00f6sser, Stern, und oft ein Blu-", "tokens": ["Oft", "St\u00e4d\u00b7te", ",", "Schl\u00f6s\u00b7ser", ",", "Stern", ",", "und", "oft", "ein", "Blu"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das an dem Scheiben-Glas durch kalten Frost er-", "tokens": ["Das", "an", "dem", "Schei\u00b7ben\u00b7Glas", "durch", "kal\u00b7ten", "Frost", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als wenn es durch die Kunst mit vielen Fleis formi-", "tokens": ["Als", "wenn", "es", "durch", "die", "Kunst", "mit", "vie\u00b7len", "Fleis", "for\u00b7mi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und von geschikter Hand sehr weislich figuri-", "tokens": ["Und", "von", "ge\u00b7schik\u00b7ter", "Hand", "sehr", "weis\u00b7lich", "fi\u00b7gu\u00b7ri"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Allein es ist doch nichts, als ein gefrorner Dufft", "tokens": ["Al\u00b7lein", "es", "ist", "doch", "nichts", ",", "als", "ein", "ge\u00b7fror\u00b7ner", "Dufft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PIS", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Schl\u00f6sser die dran stehn, sind eine nasse Lufft,", "tokens": ["Die", "Schl\u00f6s\u00b7ser", "die", "dran", "stehn", ",", "sind", "ei\u00b7ne", "nas\u00b7se", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PAV", "VVINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So bald die Sonn aufgeht, daran die Strahlen", "tokens": ["So", "bald", "die", "Sonn", "auf\u00b7geht", ",", "da\u00b7ran", "die", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "So sieh man, wie der Wald, wie Feld und Stadt", "tokens": ["So", "sieh", "man", ",", "wie", "der", "Wald", ",", "wie", "Feld", "und", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Des Feuers rege Krafft vernichtiget es gleich,", "tokens": ["Des", "Feu\u00b7ers", "re\u00b7ge", "Krafft", "ver\u00b7nich\u00b7ti\u00b7get", "es", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die W\u00e4rme schmelzt den Frost, die Bilder werden", "tokens": ["Die", "W\u00e4r\u00b7me", "schmelzt", "den", "Frost", ",", "die", "Bil\u00b7der", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Da rinnen sie hinweg, und in gar wenig Stunden,", "tokens": ["Da", "rin\u00b7nen", "sie", "hin\u00b7weg", ",", "und", "in", "gar", "we\u00b7nig", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist alle Herrligkeit, als wie ein Traum verschwun-", "tokens": ["Ist", "al\u00b7le", "Herr\u00b7lig\u00b7keit", ",", "als", "wie", "ein", "Traum", "ver\u00b7schwun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das was uns die Natur im Umkreis dieser Welt,", "tokens": ["Das", "was", "uns", "die", "Na\u00b7tur", "im", "Um\u00b7kreis", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ART", "NN", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "An Himmel, auf der Erd, als Sch\u00f6nheit vorgestelt,", "tokens": ["An", "Him\u00b7mel", ",", "auf", "der", "Erd", ",", "als", "Sch\u00f6n\u00b7heit", "vor\u00b7ge\u00b7stelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vergehet endlich auch. So wie aus unsern Sin-", "tokens": ["Ver\u00b7ge\u00b7het", "end\u00b7lich", "auch", ".", "So", "wie", "aus", "un\u00b7sern", "Sin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$.", "ADV", "KOKOM", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Berge, Th\u00e4ler, Wald im Augenblik zerrin-", "tokens": ["Die", "Ber\u00b7ge", ",", "Th\u00e4\u00b7ler", ",", "Wald", "im", "Au\u00b7gen\u00b7blik", "zer\u00b7rin"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So wird die ganze Welt, die jezt so herrlich blizt,", "tokens": ["So", "wird", "die", "gan\u00b7ze", "Welt", ",", "die", "jezt", "so", "herr\u00b7lich", "blizt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn sie das Feur zulezt in reger Glut erhizt,", "tokens": ["Wenn", "sie", "das", "Feur", "zu\u00b7lezt", "in", "re\u00b7ger", "Glut", "er\u00b7hizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Auch pl\u00f6zlich sich verkehrn, der Himmel wird ver-", "tokens": ["Auch", "pl\u00f6z\u00b7lich", "sich", "ver\u00b7kehrn", ",", "der", "Him\u00b7mel", "wird", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "VVINF", "$,", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Zerfliessen wie im Gla\u00df der Fenster, die Gestalten.", "tokens": ["Zer\u00b7flies\u00b7sen", "wie", "im", "Gla\u00df", "der", "Fens\u00b7ter", ",", "die", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}