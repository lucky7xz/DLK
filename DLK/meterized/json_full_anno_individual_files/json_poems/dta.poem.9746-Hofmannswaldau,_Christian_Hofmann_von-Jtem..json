{"dta.poem.9746": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "sl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein leser/ tritt ja nicht zu nah zu diesem stein/", "tokens": ["Mein", "le\u00b7ser", "/", "tritt", "ja", "nicht", "zu", "nah", "zu", "die\u00b7sem", "stein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "ADV", "PTKNEG", "PTKA", "ADJD", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es liegt hier eingesenckt Act\u00e4ons wahrer erbe/", "tokens": ["Es", "liegt", "hier", "ein\u00b7ge\u00b7senckt", "Ac\u00b7t\u00e4ons", "wah\u00b7rer", "er\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "NE", "ADJA", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der nur mit horn werck trieb im leben sein gewerbe.", "tokens": ["Der", "nur", "mit", "horn", "werck", "trieb", "im", "le\u00b7ben", "sein", "ge\u00b7wer\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "NN", "VVFIN", "APPRART", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vielleicht darff dieses gut itzt deine erbschafft seyn.", "tokens": ["Viel\u00b7leicht", "darff", "die\u00b7ses", "gut", "itzt", "dei\u00b7ne", "erb\u00b7schafft", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "ADJD", "ADV", "PPOSAT", "VVPP"