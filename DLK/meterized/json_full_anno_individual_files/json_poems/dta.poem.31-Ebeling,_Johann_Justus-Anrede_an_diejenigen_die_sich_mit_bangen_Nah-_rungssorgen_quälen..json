{"dta.poem.31": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Anrede  \n an diejenigen die sich mit bangen Nah-  \n rungssorgen qu\u00e4len.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zweifler! die ihr alle Morgen\ndurch die Sorgen", "tokens": ["Zweif\u00b7ler", "!", "die", "ihr", "al\u00b7le", "Mor\u00b7gen", "durch", "die", "Sor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PRELS", "PPER", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Eure Tage tr\u00fcbe macht;", "tokens": ["Eu\u00b7re", "Ta\u00b7ge", "tr\u00fc\u00b7be", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Obgleich ", "tokens": ["Ob\u00b7gleich"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Durch das Himmels-Licht anlacht.", "tokens": ["Durch", "das", "Him\u00b7mels\u00b7Licht", "an\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gehet in die gr\u00fcnen Felder,", "tokens": ["Ge\u00b7het", "in", "die", "gr\u00fc\u00b7nen", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In die G\u00e4rten, in die Aun,", "tokens": ["In", "die", "G\u00e4r\u00b7ten", ",", "in", "die", "Aun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da k\u00f6nt ihr die bangen Grillen", "tokens": ["Da", "k\u00f6nt", "ihr", "die", "ban\u00b7gen", "Gril\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und an Frucht und B\u00e4umen schaun,", "tokens": ["Und", "an", "Frucht", "und", "B\u00e4u\u00b7men", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Da\u00df ein ewig weises Walten", "tokens": ["Da\u00df", "ein", "e\u00b7wig", "wei\u00b7ses", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mensch und Viehe sey bem\u00fcht:", "tokens": ["Mensch", "und", "Vie\u00b7he", "sey", "be\u00b7m\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn ihr seht, aus Feld und B\u00e4umen", "tokens": ["Denn", "ihr", "seht", ",", "aus", "Feld", "und", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Der zu unsrer Nahrung bl\u00fcht.", "tokens": ["Der", "zu", "uns\u00b7rer", "Nah\u00b7rung", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "H\u00f6rt der V\u00f6gel zwitschernd Klingen,", "tokens": ["H\u00f6rt", "der", "V\u00f6\u00b7gel", "zwit\u00b7schernd", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Jhren Sch\u00f6pfer der sie n\u00e4hrt;", "tokens": ["Ih\u00b7ren", "Sch\u00f6p\u00b7fer", "der", "sie", "n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Sie sind froh auf ihrer Reise", "tokens": ["Sie", "sind", "froh", "auf", "ih\u00b7rer", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Auf dem Feld im Flug beschert,", "tokens": ["Auf", "dem", "Feld", "im", "Flug", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Jhr als Menschen! wolt den Schlummer", "tokens": ["Ihr", "als", "Men\u00b7schen", "!", "wolt", "den", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "KOUS", "NN", "$.", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "In der stillen Nacht zerst\u00f6hrn:", "tokens": ["In", "der", "stil\u00b7len", "Nacht", "zer\u00b7st\u00f6hrn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und an frohen Sonnen-Tagen", "tokens": ["Und", "an", "fro\u00b7hen", "Son\u00b7nen\u00b7Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wer euch wird mit Brodt ern\u00e4hrn?", "tokens": ["Wer", "euch", "wird", "mit", "Brodt", "er\u00b7n\u00e4hrn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Nur getrost! der H\u00f6chste wachet", "tokens": ["Nur", "ge\u00b7trost", "!", "der", "H\u00f6chs\u00b7te", "wa\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Da\u00df man Brodts die F\u00fclle hat:", "tokens": ["Da\u00df", "man", "Brodts", "die", "F\u00fcl\u00b7le", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Wenn uns bittre Armuth qu\u00e4let,", "tokens": ["Wenn", "uns", "bitt\u00b7re", "Ar\u00b7muth", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Weis er dennoch H\u00fclf und Rath.", "tokens": ["Weis", "er", "den\u00b7noch", "H\u00fclf", "und", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Er giebt Speise allem Viehe", "tokens": ["Er", "giebt", "Spei\u00b7se", "al\u00b7lem", "Vie\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Solt ers nicht dem Menschen thun?", "tokens": ["Solt", "ers", "nicht", "dem", "Men\u00b7schen", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Er als ein getreuer Vater", "tokens": ["Er", "als", "ein", "ge\u00b7treu\u00b7er", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Wird doch nimmer eher ruhn;", "tokens": ["Wird", "doch", "nim\u00b7mer", "e\u00b7her", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Als bis er nach Vater-Weise", "tokens": ["Als", "bis", "er", "nach", "Va\u00b7ter\u00b7Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Und als seinen Kindern giebt:", "tokens": ["Und", "als", "sei\u00b7nen", "Kin\u00b7dern", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wer dies gl\u00e4ubt mit festen Sinne,", "tokens": ["Wer", "dies", "gl\u00e4ubt", "mit", "fes\u00b7ten", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.32": {"text": "Da\u00df ihm ", "tokens": ["Da\u00df", "ihm"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Last des Kummers Dunst verfliegen,", "tokens": ["Last", "des", "Kum\u00b7mers", "Dunst", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Findet man stets Ueberflus:", "tokens": ["Fin\u00b7det", "man", "stets", "Ue\u00b7ber\u00b7flus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Aber wenn man unzufrieden", "tokens": ["A\u00b7ber", "wenn", "man", "un\u00b7zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Steter Hunger beim Genus.", "tokens": ["Ste\u00b7ter", "Hun\u00b7ger", "beim", "Ge\u00b7nus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Gott der liebt ein frohes Herze\ndas vom Schmerze", "tokens": ["Gott", "der", "liebt", "ein", "fro\u00b7hes", "Her\u00b7ze", "das", "vom", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VVFIN", "ART", "ADJA", "VVFIN", "ART", "APPRART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.38": {"text": "Eitler Sorgen sich befreit;", "tokens": ["Eit\u00b7ler", "Sor\u00b7gen", "sich", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Wer ihn sucht mit tr\u00fcben Mienen", "tokens": ["Wer", "ihn", "sucht", "mit", "tr\u00fc\u00b7ben", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Kennt nicht seine Herrlichkeit.", "tokens": ["Kennt", "nicht", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Darum fasset ins Gem\u00fcte,", "tokens": ["Da\u00b7rum", "fas\u00b7set", "ins", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Fleht dieselbe br\u00fcnstig an;", "tokens": ["Fleht", "die\u00b7sel\u00b7be", "br\u00fcns\u00b7tig", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Treibet flei\u00dfig eur Gewerbe", "tokens": ["Trei\u00b7bet", "flei\u00b7\u00dfig", "eur", "Ge\u00b7wer\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Dadurch er euch n\u00e4hren kan.", "tokens": ["Da\u00b7durch", "er", "euch", "n\u00e4h\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}