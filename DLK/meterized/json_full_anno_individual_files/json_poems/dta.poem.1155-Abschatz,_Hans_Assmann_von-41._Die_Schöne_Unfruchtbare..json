{"dta.poem.1155": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "41. Die Sch\u00f6ne Unfruchtbare.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Kan man dich billig hart und eigenn\u00fctzig nennen/ ", "tokens": ["Kan", "man", "dich", "bil\u00b7lig", "hart", "und", "ei\u00b7gen\u00b7n\u00fct\u00b7zig", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADJD", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil deines gleichen nicht von dir zu lieben seyn?", "tokens": ["Weil", "dei\u00b7nes", "glei\u00b7chen", "nicht", "von", "dir", "zu", "lie\u00b7ben", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die keusche Zynthia zeigt einsam ihren Schein/", "tokens": ["Die", "keu\u00b7sche", "Zynt\u00b7hia", "zeigt", "ein\u00b7sam", "ih\u00b7ren", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Febus sehen wir allein am Himmel brennen.", "tokens": ["Den", "Fe\u00b7bus", "se\u00b7hen", "wir", "al\u00b7lein", "am", "Him\u00b7mel", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer seine Sinnen will vom Wahn des P\u00f6fels trennen/", "tokens": ["Wer", "sei\u00b7ne", "Sin\u00b7nen", "will", "vom", "Wahn", "des", "P\u00f6\u00b7fels", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird sp\u00fcren/ da\u00df/ was sch\u00f6n/ unfruchtbar/ ungemein.", "tokens": ["Wird", "sp\u00fc\u00b7ren", "/", "da\u00df", "/", "was", "sch\u00f6n", "/", "un\u00b7frucht\u00b7bar", "/", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "VVINF", "$(", "KOUS", "$(", "PWS", "ADJD", "$(", "ADJD", "$(", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was zeuget aus sich selbst ein harter Edelstein?", "tokens": ["Was", "zeu\u00b7get", "aus", "sich", "selbst", "ein", "har\u00b7ter", "E\u00b7del\u00b7stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dennoch pflegt man ihn f\u00fcr kostbar zu erkennen.", "tokens": ["Und", "den\u00b7noch", "pflegt", "man", "ihn", "f\u00fcr", "kost\u00b7bar", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vermehrte wohl dein Lob der Kinder ihr Geschrey?", "tokens": ["Ver\u00b7mehr\u00b7te", "wohl", "dein", "Lob", "der", "Kin\u00b7der", "ihr", "Ge\u00b7schrey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was darff dein Ebenbild um deine Br\u00fcste schertzen?", "tokens": ["Was", "darff", "dein", "E\u00b7ben\u00b7bild", "um", "dei\u00b7ne", "Br\u00fcs\u00b7te", "schert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es lebet allbereit gepr\u00e4gt in tausend Hertzen.", "tokens": ["Es", "le\u00b7bet", "all\u00b7be\u00b7reit", "ge\u00b7pr\u00e4gt", "in", "tau\u00b7send", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da/ wo des Himmels Gunst die beste Specerey", "tokens": ["Da", "/", "wo", "des", "Him\u00b7mels", "Gunst", "die", "bes\u00b7te", "Spe\u00b7ce\u00b7rey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "PWAV", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit reichem Wucher zeugt/ ist in den Balsam-Gr\u00fcnden", "tokens": ["Mit", "rei\u00b7chem", "Wu\u00b7cher", "zeugt", "/", "ist", "in", "den", "Bal\u00b7sam\u00b7Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Ph\u00f6nix auch allein bey h\u00f6chster Pracht zu finden.", "tokens": ["Der", "Ph\u00f6\u00b7nix", "auch", "al\u00b7lein", "bey", "h\u00f6chs\u00b7ter", "Pracht", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}