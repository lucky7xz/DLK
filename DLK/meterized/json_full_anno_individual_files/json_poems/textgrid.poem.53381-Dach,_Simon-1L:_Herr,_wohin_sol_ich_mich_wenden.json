{"textgrid.poem.53381": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, wohin sol ich mich wenden", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, wohin sol ich mich wenden", "tokens": ["Herr", ",", "wo\u00b7hin", "sol", "ich", "mich", "wen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt in meiner letzten Noht?", "tokens": ["Jetzt", "in", "mei\u00b7ner", "letz\u00b7ten", "Noht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es drewt mir aller Enden", "tokens": ["Denn", "es", "drewt", "mir", "al\u00b7ler", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Gericht, S\u00fcnd, Hell und Tod.", "tokens": ["Dein", "Ge\u00b7richt", ",", "S\u00fcnd", ",", "Hell", "und", "Tod", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jetzt schliess' ich die kurtze Zeit", "tokens": ["Jetzt", "schliess'", "ich", "die", "kurt\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vnd tret' ein die Ewigheit", "tokens": ["Vnd", "tret'", "ein", "die", "E\u00b7wig\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da mich Qual ohn End vnd massen", "tokens": ["Da", "mich", "Qual", "ohn", "End", "vnd", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder Frewde wird vmbfassen.", "tokens": ["O\u00b7der", "Frew\u00b7de", "wird", "vmb\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Keinem kan der Himmel werden", "tokens": ["Kei\u00b7nem", "kan", "der", "Him\u00b7mel", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der nicht heilig ist und rein,", "tokens": ["Der", "nicht", "hei\u00b7lig", "ist", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich beschmutzt der Schlamm der Erden,", "tokens": ["Mich", "be\u00b7schmutzt", "der", "Schlamm", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hunde sieht man draussen seyn.", "tokens": ["Hun\u00b7de", "sieht", "man", "draus\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird die Helle denn mein Theil?", "tokens": ["Wird", "die", "Hel\u00b7le", "denn", "mein", "Theil", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist nicht Rettung, Trost, noch Heil?", "tokens": ["Ist", "nicht", "Ret\u00b7tung", ",", "Trost", ",", "noch", "Heil", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weist du, GOTT, nicht Raht zu finden", "tokens": ["Weist", "du", ",", "GoTT", ",", "nicht", "Raht", "zu", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "NE", "$,", "PTKNEG", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Diesem Grewel meiner S\u00fcnden?", "tokens": ["Die\u00b7sem", "Gre\u00b7wel", "mei\u00b7ner", "S\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df sey fern von deiner G\u00fcte,", "tokens": ["Da\u00df", "sey", "fern", "von", "dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Deine Trew ist offenbahr", "tokens": ["Dei\u00b7ne", "Trew", "ist", "of\u00b7fen\u00b7bahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd erquickt mir mein Gem\u00fcte,", "tokens": ["Vnd", "er\u00b7quickt", "mir", "mein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist je gewi\u00dflich war", "tokens": ["Es", "ist", "je", "ge\u00b7wi\u00df\u00b7lich", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd ein thewer wehrtes Wort", "tokens": ["Vnd", "ein", "the\u00b7wer", "wehr\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dessen starcken Grund kein Mord", "tokens": ["Des\u00b7sen", "star\u00b7cken", "Grund", "kein", "Mord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und kein wilder Sturm der Hellen", "tokens": ["Und", "kein", "wil\u00b7der", "Sturm", "der", "Hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig mir wird k\u00f6nnen f\u00e4llen:", "tokens": ["E\u00b7wig", "mir", "wird", "k\u00f6n\u00b7nen", "f\u00e4l\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df mein Jesus ist gekommen", "tokens": ["Da\u00df", "mein", "Je\u00b7sus", "ist", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NE", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den S\u00fcndern in die Welt,", "tokens": ["Zu", "den", "S\u00fcn\u00b7dern", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer Noht sich angenommen,", "tokens": ["Ih\u00b7rer", "Noht", "sich", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie erkauffet, nicht durch Geld", "tokens": ["Sie", "er\u00b7kauf\u00b7fet", ",", "nicht", "durch", "Geld"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sondern durch sein thewres Blut", "tokens": ["Son\u00b7dern", "durch", "sein", "thew\u00b7res", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre b\u00f6se Sache gut", "tokens": ["Ih\u00b7re", "b\u00f6\u00b7se", "Sa\u00b7che", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnd sie von der Hellen-Rachen", "tokens": ["Vnd", "sie", "von", "der", "Hel\u00b7len\u00b7Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frey, vnd selig dort zu machen.", "tokens": ["Frey", ",", "vnd", "se\u00b7lig", "dort", "zu", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solches gl\u00e4ub ich, Gott, von Hertzen,", "tokens": ["Sol\u00b7ches", "gl\u00e4ub", "ich", ",", "Gott", ",", "von", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib mir nur Best\u00e4ndigheit,", "tokens": ["Gib", "mir", "nur", "Be\u00b7st\u00e4n\u00b7dig\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcrtz mir meine Todes-Schmertzen,", "tokens": ["K\u00fcrtz", "mir", "mei\u00b7ne", "To\u00b7des\u00b7Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey mein Beystandt, mein Geleit.", "tokens": ["Sey", "mein", "Beys\u00b7tandt", ",", "mein", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "End mir selig meinen Lauff,", "tokens": ["End", "mir", "se\u00b7lig", "mei\u00b7nen", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd nimm mich zu dir hinauff", "tokens": ["Vnd", "nimm", "mich", "zu", "dir", "hin\u00b7auff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich mit der Engel Weisen", "tokens": ["Da\u00df", "ich", "mit", "der", "En\u00b7gel", "Wei\u00b7sen"], "token_info