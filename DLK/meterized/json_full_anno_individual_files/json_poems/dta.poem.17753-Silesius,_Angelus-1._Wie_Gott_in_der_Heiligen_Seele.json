{"dta.poem.17753": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "1.  Wie Gott in der Heiligen Seele", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Fragstu wie Gott das Wort in einer Seele wohne?", "tokens": ["Frags\u00b7tu", "wie", "Gott", "das", "Wort", "in", "ei\u00b7ner", "See\u00b7le", "woh\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wisse wie das Licht der Sonnen in der Welt/", "tokens": ["So", "wis\u00b7se", "wie", "das", "Licht", "der", "Son\u00b7nen", "in", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wie ein Br\u00e4utgam sich in seiner Kammer h\u00e4lt:", "tokens": ["Und", "wie", "ein", "Br\u00e4ut\u00b7gam", "sich", "in", "sei\u00b7ner", "Kam\u00b7mer", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie ein K\u00f6nig sitzt in seinem Reich und Throne:", "tokens": ["Und", "wie", "ein", "K\u00f6\u00b7nig", "sitzt", "in", "sei\u00b7nem", "Reich", "und", "Thro\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Lehrer in der Schul/ ein Vatter bey dem Sohne:", "tokens": ["Ein", "Leh\u00b7rer", "in", "der", "Schul", "/", "ein", "Vat\u00b7ter", "bey", "dem", "Soh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wie ein theurer Schatz in\u0304 einem Akkerfeld:", "tokens": ["Und", "wie", "ein", "theu\u00b7rer", "Schatz", "in\u0304", "ei\u00b7nem", "Ak\u00b7ker\u00b7feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wie ein lieber Gast in einem sch\u00f6nen Zelt:", "tokens": ["Und", "wie", "ein", "lie\u00b7ber", "Gast", "in", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie ein Kleinod ist in einer guldnen Krone.", "tokens": ["Und", "wie", "ein", "Klei\u00b7nod", "ist", "in", "ei\u00b7ner", "guld\u00b7nen", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie eine Lilie in einem Blumenthal/", "tokens": ["Wie", "ei\u00b7ne", "Li\u00b7lie", "in", "ei\u00b7nem", "Blu\u00b7ment\u00b7hal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und wie ein Seitenspiel bey einem Abendmahl:", "tokens": ["Und", "wie", "ein", "Sei\u00b7ten\u00b7spiel", "bey", "ei\u00b7nem", "A\u00b7bend\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wie ein Zimmet-\u00f6l in einer Lamp\u2019 entz\u00fcnden:", "tokens": ["Und", "wie", "ein", "Zim\u00b7met\u00b7\u00f6l", "in", "ei\u00b7ner", "Lamp'", "ent\u00b7z\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wie das Himmelbrodt in einem reinen Schrein:", "tokens": ["Und", "wie", "das", "Him\u00b7mel\u00b7brodt", "in", "ei\u00b7nem", "rei\u00b7nen", "Schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und wie ein Garten Brun\u0304/ und wie ein k\u00fchler Wein.", "tokens": ["Und", "wie", "ein", "Gar\u00b7ten", "Brun\u0304", "/", "und", "wie", "ein", "k\u00fch\u00b7ler", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NE", "$(", "KON", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sag ob er anderst wo so sch\u00f6ne wird gefunden?", "tokens": ["Sag", "ob", "er", "an\u00b7derst", "wo", "so", "sch\u00f6\u00b7ne", "wird", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "PWAV", "ADV", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}