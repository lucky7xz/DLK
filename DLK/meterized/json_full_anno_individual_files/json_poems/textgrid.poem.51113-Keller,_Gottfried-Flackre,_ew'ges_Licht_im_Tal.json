r": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es bleibt uns keine Frist,", "tokens": ["Und", "es", "bleibt", "uns", "kei\u00b7ne", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu beharren im Gedr\u00e4nge.", "tokens": ["Zu", "be\u00b7har\u00b7ren", "im", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wie der Staub im Sonnenstrahle", "tokens": ["Wie", "der", "Staub", "im", "Son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallt's vor\u00fcber, Kern und Schale \u2013", "tokens": ["Wallt's", "vor\u00b7\u00fc\u00b7ber", ",", "Kern", "und", "Scha\u00b7le", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig ist, begreifst es du,", "tokens": ["E\u00b7wig", "ist", ",", "be\u00b7greifst", "es", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehnend Herz? nur deine Ruh!", "tokens": ["Seh\u00b7nend", "Herz", "?", "nur", "dei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}