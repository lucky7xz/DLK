{"dta.poem.20864": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Des Geistes grosse Wunderkrafft/", "tokens": ["Des", "Geis\u00b7tes", "gros\u00b7se", "Wun\u00b7der\u00b7krafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dem in allen Thun und F\u00e4llen", "tokens": ["In", "dem", "in", "al\u00b7len", "Thun", "und", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Leib ihm g\u00e4ntzlich ist verhafft/", "tokens": ["Der", "Leib", "ihm", "g\u00e4ntz\u00b7lich", "ist", "ver\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem es ihm die Seele schafft/", "tokens": ["Nach\u00b7dem", "es", "ihm", "die", "See\u00b7le", "schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Fortgang richtig zu bestellen.", "tokens": ["Den", "Fort\u00b7gang", "rich\u00b7tig", "zu", "be\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Ruhm/ den dieses Werck geb\u00fchrt/", "tokens": ["Den", "Ruhm", "/", "den", "die\u00b7ses", "Werck", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gibt seine Hoheit zu errahten/", "tokens": ["Gibt", "sei\u00b7ne", "Ho\u00b7heit", "zu", "er\u00b7rah\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was Rhadamant im Schilde f\u00fchrt/", "tokens": ["Was", "Rha\u00b7da\u00b7mant", "im", "Schil\u00b7de", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Parcen Macht in bleichen Thaten/", "tokens": ["Der", "Par\u00b7cen", "Macht", "in", "blei\u00b7chen", "Tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat diesen Helden nicht ger\u00fchrt/", "tokens": ["Hat", "die\u00b7sen", "Hel\u00b7den", "nicht", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihn gelegt in Todes Schatten.", "tokens": ["Und", "ihn", "ge\u00b7legt", "in", "To\u00b7des", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}