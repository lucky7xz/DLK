{"dta.poem.5974": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.57", "da:0.28", "ro:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So ist die Erde nun formiret:", "tokens": ["So", "ist", "die", "Er\u00b7de", "nun", "for\u00b7mi\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dieses wird, ohn\u2019 von des Himmels H\u00f6hen", "tokens": ["Und", "die\u00b7ses", "wird", ",", "ohn'", "von", "des", "Him\u00b7mels", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "$,", "KOUI", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Erden anzusehen,", "tokens": ["Die", "Er\u00b7den", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch ein sehr leicht Experiment", "tokens": ["Durch", "ein", "sehr", "leicht", "Ex\u00b7pe\u00b7ri\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar leicht probiret:", "tokens": ["Gar", "leicht", "pro\u00b7bi\u00b7ret", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}