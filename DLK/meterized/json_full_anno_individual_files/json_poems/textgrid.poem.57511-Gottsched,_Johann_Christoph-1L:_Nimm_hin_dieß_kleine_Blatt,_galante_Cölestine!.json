], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Komm, w\u00e4ge meine Treu, und jener Schm\u00e4ucheln, ab:", "tokens": ["Komm", ",", "w\u00e4\u00b7ge", "mei\u00b7ne", "Treu", ",", "und", "je\u00b7ner", "Schm\u00e4u\u00b7cheln", ",", "ab", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PDAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So findest du gewi\u00df mein Herz von \u00e4chter G\u00fcte,", "tokens": ["So", "fin\u00b7dest", "du", "ge\u00b7wi\u00df", "mein", "Herz", "von", "\u00e4ch\u00b7ter", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da dir so mancher Mund nur leere Worte gab.", "tokens": ["Da", "dir", "so", "man\u00b7cher", "Mund", "nur", "lee\u00b7re", "Wor\u00b7te", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wiewohl du kennst mich schon, und wirst mich nicht verschm\u00e4hen.", "tokens": ["Wie\u00b7wohl", "du", "kennst", "mich", "schon", ",", "und", "wirst", "mich", "nicht", "ver\u00b7schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Brennt gleich mein kleines Licht bey jenen Sonnen schwach:", "tokens": ["Brennt", "gleich", "mein", "klei\u00b7nes", "Licht", "bey", "je\u00b7nen", "Son\u00b7nen", "schwach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ja soll ich gleich dein Herz nicht ganz von innen sehen;", "tokens": ["Ja", "soll", "ich", "gleich", "dein", "Herz", "nicht", "ganz", "von", "in\u00b7nen", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So \u00f6ffnest du mir doch des Herzens Vorgemach.", "tokens": ["So", "\u00f6ff\u00b7nest", "du", "mir", "doch", "des", "Her\u00b7zens", "Vor\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich bin vergn\u00fcgt damit, und habe nichts verlohren,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "da\u00b7mit", ",", "und", "ha\u00b7be", "nichts", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PAV", "$,", "KON", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn gleich ", "tokens": ["Wenn", "gleich"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Und wenn sich gleich die Brust der stolz gesinnten", "tokens": ["Und", "wenn", "sich", "gleich", "die", "Brust", "der", "stolz", "ge\u00b7sinn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "ADV", "ART", "NN", "ART", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dasselbe, wie sie glaubt, zur Residenz gemacht.", "tokens": ["Das\u00b7sel\u00b7be", ",", "wie", "sie", "glaubt", ",", "zur", "Re\u00b7si\u00b7denz", "ge\u00b7macht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein Vorzug bleibt mir doch, der meine Gro\u00dfmuth mehret;", "tokens": ["Ein", "Vor\u00b7zug", "bleibt", "mir", "doch", ",", "der", "mei\u00b7ne", "Gro\u00df\u00b7muth", "meh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df die Verwandtschaft mich, nicht sie, mit dir verbindt:", "tokens": ["Da\u00df", "die", "Ver\u00b7wandt\u00b7schaft", "mich", ",", "nicht", "sie", ",", "mit", "dir", "ver\u00b7bindt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "$,", "PTKNEG", "PPER", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn da mein Bruder dich, als eine Schwester, ehret,", "tokens": ["Denn", "da", "mein", "Bru\u00b7der", "dich", ",", "als", "ei\u00b7ne", "Schwes\u00b7ter", ",", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So glaub ich, da\u00df auch wir genau verschwistert sind.", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "auch", "wir", "ge\u00b7nau", "ver\u00b7schwis\u00b7tert", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So nimm denn, sch\u00f6nstes Kind! mein Herz zum Angebinde.", "tokens": ["So", "nimm", "denn", ",", "sch\u00f6ns\u00b7tes", "Kind", "!", "mein", "Herz", "zum", "An\u00b7ge\u00b7bin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und glaube, da\u00df es dich, bis in die Grube, liebt;", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "es", "dich", ",", "bis", "in", "die", "Gru\u00b7be", ",", "liebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ja, da\u00df ich blo\u00df in dir mein sch\u00f6nstes Labsal finde,", "tokens": ["Ja", ",", "da\u00df", "ich", "blo\u00df", "in", "dir", "mein", "sch\u00f6ns\u00b7tes", "Lab\u00b7sal", "fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dergleichen mir gewi\u00df mein eigen Gl\u00fcck nicht giebt.", "tokens": ["Derg\u00b7lei\u00b7chen", "mir", "ge\u00b7wi\u00df", "mein", "ei\u00b7gen", "Gl\u00fcck", "nicht", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Himmel schenke dir nun bald ein m\u00e4nnlich Herze,", "tokens": ["Der", "Him\u00b7mel", "schen\u00b7ke", "dir", "nun", "bald", "ein", "m\u00e4nn\u00b7lich", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das dich so treu und zart, als meine Seele meynt;", "tokens": ["Das", "dich", "so", "treu", "und", "zart", ",", "als", "mei\u00b7ne", "See\u00b7le", "meynt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So sprech ich dermaleinst, bey dem verliebten Scherze:", "tokens": ["So", "sprech", "ich", "der\u00b7ma\u00b7leinst", ",", "bey", "dem", "ver\u00b7lieb\u00b7ten", "Scher\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"te