{"textgrid.poem.57511": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimm hin die\u00df kleine Blatt, galante C\u00f6lestine!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm hin die\u00df kleine Blatt, galante C\u00f6lestine!", "tokens": ["Nimm", "hin", "die\u00df", "klei\u00b7ne", "Blatt", ",", "ga\u00b7lan\u00b7te", "C\u00f6\u00b7les\u00b7ti\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "ADJA", "NN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es k\u00f6mmt ja, wie du siehst, von keiner M\u00e4nnerhand.", "tokens": ["Es", "k\u00f6mmt", "ja", ",", "wie", "du", "siehst", ",", "von", "kei\u00b7ner", "M\u00e4n\u00b7ner\u00b7hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum wundre dich nur nicht, wenn ich mich viel erk\u00fchne,", "tokens": ["Drum", "wund\u00b7re", "dich", "nur", "nicht", ",", "wenn", "ich", "mich", "viel", "er\u00b7k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hat dir diese Schrift kein Buhler zugesandt.", "tokens": ["Es", "hat", "dir", "die\u00b7se", "Schrift", "kein", "Buh\u00b7ler", "zu\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PDAT", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Jahrfest macht mich froh; mein Wunsch soll dich verehren:", "tokens": ["Dein", "Jahr\u00b7fest", "macht", "mich", "froh", ";", "mein", "Wunsch", "soll", "dich", "ver\u00b7eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Freundschaft fodert es, die Liebe reizt mich an.", "tokens": ["Die", "Freund\u00b7schaft", "fo\u00b7dert", "es", ",", "die", "Lie\u00b7be", "reizt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und will mich \u00f6fters gleich mein Unverm\u00f6gen st\u00f6ren;", "tokens": ["Und", "will", "mich", "\u00f6f\u00b7ters", "gleich", "mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird gleichwohl der Pflicht dadurch genug gethan.", "tokens": ["So", "wird", "gleich\u00b7wohl", "der", "Pflicht", "da\u00b7durch", "ge\u00b7nug", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein, wie soll ich dich an diesem Feste binden?", "tokens": ["Al\u00b7lein", ",", "wie", "soll", "ich", "dich", "an", "die\u00b7sem", "Fes\u00b7te", "bin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VMFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was schenk ich, Sch\u00f6nste! dir, das dir gef\u00e4llig sey?", "tokens": ["Was", "schenk", "ich", ",", "Sch\u00f6ns\u00b7te", "!", "dir", ",", "das", "dir", "ge\u00b7f\u00e4l\u00b7lig", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich sinne hin und her, und weis doch nichts zu finden,", "tokens": ["Ich", "sin\u00b7ne", "hin", "und", "her", ",", "und", "weis", "doch", "nichts", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "KON", "PTKVZ", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als ein ergebnes Herz voll ungef\u00e4rbter Treu.", "tokens": ["Als", "ein", "er\u00b7geb\u00b7nes", "Herz", "voll", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie das? sprichst du, dein Herz? du hast ja stets geschwiegen,", "tokens": ["Wie", "das", "?", "sprichst", "du", ",", "dein", "Herz", "?", "du", "hast", "ja", "stets", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$.", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn andre mich damit aufs eifrigste beschenkt;", "tokens": ["Wenn", "and\u00b7re", "mich", "da\u00b7mit", "aufs", "eif\u00b7rigs\u00b7te", "be\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PAV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und hast es irgend schon, mit herzlicherm Vergn\u00fcgen,", "tokens": ["Und", "hast", "es", "ir\u00b7gend", "schon", ",", "mit", "herz\u00b7li\u00b7cherm", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auf sonst was Artiges, das dir gef\u00e4llt, gelenkt.", "tokens": ["Auf", "sonst", "was", "Ar\u00b7ti\u00b7ges", ",", "das", "dir", "ge\u00b7f\u00e4llt", ",", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "PWS", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach freylich, schwieg ich wohl, wenn sich zwo stolze Sch\u00f6nen", "tokens": ["Ach", "frey\u00b7lich", ",", "schwieg", "ich", "wohl", ",", "wenn", "sich", "zwo", "stol\u00b7ze", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PRF", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In meiner Gegenwart um deine Huld bem\u00fcht:", "tokens": ["In", "mei\u00b7ner", "Ge\u00b7gen\u00b7wart", "um", "dei\u00b7ne", "Huld", "be\u00b7m\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Warum? sie schienen nur mein Wesen zu verh\u00f6hnen,", "tokens": ["Wa\u00b7rum", "?", "sie", "schie\u00b7nen", "nur", "mein", "We\u00b7sen", "zu", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil alles itzt auf Geld, auf Staat und Ansehn sieht.", "tokens": ["Weil", "al\u00b7les", "itzt", "auf", "Geld", ",", "auf", "Staat", "und", "An\u00b7sehn", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch, Werthe! pr\u00fcfe nur mein redliches Gem\u00fcthe,", "tokens": ["Doch", ",", "Wert\u00b7he", "!", "pr\u00fc\u00b7fe", "nur", "mein", "red\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Komm, w\u00e4ge meine Treu, und jener Schm\u00e4ucheln, ab:", "tokens": ["Komm", ",", "w\u00e4\u00b7ge", "mei\u00b7ne", "Treu", ",", "und", "je\u00b7ner", "Schm\u00e4u\u00b7cheln", ",", "ab", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PDAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So findest du gewi\u00df mein Herz von \u00e4chter G\u00fcte,", "tokens": ["So", "fin\u00b7dest", "du", "ge\u00b7wi\u00df", "mein", "Herz", "von", "\u00e4ch\u00b7ter", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da dir so mancher Mund nur leere Worte gab.", "tokens": ["Da", "dir", "so", "man\u00b7cher", "Mund", "nur", "lee\u00b7re", "Wor\u00b7te", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wiewohl du kennst mich schon, und wirst mich nicht verschm\u00e4hen.", "tokens": ["Wie\u00b7wohl", "du", "kennst", "mich", "schon", ",", "und", "wirst", "mich", "nicht", "ver\u00b7schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Brennt gleich mein kleines Licht bey jenen Sonnen schwach:", "tokens": ["Brennt", "gleich", "mein", "klei\u00b7nes", "Licht", "bey", "je\u00b7nen", "Son\u00b7nen", "schwach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ja soll ich gleich dein Herz nicht ganz von innen sehen;", "tokens": ["Ja", "soll", "ich", "gleich", "dein", "Herz", "nicht", "ganz", "von", "in\u00b7nen", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So \u00f6ffnest du mir doch des Herzens Vorgemach.", "tokens": ["So", "\u00f6ff\u00b7nest", "du", "mir", "doch", "des", "Her\u00b7zens", "Vor\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich bin vergn\u00fcgt damit, und habe nichts verlohren,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "da\u00b7mit", ",", "und", "ha\u00b7be", "nichts", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PAV", "$,", "KON", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn gleich ", "tokens": ["Wenn", "gleich"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Und wenn sich gleich die Brust der stolz gesinnten", "tokens": ["Und", "wenn", "sich", "gleich", "die", "Brust", "der", "stolz", "ge\u00b7sinn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "ADV", "ART", "NN", "ART", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dasselbe, wie sie glaubt, zur Residenz gemacht.", "tokens": ["Das\u00b7sel\u00b7be", ",", "wie", "sie", "glaubt", ",", "zur", "Re\u00b7si\u00b7denz", "ge\u00b7macht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein Vorzug bleibt mir doch, der meine Gro\u00dfmuth mehret;", "tokens": ["Ein", "Vor\u00b7zug", "bleibt", "mir", "doch", ",", "der", "mei\u00b7ne", "Gro\u00df\u00b7muth", "meh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df die Verwandtschaft mich, nicht sie, mit dir verbindt:", "tokens": ["Da\u00df", "die", "Ver\u00b7wandt\u00b7schaft", "mich", ",", "nicht", "sie", ",", "mit", "dir", "ver\u00b7bindt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "$,", "PTKNEG", "PPER", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn da mein Bruder dich, als eine Schwester, ehret,", "tokens": ["Denn", "da", "mein", "Bru\u00b7der", "dich", ",", "als", "ei\u00b7ne", "Schwes\u00b7ter", ",", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So glaub ich, da\u00df auch wir genau verschwistert sind.", "tokens": ["So", "glaub", "ich", ",", "da\u00df", "auch", "wir", "ge\u00b7nau", "ver\u00b7schwis\u00b7tert", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So nimm denn, sch\u00f6nstes Kind! mein Herz zum Angebinde.", "tokens": ["So", "nimm", "denn", ",", "sch\u00f6ns\u00b7tes", "Kind", "!", "mein", "Herz", "zum", "An\u00b7ge\u00b7bin\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und glaube, da\u00df es dich, bis in die Grube, liebt;", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "es", "dich", ",", "bis", "in", "die", "Gru\u00b7be", ",", "liebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ja, da\u00df ich blo\u00df in dir mein sch\u00f6nstes Labsal finde,", "tokens": ["Ja", ",", "da\u00df", "ich", "blo\u00df", "in", "dir", "mein", "sch\u00f6ns\u00b7tes", "Lab\u00b7sal", "fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dergleichen mir gewi\u00df mein eigen Gl\u00fcck nicht giebt.", "tokens": ["Derg\u00b7lei\u00b7chen", "mir", "ge\u00b7wi\u00df", "mein", "ei\u00b7gen", "Gl\u00fcck", "nicht", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Himmel schenke dir nun bald ein m\u00e4nnlich Herze,", "tokens": ["Der", "Him\u00b7mel", "schen\u00b7ke", "dir", "nun", "bald", "ein", "m\u00e4nn\u00b7lich", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das dich so treu und zart, als meine Seele meynt;", "tokens": ["Das", "dich", "so", "treu", "und", "zart", ",", "als", "mei\u00b7ne", "See\u00b7le", "meynt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So sprech ich dermaleinst, bey dem verliebten Scherze:", "tokens": ["So", "sprech", "ich", "der\u00b7ma\u00b7leinst", ",", "bey", "dem", "ver\u00b7lieb\u00b7ten", "Scher\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Der Himmel hab an euch das sch\u00f6nste Paar vereint.", "tokens": ["Der", "Him\u00b7mel", "hab", "an", "euch", "das", "sch\u00f6ns\u00b7te", "Paar", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nimm hin die\u00df kleine Blatt, galante C\u00f6lestine!", "tokens": ["Nimm", "hin", "die\u00df", "klei\u00b7ne", "Blatt", ",", "ga\u00b7lan\u00b7te", "C\u00f6\u00b7les\u00b7ti\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "ADJA", "NN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es k\u00f6mmt ja, wie du siehst, von keiner M\u00e4nnerhand.", "tokens": ["Es", "k\u00f6mmt", "ja", ",", "wie", "du", "siehst", ",", "von", "kei\u00b7ner", "M\u00e4n\u00b7ner\u00b7hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum wundre dich nur nicht, wenn ich mich viel erk\u00fchne,", "tokens": ["Drum", "wund\u00b7re", "dich", "nur", "nicht", ",", "wenn", "ich", "mich", "viel", "er\u00b7k\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hat dir diese Schrift kein Buhler zugesandt.", "tokens": ["Es", "hat", "dir", "die\u00b7se", "Schrift", "kein", "Buh\u00b7ler", "zu\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PDAT", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Jahrfest macht mich froh; mein Wunsch soll dich verehren:", "tokens": ["Dein", "Jahr\u00b7fest", "macht", "mich", "froh", ";", "mein", "Wunsch", "soll", "dich", "ver\u00b7eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Freundschaft fodert es, die Liebe reizt mich an.", "tokens": ["Die", "Freund\u00b7schaft", "fo\u00b7dert", "es", ",", "die", "Lie\u00b7be", "reizt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und will mich \u00f6fters gleich mein Unverm\u00f6gen st\u00f6ren;", "tokens": ["Und", "will", "mich", "\u00f6f\u00b7ters", "gleich", "mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird gleichwohl der Pflicht dadurch genug gethan.", "tokens": ["So", "wird", "gleich\u00b7wohl", "der", "Pflicht", "da\u00b7durch", "ge\u00b7nug", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein, wie soll ich dich an diesem Feste binden?", "tokens": ["Al\u00b7lein", ",", "wie", "soll", "ich", "dich", "an", "die\u00b7sem", "Fes\u00b7te", "bin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VMFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was schenk ich, Sch\u00f6nste! dir, das dir gef\u00e4llig sey?", "tokens": ["Was", "schenk", "ich", ",", "Sch\u00f6ns\u00b7te", "!", "dir", ",", "das", "dir", "ge\u00b7f\u00e4l\u00b7lig", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich sinne hin und her, und weis doch nichts zu finden,", "tokens": ["Ich", "sin\u00b7ne", "hin", "und", "her", ",", "und", "weis", "doch", "nichts", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "KON", "PTKVZ", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als ein ergebnes Herz voll ungef\u00e4rbter Treu.", "tokens": ["Als", "ein", "er\u00b7geb\u00b7nes", "Herz", "voll", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie das? sprichst du, dein Herz? du hast ja stets geschwiegen,", "tokens": ["Wie", "das", "?", "sprichst", "du", ",", "dein", "Herz", "?", "du", "hast", "ja", "stets", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$.", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn andre mich damit aufs eifrigste beschenkt;", "tokens": ["Wenn", "and\u00b7re", "mich", "da\u00b7mit", "aufs", "eif\u00b7rigs\u00b7te", "be\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PAV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und hast es irgend schon, mit herzlicherm Vergn\u00fcgen,", "tokens": ["Und", "hast", "es", "ir\u00b7gend", "schon", ",", "mit", "herz\u00b7li\u00b7cherm", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auf sonst was Artiges, das dir gef\u00e4llt, gelenkt.", "tokens": ["Auf", "sonst", "was", "Ar\u00b7ti\u00b7ges", ",", "das", "dir", "ge\u00b7f\u00e4llt", ",", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "PWS", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach freylich, schwieg ich wohl, wenn sich zwo stolze Sch\u00f6nen", "tokens": ["Ach", "frey\u00b7lich", ",", "schwieg", "ich", "wohl", ",", "wenn", "sich", "zwo", "stol\u00b7ze", "Sch\u00f6\u0