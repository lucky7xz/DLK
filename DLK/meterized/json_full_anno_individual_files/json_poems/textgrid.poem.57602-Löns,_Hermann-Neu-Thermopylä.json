{"textgrid.poem.57602": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Neu-Thermopyl\u00e4", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rechts unten da winkt hinter g\u00e4hnendem Spalt", "tokens": ["Rechts", "un\u00b7ten", "da", "winkt", "hin\u00b7ter", "g\u00e4h\u00b7nen\u00b7dem", "Spalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das zwanzigste, neue Jahrhundert,", "tokens": ["Das", "zwan\u00b7zigs\u00b7te", ",", "neu\u00b7e", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Auf zackiger Klippe zusammengeballt", "tokens": ["Auf", "za\u00b7cki\u00b7ger", "Klip\u00b7pe", "zu\u00b7sam\u00b7men\u00b7ge\u00b7ballt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Stehn ruhig die jungen Dreihundert,", "tokens": ["Stehn", "ru\u00b7hig", "die", "jun\u00b7gen", "Drei\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Dazwischen auch manch strammer Berserkergreis", "tokens": ["Da\u00b7zwi\u00b7schen", "auch", "manch", "stram\u00b7mer", "Ber\u00b7ser\u00b7ker\u00b7greis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Mit schlachtnarbig ruhmvollem Leib,", "tokens": ["Mit", "schlacht\u00b7nar\u00b7big", "ruhm\u00b7vol\u00b7lem", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und rosenwangig und h\u00e4ndewei\u00df", "tokens": ["Und", "ro\u00b7sen\u00b7wan\u00b7gig", "und", "h\u00e4n\u00b7de\u00b7wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Manch stolzes und geistk\u00fchnes Weib.", "tokens": ["Manch", "stol\u00b7zes", "und", "geist\u00b7k\u00fch\u00b7nes", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Hoch bannert das heilige Fahnentuch,", "tokens": ["Hoch", "ban\u00b7nert", "das", "hei\u00b7li\u00b7ge", "Fah\u00b7nen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es brausen die heiligen Psalter,", "tokens": ["Es", "brau\u00b7sen", "die", "hei\u00b7li\u00b7gen", "Psal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und kummerlos stehen trotz Schm\u00e4hspruch und Fluch", "tokens": ["Und", "kum\u00b7mer\u00b7los", "ste\u00b7hen", "trotz", "Schm\u00e4hs\u00b7pruch", "und", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die trotzigen Bannerhalter.", "tokens": ["Die", "trot\u00b7zi\u00b7gen", "Ban\u00b7ner\u00b7hal\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die rechte Hand pre\u00dft des Revolvers Schaft,", "tokens": ["Die", "rech\u00b7te", "Hand", "pre\u00dft", "des", "Re\u00b7vol\u00b7vers", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Weh dem, der nahe sich wagt,", "tokens": ["Weh", "dem", ",", "der", "na\u00b7he", "sich", "wagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die Linke umklammert mit mannhafter Kraft", "tokens": ["Die", "Lin\u00b7ke", "um\u00b7klam\u00b7mert", "mit", "mann\u00b7haf\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Den Fahnenstiel unverzagt.", "tokens": ["Den", "Fah\u00b7nen\u00b7stiel", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Von links her st\u00fcrmen mit rohem Geschrei", "tokens": ["Von", "links", "her", "st\u00fcr\u00b7men", "mit", "ro\u00b7hem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APZR", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Feinde wutspuckende Horden,", "tokens": ["Der", "Fein\u00b7de", "wut\u00b7spu\u00b7cken\u00b7de", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mit allem, was niedrig und ekelhaft sei,", "tokens": ["Mit", "al\u00b7lem", ",", "was", "nied\u00b7rig", "und", "e\u00b7kel\u00b7haft", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die heilige Schar zu ermorden.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Schar", "zu", "er\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein Sprenggescho\u00dfhagel f\u00e4llt dicht auf uns ein,", "tokens": ["Ein", "Spreng\u00b7ge\u00b7scho\u00df\u00b7ha\u00b7gel", "f\u00e4llt", "dicht", "auf", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Umfl\u00f6tet uns Helme und Haupt,", "tokens": ["Um\u00b7fl\u00f6\u00b7tet", "uns", "Hel\u00b7me", "und", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Kein einziger weicht aus den kampffrohen Reihn,", "tokens": ["Kein", "ein\u00b7zi\u00b7ger", "weicht", "aus", "den", "kampf\u00b7fro\u00b7hen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "An den Sieg jeder Mitk\u00e4mpfer glaubt.", "tokens": ["An", "den", "Sieg", "je\u00b7der", "Mit\u00b7k\u00e4mp\u00b7fer", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und wenn einer f\u00e4llt und wenn auch sein Haupt", "tokens": ["Und", "wenn", "ei\u00b7ner", "f\u00e4llt", "und", "wenn", "auch", "sein", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "KON", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zersplittert vom Bleikugelschauer,", "tokens": ["Zer\u00b7split\u00b7tert", "vom", "Blei\u00b7ku\u00b7gel\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "La\u00dft liegen, sein Tod uns die Hoffnung nicht raubt,", "tokens": ["La\u00dft", "lie\u00b7gen", ",", "sein", "Tod", "uns", "die", "Hoff\u00b7nung", "nicht", "raubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "PPOSAT", "NN", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es ist keine Zeit f\u00fcr die Trauer,", "tokens": ["Es", "ist", "kei\u00b7ne", "Zeit", "f\u00fcr", "die", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er starb wie er k\u00e4mpfte mit trotzigem Mut,", "tokens": ["Er", "starb", "wie", "er", "k\u00e4mpf\u00b7te", "mit", "trot\u00b7zi\u00b7gem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Gelobt sei er und benedeit \u2013", "tokens": ["Ge\u00b7lobt", "sei", "er", "und", "be\u00b7ne\u00b7deit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Haut kr\u00e4ftig drauf los und watet im Blut,", "tokens": ["Haut", "kr\u00e4f\u00b7tig", "drauf", "los", "und", "wa\u00b7tet", "im", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PAV", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sein Blut hat uns alle gefeit.", "tokens": ["Sein", "Blut", "hat", "uns", "al\u00b7le", "ge\u00b7feit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Schon t\u00fcrmt sich von Leichen ein herrlicher Wall,", "tokens": ["Schon", "t\u00fcrmt", "sich", "von", "Lei\u00b7chen", "ein", "herr\u00b7li\u00b7cher", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des Feindes Kadaver uns sch\u00fctzen,", "tokens": ["Des", "Fein\u00b7des", "Ka\u00b7da\u00b7ver", "uns", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf krachende Sch\u00e4del mit knirschendem Schall", "tokens": ["Auf", "kra\u00b7chen\u00b7de", "Sch\u00e4\u00b7del", "mit", "knir\u00b7schen\u00b7dem", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die harten Gewehrkolben blitzen,", "tokens": ["Die", "har\u00b7ten", "Ge\u00b7wehr\u00b7kol\u00b7ben", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O wonniges Fechten im spritzenden Blut,", "tokens": ["O", "won\u00b7ni\u00b7ges", "Fech\u00b7ten", "im", "sprit\u00b7zen\u00b7den", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die brechenden Augen zu schaun,", "tokens": ["Die", "bre\u00b7chen\u00b7den", "Au\u00b7gen", "zu", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Zerplatzt ist des Feindes mordgrimmige Wut", "tokens": ["Zer\u00b7platzt", "ist", "des", "Fein\u00b7des", "mord\u00b7grim\u00b7mi\u00b7ge", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und es packt ihn des Unsieges Graun.", "tokens": ["Und", "es", "packt", "ihn", "des", "Un\u00b7sie\u00b7ges", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Der Sieg ist unser \u2013 hinab in die Gruft", "tokens": ["Der", "Sieg", "ist", "un\u00b7ser", "\u2013", "hin\u00b7ab", "in", "die", "Gruft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$(", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Leiber mit siegfrohem Blicke,", "tokens": ["Die", "Lei\u00b7ber", "mit", "sieg\u00b7fro\u00b7hem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcllt aus des gestorbnen Jahrhunderts Gruft,", "tokens": ["F\u00fcllt", "aus", "des", "ge\u00b7storb\u00b7nen", "Jahr\u00b7hun\u00b7derts", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus Feindleichen baut euch die Br\u00fccke,", "tokens": ["Aus", "Feind\u00b7lei\u00b7chen", "baut", "euch", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Stimmt an das neue Befreiungslied", "tokens": ["Stimmt", "an", "das", "neu\u00b7e", "Be\u00b7frei\u00b7ungs\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Im leuchtenden Fr\u00fchsonnenschein,", "tokens": ["Im", "leuch\u00b7ten\u00b7den", "Fr\u00fch\u00b7son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit klingendem Spiele die Treuschar zieht", "tokens": ["Mit", "klin\u00b7gen\u00b7dem", "Spie\u00b7le", "die", "Treu\u00b7schar", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "In das neue Jahrhundert hinein!", "tokens": ["In", "das", "neu\u00b7e", "Jahr\u00b7hun\u00b7dert", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Rechts unten da winkt hinter g\u00e4hnendem Spalt", "tokens": ["Rechts", "un\u00b7ten", "da", "winkt", "hin\u00b7ter", "g\u00e4h\u00b7nen\u00b7dem", "Spalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das zwanzigste, neue Jahrhundert,", "tokens": ["Das", "zwan\u00b7zigs\u00b7te", ",", "neu\u00b7e", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Auf zackiger Klippe zusammengeballt", "tokens": ["Auf", "za\u00b7cki\u00b7ger", "Klip\u00b7pe", "zu\u00b7sam\u00b7men\u00b7ge\u00b7ballt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Stehn ruhig die jungen Dreihundert,", "tokens": ["Stehn", "ru\u00b7hig", "die", "jun\u00b7gen", "Drei\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Dazwi