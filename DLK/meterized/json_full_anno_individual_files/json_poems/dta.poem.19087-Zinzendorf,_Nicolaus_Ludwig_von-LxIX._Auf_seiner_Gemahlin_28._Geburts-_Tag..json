.5": {"text": "Und erstickt.", "tokens": ["Und", "er\u00b7stickt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Herr! gedencke an ein Hertze,", "tokens": ["Herr", "!", "ge\u00b7den\u00b7cke", "an", "ein", "Hert\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das sich noch nicht ohne Schmertze", "tokens": ["Das", "sich", "noch", "nicht", "oh\u00b7ne", "Schmert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADV", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter deine T\u00f6dtung b\u00fcckt.", "tokens": ["Un\u00b7ter", "dei\u00b7ne", "T\u00f6d\u00b7tung", "b\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Jesu! rette deine Kraft,", "tokens": ["Je\u00b7su", "!", "ret\u00b7te", "dei\u00b7ne", "Kraft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ngst an einer Seel erwiesen,", "tokens": ["L\u00e4ngst", "an", "ei\u00b7ner", "Seel", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor diesem", "tokens": ["Die", "vor", "die\u00b7sem"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "PDAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Unsre grosse Hofnung war,", "tokens": ["Uns\u00b7re", "gros\u00