 "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und bereit seyn kan zu allen:", "tokens": ["Und", "be\u00b7reit", "seyn", "kan", "zu", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bring dein Feur zur lichten Loh!", "tokens": ["Bring", "dein", "Feur", "zur", "lich\u00b7ten", "Loh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Dem die Tugend nicht genug,", "tokens": ["Dem", "die", "Tu\u00b7gend", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ein neues Hertze fodert,", "tokens": ["Der", "ein", "neu\u00b7es", "Hert\u00b7ze", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das da lodert", "tokens": ["Das", "da", "lo\u00b7dert"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Von den Flammen seiner Glut,", "tokens": ["Von", "den", "Flam\u00b7men", "sei\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm den Muth,", "tokens": ["Nimm", "den", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Jmmer me