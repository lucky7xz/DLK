 "tokens": ["Ha\u00b7be", "acht", "auf", "ein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das du schon vor vielen Stunden", "tokens": ["Das", "du", "schon", "vor", "vie\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir verbunden,", "tokens": ["Dir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Welches auch dein sanftes Joch", "tokens": ["Wel\u00b7ches", "auch", "dein", "sanf\u00b7tes", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jmmer noch,", "tokens": ["Jm\u00b7mer", "noch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Doch nicht nach der Absicht tr\u00e4get,", "tokens": ["Doch", "nicht", "nach", "der", "Ab\u00b7sicht", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wie du ihm es aufgeleget,", "tokens": ["Wie", "du", "ihm", "es", "auf\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als es aus den Banden kroch.", "tokens": ["Als", "es", "aus", "den", "Ban\u00b7den", "kroch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Der du mit den S\u00fcndern pflegst,", "tokens": ["Der", "du", "mit", "den", "S\u00fcn\u00b7dern", "pflegst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bi\u00df zum Aergerni\u00df zu speisen,", "tokens": ["Bi\u00df", "zum", "A\u00b7er\u00b7ger\u00b7ni\u00df", "zu", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und zu weisen,", "tokens": ["Und", "zu", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da\u00df wir aus der Gnad allein,", "tokens": ["Da\u00df", "wir", "aus", "der", "Gnad", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles seyn;", "tokens": ["Al\u00b7les", "seyn", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Siege fort, du Uberwinder,", "tokens": ["Sie\u00b7ge", "fort", ",", "du", "U\u00b7ber\u00b7win\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem Gr\u00f6ssesten der S\u00fcnder,", "tokens": ["In", "dem", "Gr\u00f6s\u00b7ses\u00b7ten", "der", "S\u00fcn\u00b7der", ","], "token_info": ["word