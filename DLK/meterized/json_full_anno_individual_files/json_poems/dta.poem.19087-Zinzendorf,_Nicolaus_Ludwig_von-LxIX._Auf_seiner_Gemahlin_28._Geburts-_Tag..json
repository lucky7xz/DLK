{"dta.poem.19087": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxIX.  Auf seiner Gemahlin 28. Geburts-  \n Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "O Du H\u00fcter Ephraim,\nDieses Gedichte ist an dem Geburts-Fest der Gr\u00e4fin, bey ei-\nner vertrauten Gesellschaft oder Lotterie abgesungen wor-\nden, und ein iegliches Mitglied derselben dergestalt bedeu-\ntet, als es seine damalige Umst\u00e4nde mit sich gebracht. Des geringsten Theils der Heerde,", "tokens": ["O", "Du", "H\u00fc\u00b7ter", "Eph\u00b7raim", ",", "Die\u00b7ses", "Ge\u00b7dich\u00b7te", "ist", "an", "dem", "Ge\u00b7burts\u00b7Fest", "der", "Gr\u00e4\u00b7fin", ",", "bey", "ei", "ner", "ver\u00b7trau\u00b7ten", "Ge\u00b7sell\u00b7schaft", "o\u00b7der", "Lot\u00b7te\u00b7rie", "ab\u00b7ge\u00b7sun\u00b7gen", "wor", "den", ",", "und", "ein", "ieg\u00b7li\u00b7ches", "Mit\u00b7glied", "der\u00b7sel\u00b7ben", "der\u00b7ge\u00b7stalt", "be\u00b7deu", "tet", ",", "als", "es", "sei\u00b7ne", "da\u00b7ma\u00b7li\u00b7ge", "Um\u00b7st\u00e4n\u00b7de", "mit", "sich", "ge\u00b7bracht", ".", "Des", "ge\u00b7rings\u00b7ten", "Theils", "der", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "NE", "$,", "PDAT", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "TRUNC", "ADJA", "ADJA", "NN", "KON", "NN", "VVPP", "TRUNC", "ART", "$,", "KON", "ART", "PIAT", "NN", "PDAT", "FM.la", "FM.la", "FM.la", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PRF", "VVPP", "$.", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+--+--+--+--+--+-+-+--+-+-+-+-+-+-+-+-+-+-+-+-+--+---+-+--+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Deiner Erde!", "tokens": ["Dei\u00b7ner", "Er\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Unser H\u00e4ufl ein sieht mit Schmertz", "tokens": ["Un\u00b7ser", "H\u00e4ufl", "ein", "sieht", "mit", "Schmertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Niederw\u00e4rts;", "tokens": ["Nie\u00b7der\u00b7w\u00e4rts", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Aber unsre Sinnen blicken,", "tokens": ["A\u00b7ber", "uns\u00b7re", "Sin\u00b7nen", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mitten in dem Niederb\u00fccken,", "tokens": ["Mit\u00b7ten", "in", "dem", "Nie\u00b7der\u00b7b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In dein hocherhabnes Hertz.", "tokens": ["In", "dein", "ho\u00b7cher\u00b7hab\u00b7nes", "Hertz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hertz der G\u00f6ttlichen Natur,", "tokens": ["Hertz", "der", "G\u00f6tt\u00b7li\u00b7chen", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hertz der offenbahrten Liebe,", "tokens": ["Hertz", "der", "of\u00b7fen\u00b7bahr\u00b7ten", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hertz der Triebe,", "tokens": ["Hertz", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Unsre Hertzen opfern dir", "tokens": ["Uns\u00b7re", "Hert\u00b7zen", "op\u00b7fern", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe hier,", "tokens": ["Lie\u00b7be", "hier", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und in brennendem Verlangen,", "tokens": ["Und", "in", "bren\u00b7nen\u00b7dem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Salbung zu empfangen,", "tokens": ["Dei\u00b7ne", "Sal\u00b7bung", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oefnet sich des Geistes Th\u00fcr.", "tokens": ["Oef\u00b7net", "sich", "des", "Geis\u00b7tes", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hertz der Welt! belebe uns,", "tokens": ["Hertz", "der", "Welt", "!", "be\u00b7le\u00b7be", "uns", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr als alles, was da lebet,", "tokens": ["Mehr", "als", "al\u00b7les", ",", "was", "da", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dir webet,", "tokens": ["In", "dir", "we\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und sich HErr von deiner Macht,", "tokens": ["Und", "sich", "Herr", "von", "dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wunder-Pracht", "tokens": ["Wun\u00b7der\u00b7Pracht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und Allgegenwart ersch\u00fcttert.", "tokens": ["Und", "All\u00b7ge\u00b7gen\u00b7wart", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gottheit, unsre H\u00fctte zittert,", "tokens": ["Got\u00b7theit", ",", "uns\u00b7re", "H\u00fct\u00b7te", "zit\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber unser Hertze lacht!", "tokens": ["A\u00b7ber", "un\u00b7ser", "Hert\u00b7ze", "lacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hertz mit uns! wir schweren dir", "tokens": ["Hertz", "mit", "uns", "!", "wir", "schwe\u00b7ren", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewige Gesellen-Treue,", "tokens": ["E\u00b7wi\u00b7ge", "Ge\u00b7sel\u00b7len\u00b7Treue", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als aufs neue.", "tokens": ["Als", "aufs", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Dir ist unser Hertz bekannt;", "tokens": ["Dir", "ist", "un\u00b7ser", "Hertz", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm die Hand,", "tokens": ["Nimm", "die", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Zur Verpf\u00e4ndung aller Triebe,", "tokens": ["Zur", "Ver\u00b7pf\u00e4n\u00b7dung", "al\u00b7ler", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zur Vergeltung einer Liebe,", "tokens": ["Zur", "Ver\u00b7gel\u00b7tung", "ei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ihr Blut an uns gewandt.", "tokens": ["Die", "ihr", "Blut", "an", "uns", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hertz der Kraft! durchdringe doch,", "tokens": ["Hertz", "der", "Kraft", "!", "durch\u00b7drin\u00b7ge", "doch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Seel ist ja genesen,", "tokens": ["Uns\u00b7re", "Seel", "ist", "ja", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All ihr Wesen:", "tokens": ["All", "ihr", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mach ihr alles, was da wahr,", "tokens": ["Mach", "ihr", "al\u00b7les", ",", "was", "da", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonnen-klar;", "tokens": ["Son\u00b7nen\u00b7klar", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aber was dir nicht will taugen,", "tokens": ["A\u00b7ber", "was", "dir", "nicht", "will", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das verbirg vor unsern Augen,", "tokens": ["Das", "ver\u00b7birg", "vor", "un\u00b7sern", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00fcter der verschlo\u00dfnen Schaar!", "tokens": ["H\u00fc\u00b7ter", "der", "ver\u00b7schlo\u00df\u00b7nen", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier ist eine J\u00fcngerin,", "tokens": ["Hier", "ist", "ei\u00b7ne", "J\u00fcn\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottheit von dem grossen Worte,", "tokens": ["Got\u00b7theit", "von", "dem", "gros\u00b7sen", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die Pforte", "tokens": ["Das", "die", "Pfor\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Und der Weg der Seelen ist", "tokens": ["Und", "der", "Weg", "der", "See\u00b7len", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesu Christ!", "tokens": ["Je\u00b7su", "Christ", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Heute kam sie auf die Erde,", "tokens": ["Heu\u00b7te", "kam", "sie", "auf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heute kam sie auch zur Heerde,", "tokens": ["Heu\u00b7te", "kam", "sie", "auch", "zur", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lamm! wo du der Hirte bist.", "tokens": ["Lamm", "!", "wo", "du", "der", "Hir\u00b7te", "bist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Heute ist ihr Gnaden-Tag:", "tokens": ["Heu\u00b7te", "ist", "ihr", "Gna\u00b7den\u00b7Tag", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nig! dem sie zugeh\u00f6ret,", "tokens": ["K\u00f6\u00b7nig", "!", "dem", "sie", "zu\u00b7ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eingekehret,", "tokens": ["Ein\u00b7ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Neigt sie sich, und betet an,", "tokens": ["Neigt", "sie", "sich", ",", "und", "be\u00b7tet", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sie kan,", "tokens": ["Wie", "sie", "kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und wir wolln uns mit ihr beugen,", "tokens": ["Und", "wir", "wolln", "uns", "mit", "ihr", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Licht, wir deine andre Zeugen,", "tokens": ["Licht", ",", "wir", "dei\u00b7ne", "and\u00b7re", "Zeu\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhr Geschwister und ihr Mann.", "tokens": ["Ihr", "Ge\u00b7schwis\u00b7ter", "und", "ihr", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df dein Leben ihren Geist", "tokens": ["La\u00df", "dein", "Le\u00b7ben", "ih\u00b7ren", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf das kr\u00e4ftigste erheben,", "tokens": ["Auf", "das", "kr\u00e4f\u00b7tigs\u00b7te", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df sie leben,", "tokens": ["La\u00df", "sie", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Jhre Seele werde dir", "tokens": ["Ih\u00b7re", "See\u00b7le", "wer\u00b7de", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Zier", "tokens": ["Ei\u00b7ne", "Zier"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und ihr \u00e4usserlicher Wandel,", "tokens": ["Und", "ihr", "\u00e4us\u00b7ser\u00b7li\u00b7cher", "Wan\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeuge von dem innern Handel", "tokens": ["Zeu\u00b7ge", "von", "dem", "in\u00b7nern", "Han\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deiner Lieblichkeit in ihr!", "tokens": ["Dei\u00b7ner", "Lieb\u00b7lich\u00b7keit", "in", "ihr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Der ihr Allern\u00e4chster ist,", "tokens": ["Der", "ihr", "Al\u00b7ler\u00b7n\u00e4chs\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Findt sich nirgend wo geschwinder,", "tokens": ["Findt", "sich", "nir\u00b7gend", "wo", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als beym S\u00fcnder,", "tokens": ["Als", "beym", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Der gerecht sein Hau\u00df betrat,", "tokens": ["Der", "ge\u00b7recht", "sein", "Hau\u00df", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er bat:", "tokens": ["Weil", "er", "bat", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "O du ihm bekannte Liebe!", "tokens": ["O", "du", "ihm", "be\u00b7kann\u00b7te", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Reinige, beleb und \u00fcbe", "tokens": ["Rei\u00b7ni\u00b7ge", ",", "be\u00b7leb", "und", "\u00fc\u00b7be"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Sinn, sein Wort und That!", "tokens": ["Sei\u00b7nen", "Sinn", ",", "sein", "Wort", "und", "That", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Kr\u00e4fte m\u00fcssen von dir aus", "tokens": ["Kr\u00e4f\u00b7te", "m\u00fcs\u00b7sen", "von", "dir", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "PPER", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In ein junges Hertze gehen.", "tokens": ["In", "ein", "jun\u00b7ges", "Hert\u00b7ze", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "mea