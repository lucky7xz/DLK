{"dta.poem.18050": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schnecke hatte sich tief in ihr Haus gezogen;", "tokens": ["Die", "Schne\u00b7cke", "hat\u00b7te", "sich", "tief", "in", "ihr", "Haus", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da kam ein starcker Aar geflogen.", "tokens": ["Da", "kam", "ein", "star\u00b7cker", "Aar", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der fasset sie mit seinen Klauen an,", "tokens": ["Der", "fas\u00b7set", "sie", "mit", "sei\u00b7nen", "Klau\u00b7en", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er zweifelt, was es sey: als auf denselben Plan", "tokens": ["Er", "zwei\u00b7felt", ",", "was", "es", "sey", ":", "als", "auf", "den\u00b7sel\u00b7ben", "Plan"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$.", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gleich eine Kr\u00e4he k\u00f6mmt, und zu ihm saget: H\u00f6re,", "tokens": ["Gleich", "ei\u00b7ne", "Kr\u00e4\u00b7he", "k\u00f6mmt", ",", "und", "zu", "ihm", "sa\u00b7get", ":", "H\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "PPER", "VVFIN", "$.", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Schaal ist guter Speise voll:", "tokens": ["Die", "Schaal", "ist", "gu\u00b7ter", "Spei\u00b7se", "voll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch folgest du nicht meiner Lehre,", "tokens": ["Doch", "fol\u00b7gest", "du", "nicht", "mei\u00b7ner", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geniessest du sie nimmer wohl.", "tokens": ["Ge\u00b7nies\u00b7sest", "du", "sie", "nim\u00b7mer", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Flieg auf, und schwinge dein Gesieder;", "tokens": ["Flieg", "auf", ",", "und", "schwin\u00b7ge", "dein", "Ge\u00b7sie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dann wirff mit Macht die Schnecke nieder.", "tokens": ["Dann", "wirff", "mit", "Macht", "die", "Schne\u00b7cke", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zerbrich die Schaal und glaube mir", "tokens": ["Zer\u00b7brich", "die", "Schaal", "und", "glau\u00b7be", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Du hast dann Speise nach Begier.", "tokens": ["Du", "hast", "dann", "Spei\u00b7se", "nach", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Kr\u00e4he lehrt den Adler so.", "tokens": ["Die", "Kr\u00e4\u00b7he", "lehrt", "den", "Ad\u00b7ler", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "De\u00df ward die Schnecke gar nicht froh.", "tokens": ["De\u00df", "ward", "die", "Schne\u00b7cke", "gar", "nicht", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er warf sie und ihr gantzes Haus", "tokens": ["Er", "warf", "sie", "und", "ihr", "gant\u00b7zes", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "An einen Stein, es brach, sie fiel heraus.", "tokens": ["An", "ei\u00b7nen", "Stein", ",", "es", "brach", ",", "sie", "fiel", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Kr\u00e4he nahm der Beute wahr,", "tokens": ["Die", "Kr\u00e4\u00b7he", "nahm", "der", "Beu\u00b7te", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Fuhr zu, und a\u00df sie vor dem Aar.", "tokens": ["Fuhr", "zu", ",", "und", "a\u00df", "sie", "vor", "dem", "Aar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}