{"dta.poem.3236": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Des Herrn von Mauvillon Brief  \n von den deutschen Poeten.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198490", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Benennt man, Nassau, dich sonst von den nassen Auen,", "tokens": ["Be\u00b7nennt", "man", ",", "Nas\u00b7sau", ",", "dich", "sonst", "von", "den", "nas\u00b7sen", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "NE", "$,", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So machstu itzt gewi\u00df die Deutung offenbar;", "tokens": ["So", "machs\u00b7tu", "itzt", "ge\u00b7wi\u00df", "die", "Deu\u00b7tung", "of\u00b7fen\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und stellten sich auch nicht die nassen Auen dar,", "tokens": ["Und", "stell\u00b7ten", "sich", "auch", "nicht", "die", "nas\u00b7sen", "Au\u00b7en", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kan man doch in dir die nassen Auen schauen.", "tokens": ["So", "kan", "man", "doch", "in", "dir", "die", "nas\u00b7sen", "Au\u00b7en", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPR", "PPER", 