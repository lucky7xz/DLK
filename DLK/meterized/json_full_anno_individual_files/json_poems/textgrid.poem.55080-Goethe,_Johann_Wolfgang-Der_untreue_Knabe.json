{"textgrid.poem.55080": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der untreue Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Knabe frech genung,", "tokens": ["Es", "war", "ein", "Kna\u00b7be", "frech", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War erst aus Frankreich kommen,", "tokens": ["War", "erst", "aus", "Fran\u00b7kreich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der hatt ein armes M\u00e4del jung", "tokens": ["Der", "hatt", "ein", "ar\u00b7mes", "M\u00e4\u00b7del", "jung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar oft in Arm genommen", "tokens": ["Gar", "oft", "in", "Arm", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und liebgekost und liebgeherzt,", "tokens": ["Und", "lieb\u00b7ge\u00b7kost", "und", "lieb\u00b7ge\u00b7herzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Br\u00e4utigam herumgescherzt,", "tokens": ["Als", "Br\u00e4u\u00b7ti\u00b7gam", "her\u00b7um\u00b7ge\u00b7scherzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und endlich sie verlassen.", "tokens": ["Und", "end\u00b7lich", "sie", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das braune M\u00e4del das erfuhr,", "tokens": ["Das", "brau\u00b7ne", "M\u00e4\u00b7del", "das", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergingen ihr die Sinnen,", "tokens": ["Ver\u00b7gin\u00b7gen", "ihr", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lacht' und weint' und bet' und schwur;", "tokens": ["Sie", "lacht'", "und", "weint'", "und", "bet'", "und", "schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So fuhr die Seel von hinnen.", "tokens": ["So", "fuhr", "die", "Seel", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Stund, da sie verschieden war,", "tokens": ["Die", "Stund", ",", "da", "sie", "ver\u00b7schie\u00b7den", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird bang dem Buben, graust sein Haar,", "tokens": ["Wird", "bang", "dem", "Bu\u00b7ben", ",", "graust", "sein", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es treibt ihn fort zu Pferde.", "tokens": ["Es", "treibt", "ihn", "fort", "zu", "Pfer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er gab die Sporen kreuz und quer", "tokens": ["Er", "gab", "die", "Spo\u00b7ren", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ritt auf alle Seiten,", "tokens": ["Und", "ritt", "auf", "al\u00b7le", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Her\u00fcber, hin\u00fcber, hin und her,", "tokens": ["Her\u00b7\u00fc\u00b7ber", ",", "hin\u00b7\u00fc\u00b7ber", ",", "hin", "und", "her", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "ADV", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kann keine Ruh erreiten,", "tokens": ["Kann", "kei\u00b7ne", "Ruh", "er\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Reit' sieben Tag und sieben Nacht;", "tokens": ["Reit'", "sie\u00b7ben", "Tag", "und", "sie\u00b7ben", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "KON", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es blitzt und donnert, st\u00fcrmt und kracht,", "tokens": ["Es", "blitzt", "und", "don\u00b7nert", ",", "st\u00fcrmt", "und", "kracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Fluten rei\u00dfen \u00fcber.", "tokens": ["Die", "Flu\u00b7ten", "rei\u00b7\u00dfen", "\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und reit' in Blitz und Wetterschein", "tokens": ["Und", "reit'", "in", "Blitz", "und", "Wet\u00b7ter\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gem\u00e4uerwerk entgegen,", "tokens": ["Ge\u00b7m\u00e4u\u00b7er\u00b7werk", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bindt 's Pferd hau\u00df an und kriecht hinein", "tokens": ["Bindt", "'s", "Pferd", "hau\u00df", "an", "und", "kriecht", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "NN", "PTKVZ", "KON", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und duckt sich vor dem Regen.", "tokens": ["Und", "duckt", "sich", "vor", "dem", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wie er tappt und wie er f\u00fchlt,", "tokens": ["Und", "wie", "er", "tappt", "und", "wie", "er", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich unter ihm die Erd erw\u00fchlt;", "tokens": ["Sich", "un\u00b7ter", "ihm", "die", "Erd", "er\u00b7w\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er st\u00fcrzt wohl hundert Klafter.", "tokens": ["Er", "st\u00fcrzt", "wohl", "hun\u00b7dert", "Klaf\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als er sich ermannt vom Schlag,", "tokens": ["Und", "als", "er", "sich", "er\u00b7mannt", "vom", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht er drei Lichtlein schleichen.", "tokens": ["Sieht", "er", "drei", "Licht\u00b7lein", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er rafft sich auf und krabbelt nach;", "tokens": ["Er", "rafft", "sich", "auf", "und", "krab\u00b7belt", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lichtlein ferne weichen;", "tokens": ["Die", "Licht\u00b7lein", "fer\u00b7ne", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Irrf\u00fchren ihn die Quer und L\u00e4ng,", "tokens": ["Irr\u00b7f\u00fch\u00b7ren", "ihn", "die", "Quer", "und", "L\u00e4ng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Treppauf, treppab, durch enge G\u00e4ng,", "tokens": ["Trep\u00b7pauf", ",", "tre\u00b7ppab", ",", "durch", "en\u00b7ge", "G\u00e4ng", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verfallne, w\u00fcste Keller.", "tokens": ["Ver\u00b7fall\u00b7ne", ",", "w\u00fcs\u00b7te", "Kel\u00b7ler", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auf einmal steht er hoch im Saal,", "tokens": ["Auf", "ein\u00b7mal", "steht", "er", "hoch", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht sitzen hundert G\u00e4ste,", "tokens": ["Sieht", "sit\u00b7zen", "hun\u00b7dert", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hohl\u00e4ugig grinsen allzumal", "tokens": ["Hoh\u00b7l\u00e4u\u00b7gig", "grin\u00b7sen", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und winken ihm zum Feste.", "tokens": ["Und", "win\u00b7ken", "ihm", "zum", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sieht sein Sch\u00e4tzel untenan", "tokens": ["Er", "sieht", "sein", "Sch\u00e4t\u00b7zel", "un\u00b7te\u00b7nan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit wei\u00dfen T\u00fcchern angetan,", "tokens": ["Mit", "wei\u00b7\u00dfen", "T\u00fc\u00b7chern", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die wendt sich \u2013", "tokens": ["Die", "wendt", "sich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Es war ein Knabe frech genung,", "tokens": ["Es", "war", "ein", "Kna\u00b7be", "frech", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War erst aus Frankreich kommen,", "tokens": ["War", "erst", "aus", "Fran\u00b7kreich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der hatt ein armes M\u00e4del jung", "tokens": ["Der", "hatt", "ein", "ar\u00b7mes", "M\u00e4\u00b7del", "jung"], "token_info": ["word"