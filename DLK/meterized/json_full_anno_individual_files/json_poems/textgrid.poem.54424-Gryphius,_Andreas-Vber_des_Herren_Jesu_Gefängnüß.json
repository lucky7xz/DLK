{"textgrid.poem.54424": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Vber des Herren Jesu Gef\u00e4ngn\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie im Garten sind dem Teuffel eingegangen/", "tokens": ["Gleich", "wie", "im", "Gar\u00b7ten", "sind", "dem", "Teuf\u00b7fel", "ein\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins auffgestalte Netz/ gantz blind vnd vnbedacht/", "tokens": ["Ins", "auff\u00b7ge\u00b7stal\u00b7te", "Netz", "/", "gantz", "blind", "vnd", "vn\u00b7be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "ADV", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ihre Missethat han auff vns erblich bracht/", "tokens": ["Die", "Ih\u00b7re", "Mis\u00b7se\u00b7that", "han", "auff", "vns", "er\u00b7blich", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird im Garten auch/ doch ohne Schuld gefangen/", "tokens": ["So", "wird", "im", "Gar\u00b7ten", "auch", "/", "doch", "oh\u00b7ne", "Schuld", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADV", "$(", "ADV", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-