{"textgrid.poem.42193": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst lag ich ausgestreckt in wachem Traum,", "tokens": ["Einst", "lag", "ich", "aus\u00b7ge\u00b7streckt", "in", "wa\u00b7chem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erm\u00fcdet von der Arbeit langer N\u00e4chte,", "tokens": ["Er\u00b7m\u00fc\u00b7det", "von", "der", "Ar\u00b7beit", "lan\u00b7ger", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da frug ein Kuckucksruf aus hohem Baum,", "tokens": ["Da", "frug", "ein", "Ku\u00b7ckucks\u00b7ruf", "aus", "ho\u00b7hem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was sich das junge Herz wohl w\u00fcnschen m\u00f6chte.", "tokens": ["Was", "sich", "das", "jun\u00b7ge", "Herz", "wohl", "w\u00fcn\u00b7schen", "m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Frage war die Antwort rasch bereit,", "tokens": ["Der", "Fra\u00b7ge", "war", "die", "Ant\u00b7wort", "rasch", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun durfte nichts mir die Erf\u00fcllung rauben,", "tokens": ["Nun", "durf\u00b7te", "nichts", "mir", "die", "Er\u00b7f\u00fcl\u00b7lung", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eine unerme\u00dfne Seligkeit", "tokens": ["Und", "ei\u00b7ne", "un\u00b7er\u00b7me\u00df\u00b7ne", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erwuchs mir aus dem frommen Kinderglauben.", "tokens": ["Er\u00b7wuchs", "mir", "aus", "dem", "from\u00b7men", "Kin\u00b7der\u00b7glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Lebens Sommer ist derweil verbl\u00fcht", "tokens": ["Des", "Le\u00b7bens", "Som\u00b7mer", "ist", "der\u00b7weil", "ver\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Hoffnung sah um Hoffnung ich zerrinnen;", "tokens": ["Und", "Hoff\u00b7nung", "sah", "um", "Hoff\u00b7nung", "ich", "zer\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus meinem grellerleuchteten Gem\u00fct", "tokens": ["Aus", "mei\u00b7nem", "grel\u00b7ler\u00b7leuch\u00b7te\u00b7ten", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schlich auch besch\u00e4mt ein dunkler Wahn von hinnen.", "tokens": ["Schlich", "auch", "be\u00b7sch\u00e4mt", "ein", "dunk\u00b7ler", "Wahn", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "ART", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In diesen Zeilen fand er Unterkunft;", "tokens": ["In", "die\u00b7sen", "Zei\u00b7len", "fand", "er", "Un\u00b7ter\u00b7kunft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier liegt er f\u00fcr des Lebens Rest begraben.", "tokens": ["Hier", "liegt", "er", "f\u00fcr", "des", "Le\u00b7bens", "Rest", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wird der Mensch ein Kr\u00f6sus an Vernunft", "tokens": ["So", "wird", "der", "Mensch", "ein", "Kr\u00f6\u00b7sus", "an", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und, ach, wie bettelarm durch ihre Gaben!", "tokens": ["Und", ",", "ach", ",", "wie", "bet\u00b7tel\u00b7arm", "durch", "ih\u00b7re", "Ga\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$