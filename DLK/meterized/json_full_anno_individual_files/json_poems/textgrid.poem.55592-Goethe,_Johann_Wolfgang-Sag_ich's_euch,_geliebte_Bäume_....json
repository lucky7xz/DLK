{"textgrid.poem.55592": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Sag ich's euch, geliebte B\u00e4ume ...", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag ich's euch, geliebte B\u00e4ume,", "tokens": ["Sag", "ich's", "euch", ",", "ge\u00b7lieb\u00b7te", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich ahndevoll gepflanzt,", "tokens": ["Die", "ich", "ahn\u00b7de\u00b7voll", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die wunderbarsten Tr\u00e4ume", "tokens": ["Als", "die", "wun\u00b7der\u00b7bars\u00b7ten", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgenr\u00f6tlich mich umtanzt.", "tokens": ["Mor\u00b7gen\u00b7r\u00f6t\u00b7lich", "mich", "um\u00b7tanzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, ihr wi\u00dft es, wie ich liebe,", "tokens": ["Ach", ",", "ihr", "wi\u00dft", "es", ",", "wie", "ich", "lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die so sch\u00f6n mich wiederliebt,", "tokens": ["Die", "so", "sch\u00f6n", "mich", "wie\u00b7der\u00b7liebt", ","],