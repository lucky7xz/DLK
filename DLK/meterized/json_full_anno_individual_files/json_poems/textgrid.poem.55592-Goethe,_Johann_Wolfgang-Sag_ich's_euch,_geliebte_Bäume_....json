s": ["Brin\u00b7get", "Schat\u00b7ten", ",", "tra\u00b7get", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Neue Freude jeden Tag;", "tokens": ["Neu\u00b7e", "Freu\u00b7de", "je\u00b7den", "Tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur da\u00df ich sie dichte, dichte,", "tokens": ["Nur", "da\u00df", "ich", "sie", "dich\u00b7te", ",", "dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dicht bei ihr genie\u00dfen mag!", "tokens": ["Dicht", "bei", "ihr", "ge\u00b7nie\u00b7\u00dfen", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sag ich's euch, geliebte B\u00e4ume,", "tokens": ["Sag", "ich's", "euch", ",", "ge\u00b7lieb\u00b7te", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich ahndevoll gepflanzt,", "tokens": ["Die", "ich", "ahn\u00b7de\u00b7voll", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die wunderbarsten Tr\u00e4ume", "tokens": ["Als", "die", "wun\u00b7der\u00b7bars\u00b7ten", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "A