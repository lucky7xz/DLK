{"dta.poem.19713": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Hans Steutlinger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wollen wir singen und heben an,               ", "tokens": ["Was", "wol\u00b7len", "wir", "sin\u00b7gen", "und", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von einem Hans Steutlinger,", "tokens": ["Von", "ei\u00b7nem", "Hans", "Steut\u00b7lin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat aus dem Adel geheurathet,", "tokens": ["Hat", "aus", "dem", "A\u00b7del", "ge\u00b7heu\u00b7ra\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat geheurath ein adliche Frau.", "tokens": ["Hat", "ge\u00b7heu\u00b7rath", "ein", "ad\u00b7li\u00b7che", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ei Knechte lieber Knechte mein,", "tokens": ["Ei", "Knech\u00b7te", "lie\u00b7ber", "Knech\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sattel mir und dir zwei Pferd,", "tokens": ["Sat\u00b7tel", "mir", "und", "dir", "zwei", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PPER", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gen Freiburg wollen wir reiten,", "tokens": ["Gen", "Frei\u00b7burg", "wol\u00b7len", "wir", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gen Offenburg haben wir guten Weg.", "tokens": ["Gen", "Of\u00b7fen\u00b7burg", "ha\u00b7ben", "wir", "gu\u00b7ten", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und da ich in Freiburg eine kam,", "tokens": ["Und", "da", "ich", "in", "Frei\u00b7burg", "ei\u00b7ne", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NE", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrs jungen Herrn Friedrich sein Hau\u00df,", "tokens": ["F\u00fcrs", "jun\u00b7gen", "Herrn", "Fried\u00b7rich", "sein", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schaute der junge Herr Friedrich", "tokens": ["Da", "schau\u00b7te", "der", "jun\u00b7ge", "Herr", "Fried\u00b7rich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum obern Fenster heraus.", "tokens": ["Zum", "o\u00b7bern", "Fens\u00b7ter", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Hans Steutlinger, lieber Hans Steutlinger,", "tokens": ["Hans", "Steut\u00b7lin\u00b7ger", ",", "lie\u00b7ber", "Hans", "Steut\u00b7lin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "NE", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kommt zu mir jezt herein,", "tokens": ["Kommt", "zu", "mir", "jezt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steigt ab jezt von euerem Sattel,", "tokens": ["Steigt", "ab", "jezt", "von", "eu\u00b7e\u00b7rem", "Sat\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Helft essen die wildesten Schwein.", "tokens": ["Helft", "es\u00b7sen", "die", "wil\u00b7des\u00b7ten", "Schwein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Vom Sattel will ich wohl steigen,", "tokens": ["Vom", "Sat\u00b7tel", "will", "ich", "wohl", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will treten auch zu euch hinein,", "tokens": ["Will", "tre\u00b7ten", "auch", "zu", "euch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ihr mir wollet verhei\u00dfen,", "tokens": ["Wenn", "ihr", "mir", "wol\u00b7let", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da\u00df ich kein Gefangner mehr sey.", "tokens": ["Da\u00df", "ich", "kein", "Ge\u00b7fang\u00b7ner", "mehr", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Sie gaben dem Hans Steutlinger gute Wort,", "tokens": ["Sie", "ga\u00b7ben", "dem", "Hans", "Steut\u00b7lin\u00b7ger", "gu\u00b7te", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bis sie ihn brachten oben an Tisch:", "tokens": ["Bis", "sie", "ihn", "brach\u00b7ten", "o\u00b7ben", "an", "Tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ei i\u00df und trink Hans Steutlinger,", "tokens": ["Ei", "i\u00df", "und", "trink", "Hans", "Steut\u00b7lin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein Leben wird nimmermehr frisch.", "tokens": ["Dein", "Le\u00b7ben", "wird", "nim\u00b7mer\u00b7mehr", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Wie kann ich essen und trinken,", "tokens": ["Wie", "kann", "ich", "es\u00b7sen", "und", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wie kann ich nur fr\u00f6hlich sein,", "tokens": ["Wie", "kann", "ich", "nur", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Herz m\u00f6gt mir versinken", "tokens": ["Mein", "Herz", "m\u00f6gt", "mir", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Meth und beim k\u00fchlesten Wein.", "tokens": ["Beim", "Me\u00b7th", "und", "beim", "k\u00fch\u00b7les\u00b7ten", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Hans Steutlinger, wem vermacht ihr euer Weib,", "tokens": ["Hans", "Steut\u00b7lin\u00b7ger", ",", "wem", "ver\u00b7macht", "ihr", "eu\u00b7er", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ich vermach sie dem lieben Herrn Friederich,", "tokens": ["Ich", "ver\u00b7mach", "sie", "dem", "lie\u00b7ben", "Herrn", "Frie\u00b7de\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dem vermach ich ihren untreuen Leib,", "tokens": ["Dem", "ver\u00b7mach", "ich", "ih\u00b7ren", "un\u00b7treu\u00b7en", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der sieht sie viel lieber noch als ich.", "tokens": ["Der", "sieht", "sie", "viel", "lie\u00b7ber", "noch", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Hans Steutlinger, lieber Hans Steutlinger,", "tokens": ["Hans", "Steut\u00b7lin\u00b7ger", ",", "lie\u00b7ber", "Hans", "Steut\u00b7lin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "NE", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wem vermacht ihr eure Kind?", "tokens": ["Wem", "ver\u00b7macht", "ihr", "eu\u00b7re", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich vermach sie dem lieben Gott selber,", "tokens": ["Ich", "ver\u00b7mach", "sie", "dem", "lie\u00b7ben", "Gott", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Der wei\u00df am besten, wem sie sind.", "tokens": ["Der", "wei\u00df", "am", "bes\u00b7ten", ",", "wem", "sie", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKA", "ADJD", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Hans Steutlinger, lieber Hans Steutlinger,", "tokens": ["Hans", "Steut\u00b7lin\u00b7ger", ",", "lie\u00b7ber", "Hans", "Steut\u00b7lin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "NE", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wem vermachet ihr euer Gut?", "tokens": ["Wem", "ver\u00b7ma\u00b7chet", "ihr", "eu\u00b7er", "Gut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ich vermachs den armen Leuten,", "tokens": ["Ich", "ver\u00b7machs", "den", "ar\u00b7men", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Reichen haben selber genug.", "tokens": ["Die", "Rei\u00b7chen", "ha\u00b7ben", "sel\u00b7ber", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}