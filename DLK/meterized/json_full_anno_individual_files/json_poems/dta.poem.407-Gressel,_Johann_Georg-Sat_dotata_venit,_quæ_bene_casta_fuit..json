{"dta.poem.407": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sat dotata venit, qu\u00e6 bene casta fuit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4dgen hat genug von Gold und andern Sch\u00e4tzen/", "tokens": ["Das", "M\u00e4d\u00b7gen", "hat", "ge\u00b7nug", "von", "Gold", "und", "an\u00b7dern", "Sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das seine reine Zucht kan auf die Waage setzen.", "tokens": ["Das", "sei\u00b7ne", "rei\u00b7ne", "Zucht", "kan", "auf", "die", "Waa\u00b7ge", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dessen keuscher Brust kein geiles Feuer brennt/", "tokens": ["In", "des\u00b7sen", "keu\u00b7scher", "Brust", "kein", "gei\u00b7les", "Feu\u00b7er", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das nicht ", "tokens": ["Das", "nicht"], "token_info": ["word", "word"], "pos": ["PDS", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Dem nicht die Geilheit lacht aus ungez\u00e4hmten Augen/", "tokens": ["Dem", "nicht", "die", "Geil\u00b7heit", "lacht", "aus", "un\u00b7ge\u00b7z\u00e4hm\u00b7ten", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das nicht die Flammen will aus jeden Zunder saugen/", "tokens": ["Das", "nicht", "die", "Flam\u00b7men", "will", "aus", "je\u00b7den", "Zun\u00b7der", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Solch M\u00e4dgen ist schon reich/ die Tugend steurt es aus/", "tokens": ["Solch", "M\u00e4d\u00b7gen", "ist", "schon", "reich", "/", "die", "Tu\u00b7gend", "steurt", "es", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und f\u00fchrt es jeder Zeit in ein ber\u00fchmtes Haus.", "tokens": ["Und", "f\u00fchrt", "es", "je\u00b7der", "Zeit", "in", "ein", "be\u00b7r\u00fchm\u00b7tes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}