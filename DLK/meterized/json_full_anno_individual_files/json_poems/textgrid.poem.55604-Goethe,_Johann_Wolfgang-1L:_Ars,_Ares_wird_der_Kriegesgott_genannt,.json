{"textgrid.poem.55604": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ars, Ares wird der Kriegesgott genannt,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ars, Ares wird der Kriegesgott genannt,", "tokens": ["Ars", ",", "A\u00b7res", "wird", "der", "Krie\u00b7ges\u00b7gott", "ge\u00b7nannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ars hei\u00dft die Kunst, und A ... ist auch bekannt.", "tokens": ["Ars", "hei\u00dft", "die", "Kunst", ",", "und", "A", "...", "ist", "auch", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "KON", "NE", "$(", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welch ein Geheimnis liegt in diesen Wundert\u00f6nen!", "tokens": ["Welch", "ein", "Ge\u00b7heim\u00b7nis", "liegt", "in", "die\u00b7sen", "Wun\u00b7der\u00b7t\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sprache bleibt ein reiner Himmelshauch,", "tokens": ["Die", "Spra\u00b7che", "bleibt", "ein", "rei\u00b7ner", "Him\u00b7mels\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Empfunden nur von stillen Erdens\u00f6hnen;", "tokens": ["Emp\u00b7fun\u00b7den", "nur", "von", "stil\u00b7len", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fest liegt der Grund, bequem ist der Gebrauch,", "tokens": ["Fest", "liegt", "der", "Grund", ",", "be\u00b7quem", "ist", "der", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und wo man wohnt, da mu\u00df man sich gew\u00f6hnen.", "tokens": ["Und", "wo", "man", "wohnt", ",", "da", "mu\u00df", "man", "sich", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "$,", "ADV", "VMFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer f\u00fchlend spricht, beschw\u00e4tzt nur sich allein;", "tokens": ["Wer", "f\u00fch\u00b7lend", "spricht", ",", "be\u00b7schw\u00e4tzt", "nur", "sich", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie anders, wenn der Glocke Bimbam bammelt,", "tokens": ["Wie", "an\u00b7ders", ",", "wenn", "der", "Glo\u00b7cke", "Bim\u00b7bam", "bam\u00b7melt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dr\u00e4ngt alles zur Versammlung sich hinein!", "tokens": ["Dr\u00e4ngt", "al\u00b7les", "zur", "Ver\u00b7samm\u00b7lung", "sich", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von K\u00f6nnen kommt die Kunst, die Sch\u00f6nheit kommt vom Schein.", "tokens": ["Von", "K\u00f6n\u00b7nen", "kommt", "die", "Kunst", ",", "die", "Sch\u00f6n\u00b7heit", "kommt", "vom", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird erst nach und nach die Sprache festgerammelt,", "tokens": ["So", "wird", "erst", "nach", "und", "nach", "die", "Spra\u00b7che", "fest\u00b7ge\u00b7ram\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und was ein Volk zusammen sich gestammelt,", "tokens": ["Und", "was", "ein", "Volk", "zu\u00b7sam\u00b7men", "sich", "ge\u00b7stam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mu\u00df ewiges Gesetz f\u00fcr Herz und Seele sein.", "tokens": ["Mu\u00df", "e\u00b7wi\u00b7ges", "Ge\u00b7setz", "f\u00fcr", "Herz", "und", "See\u00b7le", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"li