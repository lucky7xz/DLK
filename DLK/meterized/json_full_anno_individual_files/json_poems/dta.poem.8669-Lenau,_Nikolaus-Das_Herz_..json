{"dta.poem.8669": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Herz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Scheitert unsre Brust an Klippen,", "tokens": ["Schei\u00b7tert", "uns\u00b7re", "Brust", "an", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hingeschellt von Sturmeswuth,", "tokens": ["Hin\u00b7ge\u00b7schellt", "von", "Stur\u00b7mes\u00b7wuth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinkt mit aufgerissnen Lippen", "tokens": ["Trinkt", "mit", "auf\u00b7ge\u00b7riss\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Wunde Schmerzensfluth;", "tokens": ["Uns\u00b7re", "Wun\u00b7de", "Schmer\u00b7zens\u00b7fluth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6pft das Herz dann hastig bange", "tokens": ["Sch\u00f6pft", "das", "Herz", "dann", "has\u00b7tig", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Brust den Thr\u00e4nengu\u00df,", "tokens": ["Aus", "der", "Brust", "den", "Thr\u00e4\u00b7nen\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil es sonst vom Wellendrange", "tokens": ["Weil", "es", "sonst", "vom", "Wel\u00b7len\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberfluthet sterben mu\u00df;", "tokens": ["Ue\u00b7ber\u00b7flut\u00b7het", "ster\u00b7ben", "mu\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann wird auch der Sturm beschworen,", "tokens": ["Dann", "wird", "auch", "der", "Sturm", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Helle wird die Finsterni\u00df,", "tokens": ["Hel\u00b7le", "wird", "die", "Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es vert\u00fcnchen milde Horen", "tokens": ["Es", "ver\u00b7t\u00fcn\u00b7chen", "mil\u00b7de", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Brust den Wundenri\u00df.", "tokens": ["An", "der", "Brust", "den", "Wun\u00b7den\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ist das Herz ein zages,", "tokens": ["A\u00b7ber", "ist", "das", "Herz", "ein", "za\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Brust die Woge trinkt;", "tokens": ["Wenn", "die", "Brust", "die", "Wo\u00b7ge", "trinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Starrt es ob des Klippenschlages", "tokens": ["Starrt", "es", "ob", "des", "Klip\u00b7pen\u00b7schla\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6rrisch, m\u00fc\u00dfig \u2014 und versinkt.", "tokens": ["St\u00f6r\u00b7risch", ",", "m\u00fc\u00b7\u00dfig", "und", "ver\u00b7sinkt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$(", "KON", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Ist's ein wildes ungez\u00e4umtes,", "tokens": ["Ist's", "ein", "wil\u00b7des", "un\u00b7ge\u00b7z\u00e4um\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird es im Tumulte scheu,", "tokens": ["Wird", "es", "im", "Tu\u00b7mul\u00b7te", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Todestrunken gl\u00fcht und sch\u00e4umt es,", "tokens": ["To\u00b7des\u00b7trun\u00b7ken", "gl\u00fcht", "und", "sch\u00e4umt", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zertr\u00fcmmert sein Geb\u00e4u.", "tokens": ["Und", "zer\u00b7tr\u00fcm\u00b7mert", "sein", "Ge\u00b7b\u00e4u", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn dann auch der Himmel heiter", "tokens": ["Wenn", "dann", "auch", "der", "Him\u00b7mel", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit lindem Hauche weht,", "tokens": ["Und", "mit", "lin\u00b7dem", "Hau\u00b7che", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Strom sanft wiegt die Scheiter;", "tokens": ["Und", "der", "Strom", "sanft", "wiegt", "die", "Schei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "F\u00fcr die Todten ist's zu sp\u00e4t.", "tokens": ["F\u00fcr", "die", "Tod\u00b7ten", "ist's", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch ihr Schifflein, h\u00f6rt, ihr andern!", "tokens": ["Doch", "ihr", "Schif\u00b7flein", ",", "h\u00f6rt", ",", "ihr", "an\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seyd ihr auch dem Sturm entwischt,", "tokens": ["Seyd", "ihr", "auch", "dem", "Sturm", "ent\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhig m\u00f6gt ihr weiter wandern,", "tokens": ["Ru\u00b7hig", "m\u00f6gt", "ihr", "wei\u00b7ter", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber nicht geh\u00f6hnt, gezischt:", "tokens": ["A\u00b7ber", "nicht", "ge\u00b7h\u00f6hnt", ",", "ge\u00b7zischt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201ewie der Nachen ward zertr\u00fcmmert!", "tokens": ["\u201e", "wie", "der", "Na\u00b7chen", "ward", "zer\u00b7tr\u00fcm\u00b7mert", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201ewie das Herz im Strom ersoff!", "tokens": ["\u201e", "wie", "das", "Herz", "im", "Strom", "er\u00b7soff", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ewarst wohl auch zu leicht gezimmert!", "tokens": ["\u201e", "warst", "wohl", "auch", "zu", "leicht", "ge\u00b7zim\u00b7mert", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ewarst wohl auch aus schlechtem Stoff!\u201c", "tokens": ["\u201e", "warst", "wohl", "auch", "aus", "schlech\u00b7tem", "Stoff", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00fctet euch, ihr andern, h\u00fctet!", "tokens": ["H\u00fc\u00b7tet", "euch", ",", "ihr", "an\u00b7dern", ",", "h\u00fc\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denkt an eurer Fahrten Rest;", "tokens": ["Denkt", "an", "eu\u00b7rer", "Fahr\u00b7ten", "Rest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Zukunft Nacht bebr\u00fctet", "tokens": ["Denn", "der", "Zu\u00b7kunft", "Nacht", "be\u00b7br\u00fc\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen Sturm im dunkeln Nest.", "tokens": ["Man\u00b7chen", "Sturm", "im", "dun\u00b7keln", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}