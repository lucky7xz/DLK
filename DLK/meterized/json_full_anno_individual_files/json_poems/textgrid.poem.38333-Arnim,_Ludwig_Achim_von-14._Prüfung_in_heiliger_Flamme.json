text": "Wenn unser Bette bl\u00fcht;", "tokens": ["Wenn", "un\u00b7ser", "Bet\u00b7te", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Gott hat dem bereitet,", "tokens": ["Was", "Gott", "hat", "dem", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der sich von ihm nicht scheidet,", "tokens": ["Der", "sich", "von", "ihm", "nicht", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+----+-", "measure": "dactylic.init"}, "line.7": {"text": "Und Liebe in sich zieht,", "tokens": ["Und", "Lie\u00b7be", "in", "sich", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die man nicht sieht.", "tokens": ["Die", "man", "nicht", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "++-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Man kann auch nicht", "tokens": ["Man", "kann", "auch", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von dem Geheimni\u00df schreiben;", "tokens": ["Von", "dem", "Ge\u00b7heim\u00b7ni\u00df", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es mu\u00df verschwiegen bleiben,", "tokens": ["Es", "mu\u00df", "ver\u00b7schwie\u00b7gen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Lieb' in uns verricht.", "tokens": ["Was", "Lieb'", "in", "uns", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist recht gro\u00df zu nennen,", "tokens": ["Es", "ist", "recht", "gro\u00df", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn Jesus will erkennen", "tokens": ["Wenn", "Je\u00b7sus", "will", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Braut in seinem Licht,", "tokens": ["Die", "Braut", "in", "sei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Man kennt es nicht.", "tokens": ["Man", "kennt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}