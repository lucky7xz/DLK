"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Brennt immerhin.", "tokens": ["Brennt", "im\u00b7mer\u00b7hin", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df es schon,", "tokens": ["Ich", "wei\u00df", "es", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wo ich den Sch\u00f6nsten funden,", "tokens": ["Wo", "ich", "den", "Sch\u00f6ns\u00b7ten", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der meinem Geist verbunden!", "tokens": ["Der", "mei\u00b7nem", "Geist", "ver\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist der Liebe Lohn,", "tokens": ["Er", "ist", "der", "Lie\u00b7be", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der sich mir selbst mu\u00df geben,", "tokens": ["Der", "sich", "mir", "selbst", "mu\u00df", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Soll anders ich noch leben.", "tokens": ["Soll", "an\u00b7ders", "ich", "noch", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo seine Sch\u00f6nheit wohn,", "tokens": ["Wo", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "wohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das wei\u00df ich schon.", "tokens": ["Das", "wei\u00df", "ich", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich hab ihn nun,", "tokens": ["Ich", "hab", "ihn", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und such ihn doch noch immer", "tokens": ["Und", "such", "ihn", "doch", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In meines Herzens Zimmer,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo er so gern will ruhn;", "tokens": ["Wo", "er", "so", "gern", "will", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das sehnliche Verlangen", "tokens": ["Das", "sehn\u00b7li\u00b7che", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Lieb' hat mich gefangen,", "tokens": ["Der", "Lieb'", "hat", "mich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mir st\u00e4tig wohl zu thun.", "tokens": ["Mir", "st\u00e4\u00b7tig", "wohl", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ich hab ihn nun.", "tokens": ["Ich", "hab", "ihn", "nun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Kein Auge sieht,", "tokens": ["Kein", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kein Herz hat \u00fcberkommen,", "tokens": ["Kein", "Herz", "hat", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Ohr hat je vernommen,", "tokens": ["Kein", "Ohr", "hat", "je", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn unser Bette bl\u00fcht;", "tokens": ["Wenn", "un\u00b7ser", "Bet\u00b7te", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Gott hat dem bereitet,", "tokens": ["Was", "Gott", "hat", "dem", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der sich von ihm nicht scheidet,", "tokens": ["Der", "sich", "von", "ihm", "nicht", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+----+-", "measure": "dactylic.init"}, "line.7": {"text": "Und Liebe in sich zieht,", "tokens": ["Und", "Lie\u00b7be", "in", "sich", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die man nicht sieht.", "tokens": ["Die", "man", "nicht", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "++-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Man kann auch nicht", "tokens": ["Man", "kann", "auch", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von dem Geheimni\u00df schreiben;", "tokens": ["Von", "dem", "Ge\u00b7heim\u00b7ni\u00df", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es mu\u00df verschwiegen bleiben,", "tokens": ["Es", "mu\u00df", "ver\u00b7schwie\u00b7gen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Lieb' in uns verricht.", "tokens": ["Was", "Lieb'", "in", "uns", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es ist recht gro\u00df zu nennen,", "tokens": ["Es", "ist", "recht", "gro\u00df", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "w