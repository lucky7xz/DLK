{"textgrid.poem.66044": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich tr\u00e4umte mich auf einem bangen Weg,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umte mich auf einem bangen Weg,", "tokens": ["Ich", "tr\u00e4um\u00b7te", "mich", "auf", "ei\u00b7nem", "ban\u00b7gen", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einem hohen, schwindelschmalen Steg,", "tokens": ["Auf", "ei\u00b7nem", "ho\u00b7hen", ",", "schwin\u00b7del\u00b7schma\u00b7len", "Steg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der f\u00fchrte mich bis an das Himmelstor.", "tokens": ["Der", "f\u00fchr\u00b7te", "mich", "bis", "an", "das", "Him\u00b7mels\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da stand ich lange, ohne Mut, davor.", "tokens": ["Da", "stand", "ich", "lan\u00b7ge", ",", "oh\u00b7ne", "Mut", ",", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUI", "NN", "$,", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und zitternd griff ich nach dem rostigen Ring,", "tokens": ["Und", "zit\u00b7ternd", "griff", "ich", "nach", "dem", "ros\u00b7ti\u00b7gen", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das Himmelsgl\u00f6cklein an zu l\u00e4uten fing,", "tokens": ["Das", "Him\u00b7mels\u00b7gl\u00f6c\u00b7klein", "an", "zu", "l\u00e4u\u00b7ten", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Herz erschrak vor seinem hellen Klang,", "tokens": ["Mein", "Herz", "er\u00b7schrak", "vor", "sei\u00b7nem", "hel\u00b7len", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein armer S\u00fcnder auf dem letzten Gang.", "tokens": ["Ein", "ar\u00b7mer", "S\u00fcn\u00b7der", "auf", "dem", "letz\u00b7ten", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann rasselte ein gro\u00dfes Schl\u00fcsselbund,", "tokens": ["Dann", "ras\u00b7sel\u00b7te", "ein", "gro\u00b7\u00dfes", "Schl\u00fcs\u00b7sel\u00b7bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Knarren, bis der Himmel offen stund,", "tokens": ["Ein", "Knar\u00b7ren", ",", "bis", "der", "Him\u00b7mel", "of\u00b7fen", "stund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch hascht ich nur von seiner Herrlichkeit", "tokens": ["Doch", "hascht", "ich", "nur", "von", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit scheuem Blinzeln einen Streifen breit,", "tokens": ["Mit", "scheu\u00b7em", "Blin\u00b7zeln", "ei\u00b7nen", "Strei\u00b7fen", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Wiesengr\u00fcn und einen Engelsfu\u00df.", "tokens": ["Ein", "Wie\u00b7sen\u00b7gr\u00fcn", "und", "ei\u00b7nen", "En\u00b7gels\u00b7fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sankt Peter barg mir jeden weitern Gru\u00df", "tokens": ["Sankt", "Pe\u00b7ter", "barg", "mir", "je\u00b7den", "wei\u00b7tern", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit breitem R\u00fccken und erschreckte mich", "tokens": ["Mit", "brei\u00b7tem", "R\u00fc\u00b7cken", "und", "er\u00b7schreck\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit barscher Frage: \u00bbFreund, wer schickte dich?\u00ab", "tokens": ["Mit", "bar\u00b7scher", "Fra\u00b7ge", ":", "\u00bb", "Freund", ",", "wer", "schick\u00b7te", "dich", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$(", "NN", "$,", "PWS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mich schickte keiner. \u00bbUnd was suchst du hier?\u00ab", "tokens": ["Mich", "schick\u00b7te", "kei\u00b7ner", ".", "\u00bb", "Und", "was", "suchst", "du", "hier", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "$(", "KON", "PWS", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach Erdennot ein ruhiges Quartier,", "tokens": ["Nach", "Er\u00b7den\u00b7not", "ein", "ru\u00b7hi\u00b7ges", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ein Fl\u00fcgelpaar und himmlisches Gewand,", "tokens": ["Ein", "Fl\u00fc\u00b7gel\u00b7paar", "und", "himm\u00b7li\u00b7sches", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Tr\u00f6pfchen Tau aus Gottes hohler Hand.", "tokens": ["Ein", "Tr\u00f6pf\u00b7chen", "Tau", "aus", "Got\u00b7tes", "hoh\u00b7ler", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbhast du zu solchen Dingen auch ein Recht,", "tokens": ["\u00bb", "hast", "du", "zu", "sol\u00b7chen", "Din\u00b7gen", "auch", "ein", "Recht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warst du auf Erden ein getreuer Knecht?\u00ab", "tokens": ["Warst", "du", "auf", "Er\u00b7den", "ein", "ge\u00b7treu\u00b7er", "Knecht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich war Poet. \u00bbUnd kommst zu Fu\u00df hier an?", "tokens": ["Ich", "war", "Po\u00b7et", ".", "\u00bb", "Und", "kommst", "zu", "Fu\u00df", "hier", "an", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "$(", "KON", "VVFIN", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo hast du deine Fl\u00fcgel hingetan?\u00ab", "tokens": ["Wo", "hast", "du", "dei\u00b7ne", "Fl\u00fc\u00b7gel", "hin\u00b7ge\u00b7tan", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich sch\u00e4mte mich, weil sie so sehr beschmutzt,", "tokens": ["Ich", "sch\u00e4m\u00b7te", "mich", ",", "weil", "sie", "so", "sehr", "be\u00b7schmutzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ihre sch\u00f6nsten Federn arg gestutzt,", "tokens": ["Und", "ih\u00b7re", "sch\u00f6ns\u00b7ten", "Fe\u00b7dern", "arg", "ge\u00b7stutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil durch das Fliegen nach dem Flitterkranz", "tokens": ["Weil", "durch", "das", "Flie\u00b7gen", "nach", "dem", "Flit\u00b7ter\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Menschenruhmes dunkel ward ihr Glanz.", "tokens": ["Des", "Men\u00b7schen\u00b7ruh\u00b7mes", "dun\u00b7kel", "ward", "ihr", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbund deinen Kranz?\u00ab Ich hab ihn abgelegt,", "tokens": ["\u00bb", "und", "dei\u00b7nen", "Kranz", "?", "\u00ab", "Ich", "hab", "ihn", "ab\u00b7ge\u00b7legt", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "$.", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df man mit andern ihn zum Kehricht fegt,", "tokens": ["Da\u00df", "man", "mit", "an\u00b7dern", "ihn", "zum", "Keh\u00b7richt", "fegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und komm nun nackt und ohne Glorienschein.", "tokens": ["Und", "komm", "nun", "nackt", "und", "oh\u00b7ne", "Glo\u00b7ri\u00b7en\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "APPR