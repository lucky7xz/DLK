{"textgrid.poem.35942": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dem Thal ist's kalt und tr\u00fcbe,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Thal ist's kalt und tr\u00fcbe,", "tokens": ["In", "dem", "Thal", "ist's", "kalt", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Pfad ist steil und schwer,", "tokens": ["Und", "mein", "Pfad", "ist", "steil", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Freuden, keine Liebe", "tokens": ["Kei\u00b7ne", "Freu\u00b7den", ",", "kei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will mein Herz hienieden mehr.", "tokens": ["Will", "mein", "Herz", "hien\u00b7ie\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort nur bl\u00fchen reine Kronen,", "tokens": ["Dort", "nur", "bl\u00fc\u00b7hen", "rei\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier ist eit'ler Blumen Schein;", "tokens": ["Hier", "ist", "eit'\u00b7ler", "Blu\u00b7men", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort nur kehrt ein friedlich Wohnen", "tokens": ["Dort", "nur", "kehrt", "ein", "fried\u00b7lich", "Woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die ew'gen H\u00fctten ein.", "tokens": ["In", "die", "ew'\u00b7gen", "H\u00fct\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meinen Weg mit buntem Prangen", "tokens": ["Mei\u00b7nen", "Weg", "mit", "bun\u00b7tem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fcckten wohl der Blumen viel,", "tokens": ["Schm\u00fcck\u00b7ten", "wohl", "der", "Blu\u00b7men", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ernst bin ich hindurchgegangen,", "tokens": ["Ernst", "bin", "ich", "hin\u00b7durch\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah nach einem andern Ziel.", "tokens": ["Sah", "nach", "ei\u00b7nem", "an\u00b7dern", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jene m\u00f6get ihr entz\u00fccken,", "tokens": ["Je\u00b7ne", "m\u00f6\u00b7get", "ihr", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen bunt, mit Duft und Schein,", "tokens": ["Blu\u00b7men", "bunt", ",", "mit", "Duft", "und", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich will meinen Kranz mir pfl\u00fccken", "tokens": ["Ich", "will", "mei\u00b7nen", "Kranz", "mir", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oben auf dem Berg' allein,", "tokens": ["O\u00b7ben", "auf", "dem", "Ber\u00b7g'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wo mein Lieb zu mir herschauet,", "tokens": ["Wo", "mein", "Lieb", "zu", "mir", "her\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ganz von Rosen \u00fcbers\u00e4't,", "tokens": ["Ganz", "von", "Ro\u00b7sen", "\u00fc\u00b7ber\u00b7s\u00e4't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es auf mich niederthauet", "tokens": ["Da\u00df", "es", "auf", "mich", "nie\u00b7dert\u00b7hau\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auf alle Welt hinweht.", "tokens": ["Und", "auf", "al\u00b7le", "Welt", "hin\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O Du Heil der Rosenf\u00fclle,", "tokens": ["O", "Du", "Heil", "der", "Ro\u00b7sen\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die Erde drob erbebt,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "drob", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sich eine dichte H\u00fclle", "tokens": ["Und", "sich", "ei\u00b7ne", "dich\u00b7te", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die Himmelslichter webt.", "tokens": ["Um", "die", "Him\u00b7mels\u00b7lich\u00b7ter", "webt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "H\u00f6r', wie in den Finsternissen", "tokens": ["H\u00f6r'", ",", "wie", "in", "den", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nt ein Lied so hell und lieb:", "tokens": ["T\u00f6nt", "ein", "Lied", "so", "hell", "und", "lieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.8": {"line.1": {"text": "Welche Gr\u00fc\u00dfe, s\u00fc\u00df wie diese,", "tokens": ["Wel\u00b7che", "Gr\u00fc\u00b7\u00dfe", ",", "s\u00fc\u00df", "wie", "die\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "ADJD", "KOKOM", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Luden je zur Hochzeit ein:", "tokens": ["Lu\u00b7den", "je", "zur", "Hoch\u00b7zeit", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.9": {"line.1": {"text": "Trosteswort an mir geschehe:", "tokens": ["Tros\u00b7tes\u00b7wort", "an", "mir", "ge\u00b7sche\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Ach nun hab' ich Freude schon.", "tokens": ["Ach", "nun", "hab'", "ich", "Freu\u00b7de", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mutter lehrt den Schmerz mich fassen,", "tokens": ["Mut\u00b7ter", "lehrt", "den", "Schmerz", "mich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da: \u00bb", "tokens": ["Da", ":", "\u00bb"], "token_info": ["word", "punct", "punct"], "pos": ["ADV", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Und ein Schwert ihr Herz durchsticht.", "tokens": ["Und", "ein", "Schwert", "ihr", "Herz", "durch\u00b7sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, wie liebet er; er blicket,", "tokens": ["Ach", ",", "wie", "lie\u00b7bet", "er", ";", "er", "bli\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht: \u00bb", "tokens": ["Spricht", ":", "\u00bb"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$.", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Seelen! str\u00f6mt zu ihm, erquicket", "tokens": ["See\u00b7len", "!", "str\u00f6mt", "zu", "ihm", ",", "er\u00b7quic\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "PPER", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euren Quell und Retter hier.", "tokens": ["Eu\u00b7ren", "Quell", "und", "Ret\u00b7ter", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "O du wonnenvoll Erschrecken,", "tokens": ["O", "du", "won\u00b7nen\u00b7voll", "Er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O du Lichtwort tief in Nacht,", "tokens": ["O", "du", "Licht\u00b7wort", "tief", "in", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die Todten kann erwecken,", "tokens": ["Das", "die", "Tod\u00b7ten", "kann", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebenswort: \u00bb", "tokens": ["Le\u00b7bens\u00b7wort", ":", "\u00bb"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+--", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Friedenswort am Lebensende,", "tokens": ["Frie\u00b7dens\u00b7wort", "am", "Le\u00b7ben\u00b7sen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da des Tempels Vorhang rei\u00dft,", "tokens": ["Da", "des", "Tem\u00b7pels", "Vor\u00b7hang", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.14": {"line.1": {"text": "Trost und Macht der sieben Worte!", "tokens": ["Trost", "und", "Macht", "der", "sie\u00b7ben", "Wor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da mein Gott euch sterbend spricht,", "tokens": ["Da", "mein", "Gott", "euch", "ster\u00b7bend", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oeffnet sich des Himmels Pforte,", "tokens": ["Oeff\u00b7net", "sich", "des", "Him\u00b7mels", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Abgrunds Fessel bricht.", "tokens": ["Und", "des", "Ab\u00b7grunds", "Fes\u00b7sel", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Hochauf athmet alles Sehnen,", "tokens": ["Hoch\u00b7auf", "ath\u00b7met", "al\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf zum Himmel schwebt ein Zug", "tokens": ["Auf", "zum", "Him\u00b7mel", "schwebt", "ein", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Erl\u00f6s'ten, licht gleich Schw\u00e4nen;", "tokens": ["Der", "Er\u00b7l\u00f6s'\u00b7ten", ",", "licht", "gleich", "Schw\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr, o mach' mich rein zum Flug!", "tokens": ["Herr", ",", "o", "mach'", "mich", "rein", "zum", "Flug", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "In dem Thal ist's kalt und tr\u00fcbe,", "tokens": ["In", "dem", "Thal", "ist's", "kalt", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Pfad ist steil und schwer,", "tokens": ["Und", "mein", "Pfad", "ist", "steil", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Freuden, keine Liebe", "tokens": ["Kei\u00b7ne", "Freu\u00b7den", ",", "kei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will mein Herz hienieden mehr.", "tokens": ["Will", "mein", "Herz", "hien\u00b7ie\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Dort nur bl\u00fchen reine Kronen,", "tokens": ["Dort", "nur", "bl\u00fc\u00b7hen", "rei\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier ist eit'ler Blumen Schein;", "tokens": ["Hier", "ist", "eit'\u00b7ler", "Blu\u00