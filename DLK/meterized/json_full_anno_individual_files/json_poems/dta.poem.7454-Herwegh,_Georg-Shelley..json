{"dta.poem.7454": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Shelley.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um seinen Gott sich doppelt schmerzlich m\u00fchend,", "tokens": ["Um", "sei\u00b7nen", "Gott", "sich", "dop\u00b7pelt", "schmerz\u00b7lich", "m\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "ADJD", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War er ihm, selbsterrungen, doppelt theuer,", "tokens": ["War", "er", "ihm", ",", "selbs\u00b7ter\u00b7run\u00b7gen", ",", "dop\u00b7pelt", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "ADJA", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Ewigen war keine Seele treuer,", "tokens": ["Dem", "E\u00b7wi\u00b7gen", "war", "kei\u00b7ne", "See\u00b7le", "treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PIAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Glaube je so ungeschw\u00e4cht und bl\u00fchend.", "tokens": ["Kein", "Glau\u00b7be", "je", "so", "un\u00b7ge\u00b7schw\u00e4cht", "und", "bl\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADV", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit allen Pulsen f\u00fcr die Menschheit gl\u00fchend,", "tokens": ["Mit", "al\u00b7len", "Pul\u00b7sen", "f\u00fcr", "die", "Menschheit", "gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sa\u00df immer mit der Hoffnung er am Steuer,", "tokens": ["Sa\u00df", "im\u00b7mer", "mit", "der", "Hoff\u00b7nung", "er", "am", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn er auch z\u00fcrnte, seines Zornes Feuer", "tokens": ["Wenn", "er", "auch", "z\u00fcrn\u00b7te", ",", "sei\u00b7nes", "Zor\u00b7nes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur gegen Sklaven und Tyrannen spr\u00fchend.", "tokens": ["Nur", "ge\u00b7gen", "Skla\u00b7ven", "und", "Ty\u00b7ran\u00b7nen", "spr\u00fc\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Elfengeist in einem Menschenleibe,", "tokens": ["Ein", "El\u00b7fen\u00b7geist", "in", "ei\u00b7nem", "Men\u00b7schen\u00b7lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der Natur Altar ein reiner Funken,", "tokens": ["Von", "der", "Na\u00b7tur", "Al\u00b7tar", "ein", "rei\u00b7ner", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und drum f\u00fcr Englands P\u00f6belsinn die Scheibe;", "tokens": ["Und", "drum", "f\u00fcr", "En\u00b7glands", "P\u00f6\u00b7bel\u00b7sinn", "die", "Schei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "NE", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Herz, vom s\u00fc\u00dfen Duft des Himmels trunken,", "tokens": ["Ein", "Herz", ",", "vom", "s\u00fc\u00b7\u00dfen", "Duft", "des", "Him\u00b7mels", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verflucht vom Vater und geliebt vom Weibe,", "tokens": ["Ver\u00b7flucht", "vom", "Va\u00b7ter", "und", "ge\u00b7liebt", "vom", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "KON", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zuletzt ein Stern im wilden Meer versunken.", "tokens": ["Zu\u00b7letzt", "ein", "Stern", "im", "wil\u00b7den", "Meer", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}