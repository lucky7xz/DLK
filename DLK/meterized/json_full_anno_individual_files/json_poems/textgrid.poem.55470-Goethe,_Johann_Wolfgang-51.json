 ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Derb und kr\u00e4ftig, hold und schicklich.", "tokens": ["Derb", "und", "kr\u00e4f\u00b7tig", ",", "hold", "und", "schick\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und man pries den Vater gl\u00fccklich.", "tokens": ["Und", "man", "pries", "den", "Va\u00b7ter", "gl\u00fcck\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieht der Dichter nah und ferne", "tokens": ["Sieht", "der", "Dich\u00b7ter", "nah", "und", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00f6hn und T\u00f6chter, lichte Sterne,", "tokens": ["S\u00f6hn", "und", "T\u00f6ch\u00b7ter", ",", "lich\u00b7te", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht sie alle wohlgeraten,", "tokens": ["Sieht", "sie", "al\u00b7le", "wohl\u00b7ge\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00fcchtig, von gepr\u00fcften Taten,", "tokens": ["T\u00fcch\u00b7tig", ",", "von", "ge\u00b7pr\u00fcf\u00b7ten", "Ta\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APP