{"textgrid.poem.42820": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Mut der reifen Jugend", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mut zeigt sich immer erst vor \u00dcbermacht.", "tokens": ["Mut", "zeigt", "sich", "im\u00b7mer", "erst", "vor", "\u00dc\u00b7ber\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mut mu\u00df mit Kenntnis der Gefahr gepaart sein.", "tokens": ["Mut", "mu\u00df", "mit", "Kennt\u00b7nis", "der", "Ge\u00b7fahr", "ge\u00b7paart", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mut will wie Edelstes diskret verwahrt sein,", "tokens": ["Mut", "will", "wie", "E\u00b7dels\u00b7tes", "dis\u00b7kret", "ver\u00b7wahrt", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KOKOM", "NN", "VVFIN", "VVPP", "VAINF", "$,"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und wer ihn fa\u00dft, der fasse mit Bedacht.", "tokens": ["Und", "wer", "ihn", "fa\u00dft", ",", "der", "fas\u00b7se", "mit", "Be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hab' Mut! Jedoch nicht, um ihn zu beweisen.", "tokens": ["Hab'", "Mut", "!", "Je\u00b7doch", "nicht", ",", "um", "ihn", "zu", "be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADV", "PTKNEG", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schick deinen Mut niemals auf Reisen.", "tokens": ["Schick", "dei\u00b7nen", "Mut", "nie\u00b7mals", "auf", "Rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Man kann mit K\u00fchnheit, doch mit Mut nie scherzen,", "tokens": ["Man", "kann", "mit", "K\u00fchn\u00b7heit", ",", "doch", "mit", "Mut", "nie", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "$,", "ADV", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn der, der Mut zeigt, hat auch Furcht im Herzen.", "tokens": ["Denn", "der", ",", "der", "Mut", "zeigt", ",", "hat", "auch", "Furcht", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Soll reife Jugend weise, \u00fcberlegen,", "tokens": ["Soll", "rei\u00b7fe", "Ju\u00b7gend", "wei\u00b7se", ",", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ma\u00dfvoll, gelehrt und unpolitisch sein??", "tokens": ["Ma\u00df\u00b7voll", ",", "ge\u00b7lehrt", "und", "un\u00b7po\u00b7li\u00b7tisch", "sein", "??"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darf sie verdreht und zukunftsblind verwegen", "tokens": ["Darf", "sie", "ver\u00b7dreht", "und", "zu\u00b7kunfts\u00b7blind", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergn\u00fcgen saufen?? \u2013 Ja! und so auch: Nein!", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "sau\u00b7fen", "??", "\u2013", "Ja", "!", "und", "so", "auch", ":", "Nein", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$.", "$(", "PTKANT", "$.", "KON", "ADV", "ADV", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df dar\u00fcber keine Regel,", "tokens": ["Ich", "wei\u00df", "da\u00b7r\u00fc\u00b7ber", "kei\u00b7ne", "Re\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df nur, wie stets das Schicksal das entschied.", "tokens": ["Wei\u00df", "nur", ",", "wie", "stets", "das", "Schick\u00b7sal", "das", "ent\u00b7schied", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch zwischen freiem Bursch und bl\u00f6dem Flegel", "tokens": ["Doch", "zwi\u00b7schen", "frei\u00b7em", "Bursch", "und", "bl\u00f6\u00b7dem", 