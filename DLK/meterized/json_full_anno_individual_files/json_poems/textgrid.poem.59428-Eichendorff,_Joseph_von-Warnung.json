{"textgrid.poem.59428": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus ist dein Urlaub und die Laut zerschlagen,", "tokens": ["Aus", "ist", "dein", "Ur\u00b7laub", "und", "die", "Laut", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPOSAT", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachts aus der stillen Stadt nun mu\u00dft du gehen,", "tokens": ["Nachts", "aus", "der", "stil\u00b7len", "Stadt", "nun", "mu\u00dft", "du", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wetterfahnen nur im Wind sich drehen,", "tokens": ["Die", "Wet\u00b7ter\u00b7fah\u00b7nen", "nur", "im", "Wind", "sich", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Tritt verhallt, mag niemand nach dir fragen.", "tokens": ["Dein", "Tritt", "ver\u00b7hallt", ",", "mag", "nie\u00b7mand", "nach", "dir", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch drau\u00dfen waldw\u00e4rts, wo du herstammst, ragen", "tokens": ["Doch", "drau\u00b7\u00dfen", "wald\u00b7w\u00e4rts", ",", "wo", "du", "her\u00b7stammst", ",", "ra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Zinnen noch der goldnen Burg, es gehen", "tokens": ["Die", "Zin\u00b7nen", "noch", "der", "gold\u00b7nen", "Burg", ",", "es", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wachen schildernd auf dem Wall, das Wehen", "tokens": ["Die", "Wa\u00b7chen", "schil\u00b7dernd", "auf", "dem", "Wall", ",", "das", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Nacht bringt ihren Ruf ins Land getragen.", "tokens": ["Der", "Nacht", "bringt", "ih\u00b7ren", "Ruf", "ins", "Land", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Engel dort mit seinem Flammendegen", "tokens": ["Der", "En\u00b7gel", "dort", "mit", "sei\u00b7nem", "Flam\u00b7men\u00b7de\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steht blankger\u00fcstet noch, das Tor zu h\u00fcten,", "tokens": ["Steht", "blank\u00b7ge\u00b7r\u00fcs\u00b7tet", "noch", ",", "das", "Tor", "zu", "h\u00fc\u00b7t