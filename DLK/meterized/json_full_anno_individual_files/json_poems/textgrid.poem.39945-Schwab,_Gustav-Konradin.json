{"textgrid.poem.39945": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Konradin", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum ist der Fr\u00fchling im Erwachen,", "tokens": ["Kaum", "ist", "der", "Fr\u00fch\u00b7ling", "im", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es bl\u00fcht der See, es bl\u00fcht der Baum,", "tokens": ["Es", "bl\u00fcht", "der", "See", ",", "es", "bl\u00fcht", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bl\u00fcht ein J\u00fcngling dort im Nachen,", "tokens": ["Es", "bl\u00fcht", "ein", "J\u00fcng\u00b7ling", "dort", "im", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er wiegt sich in der Wellen Schaum.", "tokens": ["Er", "wiegt", "sich", "in", "der", "Wel\u00b7len", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie eine Rosenknospe h\u00fcllet", "tokens": ["Wie", "ei\u00b7ne", "Ro\u00b7sen\u00b7knos\u00b7pe", "h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein junges Purpurkleid ihn ein,", "tokens": ["Ein", "jun\u00b7ges", "Pur\u00b7pur\u00b7kleid", "ihn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unter einer Krone quillet", "tokens": ["Und", "un\u00b7ter", "ei\u00b7ner", "Kro\u00b7ne", "quil\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Haar von g\u00fcldenerem Schein.", "tokens": ["Sein", "Haar", "von", "g\u00fcl\u00b7de\u00b7ne\u00b7rem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es irret auf den blauen Wellen", "tokens": ["Es", "ir\u00b7ret", "auf", "den", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein sinnend Auge, wellenblau,", "tokens": ["Sein", "sin\u00b7nend", "Au\u00b7ge", ",", "wel\u00b7len\u00b7blau", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PWAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Leyer, die er schl\u00e4gt, entschwellen", "tokens": ["Der", "Le\u00b7yer", ",", "die", "er", "schl\u00e4gt", ",", "ent\u00b7schwel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ges\u00e4nge von der sch\u00f6nsten Frau.", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "von", "der", "sch\u00f6ns\u00b7ten", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des ersten Donners Stimmen hallen,", "tokens": ["Des", "ers\u00b7ten", "Don\u00b7ners", "Stim\u00b7men", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im S\u00fcden blitzt es blutig rot;", "tokens": ["Im", "S\u00fc\u00b7den", "blitzt", "es", "blu\u00b7tig", "rot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4\u00dft sein Lied nur lauter schallen,", "tokens": ["Er", "l\u00e4\u00dft", "sein", "Lied", "nur", "lau\u00b7ter", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn k\u00fcmmert nichts, als Liebesnot.", "tokens": ["Ihn", "k\u00fcm\u00b7mert", "nichts", ",", "als", "Lie\u00b7bes\u00b7not", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn er Minne sich errungen,", "tokens": ["Und", "wenn", "er", "Min\u00b7ne", "sich", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So holt er sich dazu den Ruhm,", "tokens": ["So", "holt", "er", "sich", "da\u00b7zu", "den", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und herrscht, vom Lorberkranz umschlungen,", "tokens": ["Und", "herrscht", ",", "vom", "Lor\u00b7ber\u00b7kranz", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner V\u00e4ter Eigentum.", "tokens": ["In", "sei\u00b7ner", "V\u00e4\u00b7ter", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kind! wie du stehst im schwanken Kahne,", "tokens": ["Kind", "!", "wie", "du", "stehst", "im", "schwan\u00b7ken", "Kah\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So rufet dich ein schwanker Thron,", "tokens": ["So", "ru\u00b7fet", "dich", "ein", "schwan\u00b7ker", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vertrau' dem Schatten nicht, dem Ahne,", "tokens": ["Ver\u00b7trau'", "dem", "Schat\u00b7ten", "nicht", ",", "dem", "Ah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00dfner, armer K\u00f6nigssohn!", "tokens": ["Ver\u00b7la\u00df\u00b7ner", ",", "ar\u00b7mer", "K\u00f6\u00b7nigs\u00b7sohn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bist so stolz und unerschrocken,", "tokens": ["Du", "bist", "so", "stolz", "und", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sinkest, eh' du es geglaubt,", "tokens": ["Du", "sin\u00b7kest", ",", "eh'", "du", "es", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sitzt die Kron' auf deinen Locken,", "tokens": ["Es", "sitzt", "die", "Kron'", "auf", "dei\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als tr\u00e4umte nur davon dein Haupt! \u2013", "tokens": ["Als", "tr\u00e4um\u00b7te", "nur", "da\u00b7von", "dein", "Haupt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "PAV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er h\u00f6ret keine Warnungsstimme,", "tokens": ["Er", "h\u00f6\u00b7ret", "kei\u00b7ne", "War\u00b7nungs\u00b7stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schwimmt singend auf dem Abgrund hin,", "tokens": ["Schwimmt", "sin\u00b7gend", "auf", "dem", "Ab\u00b7grund", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wei\u00df er von des Sturmes Grimme?", "tokens": ["Was", "wei\u00df", "er", "von", "des", "Stur\u00b7mes", "Grim\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Lieb' und Leben steht sein Sinn.", "tokens": ["Nach", "Lieb'", "und", "Le\u00b7ben", "steht", "sein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So gieb ihm Leben, gieb ihm Liebe,", "tokens": ["So", "gieb", "ihm", "Le\u00b7ben", ",", "gieb", "ihm", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wonnevolles Schwabenland,", "tokens": ["Du", "won\u00b7ne\u00b7vol\u00b7les", "Schwa\u00b7ben\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdopple deine Bl\u00fctentriebe,", "tokens": ["Ver\u00b7dopp\u00b7le", "dei\u00b7ne", "Bl\u00fc\u00b7ten\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kn\u00fcpf' ihm der Minne sel'ges Band!", "tokens": ["Kn\u00fcpf'", "ihm", "der", "Min\u00b7ne", "sel'\u00b7ges", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Es hat zu leben kurz der Knabe \u2013", "tokens": ["Es", "hat", "zu", "le\u00b7ben", "kurz", "der", "Kna\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hauch' ihm entgegen Lebensluft,", "tokens": ["Hauch'", "ihm", "ent\u00b7ge\u00b7gen", "Le\u00b7bens\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Durchw\u00fcrze jede kleine Gabe", "tokens": ["Durch\u00b7w\u00fcr\u00b7ze", "je\u00b7de", "klei\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ew'ger Jugend Bl\u00fctenduft!", "tokens": ["Mit", "ew'\u00b7ger", "Ju\u00b7gend", "Bl\u00fc\u00b7ten\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Mach' ihm den Augenblick zu Jahren,", "tokens": ["Mach'", "ihm", "den", "Au\u00b7gen\u00b7blick", "zu", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den er an diesen Ufern lebt,", "tokens": ["Den", "er", "an", "die\u00b7sen", "U\u00b7fern", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er mit ungebleichten Haaren", "tokens": ["Da\u00df", "er", "mit", "un\u00b7ge\u00b7bleich\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Freude satt gen Himmel schwebt!", "tokens": ["An", "Freu\u00b7de", "satt", "gen", "Him\u00b7mel", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Was ist's? er l\u00e4\u00dft die Leyer fallen,", "tokens": ["Was", "ist's", "?", "er", "l\u00e4\u00dft", "die", "Le\u00b7yer", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er springt an's Ufer, greift zum Schwert,", "tokens": ["Er", "springt", "an's", "U\u00b7fer", ",", "greift", "zum", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O seht ihn \u00fcber Alpen wallen", "tokens": ["O", "seht", "ihn", "\u00fc\u00b7ber", "Al\u00b7pen", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit treuen M\u00e4nnern, hoch zu Pferd!", "tokens": ["Mit", "treu\u00b7en", "M\u00e4n\u00b7nern", ",", "hoch", "zu", "Pferd", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der Lust, der Liebe Lieder schweigen,", "tokens": ["Der", "Lust", ",", "der", "Lie\u00b7be", "Lie\u00b7der", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er gl\u00fcht von edlerem Gel\u00fcst;", "tokens": ["Er", "gl\u00fcht", "von", "ed\u00b7le\u00b7rem", "Ge\u00b7l\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er will der V\u00e4ter Thron besteigen \u2013", "tokens": ["Er", "will", "der", "V\u00e4\u00b7ter", "Thron", "be\u00b7stei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandelt auf das Blutger\u00fcst.", "tokens": ["Und", "wan\u00b7delt", "auf", "das", "Blut\u00b7ge\u00b7r\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Was willst du mit der Blumen Kranze", "tokens": ["Was", "willst", "du", "mit", "der", "Blu\u00b7men", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du gr\u00fcnes seebesp\u00fcltes Land?", "tokens": ["Du", "gr\u00fc\u00b7nes", "see\u00b7be\u00b7sp\u00fcl\u00b7tes", "Land", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was willst du, Luft, mit blauem Glanze?", "tokens": ["Was", "willst", "du", ",", "Luft", ",", "mit", "blau\u00b7em", "Glan\u00b7ze", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was willst du, leerer Kahn, am Strand?", "tokens": ["Was", "willst", "du", ",", "lee\u00b7rer", "Kahn", ",", "am", "Strand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ihr schm\u00fccktet euch zu seiner Wonne,", "tokens": ["Ihr", "schm\u00fcck\u00b7tet", "euch", "zu", "sei\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hin ist er ohne Wiederkehr!", "tokens": ["Hin", "ist", "er", "oh\u00b7ne", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirf einen Schleier um, o Sonne!", "tokens": ["Wirf", "ei\u00b7nen", "Schlei\u00b7er", "um", ",", "o", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der letzte Staufen ist nicht mehr.", "tokens": ["Der", "letz\u00b7te", "Stau\u00b7fen", "ist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Kaum ist der Fr\u00fchling im Erwachen,", "tokens": ["Kaum", "ist", "der", "Fr\u00fch\u00b7ling", "im", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es bl\u00fcht der See, es bl\u00fcht der Baum,", "tokens": ["Es", "bl\u00fcht", "der", "See", ",", "es", "bl\u00fcht", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bl\u00fcht ein J\u00fcngling dort im Nachen,", "tokens": ["Es", "bl\u00fcht", "ein", "J\u00fcng\u00b7ling", "dort", "im", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er wiegt sich in der Wellen Schaum.", "tokens": ["Er", "wiegt", "sich", "in", "der", "Wel\u00b7len", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Wie eine Rosenknospe h\u00fcllet", "tokens": ["Wie", "ei\u00b7ne", "Ro\u00b7sen\u00b7knos\u00b7pe", "h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein junges Purpurkleid ihn ein,", "tokens": ["Ein", "jun\u00b7ges", "Pur\u00b7pur\u00b7kleid", "ihn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unter einer Krone quillet", "tokens": ["Und", "un\u00b7ter", "ei\u00b7ner", "Kro\u00b7ne", "quil\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Haar von g\u00fcldenerem Schein.", "tokens": ["Sein", "Haar", "von", "g\u00fcl\u00b7de\u00b7ne\u00b7rem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Es irret auf den blauen Wellen", "tokens": ["Es", "ir\u00b7ret", "auf", "den", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein sinnend Auge, wellenblau,", "tokens": ["Sein", "sin\u00b7nend", "Au\u00b7ge", ",", "wel\u00b7len\u00b7blau", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PWAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Leyer, die er schl\u00e4gt, entschwellen", "tokens": ["Der", "Le\u00b7yer", ",", "die", "er", "schl\u00e4gt", ",", "ent\u00b7schwel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ges\u00e4nge von der sch\u00f6nsten Frau.", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "von", "der", "sch\u00f6ns\u00b7ten", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Des ersten Donners Stimmen hallen,", "tokens": ["Des", "ers\u00b7ten", "Don\u00b7ners", "Stim\u00b7men", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im S\u00fcden blitzt es blutig rot;", "tokens": ["Im", "S\u00fc\u00b7den", "blitzt", "es", "blu\u00b7tig", "rot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4\u00dft sein Lied nur lauter schallen,", "tokens": ["Er", "l\u00e4\u00dft", "sein", "Lied", "nur", "lau\u00b7ter", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn k\u00fcmmert nichts, als Liebesnot.", "tokens": ["Ihn", "k\u00fcm\u00b7mert", "nichts", ",", "als", "Lie\u00b7bes\u00b7not", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Und wenn er Minne sich errungen,", "tokens": ["Und", "wenn", "er", "Min\u00b7ne", "sich", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So holt er sich dazu den Ruhm,", "tokens": ["So", "holt", "er", "sich", "da\u00b7zu", "den", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und herrscht, vom Lorberkranz umschlungen,", "tokens": ["Und", "herrscht", ",", "vom", "Lor\u00b7ber\u00b7kranz", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner V\u00e4ter Eigentum.", "tokens": ["In", "sei\u00b7ner", "V\u00e4\u00b7ter", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Kind! wie du stehst im schwanken Kahne,", "tokens": ["Kind", "!", "wie", "du", "stehst", "im", "schwan\u00b7ken", "Kah\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So rufet dich ein schwanker Thron,", "tokens": ["So", "ru\u00b7fet", "dich", "ein", "schwan\u00b7ker", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vertrau' dem Schatten nicht, dem Ahne,", "tokens": ["Ver\u00b7trau'", "dem", "Schat\u00b7ten", "nicht", ",", "dem", "Ah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00dfner, armer K\u00f6nigssohn!", "tokens": ["Ver\u00b7la\u00df\u00b7ner", ",", "ar\u00b7mer", "K\u00f6\u00b7nigs\u00b7sohn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Du bist so stolz und unerschrocken,", "tokens": ["Du", "bist", "so", "stolz", "und", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sinkest, eh' du es geglaubt,", "tokens": ["Du", "sin\u00b7kest", ",", "eh'", "du", "es", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sitzt die Kron' auf deinen Locken,", "tokens": ["Es", "sitzt", "die", "Kron'", "auf", "dei\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als tr\u00e4umte nur davon dein Haupt! \u2013", "tokens": ["Als", "tr\u00e4um\u00b7te", "nur", "da\u00b7von", "dein", "Haupt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "PAV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Er h\u00f6ret keine Warnungsstimme,", "tokens": ["Er", "h\u00f6\u00b7ret", "kei\u00b7ne", "War\u00b7nungs\u00b7stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schwimmt singend auf dem Abgrund hin,", "tokens": ["Schwimmt", "sin\u00b7gend", "auf", "dem", "Ab\u00b7grund", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wei\u00df er von des Sturmes Grimme?", "tokens": ["Was", "wei\u00df", "er", "von", "des", "Stur\u00b7mes", "Grim\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Lieb' und Leben steht sein Sinn.", "tokens": ["Nach", "Lieb'", "und", "Le\u00b7ben", "steht", "sein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "So gieb ihm Leben, gieb ihm Liebe,", "tokens": ["So", "gieb", "ihm", "Le\u00b7ben", ",", "gieb", "ihm", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wonnevolles Schwabenland,", "tokens": ["Du", "won\u00b7ne\u00b7vol\u00b7les", "Schwa\u00b7ben\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdopple deine Bl\u00fctentriebe,", "tokens": ["Ver\u00b7dopp\u00b7le", "dei\u00b7ne", "Bl\u00fc\u00b7ten\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kn\u00fcpf' ihm der Minne sel'ges Band!", "tokens": ["Kn\u00fcpf'", "ihm", "der", "Min\u00b7ne", "sel'\u00b7ges", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Es hat zu leben kurz der Knabe \u2013", "tokens": ["Es", "hat", "zu", "le\u00b7ben", "kurz", "der", "Kna\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hauch' ihm entgegen Lebensluft,", "tokens": ["Hauch'", "ihm", "ent\u00b7ge\u00b7gen", "Le\u00b7bens\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Durchw\u00fcrze jede kleine Gabe", "tokens": ["Durch\u00b7w\u00fcr\u00b7ze", "je\u00b7de", "klei\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ew'ger Jugend Bl\u00fctenduft!", "tokens": ["Mit", "ew'\u00b7ger", "Ju\u00b7gend", "Bl\u00fc\u00b7ten\u00b7duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Mach' ihm den Augenblick zu Jahren,", "tokens": ["Mach'", "ihm", "den", "Au\u00b7gen\u00b7blick", "zu", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den er an diesen Ufern lebt,", "tokens": ["Den", "er", "an", "die\u00b7sen", "U\u00b7fern", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er mit ungebleichten Haaren", "tokens": ["Da\u00df", "er", "mit", "un\u00b7ge\u00b7bleich\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Freude satt gen Himmel schwebt!", "tokens": ["An", "Freu\u00b7de", "satt", "gen", "Him\u00b7mel", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Was ist's? er l\u00e4\u00dft die Leyer fallen,", "tokens": ["Was", "ist's", "?", "er", "l\u00e4\u00dft", "die", "Le\u00b7yer", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er springt an's Ufer, greift zum Schwert,", "tokens": ["Er", "springt", "an's", "U\u00b7fer", ",", "greift", "zum", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O seht ihn \u00fcber Alpen wallen", "tokens": ["O", "seht", "ihn", "\u00fc\u00b7ber", "Al\u00b7pen", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit treuen M\u00e4nnern, hoch zu Pferd!", "tokens": ["Mit", "treu\u00b7en", "M\u00e4n\u00b7nern", ",", "hoch", "zu", "Pferd", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Der Lust, der Liebe Lieder schweigen,", "tokens": ["Der", "Lust", ",", "der", "Lie\u00b7be", "Lie\u00b7der", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er gl\u00fcht von edlerem Gel\u00fcst;", "tokens": ["Er", "gl\u00fcht", "von", "ed\u00b7le\u00b7rem", "Ge\u00b7l\u00fcst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er will der V\u00e4ter Thron besteigen \u2013", "tokens": ["Er", "will", "der", "V\u00e4\u00b7ter", "Thron", "be\u00b7stei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandelt auf das Blutger\u00fcst.", "tokens": ["Und", "wan\u00b7delt", "auf", "das", "Blut\u00b7ge\u00b7r\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.29": {"line.1": {"text": "Was willst du mit der Blumen Kranze", "tokens": ["Was", "willst", "du", "mit", "der", "Blu\u00b7men", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du gr\u00fcnes seebesp\u00fcltes Land?", "tokens": ["Du", "gr\u00fc\u00b7nes", "see\u00b7be\u00b7sp\u00fcl\u00b7tes", "Land", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was willst du, Luft, mit blauem Glanze?", "tokens": ["Was", "willst", "du", ",", "Luft", ",", "mit", "blau\u00b7em", "Glan\u00b7ze", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was willst du, leerer Kahn, am Strand?", "tokens": ["Was", "willst", "du", ",", "lee\u00b7rer", "Kahn", ",", "am", "Strand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.30": {"line.1": {"text": "Ihr schm\u00fccktet euch zu seiner Wonne,", "tokens": ["Ihr", "schm\u00fcck\u00b7tet", "euch", "zu", "sei\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hin ist er ohne Wiederkehr!", "tokens": ["Hin", "ist", "er", "oh\u00b7ne", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirf einen Schleier um, o Sonne!", "tokens": ["Wirf", "ei\u00b7nen", "Schlei\u00b7er", "um", ",", "o", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der letzte Staufen ist nicht mehr.", "tokens": ["Der", "letz\u00b7te", "Stau\u00b7fen", "ist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}