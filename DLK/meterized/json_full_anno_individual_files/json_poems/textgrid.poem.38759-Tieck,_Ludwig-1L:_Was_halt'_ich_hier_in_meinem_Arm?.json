{"textgrid.poem.38759": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was halt' ich hier in meinem Arm?", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was halt' ich hier in meinem Arm?", "tokens": ["Was", "halt'", "ich", "hier", "in", "mei\u00b7nem", "Arm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was l\u00e4chelt mich an so hold und warm?", "tokens": ["Was", "l\u00e4\u00b7chelt", "mich", "an", "so", "hold", "und", "warm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es ist der Knabe, die Liebe!", "tokens": ["Es", "ist", "der", "Kna\u00b7be", ",", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich wieg' ihn und schaukl' ihn auf Knie und Schoo\u00df,", "tokens": ["Ich", "wieg'", "ihn", "und", "schau\u00b7kl'", "ihn", "auf", "Knie", "und", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wie hat er die Augen so hell und gro\u00df!", "tokens": ["Wie", "hat", "er", "die", "Au\u00b7gen", "so", "hell", "und", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "O himmlische, himmlische Liebe!", "tokens": ["O", "himm\u00b7li\u00b7sche", ",", "himm\u00b7li\u00b7sche", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Der Junge hat sch\u00f6n krausgoldenes Haar,", "tokens": ["Der", "Jun\u00b7ge", "hat", "sch\u00f6n", "kraus\u00b7gol\u00b7de\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Mund wie Rosen hell und klar,", "tokens": ["Den", "Mund", "wie", "Ro\u00b7sen", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Blumen die liebliche Wange;", "tokens": ["Wie", "Blu\u00b7men", "die", "lieb\u00b7li\u00b7che", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sein Blick ist Wonne und Himmel sein Ku\u00df,", "tokens": ["Sein", "Blick", "ist", "Won\u00b7ne", "und", "Him\u00b7mel", "sein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Red' und Gelach Paradiesesflu\u00df,", "tokens": ["Red'", "und", "Ge\u00b7lach", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Wie Engel die Stimm' im Gesange.", "tokens": ["Wie", "En\u00b7gel", "die", "Stimm'", "im", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und liebst du mich denn? \u2013 Da k\u00fc\u00dft er ein Ja!", "tokens": ["Und", "liebst", "du", "mich", "denn", "?", "\u2013", "Da", "k\u00fc\u00dft", "er", "ein", "Ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wie ich ihm tief in die Augen nun sah,", "tokens": ["Und", "wie", "ich", "ihm", "tief", "in", "die", "Au\u00b7gen", "nun", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADJD", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da schl\u00e4gt er mir grimmige Schmerzen;", "tokens": ["Da", "schl\u00e4gt", "er", "mir", "grim\u00b7mi\u00b7ge", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O b\u00f6ses Kind! ei wie t\u00fcckisch du!", "tokens": ["O", "b\u00f6\u00b7ses", "Kind", "!", "ei", "wie", "t\u00fc\u00b7ckisch", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "KOKOM", "ADJD", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo ist deine Milde, die liebliche Ruh?", "tokens": ["Wo", "ist", "dei\u00b7ne", "Mil\u00b7de", ",", "die", "lieb\u00b7li\u00b7che", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wo deine Sanftmuth, dein Scherzen?", "tokens": ["Wo", "dei\u00b7ne", "Sanft\u00b7muth", ",", "dein", "Scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun geht ein s\u00fc\u00df L\u00e4cheln ihm \u00fcber's Gesicht:", "tokens": ["Nun", "geht", "ein", "s\u00fc\u00df", "L\u00e4\u00b7cheln", "ihm", "\u00fc\u00b7ber's", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich liebe dich nicht! ich liebe dich nicht!", "tokens": ["Ich", "lie\u00b7be", "dich", "nicht", "!", "ich", "lie\u00b7be", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da setz' ich ihn nieder zu F\u00fcssen.", "tokens": ["Da", "setz'", "ich", "ihn", "nie\u00b7der", "zu", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O weh mir! so ruft nun und weinet das Kind,", "tokens": ["O", "weh", "mir", "!", "so", "ruft", "nun", "und", "wei\u00b7net", "das", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ADV", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Du B\u00f6se, o nimm mich auf geschwind,", "tokens": ["Du", "B\u00f6\u00b7se", ",", "o", "nimm", "mich", "auf", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "FM", "VVIMP", "PPER", "APPR", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich will, ich mu\u00df dich k\u00fcssen.", "tokens": ["Ich", "will", ",", "ich", "mu\u00df", "dich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich heb' ihn empor, er schreiet nur fort,", "tokens": ["Ich", "heb'", "ihn", "em\u00b7por", ",", "er", "schrei\u00b7et", "nur", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er h\u00f6rt auf kein liebkosendes Wort,", "tokens": ["Er", "h\u00f6rt", "auf", "kein", "lieb\u00b7ko\u00b7sen\u00b7des", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er spreitelt mit Beinen und H\u00e4nden:", "tokens": ["Er", "sprei\u00b7telt", "mit", "Bei\u00b7nen", "und", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mich \u00e4ngstet und bet\u00e4ubt sein Geschrei,", "tokens": ["Mich", "\u00e4ngs\u00b7tet", "und", "be\u00b7t\u00e4ubt", "sein", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mich r\u00fchren die rollenden Thr\u00e4nen dabei,", "tokens": ["Mich", "r\u00fch\u00b7ren", "die", "rol\u00b7len\u00b7den", "Thr\u00e4\u00b7nen", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Er will die Unart nicht enden.", "tokens": ["Er", "will", "die", "Un\u00b7art", "nicht", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und gr\u00f6\u00dfer die Angst, und gr\u00f6\u00dfer die Noth,", "tokens": ["Und", "gr\u00f6\u00b7\u00dfer", "die", "Angst", ",", "und", "gr\u00f6\u00b7\u00dfer", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich w\u00fcnsche mir selbst und dem Kleinen den Tod,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "selbst", "und", "dem", "Klei\u00b7nen", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich nehm' ihn und wieg' ihn zum Schlafe:", "tokens": ["Ich", "nehm'", "ihn", "und", "wieg'", "ihn", "zum", "Schla\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wie er nur schweigt, und wie er nur still,", "tokens": ["Und", "wie", "er", "nur", "schweigt", ",", "und", "wie", "er", "nur", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "KON", "PWAV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Verge\u00df ich, da\u00df ich ihn z\u00fcchtigen will,", "tokens": ["Ver\u00b7ge\u00df", "ich", ",", "da\u00df", "ich", "ihn", "z\u00fcch\u00b7ti\u00b7gen", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Meine Lieb' seine ganze Strafe.", "tokens": ["Mei\u00b7ne", "Lieb'", "sei\u00b7ne", "gan\u00b7ze", "Stra\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Da schlummert er s\u00fc\u00df, es hebt sich die Brust", "tokens": ["Da", "schlum\u00b7mert", "er", "s\u00fc\u00df", ",", "es", "hebt", "sich", "die", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom lieben Athem, ich s\u00e4tt'ge die Lust", "tokens": ["Vom", "lie\u00b7ben", "A\u00b7them", ",", "ich", "s\u00e4tt'\u00b7ge", "die", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und kann genug nicht schauen:", "tokens": ["Und", "kann", "ge\u00b7nug", "nicht", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ist er so still? Wie ist er so stumm?", "tokens": ["Wie", "ist", "er", "so", "still", "?", "Wie", "ist", "er", "so", "stumm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er schl\u00e4gt nicht, und wirft sich nicht wild herum,", "tokens": ["Er", "schl\u00e4gt", "nicht", ",", "und", "wirft", "sich", "nicht", "wild", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Er tobt nicht! es bef\u00e4llt mich ein Grauen.", "tokens": ["Er", "tobt", "nicht", "!", "es", "be\u00b7f\u00e4llt", "mich", "ein", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "O k\u00f6nnte der Schlaf nicht Tod auch seyn?", "tokens": ["O", "k\u00f6nn\u00b7te", "der", "Schlaf", "nicht", "Tod", "auch", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PTKNEG", "NN", "ADV", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich weck' ihn mit K\u00fcssen; nun h\u00f6r' ich ihn schrein,", "tokens": ["Ich", "weck'", "ihn", "mit", "K\u00fcs\u00b7sen", ";", "nun", "h\u00f6r'", "ich", "ihn", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nun schl\u00e4gt er, nun kos't er, meine Wonne, mein Sorgen,", "tokens": ["Nun", "schl\u00e4gt", "er", ",", "nun", "kos't", "er", ",", "mei\u00b7ne", "Won\u00b7ne", ",", "mein", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dann dr\u00fcckt er mich an die liebliche Brust,", "tokens": ["Dann", "dr\u00fcckt", "er", "mich", "an", "die", "lieb\u00b7li\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun bin ich sein Feind, dann Freund ihm und Lust: \u2013", "tokens": ["Nun", "bin", "ich", "sein", "Feind", ",", "dann", "Freund", "ihm", "und", "Lust", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "NN", "PPER", "KON", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So geht's bis zum Abend vom Morgen.", "tokens": ["So", "geht's", "bis", "zum", "A\u00b7bend", "vom", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Was halt' ich hier in meinem Arm?", "tokens": ["Was", "halt'", "ich", "hier", "in", "mei\u00b7nem", "Arm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was l\u00e4chelt mich an so hold und warm?", "tokens": ["Was", "l\u00e4\u00b7chelt", "mich", "an", "so", "hold", "und", "warm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es ist der Knabe, die Liebe!", "tokens": ["Es", "ist", "der", "Kna\u00b7be", ",", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich wieg' ihn und schaukl' ihn auf Knie und Schoo\u00df,", "tokens": ["Ich", "wieg'", "ihn", "und", "schau\u00b7kl'", "ihn", "auf", "Knie", "und", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wie hat er die Augen so hell und gro\u00df!", "tokens": ["Wie", "hat", "er", "die", "Au\u00b7gen", "so", "hell", "und", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "O himmlische, himmlische Liebe!", "tokens": ["O", "himm\u00b7li\u00b7sche", ",", "himm\u00b7li\u00b7sche", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Der Junge hat sch\u00f6n krausgoldenes Haar,", "tokens": ["Der", "Jun\u00b7ge", "hat", "sch\u00f6n", "kraus\u00b7gol\u00b7de\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Mund wie Rosen hell und klar,", "tokens": ["Den", "Mund", "wie", "Ro\u00b7sen", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Blumen die liebliche Wange;", "tokens": ["Wie", "Blu\u00b7men", "die", "lieb\u00b7li\u00b7che", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sein Blick ist Wonne und Himmel sein Ku\u00df,", "tokens": ["Sein", "Blick", "ist", "Won\u00b7ne", "und", "Him\u00b7mel", "sein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Red' und Gelach Paradiesesflu\u00df,", "tokens": ["Red'", "und", "Ge\u00b7lach", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Wie Engel die Stimm' im Gesange.", "tokens": ["Wie", "En\u00b7gel", "die", "Stimm'", "im", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Und liebst du mich denn? \u2013 Da k\u00fc\u00dft er ein Ja!", "tokens": ["Und", "liebst", "du", "mich", "denn", "?", "\u2013", "Da", "k\u00fc\u00dft", "er", "ein", "Ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wie ich ihm tief in die Augen nun sah,", "tokens": ["Und", "wie", "ich", "ihm", "tief", "in", "die", "Au\u00b7gen", "nun", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADJD", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da schl\u00e4gt er mir grimmige Schmerzen;", "tokens": ["Da", "schl\u00e4gt", "er", "mir", "grim\u00b7mi\u00b7ge", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O b\u00f6ses Kind! ei wie t\u00fcckisch du!", "tokens": ["O", "b\u00f6\u00b7ses", "Kind", "!", "ei", "wie", "t\u00fc\u00b7ckisch", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NE", "KOKOM", "ADJD", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo ist deine Milde, die liebliche Ruh?", "tokens": ["Wo", "ist", "dei\u00b7ne", "Mil\u00b7de", ",", "die", "lieb\u00b7li\u00b7che", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wo deine Sanftmuth, dein Scherzen?", "tokens": ["Wo", "dei\u00b7ne", "Sanft\u00b7muth", ",", "dein", "Scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Nun geht ein s\u00fc\u00df L\u00e4cheln ihm \u00fcber's Gesicht:", "tokens": ["Nun", "geht", "ein", "s\u00fc\u00df", "L\u00e4\u00b7cheln", "ihm", "\u00fc\u00b7ber's", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich liebe dich nicht! ich liebe dich nicht!", "tokens": ["Ich", "lie\u00b7be", "dich", "nicht", "!", "ich", "lie\u00b7be", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da setz' ich ihn nieder zu F\u00fcssen.", "tokens": ["Da", "setz'", "ich", "ihn", "nie\u00b7der", "zu", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "O weh mir! so ruft nun und weinet das Kind,", "tokens": ["O", "weh", "mir", "!", "so", "ruft", "nun", "und", "wei\u00b7net", "das", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ADV", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Du B\u00f6se, o nimm mich auf geschwind,", "tokens": ["Du", "B\u00f6\u00b7se", ",", "o", "nimm", "mich", "auf", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "FM", "VVIMP", "PPER", "APPR", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich will, ich mu\u00df dich k\u00fcssen.", "tokens": ["Ich", "will", ",", "ich", "mu\u00df", "dich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich heb' ihn empor, er schreiet nur fort,", "tokens": ["Ich", "heb'", "ihn", "em\u00b7por", ",", "er", "schrei\u00b7et", "nur", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er h\u00f6rt auf kein liebkosendes Wort,", "tokens": ["Er", "h\u00f6rt", "auf", "kein", "lieb\u00b7ko\u00b7sen\u00b7des", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er spreitelt mit Beinen und H\u00e4nden:", "tokens": ["Er", "sprei\u00b7telt", "mit", "Bei\u00b7nen", "und", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mich \u00e4ngstet und bet\u00e4ubt sein Geschrei,", "tokens": ["Mich", "\u00e4ngs\u00b7tet", "und", "be\u00b7t\u00e4ubt", "sein", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mich r\u00fchren die rollenden Thr\u00e4nen dabei,", "tokens": ["Mich", "r\u00fch\u00b7ren", "die", "rol\u00b7len\u00b7den", "Thr\u00e4\u00b7nen", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Er will die Unart nicht enden.", "tokens": ["Er", "will", "die", "Un\u00b7art", "nicht", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Und gr\u00f6\u00dfer die Angst, und gr\u00f6\u00dfer die Noth,", "tokens": ["Und", "gr\u00f6\u00b7\u00dfer", "die", "Angst", ",", "und", "gr\u00f6\u00b7\u00dfer", "die", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich w\u00fcnsche mir selbst und dem Kleinen den Tod,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "mir", "selbst", "und", "dem", "Klei\u00b7nen", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich nehm' ihn und wieg' ihn zum Schlafe:", "tokens": ["Ich", "nehm'", "ihn", "und", "wieg'", "ihn", "zum", "Schla\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wie er nur schweigt, und wie er nur still,", "tokens": ["Und", "wie", "er", "nur", "schweigt", ",", "und", "wie", "er", "nur", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "KON", "PWAV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Verge\u00df ich, da\u00df ich ihn z\u00fcchtigen will,", "tokens": ["Ver\u00b7ge\u00df", "ich", ",", "da\u00df", "ich", "ihn", "z\u00fcch\u00b7ti\u00b7gen", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Meine Lieb' seine ganze Strafe.", "tokens": ["Mei\u00b7ne", "Lieb'", "sei\u00b7ne", "gan\u00b7ze", "Stra\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Da schlummert er s\u00fc\u00df, es hebt sich die Brust", "tokens": ["Da", "schlum\u00b7mert", "er", "s\u00fc\u00df", ",", "es", "hebt", "sich", "die", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom lieben Athem, ich s\u00e4tt'ge die Lust", "tokens": ["Vom", "lie\u00b7ben", "A\u00b7them", ",", "ich", "s\u00e4tt'\u00b7ge", "die", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und kann genug nicht schauen:", "tokens": ["Und", "kann", "ge\u00b7nug", "nicht", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ist er so still? Wie ist er so stumm?", "tokens": ["Wie", "ist", "er", "so", "still", "?", "Wie", "ist", "er", "so", "stumm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er schl\u00e4gt nicht, und wirft sich nicht wild herum,", "tokens": ["Er", "schl\u00e4gt", "nicht", ",", "und", "wirft", "sich", "nicht", "wild", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Er tobt nicht! es bef\u00e4llt mich ein Grauen.", "tokens": ["Er", "tobt", "nicht", "!", "es", "be\u00b7f\u00e4llt", "mich", "ein", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "O k\u00f6nnte der Schlaf nicht Tod auch seyn?", "tokens": ["O", "k\u00f6nn\u00b7te", "der", "Schlaf", "nicht", "Tod", "auch", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PTKNEG", "NN", "ADV", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich weck' ihn mit K\u00fcssen; nun h\u00f6r' ich ihn schrein,", "tokens": ["Ich", "weck'", "ihn", "mit", "K\u00fcs\u00b7sen", ";", "nun", "h\u00f6r'", "ich", "ihn", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nun schl\u00e4gt er, nun kos't er, meine Wonne, mein Sorgen,", "tokens": ["Nun", "schl\u00e4gt", "er", ",", "nun", "kos't", "er", ",", "mei\u00b7ne", "Won\u00b7ne", ",", "mein", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dann dr\u00fcckt er mich an die liebliche Brust,", "tokens": ["Dann", "dr\u00fcckt", "er", "mich", "an", "die", "lieb\u00b7li\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun bin ich sein Feind, dann Freund ihm und Lust: \u2013", "tokens": ["Nun", "bin", "ich", "sein", "Feind", ",", "dann", "Freund", "ihm", "und", "Lust", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "NN", "PPER", "KON", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So geht's bis zum Abend vom Morgen.", "tokens": ["So", "geht's", "bis", "zum", "A\u00b7bend", "vom", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}