{"dta.poem.2130": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beweis g\u00f6ttlicher G\u00fcte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df aller Blumen bunte Pracht", "tokens": ["Da\u00df", "al\u00b7ler", "Blu\u00b7men", "bun\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Menschen ganz allein gemacht;", "tokens": ["F\u00fcr", "Men\u00b7schen", "ganz", "al\u00b7lein", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Davon legt die Betrachtung mir", "tokens": ["Da\u00b7von", "legt", "die", "Be\u00b7trach\u00b7tung", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Den kr\u00e4ftigsten Beweisthum f\u00fcr:", "tokens": ["Den", "kr\u00e4f\u00b7tigs\u00b7ten", "Be\u00b7wei\u00b7sthum", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Wenn sich der Mensch vom Schlaf erhebet,", "tokens": ["Wenn", "sich", "der", "Mensch", "vom", "Schlaf", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint die auch aufgewachte Schaar,", "tokens": ["Scheint", "die", "auch", "auf\u00b7ge\u00b7wach\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die in der Nacht geschlossen war,", "tokens": ["Die", "in", "der", "Nacht", "ge\u00b7schlos\u00b7sen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aufs neu versch\u00f6nert und belebet.", "tokens": ["Aufs", "neu", "ver\u00b7sch\u00f6\u00b7nert", "und", "be\u00b7le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie scheint sich \u00e4msig zu bem\u00fchn,", "tokens": ["Sie", "scheint", "sich", "\u00e4m\u00b7sig", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auch ihrer Ruh sich zu entziehn,", "tokens": ["Auch", "ih\u00b7rer", "Ruh", "sich", "zu", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und die verschlo\u00dfne Lieblichkeiten,", "tokens": ["Und", "die", "ver\u00b7schlo\u00df\u00b7ne", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In ihren Bl\u00e4ttern, zu verbreiten.", "tokens": ["In", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", ",", "zu", "ver\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie legen ihre bunte Zier", "tokens": ["Sie", "le\u00b7gen", "ih\u00b7re", "bun\u00b7te", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zum Gegenwurf der Sonne f\u00fcr,", "tokens": ["Zum", "Ge\u00b7gen\u00b7wurf", "der", "Son\u00b7ne", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die sie, bald durch- bald angestralet,", "tokens": ["Die", "sie", ",", "bald", "durch", "bald", "an\u00b7ge\u00b7stra\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "TRUNC", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zu unsrer Lust stets sch\u00f6ner mahlet,", "tokens": ["Zu", "uns\u00b7rer", "Lust", "stets", "sch\u00f6\u00b7ner", "mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Um nur aus ihren innern Sch\u00e4tzen", "tokens": ["Um", "nur", "aus", "ih\u00b7ren", "in\u00b7nern", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Nach M\u00f6glichkeit uns zu ergetzen.", "tokens": ["Nach", "M\u00f6g\u00b7lich\u00b7keit", "uns", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Gehn wir des Abends nun zur Ruh,", "tokens": ["Gehn", "wir", "des", "A\u00b7bends", "nun", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So schlie\u00dfen sich die Blumen zu,", "tokens": ["So", "schlie\u00b7\u00dfen", "sich", "die", "Blu\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Als wenn sie gleichsam zeigen wollen:", "tokens": ["Als", "wenn", "sie", "gleich\u00b7sam", "zei\u00b7gen", "wol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df sie f\u00fcr uns nur bl\u00fchen sollen,", "tokens": ["Da\u00df", "sie", "f\u00fcr", "uns", "nur", "bl\u00fc\u00b7hen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df, wenn der Menschen Aug\u2019 ihr Prangen", "tokens": ["Da\u00df", ",", "wenn", "der", "Men\u00b7schen", "Aug'", "ihr", "Pran\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nicht sehn, betrachten, und daran", "tokens": ["Nicht", "sehn", ",", "be\u00b7trach\u00b7ten", ",", "und", "da\u00b7ran"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVINF", "$,", "VVFIN", "$,", "KON", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nicht ferner sich ergetzen kann,", "tokens": ["Nicht", "fer\u00b7ner", "sich", "er\u00b7get\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sie auch zu prangen nicht verlangen.", "tokens": ["Sie", "auch", "zu", "pran\u00b7gen", "nicht", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Mich deucht, da\u00df diese neue Spur", "tokens": ["Mich", "deucht", ",", "da\u00df", "die\u00b7se", "neu\u00b7e", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der Ordnung in der Kreatur", "tokens": ["Der", "Ord\u00b7nung", "in", "der", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Von neuem eine Probe giebet,", "tokens": ["Von", "neu\u00b7em", "ei\u00b7ne", "Pro\u00b7be", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Wie sehr uns unser Sch\u00f6pfer liebet.", "tokens": ["Wie", "sehr", "uns", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}