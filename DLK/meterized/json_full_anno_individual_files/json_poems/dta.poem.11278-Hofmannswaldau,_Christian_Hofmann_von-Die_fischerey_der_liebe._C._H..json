{"dta.poem.11278": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die fischerey der liebe.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die lieb und fischerey", "tokens": ["Die", "lieb", "und", "fi\u00b7sche\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die kommen beyd\u2019 einander ziemlich bey.", "tokens": ["Die", "kom\u00b7men", "beyd'", "ein\u00b7an\u00b7der", "ziem\u00b7lich", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn, wer was liebes will erwischen,", "tokens": ["Denn", ",", "wer", "was", "lie\u00b7bes", "will", "er\u00b7wi\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PWS", "ADJA", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der pflegt herum zu fischen;", "tokens": ["Der", "pflegt", "he\u00b7rum", "zu", "fi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn er was erwischt,", "tokens": ["Und", "wenn", "er", "was", "er\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So wird erst recht gefischt;", "tokens": ["So", "wird", "erst", "recht", "ge\u00b7fischt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bi\u00df endlich seinen flu\u00df", "tokens": ["Bi\u00df", "end\u00b7lich", "sei\u00b7nen", "flu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die kinder-mutter fischen mu\u00df.", "tokens": ["Die", "kin\u00b7der\u00b7mut\u00b7ter", "fi\u00b7schen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}