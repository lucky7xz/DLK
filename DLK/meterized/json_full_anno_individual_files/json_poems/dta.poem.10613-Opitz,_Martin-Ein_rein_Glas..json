{"dta.poem.10613": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ein rein Glas.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Freylich/ freylich ist ein Glas/", "tokens": ["Frey\u00b7lich", "/", "frey\u00b7lich", "ist", "ein", "Glas", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Edle Jungfraw/ alles das/", "tokens": ["Ed\u00b7le", "Jung\u00b7fraw", "/", "al\u00b7les", "das", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PIS", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was in ewrer besten Zier/", "tokens": ["Was", "in", "ew\u00b7rer", "bes\u00b7ten", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die Sonne/ leuchtet f\u00fcr/", "tokens": ["Als", "die", "Son\u00b7ne", "/", "leuch\u00b7tet", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "VVFIN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaut/ wie sch\u00f6n die Sternen all", "tokens": ["Schaut", "/", "wie", "sch\u00f6n", "die", "Ster\u00b7nen", "all"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "PWAV", "ADJD", "ART", "NN", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leuchten au\u00df des Himmels Saal/", "tokens": ["Leuch\u00b7ten", "au\u00df", "des", "Him\u00b7mels", "Saal", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Mond sein bleiches Haar", "tokens": ["Wie", "der", "Mond", "sein", "blei\u00b7ches", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Au\u00dfgebreitet gantz vnd gar/", "tokens": ["Au\u00df\u00b7ge\u00b7brei\u00b7tet", "gantz", "vnd", "gar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie die grosse weite Welt", "tokens": ["Wie", "die", "gros\u00b7se", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schl\u00e4frig in die Bethe fellt/", "tokens": ["Schl\u00e4f\u00b7rig", "in", "die", "Be\u00b7the", "fellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie die Wasser stehen still/", "tokens": ["Wie", "die", "Was\u00b7ser", "ste\u00b7hen", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie sich nichts bewegen will.", "tokens": ["Wie", "sich", "nichts", "be\u00b7we\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Eh