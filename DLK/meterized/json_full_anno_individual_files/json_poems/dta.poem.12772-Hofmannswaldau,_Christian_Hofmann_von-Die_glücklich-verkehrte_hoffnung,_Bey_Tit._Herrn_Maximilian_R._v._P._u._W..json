{"dta.poem.12772": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die gl\u00fccklich-verkehrte hoffnung,  \n  Bey   Tit.  \n Herrn Maximilian R. v. P. u. W.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Schotten k\u00f6nigin, das muster vieler pein,", "tokens": ["Der", "Schot\u00b7ten", "k\u00f6\u00b7ni\u00b7gin", ",", "das", "mus\u00b7ter", "vie\u00b7ler", "pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PRELS", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ein gesch\u00e4rfftes beil in Engelland gef\u00e4llet,", "tokens": ["Die", "ein", "ge\u00b7sch\u00e4rff\u00b7tes", "beil", "in", "En\u00b7gel\u00b7land", "ge\u00b7f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die der nachwelt sich zum schau-spiel aufgestellet;", "tokens": ["Und", "die", "der", "nach\u00b7welt", "sich", "zum", "schau\u00b7spiel", "auf\u00b7ge\u00b7stel\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "VVFIN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Grub ihren fenstern di\u00df mit diamanten ein,", "tokens": ["Grub", "ih\u00b7ren", "fens\u00b7tern", "di\u00df", "mit", "di\u00b7a\u00b7man\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PDS", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und schrieb, wofern es wahr, was man zu schreiben pfleget:", "tokens": ["Und", "schrieb", ",", "wo\u00b7fern", "es", "wahr", ",", "was", "man", "zu", "schrei\u00b7ben", "pfle\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der hoffnung spitze hat mich in den staub geleget.", "tokens": ["Der", "hoff\u00b7nung", "spit\u00b7ze", "hat", "mich", "in", "den", "staub", "ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was diese k\u00f6nigin ihr selber zugedacht,", "tokens": ["Was", "die\u00b7se", "k\u00f6\u00b7ni\u00b7gin", "ihr", "sel\u00b7ber", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist ein gemeiner satz, den sonst die meisten f\u00fchlen.", "tokens": ["Ist", "ein", "ge\u00b7mei\u00b7ner", "satz", ",", "den", "sonst", "die", "meis\u00b7ten", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das hoffen l\u00e4st uns erst mit hundert s\u00e4tzen spielen,", "tokens": ["Das", "hof\u00b7fen", "l\u00e4st", "uns", "erst", "mit", "hun\u00b7dert", "s\u00e4t\u00b7zen", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "ADV", "APPR", "CARD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und endlich ists ein traum, wenn wir erst aufgewacht.", "tokens": ["Und", "end\u00b7lich", "ists", "ein", "traum", ",", "wenn", "wir", "erst", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer auf der hoffnung berg die spitze fast erstiegen,", "tokens": ["Wer", "auf", "der", "hoff\u00b7nung", "berg", "die", "spit\u00b7ze", "fast", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "ART", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mu\u00df offtmals unverhofft gest\u00fcrtzt im grunde liegen.", "tokens": ["Mu\u00df", "offt\u00b7mals", "un\u00b7ver\u00b7hofft", "ge\u00b7st\u00fcrtzt", "im", "grun\u00b7de", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VVPP", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man hofft bey abend-roth verkl\u00e4rte morgen-zeit,", "tokens": ["Man", "hofft", "bey", "a\u00b7ben\u00b7droth", "ver\u00b7kl\u00e4r\u00b7te", "mor\u00b7gen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Pflegt gleich der tag darauf in nebel sich zu h\u00fcllen,", "tokens": ["Pflegt", "gleich", "der", "tag", "da\u00b7rauf", "in", "ne\u00b7bel", "sich", "zu", "h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PAV", "APPR", "NE", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und in der blitzen macht vor sonnen gold zu f\u00fcllen", "tokens": ["Und", "in", "der", "blit\u00b7zen", "macht", "vor", "son\u00b7nen", "gold", "zu", "f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die \u00e4pffel essen wir, die uns die wohllust beut,", "tokens": ["Die", "\u00e4pf\u00b7fel", "es\u00b7sen", "wir", ",", "die", "uns", "die", "wohl\u00b7lust", "beut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In meynung durch den bi\u00df das leben zu vers\u00fc\u00dfen,", "tokens": ["In", "mey\u00b7nung", "durch", "den", "bi\u00df", "das", "le\u00b7ben", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "APPR", "PDS", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn wir mit schmertz und tod die kost bezahlen m\u00fcssen.", "tokens": ["Wenn", "wir", "mit", "schmertz", "und", "tod", "die", "kost", "be\u00b7zah\u00b7len", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "KON", "NN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die hoffnung stellt sich uns mit einem ancker vor;", "tokens": ["Die", "hoff\u00b7nung", "stellt", "sich", "uns", "mit", "ei\u00b7nem", "an\u00b7cker", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wir gl\u00e4uben, da\u00df das schiff der fl\u00fcchtigen gedancken", "tokens": ["Wir", "gl\u00e4u\u00b7ben", ",", "da\u00df", "das", "schiff", "der", "fl\u00fcch\u00b7ti\u00b7gen", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nicht d\u00f6rffe durch gewalt von seiner stellung wancken.", "tokens": ["Nicht", "d\u00f6rf\u00b7fe", "durch", "ge\u00b7walt", "von", "sei\u00b7ner", "stel\u00b7lung", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da bricht der ancker offt, wie leichtes schilff und rohr.", "tokens": ["Da", "bricht", "der", "an\u00b7cker", "offt", ",", "wie", "leich\u00b7tes", "schilff", "und", "rohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "PWAV", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was gro\u00df und nahe scheint, was fast in unsern h\u00e4nden,", "tokens": ["Was", "gro\u00df", "und", "na\u00b7he", "scheint", ",", "was", "fast", "in", "un\u00b7sern", "h\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Verbirgt sich, wenn die zeit ihr fern-glas pflegt zu wenden:", "tokens": ["Ver\u00b7birgt", "sich", ",", "wenn", "die", "zeit", "ihr", "fern\u00b7glas", "pflegt", "zu", "wen\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So schl\u00e4gt die hoffnung fehl. Die furcht, ihr widerspiel,", "tokens": ["So", "schl\u00e4gt", "die", "hoff\u00b7nung", "fehl", ".", "Die", "furcht", ",", "ihr", "wi\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Pflegt auch im ausgang offt, sich anders zu verm\u00e4hlen,", "tokens": ["Pflegt", "auch", "im", "aus\u00b7gang", "offt", ",", "sich", "an\u00b7ders", "zu", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "VVFIN", "ADV", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein sch\u00fctze, der den schu\u00df sich f\u00fcrchtet zu verfehlen,", "tokens": ["Ein", "sch\u00fct\u00b7ze", ",", "der", "den", "schu\u00df", "sich", "f\u00fcrch\u00b7tet", "zu", "ver\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN", "PRF", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Trifft manchmal unverhofft das vorgesteckte ziel.", "tokens": ["Trifft", "manch\u00b7mal", "un\u00b7ver\u00b7hofft", "das", "vor\u00b7ge\u00b7steck\u00b7te", "ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So hat ein sichrer leib, den fast der tod gebunden,", "tokens": ["So", "hat", "ein", "sich\u00b7rer", "leib", ",", "den", "fast", "der", "tod", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Doch die gesundheit offt in kaltem giffte funden.", "tokens": ["Doch", "die", "ge\u00b7sund\u00b7heit", "offt", "in", "kal\u00b7tem", "giff\u00b7te", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein zufall, der uns nicht, wie wir gew\u00fcnscht, ber\u00fchrt,", "tokens": ["Ein", "zu\u00b7fall", ",", "der", "uns", "nicht", ",", "wie", "wir", "ge\u00b7w\u00fcnscht", ",", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "Ein creutz und jammer-strahl von g\u00f6ttlichem geschicke,", "tokens": ["Ein", "creutz", "und", "jam\u00b7mer\u00b7strahl", "von", "g\u00f6tt\u00b7li\u00b7chem", "ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Erscheinen uns durch furcht, als lauter donner-blicke:", "tokens": ["Er\u00b7schei\u00b7nen", "uns", "durch", "furcht", ",", "als", "lau\u00b7ter", "don\u00b7ner\u00b7bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "KOUS", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da doch ein l\u00e4ger opst offt honig in sich f\u00fchrt.", "tokens": ["Da", "doch", "ein", "l\u00e4\u00b7ger", "opst", "offt", "ho\u00b7nig", "in", "sich", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADV", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So f\u00fcrchten wir den weg des todes zu beschreiten,", "tokens": ["So", "f\u00fcrch\u00b7ten", "wir", "den", "weg", "des", "to\u00b7des", "zu", "be\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der doch ein zugang ist zu tausend liebligkeiten,", "tokens": ["Der", "doch", "ein", "zu\u00b7gang", "ist", "zu", "tau\u00b7send", "lieb\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJD", "VAFIN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie unser gro\u00dfe boll die ruder treibt und lenckt,", "tokens": ["Wie", "un\u00b7ser", "gro\u00b7\u00dfe", "boll", "die", "ru\u00b7der", "treibt", "und", "lenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "ADJD", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So mu\u00df das leichte schiff der furcht und hoffnung fahren,", "tokens": ["So", "mu\u00df", "das", "leich\u00b7te", "schiff", "der", "furcht", "und", "hoff\u00b7nung", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "VVFIN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er l\u00e4sset dessen thun sich mit verwirrung paaren,", "tokens": ["Er", "l\u00e4s\u00b7set", "des\u00b7sen", "thun", "sich", "mit", "ver\u00b7wir\u00b7rung", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der trotzig wolcken-an mit seinem Babel denckt,", "tokens": ["Der", "trot\u00b7zig", "wol\u00b7cken\u00b7an", "mit", "sei\u00b7nem", "Ba\u00b7bel", "denckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und wer aus furcht schon meynt den abgrund zu ber\u00fchren,", "tokens": ["Und", "wer", "aus", "furcht", "schon", "meynt", "den", "ab\u00b7grund", "zu", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Den kan er an das haupt der guten hoffnung f\u00fchren.", "tokens": ["Den", "kan", "er", "an", "das", "haupt", "der", "gu\u00b7ten", "hoff\u00b7nung", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Hoch-Edle! derer geist itzt nichts als seuffzen kan,", "tokens": ["Hoch\u00b7Ed\u00b7le", "!", "de\u00b7rer", "geist", "itzt", "nichts", "als", "seuff\u00b7zen", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VVFIN", "ADV", "PIS", "KOKOM", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Was unsre feder schreibt von umgekehrten hoffen,", "tokens": ["Was", "uns\u00b7re", "fe\u00b7der", "schreibt", "von", "um\u00b7ge\u00b7kehr\u00b7ten", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Hat auf gewisse ma\u00df bey ihnen eingetroffen,", "tokens": ["Hat", "auf", "ge\u00b7wis\u00b7se", "ma\u00df", "bey", "ih\u00b7nen", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und auf gewisse ma\u00df sich anders dargethan.", "tokens": ["Und", "auf", "ge\u00b7wis\u00b7se", "ma\u00df", "sich", "an\u00b7ders", "dar\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Jhr gutes hoffen hat theils ihnen fehl geschlagen;", "tokens": ["Ihr", "gu\u00b7tes", "hof\u00b7fen", "hat", "theils", "ih\u00b7nen", "fehl", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VAFIN", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Theils aber kan der mund auch von erf\u00fcllung sagen.", "tokens": ["Theils", "a\u00b7ber", "kan", "der", "mund", "auch", "von", "er\u00b7f\u00fcl\u00b7lung", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Es war die meynung ja der nun entseelten pein:", "tokens": ["Es", "war", "die", "mey\u00b7nung", "ja", "der", "nun", "ent\u00b7seel\u00b7ten", "pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Der die ergrimmte hand des todes hat gef\u00e4llet,", "tokens": ["Der", "die", "er\u00b7grimm\u00b7te", "hand", "des", "to\u00b7des", "hat", "ge\u00b7f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und ihn der langen reyh der todten zugesellet,", "tokens": ["Und", "ihn", "der", "lan\u00b7gen", "reyh", "der", "tod\u00b7ten", "zu\u00b7ge\u00b7sel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Der solte mit der zeit den ahnen gleiche seyn,", "tokens": ["Der", "sol\u00b7te", "mit", "der", "zeit", "den", "ah\u00b7nen", "glei\u00b7che", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "ART", "ADJA", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und hier durch tugenden des hauses ruhm erh\u00f6hen,", "tokens": ["Und", "hier", "durch", "tu\u00b7gen\u00b7den", "des", "hau\u00b7ses", "ruhm", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und sonderlich den weg des theuren vaters g