{"textgrid.poem.54046": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der Stimmungss\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du nicht das Lieb\u00f6slied,", "tokens": ["H\u00f6rst", "du", "nicht", "das", "Lie\u00b7b\u00f6s\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wie es leis die Nacht durchzieht?", "tokens": ["wie", "es", "leis", "die", "Nacht", "durch\u00b7zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwei Herzen, die scherzen, ahnen oft nicht,", "tokens": ["Zwei", "Her\u00b7zen", ",", "die", "scher\u00b7zen", ",", "ah\u00b7nen", "oft", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "VVINF", "$,", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "da\u00df pl\u00f6tzlich beim Scherz\u00fcn entflammen die Herz\u00fcn . . .", "tokens": ["da\u00df", "pl\u00f6tz\u00b7lich", "beim", "Scher\u00b7z\u00fcn", "ent\u00b7flam\u00b7men", "die", "Her\u00b7z\u00fcn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+--+--+--++", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Du bist etwas ganz Wunderbars:", "tokens": ["Du", "bist", "et\u00b7was", "ganz", "Wun\u00b7der\u00b7bars", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich hab dich so gerne \u2013", "tokens": ["Ich", "hab", "dich", "so", "ger\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Bist mir Sonne, Mond und Sterne,", "tokens": ["Bist", "mir", "Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "meine Venus du und mein Mars . . .", "tokens": ["mei\u00b7ne", "Ve\u00b7nus", "du", "und", "mein", "Mars", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Sag mir noch einmal J\u00f6\u00e4, du sch\u00f6ne Frau!", "tokens": ["Sag", "mir", "noch", "ein\u00b7mal", "J\u00f6\u00e4", ",", "du", "sch\u00f6\u00b7ne", "Frau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sag mir noch einmal J\u00f6\u00e4!", "tokens": ["Sag", "mir", "noch", "ein\u00b7mal", "J\u00f6\u00e4", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "NE", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.11": {"text": "Ich wei\u00df genau:", "tokens": ["Ich", "wei\u00df", "ge\u00b7nau", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ich dr\u00fcck dich an mein Herriz \u2013 die Nacht und ich sind blau", "tokens": ["Ich", "dr\u00fcck", "dich", "an", "mein", "Her\u00b7riz", "\u2013", "die", "Nacht", "und", "ich", "sind", "blau"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$(", "ART", "NN", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "am Busen einer sch\u00f6nen Frau \u2013!", "tokens": ["am", "Bu\u00b7sen", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Frau", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich denke an dich sp\u00e4t und fr\u00fch,", "tokens": ["Ich", "den\u00b7ke", "an", "dich", "sp\u00e4t", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du meine Madamm Butterfl\u00fch!", "tokens": ["du", "mei\u00b7ne", "Ma\u00b7damm", "But\u00b7ter\u00b7fl\u00fch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir \u2013 da m\u00f6cht ich einmal sein", "tokens": ["Mit", "dir", "\u2013", "da", "m\u00f6cht", "ich", "ein\u00b7mal", "sein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$(", "ADV", "VMFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in R\u00fcdesheim am Rhein beim Wein!", "tokens": ["in", "R\u00fc\u00b7des\u00b7heim", "am", "Rhein", "beim", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du tr\u00e4gst ein Kind unterm Herzelein . . .", "tokens": ["Du", "tr\u00e4gst", "ein", "Kind", "un\u00b7term", "Her\u00b7ze\u00b7lein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ja, wer mag der Vater nur sein \u2013?", "tokens": ["Ja", ",", "wer", "mag", "der", "Va\u00b7ter", "nur", "sein", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PWS", "VMFIN", "ART", "NN", "ADV", "VAINF", "$(", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Drei Musketiere! Drei Kavaliere!", "tokens": ["Drei", "Mus\u00b7ke\u00b7tie\u00b7re", "!", "Drei", "Ka\u00b7va\u00b7lie\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "CARD", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00fcr die Freiheit stehen sie ein!", "tokens": ["F\u00fcr", "die", "Frei\u00b7heit", "ste\u00b7hen", "sie", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Zieht blank, Musketiere, und steckt den Degen ein", "tokens": ["Zieht", "blank", ",", "Mus\u00b7ke\u00b7tie\u00b7re", ",", "und", "steckt", "den", "De\u00b7gen", "ein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ART"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "mang die Freiheit, die Frauen und der Wein!", "tokens": ["mang", "die", "Frei\u00b7heit", ",", "die", "Frau\u00b7en", "und", "der", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich schenk dir K\u00fcsse ohne Zahl.", "tokens": ["Ich", "schenk", "dir", "K\u00fcs\u00b7se", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind hier ein durchaus feines Lokal.", "tokens": ["Wir", "sind", "hier", "ein", "durc\u00b7haus", "fei\u00b7nes", "Lo\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vor Monokeln liegen wir auf dem Bauch;", "tokens": ["Vor", "Mo\u00b7no\u00b7keln", "lie\u00b7gen", "wir", "auf", "dem", "Bauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "kommt der Kronprinz einmal, vor dem Kronprinzen auch!", "tokens": ["kommt", "der", "Kron\u00b7prinz", "ein\u00b7mal", ",", "vor", "dem", "Kron\u00b7prin\u00b7zen", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Z\u00e4hl nicht jede Flasche, die der Kellner dir nimmt", "tokens": ["Z\u00e4hl", "nicht", "je\u00b7de", "Fla\u00b7sche", ",", "die", "der", "Kell\u00b7ner", "dir", "nimmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "halbgef\u00fcllt . . .", "tokens": ["halb\u00b7ge\u00b7f\u00fcllt", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["VVFIN", "$.", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Du s\u00fc\u00dfes Engelsbild!", "tokens": ["Du", "s\u00fc\u00b7\u00dfes", "En\u00b7gels\u00b7bild", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bald ist der Moi", "tokens": ["Bald", "ist", "der", "Moi"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "vorbeu!", "tokens": ["vor\u00b7beu", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "++", "measure": "spondeus"}, "line.10": {"text": "Bleib mir treu!", "tokens": ["Bleib", "mir", "treu", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Und kaufe ihr einen Veilchenstrau\u00df,", "tokens": ["Und", "kau\u00b7fe", "ihr", "ei\u00b7nen", "Veil\u00b7chen\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "sonst fliegt die Person aus dem Ausschank hier raus!", "tokens": ["sonst", "fliegt", "die", "Per\u00b7son", "aus", "dem", "Aus\u00b7schank", "hier", "raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Zonny boy!", "tokens": ["Zon\u00b7ny", "boy", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.14": {"text": "Und kaufe ihr einen T\u00e4ddy-B\u00e4ren,", "tokens": ["Und", "kau\u00b7fe", "ihr", "ei\u00b7nen", "T\u00e4d\u00b7dy\u00b7B\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "sonst darf die Nutte hier nicht mehr verkehren!", "tokens": ["sonst", "darf", "die", "Nut\u00b7te", "hier", "nicht", "mehr", "ver\u00b7keh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Zonny boy . . .", "tokens": ["Zon\u00b7ny", "boy", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "$.", "$.", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.17": {"text": "Die deutsche Frau sei dem Manne geheiligt \u2013", "tokens": ["Die", "deut\u00b7sche", "Frau", "sei", "dem", "Man\u00b7ne", "ge\u00b7hei\u00b7ligt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "auch ist sie an den Getr\u00e4nken beteiligt . . .", "tokens": ["auch", "ist", "sie", "an", "den", "Ge\u00b7tr\u00e4n\u00b7ken", "be\u00b7tei\u00b7ligt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$.", "$.", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Keine Inkorrektheit, die uns entwischt!", "tokens": ["Kei\u00b7ne", "In\u00b7kor\u00b7rekt\u00b7heit", ",", "die", "uns", "ent\u00b7wischt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Zwischen servil und frech gibt es hier nischt.", "tokens": ["Zwi\u00b7schen", "ser\u00b7vil", "und", "frech", "gibt", "es", "hier", "nischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.21": {"text": "Hier herrscht Ordnung!", "tokens": ["Hier", "herrscht", "Ord\u00b7nung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Dort seh ich die Lo \u2013 die s\u00fc\u00dfe Kokwette \u2013", "tokens": ["Dort", "seh", "ich", "die", "Lo", "\u2013", "die", "s\u00fc\u00b7\u00dfe", "Kok\u00b7wet\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sie kommt grade von der Damentoilette!", "tokens": ["sie", "kommt", "gra\u00b7de", "von", "der", "Da\u00b7men\u00b7to\u00b7i\u00b7let\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "O sieh doch nur, wie der Mondenschein strahlt!", "tokens": ["O", "sieh", "doch", "nur", ",", "wie", "der", "Mon\u00b7den\u00b7schein", "strahlt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dabei hat das Luder nicht mal bezahlt!", "tokens": ["Da\u00b7bei", "hat", "das", "Lu\u00b7der", "nicht", "mal", "be\u00b7zahlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sie macht ein unschuldvoll Gesicht", "tokens": ["Sie", "macht", "ein", "un\u00b7schuld\u00b7voll", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und denkt, die Toilettenfrau merkt es nicht . . .", "tokens": ["und", "denkt", ",", "die", "To\u00b7i\u00b7let\u00b7ten\u00b7frau", "merkt", "es", "nicht", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$.", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Doch es gibt eine Frau, die dich niemals vergi\u00dft", "tokens": ["Doch", "es", "gibt", "ei\u00b7ne", "Frau", ",", "die", "dich", "nie\u00b7mals", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "l