: "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unaussprechliches Vergn\u00fcgen,", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da du nun die Meine bist!", "tokens": ["Da", "du", "nun", "die", "Mei\u00b7ne", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "B\u00f6t' ein K\u00f6nig seine Krone", "tokens": ["B\u00f6t'", "ein", "K\u00f6\u00b7nig", "sei\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir statt deiner Liebe an;", "tokens": ["Mir", "statt", "dei\u00b7ner", "Lie\u00b7be", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4hlt' ich dich statt seinem Throne,", "tokens": ["W\u00e4hlt'", "ich", "dich", "statt", "sei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der nicht so begl\u00fccken kann.", "tokens": ["Der", "nicht", "so", "be\u00b7gl\u00fc\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}