{"dta.poem.17328": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "178.  Was die Creatur gegen GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist ein St\u00e4ubelein in anschauung der Welt?", "tokens": ["Was", "ist", "ein", "St\u00e4u\u00b7be\u00b7lein", "in", "an\u00b7schau\u00b7ung", "der", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und was bin ich/ wenn man Gott gegen dir mich h\u00e4lt?", "tokens": ["Und", "was", "bin", "ich", "/", "wenn", "man", "Gott", "ge\u00b7gen", "dir", "mich", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "$(", "KOUS", "PIS", "NN", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}