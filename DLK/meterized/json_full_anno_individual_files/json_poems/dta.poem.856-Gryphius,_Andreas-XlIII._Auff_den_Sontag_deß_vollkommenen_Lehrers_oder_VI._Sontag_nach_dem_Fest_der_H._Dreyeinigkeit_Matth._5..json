{"dta.poem.856": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.  Auff den Sontag de\u00df vollkommenen  \n Lehrers/ oder  VI.  Sontag nach dem Fest der  \n H. Dreyeinigkeit/ Matth. 5.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Falscher Heuchelschein/ dein Phariseer leben ", "tokens": ["Dein", "Fal\u00b7scher", "Heu\u00b7chel\u00b7schein", "/", "dein", "Pha\u00b7ri\u00b7seer", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schleu\u00dft nimmermehr die Burg de\u00df grossen ", "tokens": ["Schleu\u00dft", "nim\u00b7mer\u00b7mehr", "die", "Burg", "de\u00df", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mels auff/ ", "tokens": ["mels", "auff", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "APPR", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.4": {"text": "Wer hei\u00df entbranten zorn nicht bricht dem tollen lauff/", "tokens": ["Wer", "hei\u00df", "ent\u00b7bran\u00b7ten", "zorn", "nicht", "bricht", "dem", "tol\u00b7len", "lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJA", "NN", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gleicht allen/ die zum Mord die grimme Faust erheben.", "tokens": ["Gleicht", "al\u00b7len", "/", "die", "zum", "Mord", "die", "grim\u00b7me", "Faust", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$(", "ART", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer seinem Bruder flucht/ kracht wie verdorrte Reben", "tokens": ["Wer", "sei\u00b7nem", "Bru\u00b7der", "flucht", "/", "kracht", "wie", "ver\u00b7dorr\u00b7te", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In letzter Schwefel glutt/ der hertz-verstockte hauff/", "tokens": ["In", "letz\u00b7ter", "Schwe\u00b7fel", "glutt", "/", "der", "hertz\u00b7ver\u00b7stock\u00b7te", "hauff", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ha\u00df f\u00fcr Freundschafft libt/ thut al-zu thewren kauff", "tokens": ["Der", "ha\u00df", "f\u00fcr", "Freund\u00b7schafft", "libt", "/", "thut", "al\u00b7zu", "thew\u00b7ren", "kauff"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$(", "VVFIN", "PTKA", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd zwing\u2019t Gott/ da\u00df Er mu\u00df/ jhm Recht f\u00fcr Gnade geben", "tokens": ["Vnd", "zwing't", "Gott", "/", "da\u00df", "Er", "mu\u00df", "/", "jhm", "Recht", "f\u00fcr", "Gna\u00b7de", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$(", "KOUS", "PPER", "VMFIN", "$(", "PPER", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O libster Mensch verzeih/ eh\u2019 al\u00df du auff der Bahr", "tokens": ["O", "libs\u00b7ter", "Mensch", "ver\u00b7zeih", "/", "eh'", "al\u00df", "du", "auff", "der", "Bahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$(", "KOUS", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ehr dein Schiff in den Portt de\u00df schnellen Todes fahr:", "tokens": ["Ehr", "dein", "Schiff", "in", "den", "Portt", "de\u00df", "schnel\u00b7len", "To\u00b7des", "fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da\u00df nicht der Feinde klag des Richters zorn errege/", "tokens": ["Da\u00df", "nicht", "der", "Fein\u00b7de", "klag", "des", "Rich\u00b7ters", "zorn", "er\u00b7re\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VVFIN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Gottes Vrtheil Spruch dem Kercker zu erkennt", "tokens": ["Die", "Got\u00b7tes", "Vrtheil", "Spruch", "dem", "Ker\u00b7cker", "zu", "er\u00b7kennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN", "ART", "NN", "PTKZU", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die plagt der Straffen last/ die vnau\u00dfsprechlich brennt", "tokens": ["Die", "plagt", "der", "Straf\u00b7fen", "last", "/", "die", "vn\u00b7au\u00df\u00b7sprech\u00b7lich", "brennt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$(", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df da\u00df man auch (O wann!) den letzten scherff ablege.", "tokens": ["Bi\u00df", "da\u00df", "man", "auch", "(", "O", "wann", "!", ")", "den", "letz\u00b7ten", "scherff", "ab\u00b7le\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "ADV", "$(", "NE", "PWAV", "$.", "$(", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}}}}