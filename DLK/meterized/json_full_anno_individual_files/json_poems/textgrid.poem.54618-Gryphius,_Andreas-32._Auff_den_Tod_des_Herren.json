{"textgrid.poem.54618": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "32. Auff den Tod des Herren", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen wundert da\u00df man nicht Tag in dem Mittag findet/", "tokens": ["Wen", "wun\u00b7dert", "da\u00df", "man", "nicht", "Tag", "in", "dem", "Mit\u00b7tag", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "PIS", "PTKNEG", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dem das wahre Licht in Todes Angst verschwindet?", "tokens": ["In", "dem", "das", "wah\u00b7re", "Licht", "in", "To\u00b7des", "Angst", "ver\u00b7schwin\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wen wundert da\u00df die Erd erzittert und verf\u00e4lt/", "tokens": ["Wen", "wun\u00b7dert", "da\u00df", "die", "Erd", "er\u00b7zit\u00b7tert", "und", "ver\u00b7f\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "ART", "NN", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil den/ der alles h\u00e4lt/ das Creutz genagelt h\u00e4lt?", "tokens": ["Weil", "den", "/", "der", "al\u00b7les", "h\u00e4lt", "/", "das", "Creutz", "ge\u00b7na\u00b7gelt", "h\u00e4lt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "ART", "PIS", "