{"dta.poem.10627": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Auff Herrn Sebastian Nam\u00dflers Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "So offt ich bey mir selbst/ (wie ich zuthun dann pflege/)", "tokens": ["So", "offt", "ich", "bey", "mir", "selbst", "/", "(", "wie", "ich", "zu\u00b7thun", "dann", "pfle\u00b7ge", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "PPER", "ADV", "$(", "$(", "PWAV", "PPER", "VVFIN", "ADV", "VVFIN", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebe lauff vnd art mit allem flei\u00df erwege/", "tokens": ["Der", "Lie\u00b7be", "lauff", "vnd", "art", "mit", "al\u00b7lem", "flei\u00df", "er\u00b7we\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "APPR", "PIS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Befind ich allezeit/ da\u00df jhre Tyranney/", "tokens": ["Be\u00b7find", "ich", "al\u00b7le\u00b7zeit", "/", "da\u00df", "jhre", "Ty\u00b7ran\u00b7ney", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$(", "KOUS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Nur sey ein blosser Wahn/ vnd blinde Fantasey.", "tokens": ["Nur", "sey", "ein", "blos\u00b7ser", "Wahn", "/", "vnd", "blin\u00b7de", "Fan\u00b7ta\u00b7sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Venus ist die Zier der freundlichen Jungfrawen/", "tokens": ["Die", "Ve\u00b7nus", "ist", "die", "Zier", "der", "freund\u00b7li\u00b7chen", "Jung\u00b7fra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Damit sie prangen thun auff art der stoltzen Pfawen/", "tokens": ["Da\u00b7mit", "sie", "pran\u00b7gen", "thun", "auff", "art", "der", "stolt\u00b7zen", "Pfa\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVINF", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Damit sie vnser Hertz vnd Sinnen nehmen ein/", "tokens": ["Da\u00b7mit", "sie", "vn\u00b7ser", "Hertz", "vnd", "Sin\u00b7nen", "neh\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Darau\u00df entspringen mu\u00df die bitter s\u00fcsse Pein.", "tokens": ["Dar\u00b7au\u00df", "ent\u00b7sprin\u00b7gen", "mu\u00df", "die", "bit\u00b7ter", "s\u00fcs\u00b7se", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie brechen sich herau\u00df: Ihr trachten vnd jhr sinnen/", "tokens": ["Sie", "bre\u00b7chen", "sich", "her\u00b7au\u00df", ":", "Ihr", "trach\u00b7ten", "vnd", "jhr", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$.", "PPER", "VVINF", "KON", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist eintzig vnd allein/ wie sie vns fangen k\u00fcnnen/", "tokens": ["Ist", "eint\u00b7zig", "vnd", "al\u00b7lein", "/", "wie", "sie", "vns", "fan\u00b7gen", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "$(", "PWAV", "PPER", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Kopff vor Schmuck sich beugt/ die Stirne glantzt herf\u00fcr/", "tokens": ["Der", "Kopff", "vor", "Schmuck", "sich", "beugt", "/", "die", "Stir\u00b7ne", "glantzt", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PRF", "VVFIN", "$(", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Wein ist reiff vnd feil/ der Krantz hengt vor der Th\u00fcr/", "tokens": ["Der", "Wein", "ist", "reiff", "vnd", "feil", "/", "der", "Krantz", "hengt", "vor", "der", "Th\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "NN", "$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch kan vns sonderlich der Augenglantz versehren/", "tokens": ["Doch", "kan", "vns", "son\u00b7der\u00b7lich", "der", "Au\u00b7gen\u00b7glantz", "ver\u00b7seh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das wundersch\u00f6ne Liecht thut allesampt beth\u00f6ren/", "tokens": ["Das", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Liecht", "thut", "al\u00b7le\u00b7sampt", "be\u00b7th\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Augen sein die Stra\u00df/ die Augen sein der Steg/", "tokens": ["Die", "Au\u00b7gen", "sein", "die", "Stra\u00df", "/", "die", "Au\u00b7gen", "sein", "der", "Steg", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ART", "NN", "$(", "ART", "NN", "PPOSAT", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dadurch in vnser Hertz Cupido trifft den Weg.", "tokens": ["Da\u00b7durch", "in", "vn\u00b7ser", "Hertz", "Cu\u00b7pi\u00b7do", "trifft", "den", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gantz sch\u00f6n vnd meisterlich die arge Damen wissen", "tokens": ["Gantz", "sch\u00f6n", "vnd", "meis\u00b7ter\u00b7lich", "die", "ar\u00b7ge", "Da\u00b7men", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die runden K\u00fcgelein jetz hin jetz her zuschissen/", "tokens": ["Die", "run\u00b7den", "K\u00fc\u00b7ge\u00b7lein", "jetz", "hin", "jetz", "her", "zu\u00b7schis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "ADV", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bald trucken sie sie zu/ bald werffen sie sie auff/", "tokens": ["Bald", "tru\u00b7cken", "sie", "sie", "zu", "/", "bald", "werf\u00b7fen", "sie", "sie", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "$(", "ADV", "VVFIN", "PPER", "PPER", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bi\u00df endtlich wir gemacht den thewren Liebeskauff.", "tokens": ["Bi\u00df", "endt\u00b7lich", "wir", "ge\u00b7macht", "den", "thew\u00b7ren", "Lie\u00b7bes\u00b7kauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da haben wir sie denn/ da\u00df vnser standthafft Hertze/", "tokens": ["Da", "ha\u00b7ben", "wir", "sie", "denn", "/", "da\u00df", "vn\u00b7ser", "standt\u00b7hafft", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "$(", "KOUS", "PPER", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Kunst/ Wei\u00dfheit/ Lob vnd Ehr/ mu\u00df weichen solchem schertze/", "tokens": ["Kunst", "/", "Wei\u00df\u00b7heit", "/", "Lob", "vnd", "Ehr", "/", "mu\u00df", "wei\u00b7chen", "sol\u00b7chem", "schert\u00b7ze", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "VMFIN", "VVINF", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das Griechisch vnd Latein wirdt vns gar vnbekandt/", "tokens": ["Das", "Grie\u00b7chisch", "vnd", "La\u00b7tein", "wirdt", "vns", "gar", "vn\u00b7be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "F\u00fcr Plato nemen wir den Amadis zu Handt/", "tokens": ["F\u00fcr", "Pla\u00b7to", "ne\u00b7men", "wir", "den", "A\u00b7ma\u00b7dis", "zu", "Handt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ART", "NE", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das kan ein Weibesbildt: Bald will sich der erhencken/", "tokens": ["Das", "kan", "ein", "Wei\u00b7bes\u00b7bildt", ":", "Bald", "will", "sich", "der", "er\u00b7hen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$.", "ADV", "VMFIN", "PRF", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Vor vnerh\u00f6rter Brunst/ bald will sich der ertrencken/", "tokens": ["Vor", "vner\u00b7h\u00f6r\u00b7ter", "Brunst", "/", "bald", "will", "sich", "der", "er\u00b7tren\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "VMFIN", "PRF", "ART", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.27": {"text": "Der Thr\u00e4nen weite Bach au\u00df beyden Augen quillt/", "tokens": ["Der", "Thr\u00e4\u00b7nen", "wei\u00b7te", "Bach", "au\u00df", "bey\u00b7den", "Au\u00b7gen", "quillt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Voll seufstzen ist das Hertz: Das kan ein Weibesbildt.", "tokens": ["Voll", "seufst\u00b7zen", "ist", "das", "Hertz", ":", "Das", "kan", "ein", "Wei\u00b7bes\u00b7bildt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "ART", "NN", "$.", "PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "O wie gl\u00fcckseelig ist Herr Breutigam das Leben/", "tokens": ["O", "wie", "gl\u00fcck\u00b7see\u00b7lig", "ist", "Herr", "Breu\u00b7ti\u00b7gam", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "NN", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "In das jhr euch jetzt wolt mit ewrem Lieb begeben/", "tokens": ["In", "das", "jhr", "euch", "jetzt", "wolt", "mit", "ew\u00b7rem", "Lieb", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.31": {"text": "Ihr liebet ohne Forcht/ in wahrer Freundligkeit/", "tokens": ["Ihr", "lie\u00b7bet", "oh\u00b7ne", "Forcht", "/", "in", "wah\u00b7rer", "Freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ihr seyt von Liebes Pein gantz sicher vnd befreyt.", "tokens": ["Ihr", "seyt", "von", "Lie\u00b7bes", "Pein", "gantz", "si\u00b7cher", "vnd", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wir m\u00fcssen mit Gedult an Venuswagen ziehen/", "tokens": ["Wir", "m\u00fcs\u00b7sen", "mit", "Ge\u00b7dult", "an", "Ve\u00b7nus\u00b7wa\u00b7gen", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vnd vns bey Tag vnd Nacht mit jhrem Joch bem\u00fchen/", "tokens": ["Vnd", "vns", "bey", "Tag", "vnd", "Nacht", "mit", "jhrem", "Joch", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Wir sehen Angst vnd Noth/ jhr sehet H\u00fclff vnd Rath;", "tokens": ["Wir", "se\u00b7hen", "Angst", "vnd", "Noth", "/", "jhr", "se\u00b7het", "H\u00fclff", "vnd", "Rath", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.36": {"text": "Was bey vns Hoffnung ist/ das wirdt bey