{"textgrid.poem.25023": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf schwankem Kahn ins Ungewisse", "tokens": ["Auf", "schwan\u00b7kem", "Kahn", "ins", "Un\u00b7ge\u00b7wis\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Irrt' ich durchs wildemp\u00f6rte Meer,", "tokens": ["Irrt'", "ich", "durchs", "wil\u00b7dem\u00b7p\u00f6r\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da glomm durch Wetterwolkenrisse", "tokens": ["Da", "glomm", "durch", "Wet\u00b7ter\u00b7wol\u00b7ken\u00b7ris\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein blauer Schein von oben her.", "tokens": ["Ein", "blau\u00b7er", "Schein", "von", "o\u00b7ben", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nach und nach zerrann in hellen", "tokens": ["Und", "nach", "und", "nach", "zer\u00b7rann", "in", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "KON", "APPR", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lichtglanz das Dunkel \u00fcber mir;", "tokens": ["Licht\u00b7glanz", "das", "Dun\u00b7kel", "\u00fc\u00b7ber", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ans Ufer trugen mich die Wellen", "tokens": ["Ans", "U\u00b7fer", "tru\u00b7gen", "mich", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In leisem Windeshauch zu dir.", "tokens": ["In", "lei\u00b7sem", "Win\u00b7des\u00b7hauch", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mag deiner Augen sel'ger Himmel,", "tokens": ["Mag", "dei\u00b7ner", "Au\u00b7gen", "sel'\u00b7ger", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der rettend mich dem Sturmesgraun", "tokens": ["Der", "ret\u00b7tend", "mich", "dem", "Stur\u00b7mes\u00b7graun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entrissen hat, dem Weltget\u00fcmmel,", "tokens": ["Ent\u00b7ris\u00b7sen", "hat", ",", "dem", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun ewig mir zu H\u00e4upten blaun!", "tokens": ["Nun", "e\u00b7wig", "mir", "zu", "H\u00e4up\u00b7ten", "blaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.