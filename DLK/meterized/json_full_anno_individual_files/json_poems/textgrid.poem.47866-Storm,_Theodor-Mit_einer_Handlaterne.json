{"textgrid.poem.47866": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Mit einer Handlaterne", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laterne, Laterne!", "tokens": ["La\u00b7ter\u00b7ne", ",", "La\u00b7ter\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sonne, Mond und Sterne,", "tokens": ["Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die doch sonst am Himmel stehn,", "tokens": ["Die", "doch", "sonst", "am", "Him\u00b7mel", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lassen heut sich nimmer sehn;", "tokens": ["Las\u00b7sen", "heut", "sich", "nim\u00b7mer", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen Wasserreih und Schlo\u00df", "tokens": ["Zwi\u00b7schen", "Was\u00b7ser\u00b7reih", "und", "Schlo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist die Finsternis so gro\u00df,", "tokens": ["Ist", "die", "Fins\u00b7ter\u00b7nis", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {