], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "Kein Herz gesch\u00fctzt:", "tokens": ["Kein", "Herz", "ge\u00b7sch\u00fctzt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Warum denn itzt? \u2013", "tokens": ["Wa\u00b7rum", "denn", "itzt", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "Vor dem Vergessen", "tokens": ["Vor", "dem", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Sch\u00fctzt kein Portrait", "tokens": ["Sch\u00fctzt", "kein", "Por\u00b7trait"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "Den armen Gauch,", "tokens": ["Den", "ar\u00b7men", "Gauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.50": {"text": "Drei Tag', und h\u00e4tt'", "tokens": ["Drei", "Tag'", ",", "und", "h\u00e4tt'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "$,", "KON", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.51": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.52": {"text": "Dazu gesessen.", "tokens": ["Da\u00b7zu", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.53": {"text": "Mein Schattenri\u00df", "tokens": ["Mein", "Schat\u00b7ten\u00b7ri\u00df"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Mit meinem Herzen,", "to