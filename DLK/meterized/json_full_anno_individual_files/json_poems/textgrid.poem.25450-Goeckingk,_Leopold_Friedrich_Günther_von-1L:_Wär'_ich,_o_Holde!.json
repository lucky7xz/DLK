Wei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "So wundersch\u00f6n,", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Wie zum Exempel", "tokens": ["Wie", "zum", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "In Sanssoucis,", "tokens": ["In", "Sans\u00b7sou\u00b7cis", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Auch einen Tempel", "tokens": ["Auch", "ei\u00b7nen", "Tem\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Der Freundschaft baun.", "tokens": ["Der", "Freund\u00b7schaft", "baun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Allein, Sophie!", "tokens": ["Al\u00b7lein", ",", "So\u00b7phie", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Wenn deine M\u00fche", "tokens": ["Wenn", "dei\u00b7ne", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Gleich zwanzig Beutel", "tokens": ["Gleich", "zwan\u00b7zig", "Beu\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "F\u00fcr mich noch strickt:", "tokens": ["F\u00fcr", "mich", "noch", "strickt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["A