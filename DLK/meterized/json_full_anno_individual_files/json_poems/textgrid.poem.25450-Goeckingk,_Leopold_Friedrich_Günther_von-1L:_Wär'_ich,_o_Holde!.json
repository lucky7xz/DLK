{"textgrid.poem.25450": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r' ich, o Holde!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich, o Holde!", "tokens": ["W\u00e4r'", "ich", ",", "o", "Hol\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "So reich an Golde,", "tokens": ["So", "reich", "an", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Als ich an Reimen,", "tokens": ["Als", "ich", "an", "Rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "An Morgentr\u00e4umen", "tokens": ["An", "Mor\u00b7gen\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und Possen bin:", "tokens": ["Und", "Pos\u00b7sen", "bin", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mich k\u00f6nnte Keiner", "tokens": ["Mich", "k\u00f6nn\u00b7te", "Kei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Nach meinem Sinn',", "tokens": ["Nach", "mei\u00b7nem", "Sinn'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Den unser Einer", "tokens": ["Den", "un\u00b7ser", "Ei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Nicht kann bezahlen.", "tokens": ["Nicht", "kann", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "K\u00f6nnt' ich das Gold,", "tokens": ["K\u00f6nnt'", "ich", "das", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Das Ungarn zollt,", "tokens": ["Das", "Un\u00b7garn", "zollt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Im Berge, wie", "tokens": ["Im", "Ber\u00b7ge", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Minervens Eule", "tokens": ["Mi\u00b7ner\u00b7vens", "Eu\u00b7le"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Die M\u00e4use, sehn:", "tokens": ["Die", "M\u00e4u\u00b7se", ",", "sehn", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ich lie\u00dfe traun!", "tokens": ["Ich", "lie\u00b7\u00dfe", "traun", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "In kurzer Weile", "tokens": ["In", "kur\u00b7zer", "Wei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "So wundersch\u00f6n,", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Wie zum Exempel", "tokens": ["Wie", "zum", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "In Sanssoucis,", "tokens": ["In", "Sans\u00b7sou\u00b7cis", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Auch einen Tempel", "tokens": ["Auch", "ei\u00b7nen", "Tem\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Der Freundschaft baun.", "tokens": ["Der", "Freund\u00b7schaft", "baun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Allein, Sophie!", "tokens": ["Al\u00b7lein", ",", "So\u00b7phie", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Wenn deine M\u00fche", "tokens": ["Wenn", "dei\u00b7ne", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Gleich zwanzig Beutel", "tokens": ["Gleich", "zwan\u00b7zig", "Beu\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "F\u00fcr mich noch strickt:", "tokens": ["F\u00fcr", "mich", "noch", "strickt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.27": {"text": "Die M\u00fch' ist eitel!", "tokens": ["Die", "M\u00fch'", "ist", "ei\u00b7tel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Fortuna spickt", "tokens": ["For\u00b7tu\u00b7na", "spickt"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Von allen keinen,", "tokens": ["Von", "al\u00b7len", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Wie an dem Einen", "tokens": ["Wie", "an", "dem", "Ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Man schon erblickt.", "tokens": ["Man", "schon", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Das Herz nur dr\u00fcckt,", "tokens": ["Das", "Herz", "nur", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Zum Gl\u00fcck', den Stempel", "tokens": ["Zum", "Gl\u00fcck", ",", "den", "Stem\u00b7pel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Der Freundschaft auf.", "tokens": ["Der", "Freund\u00b7schaft", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.35": {"text": "Was kommt darauf", "tokens": ["Was", "kommt", "da\u00b7rauf"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Am End' auch an,", "tokens": ["Am", "End'", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Ob tausend Mann", "tokens": ["Ob", "tau\u00b7send", "Mann"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "CARD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "An einem Tempel", "tokens": ["An", "ei\u00b7nem", "Tem\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Der Freundschaft, baun,", "tokens": ["Der", "Freund\u00b7schaft", ",", "baun", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.40": {"text": "Und, ihn zu schm\u00fccken,", "tokens": ["Und", ",", "ihn", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "An Meisterst\u00fccken", "tokens": ["An", "Meis\u00b7ter\u00b7st\u00fc\u00b7cken"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Zehn ", "tokens": ["Zehn"], "token_info": ["word"], "pos": ["CARD"], "meter": "-", "measure": "single.down"}, "line.43": {"text": "Hat bei den Alten", "tokens": ["Hat", "bei", "den", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Die\u00df vor Erkalten", "tokens": ["Die\u00df", "vor", "Er\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "Kein Herz gesch\u00fctzt:", "tokens": ["Kein", "Herz", "ge\u00b7sch\u00fctzt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Warum denn itzt? \u2013", "tokens": ["Wa\u00b7rum", "denn", "itzt", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "Vor dem Vergessen", "tokens": ["Vor", "dem", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Sch\u00fctzt kein Portrait", "tokens": ["Sch\u00fctzt", "kein", "Por\u00b7trait"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "Den armen Gauch,", "tokens": ["Den", "ar\u00b7men", "Gauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.50": {"text": "Drei Tag', und h\u00e4tt'", "tokens": ["Drei", "Tag'", ",", "und", "h\u00e4tt'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "$,", "KON", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.51": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.52": {"text": "Dazu gesessen.", "tokens": ["Da\u00b7zu", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.53": {"text": "Mein Schattenri\u00df", "tokens": ["Mein", "Schat\u00b7ten\u00b7ri\u00df"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Mit meinem Herzen,", "tokens": ["Mit", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.55": {"text": "L\u00e4\u00dft dich gewi\u00df", "tokens": ["L\u00e4\u00dft", "dich", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.56": {"text": "Das Bild verschmerzen;", "tokens": ["Das", "Bild", "ver\u00b7schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.57": {"text": "Und mehr, als die\u00df,", "tokens": ["Und", "mehr", ",", "als", "die\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PDS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.58": {"text": "Vermag im Leben", "tokens": ["Ver\u00b7mag", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.59": {"text": "Ich nichts zu geben.", "tokens": ["Ich", "nichts", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.60": {"text": "Doch ganz gewi\u00df", "tokens": ["Doch", "ganz", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.61": {"text": "Reicht beides hin,", "tokens": ["Reicht", "bei\u00b7des", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.62": {"text": "Dein Angedenken", "tokens": ["Dein", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.63": {"text": "Mir, S\u00e4ngerin!", "tokens": ["Mir", ",", "S\u00e4n\u