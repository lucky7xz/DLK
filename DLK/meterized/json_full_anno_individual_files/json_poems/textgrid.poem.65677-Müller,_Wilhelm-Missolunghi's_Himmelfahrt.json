{"textgrid.poem.65677": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Missolunghi's Himmelfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Missolunghi, ", "tokens": ["Mis\u00b7so\u00b7lung\u00b7hi", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bist in donnerndem Triumphe auf der Blitze Flammenlicht", "tokens": ["Bist", "in", "don\u00b7nern\u00b7dem", "Tri\u00b7um\u00b7phe", "auf", "der", "Blit\u00b7ze", "Flam\u00b7men\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "In den Himmel aufgeflogen, Stein und Erde, Thurm und Wall,", "tokens": ["In", "den", "Him\u00b7mel", "auf\u00b7ge\u00b7flo\u00b7gen", ",", "Stein", "und", "Er\u00b7de", ",", "Thurm", "und", "Wall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Siegeswaffen, Heldenglieder, Alles auf in ", "tokens": ["Sie\u00b7ges\u00b7waf\u00b7fen", ",", "Hel\u00b7den\u00b7glie\u00b7der", ",", "Al\u00b7les", "auf", "in"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PIS", "APPR", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Auch die Leichen, die du bargest in dem schwarzen Schoos der Gruft,", "tokens": ["Auch", "die", "Lei\u00b7chen", ",", "die", "du", "bar\u00b7gest", "in", "dem", "schwar\u00b7zen", "Schoos", "der", "Gruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Hast sie mit hinauf getragen in des \u00c4thers freie Luft,", "tokens": ["Hast", "sie", "mit", "hin\u00b7auf", "ge\u00b7tra\u00b7gen", "in", "des", "\u00c4\u00b7thers", "frei\u00b7e", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADV", "VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wo die Seelen, die in ihnen lebten ihres Lebens Tag,", "tokens": ["Wo", "die", "See\u00b7len", ",", "die", "in", "ih\u00b7nen", "leb\u00b7ten", "ih\u00b7res", "Le\u00b7bens", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Jauchzend wieder sie umfingen, die erl\u00f6sten aus der Schmach.", "tokens": ["Jauch\u00b7zend", "wie\u00b7der", "sie", "um\u00b7fin\u00b7gen", ",", "die", "er\u00b7l\u00f6s\u00b7ten", "aus", "der", "Schmach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPER", "VVINF", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Sieh, und auf der heil'gen St\u00e4tte, wo die Martyrveste stand,", "tokens": ["Sieh", ",", "und", "auf", "der", "heil'\u00b7gen", "St\u00e4t\u00b7te", ",", "wo", "die", "Mar\u00b7tyr\u00b7ves\u00b7te", "stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Liegt ein w\u00fcster Aschenhaufen an dem blutgetr\u00e4nkten Strand.", "tokens": ["Liegt", "ein", "w\u00fcs\u00b7ter", "A\u00b7schen\u00b7hau\u00b7fen", "an", "dem", "blut\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Kommt, ihr hohen Christenh\u00e4upter, die ihr mit dem Schwert der Macht", "tokens": ["Kommt", ",", "ihr", "ho\u00b7hen", "Chris\u00b7ten\u00b7h\u00e4up\u00b7ter", ",", "die", "ihr", "mit", "dem", "Schwert", "der", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Habt von ferne still gestanden und an weisen Rath gedacht,", "tokens": ["Habt", "von", "fer\u00b7ne", "still", "ge\u00b7stan\u00b7den", "und", "an", "wei\u00b7sen", "Rath", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJD", "VVPP", "KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Als die Todesglocken riefen: Helfet uns, so helf' euch Gott!", "tokens": ["Als", "die", "To\u00b7des\u00b7glo\u00b7cken", "rie\u00b7fen", ":", "Hel\u00b7fet", "uns", ",", "so", "hel\u00b7f'", "euch", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.14": {"text": "Als die Heldenherzen brachen in des Hungers grimmer Noth,", "tokens": ["Als", "die", "Hel\u00b7den\u00b7her\u00b7zen", "bra\u00b7chen", "in", "des", "Hun\u00b7gers", "grim\u00b7mer", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Kommt, von dieser Asche sammelt in die Purpurm\u00e4ntel ein,", "tokens": ["Kommt", ",", "von", "die\u00b7ser", "A\u00b7sche", "sam\u00b7melt", "in", "die", "Pur\u00b7pur\u00b7m\u00e4n\u00b7tel", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PDAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Streuet sie auf eure Kronen \u00fcber Gold und Edelstein,", "tokens": ["Streu\u00b7et", "sie", "auf", "eu\u00b7re", "Kro\u00b7nen", "\u00fc\u00b7ber", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Und so tretet vor den Richter, der des Himmels Wage h\u00e4lt,", "tokens": ["Und", "so", "tre\u00b7tet", "vor", "den", "Rich\u00b7ter", ",", "der", "des", "Him\u00b7mels", "Wa\u00b7ge", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Wann er euch dereinst wird rufen von den Thronen seiner Welt.", "tokens": ["Wann", "er", "euch", "de\u00b7reinst", "wird", "ru\u00b7fen", "von", "den", "Thro\u00b7nen", "sei\u00b7ner", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VAFIN", "VVINF", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "An dem Tage wird er fragen: Helfer ihr, mit meinem Schwert,", "tokens": ["An", "dem", "Ta\u00b7ge", "wird", "er", "fra\u00b7gen", ":", "Hel\u00b7fer", "ihr", ",", "mit", "mei\u00b7nem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$.", "NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Warum habt ihr nicht geholfen, warum habt ihr nicht gewehrt,", "tokens": ["Wa\u00b7rum", "habt", "ihr", "nicht", "ge\u00b7hol\u00b7fen", ",", "wa\u00b7rum", "habt", "ihr", "nicht", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Als der Heiden Tigerz\u00e4hne w\u00fcrgten meine kleine Schaar,", "tokens": ["Als", "der", "Hei\u00b7den", "Ti\u00b7ger\u00b7z\u00e4h\u00b7ne", "w\u00fcrg\u00b7ten", "mei\u00b7ne", "klei\u00b7ne", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Und mit ihrem Blut begossen meiner Kirche Hochaltar,", "tokens": ["Und", "mit", "ih\u00b7rem", "Blut", "be\u00b7gos\u00b7sen", "mei\u00b7ner", "Kir\u00b7che", "Hoc\u00b7hal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Als sie meines Kreuzes Banner niedertraten in den Staub,", "tokens": ["Als", "sie", "mei\u00b7nes", "Kreu\u00b7zes", "Ban\u00b7ner", "nie\u00b7der\u00b7tra\u00b7ten", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Und die Zionsburg der Freiheit ward der Sklavenhorde Raub?", "tokens": ["Und", "die", "Zions\u00b7burg", "der", "Frei\u00b7heit", "ward", "der", "Skla\u00b7ven\u00b7hor\u00b7de", "Raub", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Missolunghi, ", "tokens": ["Mis\u00b7so\u00b7lung\u00b7hi", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bist in donnerndem Triumphe auf der Blitze Flammenlicht", "tokens": ["Bist", "in", "don\u00b7nern\u00b7dem", "Tri\u00b7um\u00b7phe", "auf", "der", "Blit\u00b7ze", "Flam\u00b7men\u00b7licht"], "token_info": ["word", "wor