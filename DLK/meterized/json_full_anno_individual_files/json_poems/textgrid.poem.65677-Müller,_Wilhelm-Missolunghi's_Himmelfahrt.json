.19": {"text": "An dem Tage wird er fragen: Helfer ihr, mit meinem Schwert,", "tokens": ["An", "dem", "Ta\u00b7ge", "wird", "er", "fra\u00b7gen", ":", "Hel\u00b7fer", "ihr", ",", "mit", "mei\u00b7nem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$.", "NN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Warum habt ihr nicht geholfen, warum habt ihr nicht gewehrt,", "tokens": ["Wa\u00b7rum", "habt", "ihr", "nicht", "ge\u00b7hol\u00b7fen", ",", "wa\u00b7rum", "habt", "ihr", "nicht", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "PWAV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Als der Heiden Tigerz\u00e4hne w\u00fcrgten meine kleine Schaar,", "tokens": ["Als", "der", "Hei\u00b7den", "Ti\u00b7ger\u00b7z\u00e4h\u00b7ne", "w\u00fcrg\u00b7ten", "mei\u00b7ne", "klei\u00b7ne", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Und mit ihrem Blut begossen meiner Kirche Hochaltar,", "tokens": ["Und", "mit", "ih\u00b7rem", "Blut", "be\u00b7gos\u00b7sen", "mei\u00b7ner", "Kir\u00b7che", "Hoc\u00b7hal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Als sie meines Kreuzes Banner niedertraten in