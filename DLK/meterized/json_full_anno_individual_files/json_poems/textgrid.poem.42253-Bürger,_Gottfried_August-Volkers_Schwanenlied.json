{"textgrid.poem.42253": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Volkers Schwanenlied", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonst schlug die Lieb' aus mir so helle,", "tokens": ["Sonst", "schlug", "die", "Lieb'", "aus", "mir", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine Nachtigall am Quelle.", "tokens": ["Wie", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "am", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun hat sie meine Kunst geirrt,", "tokens": ["Nun", "hat", "sie", "mei\u00b7ne", "Kunst", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df jeder Laut zum Seufzer wird.", "tokens": ["Da\u00df", "je\u00b7der", "Laut", "zum", "Seuf\u00b7zer", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Liebe, wunders\u00fc\u00dfes Wesen,", "tokens": ["O", "Lie\u00b7be", ",", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfes", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wovon die Kranken oft genesen,", "tokens": ["Wo\u00b7von", "die", "Kran\u00b7ken", "oft", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja Tote schier vom Grab' erstehn,", "tokens": ["Ja", "To\u00b7te", "schier", "vom", "Grab'", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich dr\u00e4ngest du, ins Grab zu gehn! \u2013", "tokens": ["Mich", "dr\u00e4n\u00b7gest", "du", ",", "ins", "Grab", "zu", "gehn", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Busen hegt' ich dich so lange,", "tokens": ["Im", "Bu\u00b7sen", "hegt'", "ich", "dich", "so", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Jener die verklomte Schlange.", "tokens": ["Wie", "Je\u00b7ner", "die", "ver\u00b7klom\u00b7te", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Busen, der ihr Leben bot,", "tokens": ["Dem", "Bu\u00b7sen", ",", "der", "ihr", "Le\u00b7ben", "bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gab sie zum Lohne Schmerz und Tod.", "tokens": ["Gab", "sie", "zum", "Loh\u00b7ne", "Schmerz", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun, s\u00fc\u00dfe M\u00f6rderin des Lebens,", "tokens": ["Nun", ",", "s\u00fc\u00b7\u00dfe", "M\u00f6r\u00b7de\u00b7rin", "des", "Le\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Molly, la\u00df nur nicht vergebens", "tokens": ["O", "Mol\u00b7ly", ",", "la\u00df", "nur", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVIMP", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Flehn, mein letztes Flehen sein!", "tokens": ["Mein", "Flehn", ",", "mein", "letz\u00b7tes", "Fle\u00b7hen", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00df nicht, ach, vergi\u00df nicht mein!", "tokens": ["Ver\u00b7gi\u00df", "nicht", ",", "ach", ",", "ver\u00b7gi\u00df", "nicht", "mein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "$,", "ITJ", "$,", "VVIMP", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf meiner Gruft, wo ich verwese,", "tokens": ["Auf", "mei\u00b7ner", "Gruft", ",", "wo", "ich", "ver\u00b7we\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich, da\u00df sanftes Mitleid lese:", "tokens": ["Will", "ich", ",", "da\u00df", "sanf\u00b7tes", "Mit\u00b7leid", "le\u00b7se", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwie Volker, liebt' und litt kein Mann:", "tokens": ["\u00bb", "wie", "Vol\u00b7ker", ",", "liebt'", "und", "litt", "kein", "Mann", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "$,", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hoffnungslose starb daran.\u00ab \u2013", "tokens": ["Der", "Hoff\u00b7nungs\u00b7lo\u00b7se", "starb", "da\u00b7ran", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mir stets von Herzen wohl gefallen,", "tokens": ["Mir", "stets", "von", "Her\u00b7zen", "wohl", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mann, der voll Gotteskraft und Geist", "tokens": ["Mann", ",", "der", "voll", "Got\u00b7tes\u00b7kraft", "und", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So herzlich Tugend liebt, als preist!", "tokens": ["So", "herz\u00b7lich", "Tu\u00b7gend", "liebt", ",", "als", "preist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dir, Freund, vermach' ich Kranz und Leier,", "tokens": ["Dir", ",", "Freund", ",", "ver\u00b7mach'", "ich", "Kranz", "und", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nur geweiht zu Molly's Feier.", "tokens": ["Doch", "nur", "ge\u00b7weiht", "zu", "Mol\u00b7ly's", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Name Molly sei verwebt", "tokens": ["Der", "Na\u00b7me", "Mol\u00b7ly", "sei", "ver\u00b7webt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jedes Lied, das ihr entschwebt!", "tokens": ["In", "je\u00b7des", "Lied", ",", "das", "ihr", "ent\u00b7schwebt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es gilt der Herrlichsten von Allen,", "tokens": ["Es", "gilt", "der", "Herr\u00b7lichs\u00b7ten", "von", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die unter Gottes Sonne wallen,", "tokens": ["Die", "un\u00b7ter", "Got\u00b7tes", "Son\u00b7ne", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Volker, der verlorne Mann,", "tokens": ["Die", "Vol\u00b7ker", ",", "der", "ver\u00b7lor\u00b7ne", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Schicksal nicht erseufzen kann.", "tokens": ["Vom", "Schick\u00b7sal", "nicht", "er\u00b7seuf\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun sei, o Gott, dem Armen gn\u00e4dig!", "tokens": ["Nun", "sei", ",", "o", "Gott", ",", "dem", "Ar\u00b7men", "gn\u00e4\u00b7dig", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df aller Schuld ihn los und ledig!", "tokens": ["La\u00df", "al\u00b7ler", "Schuld", "ihn", "los", "und", "le\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "PPER", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df nie in andern Flammen ihn,", "tokens": ["La\u00df", "nie", "in", "an\u00b7dern", "Flam\u00b7men", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Flammen seiner Liebe gl\u00fchn!", "tokens": ["Als", "Flam\u00b7men", "sei\u00b7ner", "Lie\u00b7be", "gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sonst schlug die Lieb' aus mir so helle,", "tokens": ["Sonst", "schlug", "die", "Lieb'", "aus", "mir", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine Nachtigall am Quelle.", "tokens": ["Wie", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "am", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun hat sie meine Kunst geirrt,", "tokens": ["Nun", "hat", "sie", "mei\u00b7ne", "Kunst", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df jeder Laut zum Seufzer wird.", "tokens": ["Da\u00df", "je\u00b7der", "Laut", "zum", "Seuf\u00b7zer", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Liebe, wunders\u00fc\u00dfes Wesen,", "tokens": ["O", "Lie\u00b7be", ",", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfes", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wovon die Kranken oft genesen,", "tokens": ["Wo\u00b7