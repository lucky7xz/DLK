{"dta.poem.18056": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man saget, eine Wiesel gieng", "tokens": ["Man", "sa\u00b7get", ",", "ei\u00b7ne", "Wie\u00b7sel", "gieng"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In eines Wirthes Haus, wo sie viel M\u00e4use fieng.", "tokens": ["In", "ei\u00b7nes", "Wirt\u00b7hes", "Haus", ",", "wo", "sie", "viel", "M\u00e4u\u00b7se", "fi\u00b7eng", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Zulezt geschah auf einer Fahrt,", "tokens": ["Zu\u00b7lezt", "ge\u00b7schah", "auf", "ei\u00b7ner", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie von ihm gefangen ward.", "tokens": ["Da\u00df", "sie", "von", "ihm", "ge\u00b7fan\u00b7gen", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sprach sie: Wirth, du sollst mich lassen gehn.", "tokens": ["Da", "sprach", "sie", ":", "Wirth", ",", "du", "sollst", "mich", "las\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wahrhaftig ich verdient es wohl,", "tokens": ["Wahr\u00b7haf\u00b7tig", "ich", "ver\u00b7dient", "es", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df man im Frieden mich erlassen soll.", "tokens": ["Da\u00df", "man", "im", "Frie\u00b7den", "mich", "er\u00b7las\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gieb acht, wie sauber ist dein Haus,", "tokens": ["Gieb", "acht", ",", "wie", "sau\u00b7ber", "ist", "dein", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da l\u00e4uft izt weder Ratt noch Maus.", "tokens": ["Da", "l\u00e4uft", "izt", "we\u00b7der", "Ratt", "noch", "Maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Daf\u00fcr sollst du mir nun zum Lohne geben,", "tokens": ["Da\u00b7f\u00fcr", "sollst", "du", "mir", "nun", "zum", "Loh\u00b7ne", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df du mich friedlich l\u00e4ssest leben.", "tokens": ["Da\u00df", "du", "mich", "fried\u00b7lich", "l\u00e4s\u00b7sest", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das nehm ich an von dir zu einer Gabe", "tokens": ["Das", "nehm", "ich", "an", "von", "dir", "zu", "ei\u00b7ner", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "F\u00fcr allen Flei\u00df, womit ich dir gedienet habe.", "tokens": ["F\u00fcr", "al\u00b7len", "Flei\u00df", ",", "wo\u00b7mit", "ich", "dir", "ge\u00b7die\u00b7net", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Wirth sprach: Ja, das ist wohl wahr,", "tokens": ["Der", "Wirth", "sprach", ":", "Ja", ",", "das", "ist", "wohl", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PTKANT", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mein Haus ist izo gantz und gar", "tokens": ["Mein", "Haus", "ist", "i\u00b7zo", "gantz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von Ratten und von M\u00e4usen rein.", "tokens": ["Von", "Rat\u00b7ten", "und", "von", "M\u00e4u\u00b7sen", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das thatest du: doch nicht um meinetwillen;", "tokens": ["Das", "tha\u00b7test", "du", ":", "doch", "nicht", "um", "mei\u00b7net\u00b7wil\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADV", "PTKNEG", "KOUI", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nicht da\u00df du mir so wolltest n\u00fctzlich seyn;", "tokens": ["Nicht", "da\u00df", "du", "mir", "so", "woll\u00b7test", "n\u00fctz\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nein, sondern deinen Balg zu f\u00fcllen.", "tokens": ["Nein", ",", "son\u00b7dern", "dei\u00b7nen", "Balg", "zu", "f\u00fcl\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Du fiengest, Wiesel, meinen Feind;", "tokens": ["Du", "fi\u00b7en\u00b7gest", ",", "Wie\u00b7sel", ",", "mei\u00b7nen", "Feind", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.21": {"text": "Warum? Er war nicht mein, und nicht dein Freund.", "tokens": ["Wa\u00b7rum", "?", "Er", "war", "nicht", "mein", ",", "und", "nicht", "dein", "Freund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "$,", "KON", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Du wolltest meine Speis\u2019 allein,", "tokens": ["Du", "woll\u00b7test", "mei\u00b7ne", "Speis'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und ohne viel Gesellen seyn.", "tokens": ["Und", "oh\u00b7ne", "viel", "Ge\u00b7sel\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Warum denn solltest du vor mir genesen;", "tokens": ["Wa\u00b7rum", "denn", "soll\u00b7test", "du", "vor", "mir", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+----", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Dieweil du in der That mein Feind gewesen?", "tokens": ["Die\u00b7weil", "du", "in", "der", "That", "mein", "Feind", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Verderbtest du mir nicht mein Fleisch und Brod?", "tokens": ["Ver\u00b7derb\u00b7test", "du", "mir", "nicht", "mein", "Fleisch", "und", "Brod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "De\u00dfwegen denn bereite dich zum Tod.", "tokens": ["De\u00df\u00b7we\u00b7gen", "denn", "be\u00b7rei\u00b7te", "dich", "zum", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die Wiesel konnt es nicht entsagen;", "tokens": ["Die", "Wie\u00b7sel", "konnt", "es", "nicht", "ent\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Dahero mu\u00dfte sie die Straffe tragen.", "tokens": ["Da\u00b7he\u00b7ro", "mu\u00df\u00b7te", "sie", "die", "Straf\u00b7fe", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Sie ward get\u00f6det auf der Statt,", "tokens": ["Sie", "ward", "ge\u00b7t\u00f6\u00b7det", "auf", "der", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Weil ohne Willen sie gedienet hatt.", "tokens": ["Weil", "oh\u00b7ne", "Wil\u00b7len", "sie", "ge\u00b7die\u00b7net", "hatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}