{"textgrid.poem.48779": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Aus eben selbigem. Auf die g\u00fcldene Haarnadel", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du g\u00fcldne Nadel du, noch g\u00fcldener als Gold,", "tokens": ["Du", "g\u00fcld\u00b7ne", "Na\u00b7del", "du", ",", "noch", "g\u00fcl\u00b7de\u00b7ner", "als", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die du der Liebsten fielst aus ihren g\u00fcldnen Haaren,", "tokens": ["die", "du", "der", "Liebs\u00b7ten", "fielst", "aus", "ih\u00b7ren", "g\u00fcld\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ach weine nicht zu sehr, da\u00df dir di\u00df widerfahren,", "tokens": ["ach", "wei\u00b7ne", "nicht", "zu", "sehr", ",", "da\u00df", "dir", "di\u00df", "wi\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PTKA", "ADV", "$,", "KOUS", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df du ihr sch\u00f6nes H\u00e4upt, als ich wol selbsten wolt',", "tokens": ["da\u00df", "du", "ihr", "sch\u00f6\u00b7nes", "H\u00e4upt", ",", "als", "ich", "wol", "selbs\u00b7ten", "wolt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "hinfort nicht zieren wirst! Erhole deinen Mut!", "tokens": ["hin\u00b7fort", "nicht", "zie\u00b7ren", "wirst", "!", "Er\u00b7ho\u00b7le", "dei\u00b7nen", "Mut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "VAFIN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich hat kein loser Dieb bei schwarzer Nacht genommen,", "tokens": ["Dich", "hat", "kein", "lo\u00b7ser", "Dieb", "bei", "schwar\u00b7zer", "Nacht", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du bist viel weniger in R\u00e4uber H\u00e4nde kommen:", "tokens": ["du", "bist", "viel", "we\u00b7ni\u00b7ger", "in", "R\u00e4u\u00b7ber", "H\u00e4n\u00b7de", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dir war ein junges Blut von ganzem Herzen gut.", "tokens": ["dir", "war", "ein", "jun\u00b7ges", "Blut", "von", "gan\u00b7zem", "Her\u00b7zen", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn als er suchte Luft in hei\u00dfen 