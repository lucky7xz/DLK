{"textgrid.poem.47044": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, sch\u00f6ne glatte, kalte, goldne Schlange,", "tokens": ["Komm", ",", "sch\u00f6\u00b7ne", "glat\u00b7te", ",", "kal\u00b7te", ",", "gold\u00b7ne", "Schlan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf die ich starker Schlangenw\u00fcrger passe;", "tokens": ["Auf", "die", "ich", "star\u00b7ker", "Schlan\u00b7gen\u00b7w\u00fcr\u00b7ger", "pas\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast mit buntem Spiel um meine Stra\u00dfe", "tokens": ["Du", "hast", "mit", "bun\u00b7tem", "Spiel", "um", "mei\u00b7ne", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich zierlich schl\u00e4ngelnd hergewunden lange.", "tokens": ["Dich", "zier\u00b7lich", "schl\u00e4n\u00b7gelnd", "her\u00b7ge\u00b7wun\u00b7den", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Komm, schmeidige, da\u00df ich mit eh'rnem Zwange", "tokens": ["Komm", ",", "schmei\u00b7di\u00b7ge", ",", "da\u00df", "ich", "mit", "eh'r\u00b7nem", "Zwan\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich fass' und halt' und nicht sobald dich lasse;", "tokens": ["Dich", "fass'", "und", "halt'", "und", "nicht", "so\u00b7bald", "dich", "las\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "PTKNEG", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wind' du dich nur und kr\u00fcmm' dich, giftig blasse,", "tokens": ["Wind'", "du", "dich", "nur", "und", "kr\u00fcmm'", "dich", ",", "gif\u00b7tig", "blas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "KON", "VVFIN", "PPER", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir ist vor deinem s\u00fc\u00dfen Gift nicht bange.", "tokens": ["Mir", "ist", "vor", "dei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Gift", "nicht", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohlauf, mit allen deinen Schlangenk\u00fcnsten,", "tokens": ["Wohl\u00b7auf", ",", "mit", "al\u00b7len", "dei\u00b7nen", "Schlan\u00b7gen\u00b7k\u00fcns\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unb\u00e4ndig um des Feindes Leib dich ringelnd,", "tokens": ["Un\u00b7b\u00e4n\u00b7dig", "um", "des", "Fein\u00b7des", "Leib", "dich", "rin\u00b7gelnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Z\u00e4hnen blinkend, spr\u00fchend mit den Zungen:", "tokens": ["Mit", "Z\u00e4h\u00b7nen", "blin\u00b7kend", ",", "spr\u00fc\u00b7hend", "mit", "den", "Zun\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00df sehn, wer von uns beiden hier mit Br\u00fcnsten", "tokens": ["La\u00df", "sehn", ",", "wer", "von", "uns", "bei\u00b7den", "hier", "mit", "Br\u00fcns\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "PWS", "APPR", "PPER", "PIAT", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das andre wird bestehn, es so umzingelnd,", "tokens": ["Das", "and\u00b7re", "wird", "be\u00b7stehn", ",", "es", "so", "um\u00b7zin\u00b7gelnd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "VVINF", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df es bekennen mu\u00df: ich bin bezwungen.", "tokens": ["Da\u00df", "es", "be\u00b7ken\u00b7nen", "mu\u00df", ":", "ich", "bin", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Komm, sch\u00f6ne glatte, kalte, goldne