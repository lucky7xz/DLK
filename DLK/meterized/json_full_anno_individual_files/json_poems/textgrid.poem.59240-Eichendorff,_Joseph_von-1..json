{"textgrid.poem.59240": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Klugen, die nach Gott nicht wollten fragen,", "tokens": ["Die", "Klu\u00b7gen", ",", "die", "nach", "Gott", "nicht", "woll\u00b7ten", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den heil'gen Kampf gern irdisch m\u00f6chten schlichten,", "tokens": ["Den", "heil'\u00b7gen", "Kampf", "gern", "ir\u00b7disch", "m\u00f6ch\u00b7ten", "schlich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Tod kein Herz, nicht Lieb, sich aufzurichten,", "tokens": ["Zum", "Tod", "kein", "Herz", ",", "nicht", "Lieb", ",", "sich", "auf\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "PTKNEG", "NN", "$,", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzehren sich nur selbst in eitlen Klagen.", "tokens": ["Ver\u00b7zeh\u00b7ren", "sich", "nur", "selbst", "in", "eit\u00b7len", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sind alle eure Schiffe denn zerschlagen:", "tokens": ["Sind", "al\u00b7le", "eu\u00b7re", "Schif\u00b7fe", "denn", "zer\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht man die heil'ge Flagge dich aufrichten,", "tokens": ["Sieht", "man", "die", "heil'\u00b7ge", "Flag\u00b7ge", "dich", "auf\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Liebessturm, der jene mu\u00dft vernichten,", "tokens": ["Vom", "Lie\u00b7bes\u00b7sturm", ",", "der", "je\u00b7ne", "mu\u00dft", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein junges Schiff siegreich hinweggetragen.", "tokens": ["Dein", "jun\u00b7ges", "Schiff", "sieg\u00b7reich", "hin\u00b7weg\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "S\u00fcdwinde spielen lau um Laut und Locken,", "tokens": ["S\u00fcd\u00b7win\u00b7de", "spie\u00b7len", "lau", "um", "Laut", "und", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "APPR", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Morgenrot des Hutes Federn schwanken,", "tokens": ["Im", "Mor\u00b7gen\u00b7rot", "des", "Hu\u00b7tes", "Fe\u00b7dern", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word