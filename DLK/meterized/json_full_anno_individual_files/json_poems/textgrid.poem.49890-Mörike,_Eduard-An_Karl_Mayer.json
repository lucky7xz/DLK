"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Und den See dort einsam in der Wildnis,", "tokens": ["Und", "den", "See", "dort", "ein\u00b7sam", "in", "der", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "\u00dcbergr\u00fcnt von lichten Wasserlinsen.", "tokens": ["\u00dc\u00b7berg\u00b7r\u00fcnt", "von", "lich\u00b7ten", "Was\u00b7ser\u00b7lin\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "W\u00e4r ich, w\u00e4r ich selber der Gefangne!", "tokens": ["W\u00e4r", "ich", ",", "w\u00e4r", "ich", "sel\u00b7ber", "der", "Ge\u00b7fang\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sperrten sie mich ein auf sieben Monde!", "tokens": ["Sperr\u00b7ten", "sie", "mich", "ein", "auf", "sie\u00b7ben", "Mon\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Herzlich wollt ich dann des Schlie\u00dfers lachen,", "tokens": ["Herz\u00b7lich", "wollt", "ich", "dann", "des", "Schlie\u00b7\u00dfers", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn er dreifach meine T\u00fcr verschl\u00f6sse,", "tokens": ["Wenn", "er", "drei\u00b7fach", "mei\u00b7ne", "T\u00fcr", "ver\u00b7schl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mich allein mit meinem B\u00fcchlein lassend.", "tokens": ["Mich", "al\u00b7lein", "mit", "mei\u00b7nem", "B\u00fcch\u00b7lein", "las\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber wenn doch endlich insgeheime", "tokens": ["A\u00b7ber", "wenn", "doch", "end\u00b7lich", "ins\u00b7ge\u00b7hei\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eine tiefe Sehnsucht mich beschliche,", "tokens": ["Ei\u00b7ne", "tie\u00b7fe", "Sehn\u00b7sucht", "mich", "be\u00b7schli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich trauerte um Wald und Wiesen?", "tokens": ["Da\u00df", "ich", "trau\u00b7er\u00b7te", "um", "Wald", "und", "Wie\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ha! wie sehn ich mich, mich so zu sehnen!", "tokens": ["Ha", "!", "wie", "sehn", "ich", "mich", ",", "mich", "so", "zu", "seh\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Reizend w\u00e4r's, den J\u00e4ger zu beneiden,", "tokens": ["Rei\u00b7zend", "w\u00e4r's", ",", "den", "J\u00e4\u00b7ger", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der in Freiheit atmet Waldesatem,", "tokens": ["Der", "in", "Frei\u00b7heit", "at\u00b7met", "Wal\u00b7de\u00b7sa\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und den Hirten, wenn er nach Mittage", "tokens": ["Und", "den", "Hir\u00b7ten", ",", "wenn", "er", "nach", "Mit\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ruhig am besonnten H\u00fcgel lehnet!", "tokens": ["Ru\u00b7hig", "am", "be\u00b7sonn\u00b7ten", "H\u00fc\u00b7gel", "leh\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Sieh, so seltsam sind des Herzens W\u00fcnsche,", "tokens": ["Sieh", ",", "so", "selt\u00b7sam", "sind", "des", "Her\u00b7zens", "W\u00fcn\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das sich m\u00fc\u00dfig f\u00fchlt im \u00dcberflusse.", "tokens": ["Das", "sich", "m\u00fc\u00b7\u00dfig", "f\u00fchlt", "im", "\u00dc\u00b7berf\u00b7lus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}