{"textgrid.poem.49890": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Karl Mayer", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem gefangenen, betr\u00fcbten Manne", "tokens": ["Dem", "ge\u00b7fan\u00b7ge\u00b7nen", ",", "be\u00b7tr\u00fcb\u00b7ten", "Man\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hinter seinen dichten Eisenst\u00e4ben,", "tokens": ["Hin\u00b7ter", "sei\u00b7nen", "dich\u00b7ten", "Ei\u00b7sen\u00b7st\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ihm jemand deine holden Lieder", "tokens": ["Wenn", "ihm", "je\u00b7mand", "dei\u00b7ne", "hol\u00b7den", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aufs Gesimse seines Fensters legte,", "tokens": ["Aufs", "Ge\u00b7sim\u00b7se", "sei\u00b7nes", "Fens\u00b7ters", "leg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wo die liebe Sonne sich ein St\u00fcndlein", "tokens": ["Wo", "die", "lie\u00b7be", "Son\u00b7ne", "sich", "ein", "St\u00fcnd\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "ART", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "T\u00e4glich einstellt, handbreit nur ein Streifchen:", "tokens": ["T\u00e4g\u00b7lich", "ein\u00b7stellt", ",", "hand\u00b7breit", "nur", "ein", "Streif\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "O wie schimmerten ihm Wald und Auen", "tokens": ["O", "wie", "schim\u00b7mer\u00b7ten", "ihm", "Wald", "und", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sommerlich, die stillen Wiesengr\u00fcnde!", "tokens": ["Som\u00b7mer\u00b7lich", ",", "die", "stil\u00b7len", "Wie\u00b7sen\u00b7gr\u00fcn\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "O wie hastig irrten seine Schritte", "tokens": ["O", "wie", "has\u00b7tig", "irr\u00b7ten", "sei\u00b7ne", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Durch die tausend Lieblichkeiten alle,", "tokens": ["Durch", "die", "tau\u00b7send", "Lieb\u00b7lich\u00b7kei\u00b7ten", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ohne Wahl, was er zuerst begr\u00fc\u00dfe:", "tokens": ["Oh\u00b7ne", "Wahl", ",", "was", "er", "zu\u00b7erst", "be\u00b7gr\u00fc\u00b7\u00dfe", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ob das D\u00f6rflein in der Sonntagfr\u00fche,", "tokens": ["Ob", "das", "D\u00f6r\u00b7flein", "in", "der", "Sonn\u00b7tag\u00b7fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Wo die frische Dirne sich im G\u00e4rtchen", "tokens": ["Wo", "die", "fri\u00b7sche", "Dir\u00b7ne", "sich", "im", "G\u00e4rt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Einen Busenstrau\u00df zur Kirche holet;", "tokens": ["Ei\u00b7nen", "Bu\u00b7sen\u00b7strau\u00df", "zur", "Kir\u00b7che", "ho\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Ob die Tr\u00fcmmer, wo das Laub der Birke", "tokens": ["Ob", "die", "Tr\u00fcm\u00b7mer", ",", "wo", "das", "Laub", "der", "Bir\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Herbstlich rieselt aufs Gestein hernieder,", "tokens": ["Herbst\u00b7lich", "rie\u00b7selt", "aufs", "Ge\u00b7stein", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Dr\u00fcberhin der Weih im Fluge schreiend;", "tokens": ["Dr\u00fc\u00b7berh\u00b7in", "der", "Weih", "im", "Flu\u00b7ge", "schrei\u00b7end", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Und den See dort einsam in der Wildnis,", "tokens": ["Und", "den", "See", "dort", "ein\u00b7sam", "in", "der", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "\u00dcbergr\u00fcnt von lichten Wasserlinsen.", "tokens": ["\u00dc\u00b7berg\u00b7r\u00fcnt", "von", "lich\u00b7ten", "Was\u00b7ser\u00b7lin\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "W\u00e4r ich, w\u00e4r ich selber der Gefangne!", "tokens": ["W\u00e4r", "ich", ",", "w\u00e4r", "ich", "sel\u00b7ber", "der", "Ge\u00b7fang\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sperrten sie mich ein auf sieben Monde!", "tokens": ["Sperr\u00b7ten", "sie", "mich", "ein", "auf", "sie\u00b7ben", "Mon\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Herzlich wollt ich dann des Schlie\u00dfers lachen,", "tokens": ["Herz\u00b7lich", "wollt", "ich", "dann", "des", "Schlie\u00b7\u00dfers", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn er dreifach meine T\u00fcr verschl\u00f6sse,", "tokens": ["Wenn", "er", "drei\u00b7fach", "mei\u00b7ne", "T\u00fcr", "ver\u00b7schl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mich allein mit meinem B\u00fcchlein lassend.", "tokens": ["Mich", "al\u00b7lein", "mit", "mei\u00b7nem", "B\u00fcch\u00b7lein", "las\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber wenn doch endlich insgeheime", "tokens": ["A\u00b7ber", "wenn", "doch", "end\u00b7lich", "ins\u00b7ge\u00b7hei\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eine tiefe Sehnsucht mich beschliche,", "tokens": ["Ei\u00b7ne", "tie\u00b7fe", "Sehn\u00b7sucht", "mich", "be\u00b7schli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich trauerte um Wald und Wiesen?", "tokens": ["Da\u00df", "ich", "trau\u00b7er\u00b7te", "um", "Wald", "und", "Wie\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ha! wie sehn ich mich, mich so zu sehnen!", "tokens": ["Ha", "!", "wie", "sehn", "ich", "mich", ",", "mich", "so", "zu", "seh\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Reizend w\u00e4r's, den J\u00e4ger zu beneiden,", "tokens": ["Rei\u00b7zend", "w\u00e4r's", ",", "den", "J\u00e4\u00b7ger", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der in Freiheit atmet Waldesatem,", "tokens": ["Der", "in", "Frei\u00b7heit", "at\u00b7met", "Wal\u00b7de\u00b7sa\u00b7tem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und den Hirten, wenn er nach Mittage", "tokens": ["Und", "den", "Hir\u00b7ten", ",", "wenn", "er", "nach", "Mit\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ruhig am besonnten H\u00fcgel lehnet!", "tokens": ["Ru\u00b7hig", "am", "be\u00b7sonn\u00b7ten", "H\u00fc\u00b7gel", "leh\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sieh, so seltsam sind des Herzens W\u00fcnsche,", "tokens": ["Sieh", ",", "so", "selt\u00b7sam", "sind", "des", "Her\u00b7zens", "W\u00fcn\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das sich m\u00fc\u00dfig f\u