{"dta.poem.5267": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings-Erg\u00f6tzlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Wunder ist es ja, da\u00df uns die Welt", "tokens": ["Kein", "Wun\u00b7der", "ist", "es", "ja", ",", "da\u00df", "uns", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jm Fr\u00fchling etwas mehr, als sonst, gef\u00e4llt.", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "et\u00b7was", "mehr", ",", "als", "sonst", ",", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$,", "KOUS", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur ist es desto mehr noch zu beklagen,", "tokens": ["Nur", "ist", "es", "des\u00b7to", "mehr", "noch", "zu", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df alle Wunder uns nicht noch weit mehr behagen.", "tokens": ["Da\u00df", "al\u00b7le", "Wun\u00b7der", "uns", "nicht", "noch", "weit", "mehr", "be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "PTKNEG", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Jm Fr\u00fchling sp\u00fchrt man ", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "sp\u00fchrt", "man"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Vergn\u00fcgen. Man versp\u00fchrt, da Frost und Eis vorbey,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ".", "Man", "ver\u00b7sp\u00fchrt", ",", "da", "Frost", "und", "Eis", "vor\u00b7bey", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus ihrer fast noch nahen Wiedrigkeit,", "tokens": ["Aus", "ih\u00b7rer", "fast", "noch", "na\u00b7hen", "Wied\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Jm Gegenhalt der angenehmen Zeit,", "tokens": ["Jm", "Ge\u00b7gen\u00b7halt", "der", "an\u00b7ge\u00b7neh\u00b7men", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie in der Aendrung selbst was angenehmes stecke,", "tokens": ["Wie", "in", "der", "A\u00b7en\u00b7drung", "selbst", "was", "an\u00b7ge\u00b7neh\u00b7mes", "ste\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "PWS", "ADJA", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und sich im Wechsel noch um desto mehr entdecke.", "tokens": ["Und", "sich", "im", "Wech\u00b7sel", "noch", "um", "des\u00b7to", "mehr", "ent\u00b7de\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "ADV", "APPR", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Man sp\u00fchrt f\u00fcrs andere der ", "tokens": ["Man", "sp\u00fchrt", "f\u00fcrs", "an\u00b7de\u00b7re", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "PIS", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und bey derselben steht noch ein Vergn\u00fcgen offen:", "tokens": ["Und", "bey", "der\u00b7sel\u00b7ben", "steht", "noch", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "of\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es l\u00e4\u00dft uns die Natur bey dem Genu\u00df", "tokens": ["Es", "l\u00e4\u00dft", "uns", "die", "Na\u00b7tur", "bey", "dem", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und bey der Lieblichkeit so holdem Uberflu\u00df", "tokens": ["Und", "bey", "der", "Lieb\u00b7lich\u00b7keit", "so", "hol\u00b7dem", "U\u00b7ber\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Noch immer etwas ", "tokens": ["Noch", "im\u00b7mer", "et\u00b7was"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Da Knosp\u2019 und Bl\u00fcht\u2019, die auf- und von einander brechen,", "tokens": ["Da", "Knosp'", "und", "Bl\u00fcht'", ",", "die", "auf", "und", "von", "ein\u00b7an\u00b7der", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "$,", "ART", "TRUNC", "KON", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Viel zeigen, doch noch mehr versprechen.", "tokens": ["Viel", "zei\u00b7gen", ",", "doch", "noch", "mehr", "ver\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auf la\u00dft uns denn von allem, was so sch\u00f6n,", "tokens": ["Auf", "la\u00dft", "uns", "denn", "von", "al\u00b7lem", ",", "was", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVIMP", "PPER", "ADV", "APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch etwas wenigstens bedachtsam sehn!", "tokens": ["Doch", "et\u00b7was", "we\u00b7nigs\u00b7tens", "be\u00b7dacht\u00b7sam", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Man siehet jetzt auf allen Zweigen", "tokens": ["Man", "sie\u00b7het", "jetzt", "auf", "al\u00b7len", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ein liebliches Gemisch von braun und gr\u00fcn sich zeigen,", "tokens": ["Ein", "lieb\u00b7li\u00b7ches", "Ge\u00b7misch", "von", "braun", "und", "gr\u00fcn", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "KON", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wird, nicht sonder Lust, an vielen,", "tokens": ["Und", "wird", ",", "nicht", "son\u00b7der", "Lust", ",", "an", "vie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PTKNEG", "KON", "NN", "$,", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wie braun und gr\u00fcn recht durch einander spielen,", "tokens": ["Wie", "braun", "und", "gr\u00fcn", "recht", "durch", "ein\u00b7an\u00b7der", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "In einem bunten Glantz, gewahr.", "tokens": ["In", "ei\u00b7nem", "bun\u00b7ten", "Glantz", ",", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wie gl\u00e4ntzen in entw\u00f6lcktem Wetter,", "tokens": ["Wie", "gl\u00e4nt\u00b7zen", "in", "ent\u00b7w\u00f6lck\u00b7tem", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "In dieser holden Fr\u00fchlings-Zeit,", "tokens": ["In", "die\u00b7ser", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der glatten Knospen Heer, die zarten Bl\u00e4tter!", "tokens": ["Der", "glat\u00b7ten", "Knos\u00b7pen", "Heer", ",", "die", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Der ofnen Knospen Meng\u2019, an denen jedes Blatt", "tokens": ["Der", "of\u00b7nen", "Knos\u00b7pen", "Meng'", ",", "an", "de\u00b7nen", "je\u00b7des", "Blatt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "APPR", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sich noch nicht recht formirt und ausgebreitet hat,", "tokens": ["Sich", "noch", "nicht", "recht", "for\u00b7mirt", "und", "aus\u00b7ge\u00b7brei\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "ADJD", "VVPP", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}