{"textgrid.poem.53089": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Divitiis cunctisque bonis Ars inclyta pr\u00e6stat", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol dem, der sich nur lesst begn\u00fcgen", "tokens": ["Wol", "dem", ",", "der", "sich", "nur", "lesst", "be\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "ADV", "ADV", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Daran, was jmh auf Gottes Gunst", "tokens": ["Da\u00b7ran", ",", "was", "jmh", "auf", "Got\u00b7tes", "Gunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWS", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gl\u00fcck vnfeilbar zu mu\u00df f\u00fcgen,", "tokens": ["Das", "Gl\u00fcck", "vn\u00b7feil\u00b7bar", "zu", "mu\u00df", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vnd nehrt sich redlich seiner Kunst!", "tokens": ["Vnd", "nehrt", "sich", "red\u00b7lich", "sei\u00b7ner", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auff Geld vnd Guth,", "tokens": ["Ein", "an\u00b7der", "halt", "auff", "Geld", "vnd", "Guth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe Kunst vnd freyen Muth.", "tokens": ["Ich", "lie\u00b7be", "Kunst", "vnd", "frey\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie bald kan Reichthumb dich verlassen,", "tokens": ["Wie", "bald", "kan", "Reicht\u00b7humb", "dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bist du elend gnug daran!", "tokens": ["So", "bist", "du", "e\u00b7lend", "gnug", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kunst aber wird dich stets vmbfassen,", "tokens": ["Kunst", "a\u00b7ber", "wird", "dich", "stets", "vmb\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie n\u00e4hret trewlich jhren Mann.", "tokens": ["Sie", "n\u00e4h\u00b7ret", "trew\u00b7lich", "jhren", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ein ander halt auff Geld vnd Gut,", "tokens": ["Ein", "an\u00b7der", "halt", "auff", "Geld", "vnd", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe Kunst vnd freyen Muth.", "tokens": ["Ich", "lie\u00b7be", "Kunst", "vnd", "frey\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Giebt Sie mir nicht viel Goldes-Tonnen,", "tokens": ["Giebt", "Sie", "mir", "nicht", "viel", "Gol\u00b7des\u00b7Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So macht sie mich doch besser satt,", "tokens": ["So", "macht", "sie", "mich", "doch", "bes\u00b7ser", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als den sein Geld, der viel gewonnen,", "tokens": ["Als", "den", "sein", "Geld", ",", "der", "viel", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd Herr nicht ist dess, was er hat.", "tokens": ["Vnd", "Herr", "nicht", "ist", "dess", ",", "was", "er", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VAFIN", "PDS", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ein ander halt auff Geld vnd Gut,", "tokens": ["Ein", "an\u00b7der", "halt", "auff", "Geld", "vnd", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe Kunst vnd freyen Muth.", "tokens": ["Ich", "lie\u00b7be", "Kunst", "vnd", "frey\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie manchem hat der Krieg genommen", "tokens": ["Wie", "man\u00b7chem", "hat", "der", "Krieg", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was jhm vorhin das Gl\u00fccke gab,", "tokens": ["Was", "jhm", "vor\u00b7hin", "das", "Gl\u00fc\u00b7cke", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der jetzt f\u00fcr alles Geld bekommen", "tokens": ["Der", "jetzt", "f\u00fcr", "al\u00b7les", "Geld", "be\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur einen kalen Bettel-Stab!", "tokens": ["Nur", "ei\u00b7nen", "ka\u00b7len", "Bet\u00b7tel\u00b7Stab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auff Geld vnd Gut,", "tokens": ["Ein", "an\u00b7der", "halt", "auff", "Geld", "vnd", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe Kunst vnd freyen Muth.", "tokens": ["Ich", "lie\u00b7be", "Kunst", "vnd", "frey\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer was gelernt, schewt keiner Waffen,", "tokens": ["Wer", "was", "ge\u00b7lernt", ",", "schewt", "kei\u00b7ner", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kunst ist jhm f\u00fcr alles Geldt;", "tokens": ["Die", "Kunst", "ist", "jhm", "f\u00fcr", "al\u00b7les", "Geldt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mu\u00df in steten \u00e4ngsten schlaffen", "tokens": ["Der", "mu\u00df", "in", "ste\u00b7ten", "\u00e4ngs\u00b7ten", "schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nur den Schatz im Kasten helt.", "tokens": ["Der", "nur", "den", "Schatz", "im", "Kas\u00b7ten", "helt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auff Geld vnd Gut,", "tokens": ["Ein", "an\u00b7der", "halt", "auff", "Geld", "vnd", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe Kunst vnd freyen Muth.", "tokens": ["Ich", "lie\u00b7be", "Kunst", "vnd", "frey\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was ich besitz', ist nicht im Kasten;", "tokens": ["Was", "ich", "be\u00b7sitz'", ",", "ist", "nicht", "im", "Kas\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wil jemand meinen G\u00fctern an,", "tokens": ["Wil", "je\u00b7mand", "mei\u00b7nen", "G\u00fc\u00b7tern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mu\u00df mein Leben selbst antasten,", "tokens": ["Der", "mu\u00df", "mein", "Le\u00b7ben", "selbst", "an\u00b7tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist die\u00df nun hin, was darff ich dann?", "tokens": ["Ist", "die\u00df", "nun", "hin", ",", "was", "darff", "ich", "dann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PTKVZ", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auff Geldt vnd Gut,", "tokens": ["Ein", "an\u00b7der", "halt", "auff", "Geldt", "vnd", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe Kunst vnd freyen Muth.", "tokens": ["Ich", "lie\u00b7be", "Kunst", "vnd", "frey\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bring mich dahin aus diesem Lande,", "tokens": ["Bring", "mich", "da\u00b7hin", "aus", "die\u00b7sem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo nie der Tag recht bricht herf\u00fcr,", "tokens": ["Wo", "nie", "der", "Tag", "recht", "bricht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Kunst wil ich im frembden Sande", "tokens": ["Durch", "Kunst", "wil", "ich", "im", "fremb\u00b7den", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So seelig leben gleich wie hier!", "tokens": ["So", "see\u00b7lig", "le\u00b7ben", "gleich", "wie", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auff Geld vnd Gut,", "tokens": ["Ein", "an\u00b7der", "halt", "auff", "Geld", "vnd", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text"