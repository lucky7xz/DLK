"iambic.tetra"}, "line.6": {"text": "Ich liebe Kunst vnd freyen Muth.", "tokens": ["Ich", "lie\u00b7be", "Kunst", "vnd", "frey\u00b7en", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn mir der H\u00f6chste das nur giebet,", "tokens": ["Wenn", "mir", "der", "H\u00f6chs\u00b7te", "das", "nur", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mir zu leben n\u00f6tig ist,", "tokens": ["Was", "mir", "zu", "le\u00b7ben", "n\u00f6\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd eine Seele die mich liebet", "tokens": ["Vnd", "ei\u00b7ne", "See\u00b7le", "die", "mich", "lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd mich f\u00fcr allen au\u00dferkiest,", "tokens": ["Vnd", "mich", "f\u00fcr", "al\u00b7len", "au\u00b7\u00dfer\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lieb' ich vber Geld vnd Guth", "tokens": ["So", "lieb'", "ich", "vber", "Geld", "vnd", "Guth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie vnd die K\u00fcnst', vnd freyen