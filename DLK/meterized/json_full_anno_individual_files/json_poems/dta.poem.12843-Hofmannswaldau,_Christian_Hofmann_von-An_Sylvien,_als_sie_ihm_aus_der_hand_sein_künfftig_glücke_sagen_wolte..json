{"dta.poem.12843": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sylvien, als sie ihm aus der  \n hand sein k\u00fcnfftig gl\u00fccke sagen wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was macht dein sch\u00f6ner blick in meiner schlechten hand?", "tokens": ["Was", "macht", "dein", "sch\u00f6\u00b7ner", "blick", "in", "mei\u00b7ner", "schlech\u00b7ten", "hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wilst du ihr schwartzes fell an deinen sonnen bleichen?", "tokens": ["Wilst", "du", "ihr", "schwart\u00b7zes", "fell", "an", "dei\u00b7nen", "son\u00b7nen", "blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was gr\u00fcbelt doch dein aug\u2019 in den verwirrten zeichen?", "tokens": ["Was", "gr\u00fc\u00b7belt", "doch", "dein", "aug'", "in", "den", "ver\u00b7wirr\u00b7ten", "zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In diesen tiefen liegt kein richtiger verstand.", "tokens": ["In", "die\u00b7sen", "tie\u00b7fen", "liegt", "kein", "rich\u00b7ti\u00b7ger", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wilst mein k\u00fcnfftig gl\u00fcck im Venus-berge finden,", "tokens": ["Du", "wilst", "mein", "k\u00fcnff\u00b7tig", "gl\u00fcck", "im", "Ve\u00b7nus\u00b7ber\u00b7ge", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJD", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und dein verwegner witz merckt alle narben an.", "tokens": ["Und", "dein", "ver\u00b7weg\u00b7ner", "witz", "merckt", "al\u00b7le", "nar\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indessen wirst du doch nichts deutliches ergr\u00fcnden,", "tokens": ["In\u00b7des\u00b7sen", "wirst", "du", "doch", "nichts", "deut\u00b7li\u00b7ches", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil die bewu\u00dfte kunst gar leichte tr\u00fcgen kan.", "tokens": ["Weil", "die", "be\u00b7wu\u00df\u00b7te", "kunst", "gar", "leich\u00b7te", "tr\u00fc\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jedoch wo Sylvia im ernst, und nicht im schertze", "tokens": ["Je\u00b7doch", "wo", "Syl\u00b7via", "im", "ernst", ",", "und", "nicht", "im", "schert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "NE", "APPRART", "ADJD", "$,", "KON", "PTKNEG", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Mein k\u00fcnfftig gl\u00fccke sucht, und mir es sagen will,", "tokens": ["Mein", "k\u00fcnff\u00b7tig", "gl\u00fc\u00b7cke", "sucht", ",", "und", "mir", "es", "sa\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "VVFIN", "$,", "KON", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So frag\u2019 an statt der hand, in der es allzu viel", "tokens": ["So", "frag'", "an", "statt", "der", "hand", ",", "in", "der", "es", "all\u00b7zu", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PTKVZ", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PTKA", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verschlo\u00dfne r\u00e4tzel giebt, o Sylvia! dein hertze.", "tokens": ["Ver\u00b7schlo\u00df\u00b7ne", "r\u00e4t\u00b7zel", "giebt", ",", "o", "Syl\u00b7via", "!", "dein", "hert\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "FM", "NE", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}