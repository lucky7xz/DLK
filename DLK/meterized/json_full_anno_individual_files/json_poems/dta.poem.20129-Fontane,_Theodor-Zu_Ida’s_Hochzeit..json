{"dta.poem.20129": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zu Ida\u2019s Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.85", "en:0.14"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ida! es kn&#252;pft manch' sch\u00f6ne Sage", "tokens": ["I\u00b7da", "!", "es", "kn", "&#252;", "pft", "man\u00b7ch'", "sch\u00f6\u00b7ne", "Sa\u00b7ge"], "token_info": ["word", "punct", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "$(", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sich an dies Wort, aus fr\u00fchster Zeit,", "tokens": ["Sich", "an", "dies", "Wort", ",", "aus", "fr\u00fchs\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PDS", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bis an\u2019s Ende aller Tage", "tokens": ["Und", "bis", "an's", "En\u00b7de", "al\u00b7ler", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lieh ihm Homer Unsterblichkeit.", "tokens": ["Lieh", "ihm", "Ho\u00b7mer", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Berg ", "tokens": ["Berg"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Den G\u00f6tterhonig einst gezeugt,", "tokens": ["Den", "G\u00f6t\u00b7ter\u00b7ho\u00b7nig", "einst", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit dem der Nymphen treues Dienen", "tokens": ["Mit", "dem", "der", "Nym\u00b7phen", "treu\u00b7es", "Die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Zeus, den jungen, gro\u00df ges\u00e4ugt.", "tokens": ["Den", "Zeus", ",", "den", "jun\u00b7gen", ",", "gro\u00df", "ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und Ida war\u2019s, zu dessen F\u00fc\u00dfen", "tokens": ["Und", "I\u00b7da", "wa\u00b7r's", ",", "zu", "des\u00b7sen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der sch\u00f6ne Sohn des Priam\u2019s schlief,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Sohn", "des", "Pri\u00b7a\u00b7m's", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ihn aus Tr\u00e4umen, liebes\u00fc\u00dfen,", "tokens": ["Als", "ihn", "aus", "Tr\u00e4u\u00b7men", ",", "lie\u00b7be\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein G\u00f6tterstreit in\u2019s Wachen rief;", "tokens": ["Ein", "G\u00f6t\u00b7ter\u00b7streit", "in's", "Wa\u00b7chen", "rief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor ihm, (Minerven im Geleite)", "tokens": ["Vor", "ihm", ",", "(", "Mi\u00b7ner\u00b7ven", "im", "Ge\u00b7lei\u00b7te", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "$(", "NN", "APPRART", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Den Erisapfel in der Hand,", "tokens": ["Den", "E\u00b7ris\u00b7ap\u00b7fel", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stand Juno, \u2014 aber still zur Seite", "tokens": ["Stand", "Ju\u00b7no", ",", "a\u00b7ber", "still", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "$(", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die siegessichre Venus stand.", "tokens": ["Die", "sie\u00b7ges\u00b7sich\u00b7re", "Ve\u00b7nus", "stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und Juno sprach: \u201eholdsel\u2019ger Knabe,", "tokens": ["Und", "Ju\u00b7no", "sprach", ":", "\u201e", "hold\u00b7sel'\u00b7ger", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du, dem an Sch\u00f6nheit Keiner gleicht,", "tokens": ["Du", ",", "dem", "an", "Sch\u00f6n\u00b7heit", "Kei\u00b7ner", "gleicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du sei's, der diese goldne Gabe", "tokens": ["Du", "sei's", ",", "der", "die\u00b7se", "gold\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sch\u00f6nsten von uns Dreien reicht.\u201c", "tokens": ["Der", "Sch\u00f6ns\u00b7ten", "von", "uns", "Drei\u00b7en", "reicht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "CARD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sprach\u2019s; und Paris ohne Schwanken", "tokens": ["Sie", "sprach's", ";", "und", "Pa\u00b7ris", "oh\u00b7ne", "Schwan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "NE", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm hin das Pfand in guter Ruh,", "tokens": ["Nahm", "hin", "das", "Pfand", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und warf es, anmuthvoll, der schlanken", "tokens": ["Und", "warf", "es", ",", "an\u00b7muth\u00b7voll", ",", "der", "schlan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der meerentstiegnen Venus zu.", "tokens": ["Der", "mee\u00b7rent\u00b7stieg\u00b7nen", "Ve\u00b7nus", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So war\u2019s vordem. Jetzt freilich schweigen", "tokens": ["So", "wa\u00b7r's", "vor\u00b7dem", ".", "Jetzt", "frei\u00b7lich", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "$.", "ADV", "ADV", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Himmel tiefer wie das Grab,", "tokens": ["Die", "Him\u00b7mel", "tie\u00b7fer", "wie", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keine G\u00f6tterkinder steigen", "tokens": ["Und", "kei\u00b7ne", "G\u00f6t\u00b7ter\u00b7kin\u00b7der", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr vom Olymp zu uns herab;", "tokens": ["Mehr", "vom", "O\u00b7lymp", "zu", "uns", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch guten Klang, traun wie vor Zeiten,", "tokens": ["Doch", "gu\u00b7ten", "Klang", ",", "traun", "wie", "vor", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat immer noch was \u201e", "tokens": ["Hat", "im\u00b7mer", "noch", "was", "\u201e"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PWS", "$("], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.7": {"text": "Zumal wenn es den Eingeweihten", "tokens": ["Zu\u00b7mal", "wenn", "es", "den", "Ein\u00b7ge\u00b7weih\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit s\u00fc\u00dfem G\u00f6tterhonig speist.", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "G\u00f6t\u00b7ter\u00b7ho\u00b7nig", "speist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und immer noch zu ", "tokens": ["Und", "im\u00b7mer", "noch", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.2": {"text": "Streckt sich manch\u2019 Sch\u00e4fer auf die Trift,", "tokens": ["Streckt", "sich", "man\u00b7ch'", "Sch\u00e4\u00b7fer", "auf", "die", "Trift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn keine G\u00f6ttin auch, mit Gr\u00fc\u00dfen,", "tokens": ["Wenn", "kei\u00b7ne", "G\u00f6t\u00b7tin", "auch", ",", "mit", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die blauen L\u00fcfte mehr durchschifft.", "tokens": ["Die", "blau\u00b7en", "L\u00fcf\u00b7te", "mehr", "durch\u00b7schifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sch\u00e4fer unsrer Tage werden", "tokens": ["Die", "Sch\u00e4\u00b7fer", "uns\u00b7rer", "Ta\u00b7ge", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um den ", "tokens": ["Um", "den"], "token_info": ["word", "word"], "pos": ["KOUI", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Sie reichen ihrem Gott ", "tokens": ["Sie", "rei\u00b7chen", "ih\u00b7rem", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}}}}