{"textgrid.poem.37746": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fccklich bist du junger Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich bist du junger Mann,", "tokens": ["Gl\u00fcck\u00b7lich", "bist", "du", "jun\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ein Weib gefunden,", "tokens": ["Der", "ein", "Weib", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die kein andrer leiden kann,", "tokens": ["Die", "kein", "an\u00b7drer", "lei\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du bist fest verbunden;", "tokens": ["Du", "bist", "fest", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ohne Argwohn bleibt dein Herz", "tokens": ["Oh\u00b7ne", "Arg\u00b7wohn", "bleibt", "dein", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei der Jungfrau Kranze,", "tokens": ["Bei", "der", "Jung\u00b7frau", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Lachend weicht der leere Scherz", "tokens": ["La\u00b7chend", "weicht", "der", "lee\u00b7re", "Scherz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem Hochzeittanze.", "tokens": ["Von", "dem", "Hoch\u00b7zeit\u00b7tan\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Du nur kennest ihren Reitz,", "tokens": ["Du", "nur", "ken\u00b7nest", "ih\u00b7ren", "Reitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Andern scheint sie h\u00e4\u00dflich,", "tokens": ["An\u00b7dern", "scheint", "sie", "h\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch sie bringet deinem Geitz", "tokens": ["Doch", "sie", "brin\u00b7get", "dei\u00b7nem", "Geitz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Reichthum unerme\u00dflich;", "tokens": ["Reicht\u00b7hum", "un\u00b7er\u00b7me\u00df\u00b7lich", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Geld zu z\u00e4hlen ist dein Gl\u00fcck,", "tokens": ["Geld", "zu", "z\u00e4h\u00b7len", "ist", "dein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe st\u00f6rt dich nimmer,", "tokens": ["Lie\u00b7be", "st\u00f6rt", "dich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und du siehst im schielen Blick", "tokens": ["Und", "du", "siehst", "im", "schie\u00b7len", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur des Gelds Geflimmer.", "tokens": ["Nur", "des", "Gelds", "Ge\u00b7flim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hat Rubinen im Gesicht", "tokens": ["Hat", "Ru\u00b7bi\u00b7nen", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Satan ihr gedroschen,", "tokens": ["Sa\u00b7tan", "ihr", "ge\u00b7dro\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist die eitle Lust doch nicht", "tokens": ["Ist", "die", "eit\u00b7le", "Lust", "doch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Weib erloschen;", "tokens": ["In", "dem", "Weib", "er\u00b7lo\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "T\u00e4glich t\u00fcncht sie wei\u00df und roth", "tokens": ["T\u00e4g\u00b7lich", "t\u00fcncht", "sie", "wei\u00df", "und", "roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihre falben Wangen,", "tokens": ["Ih\u00b7re", "fal\u00b7ben", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und in Kleidern \u00fcberbot", "tokens": ["Und", "in", "Klei\u00b7dern", "\u00fc\u00b7ber\u00b7bot"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle, die hier prangen.", "tokens": ["Al\u00b7le", ",", "die", "hier", "pran\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Eine Brille must du heut,", "tokens": ["Ei\u00b7ne", "Bril\u00b7le", "must", "du", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir zur Hochzeit kaufen,", "tokens": ["Dir", "zur", "Hoch\u00b7zeit", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deren Gl\u00e4ser allezeit", "tokens": ["De\u00b7ren", "Gl\u00e4\u00b7ser", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["PDS", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Russig angelaufen;", "tokens": ["Rus\u00b7sig", "an\u00b7ge\u00b7lau\u00b7fen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie zur Sonnenfinsterni\u00df", "tokens": ["Wie", "zur", "Son\u00b7nen\u00b7fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kannst du so sie schauen,", "tokens": ["Kannst", "du", "so", "sie", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und kein blendend Hinderni\u00df", "tokens": ["Und", "kein", "blen\u00b7dend", "Hin\u00b7der\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Macht dir vor ihr Grauen.", "tokens": ["Macht", "dir", "vor", "ihr", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Zweye machen stets ein Paar,", "tokens": ["Zwey\u00b7e", "ma\u00b7chen", "stets", "ein", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafe fest und lange,", "tokens": ["Schla\u00b7fe", "fest", "und", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eifersucht kr\u00fcmmt dir kein Haar,", "tokens": ["Ei\u00b7fer\u00b7sucht", "kr\u00fcmmt", "dir", "kein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Sei davor nicht bange;", "tokens": ["Sei", "da\u00b7vor", "nicht", "ban\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Weil es der Gewohnheit List", "tokens": ["Weil", "es", "der", "Ge\u00b7wohn\u00b7heit", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kriegst du bald auch Kinder,", "tokens": ["Kriegst", "du", "bald", "auch", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "T\u00f6chter, wie die Mutter ist,", "tokens": ["T\u00f6ch\u00b7ter", ",", "wie", "die", "Mut\u00b7ter", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4\u00dflich auch nicht minder.", "tokens": ["H\u00e4\u00df\u00b7lich", "auch", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Schlag nur alle Spiegel ein,", "tokens": ["Schlag", "nur", "al\u00b7le", "Spie\u00b7gel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh es morgen helle,", "tokens": ["Eh", "es", "mor\u00b7gen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doppelt w\u00e4r sonst deine Pein", "tokens": ["Dop\u00b7pelt", "w\u00e4r", "sonst", "dei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Armer Schlafgeselle;", "tokens": ["Ar\u00b7mer", "Schlaf\u00b7ge\u00b7sel\u00b7le", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Leite ab den glatten Bach,", "tokens": ["Lei\u00b7te", "ab", "den", "glat\u00b7ten", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df kein Silber putzen,", "tokens": ["La\u00df", "kein", "Sil\u00b7ber", "put\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df bei ihrem Bild kein Ach", "tokens": ["Da\u00df", "bei", "ih\u00b7rem", "Bild", "kein", "Ach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann der Liebe trutzen.", "tokens": ["Kann", "der", "Lie\u00b7be", "trut\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Zieh mit ihr ins Morgenland,", "tokens": ["Zieh", "mit", "ihr", "ins", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Weib den Schleier", "tokens": ["Wo", "das", "Weib", "den", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", 