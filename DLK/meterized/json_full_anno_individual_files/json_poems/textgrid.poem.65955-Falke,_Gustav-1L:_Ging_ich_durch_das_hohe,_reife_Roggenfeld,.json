{"textgrid.poem.65955": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ging ich durch das hohe, reife Roggenfeld,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ging ich durch das hohe, reife Roggenfeld,", "tokens": ["Ging", "ich", "durch", "das", "ho\u00b7he", ",", "rei\u00b7fe", "Rog\u00b7gen\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "War voll Morgensonnenschein ringsum die Welt.", "tokens": ["War", "voll", "Mor\u00b7gen\u00b7son\u00b7nen\u00b7schein", "ring\u00b7sum", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch die gelbe, blanke Sommerherrlichkeit", "tokens": ["Durch", "die", "gel\u00b7be", ",", "blan\u00b7ke", "Som\u00b7mer\u00b7herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wand versteckt der Weg sich und nur furchenbreit.", "tokens": ["Wand", "ver\u00b7steckt", "der", "Weg", "sich", "und", "nur", "fur\u00b7chen\u00b7breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PRF", "KON", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Kam in leichtem Wiegeschritt von ungef\u00e4hr,", "tokens": ["Kam", "in", "leich\u00b7tem", "Wie\u00b7ge\u00b7schritt", "von", "un\u00b7ge\u00