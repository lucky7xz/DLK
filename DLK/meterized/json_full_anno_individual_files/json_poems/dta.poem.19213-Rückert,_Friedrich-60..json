{"dta.poem.19213": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.57", "da:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem edleren Metall ist vom unedlern immer", "tokens": ["Dem", "ed\u00b7le\u00b7ren", "Me\u00b7tall", "ist", "vom", "u\u00b7ned\u00b7lern", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein wenig beigemischt, das schw\u00e4cht nicht dessen Schimmer,", "tokens": ["Ein", "we\u00b7nig", "bei\u00b7ge\u00b7mischt", ",", "das", "schw\u00e4cht", "nicht", "des\u00b7sen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PDS", "VVFIN", "PTKNEG", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verf\u00e4lscht nicht dessen Gu\u00df; nur da\u00df es viel nicht sei,", "tokens": ["Ver\u00b7f\u00e4lscht", "nicht", "des\u00b7sen", "Gu\u00df", ";", "nur", "da\u00df", "es", "viel", "nicht", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PRELAT", "NN", "$.", "ADV", "KOUS", "PPER", "ADV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst wird zu Kupfer Gold und aus dem Silber Blei.", "tokens": ["Sonst", "wird", "zu", "Kup\u00b7fer", "Gold", "und", "aus", "dem", "Sil\u00b7ber", "Blei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}