{"dta.poem.20116": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende Douglas.  \n (Schlacht von Langside. 1568.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Heere stie\u00dfen an einander; der Tag ist", "tokens": ["Die", "Hee\u00b7re", "stie\u00b7\u00dfen", "an", "ein\u00b7an\u00b7der", ";", "der", "Tag", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "hei\u00df, der Himmel finster,", "tokens": ["hei\u00df", ",", "der", "Him\u00b7mel", "fins\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vom Hufschlag dr\u00f6hnt weithin die Haide, roth", "tokens": ["Vom", "Huf\u00b7schlag", "dr\u00f6hnt", "weit\u00b7hin", "die", "Hai\u00b7de", ",", "roth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NE", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "tropft der Thau vom schwarzen Ginster;", "tokens": ["tropft", "der", "Thau", "vom", "schwar\u00b7zen", "Gins\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es blickt die schottische Maria von nahen Schlos-", "tokens": ["Es", "blickt", "die", "schot\u00b7ti\u00b7sche", "Ma\u00b7ria", "von", "na\u00b7hen", "Schlos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ses Fensterbr\u00fcstung,", "tokens": ["ses", "Fens\u00b7ter\u00b7br\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ihr Auge haftet auf dem Kampfe, doch ", "tokens": ["Ihr", "Au\u00b7ge", "haf\u00b7tet", "auf", "dem", "Kamp\u00b7fe", ",", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kampf auf ", "tokens": ["Kampf", "auf"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Dem jungen Douglas folgt ihr Auge; sie f\u00fchlt", "tokens": ["Dem", "jun\u00b7gen", "Doug\u00b7las", "folgt", "ihr", "Au\u00b7ge", ";", "sie", "f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "ihr Herze h\u00f6her schlagen,", "tokens": ["ihr", "Her\u00b7ze", "h\u00f6\u00b7her", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist's, der sechszehnj&#228;hrige Knabe, der aus", "tokens": ["Er", "ist's", ",", "der", "sechs\u00b7zehnj", "&#228;", "hri\u00b7ge", "Kna\u00b7be", ",", "der", "aus"], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "CARD", "$(", "ADJA", "NN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "dem Kerker sie getragen,", "tokens": ["dem", "Ker\u00b7ker", "sie", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er ist\u2019s, der ihr ein Heer geworben, und durfte", "tokens": ["Er", "ist's", ",", "der", "ihr", "ein", "Heer", "ge\u00b7wor\u00b7ben", ",", "und", "durf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,", "KON", "VMFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "doch um ", "tokens": ["doch", "um"], "token_info": ["word", "word"], "pos": ["ADV", "KOUI"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Drum wirbt er jetzt um seinen Frieden und", "tokens": ["Drum", "wirbt", "er", "jetzt", "um", "sei\u00b7nen", "Frie\u00b7den", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "um das Gl\u00fcck f\u00fcr sie zu sterben.", "tokens": ["um", "das", "Gl\u00fcck", "f\u00fcr", "sie", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wen tragen aus dem Kampfget\u00fcmmel sie dort", "tokens": ["Wen", "tra\u00b7gen", "aus", "dem", "Kampf\u00b7ge\u00b7t\u00fcm\u00b7mel", "sie", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "auf zweiggeflochtner Bahre,", "tokens": ["auf", "zweig\u00b7ge\u00b7flocht\u00b7ner", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Antlitz wei\u00df, und schwarz die R\u00fcstung", "tokens": ["Das", "Ant\u00b7litz", "wei\u00df", ",", "und", "schwarz", "die", "R\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und roth von Blut die blonden Haare?!", "tokens": ["und", "roth", "von", "Blut", "die", "blon\u00b7den", "Haa\u00b7re", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Douglas ist\u2019s: Erf\u00fcllung wurde des Hoff-", "tokens": ["Der", "Doug\u00b7las", "ist's", ":", "Er\u00b7f\u00fcl\u00b7lung", "wur\u00b7de", "des", "Hoff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$.", "NN", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "nungslosen einzgem Hoffen,", "tokens": ["nungs\u00b7lo\u00b7sen", "einz\u00b7gem", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es hat ein Schwert von Murray\u2019s Mannen", "tokens": ["Es", "hat", "ein", "Schwert", "von", "Mur\u00b7ray's", "Man\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "in\u2019s tiefste Leben ihn getroffen.", "tokens": ["in's", "tiefs\u00b7te", "Le\u00b7ben", "ihn", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da liegt er, auf gewirktem Teppich, jetzt an", "tokens": ["Da", "liegt", "er", ",", "auf", "ge\u00b7wirk\u00b7tem", "Tep\u00b7pich", ",", "jetzt", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "PTKVZ"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "des alten Schlosses Stufen,", "tokens": ["des", "al\u00b7ten", "Schlos\u00b7ses", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Maria neigt sich zu ihm nieder, ein Priester", "tokens": ["Ma\u00b7ria", "neigt", "sich", "zu", "ihm", "nie\u00b7der", ",", "ein", "Pries\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "wird herbeigerufen,", "tokens": ["wird", "her\u00b7bei\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der reicht den Kelch ihm unter Thr&#228;nen, Er", "tokens": ["Der", "reicht", "den", "Kelch", "ihm", "un\u00b7ter", "Thr", "&#228;", "nen", ",", "Er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$(", "VVINF", "$,", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "aber segnet diese Stunde,", "tokens": ["a\u00b7ber", "seg\u00b7net", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4tt\u2019 langsam sonst verbluten m\u00fcssen an seines", "tokens": ["H\u00e4tt'", "lang\u00b7sam", "sonst", "ver\u00b7blu\u00b7ten", "m\u00fcs\u00b7sen", "an", "sei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "VVINF", "VMFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Herzens stiller Wunde.", "tokens": ["Her\u00b7zens", "stil\u00b7ler", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Die Brust wird kalt, es stockt sein Athem, sein", "tokens": ["Die", "Brust", "wird", "kalt", ",", "es", "stockt", "sein", "A\u00b7them", ",", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auge scheint vom Tod geschlossen,", "tokens": ["Au\u00b7ge", "scheint", "vom", "Tod", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Maria k\u00fcsst die bleiche Stirne, die schon so", "tokens": ["Ma\u00b7ria", "k\u00fcsst", "die", "blei\u00b7che", "Stir\u00b7ne", ",", "die", "schon", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "fr\u00fche Ruhm genossen:", "tokens": ["fr\u00fc\u00b7he", "Ruhm", "ge\u00b7nos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da spielt um seinen Mund ein L\u00e4cheln, auf-", "tokens": ["Da", "spielt", "um", "sei\u00b7nen", "Mund", "ein", "L\u00e4\u00b7cheln", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "glimmt ein letzter Lebensfunken,", "tokens": ["glimmt", "ein", "letz\u00b7ter", "Le\u00b7bens\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann ist er in Maria\u2019s Arme zu tiefstem Schlaf", "tokens": ["Dann", "ist", "er", "in", "Ma\u00b7ria's", "Ar\u00b7me", "zu", "tiefs\u00b7tem", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "zur\u00fcckgesunken.", "tokens": ["zu\u00b7r\u00fcck\u00b7ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}