{"textgrid.poem.26161": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Das Forsthaus", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Forsthaus sieht seit hundert Jahren die Waldwiese an,", "tokens": ["Das", "Forst\u00b7haus", "sieht", "seit", "hun\u00b7dert", "Jah\u00b7ren", "die", "Wald\u00b7wie\u00b7se", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "CARD", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und jeden Fr\u00fchling erscheinen ihm wieder Salbei und Thymian,", "tokens": ["Und", "je\u00b7den", "Fr\u00fch\u00b7ling", "er\u00b7schei\u00b7nen", "ihm", "wie\u00b7der", "Sal\u00b7bei", "und", "Thy\u00b7mi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADV", "NN", "KON", "NE", "$,"], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und wei\u00dfer Staub zieht auf der Waldstra\u00dfe an ihm vorbei.", "tokens": ["Und", "wei\u00b7\u00dfer", "Staub", "zieht", "auf", "der", "Wald\u00b7stra\u00b7\u00dfe", "an", "ihm", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Jagdhunde lungern im Staub dort zur Mittagsstunde,", "tokens": ["Die", "Jagd\u00b7hun\u00b7de", "lun\u00b7gern", "im", "Staub", "dort", "zur", "Mit\u00b7tags\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "wor