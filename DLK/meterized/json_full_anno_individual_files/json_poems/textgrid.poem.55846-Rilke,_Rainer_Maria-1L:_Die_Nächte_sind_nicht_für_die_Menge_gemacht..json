{"textgrid.poem.55846": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die N\u00e4chte sind nicht f\u00fcr die Menge gemacht.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die N\u00e4chte sind nicht f\u00fcr die Menge gemacht.", "tokens": ["Die", "N\u00e4ch\u00b7te", "sind", "nicht", "f\u00fcr", "die", "Men\u00b7ge", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Von deinem Nachbar trennt dich die Nacht,", "tokens": ["Von", "dei\u00b7nem", "Nach\u00b7bar", "trennt", "dich", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "und du sollst ihn nicht suchen trotzdem.", "tokens": ["und", "du", "sollst", "ihn", "nicht", "su\u00b7chen", "trotz\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und machst du nachts deine Stube licht,", "tokens": ["Und", "machst", "du", "nachts", "dei\u00b7ne", "Stu\u00b7be", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "um Menschen zu schauen ins Angesicht,", "tokens": ["um", "Men\u00b7schen", "zu", "schau\u00b7en", "ins", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "so mu\u00dft du bedenken: wem.", "tokens": ["so", "mu\u00dft", "du", "be\u00b7den\u00b7ken", ":", "wem", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "PWS", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Menschen sind furchtbar vom Licht entstellt,", "tokens": ["Die", "Men\u00b7schen", "sind", "furcht\u00b7bar", "vom", "Licht", "ent\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "das von ihren Gesichtern tr\u00e4uft,", "tokens": ["das", "von", "ih\u00b7ren", "Ge\u00b7sich\u00b7tern", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "und haben sie nachts sich zusammengesellt,", "tokens": ["und", "ha\u00b7ben", "sie", "nachts", "sich", "zu\u00b7sam\u00b7men\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PRF", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "so schaust du eine wankende Welt", "tokens": ["so", "schaust", "du", "ei\u00b7ne", "wan\u00b7ken\u00b7de", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "durcheinandergeh\u00e4uft.", "tokens": ["durch\u00b7ein\u00b7an\u00b7der\u00b7ge\u00b7h\u00e4uft", "."], "token