{"dta.poem.8163": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Frauenhand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Wird \u00fcber deine Lippen gehen;", "tokens": ["Wird", "\u00fc\u00b7ber", "dei\u00b7ne", "Lip\u00b7pen", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch, was so sanft dein Mund verschweigt,", "tokens": ["Doch", ",", "was", "so", "sanft", "dein", "Mund", "ver\u00b7schweigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df deine blasse Hand gestehen.", "tokens": ["Mu\u00df", "dei\u00b7ne", "blas\u00b7se", "Hand", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Hand, an der mein Auge h\u00e4ngt,", "tokens": ["Die", "Hand", ",", "an", "der", "mein", "Au\u00b7ge", "h\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt jenen feinen Zug der Schmerzen,", "tokens": ["Zeigt", "je\u00b7nen", "fei\u00b7nen", "Zug", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df in schlummerloser Nacht", "tokens": ["Und", "da\u00df", "in", "schlum\u00b7mer\u00b7lo\u00b7ser", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lag auf einem kranken Herzen.", "tokens": ["Sie", "lag", "auf", "ei\u00b7nem", "kran\u00b7ken", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}