{"dta.poem.18341": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Dichter und kein Hund!", "tokens": ["Ich", "bin", "ein", "Dich\u00b7ter", "und", "kein", "Hund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin ein freier Mann und nehme", "tokens": ["Ich", "bin", "ein", "frei\u00b7er", "Mann", "und", "neh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Feigenblatt vor meinen Mund.", "tokens": ["Kein", "Fei\u00b7gen\u00b7blatt", "vor", "mei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich seh die Welt im Dunkeln tappen,", "tokens": ["Ich", "seh", "die", "Welt", "im", "Dun\u00b7keln", "tap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich weise golden ihr ein Ziel,", "tokens": ["Ich", "wei\u00b7se", "gol\u00b7den", "ihr", "ein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und erst am letzten morschen Wappen", "tokens": ["Und", "erst", "am", "letz\u00b7ten", "mor\u00b7schen", "Wap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerschmettre ich jubelnd mein Saitenspiel!", "tokens": ["Zer\u00b7schmett\u00b7re", "ich", "ju\u00b7belnd", "mein", "Sai\u00b7ten\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}