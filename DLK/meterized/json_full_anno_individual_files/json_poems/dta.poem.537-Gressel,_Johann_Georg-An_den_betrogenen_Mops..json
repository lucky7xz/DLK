{"dta.poem.537": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den betrogenen  Mops.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mops thu die Augen auf/ und la\u00df die Narren-Possen/", "tokens": ["Mops", "thu", "die", "Au\u00b7gen", "auf", "/", "und", "la\u00df", "die", "Nar\u00b7ren\u00b7Pos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "$(", "KON", "VVIMP", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du must doch immerfort/ sonst nichts/ als W\u00e4chter seyn/", "tokens": ["Du", "must", "doch", "im\u00b7mer\u00b7fort", "/", "sonst", "nichts", "/", "als", "W\u00e4ch\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$(", "ADV", "PIS", "$(", "KOUS", "NE", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist mit Haasen-Schrot bis auf den Tod geschossen/", "tokens": ["Du", "bist", "mit", "Haa\u00b7sen\u00b7Schrot", "bis", "auf", "den", "Tod", "ge\u00b7schos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Clarinde, die betriegt dich mehr als ungemein.", "tokens": ["Cla\u00b7rin\u00b7de", ",", "die", "be\u00b7triegt", "dich", "mehr", "als", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "PPER", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Einjeder schickt dich aus/ du must von dannen weichen", "tokens": ["Ein\u00b7je\u00b7der", "schickt", "dich", "aus", "/", "du", "must", "von", "dan\u00b7nen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$(", "PPER", "VMFIN", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ein verliebtes Schiff den Haven suchen will/", "tokens": ["Wenn", "ein", "ver\u00b7lieb\u00b7tes", "Schiff", "den", "Ha\u00b7ven", "su\u00b7chen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du stehest auf der Hut/ sie darf niemand beschleichen/", "tokens": ["Du", "ste\u00b7hest", "auf", "der", "Hut", "/", "sie", "darf", "nie\u00b7mand", "be\u00b7schlei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mein/ lege doch einst ab den ungeschliffnen Brill.", "tokens": ["Mein", "/", "le\u00b7ge", "doch", "einst", "ab", "den", "un\u00b7ge\u00b7schliff\u00b7nen", "Brill", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie nennt dich zwar mein Mann/ doch must du das entbehren/", "tokens": ["Sie", "nennt", "dich", "zwar", "mein", "Mann", "/", "doch", "must", "du", "das", "ent\u00b7beh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$(", "ADV", "VMFIN", "PPER", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was dir mit recht geh\u00f6rt/ weil du nicht gut genug", "tokens": ["Was", "dir", "mit", "recht", "ge\u00b7h\u00f6rt", "/", "weil", "du", "nicht", "gut", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJD", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein/ la\u00df dich doch nicht mehr von deiner Frau beth\u00f6ren/", "tokens": ["Mein", "/", "la\u00df", "dich", "doch", "nicht", "mehr", "von", "dei\u00b7ner", "Frau", "be\u00b7th\u00f6\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du bist ein guter Mann/ doch aber nicht recht klug.", "tokens": ["Du", "bist", "ein", "gu\u00b7ter", "Mann", "/", "doch", "a\u00b7ber", "nicht", "recht", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mops thu die F\u00e4uste auf/ und hege kein Erbarmen/", "tokens": ["Mops", "thu", "die", "F\u00e4us\u00b7te", "auf", "/", "und", "he\u00b7ge", "kein", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "$(", "KON", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nimm einen Stock zur Hand/ und schlage auf dein Weib/", "tokens": ["Nimm", "ei\u00b7nen", "Stock", "zur", "Hand", "/", "und", "schla\u00b7ge", "auf", "dein", "Weib", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Schau/ wie der Buhler kan auf ihrer Brust erwarmen/", "tokens": ["Schau", "/", "wie", "der", "Buh\u00b7ler", "kan", "auf", "ih\u00b7rer", "Brust", "er\u00b7war\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Was dir versprochen ist/ ist ihm ein Zeit-Vertreib.", "tokens": ["Was", "dir", "ver\u00b7spro\u00b7chen", "ist", "/", "ist", "ihm", "ein", "Zeit\u00b7Ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$(", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch Nein! du wirst wol ", "tokens": ["Doch", "Nein", "!", "du", "wirst", "wol"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Du bist gantz wohl vergn\u00fcgt/ wenn du nur Suppen fri\u00dft/", "tokens": ["Du", "bist", "gantz", "wohl", "ver\u00b7gn\u00fcgt", "/", "wenn", "du", "nur", "Sup\u00b7pen", "fri\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$(", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein Weib darf dich ins Buch der grossen Gilde schreiben", "tokens": ["Dein", "Weib", "darf", "dich", "ins", "Buch", "der", "gros\u00b7sen", "Gil\u00b7de", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn nur ein Doppelt-Marck dein Lohn der Wache ist.", "tokens": ["Wenn", "nur", "ein", "Dop\u00b7pel\u00b7tMa\u00b7rck", "dein", "Lohn", "der", "Wa\u00b7che", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}