{"dta.poem.18662": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Im  \n Gegenw\u00e4rtigen Vergangnes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ros\u2019 und Lilie morgenthaulich", "tokens": ["Ros'", "und", "Li\u00b7lie", "mor\u00b7gent\u00b7hau\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcht im Garten meiner N\u00e4he,", "tokens": ["Bl\u00fcht", "im", "Gar\u00b7ten", "mei\u00b7ner", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinten an bebuscht und traulich", "tokens": ["Hin\u00b7ten", "an", "be\u00b7buscht", "und", "trau\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steigt der Felsen in die H\u00f6he.", "tokens": ["Steigt", "der", "Fel\u00b7sen", "in", "die", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit hohem Wald umzogen,", "tokens": ["Und", "mit", "ho\u00b7hem", "Wald", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit Ritterschloss gekr\u00f6net,", "tokens": ["Und", "mit", "Rit\u00b7ter\u00b7schloss", "ge\u00b7kr\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lenkt sich hin des Gipfels Bogen,", "tokens": ["Lenkt", "sich", "hin", "des", "Gip\u00b7fels", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis er sich dem Thal vers\u00f6hnet.", "tokens": ["Bis", "er", "sich", "dem", "Thal", "ver\u00b7s\u00f6h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und da duftets wie vor Alters,", "tokens": ["Und", "da", "duf\u00b7tets", "wie", "vor", "Al\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da wir noch von Liebe litten,", "tokens": ["Da", "wir", "noch", "von", "Lie\u00b7be", "lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Saiten meines Psalters", "tokens": ["Und", "die", "Sai\u00b7ten", "mei\u00b7nes", "Psal\u00b7ters"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Morgenstrahl sich stritten.", "tokens": ["Mit", "dem", "Mor\u00b7gen\u00b7strahl", "sich", "strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo das Jagdlied aus den B\u00fcschen,", "tokens": ["Wo", "das", "Jagd\u00b7lied", "aus", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fclle runden Tons enthauchte,", "tokens": ["F\u00fcl\u00b7le", "run\u00b7den", "Tons", "ent\u00b7hauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Anzufeuern, zu erfrischen", "tokens": ["An\u00b7zu\u00b7feu\u00b7ern", ",", "zu", "er\u00b7fri\u00b7schen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie\u2019s der Busen wollt\u2019 und brauchte.", "tokens": ["Wie's", "der", "Bu\u00b7sen", "wollt'", "und", "brauch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VMFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun die W\u00e4lder ewig sprossen", "tokens": ["Nun", "die", "W\u00e4l\u00b7der", "e\u00b7wig", "spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So ermuthigt euch mit diesen,", "tokens": ["So", "er\u00b7mut\u00b7higt", "euch", "mit", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Was ihr sonst f\u00fcr euch genossen", "tokens": ["Was", "ihr", "sonst", "f\u00fcr", "euch", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4sst in Andern sich geniessen.", "tokens": ["L\u00e4sst", "in", "An\u00b7dern", "sich", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Niemand wird uns dann beschreien", "tokens": ["Nie\u00b7mand", "wird", "uns", "dann", "be\u00b7schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dass wirs uns alleine g\u00f6nnen,", "tokens": ["Dass", "wirs", "uns", "al\u00b7lei\u00b7ne", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nun in allen Lebensreihen", "tokens": ["Nun", "in", "al\u00b7len", "Le\u00b7bens\u00b7rei\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fcsset ihr geniessen k\u00f6nnen.", "tokens": ["M\u00fcs\u00b7set", "ihr", "ge\u00b7nies\u00b7sen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mit diesem Lied und Wendung", "tokens": ["Und", "mit", "die\u00b7sem", "Lied", "und", "Wen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wir wieder bey Hafisen", "tokens": ["Sind", "wir", "wie\u00b7der", "bey", "Ha\u00b7fi\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es ziemt des Tags Vollendung", "tokens": ["Denn", "es", "ziemt", "des", "Tags", "Vol\u00b7len\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Geniessern zu geniessen", "tokens": ["Mit", "Ge\u00b7nies\u00b7sern", "zu", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}