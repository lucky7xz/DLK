{"textgrid.poem.49920": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Dem Herrn Prior der Kartause J.", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben goldne Verse mir, phal\u00e4kische,", "tokens": ["Sie", "ha\u00b7ben", "gold\u00b7ne", "Ver\u00b7se", "mir", ",", "pha\u00b7l\u00e4\u00b7ki\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das zierlichste Latein, geschickt. Ich m\u00f6chte wohl", "tokens": ["Das", "zier\u00b7lichs\u00b7te", "La\u00b7tein", ",", "ge\u00b7schickt", ".", "Ich", "m\u00f6ch\u00b7te", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$.", "PPER", "VMFIN", "ADV"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie gleicherweis erwidern; doch mit gutem Grund", "tokens": ["Sie", "glei\u00b7cher\u00b7weis", "er\u00b7wi\u00b7dern", ";", "doch", "mit", "gu\u00b7tem", "Grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Enthalt ich mich des Wagest\u00fccks, Vortrefflicher!", "tokens": ["Ent\u00b7halt", "ich", "mich", "des", "Wa\u00b7ge\u00b7st\u00fccks", ",", "Vor\u00b7treff\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "Kein Wunder, wenn ein grundgelehrter Freund Sie nur", "tokens": ["Kein", "Wun\u00b7der", ",", "wenn", "ein", "grund\u00b7ge\u00b7lehr\u00b7ter", "Freund", "Sie", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den zweiten Pater elegantiarum nennt.", "tokens": ["Den", "zwei\u00b7ten", "Pa\u00b7ter", "e\u00b7leg\u00b7an\u00b7ti\u00b7a\u00b7rum", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Etwas bedenklich scheint es zwar, ich mu\u00df gestehn,", "tokens": ["Et\u00b7was", "be\u00b7denk\u00b7lich", "scheint", "es", "zwar", ",", "ich", "mu\u00df", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ein Herr Prior, Prior des Kart\u00e4userstifts,", "tokens": ["Da\u00df", "ein", "Herr", "Pri\u00b7or", ",", "Pri\u00b7or", "des", "Kar\u00b7t\u00e4u\u00b7ser\u00b7stifts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit unserm Veroneser wettzueifern sich", "tokens": ["Mit", "un\u00b7serm", "Ve\u00b7ro\u00b7ne\u00b7ser", "wett\u00b7zu\u00b7ei\u00b7fern", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Inallewege als berufnen Meister zeigt.", "tokens": ["I\u00b7nal\u00b7le\u00b7we\u00b7ge", "als", "be\u00b7ruf\u00b7nen", "Meis\u00b7ter", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn Ihr Herr Bischof das erf\u00fchre! \u2013 doch es soll,", "tokens": ["Wenn", "Ihr", "Herr", "Bi\u00b7schof", "das", "er\u00b7f\u00fch\u00b7re", "!", "\u2013", "doch", "es", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "PDS", "VVFIN", "$.", "$(", "KON", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was \u00fcber allen T\u00fcren Ihres Klosters steht,", "tokens": ["Was", "\u00fc\u00b7ber", "al\u00b7len", "T\u00fc\u00b7ren", "Ih\u00b7res", "Klos\u00b7ters", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "An Pfosten, G\u00e4ngen, selbst am heimlichen Gemach,", "tokens": ["An", "Pfos\u00b7ten", ",", "G\u00e4n\u00b7gen", ",", "selbst", "am", "heim\u00b7li\u00b7chen", "Ge\u00b7mach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Silentium! \u2013 das strenge Wort, mir heilig sein.", "tokens": ["Si\u00b7len\u00b7ti\u00b7um", "!", "\u2013", "das", "stren\u00b7ge", "Wort", ",", "mir", "hei\u00b7lig", "sein", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "ART", "ADJA", "NN", "$,", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In wenig Tagen komm ich selbst; schon lange lockt", "tokens": ["In", "we\u00b7nig", "Ta\u00b7gen", "komm", "ich", "selbst", ";", "schon", "lan\u00b7ge", "lockt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die neue M\u00e4rzensonne mich. Dann find ich wohl", "tokens": ["Die", "neu\u00b7e", "M\u00e4r\u00b7zen\u00b7son\u00b7ne", "mich", ".", "Dann", "find", "ich", "wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Garten fr\u00fche meinen stattlich muntern Greis,", "tokens": ["Im", "Gar\u00b7ten", "fr\u00fc\u00b7he", "mei\u00b7nen", "statt\u00b7lich", "mun\u00b7tern", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besch\u00e4ftigt, wilder Rosenst\u00e4mmchen jungem Blut", "tokens": ["Be\u00b7sch\u00e4f\u00b7tigt", ",", "wil\u00b7der", "Ro\u00b7sen\u00b7st\u00e4mm\u00b7chen", "jun\u00b7gem", "Blut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch f\u00fcrstlichen Gez\u00fcchtes eingepflanzten Keim", "tokens": ["Durch", "f\u00fcrst\u00b7li\u00b7chen", "Ge\u00b7z\u00fcch\u00b7tes", "ein\u00b7ge\u00b7pflanz\u00b7ten", "Keim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Holdsel'ge Kinder zu vertraun; von weitem schon", "tokens": ["Hold\u00b7sel'\u00b7ge", "Kin\u00b7der", "zu", "ver\u00b7traun", ";", "von", "wei\u00b7tem", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$.", "APPR", "PIS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ruft er sein Salve, und behend entgegen mir", "tokens": ["Ruft", "er", "sein", "Sal\u00b7ve", ",", "und", "be\u00b7hend", "ent\u00b7ge\u00b7gen", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "ADJD", "APPR", "PPER"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Den breiten Sandweg, weichen Trittes, schreitet er,", "tokens": ["Den", "brei\u00b7ten", "Sand\u00b7weg", ",", "wei\u00b7chen", "Trit\u00b7tes", ",", "schrei\u00b7tet", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Im langen Ordenskleide, wollig, wei\u00df wie Schnee.", "tokens": ["Im", "lan\u00b7gen", "Or\u00b7dens\u00b7klei\u00b7de", ",", "wol\u00b7lig", ",", "wei\u00df", "wie", "Schnee", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "$,", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Inzwischen hier ein Hundert Schnecken, wenn's beliebt!", "tokens": ["I\u00b7nzwi\u00b7schen", "hier", "ein", "Hun\u00b7dert", "Schne\u00b7cken", ",", "wenn's", "be\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "CARD", "NN", "$,", "KOUS", "ADJD", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ich f\u00fcgte gern ein St\u00fcckchen Rotwild noch hinzu,", "tokens": ["Ich", "f\u00fcg\u00b7te", "gern", "ein", "St\u00fcck\u00b7chen", "Rot\u00b7wild", "noch", "hin\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das mir der F\u00f6rster heut geschenkt, doch f\u00e4llt mir ein,", "tokens": ["Das", "mir", "der", "F\u00f6rs\u00b7ter", "heut", "ge\u00b7schenkt", ",", "doch", "f\u00e4llt", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df man nicht Pater elegantiarum nur,", "tokens": ["Da\u00df", "man", "nicht", "Pa\u00b7ter", "e\u00b7leg\u00b7an\u00b7ti\u00b7a\u00b7rum", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielmehr auch Pater esuritionum", "tokens": ["Viel\u00b7mehr", "auch", "Pa\u00b7ter", "e\u00b7su\u00b7ri\u00b7ti\u00b7o\u00b7num"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "NE"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie haben goldne Verse mir, phal\u00e4kische,", "tokens": ["Sie", "ha\u00b7ben", "gold\u00b7ne", "Ver\u00b7se", "m