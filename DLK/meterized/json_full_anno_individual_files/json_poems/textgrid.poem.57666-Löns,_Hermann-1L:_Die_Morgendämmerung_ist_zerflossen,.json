{"textgrid.poem.57666": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Morgend\u00e4mmerung ist zerflossen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Morgend\u00e4mmerung ist zerflossen,", "tokens": ["Die", "Mor\u00b7gen\u00b7d\u00e4m\u00b7me\u00b7rung", "ist", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Sonne \u00fcber die W\u00e4lder loht,", "tokens": ["Die", "Son\u00b7ne", "\u00fc\u00b7ber", "die", "W\u00e4l\u00b7der", "loht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich gr\u00fc\u00dfe mit gro\u00dfen, frohen Augen", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "mit", "gro\u00b7\u00dfen", ",", "fro\u00b7hen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das flammende, leuchtende Morgenrot.", "tokens": ["Das", "flam\u00b7men\u00b7de", ",", "leuch\u00b7ten\u00b7de", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "O Morgenrot, so lange Jahre", "tokens": ["O", "Mor\u00b7gen\u00b7rot", ",", "so", "lan\u00b7ge", "Jah\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich gegangen in schwarzer Nacht,", "tokens": ["Bin", "ich", "ge\u00b7gan\u00b7gen", "in", "schwar\u00b7zer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mir bl\u00fchet keine helle Blume,", "tokens": ["Mir", "bl\u00fc\u00b7het", "kei\u00b7ne", "hel\u00b7le", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir hat kein Sonnenschein gelacht.", "tokens": ["Mir", "hat", "kein", "Son\u00b7nen\u00b7schein", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es fiel kein Lichtschein in mein Dunkel,", "tokens": ["Es", "fiel", "kein", "Licht\u00b7schein", "in", "mein", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Stern an meinem Himmel stand,", "tokens": ["Kein", "Stern", "an", "mei\u00b7nem", "Him\u00b7mel", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit kalter Seele, totem Herzen,", "tokens": ["Mit", "kal\u00b7ter", "See\u00b7le", ",", "to\u00b7tem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ging ich durch das schwarze Land.", "tokens": ["So", "ging", "ich", "durch", "das", "schwar\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Blumen bl\u00fchn, die V\u00f6gel singen,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fchn", ",", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wonnig warm die Sonne lacht,", "tokens": ["So", "won\u00b7nig", "warm", "die", "Son\u00b7ne", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast mit deiner hohen Liebe", "tokens": ["Du", "hast", "mit", "dei\u00b7ner", "ho\u00b7hen", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs neue mich zur Welt gebracht.", "tokens": ["Aufs", "neu\u00b7e", "mich", "zur", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Morgend\u00e4mmerung ist zerflossen,", "tokens": ["Die", "Mor\u00b7gen\u00b7d\u00e4m\u00b7me\u00b7rung", "ist", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"