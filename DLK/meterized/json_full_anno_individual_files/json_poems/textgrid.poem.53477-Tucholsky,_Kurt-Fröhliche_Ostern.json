{"textgrid.poem.53477": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00f6hliche Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da seht aufs neue dieses alte Wunder:", "tokens": ["Da", "seht", "aufs", "neu\u00b7e", "die\u00b7ses", "al\u00b7te", "Wun\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Osterhase kakelt wie ein Huhn", "tokens": ["Der", "Os\u00b7ter\u00b7ha\u00b7se", "ka\u00b7kelt", "wie", "ein", "Huhn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und fabriziert dort unter dem Holunder", "tokens": ["und", "fab\u00b7ri\u00b7ziert", "dort", "un\u00b7ter", "dem", "Ho\u00b7lun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein Ei und noch ein Ei und hat zu tun.", "tokens": ["ein", "Ei", "und", "noch", "ein", "Ei", "und", "hat", "zu", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "KON", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und auch der Mensch reckt frohbewegt die Glieder \u2013", "tokens": ["Und", "auch", "der", "Mensch", "reckt", "froh\u00b7be\u00b7wegt", "die", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON"