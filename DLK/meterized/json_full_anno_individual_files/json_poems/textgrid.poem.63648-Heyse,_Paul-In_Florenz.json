{"textgrid.poem.63648": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "In Florenz", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Florenz! O helle Tag' und N\u00e4chte,", "tokens": ["Flo\u00b7renz", "!", "O", "hel\u00b7le", "Tag'", "und", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einst hier verschw\u00e4rmt, wie liegt ihr weit!", "tokens": ["Einst", "hier", "ver\u00b7schw\u00e4rmt", ",", "wie", "liegt", "ihr", "weit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer einen Hauch uns wiederbr\u00e4chte", "tokens": ["Wer", "ei\u00b7nen", "Hauch", "uns", "wie\u00b7der\u00b7br\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wonnevollen Knospenzeit!", "tokens": ["Der", "won\u00b7ne\u00b7vol\u00b7len", "Knos\u00b7pen\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du noch so jung, so gl\u00fcckbeklommen,", "tokens": ["Du", "noch", "so", "jung", ",", "so", "gl\u00fcck\u00b7be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des G\u00f6tterneides unbewu\u00dft,", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7nei\u00b7des", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ich, der manchen Strom durchschwommen,", "tokens": ["Und", "ich", ",", "der", "man\u00b7chen", "Strom", "durch\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gelandet nun an deiner Brust!", "tokens": ["Ge\u00b7lan\u00b7det", "nun", "an", "dei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wei\u00dft du, wie in der Abendk\u00fchle", "tokens": ["Wei\u00dft", "du", ",", "wie", "in", "der", "A\u00b7bend\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wandelten den Flu\u00df entlang,", "tokens": ["Wir", "wan\u00b7del\u00b7ten", "den", "Flu\u00df", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPO", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie z\u00e4rtlich fest sich im Gew\u00fchle", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "fest", "sich", "im", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Arm um deine Schulter schlang?", "tokens": ["Mein", "Arm", "um", "dei\u00b7ne", "Schul\u00b7ter", "schlang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herab den Arno kam gefahren", "tokens": ["Her\u00b7ab", "den", "Ar\u00b7no", "kam", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NE", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Fackeln und Musik ein Kahn,", "tokens": ["Mit", "Fa\u00b7ckeln", "und", "Mu\u00b7sik", "ein", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da\u00df wir den Widerschein, den klaren,", "tokens": ["Da\u00df", "wir", "den", "Wi\u00b7der\u00b7schein", ",", "den", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In unsern Augen blitzen sahn.", "tokens": ["In", "un\u00b7sern", "Au\u00b7gen", "blit\u00b7zen", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dort im Mezzanin die Zimmer,", "tokens": ["Und", "dort", "im", "Mez\u00b7za\u00b7nin", "die", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unser junges Gl\u00fcck bewohnt,", "tokens": ["Die", "un\u00b7ser", "jun\u00b7ges", "Gl\u00fcck", "be\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo nachts mit seinem M\u00e4rchenschimmer", "tokens": ["Wo", "nachts", "mit", "sei\u00b7nem", "M\u00e4r\u00b7chen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verstohlen zu uns kam der Mond;", "tokens": ["Ver\u00b7stoh\u00b7len", "zu", "uns", "kam", "der", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn vor dem Spiegel du die Locken", "tokens": ["Wenn", "vor", "dem", "Spie\u00b7gel", "du", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir l\u00f6stest mit der schlanken Hand,", "tokens": ["Dir", "l\u00f6s\u00b7test", "mit", "der", "schlan\u00b7ken", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch stets ergl\u00fchend s\u00fc\u00dferschrocken,", "tokens": ["Noch", "stets", "er\u00b7gl\u00fc\u00b7hend", "s\u00fc\u00b7\u00dfer\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil dein Geliebter bei dir stand!", "tokens": ["Weil", "dein", "Ge\u00b7lieb\u00b7ter", "bei", "dir", "stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn ich dann beim Tageslichte", "tokens": ["Und", "wenn", "ich", "dann", "beim", "Ta\u00b7ges\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich durch die heitre Stadt gef\u00fchrt,", "tokens": ["Dich", "durch", "die", "heit\u00b7re", "Stadt", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wie ernstbem\u00fcht wir Kunstgeschichte", "tokens": ["Wie", "ernst\u00b7be\u00b7m\u00fcht", "wir", "Kunst\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Farb' und Stein und Erz studiert!", "tokens": ["In", "Fa\u00b7rb'", "und", "Stein", "und", "Erz", "stu\u00b7diert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Des Tizian himmlische Gestalten,", "tokens": ["Des", "Ti\u00b7zi\u00b7an", "himm\u00b7li\u00b7sche", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie r\u00fchrten kaum die Seele mir;", "tokens": ["Sie", "r\u00fchr\u00b7ten", "kaum", "die", "See\u00b7le", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kaum konnt' ich mich des Rufs enthalten:", "tokens": ["Kaum", "konnt'", "ich", "mich", "des", "Rufs", "ent\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich wei\u00df, was holder ist als ihr!", "tokens": ["Ich", "wei\u00df", ",", "was", "hol\u00b7der", "ist", "als", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADJA", "VAFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da sah vom hohen Fu\u00dfgestelle", "tokens": ["Da", "sah", "vom", "ho\u00b7hen", "Fu\u00df\u00b7ge\u00b7stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der eh'rne Perseus fremd mich an.", "tokens": ["Der", "eh'r\u00b7ne", "Per\u00b7seus", "fremd", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's wahr, schwerm\u00fctiger Geselle,", "tokens": ["Ist's", "wahr", ",", "schwer\u00b7m\u00fc\u00b7ti\u00b7ger", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du es einst mir angetan?", "tokens": ["Da\u00df", "du", "es", "einst", "mir", "an\u00b7ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich in hellen Jugendjahren", "tokens": ["Da\u00df", "ich", "in", "hel\u00b7len", "Ju\u00b7gend\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die M\u00e4r zu deuten wohl vermeint", "tokens": ["Die", "M\u00e4r", "zu", "deu\u00b7ten", "wohl", "ver\u00b7meint"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von jenem Haupt mit Schlangenhaaren,", "tokens": ["Von", "je\u00b7nem", "Haupt", "mit", "Schlan\u00b7gen\u00b7haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das sterbend dir die Welt versteint?", "tokens": ["Das", "ster\u00b7bend", "dir", "die", "Welt", "ver\u00b7steint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und jetzt \u2013 nur kurze Frist vergangen \u2013", "tokens": ["Und", "jetzt", "\u2013", "nur", "kur\u00b7ze", "Frist", "ver\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie anders kehren wir zur\u00fcck!", "tokens": ["Wie", "an\u00b7ders", "keh\u00b7ren", "wir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch h\u00e4lt mein Arm dich fest umfangen,", "tokens": ["Noch", "h\u00e4lt", "mein", "Arm", "dich", "fest", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch unterm Schleier weint mein Gl\u00fcck.", "tokens": ["Doch", "un\u00b7term", "Schlei\u00b7er", "weint", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du alles, was mir blieb vom Leben,", "tokens": ["Du", "al\u00b7les", ",", "was", "mir", "blieb", "vom", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So sterbensm\u00fcd, so still und bla\u00df \u2013", "tokens": ["So", "ster\u00b7bens\u00b7m\u00fcd", ",", "so", "still", "und", "bla\u00df", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich frage mit geheimem Beben:", "tokens": ["Ich", "fra\u00b7ge", "mit", "ge\u00b7hei\u00b7mem", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie lang, ihr G\u00f6tter, bleibt mir das?", "tokens": ["Wie", "lang", ",", "ihr", "G\u00f6t\u00b7ter", ",", "bleibt", "mir", "das", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, lieblich war, was wir besessen,", "tokens": ["Ja", ",", "lieb\u00b7lich", "war", ",", "was", "wir", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir dr\u00fcckten's jubelnd an die Brust.", "tokens": ["Wir", "dr\u00fcck\u00b7ten's", "ju\u00b7belnd", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch um so bittrer unermessen", "tokens": ["Doch", "um", "so", "bit\u00b7trer", "un\u00b7er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fchlt nun im Tiefsten der Verlust.", "tokens": ["W\u00fchlt", "nun", "im", "Tiefs\u00b7ten", "der", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Gl\u00fcck mit seinem s\u00fc\u00dfen Lachen,", "tokens": ["Das", "Gl\u00fcck", "mit", "sei\u00b7nem", "s\u00fc\u00b7\u00dfen", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es flog den wilden Strom hinab,", "tokens": ["Es", "flog", "den", "wil\u00b7den", "Strom", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN",