{"dta.poem.10346": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Die Botschaft .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Knecht! steh auf und sattle schnell,", "tokens": ["Mein", "Knecht", "!", "steh", "auf", "und", "satt\u00b7le", "schnell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirf dich auf dein Ro\u00df,", "tokens": ["Und", "wirf", "dich", "auf", "dein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jage rasch, durch Wald und Feld,", "tokens": ["Und", "ja\u00b7ge", "rasch", ",", "durch", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach K\u00f6nig Dunkans Schlo\u00df.", "tokens": ["Nach", "K\u00f6\u00b7nig", "Dun\u00b7kans", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort schleiche in den Stall, und wart',", "tokens": ["Dort", "schlei\u00b7che", "in", "den", "Stall", ",", "und", "wart'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis dich der Stallbub schaut.", "tokens": ["Bis", "dich", "der", "Stall\u00b7bub", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den forsch' mir aus: Sprich, welche ist", "tokens": ["Den", "for\u00b7sch'", "mir", "aus", ":", "Sprich", ",", "wel\u00b7che", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$.", "VVIMP", "$,", "PRELS", "VAFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Von Dunkans T\u00f6chtern Braut?", "tokens": ["Von", "Dun\u00b7kans", "T\u00f6ch\u00b7tern", "Braut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und spricht der Bub: \u201eDie Braune ist's\u201c,", "tokens": ["Und", "spricht", "der", "Bub", ":", "\u201e", "Die", "Brau\u00b7ne", "ist's", "\u201c", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "$(", "ART", "NN", "NE", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bring mir schnell die M\u00e4hr.", "tokens": ["So", "bring", "mir", "schnell", "die", "M\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch spricht der Bub: \u201eDie Blonde ist's\u201c,", "tokens": ["Doch", "spricht", "der", "Bub", ":", "\u201e", "Die", "Blon\u00b7de", "ist's", "\u201c", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "$(", "ART", "NN", "NE", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So eile nicht so sehr.", "tokens": ["So", "ei\u00b7le", "nicht", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann geh' zum Meister Seiler hin,", "tokens": ["Dann", "geh'", "zum", "Meis\u00b7ter", "Sei\u00b7ler", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kauf' mir einen Strick,", "tokens": ["Und", "kauf'", "mir", "ei\u00b7nen", "Strick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und reite langsam, sprich kein Wort,", "tokens": ["Und", "rei\u00b7te", "lang\u00b7sam", ",", "sprich", "kein", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bring mir den zur\u00fcck.", "tokens": ["Und", "bring", "mir", "den", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}