{"textgrid.poem.40843": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Saalgew\u00f6lb' des Urwalds ruhn im Kreis", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Saalgew\u00f6lb' des Urwalds ruhn im Kreis", "tokens": ["Im", "Saal\u00b7ge\u00b7w\u00f6lb'", "des", "Ur\u00b7walds", "ruhn", "im", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel kr\u00e4ft'ge M\u00e4nner, manch ein ernster Greis,", "tokens": ["Viel", "kr\u00e4ft'\u00b7ge", "M\u00e4n\u00b7ner", ",", "manch", "ein", "erns\u00b7ter", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wei\u00dfen Abgesandte friedlich bei", "tokens": ["Der", "Wei\u00b7\u00dfen", "Ab\u00b7ge\u00b7sand\u00b7te", "fried\u00b7lich", "bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Indianern, Waldess\u00f6hnen, stark und frei.", "tokens": ["In\u00b7di\u00b7a\u00b7nern", ",", "Wal\u00b7des\u00b7s\u00f6h\u00b7nen", ",", "stark", "und", "frei", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Friedenspfeife kreist nach altem Brauch,", "tokens": ["Die", "Frie\u00b7den\u00b7spfei\u00b7fe", "kreist", "nach", "al\u00b7tem", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der M\u00e4nner Friedenswort' umh\u00fcllt ihr Rauch,", "tokens": ["Der", "M\u00e4n\u00b7ner", "Frie\u00b7dens\u00b7wort'", "um\u00b7h\u00fcllt", "ihr", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie \u00fcber Fr\u00fchlings sch\u00f6nstem Rosenbeet", "tokens": ["Wie", "\u00fc\u00b7ber", "Fr\u00fch\u00b7lings", "sch\u00f6ns\u00b7tem", "Ro\u00b7sen\u00b7beet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In stillem Flug ein Morgenw\u00f6lkchen steht.", "tokens": ["In", "stil\u00b7lem", "Flug", "ein", "Mor\u00b7gen\u00b7w\u00f6lk\u00b7chen", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zum Bund des Friedens sind sie hier vereint.", "tokens": ["Zum", "Bund", "des", "Frie\u00b7dens", "sind", "sie", "hier", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon rann genug des Blutes ja, schon scheint", "tokens": ["Schon", "rann", "ge\u00b7nug", "des", "Blu\u00b7tes", "ja", ",", "schon", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKANT", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Belegt des gr\u00fcnen Saales Boden fast", "tokens": ["Be\u00b7legt", "des", "gr\u00fc\u00b7nen", "Saa\u00b7les", "Bo\u00b7den", "fast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit rothen Prunktapeten von Damast.", "tokens": ["Mit", "ro\u00b7then", "Prunk\u00b7ta\u00b7pe\u00b7ten", "von", "Da\u00b7mast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein H\u00e4uptling sprach: \u00bbNach V\u00e4tersitte macht", "tokens": ["Ein", "H\u00e4upt\u00b7ling", "sprach", ":", "\u00bb", "Nach", "V\u00e4\u00b7ter\u00b7sit\u00b7te", "macht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Erd' und Laub das Grab dem Beil der Schlacht.", "tokens": ["Aus", "Erd'", "und", "Laub", "das", "Grab", "dem", "Beil", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Manchen unsrer wei\u00dfen Br\u00fcder traf!", "tokens": ["Das", "Man\u00b7chen", "uns\u00b7rer", "wei\u00b7\u00dfen", "Br\u00fc\u00b7der", "traf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drin schlaf' es, ungeweckt, nun ew'gen Schlaf!\u00ab", "tokens": ["Drin", "schlaf'", "es", ",", "un\u00b7ge\u00b7weckt", ",", "nun", "ew'\u00b7gen", "Schlaf", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Andrer drauf: \u00bbDas Laub vertr\u00e4gt der Wind,", "tokens": ["Ein", "A\u00b7ndrer", "drauf", ":", "\u00bb", "Das", "Laub", "ver\u00b7tr\u00e4gt", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Erd' aufw\u00fchlt des Waldes Thier geschwind!", "tokens": ["Die", "Erd'", "auf\u00b7w\u00fchlt", "des", "Wal\u00b7des", "Thier", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum soll des Kampfes Beil geborgen sein,", "tokens": ["Drum", "soll", "des", "Kamp\u00b7fes", "Beil", "ge\u00b7bor\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Grabt's unter Wurzeln einer Ceder ein!\u00ab", "tokens": ["Grabt's", "un\u00b7ter", "Wur\u00b7zeln", "ei\u00b7ner", "Ce\u00b7der", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein Andrer drauf: \u00bbAn Wurzeln nagt der Wurm,", "tokens": ["Ein", "A\u00b7ndrer", "drauf", ":", "\u00bb", "An", "Wur\u00b7zeln", "nagt", "der", "Wurm", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu Boden schleudert Cedern selbst der Sturm!", "tokens": ["Zu", "Bo\u00b7den", "schleu\u00b7dert", "Ce\u00b7dern", "selbst", "der", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum, soll zu Tag des Unheils Beil nicht mehr,", "tokens": ["Drum", ",", "soll", "zu", "Tag", "des", "Un\u00b7heils", "Beil", "nicht", "mehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VMFIN", "APPR", "NN", "ART", "NN", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4lzt jenen Berg als Grabstein dr\u00fcberher!\u00ab", "tokens": ["W\u00e4lzt", "je\u00b7nen", "Berg", "als", "Grab\u00b7stein", "dr\u00fc\u00b7ber\u00b7her", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PDAT", "NN", "KOUS", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Andrer drauf: \u00bbSogar des Berges Bauch", "tokens": ["Ein", "A\u00b7ndrer", "drauf", ":", "\u00bb", "So\u00b7gar", "des", "Ber\u00b7ges", "Bauch"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchw\u00fchlt der Schacht des wei\u00dfen Bergmanns auch!", "tokens": ["Durch\u00b7w\u00fchlt", "der", "Schacht", "des", "wei\u0