": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Senkt's mitten in des Weltmeers gro\u00dfen See!\u00ab", "tokens": ["Senkt's", "mit\u00b7ten", "in", "des", "Welt\u00b7meers", "gro\u00b7\u00dfen", "See", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ein Greis darauf: \u00bbDie\u00df Beil von Holz und Erz", "tokens": ["Ein", "Greis", "da\u00b7rauf", ":", "\u00bb", "Die\u00df", "Beil", "von", "Holz", "und", "Erz"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "$.", "$(", "PDS", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O la\u00dft's am Tag! Doch greift in euer Herz!", "tokens": ["O", "la\u00dft's", "am", "Tag", "!", "Doch", "greift", "in", "eu\u00b7er", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$.", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drin liegt das Schlachtbeil, das vielleicht schon jetzt", "tokens": ["Drin", "liegt", "das", "Schlacht\u00b7beil", ",", "das", "viel\u00b7leicht", "schon", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von euch manch Einer frisch zum Kampfe wetzt!", "tokens": ["Von", "euch", "manch", "Ei\u00b7ner", "frisch", "zum", "Kamp\u00b7fe", "