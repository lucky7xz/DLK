{"dta.poem.3911": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 7.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wann sie ihm dann hernach von freyen St\u00fccken", "tokens": ["Wann", "sie", "ihm", "dann", "her\u00b7nach", "von", "frey\u00b7en", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schmeichelt", "tokens": ["schmei\u00b7chelt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und liebreich care\u00dfirt, ihm auf die Backen streichelt,", "tokens": ["Und", "lieb\u00b7reich", "ca\u00b7re\u00b7\u00dfirt", ",", "ihm", "auf", "die", "Ba\u00b7cken", "strei\u00b7chelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in das Kyne kneipt, so reimen wir es ein:", "tokens": ["Und", "in", "das", "Ky\u00b7ne", "kneipt", ",", "so", "rei\u00b7men", "wir", "es", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es mag vor solche M\u00fch ein Dutzend M\u00e4ulgen seyn.", "tokens": ["Es", "mag", "vor", "sol\u00b7che", "M\u00fch", "ein", "Dut\u00b7zend", "M\u00e4ul\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}