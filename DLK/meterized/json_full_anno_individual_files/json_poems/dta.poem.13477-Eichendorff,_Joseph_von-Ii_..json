{"dta.poem.13477": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wagen mu\u00dft du und fl\u00fcchtig erbeuten,", "tokens": ["Wa\u00b7gen", "mu\u00dft", "du", "und", "fl\u00fcch\u00b7tig", "er\u00b7beu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hinter uns schon durch die Nacht h\u00f6r' ich's schreiten,", "tokens": ["Hin\u00b7ter", "uns", "schon", "durch", "die", "Nacht", "h\u00f6r'", "ich's", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schwing' auf mein Ro\u00df dich nur schnell", "tokens": ["Schwing'", "auf", "mein", "Ro\u00df", "dich", "nur", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Und k\u00fc\u00df' noch im Flug mich, wildsch\u00f6nes Kind,", "tokens": ["Und", "k\u00fc\u00df'", "noch", "im", "Flug", "mich", ",", "wild\u00b7sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Geschwind,", "tokens": ["Ge\u00b7schwind", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Denn der Tod ist ein rascher Gesell.", "tokens": ["Denn", "der", "Tod", "ist", "ein", "ra\u00b7scher", "Ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}