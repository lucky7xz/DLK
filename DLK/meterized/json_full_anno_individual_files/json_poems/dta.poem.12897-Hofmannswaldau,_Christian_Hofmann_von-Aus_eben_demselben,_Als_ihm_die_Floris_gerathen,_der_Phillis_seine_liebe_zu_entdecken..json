{"dta.poem.12897": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus eben demselben,  \n Als ihm die Floris gerathen, der Phillis  \n seine liebe zu entdecken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, Floris! meine lieb' ist noch ein junges kind.", "tokens": ["Ach", ",", "Flo\u00b7ris", "!", "mei\u00b7ne", "lieb'", "ist", "noch", "ein", "jun\u00b7ges", "kind", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "PPOSAT", "VVFIN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie? soll das arme ding schon mutter sprechen k\u00f6nnen?", "tokens": ["Wie", "?", "soll", "das", "ar\u00b7me", "ding", "schon", "mut\u00b7ter", "spre\u00b7chen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ART", "ADJA", "NN", "ADV", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du sprichst: Sie solle sich der Phillis sclavin nennen,", "tokens": ["Du", "sprichst", ":", "Sie", "sol\u00b7le", "sich", "der", "Phil\u00b7lis", "scla\u00b7vin", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PRF", "ART", "NE", "NE", "VVINF", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sagen: da\u00df ihr glantz der Venus abgewinnt.", "tokens": ["Und", "sa\u00b7gen", ":", "da\u00df", "ihr", "glantz", "der", "Ve\u00b7nus", "ab\u00b7ge\u00b7winnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie soll der augen nacht, den schnee der zarten brust,", "tokens": ["Sie", "soll", "der", "au\u00b7gen", "nacht", ",", "den", "schnee", "der", "zar\u00b7ten", "brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der wangen lilgen-feld, bis zu der sonne f\u00fchren,", "tokens": ["Der", "wan\u00b7gen", "lil\u00b7gen\u00b7feld", ",", "bis", "zu", "der", "son\u00b7ne", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den anmuth-vollen mund mit frischen rosen zieren,", "tokens": ["Den", "an\u00b7muth\u00b7vol\u00b7len", "mund", "mit", "fri\u00b7schen", "ro\u00b7sen", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ruffen: Phillis ist das paradie\u00df der lust.", "tokens": ["Und", "ruf\u00b7fen", ":", "Phil\u00b7lis", "ist", "das", "pa\u00b7ra\u00b7die\u00df", "der", "lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch meine liebe schwimmt noch in den ersten thr\u00e4nen,", "tokens": ["Doch", "mei\u00b7ne", "lie\u00b7be", "schwimmt", "noch", "in", "den", "ers\u00b7ten", "thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer kan ein windel-kind so zeitlich abgewehnen?", "tokens": ["Wer", "kan", "ein", "win\u00b7del\u00b7kind", "so", "zeit\u00b7lich", "ab\u00b7ge\u00b7weh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nein, Sch\u00f6ne Freundin! nein, dein vorschlag geht nicht an.", "tokens": ["Nein", ",", "Sch\u00f6\u00b7ne", "Freun\u00b7din", "!", "nein", ",", "dein", "vor\u00b7schlag", "geht", "nicht", "an", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$.", "PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich wei\u00df wohl, da\u00df ein kind von anderthalben tagen", "tokens": ["Ich", "wei\u00df", "wohl", ",", "da\u00df", "ein", "kind", "von", "an\u00b7der\u00b7thal\u00b7ben", "ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Noch ach und thr\u00e4nen mu\u00df in mund und augen tragen;", "tokens": ["Noch", "ach", "und", "thr\u00e4\u00b7nen", "mu\u00df", "in", "mund", "und", "au\u00b7gen", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "XY", "KON", "VVINF", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nicht aber, da\u00df es lacht und worte machen kan.", "tokens": ["Nicht", "a\u00b7ber", ",", "da\u00df", "es", "lacht", "und", "wor\u00b7te", "ma\u00b7chen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPER", "VVFIN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}