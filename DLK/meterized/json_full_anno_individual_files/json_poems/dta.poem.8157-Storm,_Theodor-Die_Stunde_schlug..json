{"dta.poem.8157": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Stunde schlug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Liegt zitternd in der meinen;", "tokens": ["Liegt", "zit\u00b7ternd", "in", "der", "mei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meine Lippen streiften schon", "tokens": ["An", "mei\u00b7ne", "Lip\u00b7pen", "streif\u00b7ten", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit scheuem Druck die deinen;", "tokens": ["Mit", "scheu\u00b7em", "Druck", "die", "dei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es zuckten aus dem vollen Kelch", "tokens": ["Es", "zuck\u00b7ten", "aus", "dem", "vol\u00b7len", "Kelch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Electrisch schon die Funken \u2014", "tokens": ["E\u00b7lect\u00b7risch", "schon", "die", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O fasse Muth, und fliehe nicht,", "tokens": ["O", "fas\u00b7se", "Muth", ",", "und", "flie\u00b7he", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bevor wir ganz getrunken!", "tokens": ["Be\u00b7vor", "wir", "ganz", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Lippen, die mich so ber\u00fchrt,", "tokens": ["Die", "Lip\u00b7pen", ",", "die", "mich", "so", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind nicht mehr deine eignen,", "tokens": ["Sind", "nicht", "mehr", "dei\u00b7ne", "eig\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnen doch, so lang' du lebst,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "doch", ",", "so", "lang'", "du", "lebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die meinen nicht verleugnen;", "tokens": ["Die", "mei\u00b7nen", "nicht", "ver\u00b7leug\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Lippen, die sich so ber\u00fchrt,", "tokens": ["Die", "Lip\u00b7pen", ",", "die", "sich", "so", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind rettungslos gefangen;", "tokens": ["Sind", "ret\u00b7tungs\u00b7los", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sp\u00e4t oder fr\u00fch, sie m\u00fcssen doch", "tokens": ["Sp\u00e4t", "o\u00b7der", "fr\u00fch", ",", "sie", "m\u00fcs\u00b7sen", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich t\u00f6dtlich heimverlangen.", "tokens": ["Sich", "t\u00f6dt\u00b7lich", "heim\u00b7ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}