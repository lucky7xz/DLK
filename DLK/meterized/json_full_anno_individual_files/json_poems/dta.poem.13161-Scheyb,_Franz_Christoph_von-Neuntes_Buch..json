{"dta.poem.13161": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ekurz in dem Ehren-Bau seht ihr das Tugend-Herz,", "tokens": ["\u201e", "kurz", "in", "dem", "Eh\u00b7ren\u00b7Bau", "seht", "ihr", "das", "Tu\u00b7gen\u00b7dHerz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso GOtt der K\u00f6niginn, der Feinde Sinn zum Schmerz", "tokens": ["\u201e", "so", "Gott", "der", "K\u00f6\u00b7ni\u00b7ginn", ",", "der", "Fein\u00b7de", "Sinn", "zum", "Schmerz"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "275\u201dUnd uns zur Wohlfart, gab. Seyd ihr damit zu frieden;", "tokens": ["\"", "Und", "uns", "zur", "Wohl\u00b7fart", ",", "gab", ".", "Seyd", "ihr", "da\u00b7mit", "zu", "frie\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "APPRART", "NN", "$,", "VVFIN", "$.", "VAIMP", "PPER", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso steht der Tempel schon; so ist die Frag\u2019 entschieden.", "tokens": ["\u201e", "so", "steht", "der", "Tem\u00b7pel", "schon", ";", "so", "ist", "die", "Frag'", "ent\u00b7schie\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hierauf gieng sie zur\u00fcck, nachdem sie so beschlo\u00df,", "tokens": ["Hier\u00b7auf", "gieng", "sie", "zu\u00b7r\u00fcck", ",", "nach\u00b7dem", "sie", "so", "be\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ward zu meiner Lust des Kreises Mitgeno\u00df.", "tokens": ["Und", "ward", "zu", "mei\u00b7ner", "Lust", "des", "Krei\u00b7ses", "Mit\u00b7ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}