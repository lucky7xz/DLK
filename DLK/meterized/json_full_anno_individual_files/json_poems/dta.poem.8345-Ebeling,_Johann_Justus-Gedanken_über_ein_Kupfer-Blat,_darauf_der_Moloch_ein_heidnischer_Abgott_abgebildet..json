{"dta.poem.8345": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken \u00fcber ein Kupfer-Blat,  \n darauf der Moloch ein heidnischer  \n Abgott abgebildet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein scheu\u00dflich G\u00f6tzenbild f\u00e4llt mir jetzt\nins Gesicht,", "tokens": ["Ein", "scheu\u00df\u00b7lich", "G\u00f6t\u00b7zen\u00b7bild", "f\u00e4llt", "mir", "jetzt", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Davon das Alterthum, als Am-", "tokens": ["Da\u00b7von", "das", "Al\u00b7ter\u00b7thum", ",", "als", "Am"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "ART", "NN", "$,", "KOUS", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df es aus Ertz gemacht, und doch", "tokens": ["Da\u00df", "es", "aus", "Ertz", "ge\u00b7macht", ",", "und", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von denen die der Wahn des Teu-", "tokens": ["Von", "de\u00b7nen", "die", "der", "Wahn", "des", "Teu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies ausgeh\u00f6hlte Bild das wie ein Ochs aussieht,", "tokens": ["Dies", "aus\u00b7ge\u00b7h\u00f6hl\u00b7te", "Bild", "das", "wie", "ein", "Ochs", "aus\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ART", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und von dem innren Feur der heissen Flammen", "tokens": ["Und", "von", "dem", "inn\u00b7ren", "Feur", "der", "heis\u00b7sen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Spannt seine Arme aus, die Kinder zu umfassen,", "tokens": ["Spannt", "sei\u00b7ne", "Ar\u00b7me", "aus", ",", "die", "Kin\u00b7der", "zu", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die, Eltern ihm zu ehrn, darin verbrennen lassen.", "tokens": ["Die", ",", "El\u00b7tern", "ihm", "zu", "ehrn", ",", "da\u00b7rin", "ver\u00b7bren\u00b7nen", "las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "PPER", "PTKZU", "VVINF", "$,", "PAV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mir schauderte die Haut bei dem gemahlten Bild,", "tokens": ["Mir", "schau\u00b7der\u00b7te", "die", "Haut", "bei", "dem", "ge\u00b7mahl\u00b7ten", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das wie ein Ochse sieht, obsgleich nicht lebt und", "tokens": ["Das", "wie", "ein", "O\u00b7chse", "sieht", ",", "obs\u00b7gleich", "nicht", "lebt", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ART", "NN", "VVFIN", "$,", "KOUS", "PTKNEG", "VVFIN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ich dachte nimmermehr wird man dergleichen G\u00f6z-", "tokens": ["Ich", "dach\u00b7te", "nim\u00b7mer\u00b7mehr", "wird", "man", "derg\u00b7lei\u00b7chen", "G\u00f6z"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VAFIN", "PIS", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Kinder, als sein Blut, in seine Arme setzen.", "tokens": ["Die", "Kin\u00b7der", ",", "als", "sein", "Blut", ",", "in", "sei\u00b7ne", "Ar\u00b7me", "set\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch hat der Aberglaub die Z\u00e4rtlichkeit besiegt,", "tokens": ["Doch", "hat", "der", "A\u00b7berg\u00b7laub", "die", "Z\u00e4rt\u00b7lich\u00b7keit", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Kind darin gelegt, wies im Gem\u00e4hlde liegt,", "tokens": ["Ein", "Kind", "da\u00b7rin", "ge\u00b7legt", ",", "wies", "im", "Ge\u00b7m\u00e4hl\u00b7de", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$,", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Flammen eingeweiht, die Kinder lassen qu\u00e4-", "tokens": ["Den", "Flam\u00b7men", "ein\u00b7ge\u00b7weiht", ",", "die", "Kin\u00b7der", "las\u00b7sen", "qu\u00e4"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich schalt die Grausamkeit der Alten in der Seelen", "tokens": ["Ich", "schalt", "die", "Grau\u00b7sam\u00b7keit", "der", "Al\u00b7ten", "in", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die solche Wuth ver\u00fcbt: Jedoch es fiel mir bei,", "tokens": ["Die", "sol\u00b7che", "Wuth", "ver\u00b7\u00fcbt", ":", "Je\u00b7doch", "es", "fiel", "mir", "bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$.", "ADV", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df solche b\u00f6se Art auch noch anjetzo sey:", "tokens": ["Da\u00df", "sol\u00b7che", "b\u00f6\u00b7se", "Art", "auch", "noch", "an\u00b7je\u00b7tzo", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ADV", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie viele Eltern sind, die untern Christen leben,", "tokens": ["Wie", "vie\u00b7le", "El\u00b7tern", "sind", ",", "die", "un\u00b7tern", "Chris\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ihre Kinder auch zum Molochs-Opfer geben:", "tokens": ["Die", "ih\u00b7re", "Kin\u00b7der", "auch", "zum", "Mo\u00b7lochs\u00b7Op\u00b7fer", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie viele geben sie aus sch\u00e4ndlichen Gewinn,", "tokens": ["Wie", "vie\u00b7le", "ge\u00b7ben", "sie", "aus", "sch\u00e4nd\u00b7li\u00b7chen", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Zum Wollust Feur verbannt, in Buhler Arme hin,", "tokens": ["Zum", "Wol\u00b7lust", "Feur", "ver\u00b7bannt", ",", "in", "Buh\u00b7ler", "Ar\u00b7me", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVPP", "$,", "APPR", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die jenem Moloch gleich, darinnen sie verderben,", "tokens": ["Die", "je\u00b7nem", "Mo\u00b7loch", "gleich", ",", "da\u00b7rin\u00b7nen", "sie", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADV", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nicht an dem Leib allein, woll an der Seel erster-", "tokens": ["Nicht", "an", "dem", "Leib", "al\u00b7lein", ",", "woll", "an", "der", "Seel", "ers\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "$,", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Wie viele Kinder sind, die sich der Wollust weihn,", "tokens": ["Wie", "vie\u00b7le", "Kin\u00b7der", "sind", ",", "die", "sich", "der", "Wol\u00b7lust", "weihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und \u00fcber ihren Tod und Untergang noch freun.", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7ren", "Tod", "und", "Un\u00b7ter\u00b7gang", "noch", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach! w\u00fcrde dieser Dienst der Wollust auch zer-", "tokens": ["Ach", "!", "w\u00fcr\u00b7de", "die\u00b7ser", "Dienst", "der", "Wol\u00b7lust", "auch", "zer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PDAT", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wie Moloch und sein Dienst, den Fr\u00f6mmigkeit, ver-", "tokens": ["Wie", "Mo\u00b7loch", "und", "sein", "Dienst", ",", "den", "Fr\u00f6m\u00b7mig\u00b7keit", ",", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "NE", "KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}