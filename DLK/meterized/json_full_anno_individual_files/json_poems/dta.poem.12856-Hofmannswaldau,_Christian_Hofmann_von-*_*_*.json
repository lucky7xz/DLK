{"dta.poem.12856": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts kan des lebens sauren wein", "tokens": ["Nichts", "kan", "des", "le\u00b7bens", "sau\u00b7ren", "wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr vers\u00fcssen,", "tokens": ["Mehr", "ver\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Als lieben, und geliebet seyn", "tokens": ["Als", "lie\u00b7ben", ",", "und", "ge\u00b7lie\u00b7bet", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VVINF", "$,", "KON", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und es wissen.", "tokens": ["Und", "es", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}