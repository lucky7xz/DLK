{"dta.poem.20918": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Graf Ludwig an seine  \n Gemahlin.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Brief aus frembder Luft doch von bekan-", "tokens": ["Ein", "Brief", "aus", "fremb\u00b7der", "Luft", "doch", "von", "be\u00b7kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ten H\u00e4nden/", "tokens": ["ten", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Begr\u00fc\u00dft un\u0303 k\u00fc\u00dft dich itzt/ so gut er k\u00fcssen kan/", "tokens": ["Be\u00b7gr\u00fc\u00dft", "u\u00f1", "k\u00fc\u00dft", "dich", "itzt", "/", "so", "gut", "er", "k\u00fcs\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PPER", "ADV", "$(", "ADV", "ADJD", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hei\u00dft die gr\u00fcne Treu mich dieses \u00fcbersen-", "tokens": ["Es", "hei\u00dft", "die", "gr\u00fc\u00b7ne", "Treu", "mich", "die\u00b7ses", "\u00fc\u00b7ber\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Ich wei\u00df du nimbst es auch mit solchen Hertzen an.", "tokens": ["Ich", "wei\u00df", "du", "nimbst", "es", "auch", "mit", "sol\u00b7chen", "Hert\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich darf dir nicht zuviel von meinem Namen sagen", "tokens": ["Ich", "darf", "dir", "nicht", "zu\u00b7viel", "von", "mei\u00b7nem", "Na\u00b7men", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die kleinste Silbe hier entdeckt dir wer ich bin/", "tokens": ["Die", "kleins\u00b7te", "Sil\u00b7be", "hier", "ent\u00b7deckt", "dir", "wer", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie denckt mein Hertze dir/ wo m\u00f6glich/ f\u00fcrzutragen/", "tokens": ["Sie", "denckt", "mein", "Hert\u00b7ze", "dir", "/", "wo", "m\u00f6g\u00b7lich", "/", "f\u00fcr\u00b7zu\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "PPER", "$(", "PWAV", "ADJD", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und reicht/ so gut sie kan/ auch di\u00df im Briefe hin.", "tokens": ["Und", "reicht", "/", "so", "gut", "sie", "kan", "/", "auch", "di\u00df", "im", "Brie\u00b7fe", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "ADJD", "PPER", "VMFIN", "$(", "ADV", "PDS", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du kenst die alte Schrifft und auch die alten Sinnen/", "tokens": ["Du", "kenst", "die", "al\u00b7te", "Schrifft", "und", "auch", "die", "al\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die noch kein Saracen hat in die Fessel bracht/", "tokens": ["Die", "noch", "kein", "Sa\u00b7ra\u00b7cen", "hat", "in", "die", "Fes\u00b7sel", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich schwere/ da\u00df sie dich so eifrig lieben k\u00f6nnen/", "tokens": ["Ich", "schwe\u00b7re", "/", "da\u00df", "sie", "dich", "so", "eif\u00b7rig", "lie\u00b7ben", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als in dem Hochzeit Tag und in der ersten Nacht.", "tokens": ["Als", "in", "dem", "Hoch\u00b7zeit", "Tag", "und", "in", "der", "ers\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du weist die Liebe l\u00e4st sich nicht durch Meilen messen", "tokens": ["Du", "weist", "die", "Lie\u00b7be", "l\u00e4st", "sich", "nicht", "durch", "Mei\u00b7len", "mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie w\u00e4chst nicht ungemein in unbekanter Luft.", "tokens": ["Sie", "w\u00e4chst", "nicht", "un\u00b7ge\u00b7mein", "in", "un\u00b7be\u00b7kan\u00b7ter", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was recht gegr\u00fcndet ist/ das l\u00e4st sich nicht vergessen/", "tokens": ["Was", "recht", "ge\u00b7gr\u00fcn\u00b7det", "ist", "/", "das", "l\u00e4st", "sich", "nicht", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "VAFIN", "$(", "PDS", "VVFIN", "PRF", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und ihre Wurtzel dringt bi\u00df in die kalte Gruft.", "tokens": ["Und", "ih\u00b7re", "Wurt\u00b7zel", "dringt", "bi\u00df", "in", "die", "kal\u00b7te", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch Hitze kan sie nicht wie Blum und Gr\u00e4ser sterben", "tokens": ["Durch", "Hit\u00b7ze", "kan", "sie", "nicht", "wie", "Blum", "und", "Gr\u00e4\u00b7ser", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKNEG", "KOKOM", "NE", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die K\u00e4lte hemmt sie nicht wie einen Wasserflu\u00df/", "tokens": ["Die", "K\u00e4l\u00b7te", "hemmt", "sie", "nicht", "wie", "ei\u00b7nen", "Was\u00b7ser\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die N\u00e4sse wei\u00df sie nicht wie Farben zuverderben/", "tokens": ["Die", "N\u00e4s\u00b7se", "wei\u00df", "sie", "nicht", "wie", "Far\u00b7ben", "zu\u00b7ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man schaut wie Staal und Seein ihr oftmahls wei-", "tokens": ["Man", "schaut", "wie", "Staal", "und", "Se\u00b7ein", "ihr", "oft\u00b7mahls", "wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KOKOM", "NN", "KON", "NN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "chen mu\u00df.", "tokens": ["chen", "mu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Die wahre Freundschafft kan kein Saracen beschnei-", "tokens": ["Die", "wah\u00b7re", "Freund\u00b7schafft", "kan", "kein", "Sa\u00b7ra\u00b7cen", "be\u00b7schnei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Es st\u00f6hrt der Alcoran getreue Liebe nicht/", "tokens": ["Es", "st\u00f6hrt", "der", "Al\u00b7co\u00b7ran", "ge\u00b7treu\u00b7e", "Lie\u00b7be", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es kan der Mahomet Sie in dem Tempel leiden/", "tokens": ["Es", "kan", "der", "Ma\u00b7ho\u00b7met", "Sie", "in", "dem", "Tem\u00b7pel", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Und keine Satzung ist/ so ihr zuwieder spricht.", "tokens": ["Und", "kei\u00b7ne", "Sat\u00b7zung", "ist", "/", "so", "ihr", "zu\u00b7wie\u00b7der", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$(", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Schatz/ itzt heisset mich ein Zufall kl\u00e4hrer schrei-", "tokens": ["Mein", "Schatz", "/", "itzt", "heis\u00b7set", "mich", "ein", "Zu\u00b7fall", "kl\u00e4h\u00b7rer", "schrei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Es mindert wie es scheint/ sich nun das alte Joch/", "tokens": ["Es", "min\u00b7dert", "wie", "es", "scheint", "/", "sich", "nun", "das", "al\u00b7te", "Joch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VVFIN", "$(", "PRF", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich kan mit mehrer Lust itzt meine Rinder treiben/", "tokens": ["Ich", "kan", "mit", "meh\u00b7rer", "Lust", "itzt", "mei\u00b7ne", "Rin\u00b7der", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und mein Gel\u00fccke bl\u00fcht auch untern Heyden noch.", "tokens": ["Und", "mein", "Ge\u00b7l\u00fc\u00b7cke", "bl\u00fcht", "auch", "un\u00b7tern", "Hey\u00b7den", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein Edles Weib von mehr als F\u00fcrstlichen Gebl\u00fcthe/", "tokens": ["Ein", "Ed\u00b7les", "Weib", "von", "mehr", "als", "F\u00fcrst\u00b7li\u00b7chen", "Ge\u00b7bl\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "KOKOM", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "(ich wei\u00df nicht ob sie mir Weib oder Engel ist)", "tokens": ["(", "ich", "wei\u00df", "nicht", "ob", "sie", "mir", "Weib", "o\u00b7der", "En\u00b7gel", "ist", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "PPER", "NN", "KON", "NE", "VAFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Die hat vor kurtzer Zeit mit traurigem Gem\u00fcthe", "tokens": ["Die", "hat", "vor", "kurt\u00b7zer", "Zeit", "mit", "trau\u00b7ri\u00b7gem", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mein schweres Joch betracht/ und meine Noth erkie\u00df", "tokens": ["Mein", "schwe\u00b7res", "Joch", "be\u00b7tracht", "/", "und", "mei\u00b7ne", "Noth", "er\u00b7kie\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Es schien/ sie ward durch mich und meine Qvaal ge-", "tokens": ["Es", "schien", "/", "sie", "ward", "durch", "mich", "und", "mei\u00b7ne", "Qvaal", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "bunden/", "tokens": ["bun\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["ADJA", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Kein Striemen lief mir auf/ den sie nicht auch em-", "tokens": ["Kein", "Strie\u00b7men", "lief", "mir", "auf", "/", "den", "sie", "nicht", "auch", "em"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "PPER", "PTKNEG", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "pfand/", "tokens": ["pfand", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Die Sch\u00e4den so ich trug die wurden ihr zur Wunden/", "tokens": ["Die", "Sch\u00e4\u00b7den", "so", "ich", "trug", "die", "wur\u00b7den", "ihr", "zur", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "ART", "VAFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und meine Dinstbarkeit war ihr gemeines Bandt.", "tokens": ["Und", "mei\u00b7ne", "Dinst\u00b7bar\u00b7keit", "war", "ihr", "ge\u00b7mei\u00b7nes", "Bandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Der Schwei\u00df auf meiner Brust hat Thr\u00e4nen ihr er-", "tokens": ["Der", "Schwei\u00df", "auf", "mei\u00b7ner", "Brust", "hat", "Thr\u00e4\u00b7nen", "ihr", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "reget/", "tokens": ["re\u00b7get", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.46": {"text": "Mein Seuffzer