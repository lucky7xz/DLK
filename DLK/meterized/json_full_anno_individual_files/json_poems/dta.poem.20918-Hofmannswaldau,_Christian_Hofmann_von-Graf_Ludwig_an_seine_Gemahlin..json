/", "tokens": ["Wie", "solt", "ich", "/", "liebs\u00b7tes", "Weib", "/", "nicht", "ei\u00b7ne", "See\u00b7le", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$(", "ADJA", "NN", "$(", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die mich dir wiederbringt/ und meine Fessel bricht?", "tokens": ["Die", "mich", "dir", "wie\u00b7der\u00b7bringt", "/", "und", "mei\u00b7ne", "Fes\u00b7sel", "bricht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Nicht meine/ da\u00df mich hier ein Geist der Wollust trei-", "tokens": ["Nicht", "mei\u00b7ne", "/", "da\u00df", "mich", "hier", "ein", "Geist", "der", "Wol\u00b7lust", "trei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "bet", "tokens": ["bet"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.64": {"text": "Zeit und auch Ungel\u00fcck hat solches l\u00e4ngst verjagt.", "tokens": ["Zeit", "und", "auch", "Un\u00b7ge\u00b7l\u00fcck", "hat", "sol\u00b7ches", "l\u00e4ngst", "ver\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Denn wem der Tugend Stam recht an der Brust be-", "tokens": ["Denn", "wem", "der", "Tu\u00b7gend", "S