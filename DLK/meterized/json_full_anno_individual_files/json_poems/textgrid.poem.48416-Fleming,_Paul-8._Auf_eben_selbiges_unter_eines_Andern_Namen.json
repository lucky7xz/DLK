{"textgrid.poem.48416": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Auf eben selbiges unter eines Andern Namen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ich auch, wertster Freund, wie mu\u00df ich doch beweinen", "tokens": ["Und", "ich", "auch", ",", "wert\u00b7ster", "Freund", ",", "wie", "mu\u00df", "ich", "doch", "be\u00b7wei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mein allzufr\u00fches Leid! Du N\u00fctzlichster der Meinen,", "tokens": ["mein", "all\u00b7zu\u00b7fr\u00fc\u00b7hes", "Leid", "!", "Du", "N\u00fctz\u00b7lichs\u00b7ter", "der", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "stehst mir zu zeitlich ab; dein ", "tokens": ["stehst", "mir", "zu", "zeit\u00b7lich", "ab", ";", "dein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "PTKVZ", "$.", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wie seufzen wir doch gnung und trauren recht um dich,", "tokens": ["wie", "seuf\u00b7zen", "wir", "doch", "gnung", "und", "trau\u00b7ren", "recht", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "du br\u00fcderlicher noch, als Br\u00fcder sind zu nennen!", "tokens": ["du", "br\u00fc\u00b7der\u00b7li\u00b7cher", "noch", ",", "als", "Br\u00fc\u00b7der", "sind", "zu", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADV", "$,", "KOUS", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat uns denn also bald ein Stiefblick k\u00f6nnen trennen,", "tokens": ["Hat", "uns", "denn", "al\u00b7so", "bald", "ein", "Stief\u00b7blick", "k\u00f6n\u00b7nen", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ein Stiefblick, den der Tod auf dich und uns gebracht,", "tokens": ["ein", "Stief\u00b7blick", ",", "den", "der", "Tod", "auf", "dich", "und", "uns", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der dich zu einer Leich', uns Waisen hat gemacht?", "tokens": ["der", "dich", "zu", "ei\u00b7ner", "Leich'", ",", "uns", "Wai\u00b7sen", "hat", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "$,", "PPER", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer wird uns ferner nun mit stiller Griffe Weisen,", "tokens": ["Wer", "wird", "uns", "fer\u00b7ner", "nun", "mit", "stil\u00b7ler", "Grif\u00b7fe", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "gelehrtem Unterricht, erfahrner Weisheit speisen", "tokens": ["ge\u00b7lehr\u00b7tem", "Un\u00b7ter\u00b7richt", ",", "er\u00b7fahr\u00b7ner", "Weis\u00b7heit", "spei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und unser Lehrer sein? Wer wird uns nun forthin", "tokens": ["und", "un\u00b7ser", "Leh\u00b7rer", "sein", "?", "Wer", "wird", "uns", "nun", "for\u00b7thin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAINF", "$.", "PWS", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "auf \u00c4sculapens Hain' und gr\u00fcne H\u00fcgel ziehn", "tokens": ["auf", "\u00c4\u00b7scu\u00b7la\u00b7pens", "Hain'", "und", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und uns der Parzen Ha\u00df, die guten Kr\u00e4uter zeigen,", "tokens": ["und", "uns", "der", "Par\u00b7zen", "Ha\u00df", ",", "die", "gu\u00b7ten", "Kr\u00e4u\u00b7ter", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "was au\u00dfer ihnen steht und innerlich zu eigen?", "tokens": ["was", "au\u00b7\u00dfer", "ih\u00b7nen", "steht", "und", "in\u00b7ner\u00b7lich", "zu", "ei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "KON", "ADJD", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Di\u00df hast du vor getan, fort wird es nicht geschehn,", "tokens": ["Di\u00df", "hast", "du", "vor", "ge\u00b7tan", ",", "fort", "wird", "es", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "VVPP", "$,", "PTKVZ", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "nun wir dich ohne Seel' und Leben vor uns sehn", "tokens": ["nun", "wir", "dich", "oh\u00b7ne", "Seel'", "und", "Le\u00b7ben", "vor", "uns", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PRF", "APPR", "NN", "KON", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "auf deiner Bahre stehn. Wer h\u00e4tte sollen denken,", "tokens": ["auf", "dei\u00b7ner", "Bah\u00b7re", "stehn", ".", "Wer", "h\u00e4t\u00b7te", "sol\u00b7len", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "PWS", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "da\u00df wir dich so geschwind' ins Schwere m\u00fcsten senken", "tokens": ["da\u00df", "wir", "dich", "so", "ge\u00b7schwind'", "ins", "Schwe\u00b7re", "m\u00fcs\u00b7ten", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und zusehn, da\u00df so bald dein freundliches Gesicht'", "tokens": ["und", "zu\u00b7sehn", ",", "da\u00df", "so", "bald", "dein", "freund\u00b7li\u00b7ches", "Ge\u00b7sicht'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUS", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "uns nicht mehr sehen solt' und geben Nacht vor Liecht?", "tokens": ["uns", "nicht", "mehr", "se\u00b7hen", "solt'", "und", "ge\u00b7ben", "Nacht", "vor", "Liecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vor Alles ist nun Nichts. Wags einer nun und traue", "tokens": ["Vor", "Al\u00b7les", "ist", "nun", "Nichts", ".", "Wags", "ei\u00b7ner", "nun", "und", "trau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "ADV", "PIS", "$.", "NN", "ART", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "auf seinen frischen Leib! Wenn ich dich noch beschaue,", "tokens": ["auf", "sei\u00b7nen", "fri\u00b7schen", "Leib", "!", "Wenn", "ich", "dich", "noch", "be\u00b7schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "zwar in Gedanken nur, so gl\u00e4ub' ich kaum gar bald,", "tokens": ["zwar", "in", "Ge\u00b7dan\u00b7ken", "nur", ",", "so", "gl\u00e4ub'", "ich", "kaum", "gar", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "da\u00df dich h\u00e4tt' also schnell die \u00e4u\u00dferste Gewalt", "tokens": ["da\u00df", "dich", "h\u00e4tt'", "al\u00b7so", "schnell", "die", "\u00e4u\u00b7\u00dfers\u00b7te", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.25": {"text": "befallen und ins Grab zu Vielen sto\u00dfen k\u00f6nnen.", "tokens": ["be\u00b7fal\u00b7len", "und", "ins", "Grab", "zu", "Vie\u00b7len", "sto\u00b7\u00dfen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPRART", "NN", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was fangen wir nun an, was sollen wir beginnen,", "tokens": ["Was", "fan\u00b7gen", "wir", "nun", "an", ",", "was", "sol\u00b7len", "wir", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "wir, Deine noch wie vor, wir ewig Deine wir?", "tokens": ["wir", ",", "Dei\u00b7ne", "noch", "wie", "vor", ",", "wir", "e\u00b7wig", "Dei\u00b7ne", "wir", "?"], "token_info": ["word", "punct", "word", "word",