{"textgrid.poem.24093": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Neue Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer diese Verse liest, die nur von Liebe singen,", "tokens": ["Wer", "die\u00b7se", "Ver\u00b7se", "liest", ",", "die", "nur", "von", "Lie\u00b7be", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,", "PRELS", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wisse wohl: es ist kein hei\u00dfer Atem,", "tokens": ["Der", "wis\u00b7se", "wohl", ":", "es", "ist", "kein", "hei\u00b7\u00dfer", "A\u00b7tem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ihre Fl\u00fcgel hebt, und kein Begehren.", "tokens": ["Der", "ih\u00b7re", "Fl\u00fc\u00b7gel", "hebt", ",", "und", "kein", "Be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Gl\u00fcck hat sie gesungen, nicht der Wunsch.", "tokens": ["Das", "Gl\u00fcck", "hat", "sie", "ge\u00b7sun\u00b7gen", ",", "nicht", "der", "Wunsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vielleicht ist Sehnsucht ganz von ferne drin,", "tokens": ["Viel\u00b7leicht", "ist", "Sehn\u00b7sucht", "ganz", "von", "fer\u00b7ne", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch also fern, da\u00df nur ein leises Rauschen", "tokens": ["Doch", "al\u00b7so", "fern", ",", "da\u00df", "nur", "ein", "lei\u00b7ses", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus diesem weiten Meere tr\u00f6stlich klingt,", "tokens": ["Aus", "die\u00b7sem", "wei\u00b7ten", "Mee\u00b7re", "tr\u00f6st\u00b7lich", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht brausend, drohend.", "tokens": ["Nicht", "brau\u00b7send", ",", "dro\u00b7hend", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Bin ich doch ein M\u00f6nch", "tokens": ["Bin", "ich", "doch", "ein", "M\u00f6nch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In Mauerfrieden, Stille um mich her", "tokens": ["In", "Mau\u00b7er\u00b7frie\u00b7den", ",", "Stil\u00b7le", "um", "mich", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und rings ein Glanz von milden Z\u00e4rt