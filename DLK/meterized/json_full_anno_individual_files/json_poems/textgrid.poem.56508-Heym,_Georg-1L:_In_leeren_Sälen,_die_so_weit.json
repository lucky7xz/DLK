{"textgrid.poem.56508": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: In leeren S\u00e4len, die so weit", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In leeren S\u00e4len, die so weit", "tokens": ["In", "lee\u00b7ren", "S\u00e4\u00b7len", ",", "die", "so", "weit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie leerer Atem, im Abende tot", "tokens": ["Wie", "lee\u00b7rer", "A\u00b7tem", ",", "im", "A\u00b7ben\u00b7de", "tot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "APPRART", "NN", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stehet er breit mit dem Feierkleid", "tokens": ["Ste\u00b7het", "er", "breit", "mit", "dem", "Fei\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und der t\u00fcrmenden M\u00fctze rot.", "tokens": ["Und", "der", "t\u00fcr\u00b7men\u00b7den", "M\u00fct\u00b7ze", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Mauern flohen von ihm hinweg,", "tokens": ["Die", "Mau\u00b7ern", "flo\u00b7hen", "von", "ihm", "hin\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die krummen S\u00e4ulen i