{"textgrid.poem.48683": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. \u00dcber sein Gel\u00fcbde", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe fast geirrt. Was soll ich dem doch geben,", "tokens": ["Ich", "ha\u00b7be", "fast", "ge\u00b7irrt", ".", "Was", "soll", "ich", "dem", "doch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "PWS", "VMFIN", "PPER", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Alles giebt und hat? und was verpflicht ich mich", "tokens": ["der", "Al\u00b7les", "giebt", "und", "hat", "?", "und", "was", "ver\u00b7pflicht", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "KON", "VAFIN", "$.", "KON", "PWS", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf etwas, das mich kn\u00fcpft und nichts doch hat auf sich?", "tokens": ["auf", "et\u00b7was", ",", "das", "mich", "kn\u00fcpft", "und", "nichts", "doch", "hat", "auf", "sich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "KON", "PIS", "ADV", "VAFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum verred' ich das, dadurch ich doch mu\u00df leben?", "tokens": ["Wa\u00b7rum", "ver\u00b7red'", "ich", "das", ",", "da\u00b7durch", "ich", "doch", "mu\u00df", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "$,", "PAV", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich habe fast geirrt. Doch acht ich hierbeineben,", "tokens": ["Ich", "ha\u00b7be", "fast", "ge\u00b7irrt", ".", "Doch", "acht", "ich", "hier\u00b7bei\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "KON", "CARD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "es sei so unrecht nicht, was du, mein Herze, dich", "tokens": ["es", "sei", "so", "un\u00b7recht", "nicht", ",", "was", "du", ",", "mein", "Her\u00b7ze", ",", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKNEG", "$,", "PWS", "PPER", "$,", "PPOSAT", "VVFIN", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zu tun erboten hast. So ists doch christlich, sprich?", "tokens": ["zu", "tun", "er\u00b7bo\u00b7ten", "hast", ".", "So", "ists", "doch", "christ\u00b7lich", ",", "sprich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "VAFIN", "$.", "ADV", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ists unvonn\u00f6ten schon, wer will dir widerstreben?", "tokens": ["Ists", "un\u00b7von\u00b7n\u00f6\u00b7ten", "schon", ",", "wer", "will", "dir", "wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADV", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott sieht die Herzen an und sieht nicht an die Gaben,", "tokens": ["Gott", "sieht", "die", "Her\u00b7zen", "an", "und", "sieht", "nicht", "an", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die sie schon zuvorhin von ihm entfangen haben.", "tokens": ["die", "sie", "schon", "zu\u00b7vor\u00b7hin", "von", "ihm", "ent\u00b7fan\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schau in und um dich her, was ist wol seine nicht?", "tokens": ["Schau", "in", "und", "um", "dich", "her", ",", "was", "ist", "wol", "sei\u00b7ne", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "PPER", "PTKVZ", "$,", "PWS", "VAFIN", "ADV", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Herr, was mein Mund geredt, das soll das Herze halten,", "tokens": ["Herr", ",", "was", "mein", "Mund", "ge\u00b7redt", ",", "das", "soll", "das", "Her\u00b7ze", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,", "PDS", "VMFIN", "PDS", "VVFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "doch soll ich Beides tun, so mustu helfen walten;", "tokens": ["doch", "soll", "ich", "Bei\u00b7des", "tun", ",", "so", "mus\u00b7tu", "hel\u00b7fen", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,", "ADV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hilfst du mir halten nicht, so hilft mich keine Pflicht.", "tokens": ["hilfst", "du", "mir", "hal\u00b7ten", "nicht", ",", "so", "hilft", "mich", "kei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich habe fast geirrt. Was soll ich dem doch geben,", "tokens": ["Ich", "ha\u00b7be", "fast", "ge\u00b7irrt", ".", "Was", "soll", "ich", "dem", "doch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "PWS", "VMFIN", "PPER", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Alles giebt und hat? und was verpflicht ich mich", "tokens": ["der", "Al\u00b7les", "giebt", "und", "hat", "?", "und", "was", "ver\u00b7pflicht", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "KON", "VAFIN", "$.", "KON", "PWS", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf etwas, das mich kn\u00fcpft und nichts doch hat auf sich?", "tokens": ["auf", "et\u00b7was", ",", "das", "mich", "kn\u00fcpft", "und", "nichts", "doch", "hat", "auf", "s