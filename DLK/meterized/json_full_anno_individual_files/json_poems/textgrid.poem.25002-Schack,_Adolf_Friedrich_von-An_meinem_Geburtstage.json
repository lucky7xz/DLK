{"textgrid.poem.25002": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An meinem Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge Tag l\u00e4\u00dft Thal und H\u00f6hn", "tokens": ["Der", "jun\u00b7ge", "Tag", "l\u00e4\u00dft", "Thal", "und", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Abglanz seines L\u00e4chelns glimmen;", "tokens": ["Im", "Ab\u00b7glanz", "sei\u00b7nes", "L\u00e4\u00b7chelns", "glim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von allen Seiten schallt Get\u00f6n", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "schallt", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herden, die an Felsen klimmen;", "tokens": ["Der", "Her\u00b7den", ",", "die", "an", "Fel\u00b7sen", "klim\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die goldnen Sommerf\u00e4den schwimmen", "tokens": ["Die", "gold\u00b7nen", "Som\u00b7mer\u00b7f\u00e4\u00b7den", "schwim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Boote durch der L\u00fcfte Meer;", "tokens": ["Wie", "Boo\u00b7te", "durch", "der", "L\u00fcf\u00b7te", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es t\u00f6nt gleich tausend Liebesstimmen", "tokens": ["Es", "t\u00f6nt", "gleich", "tau\u00b7send", "Lie\u00b7bess\u00b7tim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der V\u00f6gel Zwitschern um mich her.", "tokens": ["Der", "V\u00f6\u00b7gel", "Zwit\u00b7schern", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort unten flie\u00dft der alte Rhein;", "tokens": ["Dort", "un\u00b7ten", "flie\u00dft", "der", "al\u00b7te", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe muntre Kinder spielen;", "tokens": ["Ich", "se\u00b7he", "mun\u00b7tre", "Kin\u00b7der", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh' im heitern Sonnenschein", "tokens": ["Ich", "seh'", "im", "hei\u00b7tern", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00fcten an den schlanken Stielen", "tokens": ["Die", "Bl\u00fc\u00b7ten", "an", "den", "schlan\u00b7ken", "Stie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geschaukelt von des Windes Fl\u00fcgel;", "tokens": ["Ge\u00b7schau\u00b7kelt", "von", "des", "Win\u00b7des", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch ich mag nimmer fr\u00f6hlich sein", "tokens": ["Doch", "ich", "mag", "nim\u00b7mer", "fr\u00f6h\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schaue vom bemoosten H\u00fcgel", "tokens": ["Und", "schau\u00b7e", "vom", "be\u00b7moos\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit tr\u00fcbem Blick ins Land hinein.", "tokens": ["Mit", "tr\u00fc\u00b7bem", "Blick", "ins", "Land", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ruht' ich einst so sanft und tief,", "tokens": ["Wie", "ruht'", "ich", "einst", "so", "sanft", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh zu des Erdenlebens Kummer,", "tokens": ["Eh", "zu", "des", "Er\u00b7den\u00b7le\u00b7bens", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mich dieser Tag ins Dasein rief!", "tokens": ["Mich", "die\u00b7ser", "Tag", "ins", "Da\u00b7sein", "rief", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Nichtsein ist der beste Schlummer!", "tokens": ["Das", "Nich\u00b7tsein", "ist", "der", "bes\u00b7te", "Schlum\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer bist du, namenloses Wesen,", "tokens": ["Wer", "bist", "du", ",", "na\u00b7men\u00b7lo\u00b7ses", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das mich geweckt, als ich ihn schlief?", "tokens": ["Das", "mich", "ge\u00b7weckt", ",", "als", "ich", "ihn", "schlief", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer ist der Bittende gewesen?", "tokens": ["Wer", "ist", "der", "Bit\u00b7ten\u00b7de", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer reichte dir den Vollmachtsbrief?", "tokens": ["Wer", "reich\u00b7te", "dir", "den", "Voll\u00b7machts\u00b7brief", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch schwebt vor meinem Geist ein Bild", "tokens": ["Noch", "schwebt", "vor", "mei\u00b7nem", "Geist", "ein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meinen fr\u00fchsten Kindertagen,", "tokens": ["Aus", "mei\u00b7nen", "fr\u00fchs\u00b7ten", "Kin\u00b7der\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als mich die Mutter engelmild", "tokens": ["Als", "mich", "die", "Mut\u00b7ter", "en\u00b7gel\u00b7mild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ihrer lieben Brust getragen;", "tokens": ["An", "ih\u00b7rer", "lie\u00b7ben", "Brust", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie lie\u00df den Lebensquell mich saugen,", "tokens": ["Sie", "lie\u00df", "den", "Le\u00b7bens\u00b7quell", "mich", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der aus dem Mutterbusen quillt,", "tokens": ["Der", "aus", "dem", "Mut\u00b7ter\u00b7bu\u00b7sen", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sang und sah mir in die Augen,", "tokens": ["Und", "sang", "und", "sah", "mir", "in", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis sie den Weinenden gestillt.", "tokens": ["Bis", "sie", "den", "Wei\u00b7nen\u00b7den", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sah mich tief und tiefer an,", "tokens": ["Sie", "sah", "mich", "tief", "und", "tie\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und traur'ger wurden ihre Lieder,", "tokens": ["Und", "traur'\u00b7ger", "wur\u00b7den", "ih\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine hei\u00dfe Thr\u00e4ne rann", "tokens": ["Und", "ei\u00b7ne", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7ne", "rann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf das geliebte Kind hernieder;", "tokens": ["Auf", "das", "ge\u00b7lieb\u00b7te", "Kind", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hatte wohl zu tief gesehen", "tokens": ["Sie", "hat\u00b7te", "wohl", "zu", "tief", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ahnt' im kindlichen Gesicht", "tokens": ["Und", "ahnt'", "im", "kind\u00b7li\u00b7chen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon all die Leiden und die Wehen,", "tokens": ["Schon", "all", "die", "Lei\u00b7den", "und", "die", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor denen jetzt mein Herz zerbricht.", "tokens": ["Vor", "de\u00b7nen", "jetzt", "mein", "Herz", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Sommer flieht, der Herbst beginnt!", "tokens": ["Der", "Som\u00b7mer", "flieht", ",", "der", "Herbst", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon sinken matte Schmetterlinge", "tokens": ["Schon", "sin\u00b7ken", "mat\u00b7te", "Schmet\u00b7ter\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Bl\u00e4tter sterbend in den Wind;", "tokens": ["Und", "Bl\u00e4t\u00b7ter", "ster\u00b7bend", "in", "den", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schwalbe pr\u00fcft zum Flug die Schwinge,", "tokens": ["Die", "Schwal\u00b7be", "pr\u00fcft", "zum", "Flug", "die", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und bange zittert's durch die Reiser,", "tokens": ["Und", "ban\u00b7ge", "zit\u00b7tert's", "durch", "die", "Rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sie der k\u00e4ltre Hauch durchrinnt,", "tokens": ["Wie", "sie", "der", "k\u00e4lt\u00b7re", "Hauch", "durch\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und fl\u00fcstern h\u00f6r' ich's leis' und leiser:", "tokens": ["Und", "fl\u00fcs\u00b7tern", "h\u00f6r'", "ich's", "leis'", "und", "lei\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Komm schlafen, armes, m\u00fcdes Kind!", "tokens": ["Komm", "schla\u00b7fen", ",", "ar\u00b7mes", ",", "m\u00fc\u00b7des", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der junge Tag l\u00e4\u00dft Thal und H\u00f6hn", "tokens": ["Der", "jun\u00b7ge", "Tag", "l\u00e4\u00dft", "Thal", "und", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Abglanz seines L\u00e4chelns glimmen;", "tokens": ["Im", "Ab\u00b7glanz", "sei\u00b7nes", "L\u00e4\u00b7chelns", "glim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von allen Seiten schallt Get\u00f6n", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "schallt", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herden, die an Felsen klimmen;", "tokens": ["Der", "Her\u00b7den", ",", "die", "an", "Fel\u00b7sen", "klim\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die goldnen Sommerf\u00e4den schwimmen", "tokens": ["Die", "gold\u00b7nen", "Som\u00b7mer\u00b7f\u00e4\u00b7den", "schwim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Boote durch der L\u00fcfte Meer;", "tokens": ["Wie", "Boo\u00b7te", "durch", "der", "L\u00fcf\u00b7te", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es t\u00f6nt gleich tausend Liebesstimmen", "tokens": ["Es", "t\u00f6nt", "gleich", "tau\u00b7send", "Lie\u00b7bess\u00b7tim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN"], "meter": "-+-+