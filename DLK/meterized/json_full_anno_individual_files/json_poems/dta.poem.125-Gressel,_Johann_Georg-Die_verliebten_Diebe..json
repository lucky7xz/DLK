{"dta.poem.125": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die verliebten Diebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr M\u00e4dgens ihr seyd Diebe", "tokens": ["Ihr", "M\u00e4d\u00b7gens", "ihr", "seyd", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jhr stehlet uns das Hertz/", "tokens": ["Ihr", "steh\u00b7let", "uns", "das", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sonst vers\u00fc\u00dfte Liebe", "tokens": ["Die", "sonst", "ver\u00b7s\u00fc\u00df\u00b7te", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist nur ein herber Schmertz.", "tokens": ["Ist", "nur", "ein", "her\u00b7ber", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es lockt mit s\u00fcssen Blicken", "tokens": ["Es", "lockt", "mit", "s\u00fcs\u00b7sen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eur sch\u00f6nes Angesicht/", "tokens": ["Eur", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es sucht uns zu bestricken/", "tokens": ["Es", "sucht", "uns", "zu", "be\u00b7stri\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Netz ist aufgericht.", "tokens": ["Das", "Netz", "ist", "auf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ob gleich von euren Wangen/", "tokens": ["Ob", "gleich", "von", "eu\u00b7ren", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die mit Rubin bedeckt", "tokens": ["Die", "mit", "Ru\u00b7bin", "be\u00b7deckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Wie Purpur Rosen prangen/", "tokens": ["Wie", "Pur\u00b7pur", "Ro\u00b7sen", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wol mancher Zucker schmeckt.", "tokens": ["Wol", "man\u00b7cher", "Zu\u00b7cker", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Doch wei\u00dft da\u00df dieses K\u00fcssen", "tokens": ["Doch", "wei\u00dft", "da\u00df", "die\u00b7ses", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Uns gar zu sch\u00e4dlich ist/", "tokens": ["Uns", "gar", "zu", "sch\u00e4d\u00b7lich", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und der vergiffte Bissen", "tokens": ["Und", "der", "ver\u00b7giff\u00b7te", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Durch Marck und Adern frist.", "tokens": ["Durch", "Marck", "und", "A\u00b7dern", "frist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Mich hat noch nie vergn\u00fcget", "tokens": ["Mich", "hat", "noch", "nie", "ver\u00b7gn\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Noch ein\u2019ge Lust geschafft", "tokens": ["Noch", "ein'\u00b7ge", "Lust", "ge\u00b7schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Wenn auf den Lippen lieget", "tokens": ["Wenn", "auf", "den", "Lip\u00b7pen", "lie\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Der Wollust ", "tokens": ["Der", "Wol\u00b7lust"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Der Br\u00fcste Liebes-Rosen/", "tokens": ["Der", "Br\u00fcs\u00b7te", "Lie\u00b7bes\u00b7Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Die unvergleichlich bl\u00fchn/", "tokens": ["Die", "un\u00b7ver\u00b7gleich\u00b7lich", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Mit freuden lieb zu kosen", "tokens": ["Mit", "freu\u00b7den", "lieb", "zu", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Mag ich mich nicht bem\u00fchn/", "tokens": ["Mag", "ich", "mich", "nicht", "be\u00b7m\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Nennt sie ein Safft der Bienen/", "tokens": ["Nennt", "sie", "ein", "Safft", "der", "Bie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Ja nennt sie Zucker-Lust/", "tokens": ["Ja", "nennt", "sie", "Zu\u00b7cke\u00b7rLust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Ich will das nicht bedienen", "tokens": ["Ich", "will", "das", "nicht", "be\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Was mir ein Dornen-Wust.", "tokens": ["Was", "mir", "ein", "Dor\u00b7nen\u00b7Wust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Last ander eure Wangen", "tokens": ["Last", "an\u00b7der", "eu\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.30": {"text": "Die Br\u00fcste und die Schoo\u00df", "tokens": ["Die", "Br\u00fcs\u00b7te", "und", "die", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Bek\u00fcssen und umpfangen/", "tokens": ["Be\u00b7k\u00fcs\u00b7sen", "und", "um\u00b7pfan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Ich lebe Z\u00fcgel lo\u00df.", "tokens": ["Ich", "le\u00b7be", "Z\u00fc\u00b7gel", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Ich will in Freyheit bleiben", "tokens": ["Ich", "will", "in", "Frey\u00b7heit", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Ich mag kein Sclave seyn/", "tokens": ["Ich", "mag", "kein", "Scla\u00b7ve", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Ich will die Zeit vertreiben", "tokens": ["Ich", "will", "die", "Zeit", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Mit Lust und nicht mit Pein.", "tokens": ["Mit", "Lust", "und", "nicht", "mit", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Mein sagt/ was ist das Lieben/", "tokens": ["Mein", "sagt", "/", "was", "ist", "das", "Lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "PWS", "VAFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Und K\u00fcssen eurer Brust?", "tokens": ["Und", "K\u00fcs\u00b7sen", "eu\u00b7rer", "Brust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Nichts/ denn ein solch Betr\u00fcben/", "tokens": ["Nichts", "/", "denn", "ein", "solch", "Be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "KON", "ART", "PIAT", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.40": {"text": "Das Gallen schenckt vor Lust.", "tokens": ["Das", "Gal\u00b7len", "schenckt", "vor", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Ich hasse eure Liebe", "tokens": ["Ich", "has\u00b7se", "eu\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Eur Huld und Freundlich seyn/", "tokens": ["Eur", "Huld", "und", "Freund\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Ich halte euch vor Diebe", "tokens": ["Ich", "hal\u00b7te", "euch", "vor", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Stellt nur eur Schmeicheln ein.", "tokens": ["Stellt", "nur", "eur", "Schmei\u00b7cheln", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}