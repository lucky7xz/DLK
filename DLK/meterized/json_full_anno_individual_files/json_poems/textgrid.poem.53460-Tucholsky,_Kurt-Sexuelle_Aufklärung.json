{"textgrid.poem.53460": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sexuelle Aufkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt ein, mein Sohn, in dieses Variet\u00e9!", "tokens": ["Tritt", "ein", ",", "mein", "Sohn", ",", "in", "die\u00b7ses", "Va\u00b7ri\u00b7e\u00b7t\u00e9", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heiligen Hallen f\u00fcllt ein lieblich Odium", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Hal\u00b7len", "f\u00fcllt", "ein", "lieb\u00b7lich", "O\u00b7dium"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NE"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "von Rauchtabak, Parfums und E\u00dfb\u00fcff\u00e9.", "tokens": ["von", "Rauch\u00b7ta\u00b7bak", ",", "Par\u00b7fums", "und", "E\u00df\u00b7b\u00fcff\u00e9", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die blonde Emmy t\u00e4nzelt auf das Podium,", "tokens": ["Die", "blon\u00b7de", "Em\u00b7my", "t\u00e4n\u00b7zelt", "auf", "das", "Po\u00b7di\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der erste und der einzige Geiger schmiert \u203aKollodium\u2039", "tokens": ["der", "ers\u00b7te", "und", "der", "ein\u00b7zi\u00b7ge", "Gei\u00b7ger", "schmiert", "\u203a", "Kol\u00b7lo\u00b7di\u00b7um", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "VVFIN", "$(", "NE", "$("], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "auf seine Fiedel f\u00fcr das hohe C . . .", "tokens": ["auf", "sei\u00b7ne", "Fie\u00b7del", "f\u00fcr", "das", "ho\u00b7he", "C", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NE", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So blieb es, und so ists seit drei\u00dfig Jahren \u2013", "tokens": ["So", "blieb", "es", ",", "und", "so", "ists", "seit", "drei\u00b7\u00dfig", "Jah\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "VAFIN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "drum ist dein alter Vater mit dir hergefahren.", "tokens": ["drum", "ist", "dein", "al\u00b7ter", "Va\u00b7ter", "mit", "dir", "her\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sieh jenes M\u00e4dchen! Erster Jugendbl\u00fcte", "tokens": ["Sieh", "je\u00b7nes", "M\u00e4d\u00b7chen", "!", "Ers\u00b7ter", "Ju\u00b7gend\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PDAT", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "leichtrosa Schimmer ziert das reizende Gesicht.", "tokens": ["leicht\u00b7ro\u00b7sa", "Schim\u00b7mer", "ziert", "das", "rei\u00b7zen\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So war sie schon, als ich mich noch um sie bem\u00fchte,", "tokens": ["So", "war", "sie", "schon", ",", "als", "ich", "mich", "noch", "um", "sie", "be\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und wahrlich: ich blamiert mich nicht!", "tokens": ["und", "wahr\u00b7lich", ":", "ich", "bla\u00b7miert", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Siehst du sie jetzt, wie sie voll Scham ergl\u00fchte?", "tokens": ["Siehst", "du", "sie", "jetzt", ",", "wie", "sie", "voll", "Scham", "er\u00b7gl\u00fch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was fl\u00fcstert sie? \u00bbDet die de Motten kricht . . . !\u00ab", "tokens": ["Was", "fl\u00fcs\u00b7tert", "sie", "?", "\u00bb", "Det", "die", "de", "Mot\u00b7ten", "kricht", ".", ".", ".", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "VVFIN", "ART", "NE", "NN", "VVFIN", "$.", "$.", "$.", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wie klingt mir dieser Wahlspruch doch vertraut", "tokens": ["Wie", "klingt", "mir", "die\u00b7ser", "Wahl\u00b7spruch", "doch", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "aus jener Zeit, da ich den Referendar gebaut!", "tokens": ["aus", "je\u00b7ner", "Zeit", ",", "da", "ich", "den", "Re\u00b7fe\u00b7ren\u00b7dar", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, du meine Tugendlilie,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "mei\u00b7ne", "Tu\u00b7gend\u00b7li\u00b7lie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "du altes Flitterkleid, du Tamburin!", "tokens": ["du", "al\u00b7tes", "Flit\u00b7ter\u00b7kleid", ",", "du", "Tam\u00b7bu\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nimm du sie hin, mein Sohn \u2013 es bleibt in der Familie \u2013", "tokens": ["Nimm", "du", "sie", "hin", ",", "mein", "Sohn", "\u2013", "es", "bleibt", "in", "der", "Fa\u00b7mi\u00b7lie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und lern bei ihr: es gibt nur ein Berlin!", "tokens": ["und", "lern", "bei", "ihr", ":", "es", "gibt", "nur", "ein", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "$.", "PPER", "VVFIN", "ADV", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun aber spitz die Ohren, denn gleich singt Ottilie", "tokens": ["Nun", "a\u00b7ber", "spitz", "die", "Oh\u00b7ren", ",", "denn", "gleich", "singt", "Ot\u00b7ti\u00b7lie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "NE"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "ihr Lieblingslied vom kleinen Zeppeliihn . . .", "tokens": ["ihr", "Lieb\u00b7lings\u00b7lied", "vom", "klei\u00b7nen", "Zep\u00b7pe\u00b7liihn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kriegst du sie nicht, soll dich der Teufel holen!", "tokens": ["Kriegst", "du", "sie", "nicht", ",", "soll", "dich", "der", "Teu\u00b7fel", "ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verhalt dich brav \u2013 und damit Gott befohlen!", "tokens": ["Ver\u00b7halt", "dich", "brav", "\u2013", "und", "da\u00b7mit", "Gott", "be\u00b7foh\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$(", "KON", "PAV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Tritt ein, mein Sohn, in dieses Variet\u00e9!", "tokens": ["Tritt", "ein", ",", "mein", "Sohn", ",", "in", "die\u00b7ses", "Va\u00b7ri\u00b7e\u00b7t\u00e9", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heiligen Hallen f\u00fcllt ein lieblich Odium", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Hal\u00b7len", "f\u00fcllt", "ein", "lieb\u00b7lich", "O\u00b7dium"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NE"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "von Rauchtabak, Parfums und E\u00dfb\u00fcff\u00e9.", "tokens": ["von", "Rauch\u00b7ta\u00b7bak", ",", "Par\u00b7fums", "und", "E\u00df\u00b7b\u00fcff\u00e9", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die blonde Emmy t\u00e4nzelt auf das Podium,", "tokens": ["Die", "blon\u00b7de", "Em\u00b7my", "t\u00e4n\u00b7zelt", "auf", "das", "Po\u00b7di\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der erste und der einzige Geiger schmiert \u203aKollodium\u2039", "tokens": ["der", "ers\u00b7te", "und", "der", "ein\u00b7zi\u00b7ge", "Gei