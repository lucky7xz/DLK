{"textgrid.poem.60124": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willkommen, B\u00e4chlein! wie so hell!", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, B\u00e4chlein! wie so hell!", "tokens": ["Will\u00b7kom\u00b7men", ",", "B\u00e4ch\u00b7lein", "!", "wie", "so", "hell", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie rasch dein Gang ins Thal hernieder!", "tokens": ["Wie", "rasch", "dein", "Gang", "ins", "Thal", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer \u00f6ffnete den Felsenquell?", "tokens": ["Wer", "\u00f6ff\u00b7ne\u00b7te", "den", "Fel\u00b7sen\u00b7quell", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schuf dich keiner meiner Br\u00fcder.", "tokens": ["Es", "schuf", "dich", "kei\u00b7ner", "mei\u00b7ner", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Willkommen Zephyr, auf der Flur!", "tokens": ["Will\u00b7kom\u00b7men", "Ze\u00b7phyr", ",", "auf", "der", "Flur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "We\u00df Auge noch hat dich gesehen?", "tokens": ["We\u00df", "Au\u00b7ge", "noch", "hat", "dich", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer deine St\u00e4tte, deine Spur?", "tokens": ["Wer", "dei\u00b7ne", "St\u00e4t\u00b7te", ",", "dei\u00b7ne", "Spur", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Sohn der Erde hie\u00df dich wehen.", "tokens": ["Kein", "Sohn", "der", "Er\u00b7de", "hie\u00df", "dich", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du selbst, o B\u00e4chlein, h\u00f6rtest nie", "tokens": ["Du", "selbst", ",", "o", "B\u00e4ch\u00b7lein", ",", "h\u00f6r\u00b7test", "nie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Rauschen deiner kleinen Wellen", "tokens": ["Zum", "Rau\u00b7schen", "dei\u00b7ner", "klei\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verj\u00fcngter B\u00fcsche Melodie", "tokens": ["Ver\u00b7j\u00fcng\u00b7ter", "B\u00fc\u00b7sche", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom gr\u00fcnen Ufer sich gesellen;", "tokens": ["Vom", "gr\u00fc\u00b7nen", "U\u00b7fer", "sich", "ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dennoch redest du mit mir", "tokens": ["Und", "den\u00b7noch", "re\u00b7dest", "du", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In stillen Abendd\u00e4mmerungen;", "tokens": ["In", "stil\u00b7len", "A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schon hat dein leises Murmeln hier", "tokens": ["Schon", "hat", "dein", "lei\u00b7ses", "Mur\u00b7meln", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit s\u00fc\u00dfem Schauer mich durchdrungen.", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Schau\u00b7er", "mich", "durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du Zephyr wei\u00dft nicht, wie, erfreut", "tokens": ["Du", "Ze\u00b7phyr", "wei\u00dft", "nicht", ",", "wie", ",", "er\u00b7freut"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "NN", "VVFIN", "PTKNEG", "$,", "PWAV", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deinem Hauch, die Staude s\u00e4uselt,", "tokens": ["Von", "dei\u00b7nem", "Hauch", ",", "die", "Stau\u00b7de", "s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Bl\u00fcmchen Wohlger\u00fcche streut,", "tokens": ["Das", "Bl\u00fcm\u00b7chen", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "streut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aehre wallt, der Hain sich kr\u00e4uselt;", "tokens": ["Die", "A\u00b7eh\u00b7re", "wallt", ",", "der", "Hain", "sich", "kr\u00e4u\u00b7selt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und dennoch, gleich dem Epheu, bebt,", "tokens": ["Und", "den\u00b7noch", ",", "gleich", "dem", "E\u00b7pheu", ",", "bebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du mir lispelst von den H\u00fcgeln,", "tokens": ["Wenn", "du", "mir", "lis\u00b7pelst", "von", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein klopfend Herz; die Seele schwebt", "tokens": ["Mein", "klop\u00b7fend", "Herz", ";", "die", "See\u00b7le", "schwebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deinen unsichtbaren Fl\u00fcgeln.", "tokens": ["Auf", "dei\u00b7nen", "un\u00b7sicht\u00b7ba\u00b7ren", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Woher dies wonnige Gef\u00fchl,", "tokens": ["Wo\u00b7her", "dies", "won\u00b7ni\u00b7ge", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hoch sich hebenden Gedanken?", "tokens": ["Die", "hoch", "sich", "he\u00b7ben\u00b7den", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was rauschet mir im Wellenspiel?", "tokens": ["Was", "rau\u00b7schet", "mir", "im", "Wel\u00b7len\u00b7spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was fl\u00fcstert in des Weinstocks Ranken?", "tokens": ["Was", "fl\u00fcs\u00b7tert", "in", "des", "Wein\u00b7stocks", "Ran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Mayenl\u00fcftchen kennt mich nicht;", "tokens": ["Das", "Ma\u00b7yen\u00b7l\u00fcft\u00b7chen", "kennt", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem B\u00e4chlein sang ich j\u00fcngst die Feyer", "tokens": ["Dem", "B\u00e4ch\u00b7lein", "sang", "ich", "j\u00fcngst", "die", "Fe\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Bl\u00fcthenmonds im Rosenlicht;", "tokens": ["Des", "Bl\u00fc\u00b7then\u00b7monds", "im", "Ro\u00b7sen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm aber t\u00f6nte keine Leyer.", "tokens": ["Ihm", "a\u00b7ber", "t\u00f6n\u00b7te", "kei\u00b7ne", "Le\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Woher denn, um der Quelle Rand,", "tokens": ["Wo\u00b7her", "denn", ",", "um", "der", "Quel\u00b7le", "Rand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUI", "ART", "NN", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Woher das ahndungsvolle Wehen?", "tokens": ["Wo\u00b7her", "das", "ahn\u00b7dungs\u00b7vol\u00b7le", "We\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Geist, dem meinigen verwandt,", "tokens": ["Ein", "Geist", ",", "dem", "mei\u00b7ni\u00b7gen", "ver\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "PPOSS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df kennen mich, und mich verstehen,", "tokens": ["Mu\u00df", "ken\u00b7nen", "mich", ",", "und", "mich", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mir nahe seyn im Wasserfall,", "tokens": ["Mir", "na\u00b7he", "seyn", "im", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Hauch des Windes Antwort geben,", "tokens": ["Im", "Hauch", "des", "Win\u00b7des", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllen alles \u00fcberall", "tokens": ["Er\u00b7f\u00fcl\u00b7len", "al\u00b7les", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freud und Liebe, Kraft und Leben.", "tokens": ["Mit", "Freud", "und", "Lie\u00b7be", ",", "Kraft", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Es ist der Herr, der \u00fcberall", "tokens": ["Es", "ist", "der", "Herr", ",", "der", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wiesenduft, im Sturme schwebet,", "tokens": ["Im", "Wie\u00b7sen\u00b7duft", ",", "im", "Stur\u00b7me", "schwe\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Abendthau und Wasserfall,", "tokens": ["Der", "A\u00b7bendt\u00b7hau", "und", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Himmel, Erd' und Meer belebet;", "tokens": ["Und", "Him\u00b7mel", ",", "Erd'", "und", "Meer", "be\u00b7le\u00b7bet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er, welcher aufs besonnte Land", "tokens": ["Er", ",", "wel\u00b7cher", "aufs", "be\u00b7sonn\u00b7te", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den k\u00fchlen Flug des Zephyrs leitet,", "tokens": ["Den", "k\u00fch\u00b7len", "Flug", "des", "Ze\u00b7phyrs", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er, der mit unsichtbarer Hand", "tokens": ["Er", ",", "der", "mit", "un\u00b7sicht\u00b7ba\u00b7rer", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Wurme seinen Tisch bereitet.", "tokens": ["Dem", "Wur\u00b7me", "sei\u00b7nen", "Tisch", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der z\u00e4hlet meines Pulses Schlag,", "tokens": ["Der", "z\u00e4h\u00b7let", "mei\u00b7nes", "Pul\u00b7ses", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt meiner W\u00fcnsche leises Flehen;", "tokens": ["H\u00f6rt", "mei\u00b7ner", "W\u00fcn\u00b7sche", "lei\u00b7ses", "Fle\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, schmachtet meine Seel' ihm nach,", "tokens": ["Und", ",", "schmach\u00b7tet", "mei\u00b7ne", "Seel'", "ihm", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So f\u00fchl' ich seiner Fl\u00fcgel Wehen.", "tokens": ["So", "f\u00fchl'", "ich", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der Tag verk\u00fcndiget der Nacht,", "tokens": ["Der", "Tag", "ver\u00b7k\u00fcn\u00b7di\u00b7get", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht dem Tage seinen Nahmen,", "tokens": ["Die", "Nacht", "dem", "Ta\u00b7ge", "sei\u00b7nen", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Himmel preisen seine Macht,", "tokens": ["Die", "Him\u00b7mel", "prei\u00b7sen", "sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tief im Herzen schallt mein Amen.", "tokens": ["Und", "tief", "im", "Her\u00b7zen", "schallt", "mein", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Wohl mir, ich wei\u00df, woher es schallt,", "tokens": ["Wohl", "mir", ",", "ich", "wei\u00df", ",", "wo\u00b7her", "es", "schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es deutet hin in gro\u00dfe Fernen;", "tokens": ["Es", "deu\u00b7tet", "hin", "in", "gro\u00b7\u00dfe", "Fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Tief unter meiner Hoffnung wallt", "tokens": ["Tief", "un\u00b7ter", "mei\u00b7ner", "Hoff\u00b7nung", "wallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel hin mit seinen Sternen.", "tokens": ["Der", "Him\u00b7mel", "hin", "mit", "sei\u00b7nen", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measu