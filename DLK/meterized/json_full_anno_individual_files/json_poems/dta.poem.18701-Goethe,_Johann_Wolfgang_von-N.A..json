{"dta.poem.18701": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wer befehlen kann wird loben", "tokens": ["Wer", "be\u00b7feh\u00b7len", "kann", "wird", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVINF", "VMFIN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er wird auch wieder schelten,", "tokens": ["Und", "er", "wird", "auch", "wie\u00b7der", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das muss dir, treuer Diener,", "tokens": ["Und", "das", "muss", "dir", ",", "treu\u00b7er", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines wie das andre gelten.", "tokens": ["Ei\u00b7nes", "wie", "das", "and\u00b7re", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn er lobt wohl das Geringe,", "tokens": ["Denn", "er", "lobt", "wohl", "das", "Ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schilt auch, wo er sollte loben,", "tokens": ["Schilt", "auch", ",", "wo", "er", "soll\u00b7te", "lo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber bleibst du guter Dinge", "tokens": ["A\u00b7ber", "bleibst", "du", "gu\u00b7ter", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird er dich zuletzt erproben.", "tokens": ["Wird", "er", "dich", "zu\u00b7letzt", "er\u00b7pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so haltet\u2019s auch ihr Hohen", "tokens": ["Und", "so", "hal\u00b7tet's", "auch", "ihr", "Ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen Gott wie der Geringe,", "tokens": ["Ge\u00b7gen", "Gott", "wie", "der", "Ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Thut und leidet, wie sich\u2019s findet,", "tokens": ["Thut", "und", "lei\u00b7det", ",", "wie", "sich's", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt nur immer guter Dinge.", "tokens": ["Bleibt", "nur", "im\u00b7mer", "gu\u00b7ter", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}