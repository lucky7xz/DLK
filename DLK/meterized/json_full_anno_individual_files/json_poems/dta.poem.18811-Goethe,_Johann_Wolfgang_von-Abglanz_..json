{"dta.poem.18811": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Abglanz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Spiegel er ist mir geworden,", "tokens": ["Ein", "Spie\u00b7gel", "er", "ist", "mir", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "PPER", "VAPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich sehe so gerne hinein,", "tokens": ["Ich", "se\u00b7he", "so", "ger\u00b7ne", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Als hinge des Kaysers Orden", "tokens": ["Als", "hin\u00b7ge", "des", "Kay\u00b7sers", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An mir mit Doppelschein;", "tokens": ["An", "mir", "mit", "Dop\u00b7pel\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Nicht etwa selbstgef\u00e4llig", "tokens": ["Nicht", "et\u00b7wa", "selbst\u00b7ge\u00b7f\u00e4l\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Such\u2019 ich mich \u00fcberall;", "tokens": ["Such'", "ich", "mich", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich bin so gerne gesellig", "tokens": ["Ich", "bin", "so", "ger\u00b7ne", "ge\u00b7sel\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und das ist hier der Fall.", "tokens": ["Und", "das", "ist", "hier", "der", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich nun vorm Spiegel stehe,", "tokens": ["Wenn", "ich", "nun", "vorm", "Spie\u00b7gel", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im stillen Wittwerhaus,", "tokens": ["Im", "stil\u00b7len", "Witt\u00b7wer\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleich guckt, eh\u2019 ich mich versehe,", "tokens": ["Gleich", "guckt", ",", "eh'", "ich", "mich", "ver\u00b7se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Liebchen mit heraus.", "tokens": ["Das", "Lieb\u00b7chen", "mit", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schnell kehr\u2019 ich mich um, und wieder", "tokens": ["Schnell", "kehr'", "ich", "mich", "um", ",", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Verschwand sie die ich sah,", "tokens": ["Ver\u00b7schwand", "sie", "die", "ich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann blick ich in meine Lieder,", "tokens": ["Dann", "blick", "ich", "in", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gleich ist sie wieder da.", "tokens": ["Gleich", "ist", "sie", "wie\u00b7der", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die schreib\u2019 ich immer sch\u00f6ner", "tokens": ["Die", "schreib'", "ich", "im\u00b7mer", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mehr nach meinem Sinn,", "tokens": ["Und", "mehr", "nach", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trotz Krittler und Verh\u00f6hner,", "tokens": ["Trotz", "Kritt\u00b7ler", "und", "Ver\u00b7h\u00f6h\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu t\u00e4glichem Gewinn.", "tokens": ["Zu", "t\u00e4g\u00b7li\u00b7chem", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Bild in reichen Schranken", "tokens": ["Ihr", "Bild", "in", "rei\u00b7chen", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verherrlichet sich nur,", "tokens": ["Ver\u00b7herr\u00b7li\u00b7chet", "sich", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In goldnen Rosenranken", "tokens": ["In", "gold\u00b7nen", "Ro\u00b7sen\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und R\u00e4hmchen von Lasur.", "tokens": ["Und", "R\u00e4hm\u00b7chen", "von", "La\u00b7sur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}