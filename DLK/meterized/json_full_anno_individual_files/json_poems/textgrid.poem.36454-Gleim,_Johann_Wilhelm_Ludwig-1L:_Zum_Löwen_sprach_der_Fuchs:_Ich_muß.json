{"textgrid.poem.36454": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum L\u00f6wen sprach der Fuchs: Ich mu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum L\u00f6wen sprach der Fuchs: Ich mu\u00df", "tokens": ["Zum", "L\u00f6\u00b7wen", "sprach", "der", "Fuchs", ":", "Ich", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "$.", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dirs endlich nur gestehen, mein Verdru\u00df", "tokens": ["Dirs", "end\u00b7lich", "nur", "ge\u00b7ste\u00b7hen", ",", "mein", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "ADV", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat sonst kein Ende:", "tokens": ["Hat", "sonst", "kein", "En\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Esel spricht von dir nicht gut;", "tokens": ["Der", "E\u00b7sel", "spricht", "von", "dir", "nicht", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sagt: was ich an dir zu loben f\u00e4nde,", "tokens": ["Er", "sagt", ":", "was", "ich", "an", "dir", "zu", "lo\u00b7ben", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "PPER", "APPR", "PPER", "PTKZU", "VVINF",