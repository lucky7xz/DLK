{"dta.poem.8577": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In den Grunewald,", "tokens": ["In", "den", "Gru\u00b7ne\u00b7wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "seit f\u00fcnf Uhr fr\u00fch,", "tokens": ["seit", "f\u00fcnf", "Uhr", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "spie Berlin seine Extraz\u00fcge.", "tokens": ["spie", "Ber\u00b7lin", "sei\u00b7ne", "Ext\u00b7ra\u00b7z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ueber die Br\u00fccke von Halensee,", "tokens": ["Ue\u00b7ber", "die", "Br\u00fc\u00b7cke", "von", "Ha\u00b7len\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u00fcber Spandau, Schmargendorf, \u00fcber den Pichelsberg,", "tokens": ["\u00fc\u00b7ber", "Span\u00b7dau", ",", "Schmar\u00b7gen\u00b7dorf", ",", "\u00fc\u00b7ber", "den", "Pi\u00b7chels\u00b7berg", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "von allen Seiten,", "tokens": ["von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "zwischen trommelnden Turnerz\u00fcgen, zwischen Kremsern mit Musik,", "tokens": ["zwi\u00b7schen", "trom\u00b7meln\u00b7den", "Tur\u00b7ner\u00b7z\u00fc\u00b7gen", ",", "zwi\u00b7schen", "Krem\u00b7sern", "mit", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "entlang die schimmernde Havel,", "tokens": ["ent\u00b7lang", "die", "schim\u00b7mern\u00b7de", "Ha\u00b7vel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "kilometerten sich die Chausseefl\u00f6he.", "tokens": ["ki\u00b7lo\u00b7me\u00b7ter\u00b7ten", "sich", "die", "Chaus\u00b7see\u00b7fl\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "\u201epankow, Pankow, Pankow, Kille, Kille\u201c\u201eRixdorfer\u201c \u201eSchunkelwalzer\u201c", "tokens": ["\u201e", "pan\u00b7kow", ",", "Pan\u00b7kow", ",", "Pan\u00b7kow", ",", "Kil\u00b7le", ",", "Kil\u00b7le", "\u201c", "\u201e", "Rix\u00b7dor\u00b7fer", "\u201c", "\u201e", "Schun\u00b7kel\u00b7wal\u00b7zer", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "NE", "$,", "NN", "$,", "NN", "$(", "$(", "NN", "$(", "$(", "NN", "$("], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "\u201eholzauktion!\u201c", "tokens": ["\u201e", "holz\u00b7auk\u00b7ti\u00b7on", "!", "\u201c"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Jetzt ist es Nacht.", "tokens": ["Jetzt", "ist", "es", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Noch immer", "tokens": ["Noch", "im\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "aas der Hundequ\u00e4le", "tokens": ["aas", "der", "Hun\u00b7de\u00b7qu\u00e4\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "quietscht und emp\u00f6rt sich der Leierkasten.", "tokens": ["quietscht", "und", "em\u00b7p\u00f6rt", "sich", "der", "Lei\u00b7er\u00b7kas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Hinter den Bahndamm, zwischen die dunklen Kuscheln,", "tokens": ["Hin\u00b7ter", "den", "Bahn\u00b7damm", ",", "zwi\u00b7schen", "die", "dunk\u00b7len", "Ku\u00b7scheln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "verschwindet", "tokens": ["ver\u00b7schwin\u00b7det"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "eine brennende Cigarre, ein Pfingstkleid.", "tokens": ["ei\u00b7ne", "bren\u00b7nen\u00b7de", "Ci\u00b7gar\u00b7re", ",", "ein", "Pfingst\u00b7kleid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Luna: l\u00e4chelt.", "tokens": ["Lu\u00b7na", ":", "l\u00e4\u00b7chelt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Zwischen weggeworfnem Stullenpapier und Eierschalen", "tokens": ["Zwi\u00b7schen", "weg\u00b7ge\u00b7worf\u00b7nem", "Stul\u00b7len\u00b7pa\u00b7pier", "und", "Ei\u00b7er\u00b7scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "suchen sie die blaue Blume!", "tokens": ["su\u00b7chen", "sie", "die", "blau\u00b7e", "Blu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}