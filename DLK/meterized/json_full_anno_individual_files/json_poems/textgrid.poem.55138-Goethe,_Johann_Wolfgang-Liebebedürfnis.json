{"textgrid.poem.55138": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Liebebed\u00fcrfnis", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer vernimmt mich? ach, wem soll ich's klagen?", "tokens": ["Wer", "ver\u00b7nimmt", "mich", "?", "ach", ",", "wem", "soll", "ich's", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "XY", "$,", "PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wer's vern\u00e4hme, w\u00fcrd er mich bedauern?", "tokens": ["Wer's", "ver\u00b7n\u00e4h\u00b7me", ",", "w\u00fcrd", "er", "mich", "be\u00b7dau\u00b7ern", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, die Lippe, die so manche Freude", "tokens": ["Ach", ",", "die", "Lip\u00b7pe", ",", "die", "so", "man\u00b7che", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sonst genossen hat und sonst gegeben,", "tokens": ["Sonst", "ge\u00b7nos\u00b7sen", "hat", "und", "sonst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ist gespalten, und sie schmerzt erb\u00e4rmlich.", "tokens": ["Ist", "ge\u00b7spal\u00b7ten", ",", "und", "sie", "schmerzt", "er\u00b7b\u00e4rm\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und sie ist nicht etwa wund geworden,", "tokens": ["Und", "sie", "ist", "nicht", "et\u00b7wa", "wund", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Weil die Liebste mich zu wild ergriffen,", "tokens": ["Weil", "die", "Liebs\u00b7te", "mich", "zu", "wild", "er\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKA", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Hold mich angebissen, da\u00df sie, fester", "tokens": ["Hold", "mich", "an\u00b7ge\u00b7bis\u00b7sen", ",", "da\u00df", "sie", ",", "fes\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "PPER", "VVPP", "$,", "KOUS", "PPER", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sich des Freunds versichernd, ihn gen\u00f6sse:", "tokens": ["Sich", "des", "Freunds", "ver\u00b7si\u00b7chernd", ",", "ihn", "ge\u00b7n\u00f6s\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Nein, das zarte Lippchen ist gesprungen,", "tokens": ["Nein", ",", "das", "zar\u00b7te", "Lipp\u00b7chen", "ist", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Weil nun \u00fcber Reif und Frost die Winde", "tokens": ["Weil", "nun", "\u00fc\u00b7ber", "Reif", "und", "Frost", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Spitz und scharf und lieblos mir begegnen.", "tokens": ["Spitz", "und", "scharf", "und", "lieb\u00b7los", "mir", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und nun soll mir Saft der edlen Traube,", "tokens": ["Und", "nun", "soll", "mir", "Saft", "der", "ed\u00b7len", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Mit dem Saft der Bienen bei dem Feuer", "tokens": ["Mit", "dem", "Saft", "der", "Bie\u00b7nen", "bei", "dem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meines Herds vereinigt, Lindrung schaffen.", "tokens": ["Mei\u00b7nes", "Herds", "ver\u00b7ei\u00b7nigt", ",", "Lind\u00b7rung", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, was will das helfen, mischt die Liebe", "tokens": ["Ach", ",", "was", "will", "das", "hel\u00b7fen", ",", "mischt", "die", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VMFIN", "PDS", "VVINF", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht ein Tr\u00f6pfchen ihres Balsams drunter?", "tokens": ["Nicht", "ein", "Tr\u00f6pf\u00b7chen", "ih\u00b7res", "Bal\u00b7sams", "drun\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wer vernimmt mich? ach, wem soll ich's klagen?", "tokens": ["Wer", "ver\u00b7nimmt", "mich", "?", "ach", ",", "wem", "soll", "ich's", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "XY", "$,", "PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wer's vern\u00e4hme, w\u00fcrd er mich bedauern?", "tokens": ["Wer's", "ver\u00b7n\u00e4h\u00b7me", ",", "w\u00fcrd", "er", "mich", "be\u00b7dau\u00b7ern", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, die Lippe, die so manche Freude", "tokens": ["Ach", ",", "die", "Lip\u00b7pe", ",", "die", "so", "man\u00b7che", "Freu\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sonst genossen hat und sonst gegeben,", "tokens": ["Sonst", "ge\u00b7nos\u00b7sen", "hat", "und", "sonst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", 