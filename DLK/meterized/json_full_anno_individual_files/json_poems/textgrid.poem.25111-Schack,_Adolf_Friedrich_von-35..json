{"textgrid.poem.25111": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dahin der Jugend Wonnen,", "tokens": ["Da\u00b7hin", "der", "Ju\u00b7gend", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und selbst ihr s\u00fc\u00dfes Weh", "tokens": ["Und", "selbst", "ihr", "s\u00fc\u00b7\u00dfes", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zerstoben und zerronnen", "tokens": ["Zer\u00b7sto\u00b7ben", "und", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Fr\u00fchlings-Bl\u00fctenschnee.", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7B\u00b7l\u00fc\u00b7ten\u00b7schnee", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht jauchzt mehr zu den Sternen", "tokens": ["Nicht", "jauchzt", "mehr", "zu", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Herz wie sonst empor;", "tokens": ["Mein", "Herz", "wie", "sonst", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es starrt in \u00f6de Fernen", "tokens": ["Es", "starrt", "in", "\u00f6\u