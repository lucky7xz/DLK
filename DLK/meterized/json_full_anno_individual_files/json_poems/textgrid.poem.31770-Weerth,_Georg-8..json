ger!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "all", "mein", "\u00c4r\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und lange noch werde ich denken an", "tokens": ["Und", "lan\u00b7ge", "noch", "wer\u00b7de", "ich", "den\u00b7ken", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "VVFIN", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Euern g\u00f6ttlichen Scharlachberger.", "tokens": ["Eu\u00b7ern", "g\u00f6tt\u00b7li\u00b7chen", "Schar\u00b7lach\u00b7ber\u00b7ger", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Ade! Euer Wein war trefflich; und", "tokens": ["A\u00b7de", "!", "Eu\u00b7er", "Wein", "war", "treff\u00b7lich", ";", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "KON"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihn preis ich nach allen Winden \u2013", "tokens": ["Ihn", "preis", "ich", "nach", "al\u00b7len", "Win\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einst wird auch schlagen unsere Stund,", "tokens": ["Einst", "wird", "auch", "schla\u00b7gen", "un\u00b7se\u00b7re", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da wird sich alles finden.\u00ab", "tokens": ["Da", "wird", "sich", "al\u00b7les", "fin\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}