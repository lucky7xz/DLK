{"textgrid.poem.31770": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie schliefen. \u2013 So schlief auch Polyphem;", "tokens": ["Sie", "schlie\u00b7fen", ".", "\u2013", "So", "schlief", "auch", "Po\u00b7ly\u00b7phem", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und geblendet ward der Riese", "tokens": ["Und", "ge\u00b7blen\u00b7det", "ward", "der", "Rie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den herrlichen Dulder Odysseus. Soll", "tokens": ["Durch", "den", "herr\u00b7li\u00b7chen", "Dul\u00b7der", "O\u00b7dys\u00b7seus", ".", "Soll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$.", "VMFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ich jetzo blenden auch diese?", "tokens": ["Ich", "jet\u00b7zo", "blen\u00b7den", "auch", "die\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PDS", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ja, soll ich mit gl\u00fchendem Korkzieher euch", "tokens": ["Ja", ",", "soll", "ich", "mit", "gl\u00fc\u00b7hen\u00b7dem", "Kork\u00b7zie\u00b7her", "euch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VMFIN", "PPER", "APPR", "ADJA", "NN", "PPER"], "meter": "+-+-+--++-+", "measur