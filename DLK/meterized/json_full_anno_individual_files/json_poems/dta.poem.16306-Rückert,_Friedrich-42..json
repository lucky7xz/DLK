{"dta.poem.16306": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, auf dem Pfuhl wie schwimmt das zarte Lotosblatt!", "tokens": ["Sieh", ",", "auf", "dem", "Pfuhl", "wie", "schwimmt", "das", "zar\u00b7te", "Lo\u00b7tos\u00b7blatt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "KOKOM", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibt der Reine rein auch an unreiner Statt.", "tokens": ["So", "bleibt", "der", "Rei\u00b7ne", "rein", "auch", "an", "un\u00b7rei\u00b7ner", "Statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es sinkt nicht in die Flut, es ist von ihr gehoben,", "tokens": ["Es", "sinkt", "nicht", "in", "die", "Flut", ",", "es", "ist", "von", "ihr", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Flut netzt unten es, doch immer schwimmt es oben.", "tokens": ["Die", "Flut", "netzt", "un\u00b7ten", "es", ",", "doch", "im\u00b7mer", "schwimmt", "es", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "$,", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es wandelt dr\u00fcben Schlamm in Bl\u00fcten himmelblau,", "tokens": ["Es", "wan\u00b7delt", "dr\u00fc\u00b7ben", "Schlamm", "in", "Bl\u00fc\u00b7ten", "him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freudig f\u00e4llt darauf in jeder Nacht der Thau.", "tokens": ["Und", "freu\u00b7dig", "f\u00e4llt", "da\u00b7rauf", "in", "je\u00b7der", "Nacht", "der", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PAV", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O schilt mir nicht den Pfuhl, der solche Bl\u00fcte n\u00e4hrt!", "tokens": ["O", "schilt", "mir", "nicht", "den", "Pfuhl", ",", "der", "sol\u00b7che", "Bl\u00fc\u00b7te", "n\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dunkle Mutter ist durchs lichte Kind verkl\u00e4rt.", "tokens": ["Die", "dunk\u00b7le", "Mut\u00b7ter", "ist", "durchs", "lich\u00b7te", "Kind", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Schilt nicht die Welt, sie woll' ein reines Herz verderben;", "tokens": ["Schilt", "nicht", "die", "Welt", ",", "sie", "woll'", "ein", "rei\u00b7nes", "Herz", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie will durchs reine Herz die Reinheit selbst erwerben.", "tokens": ["Sie", "will", "durchs", "rei\u00b7ne", "Herz", "die", "Rein\u00b7heit", "selbst", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Lotosblume bl\u00fcht darum in Herzgestalt,", "tokens": ["Die", "Lo\u00b7tos\u00b7blu\u00b7me", "bl\u00fcht", "da\u00b7rum", "in", "Herz\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du zufrieden seist mit deinem Aufenthalt.", "tokens": ["Da\u00df", "du", "zu\u00b7frie\u00b7den", "seist", "mit", "dei\u00b7nem", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}