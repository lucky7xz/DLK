{"textgrid.poem.48801": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "26. Auf einen Gedenkring", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ring, an dem schlechter Nichts als Gold zu achten ist,", "tokens": ["Ring", ",", "an", "dem", "schlech\u00b7ter", "Nichts", "als", "Gold", "zu", "ach\u00b7ten", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "PIS", "KOKOM", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ring, der du billich hast den Namen von Gedenken,", "tokens": ["Ring", ",", "der", "du", "bil\u00b7lich", "hast", "den", "Na\u00b7men", "von", "Ge\u00b7den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ring, der du gehest hin der Liebsten dich zu schenken,", "tokens": ["Ring", ",", "der", "du", "ge\u00b7hest", "hin", "der", "Liebs\u00b7ten", "dich", "zu", "schen\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ring, es ist fast zu hoch, auf was du dich bem\u00fchst.", "tokens": ["Ring", ",", "es", "ist", "fast", "zu", "hoch", ",", "auf", "was", "du", "dich", "be\u00b7m\u00fchst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Ring, schaue zu, da\u00df du dich etwan nicht versiehst,", "tokens": ["Ring", ",", "schau\u00b7e", "zu", ",", "da\u00df", "du", "dich", "et\u00b7wan", "nicht", "ver\u00b7siehst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Ring, der du meiner Lust den Finger solt umschrenken.", "tokens": ["Ring", ",", "der", "du", "mei\u00b7ner", "Lust", "den", "Fin\u00b7ger", "solt", "um\u00b7schren\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ring, traust du dir, ihr Herz in Gunst mir zuzulenken,", "tokens": ["Ring", ",", "traust", "du", "dir", ",", "ihr", "Herz", "in", "Gunst", "mir", "zu\u00b7zu\u00b7len\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ring, an dem sie die Schrift der Qual des Herzens liest?", "tokens": ["Ring", ",", "an", "dem", "sie", "die", "Schrift", "der", "Qual", "des", "Her\u00b7zens", "liest", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ring, an Verm\u00f6gen arm, Ring, an der Kunst nit reich,", "tokens": ["Ring", ",", "an", "Ver\u00b7m\u00f6\u00b7gen", "arm", ",", "Ring", ",", "an", "der", "Kunst", "nit", "reich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "ADJD", "$,", "NN", "$,", "APPR", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Ring, sieh' zu, da\u00df dich nicht dein Vorwitz mache bleich.", "tokens": ["Ring", ",", "sieh'", "zu", ",", "da\u00df", "dich", "nicht", "dein", "Vor\u00b7witz", "ma\u00b7che", "bleich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ring, reise nun denn hin, Ring, reiche dich der Rechten,", "tokens": ["Ring", ",", "rei\u00b7se", "nun", "denn", "hin", ",", "Ring", ",", "rei\u00b7che", "dich", "der", "Rech\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "++-+-+++-+-+-", "measure": "unknown.measure.octa.plus"}}, "stanza.4": {"line.1": {"text": "Ring, nun zu guter Nacht! Ring, tut sie gleich wie du?", "tokens": ["Ring", ",", "nun", "zu", "gu\u00b7ter", "Nacht", "!", "Ring", ",", "tut", "sie", "gleich", "wie", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "w