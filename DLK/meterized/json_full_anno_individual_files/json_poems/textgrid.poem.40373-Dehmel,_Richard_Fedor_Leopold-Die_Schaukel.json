{"textgrid.poem.40373": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Die Schaukel", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meiner Schaukel in die H\u00f6h,", "tokens": ["Auf", "mei\u00b7ner", "Schau\u00b7kel", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was kann es Sch\u00f6neres geben!", "tokens": ["was", "kann", "es", "Sch\u00f6\u00b7ne\u00b7res", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So hoch, so weit: die ganze Chaussee", "tokens": ["So", "hoch", ",", "so", "weit", ":", "die", "gan\u00b7ze", "Chaus\u00b7see"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und alle H\u00e4user schweben.", "tokens": ["und", "al\u00b7le", "H\u00e4u\u00b7ser", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Weit \u00fcber die G\u00e4rten hoch, juchhee,", "tokens": ["Weit", "\u00fc\u00b7ber", "die", "G\u00e4r\u00b7ten", "hoch", ",", "juch\u00b7hee", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "$,", "ADJA", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ich lasse mich fliegen, fliegen;", "tokens": ["ich", "las\u00b7se", "mich", "flie\u00b7gen", ",", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und alles sieht man, Wald und See,", "tokens": ["und", "al\u00b7les", "sieht", "man", ",", "Wald", "und", "See", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIS", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ganz anders stehn und liegen.", "tokens": ["ganz", "an\u00b7ders", "stehn", "und", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hoch in die H\u00f6h! Wo ist mein Zeh?", "tokens": ["Hoch", "in", "die", "H\u00f6h", "!", "Wo", "ist", "mein", "Zeh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Himmel! ich glaube, ich falle!", "tokens": ["Im", "Him\u00b7mel", "!", "ich", "glau\u00b7be", ",", "ich", "fal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das tut so tief, so s\u00fc\u00df dann weh,", "tokens": ["Das", "tut", "so", "tief", ",", "so", "s\u00fc\u00df", "dann", "weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "ADV