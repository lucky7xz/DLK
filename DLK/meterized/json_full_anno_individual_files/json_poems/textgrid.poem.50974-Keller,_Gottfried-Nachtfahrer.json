{"textgrid.poem.50974": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Nachtfahrer", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wiegt die Nacht mit himmelweiten Schwingen", "tokens": ["Es", "wiegt", "die", "Nacht", "mit", "him\u00b7mel\u00b7wei\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich auf der S\u00fcdsee blauen Wasserg\u00e4rten,", "tokens": ["Sich", "auf", "der", "S\u00fcd\u00b7see", "blau\u00b7en", "Was\u00b7ser\u00b7g\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daraus zur\u00fcck wie Silberlilien springen", "tokens": ["Da\u00b7raus", "zu\u00b7r\u00fcck", "wie", "Sil\u00b7ber\u00b7li\u00b7li\u00b7en", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Sterne, die in tiefer Flut verkl\u00e4rten.", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "in", "tie\u00b7fer", "Flut", "ver\u00b7kl\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ein entschlummert Kind an Mutterbr\u00fcsten", "tokens": ["Wie", "ein", "ent\u00b7schlum\u00b7mert", "Kind", "an", "Mut\u00b7ter\u00b7br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruht eine Insel selig in den Wogen:", "tokens": ["Ruht", "ei\u00b7ne", "In\u00b7sel", "se\u00b7lig", "in", "den", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So weich und wei\u00df ist um die gr\u00fcnen K\u00fcsten", "tokens": ["So", "weich", "und", "wei\u00df", "ist", "um", "die", "gr\u00fc\u00b7nen", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Brandung rings, ein Mutterarm, gezogen.", "tokens": ["Die", "Bran\u00b7dung", "rings", ",", "ein", "Mut\u00b7ter\u00b7arm", ",", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wollt, es w\u00e4r mein Herz so dicht umflossen", "tokens": ["Ich", "wollt", ",", "es", "w\u00e4r", "mein", "Herz", "so", "dicht", "um\u00b7flos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einem Meer der Ruhe und der Klarheit", "tokens": ["Von", "ei\u00b7nem", "Meer", "der", "Ru\u00b7he", "und", "der", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dr\u00fcberhin ein Himmel ausgegossen,", "tokens": ["Und", "dr\u00fc\u00b7berh\u00b7in", "ein", "Him\u00b7mel", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des einz'ges Licht das Sonnenlicht der Wahrheit!", "tokens": ["Des", "einz'\u00b7ges", "Licht", "das", "Son\u00b7nen\u00b7licht", "der", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sch\u00f6ne Menschen schlafen in den B\u00fcschen,", "tokens": ["Und", "sch\u00f6\u00b7ne", "Men\u00b7schen", "schla\u00b7fen", "in", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Bildwerk in ein Blumentuch gewoben;", "tokens": ["Wie", "Bild\u00b7werk", "in", "ein", "Blu\u00b7men\u00b7tuch", "ge\u00b7wo\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ein erstorbnes Auge kann erfrischen,", "tokens": ["Was", "ein", "ers\u00b7torb\u00b7nes", "Au\u00b7ge", "kann", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das hat ein Gott hier sorglich aufgehoben. \u2013", "tokens": ["Das", "hat", "ein", "Gott", "hier", "sorg\u00b7lich", "auf\u00b7ge\u00b7ho\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Blitz \u2013 ein Krach! \u2013 die stille Luft erzittert,", "tokens": ["Ein", "Blitz", "\u2013", "ein", "Krach", "!", "\u2013", "die", "stil\u00b7le", "Luft", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$.", "$(", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dicht w\u00e4lzt ein Rauch sich auf gekr\u00e4ustem Spiegel \u2013", "tokens": ["Dicht", "w\u00e4lzt", "ein", "Rauch", "sich", "auf", "ge\u00b7kr\u00e4us\u00b7tem", "Spie\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wasserdrache, der den Raub gewittert,", "tokens": ["Ein", "Was\u00b7ser\u00b7dra\u00b7che", ",", "der", "den", "Raub", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So naht es pfeilschnell mit gespreiztem Fl\u00fcgel!", "tokens": ["So", "naht", "es", "pfeil\u00b7schnell", "mit", "ge\u00b7spreiz\u00b7tem", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wach auf, wach auf, du stiller Menschengarten!", "tokens": ["Wach", "auf", ",", "wach", "auf", ",", "du", "stil\u00b7ler", "Men\u00b7schen\u00b7gar\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gib deine Bl\u00fcte hin f\u00fcr Glaskorallen!", "tokens": ["Gib", "dei\u00b7ne", "Bl\u00fc\u00b7te", "hin", "f\u00fcr", "Glas\u00b7ko\u00b7ral\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieh, deines unschuldvollen Fleisches warten,", "tokens": ["Sieh", ",", "dei\u00b7nes", "un\u00b7schuld\u00b7vol\u00b7len", "Flei\u00b7sches", "war\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sanftes Volk, Europas scharfe Krallen!", "tokens": ["Du", "sanf\u00b7tes", "Volk", ",", "Eu\u00b7ro\u00b7pas", "schar\u00b7fe", "Kral\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Anker rasseln und die Segel sinken.", "tokens": ["Die", "An\u00b7ker", "ras\u00b7seln", "und", "die", "Se\u00b7gel", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie schneidend schallt das Wort der fremden Ferne!", "tokens": ["Wie", "schnei\u00b7dend", "schallt", "das", "Wort", "der", "frem\u00b7den", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vielhundert Bleichgesichter l\u00fcstern blinken", "tokens": ["Viel\u00b7hun\u00b7dert", "Bleich\u00b7ge\u00b7sich\u00b7ter", "l\u00fcs\u00b7tern", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im fahlen Schein der tr\u00fcben Schiffslaterne.", "tokens": ["Im", "fah\u00b7len", "Schein", "der", "tr\u00fc\u00b7ben", "Schiffs\u00b7la\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Zuvorderst aus des Schiffes schwarzen W\u00e4nden", "tokens": ["Zu\u00b7vor\u00b7derst", "aus", "des", "Schif\u00b7fes", "schwar\u00b7zen", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ragt schw\u00e4rzer in der giererf\u00fcllten Rotte", "tokens": ["Ragt", "schw\u00e4r\u00b7zer", "in", "der", "gie\u00b7rer\u00b7f\u00fcll\u00b7ten", "Rot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Christenpriester, schwingend in den H\u00e4nden", "tokens": ["Der", "Chris\u00b7ten\u00b7pries\u00b7ter", ",", "schwin\u00b7gend", "in", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Marterholz mit dem gequ\u00e4lten Gotte.", "tokens": ["Das", "Mar\u00b7ter\u00b7holz", "mit", "dem", "ge\u00b7qu\u00e4l\u00b7ten", "Got\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Es wiegt die Nacht mit himmelweiten Schwingen", "tokens": ["Es", "wiegt", "die", "Nacht", "mit", "him\u00b7mel\u00b7wei\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich auf der S\u00fcdsee blauen Wasserg\u00e4rten,", "tokens": ["Sich", "auf", "der", "S\u00fcd\u00b7see", "blau\u00b7en", "Was\u00b7ser\u00b7g\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daraus zur\u00fcck wie Silberlilien springen", "tokens": ["Da\u00b7raus", "zu\u00b7r\u00fcck", "wie", "Sil\u00b7ber\u00b7li\u00b7li\u00b7en", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Sterne, die in tiefer Flut verkl\u00e4rten.", "tokens": ["Die", "Ster\u00b7ne", ",", "die", "in", "tie\u00b7fer", "Flut", "ver\u00b7kl\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie ein entschlummert Kind an Mutterbr\u00fcsten