{"dta.poem.18601": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "P abst  P auli des  IV.  welcher  \n die Inqvisition gestifftet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt des Nero Freund/ ein Hencker der Gewissen/", "tokens": ["Hier", "liegt", "des", "Ne\u00b7ro", "Freund", "/", "ein", "Hen\u00b7cker", "der", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein heiligstes Gericht heist ihn Plutonis Sohn.", "tokens": ["Sein", "hei\u00b7ligs\u00b7tes", "Ge\u00b7richt", "heist", "ihn", "Plu\u00b7to\u00b7nis", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Allein er mu\u00df mit Recht die eigne Ruthe k\u00fcssen/", "tokens": ["Al\u00b7lein", "er", "mu\u00df", "mit", "Recht", "die", "eig\u00b7ne", "Ru\u00b7the", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Seele f\u00e4hrt nun auch zur Inqvisition.", "tokens": ["Die", "See\u00b7le", "f\u00e4hrt", "nun", "auch", "zur", "I\u00b7nqvi\u00b7si\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word"