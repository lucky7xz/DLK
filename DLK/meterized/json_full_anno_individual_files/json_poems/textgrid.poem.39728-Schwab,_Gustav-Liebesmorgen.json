{"textgrid.poem.39728": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Liebesmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelagert sprachlos sa\u00dfen wir im Kreise,", "tokens": ["Ge\u00b7la\u00b7gert", "sprach\u00b7los", "sa\u00b7\u00dfen", "wir", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Jeder sann den Morgentr\u00e4umen nach;", "tokens": ["Ein", "Je\u00b7der", "sann", "den", "Mor\u00b7gen\u00b7tr\u00e4u\u00b7men", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da \u00f6ffnete die Pforte sich, und leise", "tokens": ["Da", "\u00f6ff\u00b7ne\u00b7te", "die", "Pfor\u00b7te", "sich", ",", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tratst du herein und standst in dem Gemach,", "tokens": ["Tratst", "du", "her\u00b7ein", "und", "standst", "in", "dem", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und neigtest dich nach deiner holden Weise,", "tokens": ["Und", "neig\u00b7test", "dich", "nach", "dei\u00b7ner", "hol\u00b7den", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punc