{"textgrid.poem.42897": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Vers\u00f6hnung", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lie\u00dfe sich alles vers\u00f6hnen,", "tokens": ["Es", "lie\u00b7\u00dfe", "sich", "al\u00b7les", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wenn keine Rechenkunst es will.", "tokens": ["Wenn", "kei\u00b7ne", "Re\u00b7chen\u00b7kunst", "es", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In einer sch\u00f6nen,", "tokens": ["In", "ei\u00b7ner", "sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ganz neuen und scheuen", "tokens": ["Ganz", "neu\u00b7en", "und", "scheu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Stunde spricht ein Bereuen", "tokens": ["Stun\u00b7de", "spricht", "ein", "Be\u00b7reu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "So mutig still.", "tokens": ["So", "mu\u00b7tig", "still", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es kann ein ergreifend Gedicht", "tokens": ["Es", "kann", "ein", "er\u00b7grei\u00b7fend", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Werden, das kurze Leben,", "tokens": ["Wer\u00b7den", ",", "das", "kur\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ein Vergeben", "tokens": ["Wenn", "ein", "Ver\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aus Fr\u00f6mmigkeit schlicht", "tokens": ["Aus", "Fr\u00f6m\u00b7mig\u00b7keit", "schlicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sein Innerstes spricht.", "tokens": ["Sein", "In\u00b7ners\u00b7tes", "spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zwei Liebende auseinandergerissen:", "tokens": ["Zwei", "Lie\u00b7ben\u00b7de", "aus\u00b7ein\u00b7an\u00b7der\u00b7ge\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gut wollen und einfach sein!", "tokens": ["Gut", "wol\u00b7len", "und", "ein\u00b7fach", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "KON", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn beide das wissen,", "tokens": ["Wenn", "bei\u00b7de", "das", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "VVINF", "$,"], "meter": "-+--+-", "measure"