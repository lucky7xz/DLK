{"textgrid.poem.36296": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Mahomets Flucht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gen Medina floh Mahoma,", "tokens": ["Gen", "Me\u00b7di\u00b7na", "floh", "Ma\u00b7ho\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00fcttelt tief entbrannt von Zorne,", "tokens": ["Sch\u00fct\u00b7telt", "tief", "ent\u00b7brannt", "von", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Mutterlandes Grenze,", "tokens": ["An", "des", "Mut\u00b7ter\u00b7lan\u00b7des", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich den Staub von seinen Sohlen.", "tokens": ["Sich", "den", "Staub", "von", "sei\u00b7nen", "Soh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbundankbare Stammverwandte,", "tokens": ["\u00bb", "un\u00b7dank\u00b7ba\u00b7re", "Stamm\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo des Neides Zahn nicht ruhte,", "tokens": ["Wo", "des", "Nei\u00b7des", "Zahn", "nicht", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ich fern von euch entflohen,", "tokens": ["Bis", "ich", "fern", "von", "euch", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gern entsagend meinem Blute,", "tokens": ["Gern", "ent\u00b7sa\u00b7gend", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja den Staub auch unsers Bodens,", "tokens": ["Ja", "den", "Staub", "auch", "un\u00b7sers", "Bo\u00b7dens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mich zu rein'gen, von mir werfe,", "tokens": ["Mich", "zu", "rein'\u00b7gen", ",", "von", "mir", "wer\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Fremden Schicksals Rufe folgend.", "tokens": ["Frem\u00b7den", "Schick\u00b7sals", "Ru\u00b7fe", "fol\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "O, ihr tr\u00fcbgesinnten Herzen,", "tokens": ["O", ",", "ihr", "tr\u00fcb\u00b7ge\u00b7sinn\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Anzufeinden stets das Hohe!", "tokens": ["An\u00b7zu\u00b7fein\u00b7den", "stets", "das", "Ho\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Welcher Geist hatt' euch verdunkelt,", "tokens": ["Wel\u00b7cher", "Geist", "hatt'", "euch", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df ihr mich nicht dulden wolltet!", "tokens": ["Da\u00df", "ihr", "mich", "nicht", "dul\u00b7den", "woll\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "War vom adlichsten der St\u00e4mme", "tokens": ["War", "vom", "ad\u00b7lichs\u00b7ten", "der", "St\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.17": {"text": "Ich nicht ehrenvoll entsprossen?", "tokens": ["Ich", "nicht", "eh\u00b7ren\u00b7voll", "ent\u00b7spros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wem noch flie\u00dfen von den Lippen", "tokens": ["Wem", "noch", "flie\u00b7\u00dfen", "von", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Des Gesanges Str\u00f6me goldner?", "tokens": ["Des", "Ge\u00b7san\u00b7ges", "Str\u00f6\u00b7me", "gold\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sieben Dichterwerke strahlen", "tokens": ["Sie\u00b7ben", "Dich\u00b7ter\u00b7wer\u00b7ke", "strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Auf den goldgewirkten Rollen;", "tokens": ["Auf", "den", "gold\u00b7ge\u00b7wirk\u00b7ten", "Rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Doch mit nichten darf ich scheuen,", "tokens": ["Doch", "mit", "nich\u00b7ten", "darf", "ich", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "K\u00fchn zu spannen gleichen Bogen,", "tokens": ["K\u00fchn", "zu", "span\u00b7nen", "glei\u00b7chen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Hoher Rede Kunstgewebe", "tokens": ["Ho\u00b7her", "Re\u00b7de", "Kunst\u00b7ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Schm\u00fcckend zu so edelm Lohne.", "tokens": ["Schm\u00fc\u00b7ckend", "zu", "so", "e\u00b7delm", "Loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Denn nicht eilen von der Lippe", "tokens": ["Denn", "nicht", "ei\u00b7len", "von", "der", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Fl\u00fcchtig mir nur sch\u00f6ne Worte;", "tokens": ["Fl\u00fcch\u00b7tig", "mir", "nur", "sch\u00f6\u00b7ne", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Die ich dichte, meine Rede", "tokens": ["Die", "ich", "dich\u00b7te", ",", "mei\u00b7ne", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Quillt hervor aus dem Verborgnen,", "tokens": ["Quillt", "her\u00b7vor", "aus", "dem", "Ver\u00b7borg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Schwingt sich ruhig fort im Sturme,", "tokens": ["Schwingt", "sich", "ru\u00b7hig", "fort", "im", "Stur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Flammend steigt sie auf zur Sonne,", "tokens": ["Flam\u00b7mend", "steigt", "sie", "auf", "zur", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und ich darf mich k\u00fchnlich stellen", "tokens": ["Und", "ich", "darf", "mich", "k\u00fchn\u00b7lich", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Zu den Alten unsers Volkes.", "tokens": ["Zu", "den", "Al\u00b7ten", "un\u00b7sers", "Vol\u00b7kes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wer ist kundiger des Ruhmes,", "tokens": ["Wer", "ist", "kun\u00b7di\u00b7ger", "des", "Ruh\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Den die V\u00e4ter einst erworben,", "tokens": ["Den", "die", "V\u00e4\u00b7ter", "einst", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Jene Helden unsers Stammes,", "tokens": ["Je\u00b7ne", "Hel\u00b7den", "un\u00b7sers", "Stam\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Denen nicht die Enkel folgen?", "tokens": ["De\u00b7nen", "nicht", "die", "En\u00b7kel", "fol\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Das ist Lug nur und Verleumdung,", "tokens": ["Das", "ist", "Lug", "nur", "und", "Ver\u00b7leum\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Da\u00df ich Neues stiften wollte,", "tokens": ["Da\u00df", "ich", "Neu\u00b7es", "stif\u00b7ten", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Mich nur meinend, wie ihr sprachet,", "tokens": ["Mich", "nur", "mei\u00b7nend", ",", "wie", "ihr", "spra\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Einzig dienen meinem Stolze.", "tokens": ["Ein\u00b7zig", "die\u00b7nen", "mei\u00b7nem", "Stol\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Unsrer V\u00e4ter alten Glauben,", "tokens": ["Uns\u00b7rer", "V\u00e4\u00b7ter", "al\u00b7ten", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "F\u00fcr den mancher Held gestorben,", "tokens": ["F\u00fcr", "den", "man\u00b7cher", "Held", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Eh' die falschen G\u00f6tzen kamen,", "tokens": ["Eh'", "die", "fal\u00b7schen", "G\u00f6t\u00b7zen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Hab' ich neu entz\u00fcnden wollen;", "tokens": ["Hab'", "ich", "neu", "ent\u00b7z\u00fcn\u00b7den", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Jenen hohen Feuerglauben,", "tokens": ["Je\u00b7nen", "ho\u00b7hen", "Feu\u00b7er\u00b7glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Der dem Born des Lichts entflossen,", "tokens": ["Der", "dem", "Born", "des", "Lichts", "ent\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Aus des Lichtes Strahlen wollt' ich", "tokens": ["Aus", "des", "Lich\u00b7tes", "Strah\u00b7len", "wollt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Bilden Einer Wahrheit Sonne,", "tokens": ["Bil\u00b7den", "Ei\u00b7ner", "Wahr\u00b7heit", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Und der neuen G\u00f6tzen Schimmer", "tokens": ["Und", "der", "neu\u00b7en", "G\u00f6t\u00b7zen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Niedersplittern auf den Boden.", "tokens": ["Nie\u00b7der\u00b7split\u00b7tern", "auf", "den", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Dunkel wogen nun die Winde,", "tokens": ["Dun\u00b7kel", "wo\u00b7gen", "nun", "die", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Und ich sehe schon die Wolken,", "tokens": ["Und", "ich", "se\u00b7he", "schon", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Die mein Vaterland umkreisen,", "tokens": ["Die", "mein", "Va\u00b7ter\u00b7land", "um\u00b7krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Und die Donner die ihm drohen.", "tokens": ["Und", "die", "Don\u00b7ner", "die", "ihm", "dro\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wach geworden ist das Wilde,", "tokens": ["Wach", "ge\u00b7wor\u00b7den", "ist", "das", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Die Verw\u00fcstung braust im Strome;", "tokens": ["Die", "Ver\u00b7w\u00fcs\u00b7tung", "braust", "im", "Stro\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Fluten wachsen \u00fcber Fluten", "tokens": ["Flu\u00b7ten", "wach\u00b7sen", "\u00fc\u00b7ber", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Jenem Lande, dem entflohen", "tokens": ["Je\u00b7nem", "Lan\u00b7de", ",", "dem", "ent\u00b7flo\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Noch mein Liebesblick gefesselt", "tokens": ["Noch", "mein", "Lie\u00b7bes\u00b7blick", "ge\u00b7fes\u00b7selt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Treulich anh\u00e4ngt, oft betrogen.", "tokens": ["Treu\u00b7lich", "an\u00b7h\u00e4ngt", ",", "oft", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Traurend flieh' ich, schaue traurend", "tokens": ["Trau\u00b7rend", "flieh'", "ich", ",", "schau\u00b7e", "trau\u00b7rend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "R\u00fcckw\u00e4rts nach dem Mutterboden,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "nach", "dem", "Mut\u00b7ter\u00b7bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Einsam wandelnd durch die W\u00fcste,", "tokens": ["Ein\u00b7sam", "wan\u00b7delnd", "durch", "die", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Als der letzte meines Volkes.", "tokens": ["Als", "der", "letz\u00b7te", "mei\u00b7nes", "Vol\u00b7kes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gen Medina floh Mahoma,", "tokens": ["Gen", "Me\u00b7di\u00b7na", "floh", "Ma\u00b7ho\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00fcttelt tief entbrannt von Zorne,", "tokens": ["Sch\u00fct\u00b7telt", "tief", "ent\u00b7brannt", "von", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An des Mutterlandes Grenze,", "tokens": ["An", "des", "Mut\u00b7ter\u00b7lan\u00b7des", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich den Staub von seinen Sohlen.", "tokens": ["Sich", "den", "Staub", "von", "sei\u00b7nen", "Soh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbundankbare Stammverwandte,", "tokens": ["\u00bb", "un\u00b7dank\u00b7ba\u00b7re", "Stamm\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo des Neides Zahn nicht ruhte,", "tokens": ["Wo", "des", "Nei\u00b7des", "Zahn", "nicht", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis ich fern von euch entflohen,", "tokens": ["Bis", "ich", "fern", "von", "euch", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gern entsagend meinem Blute,", "tokens": ["Gern", "ent\u00b7sa\u00b7gend", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja den Staub auch unsers Bodens,", "tokens": ["Ja", "den", "Staub", "auch", "un\u00b7sers", "Bo\u00b7dens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mich zu rein'gen, von mir werfe,", "tokens": ["Mich", "zu", "rein'\u00b7gen", ",", "von", "mir", "wer\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Fremden Schicksals Rufe folgend.", "tokens": ["Frem\u00b7den", "Schick\u00b7sals", "Ru\u00b7fe", "fol\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "O, ihr tr\u00fcbgesinnten Herzen,", "tokens": ["O", ",", "ihr", "tr\u00fcb\u00b7ge\u00b7sinn\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Anzufeinden stets das Hohe!", "tokens": ["An\u00b7zu\u00b7fein\u00b7den", "stets", "das", "Ho\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Welcher Geist hatt' euch verdunkelt,", "tokens": ["Wel\u00b7cher", "Geist", "hatt'", "euch", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df ihr mich nicht dulden wolltet!", "tokens": ["Da\u00df", "ihr", "mich", "nicht", "dul\u00b7den", "woll\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "War vom adlichsten der St\u00e4mme", "tokens": ["War", "vom", "ad\u00b7lichs\u00b7ten", "der", "St\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.17": {"text": "Ich nicht ehrenvoll entsprossen?", "tokens": ["Ich", "nicht", "eh\u00b7ren\u00b7voll", "ent\u00b7spros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wem noch flie\u00dfen von den Lippen", "tokens": ["Wem", "noch", "flie\u00b7\u00dfen", "von", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Des Gesanges Str\u00f6me goldner?", "tokens": ["Des", "Ge\u00b7san\u00b7ges", "Str\u00f6\u00b7me", "gold\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sieben Dichterwerke strahlen", "tokens": ["Sie\u00b7ben", "Dich\u00b7ter\u00b7wer\u00b7ke", "strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Auf den goldgewirkten Rollen;", "tokens": ["Auf", "den", "gold\u00b7ge\u00b7wirk\u00b7ten", "Rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Doch mit nichten darf ich scheuen,", "tokens": ["Doch", "mit", "nich\u00b7ten", "darf", "ich", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "K\u00fchn zu spannen gleichen Bogen,", "tokens": ["K\u00fchn", "zu", "span\u00b7nen", "glei\u00b7chen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Hoher Rede Kunstgewebe", "tokens": ["Ho\u00b7her", "Re\u00b7de", "Kunst\u00b7ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Schm\u00fcckend zu so edelm Lohne.", "tokens": ["Schm\u00fc\u00b7ckend", "zu", "so", "e\u00b7delm", "Loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Denn nicht eilen von der Lippe", "tokens": ["Denn", "nicht", "ei\u00b7len", "von", "der", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Fl\u00fcchtig mir nur sch\u00f6ne Worte;", "tokens": ["Fl\u00fcch\u00b7tig", "mir", "nur", "sch\u00f6\u00b7ne", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Die ich dichte, meine Rede", "tokens": ["Die", "ich", "dich\u00b7te", ",", "mei\u00b7ne", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Quillt hervor aus dem Verborgnen,", "tokens": ["Quillt", "her\u00b7vor", "aus", "dem", "Ver\u00b7borg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Schwingt sich ruhig fort im Sturme,", "tokens": ["Schwingt", "sich", "ru\u00b7hig", "fort", "im", "Stur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Flammend steigt sie auf zur Sonne,", "tokens": ["Flam\u00b7mend", "steigt", "sie", "auf", "zur", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und ich darf mich k\u00fchnlich stellen", "tokens": ["Und", "ich", "darf", "mich", "k\u00fchn\u00b7lich", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Zu den Alten unsers Volkes.", "tokens": ["Zu", "den", "Al\u00b7ten", "un\u00b7sers", "Vol\u00b7kes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wer ist kundiger des Ruhmes,", "tokens": ["Wer", "ist", "kun\u00b7di\u00b7ger", "des", "Ruh\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Den die V\u00e4ter einst erworben,", "tokens": ["Den", "die", "V\u00e4\u00b7ter", "einst", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Jene Helden unsers Stammes,", "tokens": ["Je\u00b7ne", "Hel\u00b7den", "un\u00b7sers", "Stam\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Denen nicht die Enkel folgen?", "tokens": ["De\u00b7nen", "nicht", "die", "En\u00b7kel", "fol\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Das ist Lug nur und Verleumdung,", "tokens": ["Das", "ist", "Lug", "nur", "und", "Ver\u00b7leum\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Da\u00df ich Neues stiften wollte,", "tokens": ["Da\u00df", "ich", "Neu\u00b7es", "stif\u00b7ten", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Mich nur meinend, wie ihr sprachet,", "tokens": ["Mich", "nur", "mei\u00b7nend", ",", "wie", "ihr", "spra\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Einzig dienen meinem Stolze.", "tokens": ["Ein\u00b7zig", "die\u00b7nen", "mei\u00b7nem", "Stol\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Unsrer V\u00e4ter alten Glauben,", "tokens": ["Uns\u00b7rer", "V\u00e4\u00b7ter", "al\u00b7ten", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "F\u00fcr den mancher Held gestorben,", "tokens": ["F\u00fcr", "den", "man\u00b7cher", "Held", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Eh' die falschen G\u00f6tzen kamen,", "tokens": ["Eh'", "die", "fal\u00b7schen", "G\u00f6t\u00b7zen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Hab' ich neu entz\u00fcnden wollen;", "tokens": ["Hab'", "ich", "neu", "ent\u00b7z\u00fcn\u00b7den", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Jenen hohen Feuerglauben,", "tokens": ["Je\u00b7nen", "ho\u00b7hen", "Feu\u00b7er\u00b7glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Der dem Born des Lichts entflossen,", "tokens": ["Der", "dem", "Born", "des", "Lichts", "ent\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Aus des Lichtes Strahlen wollt' ich", "tokens": ["Aus", "des", "Lich\u00b7tes", "Strah\u00b7len", "wollt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VMFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Bilden Einer Wahrheit Sonne,", "tokens": ["Bil\u00b7den", "Ei\u00b7ner", "Wahr\u00b7heit", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Und der neuen G\u00f6tzen Schimmer", "tokens": ["Und", "der", "neu\u00b7en", "G\u00f6t\u00b7zen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Niedersplittern auf den Boden.", "tokens": ["Nie\u00b7der\u00b7split\u00b7tern", "auf", "den", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Dunkel wogen nun die Winde,", "tokens": ["Dun\u00b7kel", "wo\u00b7gen", "nun", "die", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Und ich sehe schon die Wolken,", "tokens": ["Und", "ich", "se\u00b7he", "schon", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Die mein Vaterland umkreisen,", "tokens": ["Die", "mein", "Va\u00b7ter\u00b7land", "um\u00b7krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Und die Donner die ihm drohen.", "tokens": ["Und", "die", "Don\u00b7ner", "die", "ihm", "dro\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wach geworden ist das Wilde,", "tokens": ["Wach", "ge\u00b7wor\u00b7den", "ist", "das", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Die Verw\u00fcstung braust im Strome;", "tokens": ["Die", "Ver\u00b7w\u00fcs\u00b7tung", "braust", "im", "Stro\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Fluten wachsen \u00fcber Fluten", "tokens": ["Flu\u00b7ten", "wach\u00b7sen", "\u00fc\u00b7ber", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Jenem Lande, dem entflohen", "tokens": ["Je\u00b7nem", "Lan\u00b7de", ",", "dem", "ent\u00b7flo\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Noch mein Liebesblick gefesselt", "tokens": ["Noch", "mein", "Lie\u00b7bes\u00b7blick", "ge\u00b7fes\u00b7selt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Treulich anh\u00e4ngt, oft betrogen.", "tokens": ["Treu\u00b7lich", "an\u00b7h\u00e4ngt", ",", "oft", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Traurend flieh' ich, schaue traurend", "tokens": ["Trau\u00b7rend", "flieh'", "ich", ",", "schau\u00b7e", "trau\u00b7rend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "R\u00fcckw\u00e4rts nach dem Mutterboden,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "nach", "dem", "Mut\u00b7ter\u00b7bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Einsam wandelnd durch die W\u00fcste,", "tokens": ["Ein\u00b7sam", "wan\u00b7delnd", "durch", "die", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Als der letzte meines Volkes.", "tokens": ["Als", "der", "letz\u00b7te", "mei\u00b7nes", "Vol\u00b7kes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}