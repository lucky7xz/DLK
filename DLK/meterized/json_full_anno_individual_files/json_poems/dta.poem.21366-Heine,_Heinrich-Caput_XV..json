{"dta.poem.21366": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er sprach mit schmunzelndem Gesicht,", "tokens": ["Er", "sprach", "mit", "schmun\u00b7zeln\u00b7dem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich ihn ansah verwundert:", "tokens": ["Als", "ich", "ihn", "an\u00b7sah", "ver\u00b7wun\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eich zahle einen Dukaten per Mann,", "tokens": ["\u201e", "ich", "zah\u00b7le", "ei\u00b7nen", "Du\u00b7ka\u00b7ten", "per", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Sold, nach jedem Jahrhundert.\u201c", "tokens": ["Als", "Sold", ",", "nach", "je\u00b7dem", "Jahr\u00b7hun\u00b7dert", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NE", "$,", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Saale wo die Pferde stehn", "tokens": ["Im", "Saa\u00b7le", "wo", "die", "Pfer\u00b7de", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In langen, schweigenden Reihen,", "tokens": ["In", "lan\u00b7gen", ",", "schwei\u00b7gen\u00b7den", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da rieb der Kaiser sich die H\u00e4nd\u2019,", "tokens": ["Da", "rieb", "der", "Kai\u00b7ser", "sich", "die", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schien sonderbar sich zu freuen.", "tokens": ["Schien", "son\u00b7der\u00b7bar", "sich", "zu", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Er z\u00e4hlte die G\u00e4ule, St\u00fcck vor St\u00fcck,", "tokens": ["Er", "z\u00e4hl\u00b7te", "die", "G\u00e4u\u00b7le", ",", "St\u00fcck", "vor", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kl\u00e4tschelte ihnen die Rippen;", "tokens": ["Und", "kl\u00e4t\u00b7schel\u00b7te", "ih\u00b7nen", "die", "Rip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er z\u00e4hlte und z\u00e4hlte, mit \u00e4ngstlicher Hast", "tokens": ["Er", "z\u00e4hl\u00b7te", "und", "z\u00e4hl\u00b7te", ",", "mit", "\u00e4ngst\u00b7li\u00b7cher", "Hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bewegten sich seine Lippen.", "tokens": ["Be\u00b7weg\u00b7ten", "sich", "sei\u00b7ne", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201edas ist noch nicht die rechte Zahl\u201c \u2013", "tokens": ["\u201e", "das", "ist", "noch", "nicht", "die", "rech\u00b7te", "Zahl", "\u201c", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach er zuletzt verdrossen \u2013", "tokens": ["Sprach", "er", "zu\u00b7letzt", "ver\u00b7dros\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201esoldaten und Waffen hab\u2019 ich genung,", "tokens": ["\u201e", "sol\u00b7da\u00b7ten", "und", "Waf\u00b7fen", "hab'", "ich", "ge\u00b7nung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "KON", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch fehlt es noch an Rossen.", "tokens": ["Doch", "fehlt", "es", "noch", "an", "Ros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}