{"dta.poem.2614": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Todes-Liebe  \n  Jfr. M. S. S. v. L. den 7. Aug.  1667.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wilstu/ Seelige/ nicht mehr im Schatten leben?", "tokens": ["So", "wils\u00b7tu", "/", "See\u00b7li\u00b7ge", "/", "nicht", "mehr", "im", "Schat\u00b7ten", "le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "NN", "$(", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So eilt dein reiner Geist dem Abend zu entgehn?", "tokens": ["So", "eilt", "dein", "rei\u00b7ner", "Geist", "dem", "A\u00b7bend", "zu", "ent\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sol kein sterblich Arm dir hier ein Brant-Kleid web\u1ebd?", "tokens": ["So", "sol", "kein", "sterb\u00b7lich", "Arm", "dir", "hier", "ein", "Brant\u00b7Kleid", "we\u00b7b\u1ebd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "ADJD", "NN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nein; denn du wilst verkl\u00e4rt dort bey den Engeln stehn.", "tokens": ["Nein", ";", "denn", "du", "wilst", "ver\u00b7kl\u00e4rt", "dort", "bey", "den", "En\u00b7geln", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "PPER", "VMFIN", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beliebt dir ferner nicht den Schau-Platz zu beschreiten/", "tokens": ["Be\u00b7liebt", "dir", "fer\u00b7ner", "nicht", "den", "Schau\u00b7Platz", "zu", "be\u00b7schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der deine Jugend selbst zum Bild der Sch\u00f6nheit macht?", "tokens": ["Der", "dei\u00b7ne", "Ju\u00b7gend", "selbst", "zum", "Bild", "der", "Sch\u00f6n\u00b7heit", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schl\u00e4gstu gro\u00dfm\u00fcthig aus so viel Ergetzligkeiten/", "tokens": ["Schl\u00e4gs\u00b7tu", "gro\u00df\u00b7m\u00fct\u00b7hig", "aus", "so", "viel", "Er\u00b7getz\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ADV", "PIAT", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Und legst ver\u00e4chtlich hin den Fr\u00fchling deiner Pracht?", "tokens": ["Und", "legst", "ver\u00b7\u00e4cht\u00b7lich", "hin", "den", "Fr\u00fch\u00b7ling", "dei\u00b7ner", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mu\u00df nun das schwartze Grab der Glieder Anmuth fressen?", "tokens": ["Mu\u00df", "nun", "das", "schwart\u00b7ze", "Grab", "der", "Glie\u00b7der", "An\u00b7muth", "fres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verf\u00e4llt der Jahre Blum in einen d\u00fcrren Sand?", "tokens": ["Ver\u00b7f\u00e4llt", "der", "Jah\u00b7re", "Blum", "in", "ei\u00b7nen", "d\u00fcr\u00b7ren", "Sand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kr\u00f6hnt dein so sch\u00f6nes Haupt ein Strauch von Leid-Cypressen/", "tokens": ["Kr\u00b7\u00f6hnt", "dein", "so", "sch\u00f6\u00b7nes", "Haupt", "ein", "Strauch", "von", "Lei\u00b7dCy\u00b7pres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADV", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Da doch die gantze Stadt dir Myrten zuerkannt?", "tokens": ["Da", "doch", "die", "gant\u00b7ze", "Stadt", "dir", "Myr\u00b7ten", "zu\u00b7er\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach jal so viel/ deucht uns/ es sey von dir verdorben/", "tokens": ["Ach", "jal", "so", "viel", "/", "deucht", "uns", "/", "es", "sey", "von", "dir", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ADV", "ADV", "$(", "VVFIN", "PPER", "$(", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wir albern Sterblichen/ wir nennen di\u00df Verlust/", "tokens": ["Wir", "al\u00b7bern", "Sterb\u00b7li\u00b7chen", "/", "wir", "nen\u00b7nen", "di\u00df", "Ver\u00b7lust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$(", "PPER", "VVFIN", "PDS", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da doch die Seeligste im wenigsten gestorben/", "tokens": ["Da", "doch", "die", "See\u00b7ligs\u00b7te", "im", "we\u00b7nigs\u00b7ten", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "PIS", "VVPP", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "Sie lebt/ und lieget nun dem Heyland an der Brust.", "tokens": ["Sie", "lebt", "/", "und", "lie\u00b7get", "nun", "dem", "Hey\u00b7land", "an", "der", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Fleuch/ rieff er/ Freundin/ fleuch/ und geh\u2019 in deine Kammer/", "tokens": ["Fleuch", "/", "rieff", "er", "/", "Freun\u00b7din", "/", "fleuch", "/", "und", "geh'", "in", "dei\u00b7ne", "Kam\u00b7mer", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "$(", "NN", "$(", "NN", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Verbirg dich/ bi\u00df du wirst mein Antlitz wieder sehn/", "tokens": ["Ver\u00b7birg", "dich", "/", "bi\u00df", "du", "wirst", "mein", "Ant\u00b7litz", "wie\u00b7der", "sehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Jrrsaal dieser Welt hegt nichts als Angst und Jammer/", "tokens": ["Der", "Jrr\u00b7saal", "die\u00b7ser", "Welt", "hegt", "nichts", "als", "Angst", "und", "Jam\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wer sich vergehen wird/ umb den ists auch geschehn.", "tokens": ["Wer", "sich", "ver\u00b7ge\u00b7hen", "wird", "/", "umb", "den", "ists", "auch", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVINF", "VAFIN", "$(", "APPR", "ART", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Stimme trieb sie an die Erde zu verlachen/", "tokens": ["Die", "Stim\u00b7me", "trieb", "sie", "an", "die", "Er\u00b7de", "zu", "ver\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und was sonst pr\u00e4chtig scheint/ das sah\u2019 ihr he\u00dflich aus/", "tokens": ["Und", "was", "sonst", "pr\u00e4ch\u00b7tig", "scheint", "/", "das", "sah'", "ihr", "he\u00df\u00b7lich", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie wuste/ da\u00df wie gro\u00df wir unsre Wercke machen/", "tokens": ["Sie", "wus\u00b7te", "/", "da\u00df", "wie", "gro\u00df", "wir", "uns\u00b7re", "Wer\u00b7cke", "ma\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "KOKOM", "ADJD", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie endlich doch vergehn in Asche/ Staub und Grau\u00df.", "tokens": ["Sie", "end\u00b7lich", "doch", "ver\u00b7gehn", "in", "A\u00b7sche", "/", "Staub", "und", "Grau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da\u00df die Begierden uns im Leben noch verzehren/", "tokens": ["Da\u00df", "die", "Be\u00b7gier\u00b7den", "uns", "im", "Le\u00b7ben", "noch", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Kindheit kennt sich nicht/ die Jugend brennt voll Gluth/", "tokens": ["Die", "Kind\u00b7heit", "kennt", "sich", "nicht", "/", "die", "Ju\u00b7gend", "brennt", "voll", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "$(", "ART", "NN", "VVFIN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den Kern der besten Zeit wird Sorg\u2019 und Neid beschweren/", "tokens": ["Den", "Kern", "der", "bes\u00b7ten", "Zeit", "wird", "Sor\u00b7g'", "und", "Neid", "be\u00b7schwe\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Das Alter wei\u00df es nicht/ wie es so kindisch thut.", "tokens": ["Das", "Al\u00b7ter", "wei\u00df", "es", "nicht", "/", "wie", "es", "so", "kin\u00b7disch", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und solt\u2019 ein zarter Geist nicht diese Dornen fliehen/", "tokens": ["Und", "solt'", "ein", "zar\u00b7ter", "Geist", "nicht", "die\u00b7se", "Dor\u00b7nen", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die umb der Keuschheit Blum/ als Feind\u2019 im Angriff stehn?", "tokens": ["Die", "umb", "der", "Keuschheit", "Blum", "/", "als", "Feind'", "im", "An\u00b7griff", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NE", "$(", "KOUS", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Sol sich ein reines Hertz dem Netze nicht entziehen/", "tokens": ["Sol", "sich", "ein", "rei\u00b7nes", "Hertz", "dem", "Net\u00b7ze", "nicht", "ent\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das zwar die Wollust spinnt/ doch macht/ da\u00df wir vergehn?", "tokens": ["Das", "zwar", "die", "Wol\u00b7lust", "spinnt", "/", "doch", "macht", "/", "da\u00df", "wir", "ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVFIN", "$(", "ADV", "VVFIN", "$(", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es hat die ", "tokens": ["Es", "hat", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.34": {"text": "Als die ihr Br\u00e4utigam ihr von dem Himmel wie\u00df/", "tokens": ["Als", "die", "ihr", "Br\u00e4u\u00b7ti\u00b7gam", "ihr", "von", "dem", "Him\u00b7mel", "wie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und die uns Sterbliche mit GOtt so fest verbindet/", "tokens": ["Und", "die", "uns", "Sterb\u00b7li\u00b7che", "mit", "Gott", "so", "fest", "ver\u00b7bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "NN", "APPR", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und zu Einwohnern macht in jenem Paradie\u00df.", "tokens": ["Und", "zu", "Ein\u00b7woh\u00b7nern", "macht", "in", "je\u00b7nem", "Pa\u00b7ra\u00b7die\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wen solche Liebe r\u00fchrt/ und solch Verlangen brennet/", "tokens": ["Wen", "sol\u00b7che", "Lie\u00b7be", "r\u00fchrt", "/", "und", "solch", "Ver\u00b7lan\u00b7gen", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$(", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der eilet aus dem Thal der Thr\u00e4nen Tag und Nacht/", "tokens": ["Der", "ei\u00b7let", "aus", "dem", "Thal", "der", "Thr\u00e4\u00b7nen", "Tag", "und", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wei\u00df/ da\u00df weder Noth noch Tod den Vorsatz trennet/", "tokens": ["Und", "wei\u00df", "/", "da\u00df", "we\u00b7der", "Noth", "noch", "Tod", "den", "Vor\u00b7satz", "tren\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "KON", "NN", "ADV", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Den Glaube unterbaut/ und Andacht embsig macht.", "tokens": ["Den", "Glau\u00b7be", "un\u00b7ter\u00b7baut", "/", "u