{"dta.poem.4389": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unbilliger  \n Gebrauch unserer kurzen Zeit,  \n samt einem  \n  bessern Begriff von derselben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem ewig-seligen ununterbrochenen Vergn\u00fcgen", "tokens": ["In", "ei\u00b7nem", "e\u00b7wig\u00b7se\u00b7li\u00b7gen", "un\u00b7un\u00b7ter\u00b7bro\u00b7che\u00b7nen", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "War, ist, und bleibt die wahre Gottheit von Ewigkeit", "tokens": ["War", ",", "ist", ",", "und", "bleibt", "die", "wah\u00b7re", "Got\u00b7theit", "von", "E\u00b7wig\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "VAFIN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "zu Ewigkeit.", "tokens": ["zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Erde w\u00e4hret eine lange uns allen unbekannte Zeit.", "tokens": ["Die", "Er\u00b7de", "w\u00e4h\u00b7ret", "ei\u00b7ne", "lan\u00b7ge", "uns", "al\u00b7len", "un\u00b7be\u00b7kann\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADV", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Der Mensch kommt gleichsam, wenn er kommt, aus einem", "tokens": ["Der", "Mensch", "kommt", "gleich\u00b7sam", ",", "wenn", "er", "kommt", ",", "aus", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "tiefen Nichts gestiegen,", "tokens": ["tie\u00b7fen", "Nichts", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Bleibt, im Vergleich mit jener Zeit, kaum eine Viertel-", "tokens": ["Bleibt", ",", "im", "Ver\u00b7gleich", "mit", "je\u00b7ner", "Zeit", ",", "kaum", "ei\u00b7ne", "Vier\u00b7tel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPRART", "NN", "APPR", "PDAT", "NN", "$,", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Stunde hier.", "tokens": ["Stun\u00b7de", "hier", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Die Viertel-Stunde wenden viele, die sich selbst Philoso-", "tokens": ["Die", "Vier\u00b7tel\u00b7Stun\u00b7de", "wen\u00b7den", "vie\u00b7le", ",", "die", "sich", "selbst", "Phi\u00b7lo\u00b7so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "PRELS", "PRF", "ADV", "TRUNC"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "phen nennen,", "tokens": ["phen", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVINF", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "Auf welche Weise sie der Gottheit verborgne Weg\u2019 ergr\u00fc-", "tokens": ["Auf", "wel\u00b7che", "Wei\u00b7se", "sie", "der", "Got\u00b7theit", "ver\u00b7borg\u00b7ne", "Weg'", "er\u00b7gr\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PPER", "ART", "NN", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "beln k\u00f6nnen,", "tokens": ["beln", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Mit angespannten Kr\u00e4ften an, gespornt von eitler Ehr-", "tokens": ["Mit", "an\u00b7ge\u00b7spann\u00b7ten", "Kr\u00e4f\u00b7ten", "an", ",", "ge\u00b7spornt", "von", "eit\u00b7ler", "Ehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Begier.", "tokens": ["Be\u00b7gier", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Dann tritt der Mensch zur Welt hinaus. H\u00e4tt\u2019 er, wofern", "tokens": ["Dann", "tritt", "der", "Mensch", "zur", "Welt", "hin\u00b7aus", ".", "H\u00e4tt'", "er", ",", "wo\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$.", "VVFIN", "PPER", "$,", "KOUS"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "er nachgedacht,", "tokens": ["er", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Wie herrlich GOtt, Der alles macht,", "tokens": ["Wie", "herr\u00b7lich", "Gott", ",", "Der", "al\u00b7les", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "In Seiner Creaturen Pracht;", "tokens": ["In", "Sei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Nicht weit vern\u00fcnftiger und besser sein Viertel-St\u00fcndchen", "tokens": ["Nicht", "weit", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "und", "bes\u00b7ser", "sein", "Vier\u00b7tel\u00b7St\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADJD", "KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "zugebracht?", "tokens": ["zu\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Zumahl, da etwas in der Zeit, so w\u00fcrklich ein Geheimni\u00df,", "tokens": ["Zu\u00b7mahl", ",", "da", "et\u00b7was", "in", "der", "Zeit", ",", "so", "w\u00fcrk\u00b7lich", "ein", "Ge\u00b7heim\u00b7ni\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "stecket,", "tokens": ["ste\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Und wovon folgende Erzehlung uns was verborgenes", "tokens": ["Und", "wo\u00b7von", "fol\u00b7gen\u00b7de", "Er\u00b7zeh\u00b7lung", "uns", "was", "ver\u00b7bor\u00b7ge\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN", "PPER", "PIS", "ADJA"], "meter": "-+-+---+-+--+--", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "entdecket.", "tokens": ["ent\u00b7de\u00b7cket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Durch deren Inhalt mir was grosses, obgleich dieselbe", "tokens": ["Durch", "de\u00b7ren", "In\u00b7halt", "mir", "was", "gros\u00b7ses", ",", "ob\u00b7gleich", "die\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PIS", "ADJA", "$,", "KOUS", "PDAT"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.26": {"text": "nur erdacht,", "tokens": ["nur", "er\u00b7dacht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Von einem sch\u00f6nen Ort der Schrift, der dunkel sonst, wird", "tokens": ["Von", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Ort", "der", "Schrift", ",", "der", "dun\u00b7kel", "sonst", ",", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "ADV", "$,", "VAFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.28": {"text": "beygebracht:", "tokens": ["bey\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Wie es doch k\u00f6nne m\u00f6glich seyn, ohn\u2019 alle dunkle Deu-", "tokens": ["Wie", "es", "doch", "k\u00f6n\u00b7ne", "m\u00f6g\u00b7lich", "seyn", ",", "ohn'", "al\u00b7le", "dunk\u00b7le", "Deu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$,", "KOUI", "PIAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.30": {"text": "teley,", "tokens": ["te\u00b7ley", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.31": {"text": "Da\u00df tausend Jahr f\u00fcr GOtt ein Tag, ein Tag wie tau-", "tokens": ["Da\u00df", "tau\u00b7send", "Jahr", "f\u00fcr", "Gott", "ein", "Tag", ",", "ein", "Tag", "wie", "tau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "APPR", "NN", "ART", "NN", "$,", "ART", "NN", "KOKOM", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "send Jahre sey.", "tokens": ["send", "Jah\u00b7re", "sey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}