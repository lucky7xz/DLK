{"textgrid.poem.38870": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es steigen alle Kr\u00e4fte aus dem Kerne,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steigen alle Kr\u00e4fte aus dem Kerne,", "tokens": ["Es", "stei\u00b7gen", "al\u00b7le", "Kr\u00e4f\u00b7te", "aus", "dem", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wurzeln in ihr stilles Herz zur\u00fccke,", "tokens": ["Und", "wur\u00b7zeln", "in", "ihr", "stil\u00b7les", "Herz", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So giebt Natur uns tausend Liebesblicke,", "tokens": ["So", "giebt", "Na\u00b7tur", "uns", "tau\u00b7send", "Lie\u00b7bes\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Damit der Mensch der Gottheit Liebe lerne.", "tokens": ["Da\u00b7mit", "der", "Mensch", "der", "Got\u00b7theit", "Lie\u00b7be", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich weihe mich dem gro\u00dfen Schauspiel gerne,", "tokens": ["Ich", "wei\u00b7he", "mich", "dem", "gro\u00b7\u00dfen", "Schau\u00b7spiel", "ger\u00b7ne", ","], "token_info": ["wor