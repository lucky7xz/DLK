{"dta.poem.12971": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das zw\u00f6lffte capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl uns, wenn creutz und angst uns in die schule f\u00fchren!", "tokens": ["Wohl", "uns", ",", "wenn", "creutz", "und", "angst", "uns", "in", "die", "schu\u00b7le", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn also lernet man fein in sich selber gehn.", "tokens": ["Denn", "al\u00b7so", "ler\u00b7net", "man", "fein", "in", "sich", "sel\u00b7ber", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADJD", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie \u00fcbel d\u00fcrfft es itzt um dem Manasse stehn;", "tokens": ["Wie", "\u00fc\u00b7bel", "d\u00fcrfft", "es", "itzt", "um", "dem", "Ma\u00b7nas\u00b7se", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00e4tt\u2019 ihn der H\u00f6chste nicht durch so viel jammer-th\u00fcren", "tokens": ["H\u00e4tt'", "ihn", "der", "H\u00f6chs\u00b7te", "nicht", "durch", "so", "viel", "jam\u00b7mer\u00b7th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zur bu\u00df, und durch die bu\u00df aus der verdammten nacht", "tokens": ["Zur", "bu\u00df", ",", "und", "durch", "die", "bu\u00df", "aus", "der", "ver\u00b7damm\u00b7ten", "nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der h\u00f6llen, an das licht der seeligkeit gebracht.", "tokens": ["Der", "h\u00f6l\u00b7len", ",", "an", "das", "licht", "der", "see\u00b7lig\u00b7keit", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jm gl\u00fccke wird der mensch sein elend selten inne:", "tokens": ["Jm", "gl\u00fc\u00b7cke", "wird", "der", "mensch", "sein", "e\u00b7lend", "sel\u00b7ten", "in\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "PPOSAT", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sieht der erden sch\u00e4tz als unverg\u00e4nglich an;", "tokens": ["Man", "sieht", "der", "er\u00b7den", "sch\u00e4tz", "als", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADJD", "KOKOM", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "D\u00e4mpfft aber creutz und pein den vorgefa\u00dften wahn,", "tokens": ["D\u00e4mpfft", "a\u00b7ber", "creutz", "und", "pein", "den", "vor\u00b7ge\u00b7fa\u00df\u00b7ten", "wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sieht man sonnen-klar: die welt sey eine spinne,", "tokens": ["So", "sieht", "man", "son\u00b7nen\u00b7klar", ":", "die", "welt", "sey", "ei\u00b7ne", "spin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$.", "ART", "NN", "VAFIN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die falsch gewebe wirckt, ihr himmelreich ein traum,", "tokens": ["Die", "falsch", "ge\u00b7we\u00b7be", "wirckt", ",", "ihr", "him\u00b7mel\u00b7reich", "ein", "traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "VVFIN", "$,", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unser hoffnungs-grund ein leichter meeres-schaum.", "tokens": ["Und", "un\u00b7ser", "hoff\u00b7nungs\u00b7grund", "ein", "leich\u00b7ter", "mee\u00b7res\u00b7schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist sehr gut f\u00fcr uns, wenn manchmal rohe leute", "tokens": ["Es", "ist", "sehr", "gut", "f\u00fcr", "uns", ",", "wenn", "manch\u00b7mal", "ro\u00b7he", "leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr falsches l\u00e4ster-gifft auf unsre tugend speyn.", "tokens": ["Ihr", "fal\u00b7sches", "l\u00e4s\u00b7ter\u00b7gifft", "auf", "uns\u00b7re", "tu\u00b7gend", "speyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ehrgeitz nistet sonst leicht in den hertzen ein.", "tokens": ["Der", "ehr\u00b7geitz", "nis\u00b7tet", "sonst", "leicht", "in", "den", "hert\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So aber tragen wir der demuth gold zur beute,", "tokens": ["So", "a\u00b7ber", "tra\u00b7gen", "wir", "der", "de\u00b7muth", "gold", "zur", "beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "APPRART", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Von der verl\u00e4umdung weg. So ist ein bittrer feind", "tokens": ["Von", "der", "ver\u00b7l\u00e4um\u00b7dung", "weg", ".", "So", "ist", "ein", "bit\u00b7trer", "feind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Uns manchmal n\u00fctzlicher, als ein geneigter freund.", "tokens": ["Uns", "manch\u00b7mal", "n\u00fctz\u00b7li\u00b7cher", ",", "als", "ein", "ge\u00b7neig\u00b7ter", "freund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die unverdiente schmach versetzet durch ihr st\u00fcrmen", "tokens": ["Die", "un\u00b7ver\u00b7dien\u00b7te", "schmach", "ver\u00b7set\u00b7zet", "durch", "ihr", "st\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des eyfers lauen trieb in eine volle glut:", "tokens": ["Des", "ey\u00b7fers", "lau\u00b7en", "trieb", "in", "ei\u00b7ne", "vol\u00b7le", "glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wenn uns alle welt gewalt und unrecht thut,", "tokens": ["Denn", "wenn", "uns", "al\u00b7le", "welt", "ge\u00b7walt", "und", "un\u00b7recht", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VVPP", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und niemand reden will, die unschuld zu beschirmen;", "tokens": ["Und", "nie\u00b7mand", "re\u00b7den", "will", ",", "die", "un\u00b7schuld", "zu", "be\u00b7schir\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So nimmt man GOtt allein, der alles sehen kan,", "tokens": ["So", "nimmt", "man", "Gott", "al\u00b7lein", ",", "der", "al\u00b7les", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "ADV", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unser hertze kennt, zu seinem zeugen an.", "tokens": ["Und", "un\u00b7ser", "hert\u00b7ze", "kennt", ",", "zu", "sei\u00b7nem", "zeu\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach! da\u00df wir uns nicht gantz in GOttes schutz begeben", "tokens": ["Ach", "!", "da\u00df", "wir", "uns", "nicht", "gantz", "in", "Got\u00b7tes", "schutz", "be\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in die einsamkeit des sichren felsens fliehn!", "tokens": ["Und", "in", "die", "ein\u00b7sam\u00b7keit", "des", "sich\u00b7ren", "fel\u00b7sens", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00fcrden wir uns nicht um menschen-trost bem\u00fchn,", "tokens": ["So", "w\u00fcr\u00b7den", "wir", "uns", "nicht", "um", "men\u00b7schen\u00b7trost", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mit den lippen noch an faulen pf\u00fctzen kleben.", "tokens": ["Und", "mit", "den", "lip\u00b7pen", "noch", "an", "fau\u00b7len", "pf\u00fct\u00b7zen", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil da der lebens-brunn aus offnen fieinen qvillt,", "tokens": ["Weil", "da", "der", "le\u00b7bens\u00b7brunn", "aus", "off\u00b7nen", "fi\u00b7ei\u00b7nen", "qvillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Und den empfundnen durst der d\u00fcrre seele stillt.", "tokens": ["Und", "den", "emp\u00b7fund\u00b7nen", "durst", "der", "d\u00fcr\u00b7re", "see\u00b7le", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Indessen l\u00e4st sich doch die gnade nicht erm\u00fcden,", "tokens": ["In\u00b7des\u00b7sen", "l\u00e4st", "sich", "doch", "die", "gna\u00b7de", "nicht", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis das gem\u00fcthe sich ihr v\u00f6llig \u00fcbergiebt;", "tokens": ["Bis", "das", "ge\u00b7m\u00fc\u00b7the", "sich", "ihr", "v\u00f6l\u00b7lig", "\u00fc\u00b7berg\u00b7iebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PRF", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wer aus schwachheit noch die creaturen liebt,", "tokens": ["Denn", "wer", "aus", "schwach\u00b7heit", "noch", "die", "crea\u00b7tu\u00b7ren", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dem nimmt sie unversehns den \u00e4userlichen frieden,", "tokens": ["Dem", "nimmt", "sie", "un\u00b7ver\u00b7sehns", "den", "\u00e4u\u00b7ser\u00b7li\u00b7chen", "frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was der arme mensch zu seinem trost erkiest;", "tokens": ["Und", "was", "der", "ar\u00b7me", "mensch", "zu", "sei\u00b7nem", "trost", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann sieht der arme mensch, da\u00df alles eitel ist.", "tokens": ["Dann", "sieht", "der", "ar\u00b7me", "mensch", ",", "da\u00df", "al\u00b7les", "ei\u00b7tel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Denn giebt er gerne zu, da\u00df ausser GOttes g\u00fcte", "tokens": ["Denn", "giebt", "er", "ger\u00b7ne", "zu", ",", "da\u00df", "aus\u00b7ser", "Got\u00b7tes", "g\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein brunn des friedens ist. Dann leidet er, und schreyt", "tokens": ["Kein", "brunn", "des", "frie\u00b7dens", "ist", ".", "Dann", "lei\u00b7det", "er", ",", "und", "schreyt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit brennnender begierd: O geist der ewigkeit!", "tokens": ["Mit", "brenn\u00b7nen\u00b7der", "be\u00b7gierd", ":", "O", "geist", "der", "e\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ist es aus mit mir? Wenn werd ich einst die h\u00fctte", "tokens": ["Wenn", "ist", "es", "aus", "mit", "mir", "?", "Wenn", "werd", "ich", "einst", "die", "h\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "APPR", "PPER", "$.", "KOUS", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des leibes, und der angst des eitlen lebens los?", "tokens": ["Des", "lei\u00b7bes", 