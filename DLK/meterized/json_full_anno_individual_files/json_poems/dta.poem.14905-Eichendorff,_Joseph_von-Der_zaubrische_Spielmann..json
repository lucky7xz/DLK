{"dta.poem.14905": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der zaubrische Spielmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, im Wind verfliegt mein Gr\u00fc\u00dfen!", "tokens": ["Ach", ",", "im", "Wind", "ver\u00b7fliegt", "mein", "Gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal, eh' der Tag erwacht,", "tokens": ["Ein\u00b7mal", ",", "eh'", "der", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' ich Deinen Mund nur k\u00fcssen,", "tokens": ["M\u00f6cht'", "ich", "Dei\u00b7nen", "Mund", "nur", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterbend so in s\u00fc\u00dfer Nacht!", "tokens": ["Ster\u00b7bend", "so", "in", "s\u00fc\u00b7\u00dfer", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}