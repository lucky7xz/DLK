{"textgrid.poem.67056": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die B\u00fccher der Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! Herr!", "tokens": ["Herr", "!", "Herr", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Unterwunden hab ich mich,", "tokens": ["Un\u00b7ter\u00b7wun\u00b7den", "hab", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu singen dir", "tokens": ["Zu", "sin\u00b7gen", "dir"], "token_info": ["word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bebenden Lobgesang.", "tokens": ["Be\u00b7ben\u00b7den", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Dort oben", "tokens": ["Dort", "o\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "In all der Himmel h\u00f6chstem Himmel,", "tokens": ["In", "all", "der", "Him\u00b7mel", "h\u00f6chs\u00b7tem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch \u00fcber dem Siriusstern,", "tokens": ["Hoch", "\u00fc\u00b7ber", "dem", "Si\u00b7rius\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hoch \u00fcber Uranus Scheitel,", "tokens": ["Hoch", "\u00fc\u00b7ber", "U\u00b7ra\u00b7nus", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo von Anbeginn", "tokens": ["Wo", "von", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wandelte der heilige Seraph", "tokens": ["Wan\u00b7del\u00b7te", "der", "hei\u00b7li\u00b7ge", "Se\u00b7raph"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mit feirender, erbebender Anbetung", "tokens": ["Mit", "fei\u00b7ren\u00b7der", ",", "er\u00b7be\u00b7ben\u00b7der", "An\u00b7be\u00b7tung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ums Heiligtum des Unnennbaren,", "tokens": ["Ums", "Hei\u00b7lig\u00b7tum", "des", "Un\u00b7nenn\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da steht im Heiligtum ein Buch", "tokens": ["Da", "steht", "im", "Hei\u00b7lig\u00b7tum", "ein", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und im Buche geschrieben", "tokens": ["Und", "im", "Bu\u00b7che", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "All die Millionenreihen", "tokens": ["All", "die", "Mil\u00b7li\u00b7o\u00b7nen\u00b7rei\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschentage \u2013", "tokens": ["Men\u00b7schen\u00b7ta\u00b7ge", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Da steht geschrieben \u2013", "tokens": ["Da", "steht", "ge\u00b7schrie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "L\u00e4nderverw\u00fcstung und V\u00f6lkerverheerung,", "tokens": ["L\u00e4n\u00b7der\u00b7ver\u00b7w\u00fcs\u00b7tung", "und", "V\u00f6l\u00b7ker\u00b7ver\u00b7hee\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Und feindliches Kriegergemetzel,", "tokens": ["Und", "feind\u00b7li\u00b7ches", "Krie\u00b7ger\u00b7ge\u00b7met\u00b7zel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und w\u00fcrgende K\u00f6nige \u2013", "tokens": ["Und", "w\u00fcr\u00b7gen\u00b7de", "K\u00f6\u00b7ni\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Ro\u00df und Wagen,", "tokens": ["Mit", "Ro\u00df", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und Reuter und Waffen,", "tokens": ["Und", "Reu\u00b7ter", "und", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und Szepter um sich her;", "tokens": ["Und", "Szep\u00b7ter", "um", "sich", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und giftge Tyrannen,", "tokens": ["Und", "gift\u00b7ge", "Ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit grimmigem Stachel,", "tokens": ["Mit", "grim\u00b7mi\u00b7gem", "Sta\u00b7chel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Tief in der Unschuld Herz.", "tokens": ["Tief", "in", "der", "Un\u00b7schuld", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und schr\u00f6ckliche Fluten", "tokens": ["Und", "schr\u00f6ck\u00b7li\u00b7che", "Flu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Verschlingend die Frommen,", "tokens": ["Ver\u00b7schlin\u00b7gend", "die", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Verschlingend die S\u00fcnder,", "tokens": ["Ver\u00b7schlin\u00b7gend", "die", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Zerrei\u00dfend die H\u00e4user", "tokens": ["Zer\u00b7rei\u00b7\u00dfend", "die", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Der Frommen, der S\u00fcnder.", "tokens": ["Der", "From\u00b7men", ",", "der", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Und fressende Feuer \u2013", "tokens": ["Und", "fres\u00b7sen\u00b7de", "Feu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Pal\u00e4ste und T\u00fcrme", "tokens": ["Pa\u00b7l\u00e4s\u00b7te", "und", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Mit ehernen Toren,", "tokens": ["Mit", "e\u00b7her\u00b7nen", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Gigantischen Mauern", "tokens": ["Gi\u00b7gan\u00b7ti\u00b7schen", "Mau\u00b7ern"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Zernichtend im Augenblick.", "tokens": ["Zer\u00b7nich\u00b7tend", "im", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Ge\u00f6ffnete Erden", "tokens": ["Ge\u00b7\u00f6ff\u00b7ne\u00b7te", "Er\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Mit schwefelndem Rachen", "tokens": ["Mit", "schwe\u00b7feln\u00b7dem", "Ra\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Ins rauchende Dunkel", "tokens": ["Ins", "rau\u00b7chen\u00b7de", "Dun\u00b7kel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Den Vater, die Kinder,", "tokens": ["Den", "Va\u00b7ter", ",", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Die Mutter, den S\u00e4ugling,", "tokens": ["Die", "Mut\u00b7ter", ",", "den", "S\u00e4ug\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "In Weheger\u00f6chel", "tokens": ["In", "We\u00b7he\u00b7ge\u00b7r\u00f6\u00b7chel"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Und Sterbegewinsel", "tokens": ["Und", "Ster\u00b7be\u00b7ge\u00b7win\u00b7sel"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Hinuntergurgelnd. \u2013", "tokens": ["Hin\u00b7un\u00b7ter\u00b7gur\u00b7gelnd", ".", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["VVPP", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Da steht geschrieben", "tokens": ["Da", "steht", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Vatermord! Brudermord!", "tokens": ["Va\u00b7ter\u00b7mord", "!", "Bru\u00b7der\u00b7mord", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "S\u00e4uglinge blaugew\u00fcrgt!", "tokens": ["S\u00e4ug\u00b7lin\u00b7ge", "blau\u00b7ge\u00b7w\u00fcrgt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Greulich! Greulich!", "tokens": ["Greu\u00b7lich", "!", "Greu\u00b7lich", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Um ein Linsengericht", "tokens": ["Um", "ein", "Lin\u00b7sen\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "D\u00e4rmzerfressendes Gift", "tokens": ["D\u00e4rm\u00b7zer\u00b7fres\u00b7sen\u00b7des", "Gift"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Dem guten, sicheren Freund gemischt. \u2013", "tokens": ["Dem", "gu\u00b7ten", ",", "si\u00b7che\u00b7ren", "Freund", "ge\u00b7mischt", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hohlaugigte Kr\u00fcppel,", "tokens": ["Hohl\u00b7au\u00b7gig\u00b7te", "Kr\u00fcp\u00b7pel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ihrer Onansschande", "tokens": ["Ih\u00b7rer", "O\u00b7nans\u00b7schan\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Teuflische Opfer \u2013.", "tokens": ["Teuf\u00b7li\u00b7sche", "Op\u00b7fer", "\u201