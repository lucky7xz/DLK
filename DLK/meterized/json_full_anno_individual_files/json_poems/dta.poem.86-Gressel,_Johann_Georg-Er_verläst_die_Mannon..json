{"dta.poem.86": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verl\u00e4st die  Mannon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lache eurer Strengigkeiten/ mir gef\u00e4llt eur stellen nicht/", "tokens": ["Ich", "la\u00b7che", "eu\u00b7rer", "Stren\u00b7gig\u00b7kei\u00b7ten", "/", "mir", "ge\u00b7f\u00e4llt", "eur", "stel\u00b7len", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Diese Worte recht im Ernste/ ", "tokens": ["Die\u00b7se", "Wor\u00b7te", "recht", "im", "Erns\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen euch aus Lieb\u2019 und Gunst meine Geister nicht mehr", "tokens": ["Ge\u00b7gen", "euch", "aus", "Lieb'", "und", "Gunst", "mei\u00b7ne", "Geis\u00b7ter", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "PTKNEG", "ADV"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "brennen/", "tokens": ["bren\u00b7nen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Weil sie mehr als allzuwol euren b\u00f6sen Mi\u00dfbrauch kennen.", "tokens": ["Weil", "sie", "mehr", "als", "all\u00b7zu\u00b7wol", "eu\u00b7ren", "b\u00f6\u00b7sen", "Mi\u00df\u00b7brauch", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+----+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Liebet welchen/ den ihr wollet/ meinentwegen ists verg\u00fcnt/", "tokens": ["Lie\u00b7bet", "wel\u00b7chen", "/", "den", "ihr", "wol\u00b7let", "/", "mei\u00b7nent\u00b7we\u00b7gen", "ists", "ver\u00b7g\u00fcnt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWAT", "$(", "ART", "PPER", "VMFIN", "$(", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Ich verlange von euch nichtes/ der ich lang gewesen blind.", "tokens": ["Ich", "ver\u00b7lan\u00b7ge", "von", "euch", "nich\u00b7tes", "/", "der", "ich", "lang", "ge\u00b7we\u00b7sen", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIS", "$(", "PRELS", "PPER", "ADJD", "VAPP", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Jhr seyd meine Herrscherin/ ", "tokens": ["Ihr", "seyd", "mei\u00b7ne", "Herr\u00b7sche\u00b7rin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Endlich mu\u00df der krancke Geist von dem Fieber doch genesen.", "tokens": ["End\u00b7lich", "mu\u00df", "der", "kran\u00b7cke", "Geist", "von", "dem", "Fie\u00b7ber", "doch", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+--+--", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Adjeu Mannon, Gl\u00fcck zur Reise/ meine vormahls veste Treu", "tokens": ["Ad\u00b7jeu", "Man\u00b7non", ",", "Gl\u00fcck", "zur", "Rei\u00b7se", "/", "mei\u00b7ne", "vor\u00b7mahls", "ves\u00b7te", "Treu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NN", "APPRART", "NN", "$(", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Ist verblichen und verloschen/ und die Schw\u00fcre sind vorbey/", "tokens": ["Ist", "ver\u00b7bli\u00b7chen", "und", "ver\u00b7lo\u00b7schen", "/", "und", "die", "Schw\u00fc\u00b7re", "sind", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVINF", "$(", "KON", "ART", "NN", "VAFIN", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Mein Hertz/ da\u00df sich endlich hat euren Ketten noch entrissen/", "tokens": ["Mein", "Hertz", "/", "da\u00df", "sich", "end\u00b7lich", "hat", "eu\u00b7ren", "Ket\u00b7ten", "noch", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PRF", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Soll forthin in stiller Ruh seine Zeit vor sich beschliessen.", "tokens": ["Soll", "for\u00b7thin", "in", "stil\u00b7ler", "Ruh", "sei\u00b7ne", "Zeit", "vor", "sich", "be\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}