{"textgrid.poem.67287": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbin Babels Aue stand an dem Stromgestad'", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbin Babels Aue stand an dem Stromgestad'", "tokens": ["\u00bb", "in", "Ba\u00b7bels", "Au\u00b7e", "stand", "an", "dem", "Strom\u00b7ge\u00b7stad'"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NE", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Baum des Schattens; dick aus der Wurzel scho\u00df", "tokens": ["Ein", "Baum", "des", "Schat\u00b7tens", ";", "dick", "aus", "der", "Wur\u00b7zel", "scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Riesenstamm, die sch\u00f6nen Aeste", "tokens": ["Sein", "Rie\u00b7sen\u00b7stamm", ",", "die", "sch\u00f6\u00b7nen", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Huben sich stolz in des Himmels Wolken\u00ab \u2013", "tokens": ["Hu\u00b7ben", "sich", "stolz", "in", "des", "Him\u00b7mels", "Wol\u00b7ken", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "NN", "$(", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Im Unflathspfuhl der j\u00fcngeren Babylon,", "tokens": ["Im", "Un\u00b7flath\u00b7spfuhl", "der", "j\u00fcn\u00b7ge\u00b7ren", "Ba\u00b7by\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort wo die Gr\u00e4uel ihrer Erw\u00fcrgungen", "tokens": ["Dort", "wo", "die", "Gr\u00e4u\u00b7el", "ih\u00b7rer", "Er\u00b7w\u00fcr\u00b7gun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der H\u00f6h' entsp\u00fchlten, wo der Sumpf die", "tokens": ["Der", "H\u00f6h'", "ent\u00b7sp\u00fchl\u00b7ten", ",", "wo", "der", "Sumpf", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00fcfte verpestend und sch\u00e4umend aufgohr \u2013", "tokens": ["L\u00fcf\u00b7te", "ver\u00b7pes\u00b7tend", "und", "sch\u00e4u\u00b7mend", "auf\u00b7gohr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "VVFIN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Einst im Triumphe zog, mit vergoldetem", "tokens": ["Einst", "im", "Tri\u00b7um\u00b7phe", "zog", ",", "mit", "ver\u00b7gol\u00b7de\u00b7tem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,", "APPR", "ADJA"], "meter": "+--+-++-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Geh\u00f6rn und Kr\u00e4nzen prangend, ein Stiergespann", "tokens": ["Ge\u00b7h\u00f6rn", "und", "Kr\u00e4n\u00b7zen", "pran\u00b7gend", ",", "ein", "Stier\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die G\u00f6ttinn Freiheit, ihre Schwester-", "tokens": ["Die", "G\u00f6t\u00b7tinn", "Frei\u00b7heit", ",", "ih\u00b7re", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Metze Vernunft auf dem Hochaltar stand;", "tokens": ["Met\u00b7ze", "Ver\u00b7nunft", "auf", "dem", "Hoc\u00b7hal\u00b7tar", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Schlachtbank witternd, br\u00fclleten, str\u00e4ubten sich,", "tokens": ["Die", "Schlacht\u00b7bank", "wit\u00b7ternd", ",", "br\u00fcl\u00b7le\u00b7ten", ",", "str\u00e4ub\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo str\u00f6mend flo\u00df das Blut der Enthaupteten,", "tokens": ["Wo", "str\u00f6\u00b7mend", "flo\u00df", "das", "Blut", "der", "Ent\u00b7haup\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Stiere, eingewurzelt standen", "tokens": ["Die", "Stie\u00b7re", ",", "ein\u00b7ge\u00b7wur\u00b7zelt", "stan\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie, und es kroch in ihr Joch der P\u00f6bel \u2013", "tokens": ["Sie", ",", "und", "es", "kroch", "in", "ihr", "Joch", "der", "P\u00f6\u00b7bel", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dort schwoll empor, des Mords und der F\u00e4ulni\u00df Sohn,", "tokens": ["Dort", "schwoll", "em\u00b7por", ",", "des", "Mords", "und", "der", "F\u00e4ul\u00b7ni\u00df", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Riesenaufwuchs unter dem Pilzgeschlecht,", "tokens": ["Ein", "Rie\u00b7sen\u00b7auf\u00b7wuchs", "un\u00b7ter", "dem", "Pilz\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er prunkend, strotzend, hoch und breit sich", "tokens": ["Er", "prun\u00b7kend", ",", "strot\u00b7zend", ",", "hoch", "und", "breit", "sich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dehnend und br\u00fcstend in eitler Hoffart.", "tokens": ["Deh\u00b7nend", "und", "br\u00fcs\u00b7tend", "in", "eit\u00b7ler", "Hof\u00b7fart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Das Schlammgew\u00fcrm umkroch, es umflattert' ihn", "tokens": ["Das", "Schlamm\u00b7ge\u00b7w\u00fcrm", "um\u00b7kroch", ",", "es", "um\u00b7flat\u00b7tert'", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Fliegenk\u00f6nigs stachelbewaffnete", "tokens": ["Des", "Flie\u00b7gen\u00b7k\u00f6\u00b7nigs", "sta\u00b7chel\u00b7be\u00b7waff\u00b7ne\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ha! Ehrenlegion! Anbetend", "tokens": ["Ha", "!", "Eh\u00b7ren\u00b7le\u00b7gi\u00b7on", "!", "An\u00b7be\u00b7tend"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ITJ", "$.", "NN", "$.", "NN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Summte von fernher das Ungeziefer.", "tokens": ["Summ\u00b7te", "von", "fern\u00b7her", "das", "Un\u00b7ge\u00b7zie\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Der Aufgebl\u00e4hte dunstete Moderhauch", "tokens": ["Der", "Auf\u00b7ge\u00b7bl\u00e4h\u00b7te", "duns\u00b7te\u00b7te", "Mo\u00b7der\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umher; doch Dank der Wolke des Ekelqualms,", "tokens": ["Um\u00b7her", ";", "doch", "Dank", "der", "Wol\u00b7ke", "des", "E\u00b7kel\u00b7qualms", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie winkte Warnung, da\u00df ein keusches", "tokens": ["Sie", "wink\u00b7te", "War\u00b7nung", ",", "da\u00df", "ein", "keu\u00b7sches"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auge sich wende vom Scheusal abw\u00e4rts.", "tokens": ["Au\u00b7ge", "sich", "wen\u00b7de", "vom", "Scheu\u00b7sal", "ab\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Da scholl der Rache Stunde! Zertreten lag", "tokens": ["Da", "scholl", "der", "Ra\u00b7che", "Stun\u00b7de", "!", "Zer\u00b7tre\u00b7ten", "lag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu Staub und Koth gemalmet der Wunderpilz,", "tokens": ["Zu", "Staub", "und", "Koth", "ge\u00b7mal\u00b7met", "der", "Wun\u00b7der\u00b7pilz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und seiner Sippschaft Pfifferlinge", "tokens": ["Und", "sei\u00b7ner", "Sipp\u00b7schaft", "Pfif\u00b7fer\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Waren verstiebt mit dem Schwarm der Schranzen.", "tokens": ["Wa\u00b7ren", "ver\u00b7stiebt", "mit", "dem", "Schwarm", "der", "Schran\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "So Er, de\u00df Bild der Sprosse des Pfuhles war,", "tokens": ["So", "Er", ",", "de\u00df", "Bild", "der", "Spros\u00b7se", "des", "Pfuh\u00b7les", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "NN", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gest\u00fcrzet liegt Er! Jubel! mit Wurzel und", "tokens": ["Ge\u00b7st\u00fcr\u00b7zet", "liegt", "Er", "!", "Ju\u00b7bel", "!", "mit", "Wur\u00b7zel", "und"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "NN", "$.", "APPR", "NN", "KON"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Stamm! \u2013 Was Stamm und Wurzel? Er ein", "tokens": ["Mit", "Stamm", "!", "\u2013", "Was", "Stamm", "und", "Wur\u00b7zel", "?", "Er", "ein"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "$(", "PWS", "NN", "KON", "NN", "$.", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "D\u00e4mmerungs-F\u00fcndling aus fernem Eiland!", "tokens": ["D\u00e4m\u00b7me\u00b7rungs\u00b7F\u00fcnd\u00b7ling", "aus", "fer\u00b7nem", "Ei\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Ihm schlug im Sturze nicht des Gewissens Puls,", "tokens": ["Ihm", "schlug", "im", "Stur\u00b7ze", "nicht", "des", "Ge\u00b7wis\u00b7sens", "Puls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er schnob noch Mord und Frevel und Flammenwuth;", "tokens": ["Er", "schnob", "noch", "Mord", "und", "Fre\u00b7vel", "und", "Flam\u00b7men\u00b7wuth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch bald entsank die Heldenlarv', es", "tokens": ["Doch", "bald", "ent\u00b7sank", "die", "Hel\u00b7den\u00b7la\u00b7rv'", ",", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schrumpfte der Prahler in eigne Kleinheit.", "tokens": ["Schrumpf\u00b7te", "der", "Prah\u00b7ler", "in", "eig\u00b7ne", "Klein\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Er schwind' und schwinde, winzig und winziger!", "tokens": ["Er", "schwind'", "und", "schwin\u00b7de", ",", "win\u00b7zig", "und", "win\u00b7zi\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Einst ein Kolossus seiner Verblendeten. \u2013", "tokens": ["Einst", "ein", "Ko\u00b7los\u00b7sus", "sei\u00b7ner", "Ver\u00b7blen\u00b7de\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kommt nun, beschaut, entt\u00e4uscht, ihn nah' und", "tokens": ["Kommt", "nun", ",", "be\u00b7schaut", ",", "ent\u00b7t\u00e4uscht", ",", "ihn", "nah'", "und"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "$,", "VVPP", "$,", "PPER", "PTKVZ", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "N\u00e4her, nicht ohne Vergr\u00f6\u00dfrungsbrille!", "tokens": ["N\u00e4\u00b7her", ",", "nicht", "oh\u00b7ne", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7bril\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "\u00bbin Babels Aue stand an dem Stromgestad'", "tokens": ["\u00bb", "in", "Ba\u00b7bels", "Au\u00b7e", "stand", "an", "dem", "Strom\u00b7ge\u00b7stad'"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NE", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Baum des Schattens; dick aus der Wurzel scho\u00df", "tokens": ["Ein", "Baum", "des", "Schat\u00b7tens", ";", "dick", "aus", "der", "Wur\u00b7zel", "scho\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Riesenstamm, die sch\u00f6nen Aeste", "tokens": ["Sein", "Rie\u00b7sen\u00b7stamm", ",", "die", "sch\u00f6\u00b7nen", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Huben sich stolz in des Himmels Wolken\u00ab \u2013", "tokens": ["Hu\u00b7ben", "sich", "stolz", "in"