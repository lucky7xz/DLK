{"dta.poem.20328": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht sch\u00e4me dich/ du saubere Melinde/", "tokens": ["Nicht", "sch\u00e4\u00b7me", "dich", "/", "du", "sau\u00b7be\u00b7re", "Me\u00b7lin\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df deine zarte reinligkeit", "tokens": ["Da\u00df", "dei\u00b7ne", "zar\u00b7te", "rein\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der feuchte mond verweist in eine binde/", "tokens": ["Der", "feuch\u00b7te", "mond", "ver\u00b7weist", "in", "ei\u00b7ne", "bin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dir den bunten einflu\u00df dr\u00e4ut.", "tokens": ["Und", "dir", "den", "bun\u00b7ten", "ein\u00b7flu\u00df", "dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der grosse belt hegt ebb\u2019 und flut/", "tokens": ["Der", "gros\u00b7se", "belt", "hegt", "ebb'", "und", "flut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was wunder/ wenns der mensch der kleine thut.", "tokens": ["Was", "wun\u00b7der", "/", "wenns", "der", "mensch", "der", "klei\u00b7ne", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "KOUS", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die r\u00f6thligkeit bey deinen bunten sachen", "tokens": ["Die", "r\u00f6th\u00b7lig\u00b7keit", "bey", "dei\u00b7nen", "bun\u00b7ten", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat niemahls deinen schoo\u00df versehrt.", "tokens": ["Hat", "nie\u00b7mahls", "dei\u00b7nen", "schoo\u00df", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie muscheln sich durch purpur theuer machen/", "tokens": ["Wie", "mu\u00b7scheln", "sich", "durch", "pur\u00b7pur", "theu\u00b7er", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PRF", "APPR", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So macht dein schnecken-blut dich werth.", "tokens": ["So", "macht", "dein", "schne\u00b7cken\u00b7blut", "dich", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer liebt dein dinten-meer wohl nicht/", "tokens": ["Wer", "liebt", "dein", "din\u00b7ten\u00b7meer", "wohl", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil man daraus corallen zincken bricht.", "tokens": ["Weil", "man", "da\u00b7raus", "co\u00b7ral\u00b7len", "zin\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur einmahl bringt das gantze jahr uns nelcken/", "tokens": ["Nur", "ein\u00b7mahl", "bringt", "das", "gant\u00b7ze", "jahr", "uns", "nel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein blumen-busch bringts monatlich/", "tokens": ["Dein", "blu\u00b7men\u00b7busch", "bringts", "mo\u00b7nat\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein rosen-strauch mag nicht verwelcken/", "tokens": ["Dein", "ro\u00b7sen\u00b7strauch", "mag", "nicht", "ver\u00b7wel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein dorn der h\u00e4lt bey dir nicht stich/", "tokens": ["Sein", "dorn", "der", "h\u00e4lt", "bey", "dir", "nicht", "stich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "APPR", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn was die sanfften bl\u00e4tter macht/", "tokens": ["Denn", "was", "die", "sanff\u00b7ten", "bl\u00e4t\u00b7ter", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist ein thau von der johannis-nacht.", "tokens": ["Das", "ist", "ein", "thau", "von", "der", "jo\u00b7han\u00b7nis\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kanst du gleich nicht die hurtgen lenden r\u00fchren/", "tokens": ["Kanst", "du", "gleich", "nicht", "die", "hurt\u00b7gen", "len\u00b7den", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Lobt man dich doch im stille stehn/", "tokens": ["Lobt", "man", "dich", "doch", "im", "stil\u00b7le", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der augenblau wird leichtlich sich verlieren/", "tokens": ["Der", "au\u00b7gen\u00b7blau", "wird", "leicht\u00b7lich", "sich", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn wirst du seyn noch eins so sch\u00f6n.", "tokens": ["Denn", "wirst", "du", "seyn", "noch", "eins", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VAINF", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man sammlet/ spricht die gantze welt/", "tokens": ["Man", "samm\u00b7let", "/", "spricht", "die", "gant\u00b7ze", "welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel besser frucht/ wenn starcke bl\u00fcte f\u00e4llt.", "tokens": ["Viel", "bes\u00b7ser", "frucht", "/", "wenn", "star\u00b7cke", "bl\u00fc\u00b7te", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$(", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "La\u00df mich darum doch keine fasten halten/", "tokens": ["La\u00df", "mich", "da\u00b7rum", "doch", "kei\u00b7ne", "fas\u00b7ten", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein k\u00f6nig nimmt den schranck zwar ein/", "tokens": ["Ein", "k\u00f6\u00b7nig", "nimmt", "den", "schranck", "zwar", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "VVFIN", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mu\u00df er fort/ wenn sich die wasser spalten/", "tokens": ["Doch", "mu\u00df", "er", "fort", "/", "wenn", "sich", "die", "was\u00b7ser", "spal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKVZ", "$(", "KOUS", "PRF", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der geist mu\u00df ausgestossen seyn.", "tokens": ["Der", "geist", "mu\u00df", "aus\u00b7ge\u00b7stos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man geht/ wie iedermann bekandt/", "tokens": ["Man", "geht", "/", "wie", "ie\u00b7der\u00b7mann", "be\u00b7kandt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOKOM", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchs rothe meer in das gelobte land.", "tokens": ["Durchs", "ro\u00b7the", "meer", "in", "das", "ge\u00b7lob\u00b7te", "land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}