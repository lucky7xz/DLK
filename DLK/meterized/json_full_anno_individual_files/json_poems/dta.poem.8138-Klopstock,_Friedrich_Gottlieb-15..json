{"dta.poem.8138": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gewand wei\u00df, bluthell hob zum Thron", "tokens": ["Das", "Ge\u00b7wand", "wei\u00df", ",", "blut\u00b7hell", "hob", "zum", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Sie sich empor, stand ernst, anschaunselig da,", "tokens": ["Sie", "sich", "em\u00b7por", ",", "stand", "ernst", ",", "an\u00b7schaun\u00b7se\u00b7lig", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PRF", "PTKVZ", "$,", "VVFIN", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schimmerte die Braut! Sanften Ton, festliche Melodien,", "tokens": ["Schim\u00b7mer\u00b7te", "die", "Braut", "!", "Sanf\u00b7ten", "Ton", ",", "fest\u00b7li\u00b7che", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+---++-++--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Freudigeres Gef\u00fchl, str\u00f6mtet ihr, Donnerer in dem Gericht!", "tokens": ["Freu\u00b7di\u00b7ge\u00b7res", "Ge\u00b7f\u00fchl", ",", "str\u00f6m\u00b7tet", "ihr", ",", "Don\u00b7ne\u00b7rer", "in", "dem", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+---+-+-+-+", "measure": "iambic.septa.invert"}}}}}