{"textgrid.poem.38023": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1. Epistel", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mein Herz in deines hinein geschlossen,", "tokens": ["Ich", "ha\u00b7be", "mein", "Herz", "in", "dei\u00b7nes", "hin\u00b7ein", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Darin liegen begraben", "tokens": ["Da\u00b7rin", "lie\u00b7gen", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Drei g\u00fcldene Buchstaben,", "tokens": ["Drei", "g\u00fcl\u00b7de\u00b7ne", "Buch\u00b7sta\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der erste ist von rothem Gold,", "tokens": ["Der", "ers\u00b7te", "ist", "von", "ro\u00b7them", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich dir bin von Herzen hold;", "tokens": ["Da\u00df", "ich", "dir", "bin", "von", "Her\u00b7zen", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ander ist von Edelstein,", "tokens": ["Der", "an\u00b7der", "ist", "von", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich wollt du w\u00e4rst die Liebste mein,", "tokens": ["Ich", "wollt", "du", "w\u00e4rst", "die", "Liebs\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der dritt, der ist von Sammet und Seiden,", "tokens": ["Der", "dritt", ",", "der", "ist", "von", "Sam\u00b7met", "und", "Sei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "VAFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Du sollst all andere meiden;", "tokens": ["Du", "sollst", "all", "an\u00b7de\u00b7re", "mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "So w\u00fcnsch, ich dir ein g\u00fcldenes Schlafk\u00e4mmerlein.", "tokens": ["So", "w\u00fcnsch", ",", "ich", "dir", "ein", "g\u00fcl\u00b7de\u00b7nes", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Von Kristall ein Fensterlein,", "tokens": ["Von", "Kris\u00b7tall", "ein", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Von Sammet ein Bett,", "tokens": ["Von", "Sam\u00b7met", "ein", "Bett", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Von Zimmet eine Th\u00fcr,", "tokens": ["Von", "Zim\u00b7met", "ei\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Von N\u00e4gelein ein Riegel daf\u00fcr,", "tokens": ["Von", "N\u00e4\u00b7ge\u00b7lein", "ein", "Rie\u00b7gel", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Von Muskaten eine Schwell", "tokens": ["Von", "Mus\u00b7ka\u00b7ten", "ei\u00b7ne", "Schwell"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Und mich zu deinem Schlafgesell.", "tokens": ["Und", "mich", "zu", "dei\u00b7nem", "Schlaf\u00b7ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieses w\u00fcnsch ich der H\u00fcbschen und Feinen,", "tokens": ["Die\u00b7ses", "w\u00fcnsch", "ich", "der", "H\u00fcb\u00b7schen", "und", "Fei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der Zarten und Reinen,", "tokens": ["Der", "Zar\u00b7ten", "und", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Tugendreichen,", "tokens": ["Der", "Tu\u00b7gen\u00b7drei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So nicht ihres gleichen,", "tokens": ["So", "nicht", "ih\u00b7res", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wir wollen Freund sein", "tokens": ["Wir", "wol\u00b7len", "Freund", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bis in das Grab hinein.", "tokens": ["Bis", "in", "das", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hiermit bist du tausendmal gek\u00fc\u00dft auf deine Hand,", "tokens": ["Hier\u00b7mit", "bist", "du", "tau\u00b7send\u00b7mal", "ge\u00b7k\u00fc\u00dft", "auf", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Das geb ich dir zum Unterpfand,", "tokens": ["Das", "geb", "ich", "dir", "zum", "Un\u00b7ter\u00b7pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich schick dir ein Gru\u00df von Sammet und von Gold,", "tokens": ["Ich", "schick", "dir", "ein", "Gru\u00df", "von", "Sam\u00b7met", "und", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Du bist mir lieb und ich dir hold,", "tokens": ["Du", "bist", "mir", "lieb", "und", "ich", "dir", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So werd ich hernach dir Freund doch bleiben,", "tokens": ["So", "werd", "ich", "her\u00b7nach", "dir", "Freund", "doch", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "So lange die Rosse den Wagen thun treiben,", "tokens": ["So", "lan\u00b7ge", "die", "Ros\u00b7se", "den", "Wa\u00b7gen", "thun", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "So lange der Main schwimmet durch den Rhein,", "tokens": ["So", "lan\u00b7ge", "der", "Main", "schwim\u00b7met", "durch", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So lange werd ich der Freund doch sein;", "tokens": ["So", "lan\u00b7ge", "werd", "ich", "der", "Freund", "doch", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Geschrieben im Jahr,", "tokens": ["Ge\u00b7schrie\u00b7ben", "im", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Da die Liebe Feuer war,", "tokens": ["Da", "die", "Lie\u00b7be", "Feu\u00b7er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "Ob schon die Augen gleich weit von einander", "tokens": ["Ob", "schon", "die", "Au\u00b7gen", "gleich", "weit", "von", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "APPR", "PRF"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Ein Herz doch allzeit liebet das andre,", "tokens": ["Ein", "Herz", "doch", "all\u00b7zeit", "lie\u00b7bet", "das", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Den Namen will ich nicht nennen,", "tokens": ["Den", "Na\u00b7men", "will", "ich", "nicht", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.20": {"text": "Wenn du mich liebst, wirst du mich wohl kennen.", "tokens": ["Wenn", "du", "mich", "liebst", ",", "wirst", "du", "mich", "wohl", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVINF