hr Mittler", "tokens": ["Ihr", "Ver\u00b7s\u00f6h\u00b7ner", "und", "ihr", "Mitt\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das Kind in ihrem Schoo\u00df.", "tokens": ["Ist", "das", "Kind", "in", "ih\u00b7rem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wird es doch gekreuzigt werden", "tokens": ["Wird", "es", "doch", "ge\u00b7kreu\u00b7zigt", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Wiege bis an's Grab,", "tokens": ["Von", "der", "Wie\u00b7ge", "bis", "an's", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so zahlt es \u00fcberreichlich", "tokens": ["Und", "so", "zahlt", "es", "\u00fc\u00b7berr\u00b7eich\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle ihre Schulden ab.", "tokens": ["Al\u00b7le", "ih\u00b7re", "Schul\u00b7den", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}