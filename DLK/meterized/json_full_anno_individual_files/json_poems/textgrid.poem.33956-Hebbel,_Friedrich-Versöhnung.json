], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Vor der Nachbarinnen Augen", "tokens": ["Vor", "der", "Nach\u00b7ba\u00b7rin\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bebt das ihre scheu zur\u00fcck,", "tokens": ["Bebt", "das", "ih\u00b7re", "scheu", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPOSAT", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber frei hinauf zum Himmel", "tokens": ["A\u00b7ber", "frei", "hin\u00b7auf", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wendet sie den reinen Blick.", "tokens": ["Wen\u00b7det", "sie", "den", "rei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In ein Haus der Anverwandten", "tokens": ["In", "ein", "Haus", "der", "An\u00b7ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt sie nur mit Angst und Pein,", "tokens": ["Tritt", "sie", "nur", "mit", "Angst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber in des Ew'gen Tempel", "tokens": ["A\u00b7ber", "in", "des", "Ew'\u00b7gen", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KO