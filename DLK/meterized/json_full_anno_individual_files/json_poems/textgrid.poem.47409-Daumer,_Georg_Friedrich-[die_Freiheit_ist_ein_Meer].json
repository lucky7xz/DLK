{"textgrid.poem.47409": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Freiheit ist ein Meer]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit ist ein Meer", "tokens": ["Die", "Frei\u00b7heit", "ist", "ein", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und seine Fische Herzen;", "tokens": ["Und", "sei\u00b7ne", "Fi\u00b7sche", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schwimmen ohne Schmerzen", "tokens": ["Sie", "schwim\u00b7men", "oh\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Behaglich hin und her.", "tokens": ["Be\u00b7hag\u00b7lich", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch diese Lust, wie Schade!", "tokens": ["Doch", "die\u00b7se", "Lust", ",", "wie", "Scha\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist von geringer Dauer;", "tokens": ["Ist", "von", "ge\u00b7rin\u00b7ger", "Dau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es wohnet am Gestade,", "tokens": ["Es", "woh\u00b7net", "am", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es stehet auf der Lauer", "tokens": ["Es", "ste\u00b7het", "auf", "der", "Lau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Liebe, die Fischerin.", "tokens": ["Lie\u00b7be", ",", "die", "Fi\u00b7sche\u00b7rin", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Sie fischt mit eignen Angeln;", "tokens": ["Sie", "fischt", "mit", "eig\u00b7nen", "An\u00b7geln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sie fischt mit Ambralocken;", "tokens": ["Sie", "fischt", "mit", "A\u00b7mbra\u00b7lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die purpurrothen Fischchen,", "tokens": ["Die", "pur\u00b7pur\u00b7ro\u00b7then", "Fischchen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Sie kommen unerschrocken,", "tokens": ["Sie", "kom\u00b7men", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sie lassen von der argen", "tokens": ["Sie", "las\u00b7sen", "von", "der", "ar\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "