{"textgrid.poem.59429": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die heilige Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Meer, von Schiffen irr durchflogen,", "tokens": ["Es", "ist", "ein", "Meer", ",", "von", "Schif\u00b7fen", "irr", "durch\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die steuern rastlos nach den falschen Landen,", "tokens": ["Die", "steu\u00b7ern", "rast\u00b7los", "nach", "den", "fal\u00b7schen", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die alle suchen und wo alle stranden", "tokens": ["Die", "al\u00b7le", "su\u00b7chen", "und", "wo", "al\u00b7le", "stran\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVINF", "KON", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf schwanker Flut, die jeden noch betrogen.", "tokens": ["Auf", "schwan\u00b7ker", "Flut", ",", "die", "je\u00b7den", "noch", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist im w\u00fcsten Meer ein Felsenbogen,", "tokens": ["Es", "ist", "im", "w\u00fcs\u00b7ten", "Meer", "ein", "Fel\u00b7sen\u00b7bo\u00b7gen", 