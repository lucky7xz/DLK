"}}, "stanza.15": {"line.1": {"text": "M\u00e4gdlein, woll' zur Erde sehen,", "tokens": ["M\u00e4gd\u00b7lein", ",", "woll'", "zur", "Er\u00b7de", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort la\u00df deine Augen weiden,", "tokens": ["Dort", "la\u00df", "dei\u00b7ne", "Au\u00b7gen", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis sie sch\u00fcchtern auferstehen,", "tokens": ["Bis", "sie", "sch\u00fcch\u00b7tern", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dich wie zwei Sterne kleiden.", "tokens": ["Und", "dich", "wie", "zwei", "Ster\u00b7ne", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOKOM", "CARD", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "M\u00e4gdlein, diese Augensterne", "tokens": ["M\u00e4gd\u00b7lein", ",", "die\u00b7se", "Au\u00b7gens\u00b7ter\u00b7ne"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Magst du dann dem Himmel weihen,", "tokens": ["Magst", "du", "dann", "dem", "Him\u00b7mel", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Erde lieben lerne,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "lie\u00b7ben", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00dft du ihr die Augen leihen.", "tokens": ["Mu\u00dft", "du", "ihr", "die", "Au\u00b7gen", "lei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}