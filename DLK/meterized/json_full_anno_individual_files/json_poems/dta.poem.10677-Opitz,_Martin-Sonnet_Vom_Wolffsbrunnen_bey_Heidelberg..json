{"dta.poem.10677": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n Vom Wolffsbrunnen bey Heidelberg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv edele Fonteyn mit Ruh vnd Lust vmbgeben/", "tokens": ["Dv", "e\u00b7de\u00b7le", "Fon\u00b7teyn", "mit", "Ruh", "vnd", "Lust", "vmb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Mit Bergen hier vnd dar/ als einer Burg/ vmbringt/", "tokens": ["Mit", "Ber\u00b7gen", "hier", "vnd", "dar", "/", "als", "ei\u00b7ner", "Burg", "/", "vm\u00b7bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "PTKVZ", "$(", "KOUS", "ART", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Printz aller sch\u00f6nen Quell/ au\u00df welchem Wasser dringt", "tokens": ["Printz", "al\u00b7ler", "sch\u00f6\u00b7nen", "Quell", "/", "au\u00df", "wel\u00b7chem", "Was\u00b7ser", "dringt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "ADJA", "NN", "$(", "APPR", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Anm\u00fctiger dann Milch/ vnd k\u00f6stlicher dann Reben/", "tokens": ["An\u00b7m\u00fc\u00b7ti\u00b7ger", "dann", "Milch", "/", "vnd", "k\u00f6st\u00b7li\u00b7cher", "dann", "Re\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$(", "KON", "ADJD", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da vnsers Landes Kron vnd Haupt mit seinem Leben/", "tokens": ["Da", "vn\u00b7sers", "Lan\u00b7des", "Kron", "vnd", "Haupt", "mit", "sei\u00b7nem", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der werden Nymf/ offt selbst die Zeit in frewd zubringt/", "tokens": ["Der", "wer\u00b7den", "Nymf", "/", "offt", "selbst", "die", "Zeit", "in", "frewd", "zu\u00b7bringt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$(", "ADV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da jhr manch V\u00f6gelein zu ehren lieblich singt/", "tokens": ["Da", "jhr", "manch", "V\u00f6\u00b7ge\u00b7lein", "zu", "eh\u00b7ren", "lieb\u00b7lich", "singt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da nur ergetzligkeit vnd keusche Wollust schweben/", "tokens": ["Da", "nur", "er\u00b7getz\u00b7lig\u00b7keit", "vnd", "keu\u00b7sche", "Wol\u00b7lust", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vergeblich bistu nicht in diesem gr\u00fcnen Thal/", "tokens": ["Ver\u00b7geb\u00b7lich", "bis\u00b7tu", "nicht", "in", "die\u00b7sem", "gr\u00fc\u00b7nen", "Thal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKNEG", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von Klippen vnd Gebirg beschlossen vberal/", "tokens": ["Von", "Klip\u00b7pen", "vnd", "Ge\u00b7birg", "be\u00b7schlos\u00b7sen", "vbe\u00b7ral", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Die k\u00fcnstliche Natur hat darumb dich vmbfangen", "tokens": ["Die", "k\u00fcnst\u00b7li\u00b7che", "Na\u00b7tur", "hat", "da\u00b7rumb", "dich", "vmb\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "PPER", "VVINF"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Mit Felsen vnd Geb\u00fcsch/ auff da\u00df man wissen soll", "tokens": ["Mit", "Fel\u00b7sen", "vnd", "Ge\u00b7b\u00fcsch", "/", "auff", "da\u00df", "man", "wis\u00b7sen", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$(", "APPR", "KOUS", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df alle Fr\u00f6ligkeit sey M\u00fch vnd arbeit voll/", "tokens": ["Da\u00df", "al\u00b7le", "Fr\u00f6\u00b7lig\u00b7keit", "sey", "M\u00fch", "vnd", "ar\u00b7beit", "voll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd da\u00df auch nichts so sch\u00f6n/ es sey schwer zu erlangen.", "tokens": ["Vnd", "da\u00df", "auch", "nichts", "so", "sch\u00f6n", "/", "es", "sey", "schwer", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "ADV", "ADJD", "$(", "PPER", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}