{"textgrid.poem.46133": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von euern g\u00f6tlichen vorfahren,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von euern g\u00f6tlichen vorfahren,", "tokens": ["Von", "eu\u00b7ern", "g\u00f6t\u00b7li\u00b7chen", "vor\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als die durch ihrer manheit prob", "tokens": ["als", "die", "durch", "ih\u00b7rer", "man\u00b7heit", "prob"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "gefunden ein wehrhaftes lob", "tokens": ["ge\u00b7fun\u00b7den", "ein", "wehr\u00b7haf\u00b7tes", "lob"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in kriegs unsterblichen gefahren;", "tokens": ["in", "kriegs", "uns\u00b7terb\u00b7li\u00b7chen", "ge\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von ihrer weisheit, macht, verstand,", "tokens": ["Von", "ih\u00b7rer", "weis\u00b7heit", ",", "macht", ",", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "damit den freinden sie gen\u00fctzet", "tokens": ["da\u00b7mit", "den", "frein\u00b7den", "sie", "ge\u00b7n\u00fct\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "und wider manche feind das land", "tokens": ["und", "wi\u00b7der", "man\u00b7che", "feind", "das", "land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "oftmals in h\u00f6chster not gesch\u00fctzet:", "tokens": ["oft\u00b7mals", "in", "h\u00f6chs\u00b7ter", "not", "ge\u00b7sch\u00fct\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Ja, von dem helden selbs allein", "tokens": ["Ja", ",", "von", "dem", "hel\u00b7den", "selbs", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "ADV", "ADV"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "von dessen ruhms beschreitem schein", "tokens": ["von", "des\u00b7sen", "ruhms", "be\u00b7schrei\u00b7tem", "schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "das land, ja das ganz erdreich zeuget,", "tokens": ["das", "land", ",", "ja", "das", "ganz", "er\u00b7dreich", "zeu\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "von dessen samen ihr gezeuget:", "tokens": ["von", "des\u00b7sen", "sa\u00b7men", "ihr", "ge\u00b7zeu\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wolt ich ein solches lob beschreiben,", "tokens": ["Wolt", "ich", "ein", "sol\u00b7ches", "lob", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df ihres namens wahrer preis", "tokens": ["da\u00df", "ih\u00b7res", "na\u00b7mens", "wah\u00b7rer", "preis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "solt, o prinz, wie sie from und weis,", "tokens": ["solt", ",", "o", "prinz", ",", "wie", "sie", "from", "und", "weis", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "FM", "FM", "$,", "PWAV", "PPER", "ADJD", "KON", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "auch euers namens preis verbleiben:", "tokens": ["auch", "eu\u00b7ers", "na\u00b7mens", "preis", "ver\u00b7blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die tugend aber, welche euch", "tokens": ["Die", "tu\u00b7gend", "a\u00b7ber", ",", "wel\u00b7che", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "kan liebreich und lobreicher machen,", "tokens": ["kan", "lieb\u00b7reich", "und", "lob\u00b7rei\u00b7cher", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "w\u00fcrd euch ab fremdem verdienst gleich", "tokens": ["w\u00fcrd", "euch", "ab", "frem\u00b7dem", "ver\u00b7dienst", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "schamrot zu werden verursachen.", "tokens": ["scham\u00b7rot", "zu", "wer\u00b7den", "ver\u00b7ur\u00b7sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Darum ich euch vil mehr begehr,", "tokens": ["Da\u00b7rum", "ich", "euch", "vil", "mehr", "be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "erkennend euer eigne ehr,", "tokens": ["er\u00b7ken\u00b7nend", "eu\u00b7er", "eig\u00b7ne", "ehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "ein eigen altar aufzurichten", "tokens": ["ein", "ei\u00b7gen", "al\u00b7tar", "auf\u00b7zu\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "und aufzuopfern eigne fr\u00fcchten.", "tokens": ["und", "auf\u00b7zu\u00b7o\u00b7pfern", "eig\u00b7ne", "fr\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wolt ich sein der frucht gewehret,", "tokens": ["Doch", "wolt", "ich", "sein", "der", "frucht", "ge\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dan m\u00fch ohn frucht macht allen bang,", "tokens": ["dan", "m\u00fch", "ohn", "frucht", "macht", "al\u00b7len", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "PIAT", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "die recht in seinem lobgesang", "tokens": ["die", "recht", "in", "sei\u00b7nem", "lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "auch Gallimachus selbs begehret:", "tokens": ["auch", "Gal\u00b7li\u00b7ma\u00b7chus", "selbs", "be\u00b7ge\u00b7hret", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mit tugend, spricht er, und mit hab", "tokens": ["Mit", "tu\u00b7gend", ",", "spricht", "er", ",", "und", "mit", "hab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "APPR", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "thu mich, o Jupiter, begaben!", "tokens": ["thu", "mich", ",", "o", "Ju\u00b7pi\u00b7ter", ",", "be\u00b7ga\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "ohn gut die tugend ist schabab,", "tokens": ["ohn", "gut", "die", "tu\u00b7gend", "ist", "scha\u00b7bab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "kein gut woh ich ohn tugend haben!", "tokens": ["kein", "gut", "woh", "ich", "ohn", "tu\u00b7gend", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVFIN", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dan keines ohn das ander gut:", "tokens": ["Dan", "kei\u00b7nes", "ohn", "das", "an\u00b7der", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "darum gib beeder, da\u00df mein mut", "tokens": ["da\u00b7rum", "gib", "bee\u00b7der", ",", "da\u00df", "mein", "mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVIMP", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "sich stets der tugend m\u00f6g ergeben", "tokens": ["sich", "stets", "der", "tu\u00b7gend", "m\u00f6g", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "und auch mit g\u00fctern fr\u00f6lich leben.", "tokens": ["und", "auch", "mit", "g\u00fc\u00b7tern", "fr\u00f6\u00b7lich", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die helden hielten hoch vor jahren", "tokens": ["Die", "hel\u00b7den", "hiel\u00b7ten", "hoch", "vor", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der dichter treflichen verstand", "tokens": ["der", "dich\u00b7ter", "tref\u00b7li\u00b7chen", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und ihnen lie\u00df ihr milde hand", "tokens": ["und", "ih\u00b7nen", "lie\u00df", "ihr", "mil\u00b7de", "hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vil ehr und g\u00fcter widerfahren:", "tokens": ["vil", "ehr", "und", "g\u00fc\u00b7ter", "wi\u00b7der\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die helden haben auch allzeit", "tokens": ["Die", "hel\u00b7den", "ha\u00b7ben", "auch", "all\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "billich die poesi geliebet,", "tokens": ["bil\u00b7lich", "die", "po\u00b7e\u00b7si", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "ja, ihrer vil auch nach dem streit", "tokens": ["ja", ",", "ih\u00b7rer", "vil", "auch", "nach", "dem", "streit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "sich selbs darinnen gern ge\u00fcbet.", "tokens": ["sich", "selb