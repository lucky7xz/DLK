{"dta.poem.9927": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab augen und kan nicht sehen himmel oder erden/", "tokens": ["Ich", "hab", "au\u00b7gen", "und", "kan", "nicht", "se\u00b7hen", "him\u00b7mel", "o\u00b7der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "VMFIN", "PTKNEG", "VVINF", "NN", "KON", "NN", "$("], "meter": "-+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der verliebten augen offt sehr durch mich betrogen werden/", "tokens": ["Der", "ver\u00b7lieb\u00b7ten", "au\u00b7gen", "offt", "sehr", "durch", "mich", "be\u00b7tro\u00b7gen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$("], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Frauenzim\u0303er dien\u2019 ich endlich/ wenn die sonne scheinet hei\u00df/", "tokens": ["Frau\u00b7en\u00b7zim\u0303er", "dien'", "ich", "end\u00b7lich", "/", "wenn", "die", "son\u00b7ne", "schei\u00b7net", "hei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$(", "KOUS", "ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Sch\u00f6n und he\u00dflich ist mir eines/ achte weder schwartz noch", "tokens": ["Sch\u00f6n", "und", "he\u00df\u00b7lich", "ist", "mir", "ei\u00b7nes", "/", "ach\u00b7te", "we\u00b7der", "schwartz", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "PPER", "ART", "$(", "ADJA", "KON", "ADJD", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "wei\u00df.", "tokens": ["wei\u00df", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}