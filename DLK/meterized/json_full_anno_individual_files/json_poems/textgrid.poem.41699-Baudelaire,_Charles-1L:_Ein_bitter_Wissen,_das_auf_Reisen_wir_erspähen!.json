{"textgrid.poem.41699": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein bitter Wissen, das auf Reisen wir ersp\u00e4hen!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein bitter Wissen, das auf Reisen wir ersp\u00e4hen!", "tokens": ["Ein", "bit\u00b7ter", "Wis\u00b7sen", ",", "das", "auf", "Rei\u00b7sen", "wir", "er\u00b7sp\u00e4\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt l\u00e4\u00dft, eng und klein, f\u00fcr ewig festgebannt,", "tokens": ["Die", "Welt", "l\u00e4\u00dft", ",", "eng", "und", "klein", ",", "f\u00fcr", "e\u00b7wig", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Uns gestern, morgen, heut das eigne Bildnis sehen,", "tokens": ["Uns", "ge\u00b7stern", ",", "mor\u00b7gen", ",", "heut", "das", "eig\u00b7ne", "Bild\u00b7nis", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Oase tiefen Grauns in \u00f6der W\u00fcsten Sand!", "tokens": ["O\u00b7a\u00b7se", "tie\u00b7fen", "Grauns", "in", "\u00f6\u00b7der", "W\u00fcs\u00b7ten", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mu\u00df bleiben man, mu\u00df fliehn? Kannst bleiben du, so bleibe;", "tokens": ["Mu\u00df", "blei\u00b7ben", "man", ",", "mu\u00df", "fliehn", "?", "Kannst", "blei\u00b7ben", "du", ",", "so", "blei\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PIS", "$,", "VMFIN", "VVINF", "$.", "NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geh, wenn dir's not! Der flieht, der duckt verborgen sich,", "tokens": ["Geh", ",", "wenn", "dir's", "not", "!", "Der", "flieht", ",", "der", "duckt", "ver\u00b7bor\u00b7gen", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIS", "VVFIN", "$.", "ART", "VVFIN", "$,", "PRELS", "ADJD", "VVPP", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df er die Wachsamkeit des Feindes hintertreibe,", "tokens": ["Da\u00df", "er", "die", "Wach\u00b7sam\u00b7keit", "des", "Fein\u00b7des", "hin\u00b7ter\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Zeit! \u2013 O L\u00e4ufer sind, die unab\u00e4nderlich", "tokens": ["Der", "Zeit", "!", "\u2013", "O", "L\u00e4u\u00b7fer", "sind", ",", "die", "un\u00b7ab\u00b7\u00e4n\u00b7der\u00b7lich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "NE", "NN", "VAFIN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie die Apostel und der ewge Jude eilen,", "tokens": ["Wie", "die", "A\u00b7pos\u00b7tel", "und", "der", "ew\u00b7ge", "Ju\u00b7de", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schar, der Kiel und Rad nie schnell genug erschien,", "tokens": ["Die", "Schar", ",", "der", "Kiel", "und", "Rad", "nie", "schnell", "ge\u00b7nug", "er\u00b7schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu fliehn des Gegners Netz; und andere verweilen", "tokens": ["Zu", "fliehn", "des", "Geg\u00b7ners", "Netz", ";", "und", "an\u00b7de\u00b7re", "ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "NN", "NN", "$.", "KON", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am Ort, der sie gebar, und t\u00f6ten dennoch ihn.", "tokens": ["Am", "Ort", ",", "der", "sie", "ge\u00b7bar", ",", "und", "t\u00f6\u00b7ten", "den\u00b7noch", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wann endlich seinen Fu\u00df im R\u00fccken wir gewahren,", "tokens": ["Wann", "end\u00b7lich", "sei\u00b7nen", "Fu\u00df", "im", "R\u00fc\u00b7cken", "wir", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann k\u00f6nnen hoffen wir und rufen laut; Voran!", "tokens": ["Dann", "k\u00f6n\u00b7nen", "hof\u00b7fen", "wir", "und", "ru\u00b7fen", "laut", ";", "Vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wie vor Zeiten einst gen China wir gefahren,", "tokens": ["So", "wie", "vor", "Zei\u00b7ten", "einst", "gen", "Chi\u00b7na", "wir", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "ADV", "APPR", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Blick auf weiter See, die Haare im Orkan.", "tokens": ["Den", "Blick", "auf", "wei\u00b7ter", "See", ",", "die", "Haa\u00b7re", "im", "Or\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir werden froh das Meer der Finsternisse gr\u00fc\u00dfen,", "tokens": ["Wir", "wer\u00b7den", "froh", "das", "Meer", "der", "Fins\u00b7ter\u00b7nis\u00b7se", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem jungen Wandrer gleich, des Herz sich freudig hebt,", "tokens": ["Dem", "jun\u00b7gen", "Wand\u00b7rer", "gleich", ",", "des", "Herz", "sich", "freu\u00b7dig", "hebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "H\u00f6rt diese Stimmen ihr, die dunklen, t\u00f6dlich-s\u00fc\u00dfen,", "tokens": ["H\u00f6rt", "die\u00b7se", "Stim\u00b7men", "ihr", ",", "die", "dunk\u00b7len", ",", "t\u00f6d\u00b7lich\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "PPER", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die singen: Kommt hierher, die ihr zu speisen strebt", "tokens": ["Die", "sin\u00b7gen", ":", "Kommt", "hier\u00b7her", ",", "die", "ihr", "zu", "spei\u00b7sen", "strebt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "PAV", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vom Lotus selgen Dufts. Hier erntet ihr alleine", "tokens": ["Vom", "Lo\u00b7tus", "sel\u00b7gen", "Dufts", ".", "Hier", "ern\u00b7tet", "ihr", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wunderfrucht, nach der ihr hungernd lang geirrt;", "tokens": ["Die", "Wun\u00b7der\u00b7frucht", ",", "nach", "der", "ihr", "hun\u00b7gernd", "lang", "ge\u00b7irrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt ihr berauschen euch am seltsam-milden Scheine", "tokens": ["Kommt", "ihr", "be\u00b7rau\u00b7schen", "euch", "am", "selt\u00b7sam\u00b7mil\u00b7den", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Sommernachmittags, der niemals enden wird?", "tokens": ["Des", "Som\u00b7mer\u00b7nach\u00b7mit\u00b7tags", ",", "der", "nie\u00b7mals", "en\u00b7den", "wird", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die traute Stimme weist uns Schatten, l\u00e4ngst begraben;", "tokens": ["Die", "trau\u00b7te", "Stim\u00b7me", "weist", "uns", "Schat\u00b7ten", ",", "l\u00e4ngst", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schar der Pylade erschlie\u00dft die Arme weit.", "tokens": ["Die", "Schar", "der", "Py\u00b7la\u00b7de", "er\u00b7schlie\u00dft", "die", "Ar\u00b7me", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "\u00bbschwimm zu Elektren hin, dein m\u00fcdes Herz zu laben!\u00ab", "tokens": ["\u00bb", "schwimm", "zu", "E\u00b7lekt\u00b7ren", "hin", ",", "dein", "m\u00fc\u00b7des", "Herz", "zu", "la\u00b7ben", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "APPR", "NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ruft sie, der wir die Knie gek\u00fc\u00dft vor langer Zeit.", "tokens": ["Ruft", "sie", ",", "der", "wir", "die", "Knie", "ge\u00b7k\u00fc\u00dft", "vor", "lan\u00b7ger", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein bitter Wissen, das auf Reisen wir ersp\u00e4hen!", "tokens": ["Ein", "bit\u00b7ter", "Wis\u00b7sen", ",", "das", "auf", "Rei\u00b7sen", "wir", "er\u00b7sp\u00e4\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt l\u00e4\u00dft, eng und klein, f\u00fcr ewig festgebannt,", "tokens": ["Die", "Welt", "l\u00e4\u00dft", ",", "eng", "und", "klein", ",", "f\u00fcr", "e\u00b7wig", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iamb