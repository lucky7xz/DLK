{"textgrid.poem.65312": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Nachtst\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4llt ein Stern vom Himmel,", "tokens": ["Es", "f\u00e4llt", "ein", "Stern", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich fing' ihn auf so gern!", "tokens": ["Ich", "fing'", "ihn", "auf", "so", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin bist du gefallen,", "tokens": ["Wo\u00b7hin", "bist", "du", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du wundersch\u00f6ner Stern?", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Stern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbin's Meer bin ich gefallen,", "tokens": ["\u00bb", "in's", "Meer", "bin", "ich", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief in die schwarze Fluth;", "tokens": ["Tief", "in", "die", "schwar\u00b7ze", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Leuchten mu\u00df ich lassen,", "tokens": ["Das", "Leuch\u00b7ten", "mu\u00df", "ich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und in mir brennt die Gluth.\u00ab", "tokens": ["Und", "in", "mir", "brennt", "die", "Gluth", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dianen seh' ich wandeln", "tokens": ["Di\u00b7a\u00b7nen", "seh'", "ich", "wan\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl \u00fcber das tiefe Meer.", "tokens": ["Wohl", "\u00fc\u00b7ber", "das", "tie\u00b7fe", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was schleichst du, keusche G\u00f6ttin,", "tokens": ["Was", "schleichst", "du", ",", "keu\u00b7sche", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So traurig hin und her?", "tokens": ["So", "trau\u00b7rig", "hin", "und", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbmein Stern ist mir gefallen", "tokens": ["\u00bb", "mein", "Stern", "ist", "mir", "ge\u00b7fal\u00b7len"], "token_info": ["punct", "word", "word", "