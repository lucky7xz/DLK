{"textgrid.poem.60761": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gelder eingetrieben,", "tokens": ["Wer", "Gel\u00b7der", "ein\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchbebt die Nacht vor Dieben;", "tokens": ["Durch\u00b7bebt", "die", "Nacht", "vor", "Die\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir, der ich nichts besitze,", "tokens": ["Mir", ",", "der", "ich", "nichts", "be\u00b7sit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergeht sie nach Belieben.", "tokens": ["Ver\u00b7geht", "sie", "nach", "Be\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es dunkeln zwar die L\u00fcfte,", "tokens": ["Es", "dun\u00b7keln", "zwar", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch sind sie rein geblieben;", "tokens": ["Doch", "sind", "sie", "rein", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da senkt des Himmels Wagen", "tokens": ["Da", "senkt", "des", "Him\u00b7mels", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Sterne heil'ge Sieben.", "tokens": ["Der", "Ster\u00b7ne", "heil'\u00b7ge", "Sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "O lernt die Welt beschauen,", "tokens": ["O", "lernt", "die", "Welt", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Dann lernt ihr auch sie lieben!", "tokens": ["Dann", "lernt", "ihr", "auch", "sie", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bem\u00e4chtigt euch der Tage,", "tokens": ["Be\u00b7m\u00e4ch\u00b7tigt", "euch", "der", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die Jedem schnell zerstieben!", "tokens": ["Die", "Je\u00b7dem", "schnell", "zer\u00b7stie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Welt ist eine Tafel,", "tokens": ["Die", "Welt", "ist", "ei\u00b7ne", "Ta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Noch viel ist unbeschrieben.", "toke