{"textgrid.poem.43893": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwey Tage soll ich dich und deinen Umgang meiden,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwey Tage soll ich dich und deinen Umgang meiden,", "tokens": ["Zwey", "Ta\u00b7ge", "soll", "ich", "dich", "und", "dei\u00b7nen", "Um\u00b7gang", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "PRF", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du treue Magdalis, das geht mir bitter ein.", "tokens": ["Du", "treu\u00b7e", "Mag\u00b7da\u00b7lis", ",", "das", "geht", "mir", "bit\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bringt mir die kurze Zeit solch ungemeines Leiden,", "tokens": ["Bringt", "mir", "die", "kur\u00b7ze", "Zeit", "solch", "un\u00b7ge\u00b7mei\u00b7nes", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie gro\u00df, bedenck es doch, wird nicht mein Schmerzen seyn,", "tokens": ["Wie", "gro\u00df", ",", "be\u00b7denck", "es", "doch", ",", "wird", "nicht", "mein", "Schmer\u00b7zen", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "PTKNEG", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn ich das Vaterland mit einer fremden Erde", "tokens": ["Wenn", "ich", "das", "Va\u00b7ter\u00b7land", "mit", "ei\u00b7ner", "frem\u00b7den", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf so viel Jahre bald einmahl vertauschen werde.", "tokens": ["Auf", "so", "viel", "Jah\u00b7re", "bald", "ein\u00b7mahl", "ver\u00b7tau\u00b7schen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADV", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wiltu deine Gunst mir zur Gefehrtin schencken", "tokens": ["Doch", "wil\u00b7tu", "dei\u00b7ne", "Gunst", "mir", "zur", "Ge\u00b7fehr\u00b7tin", "schen\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcrdigt deine Huld mich der Best\u00e4ndigkeit,", "tokens": ["Und", "w\u00fcr\u00b7digt", "dei\u00b7ne", "Huld", "mich", "der", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So glaube, da\u00df der Trost von deinem Angedencken", "tokens": ["So", "glau\u00b7be", ",", "da\u00df", "der", "Trost", "von", "dei\u00b7nem", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den sch\u00e4rfsten Wermuth mir mit Zucker \u00fcberstreut,", "tokens": ["Den", "sch\u00e4rfs\u00b7ten", "Wer\u00b7muth", "mir", "mit", "Zu\u00b7cker", "\u00fc\u00b7bers\u00b7treut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und g\u00f6nne meiner Brust, da\u00df sie dein Herz behalte,", "tokens": ["Und", "g\u00f6n\u00b7ne", "mei\u00b7ner", "Brust", ",", "da\u00df", "sie", "dein", "Herz", "be\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bis mein entseelter Leib einst in der Gruft erkalte.", "tokens": ["Bis", "mein", "ent\u00b7seel\u00b7ter", "Leib", "einst", "in", "der", "Gruft", "er\u00b7kal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwey Tage soll ich dich und deinen Umgang meiden,", "tokens": ["Zwey", "Ta\u00b7ge", "soll", "ich", "dich", "und", "dei\u00b7nen", "Um\u00b7gang", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "PRF", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iamb