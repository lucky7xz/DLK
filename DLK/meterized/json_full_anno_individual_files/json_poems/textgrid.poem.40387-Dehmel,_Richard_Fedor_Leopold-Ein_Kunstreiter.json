{"textgrid.poem.40387": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Kunstreiter", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Kunstreiter werden,", "tokens": ["Ich", "kann", "ein", "Kuns\u00b7trei\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "das kann nicht jedermann;", "tokens": ["das", "kann", "nicht", "je\u00b7der\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "nur wer bis in die Zehenspitzen", "tokens": ["nur", "wer", "bis", "in", "die", "Ze\u00b7hen\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sich selber b\u00e4ndigen kann,", "tokens": ["sich", "sel\u00b7ber", "b\u00e4n\u00b7di\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "bis in die Turnschuhspitzen!", "tokens": ["bis", "in", "die", "Turn\u00b7schuh\u00b7spit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hei, wenn beim gro\u00dfen Luftsprung", "tokens": ["Hei", ",", "wenn", "beim", "gro\u00b7\u00dfen", "Luft\u00b7sprung"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPRART", "AD