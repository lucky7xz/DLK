, "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn durch das Land ", "tokens": ["Wenn", "durch", "das", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Da silbern wallt", "tokens": ["Da", "sil\u00b7bern", "wallt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Im Osten auf des Morgens Fahne,", "tokens": ["Im", "Os\u00b7ten", "auf", "des", "Mor\u00b7gens", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und, ein zerflossner Nebelstreif,", "tokens": ["Und", ",", "ein", "zer\u00b7floss\u00b7ner", "Ne\u00b7bel\u00b7streif", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Meister f\u00e4hrt empor am Krahne. \u2014", "tokens": ["Der", "Meis\u00b7ter", "f\u00e4hrt", "em\u00b7por", "am", "Krah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit R\u00e4derknarren und Gepfeif,", "tokens": ["Mit", "R\u00e4\u00b7der\u00b7knar\u00b7ren", "und", "Ge\u00b7pfeif", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein rauchend Ungeheuer, sch\u00e4umt", "tokens": ["Ein", "rau\u00b7chend", "Un\u0