{"dta.poem.7381": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Meister Gerhard von C\u00f6ln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn in den linden Vollmondn\u00e4chten", "tokens": ["Wenn", "in", "den", "lin\u00b7den", "Voll\u00b7mond\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nebel lagern \u00fcber'm Rhein,", "tokens": ["Die", "Ne\u00b7bel", "la\u00b7gern", "\u00fc\u00b7ber'm", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und graue Silberf\u00e4den flechten", "tokens": ["Und", "grau\u00b7e", "Sil\u00b7ber\u00b7f\u00e4\u00b7den", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Florgewand dem Heilgenschrein:", "tokens": ["Ein", "Flor\u00b7ge\u00b7wand", "dem", "Heil\u00b7gen\u00b7schrein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es tr\u00e4umt die Waldung, duftums\u00e4umt,", "tokens": ["Es", "tr\u00e4umt", "die", "Wal\u00b7dung", ",", "duf\u00b7tum\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es tr\u00e4umt die dunkle Fluthenschlange,", "tokens": ["Es", "tr\u00e4umt", "die", "dunk\u00b7le", "Flut\u00b7hen\u00b7schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie eine Robbe liegt am Hange", "tokens": ["Wie", "ei\u00b7ne", "Rob\u00b7be", "liegt", "am", "Han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Sch\u00fcrg' und tr\u00e4umt.", "tokens": ["Der", "Sch\u00fcr\u00b7g'", "und", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Tief zieht die Nacht den feuchten Odem,", "tokens": ["Tief", "zieht", "die", "Nacht", "den", "feuch\u00b7ten", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Des Walles Gr\u00e4ser zucken matt,", "tokens": ["Des", "Wal\u00b7les", "Gr\u00e4\u00b7ser", "zu\u00b7cken", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein zerhauchter Grabesbrodem", "tokens": ["Und", "ein", "zer\u00b7hauch\u00b7ter", "Gra\u00b7bes\u00b7bro\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt \u00fcber der entschlafnen Stadt:", "tokens": ["Liegt", "\u00fc\u00b7ber", "der", "ent\u00b7schlaf\u00b7nen", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie h\u00f6rt das Schlummerlied der Well'n,", "tokens": ["Sie", "h\u00f6rt", "das", "Schlum\u00b7mer\u00b7lied", "der", "Well'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das leise murmelnde Gesch\u00e4ume,", "tokens": ["Das", "lei\u00b7se", "mur\u00b7meln\u00b7de", "Ge\u00b7sch\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und tiefer, tiefer sinkt in Tr\u00e4ume", "tokens": ["Und", "tie\u00b7fer", ",", "tie\u00b7fer", "sinkt", "in", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das alte C\u00f6ln.", "tokens": ["Das", "al\u00b7te", "C\u00f6ln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dort wo die graue Cathedrale,", "tokens": ["Dort", "wo", "die", "grau\u00b7e", "Cat\u00b7hed\u00b7ra\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein riesenhafter Zeitentraum,", "tokens": ["Ein", "rie\u00b7sen\u00b7haf\u00b7ter", "Zei\u00b7ten\u00b7traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entsteigt dem d\u00fcstern Tr\u00fcmmermale", "tokens": ["Ent\u00b7steigt", "dem", "d\u00fcs\u00b7tern", "Tr\u00fcm\u00b7mer\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Macht, die auch zerrann wie Schaum \u2014", "tokens": ["Der", "Macht", ",", "die", "auch", "zer\u00b7rann", "wie", "Schaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort, in der Scheibe Purpurrund", "tokens": ["Dort", ",", "in", "der", "Schei\u00b7be", "Pur\u00b7pur\u00b7rund"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat taumelnd sich der Stral gegossen", "tokens": ["Hat", "tau\u00b7melnd", "sich", "der", "Stral", "ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PRF", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sinkt, und sinkt, in Traum zerflossen,", "tokens": ["Und", "sinkt", ",", "und", "sinkt", ",", "in", "Traum", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis auf den Grund.", "tokens": ["Bis", "auf", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wie ist es schauerlich im weiten", "tokens": ["Wie", "ist", "es", "schau\u00b7er\u00b7lich", "im", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versteinten \u00f6den Palmenwald,", "tokens": ["Ver\u00b7stein\u00b7ten", "\u00f6\u00b7den", "Pal\u00b7men\u00b7wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo die Gedanken niedergleiten", "tokens": ["Wo", "die", "Ge\u00b7dan\u00b7ken", "nie\u00b7derg\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Anakonden schwer und kalt;", "tokens": ["Wie", "A\u00b7na\u00b7kon\u00b7den", "schwer", "und", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und blutig sich der Schatten hebt", "tokens": ["Und", "blu\u00b7tig", "sich", "der", "Schat\u00b7ten", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am blut'gen M\u00e4rtyrer der Scheibe,", "tokens": ["Am", "blut'\u00b7gen", "M\u00e4r\u00b7ty\u00b7rer", "der", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie neben dem gebannten Leibe", "tokens": ["Wie", "ne\u00b7ben", "dem", "ge\u00b7bann\u00b7ten", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Die Seele schwebt. ", "tokens": ["Die", "See\u00b7le", "schwebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der Ampel Schein verlosch, im Schiffe", "tokens": ["Der", "Am\u00b7pel", "Schein", "ver\u00b7losch", ",", "im", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4ft halbgeschlossen Blum' und Kraut;", "tokens": ["Schl\u00e4ft", "halb\u00b7ge\u00b7schlos\u00b7sen", "Blum'", "und", "Kraut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie nackt gesp\u00fclte Uferriffe", "tokens": ["Wie", "nackt", "ge\u00b7sp\u00fcl\u00b7te", "Uf\u00b7er\u00b7rif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Streben lehnen, tief ergraut;", "tokens": ["Die", "Stre\u00b7ben", "leh\u00b7nen", ",", "tief", "er\u00b7graut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Anschwellend zum Altare dort,", "tokens": ["An\u00b7schwel\u00b7lend", "zum", "Al\u00b7ta\u00b7re", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann aufw\u00e4rts dehnend, lang gezogen,", "tokens": ["Dann", "auf\u00b7w\u00e4rts", "deh\u00b7nend", ",", "lang", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Schlingen die H\u00e4upter sie zu Bogen,", "tokens": ["Schlin\u00b7gen", "die", "H\u00e4up\u00b7ter", "sie", "zu", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und schlummern fort.", "tokens": ["Und", "schlum\u00b7mern", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und immer schwerer will es rinnen", "tokens": ["Und", "im\u00b7mer", "schwe\u00b7rer", "will", "es", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Quader, S\u00e4ulenknauf und Schaft,", "tokens": ["Von", "Qua\u00b7der", ",", "S\u00e4u\u00b7len\u00b7knauf", "und", "Schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in dem Strale will's gewinnen", "tokens": ["Und", "in", "dem", "Stra\u00b7le", "will's", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein dunstig Leben, geisterhaft:", "tokens": ["Ein", "duns\u00b7tig", "Le\u00b7ben", ",", "geis\u00b7ter\u00b7haft", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da horch! es dr\u00f6hnt im Thurme \u2014 ha!", "tokens": ["Da", "horch", "!", "es", "dr\u00f6hnt", "im", "Thur\u00b7me", "ha", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Glocke summt \u2014 da leise s\u00e4uselt", "tokens": ["Die", "Glo\u00b7cke", "summt", "da", "lei\u00b7se", "s\u00e4u\u00b7selt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "ADJD", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Dunst, er zucket, wimmelt, kr\u00e4uselt, \u2014", "tokens": ["Der", "Dunst", ",", "er", "zu\u00b7cket", ",", "wim\u00b7melt", ",", "kr\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nun steht es da! \u2014", "tokens": ["Nun", "steht", "es", "da", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ein Nebelm\u00e4ntlein umgeschlagen,", "tokens": ["Ein", "Ne\u00b7bel\u00b7m\u00e4nt\u00b7lein", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein graues K\u00e4ppchen, grau Gewand,", "tokens": ["Ein", "grau\u00b7es", "K\u00e4pp\u00b7chen", ",", "grau", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am grauen Halse grauer Kragen,", "tokens": ["Am", "grau\u00b7en", "Hal\u00b7se", "grau\u00b7er", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Richtmaa\u00df in der Aschenhand.", "tokens": ["Das", "Richt\u00b7ma\u00b7a\u00df", "in", "der", "A\u00b7schen\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durch seine Glieder zitternd geht", "tokens": ["Durch", "sei\u00b7ne", "Glie\u00b7der", "zit\u00b7ternd", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Stral wie in verhaltner Trauer,", "tokens": ["Der", "Stral", "wie", "in", "ver\u00b7halt\u00b7ner", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch an dem Estrich, an der Mauer", "tokens": ["Doch", "an", "dem", "Est\u00b7rich", ",", "an", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Schatten steht.", "tokens": ["Kein", "Schat\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Es wiegt das Haupt nach allen Seiten,", "tokens": ["Es", "wiegt", "das", "Haupt", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN"