"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Und manch verachter Wurm hilfft auff der P\u00e4\u00dfligkeit:", "tokens": ["Und", "manch", "ver\u00b7ach\u00b7ter", "Wurm", "hilfft", "auff", "der", "P\u00e4\u00df\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da\u00df uns der Natterstich bi\u00df auff den Tod verletzet/", "tokens": ["Da\u00df", "uns", "der", "Nat\u00b7ter\u00b7stich", "bi\u00df", "auff", "den", "Tod", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hingegen auch ihr Saltz vor Pest und Gifft besch\u00fctzt/", "tokens": ["Hin\u00b7ge\u00b7gen", "auch", "ihr", "Saltz", "vor", "Pest", "und", "Gifft", "be\u00b7sch\u00fctzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.39": {"text": "Gleich einer Panace sich Seuchen wiedersetzet/", "tokens": ["Gleich", "ei\u00b7ner", "Pa\u00b7na\u00b7ce", "sich", "Seu\u00b7chen", "wie\u00b7der\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die sonst unheilbar seyn/ und da kein Artzt nichts n\u00fctzt:", "tokens": ["Die", "sonst", "un\u00b7heil\u00b7bar", 