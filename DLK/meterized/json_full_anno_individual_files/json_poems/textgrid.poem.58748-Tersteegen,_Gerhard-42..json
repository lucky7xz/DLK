{"textgrid.poem.58748": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weide meine Schafe! Joh. 21, 16", "tokens": ["Wei\u00b7de", "mei\u00b7ne", "Scha\u00b7fe", "!", "Joh", ".", "21", ",", "16"], "token_info": ["word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["NN", "PPOSAT", "NN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Gott, mein Hirte, komm zu Hilfe deiner Herde,", "tokens": ["Mein", "Gott", ",", "mein", "Hir\u00b7te", ",", "komm", "zu", "Hil\u00b7fe", "dei\u00b7ner", "Her\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die W\u00f6lfe dringen stark in deinen Schafstall ein,", "tokens": ["Die", "W\u00f6l\u00b7fe", "drin\u00b7gen", "stark", "in", "dei\u00b7nen", "Schaf\u00b7stall", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie rei\u00dfen weg ein armes Sch\u00e4felein;", "tokens": ["Sie", "rei\u00b7\u00dfen", "weg", "ein", "ar\u00b7mes", "Sch\u00e4\u00b7fe\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bitte dich, ach komm, eh' alles w\u00fcste werde!", "tokens": ["Ich", "bit\u00b7te", "dich", ",", "ach", "komm", ",", "eh'", "al\u00b7les", "w\u00fcs\u00b7te", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ITJ", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du gabst mir oft Befehl, ich sollt' die Schafe speisen;", "tokens": ["Du", "gabst", "mir", "oft", "Be\u00b7fehl", ",", "ich", "sollt'", "die", "Scha\u00b7fe", "spei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O du, in dem man stets die sch\u00f6nste Weide find't,", "tokens": ["O", "du", ",", "in", "dem", "man", "stets", "die", "sch\u00f6ns\u00b7te", "Wei\u00b7de", "find't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PRELS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach, f\u00fchre sie doch selbst, sonst sie des Todes sind!", "tokens": ["Ach", ",", "f\u00fch\u00b7re", "sie", "doch", "selbst", ",", "sonst", "sie", "des", "To\u00b7des", "sind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Wolf, der voller Wut, kommt dort, sie zu zerrei\u00dfen.", "tokens": ["Ein", "Wolf", ",", "der", "vol\u00b7ler", "Wut", ",", "kommt", "dort", ",", "sie", "zu", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ach, du hast ihn erlegt, die Furcht ist mir verschwunden;", "tokens": ["Ach", ",", "du", "hast", "ihn", "er\u00b7legt", ",", "die", "Furcht", "ist", "mir", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun, liebste Sch\u00e4flein, kommt und weidet ungescheut,", "tokens": ["Nun", ",", "liebs\u00b7te", "Sch\u00e4f\u00b7lein", ",", "kommt", "und", "wei\u00b7det", "un\u00b7ge\u00b7scheut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Folgt eurem Hirten nur, folgt mit Best\u00e4ndigkeit,", "tokens": ["Folgt", "eu\u00b7rem", "Hir\u00b7ten", "nur", ",", "folgt", "mit", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei ihm wird \u00dcberflu\u00df und alles Gut' gefunden!", "tokens": ["Bei", "ihm", "wird", "\u00dc\u00b7berf\u00b7lu\u00df", "und", "al\u00b7les", "Gut'", "ge\u00b7fun\u00b7den", "!"], "t