{"dta.poem.14715": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unbekannte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr d\u00fcnkt, er w\u00e4r' schon einst im Dorf gewesen,", "tokens": ["Ihr", "d\u00fcnkt", ",", "er", "w\u00e4r'", "schon", "einst", "im", "Dorf", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch so fremd und seltsam war die Tracht,", "tokens": ["Und", "doch", "so", "fremd", "und", "selt\u00b7sam", "war", "die", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In seinen Mienen feur'ge Schrift zu lesen", "tokens": ["In", "sei\u00b7nen", "Mie\u00b7nen", "feur'\u00b7ge", "Schrift", "zu", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich Wetterleuchten fern bei stiller Nacht,", "tokens": ["Gleich", "Wet\u00b7ter\u00b7leuch\u00b7ten", "fern", "bei", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und traf sein Auge sie, wollt' ihr fast grauen,", "tokens": ["Und", "traf", "sein", "Au\u00b7ge", "sie", ",", "wollt'", "ihr", "fast", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "VMFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Denn 's war, wie in den Himmelsgrund zu schauen.", "tokens": ["Denn", "'s", "war", ",", "wie", "in", "den", "Him\u00b7mels\u00b7grund", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWAV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}}}}