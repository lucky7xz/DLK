{"dta.poem.18554": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E r ist ungl\u00fcckselig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Annehmlichste der Zeit! Wie lange hat mein Hertze", "tokens": ["An\u00b7nehm\u00b7lichs\u00b7te", "der", "Zeit", "!", "Wie", "lan\u00b7ge", "hat", "mein", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "PWAV", "ADV", "VAFIN", "PPOSAT", "VVFIN"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Aus Ehrerbiethung dich mit Seufftzern nur verehrt?", "tokens": ["Aus", "Ehr\u00b7er\u00b7bie\u00b7thung", "dich", "mit", "Seufft\u00b7zern", "nur", "ver\u00b7ehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie lange brennet nun der Augen Liebes-Kertze/", "tokens": ["Wie", "lan\u00b7ge", "bren\u00b7net", "nun", "der", "Au\u00b7gen", "Lie\u00b7bes\u00b7Kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh' auch der Flammen Brand aus meinen Munde f\u00e4hrt?", "tokens": ["Eh'", "auch", "der", "Flam\u00b7men", "Brand", "aus", "mei\u00b7nen", "Mun\u00b7de", "f\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch endlich mu\u00df das Hertz aus tausend Schmertzen sprechen/", "tokens": ["Doch", "end\u00b7lich", "mu\u00df", "das", "Hertz", "aus", "tau\u00b7send", "Schmert\u00b7zen", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und den Granaten gleich durch viele Kernen brechen.", "tokens": ["Und", "den", "Gra\u00b7na\u00b7ten", "gleich", "durch", "vie\u00b7le", "Ker\u00b7nen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich h\u00e4tte meine Qvaal mit Aschen zugedecket/", "tokens": ["Ich", "h\u00e4t\u00b7te", "mei\u00b7ne", "Qvaal", "mit", "A\u00b7schen", "zu\u00b7ge\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein klagen k\u00e4me nun zu deinen Ohren nicht/", "tokens": ["Mein", "kla\u00b7gen", "k\u00e4\u00b7me", "nun", "zu", "dei\u00b7nen", "Oh\u00b7ren", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn deine Lieblichkeit nicht meine Glut erwecket/", "tokens": ["Wenn", "dei\u00b7ne", "Lieb\u00b7lich\u00b7keit", "nicht", "mei\u00b7ne", "Glut", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie mit voller Macht nun in die Flammen bricht.", "tokens": ["Da\u00df", "sie", "mit", "vol\u00b7ler", "Macht", "nun", "in", "die", "Flam\u00b7men", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Hertz und Aetna mu\u00df offt stilles Feuer hegen/", "tokens": ["Ein", "Hertz", "und", "A\u00b7et\u00b7na", "mu\u00df", "offt", "stil\u00b7les", "Feu\u00b7er", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VMFIN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nur da\u00df sich einst die Wuth soll desto mehr erregen.", "tokens": ["Nur", "da\u00df", "sich", "einst", "die", "Wuth", "soll", "des\u00b7to", "mehr", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "ADV", "ART", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich brenne/ sch\u00f6nstes Kind/ jedoch in keuschen Flammen/", "tokens": ["Ich", "bren\u00b7ne", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "je\u00b7doch", "in", "keu\u00b7schen", "Flam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Flammen deren Ru\u00df den edlen Ruhm nicht schw\u00e4rtzt:", "tokens": ["In", "Flam\u00b7men", "de\u00b7ren", "Ru\u00df", "den", "ed\u00b7len", "Ruhm", "nicht", "schw\u00e4rtzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRELAT", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Feuer/ das allein mu\u00df von dem Himmel stammen/", "tokens": ["Durch", "Feu\u00b7er", "/", "das", "al\u00b7lein", "mu\u00df", "von", "dem", "Him\u00b7mel", "stam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PDS", "ADV", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Womit der Sternen Gluth in reinen Wesen schertzt.", "tokens": ["Wo\u00b7mit", "der", "Ster\u00b7nen", "Gluth", "in", "rei\u00b7nen", "We\u00b7sen", "schertzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja deinen Tugenden und sittsamsten Geberden", "tokens": ["Ja", "dei\u00b7nen", "Tu\u00b7gen\u00b7den", "und", "sitt\u00b7sams\u00b7ten", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Mu\u00df mein getreues Hertz zum Opffer Heerde werden.", "tokens": ["Mu\u00df", "mein", "ge\u00b7treu\u00b7es", "Hertz", "zum", "Opf\u00b7fer", "Heer\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Gartien/ so dir um deine Lippen schweben/", "tokens": ["Die", "Gar\u00b7ti\u00b7en", "/", "so", "dir", "um", "dei\u00b7ne", "Lip\u00b7pen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Holdseeligkeit damit dein Auge strahlt/", "tokens": ["Und", "die", "Hold\u00b7see\u00b7lig\u00b7keit", "da\u00b7mit", "dein", "Au\u00b7ge", "strahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wei\u00df kein beredter Mund genugsam zu erheben/", "tokens": ["Wei\u00df", "kein", "be\u00b7red\u00b7ter", "Mund", "ge\u00b7nug\u00b7sam", "zu", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Pinsel ist/ der dich nach deiner W\u00fcrde mahlt;", "tokens": ["Kein", "Pin\u00b7sel", "ist", "/", "der", "dich", "nach", "dei\u00b7ner", "W\u00fcr\u00b7de", "mahlt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$(", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch mu\u00df nun alle Kunst bey deiner Pracht verschwinden/", "tokens": ["Doch", "mu\u00df", "nun", "al\u00b7le", "Kunst", "bey", "dei\u00b7ner", "Pracht", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So schau in meine Brust/ du wirst dein Bildni\u00df sinden.", "tokens": ["So", "schau", "in", "mei\u00b7ne", "Brust", "/", "du", "wirst", "dein", "Bild\u00b7ni\u00df", "sin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hier hat die Ewigkeit ihm den Altar geweihet/", "tokens": ["Hier", "hat", "die", "E\u00b7wig\u00b7keit", "ihm", "den", "Al\u00b7tar", "ge\u00b7wei\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Sch\u00f6nheit giest stets Oehl in meinen Brand.", "tokens": ["Und", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "giest", "stets", "O\u00b7ehl", "in", "mei\u00b7nen", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was aber hilfft ein Schatz/ den uns ein Traum verleihet?", "tokens": ["Was", "a\u00b7ber", "hilfft", "ein", "Schatz", "/", "den", "uns", "ein", "Traum", "ver\u00b7lei\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "$(", "ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was sehn ich mich umsonst nach den gelobten Land?", "tokens": ["Was", "sehn", "ich", "mich", "um\u00b7sonst", "nach", "den", "ge\u00b7lob\u00b7ten", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach meiner Hoffnung Schiff schwebt auf erz\u00fcrnten Wellen/", "tokens": ["Ach", "mei\u00b7ner", "Hoff\u00b7nung", "Schiff", "schwebt", "auf", "er\u00b7z\u00fcrn\u00b7ten", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die meines Gl\u00fcckes H\u00f6h' in bauen wieder f\u00e4llen!", "tokens": ["Die", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "H\u00f6h'", "in", "bau\u00b7en", "wie\u00b7der", "f\u00e4l\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mein sch\u00f6ner Freuden Stern scheint mir zwar viele Stunden/", "tokens": ["Mein", "sch\u00f6\u00b7ner", "Freu\u00b7den", "Stern", "scheint", "mir", "zwar", "vie\u00b7le", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und w\u00fcrde mir vielleicht ein Licht zum Haffen seyn/", "tokens": ["Und", "w\u00fcr\u00b7de", "mir", "viel\u00b7leicht", "ein", "Licht", "zum", "Haf\u00b7fen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn nur mein Ungel\u00fcck nicht Wolcken h\u00e4tt erfunden/", "tokens": ["Wenn", "nur", "mein", "Un\u00b7ge\u00b7l\u00fcck", "nicht", "Wol\u00b7cken", "h\u00e4tt", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die meinen Liebeskahn stets Sturm und Wetter dr\u00e4un.", "tokens": ["Die", "mei\u00b7nen", "Lie\u00b7bes\u00b7kahn", "stets", "Sturm", "und", "Wet\u00b7ter", "dr\u00e4un", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Such ich nun \u00fcberall die Klagen auszubreiten", "tokens": ["Such", "ich", "nun", "\u00fc\u00b7be\u00b7rall", "die", "Kla\u00b7gen", "aus\u00b7zu\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "ADV", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Rufft mein Verh\u00e4ngni\u00df doch nichts als Unm\u00f6glichkeiten.", "tokens": ["Rufft", "mein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "doch", "nichts", "als", "Un\u00b7m\u00f6g\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ach Himmel! Hast du mich zu dieser Gluth bestimmet/", "tokens": ["Ach", "Him\u00b7mel", "!", "Hast", "du", "mich", "zu", "die\u00b7ser", "Gluth", "be\u00b7stim\u00b7met", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VAFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum blitzt denn dein Grimm auf ihr Verboth so scharff?", "tokens": ["Wa\u00b7rum", "blitzt", "denn", "dein", "Grimm", "auf", "ihr", "Ver\u00b7both", "so", "scharff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NE", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wer sieht den heissen Brand/ der in der Asche glimmet?", "tokens": ["Wer", "sieht", "den", "heis\u00b7sen", "Brand", "/", "der", "in", "der", "A\u00b7sche", "glim\u00b7met", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer leschet/ wenn man nicht recht Feuer ruffen darff?", "tokens": ["Wer", "le\u00b7schet", "/", "wenn", "man", "nicht", "recht", "Feu\u00b7er", "ruf\u00b7fen", "darff", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PIS", "PTKNEG", "ADJD", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du l\u00e4st dein Paradie\u00df mir nur zum Schmertzen sehen/", "tokens": ["Du", "l\u00e4st", "dein", "Pa\u00b7ra\u00b7die\u00df", "mir", "nur", "zum", "Schmert\u00b7zen", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil du den Weg nicht zeigst/ vergn\u00fcgt hinein zu gehen.", "tokens": ["Weil", "du", "den", "Weg", "nicht", "zeigst", "/", "ver\u00b7gn\u00fcgt", "hin\u00b7ein", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$(", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Warum verschwendest du den Uberflu\u00df von Sch\u00e4tzen", "tokens": ["Wa\u00b7rum", "ver\u00b7schwen\u00b7dest", "du", "den", "U\u00b7ber\u00b7flu\u00df", "von", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mich nur hier allein zu den Magnete ziehn?", "tokens": ["Die", "mich", "nur", "hier", "al\u00b7lein", "zu", "den", "Mag\u00b7ne\u00b7te", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kan die Annehmlichkeit mich sonsten nicht ergetzen/", "tokens": ["Kan", "die", "An\u00b7nehm\u00b7lich\u00b7keit", "mich", "sons\u00b7ten", "nicht", "er\u00b7get\u00b7zen", "/"