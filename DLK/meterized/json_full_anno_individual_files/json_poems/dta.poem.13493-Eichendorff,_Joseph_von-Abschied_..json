{"dta.poem.13493": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O Th\u00e4ler weit, o H\u00f6hen,", "tokens": ["O", "Th\u00e4\u00b7ler", "weit", ",", "o", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O sch\u00f6ner gr\u00fcner Wald,", "tokens": ["O", "sch\u00f6\u00b7ner", "gr\u00fc\u00b7ner", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du meiner Lust und Wehen", "tokens": ["Du", "mei\u00b7ner", "Lust", "und", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "And\u00e4cht'ger Aufenthalt!", "tokens": ["An\u00b7d\u00e4cht'\u00b7ger", "Auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da drau\u00dfen, stets betrogen,", "tokens": ["Da", "drau\u00b7\u00dfen", ",", "stets", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Saus't die gesch\u00e4ft'ge Welt,", "tokens": ["Saus't", "die", "ge\u00b7sch\u00e4ft'\u00b7ge", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schlag' noch einmal die Bogen", "tokens": ["Schlag'", "noch", "ein\u00b7mal", "die", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um mich, du gr\u00fcnes Zelt!", "tokens": ["Um", "mich", ",", "du", "gr\u00fc\u00b7nes", "Zelt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn es beginnt zu tagen,", "tokens": ["Wenn", "es", "be\u00b7ginnt", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde dampft und blinkt,", "tokens": ["Die", "Er\u00b7de", "dampft", "und", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die V\u00f6gel lustig schlagen,", "tokens": ["Die", "V\u00f6\u00b7gel", "lus\u00b7tig", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df dir dein Herz erklingt:", "tokens": ["Da\u00df", "dir", "dein", "Herz", "er\u00b7klingt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da mag vergehn, verwehen", "tokens": ["Da", "mag", "ver\u00b7gehn", ",", "ver\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das tr\u00fcbe Erdenleid,", "tokens": ["Das", "tr\u00fc\u00b7be", "Er\u00b7den\u00b7leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sollst du auferstehen", "tokens": ["Da", "sollst", "du", "auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In junger Herrlichkeit!", "tokens": ["In", "jun\u00b7ger", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da steht im Wald geschrieben,", "tokens": ["Da", "steht", "im", "Wald", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein stilles, ernstes Wort", "tokens": ["Ein", "stil\u00b7les", ",", "erns\u00b7tes", "Wort"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von rechtem Thun und Lieben,", "tokens": ["Von", "rech\u00b7tem", "Thun", "und", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was des Menschen Hort.", "tokens": ["Und", "was", "des", "Men\u00b7schen", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe treu gelesen", "tokens": ["Ich", "ha\u00b7be", "treu", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Worte schlicht und wahr,", "tokens": ["Die", "Wor\u00b7te", "schlicht", "und", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und durch mein ganzes Wesen", "tokens": ["Und", "durch", "mein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ward's unaussprechlich klar.", "tokens": ["Ward's", "un\u00b7aus\u00b7sprech\u00b7lich", "klar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bald werd' ich dich verlassen,", "tokens": ["Bald", "werd'", "ich", "dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fremd in der Fremde geh'n,", "tokens": ["Fremd", "in", "der", "Frem\u00b7de", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Auf buntbewegten Gassen", "tokens": ["Auf", "bunt\u00b7be\u00b7weg\u00b7ten", "Gas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens Schauspiel sehn;", "tokens": ["Des", "Le\u00b7bens", "Schau\u00b7spiel", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und mitten in dem Leben", "tokens": ["Und", "mit\u00b7ten", "in", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird deines Ernst's Gewalt", "tokens": ["Wird", "dei\u00b7nes", "Ernst's", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich Einsamen erheben,", "tokens": ["Mich", "Ein\u00b7sa\u00b7men", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wird mein Herz nicht alt.", "tokens": ["So", "wird", "mein", "Herz", "nicht", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}