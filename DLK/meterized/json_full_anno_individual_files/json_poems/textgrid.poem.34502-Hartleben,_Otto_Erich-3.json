{"textgrid.poem.34502": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Feldern war die Frucht geschnitten,", "tokens": ["Auf", "den", "Fel\u00b7dern", "war", "die", "Frucht", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "in die Scheunen war sie eingefahren,", "tokens": ["in", "die", "Scheu\u00b7nen", "war", "sie", "ein\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und es kam der frohe Tag der Ernte.", "tokens": ["und", "es", "kam", "der", "fro\u00b7he", "Tag", "der", "Ern\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf der Tenne worfelte die Gerste", "tokens": ["Auf", "der", "Ten\u00b7ne", "wor\u00b7fel\u00b7te", "die", "Gers\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Boas mit den Knaben und den Dirnen.", "tokens": ["Boas", "mit", "den", "Kna\u00b7ben", "und", "den", "Dir\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Tief im Herzen war ein Keim entsprossen,", "tokens": ["Tief", "im", "Her\u00b7zen", "war", "ein", "Keim", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "tief in heissem, demuthvollem Herzen:", "tokens": ["tief", "in", "heis\u00b7sem", ",", "de\u00b7muth\u00b7vol\u00b7lem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "$,", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "sich zu geben dem geliebten Herren,", "tokens": ["sich", "zu", "ge\u00b7ben", "dem", "ge\u00b7lieb\u00b7ten", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "das gedachte Ruth, die Moabitin.", "tokens": ["das", "ge\u00b7dach\u00b7te", "Ruth", ",", "die", "Mo\u00b7a\u00b7bi\u00b7tin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und sie badete und salbte sorglich", "tokens": ["Und", "sie", "ba\u00b7de\u00b7te", "und", "salb\u00b7te", "sorg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "ihren Leib zu ihrer Liebe Feier,", "tokens": ["ih\u00b7ren", "Leib", "zu", "ih\u00b7rer", "Lie\u00b7be", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und sie schm\u00fcckte sich mit Festgewanden.", "tokens": ["und", "sie", "schm\u00fcck\u00b7te", "sich", "mit", "Fest\u00b7ge\u00b7wan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Froh des Segens und der reichen Ernte,", "tokens": ["Froh", "des", "Se\u00b7gens", "und", "der", "rei\u00b7chen", "Ern\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die der Herr gesch\u00fcttet auf die Tenne,", "tokens": ["die", "der", "Herr", "ge\u00b7sch\u00fct\u00b7tet", "auf", "die", "Ten\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "trank und schmauste mit den Seinen allen", "tokens": ["trank", "und", "schmaus\u00b7te", "mit", "den", "Sei\u00b7nen", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ART", "PPOSS", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Boas und sein Herz ward guter Dinge.", "tokens": ["Boas", "und", "sein", "Herz", "ward", "gu\u00b7ter", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aber da die Nacht herabgesunken,", "tokens": ["A\u00b7ber", "da", "die", "Nacht", "her\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "und der laute Schwarm sich rings verlaufen,", "tokens": ["und", "der", "lau\u00b7te", "Schwarm", "sich", "rings", "ver\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "legt er sich, von M\u00fcdigkeit bew\u00e4ltigt,", "tokens": ["legt", "er", "sich", ",", "von", "M\u00fc\u00b7dig\u00b7keit", "be\u00b7w\u00e4l\u00b7tigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "auf die Tenne, hinter eine Mandel.", "tokens": ["auf", "die", "Ten\u00b7ne", ",", "hin\u00b7ter", "ei\u00b7ne", "Man\u00b7del", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und die Liebe wandelt durch die Felder", "tokens": ["Und", "die", "Lie\u00b7be", "wan\u00b7delt", "durch", "die", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und durchschleicht die Nacht auf leichten Sohlen.", "tokens": ["und", "durch\u00b7schleicht", "die", "Nacht", "auf", "leich\u00b7ten", "Soh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zu dem Herrn, dem sie sich eigen f\u00fchlet,", "tokens": ["Zu", "dem", "Herrn", ",", "dem", "sie", "sich", "ei\u00b7gen", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wandelt treulich Ruth, die Moabitin.", "tokens": ["wan\u00b7delt", "treu\u00b7lich", "Ruth", ",", "die", "Mo\u00b7a\u00b7bi\u00b7tin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und sie legt sich still zu seinen F\u00fcssen", "tokens": ["Und", "sie", "legt", "sich", "still", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "wartend nieder: ihre stumme Demuth,", "tokens": ["war\u00b7tend", "nie\u00b7der", ":", "ih\u00b7re", "stum\u00b7me", "De\u00b7muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "ihres hingegebnen Herzens Wonne", "tokens": ["ih\u00b7res", "hin\u00b7ge\u00b7geb\u00b7nen", "Her\u00b7zens", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "wacht und athmet in der Hut der Schatten.", "tokens": ["wacht", "und", "ath\u00b7met", "in", "der", "Hut", "der", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Als die Mitternacht herangekommen,", "tokens": ["Als", "die", "Mit\u00b7ter\u00b7nacht", "her\u00b7an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schrickt der Mann zusammen und erzittert.", "tokens": ["schrickt", "der", "Mann", "zu\u00b7sam\u00b7men", "und", "er\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Er erwacht und siehe, ihm zu F\u00fcssen", "tokens": ["Er", "er\u00b7wacht", "und", "sie\u00b7he", ",", "ihm", "zu", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVIMP", "$,", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "liegt ein junges Weib.", "tokens": ["liegt", "ein", "jun\u00b7ges", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wer bist du? fragt er.", "tokens": ["Wer", "bist", "du", "?", "fragt", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich bin Ruth, bin deine Magd und Dirne.", "tokens": ["Ich", "bin", "Ruth", ",", "bin", "dei\u00b7ne", "Magd", "und", "Dir\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gieb mir Ruhe! Breite deine Fl\u00fcgel", "tokens": ["Gieb", "mir", "Ru\u00b7he", "!", "Brei\u00b7te", "dei\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "$.", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00fcber mich! Ich bin zu dir gekommen,", "tokens": ["\u00fc\u00b7ber", "mich", "!", "Ich", "bin", "zu", "dir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "denn du bist der Herr, und will dir dienen.", "tokens": ["denn", "du", "bist", "der", "Herr", ",", "und", "will", "dir", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$,", "KON", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und ger\u00fchrt ob so viel armer Demuth,", "tokens": ["Und", "ge\u00b7r\u00fchrt", "ob", "so", "viel", "ar\u00b7mer", "De\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KOUS", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "hebt sie Boas auf und spricht voll Mitleid:", "tokens": ["hebt", "sie", "Boas", "auf", "und", "spricht", "voll", "Mit\u00b7leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PTKVZ", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Sei dem Herrn gesegnet, meine Tochter!", "tokens": ["Sei", "dem", "Herrn", "ge\u00b7seg\u00b7net", ",", "mei\u00b7ne", "Toch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was du geben kannst, hast du gegeben,", "tokens": ["Was", "du", "ge\u00b7ben", "kannst", ",", "hast", "du", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "was du giebst, ist gross wie deine Liebe,", "tokens": ["was", "du", "giebst", ",", "ist", "gross", "wie", "dei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und an mir hast du sie nicht vergeudet.", "tokens": ["und", "an", "mir", "hast", "du", "sie", "nicht", "ver\u00b7geu\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie du sagst, so soll es dir geschehen:", "tokens": ["Wie", "du", "sagst", ",", "so", "soll", "es", "dir", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meines Volkes ganze Stadt erfahre,", "tokens": ["Mei\u00b7nes", "Vol\u00b7kes", "gan\u00b7ze", "Stadt", "er\u00b7fah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "dass ich dich erw\u00e4hlt zu meinem Weibe.", "tokens": ["dass", "ich", "dich", "er\u00b7w\u00e4hlt", "zu", "mei\u00b7nem", "Wei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schlafe nun bis an den andern Morgen!", "tokens": ["Schla\u00b7fe", "nun", "bis", "an", "den", "an\u00b7dern", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Keines Menschen Seele soll erkunden,", "tokens": ["Kei\u00b7nes", "Men\u00b7schen", "See\u00b7le", "soll", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "dass ein Weib gekommen in die Tenne:", "tokens": ["dass", "ein", "Weib", "ge\u00b7kom\u00b7men", "in", "die", "Ten\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "morgen sollst du meine Gattin heissen ...", "tokens": ["mor\u00b7gen", "sollst", "du", "mei\u00b7ne", "Gat\u00b7tin", "heis\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.p