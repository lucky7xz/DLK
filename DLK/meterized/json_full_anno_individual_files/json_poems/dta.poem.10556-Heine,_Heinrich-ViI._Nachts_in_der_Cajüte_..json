{"dta.poem.10556": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Nachts in der Caj\u00fcte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "An die blaue Himmelsdecke,", "tokens": ["An", "die", "blau\u00b7e", "Him\u00b7mels\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die sch\u00f6nen Sterne blinken,", "tokens": ["Wo", "die", "sch\u00f6\u00b7nen", "Ster\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht' ich pressen meine Lippen,", "tokens": ["M\u00f6cht'", "ich", "pres\u00b7sen", "mei\u00b7ne", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pressen wild und st\u00fcrmisch weinen.", "tokens": ["Pres\u00b7sen", "wild", "und", "st\u00fcr\u00b7misch", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene Sterne sind die Augen", "tokens": ["Je\u00b7ne", "Ster\u00b7ne", "sind", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Liebsten, tausendf\u00e4ltig", "tokens": ["Mei\u00b7ner", "Liebs\u00b7ten", ",", "tau\u00b7send\u00b7f\u00e4l\u00b7tig"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schimmern sie und gr\u00fc\u00dfen freundlich,", "tokens": ["Schim\u00b7mern", "sie", "und", "gr\u00fc\u00b7\u00dfen", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der blauen Himmelsdecke.", "tokens": ["Aus", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach der blauen Himmelsdecke,", "tokens": ["Nach", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den Augen der Geliebten,", "tokens": ["Nach", "den", "Au\u00b7gen", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heb' ich andachtsvoll die Arme,", "tokens": ["Heb'", "ich", "an\u00b7dachts\u00b7voll", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und ich bete und ich flehe:", "tokens": ["Und", "ich", "be\u00b7te", "und", "ich", "fle\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Holde Augen, Gnadenlichter,", "tokens": ["Hol\u00b7de", "Au\u00b7gen", ",", "Gna\u00b7den\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O, beseligt meine Seele,", "tokens": ["O", ",", "be\u00b7se\u00b7ligt", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft mich sterben und erwerben", "tokens": ["La\u00dft", "mich", "ster\u00b7ben", "und", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch und Euren ganzen Himmel!", "tokens": ["Euch", "und", "Eu\u00b7ren", "gan\u00b7zen", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}