{"dta.poem.3133": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "178.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein wahres Wort: der K\u00fcnstler wird geboren;", "tokens": ["Es", "ist", "ein", "wah\u00b7res", "Wort", ":", "der", "K\u00fcnst\u00b7ler", "wird", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch jede Wahrheit wird Irrthum im Mund der Thoren.", "tokens": ["Doch", "je\u00b7de", "Wahr\u00b7heit", "wird", "Irr\u00b7thum", "im", "Mund", "der", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Geboren wird mit ihm der Kunsttrieb, nicht die Kunst;", "tokens": ["Ge\u00b7bo\u00b7ren", "wird", "mit", "ihm", "der", "Kunst\u00b7trieb", ",", "nicht", "die", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Bildung ist sein Werk, die Anlag' Himmelsgunst.", "tokens": ["Die", "Bil\u00b7dung", "ist", "sein", "Werk", ",", "die", "An\u00b7lag'", "Him\u00b7mels\u00b7gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geboren zur Vernunft, ist auch nicht gleich vern\u00fcnftig", "tokens": ["Ge\u00b7bo\u00b7ren", "zur", "Ver\u00b7nunft", ",", "ist", "auch", "nicht", "gleich", "ver\u00b7n\u00fcnf\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mensch, doch wenn er fein dazu thut, wird ers k\u00fcnftig.", "tokens": ["Der", "Mensch", ",", "doch", "wenn", "er", "fein", "da\u00b7zu", "thut", ",", "wird", "ers", "k\u00fcnf\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "KOUS", "PPER", "ADJD", "PAV", "VVFIN", "$,", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}