, "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Denn in der wimmelnden Schar der Kindheitsbilder als erstes", "tokens": ["Denn", "in", "der", "wim\u00b7meln\u00b7den", "Schar", "der", "Kind\u00b7heits\u00b7bil\u00b7der", "als", "ers\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "KOUS", "ADJA"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "F\u00fchrt die Erinnerung stets lustiges Taubenspiel auf,", "tokens": ["F\u00fchrt", "die", "E\u00b7rin\u00b7ne\u00b7rung", "stets", "lus\u00b7ti\u00b7ges", "Tau\u00b7ben\u00b7spiel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Stellet das Haus mir hin, den Garten, die Blumen, die V\u00f6gel,", "tokens": ["Stel\u00b7let", "das", "Haus", "mir", "hin", ",", "den", "Gar\u00b7ten", ",", "die", "Blu\u00b7men", ",", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Alles im d\u00e4mmernden Schein, wie es die Kindheit geliebt:", "tokens": ["Al\u00b7les", "im", "d\u00e4m\u00b7mern\u00b7den", "Schein", ",", "wie", "es", "die", "Kind\u00b7heit", "ge\u00b7liebt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Schneewei\u00df flattern die Kleinen noch auf, und schneewei\u00df die Alten", "tokens": ["Schnee\u00b7wei\u00df", "flat\u00b7tern", "die", "Klei\u00b7nen", "noch", "auf", ",", "und", "schnee\u00b7wei\u00df", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Durch die Raute herein, welche zum K\u00e4mmerchen f\u00fchrt,", "tokens": ["Durch", "die", "Rau\u00b7te", "her\u00b7ein", ",", "wel\u00b7che", "zum", "K\u00e4m\u00b7mer\u00b7chen", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "M\u00fctterchen streut noch das Korn, das B\u00fcblein klatscht in die H\u00e4nde;", "tokens": ["M\u00fct\u00b7ter\u00b7chen", "streut", "noch", "das", "Korn", ",", "das", "B\u00fcb\u00b7lein", "klatscht", "in", "die", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Auch das Nest im Kamin seh' ich wie damals noch heut.", "tokens": ["Auch", "das", "Nest", "im", "Ka\u00b7min", "seh'", "ich", "wie", "da\u00b7mals", "noch", "heut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "KOKOM", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochai