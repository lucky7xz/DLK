{"textgrid.poem.59577": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die Tauben", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundliche V\u00f6gel, o seid gegr\u00fc\u00dft mir, flatternde Tauben!", "tokens": ["Freund\u00b7li\u00b7che", "V\u00f6\u00b7gel", ",", "o", "seid", "ge\u00b7gr\u00fc\u00dft", "mir", ",", "flat\u00b7tern\u00b7de", "Tau\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "VAFIN", "VVPP", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Einst das Spiel meines Tags, jetzt das Spiel meiner Nacht,", "tokens": ["Einst", "das", "Spiel", "mei\u00b7nes", "Tags", ",", "jetzt", "das", "Spiel", "mei\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Seid mir gegr\u00fc\u00dft! Ihr bringt mir immer so liebe Gesellschaft,", "tokens": ["Seid", "mir", "ge\u00b7gr\u00fc\u00dft", "!", "Ihr", "bringt", "mir", "im\u00b7mer", "so", "lie\u00b7be", "Ge\u00b7sell\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "---+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wann mir der Schlaf aufs Bett schleicht mit dem seligen Traum,", "tokens": ["Wann", "mir", "der", "Schlaf", "aufs", "Bett", "schleicht", "mit", "dem", "se\u00b7li\u00b7gen", "Traum", ","], "toke