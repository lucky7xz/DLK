{"textgrid.poem.48266": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Admiral Herluf Trolles Begr\u00e4bnis", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbherluf Trolle fiel. Der Tod trat ihn an,", "tokens": ["\u00bb", "her\u00b7luf", "Trol\u00b7le", "fiel", ".", "Der", "Tod", "trat", "ihn", "an", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "An Bord, auf der H\u00f6he von Pommern,", "tokens": ["An", "Bord", ",", "auf", "der", "H\u00f6\u00b7he", "von", "Pom\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wir hatten keinen be\u00dfren Mann,", "tokens": ["Wir", "hat\u00b7ten", "kei\u00b7nen", "be\u00df\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Keinen Tapferern, keinen Frommern.\u00ab", "tokens": ["Kei\u00b7nen", "Tap\u00b7fe\u00b7rern", ",", "kei\u00b7nen", "From\u00b7mern", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und am dritten Tage, die Flagge halbmast,", "tokens": ["Und", "am", "drit\u00b7ten", "Ta\u00b7ge", ",", "die", "Flag\u00b7ge", "halb\u00b7mast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei Nestved, an Seelands K\u00fcste,", "tokens": ["Bei", "Nest\u00b7ved", ",", "an", "See\u00b7lands", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Landeten sie die geliebte Last \u2013", "tokens": ["Lan\u00b7de\u00b7ten", "sie", "die", "ge\u00b7lieb\u00b7te", "Last", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Der Tag ging eben zu R\u00fcste.", "tokens": ["Der", "Tag", "ging", "e\u00b7ben", "zu", "R\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Landeten ihren Admiral,", "tokens": ["Lan\u00b7de\u00b7ten", "ih\u00b7ren", "Ad\u00b7mi\u00b7ral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und in ein Bahrtuch geschlagen,", "tokens": ["Und", "in", "ein", "Bahr\u00b7tuch", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Haben sie, rastend ein einzig Mal,", "tokens": ["Ha\u00b7ben", "sie", ",", "ras\u00b7tend", "ein", "ein\u00b7zig", "Mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ihn bis Herlufsholm getragen.", "tokens": ["Ihn", "bis", "Her\u00b7lufs\u00b7holm", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Einen Boten sandten sie meldend voraus \u2013", "tokens": ["Ei\u00b7nen", "Bo\u00b7ten", "sand\u00b7ten", "sie", "mel\u00b7dend", "vo\u00b7raus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und als in den Schlo\u00dfhof sie schritten,", "tokens": ["Und", "als", "in", "den", "Schlo\u00df\u00b7hof", "sie", "schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Witwe stand vor dem Trauerhaus", "tokens": ["Die", "Wit\u00b7we", "stand", "vor", "dem", "Trau\u00b7er\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In ihrer Frauen Mitten.", "tokens": ["In", "ih\u00b7rer", "Frau\u00b7en", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Am Eingange stand sie, gr\u00fc\u00dfte den Zug,", "tokens": ["Am", "Ein\u00b7gan\u00b7ge", "stand", "sie", ",", "gr\u00fc\u00df\u00b7te", "den", "Zug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Aufrecht und ungebrochen.", "tokens": ["Auf\u00b7recht", "und", "un\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und der Erste (der das Bahrtuch trug)", "tokens": ["Und", "der", "Ers\u00b7te", "(", "der", "das", "Bahr\u00b7tuch", "trug", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$(", "ART", "ART", "NN", "VVFIN", "$("], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Trat vor und hat gesprochen:", "tokens": ["Trat", "vor", "und", "hat", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwas geschehen, wir sandten die Meldung dir,", "tokens": ["\u00bb", "was", "ge\u00b7sche\u00b7hen", ",", "wir", "sand\u00b7ten", "die", "Mel\u00b7dung", "dir", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Eh' den Weg wir selber gingen,", "tokens": ["Eh'", "den", "Weg", "wir", "sel\u00b7ber", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Seel' ist frei, seine H\u00fcll' ist hier,", "tokens": ["Sei\u00b7ne", "Seel'", "ist", "frei", ",", "sei\u00b7ne", "H\u00fcll'", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Du wei\u00dft, wen wir dir bringen.", "tokens": ["Du", "wei\u00dft", ",", "wen", "wir", "dir", "brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "An der pommerschen K\u00fcste, vor Pudagla-Golm,", "tokens": ["An", "der", "pom\u00b7mer\u00b7schen", "K\u00fcs\u00b7te", ",", "vor", "Pu\u00b7dagla\u00b7Golm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NE", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Um den schwankenden Sieg uns zu retten,", "tokens": ["Um", "den", "schwan\u00b7ken\u00b7den", "Sieg", "uns", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "So fiel er. Nun, Herrin von Herlufsholm,", "tokens": ["So", "fiel", "er", ".", "Nun", ",", "Her\u00b7rin", "von", "Her\u00b7lufs\u00b7holm", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "$,", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sage, wohin wir ihn betten.", "tokens": ["Sa\u00b7ge", ",", "wo\u00b7hin", "wir", "ihn", "bet\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Betten wir ihn in den Totensaal", "tokens": ["Bet\u00b7ten", "wir", "ihn", "in", "den", "To\u00b7ten\u00b7saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von Thorslund oder Olafskirche?", "tokens": ["Von", "Thor\u00b7slund", "o\u00b7der", "O\u00b7lafs\u00b7kir\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Betten wir ihn in Gjeddesdal", "tokens": ["Bet\u00b7ten", "wir", "ihn", "in", "Gjed\u00b7des\u00b7dal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Unter der Trauerbirke?", "tokens": ["Un\u00b7ter", "der", "Trau\u00b7er\u00b7bir\u00b7ke", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Betten wir ihn in die Kryptkapell'n,", "tokens": ["Bet\u00b7ten", "wir", "ihn", "in", "die", "Krypt\u00b7ka\u00b7pell'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In Roskilde, Leire, Ringstede?", "tokens": ["In", "Ros\u00b7kil\u00b7de", ",", "Lei\u00b7re", ",", "Rings\u00b7te\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sage, Herrin, wohin wir ihn stell'n,", "tokens": ["Sa\u00b7ge", ",", "Her\u00b7rin", ",", "wo\u00b7hin", "wir", "ihn", "stell'n", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Eine Ruhest\u00e4tt' f\u00fcr ihn hat jede.", "tokens": ["Ei\u00b7ne", "Ru\u00b7he\u00b7st\u00e4tt'", "f\u00fcr", "ihn", "hat", "je\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "PIAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Jeder Kirche gab er, um was sie bat,", "tokens": ["Je\u00b7der", "Kir\u00b7che", "gab", "er", ",", "um", "was", "sie", "bat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alt\u00e4re, T\u00fcrme, Glocken,", "tokens": ["Al\u00b7t\u00e4\u00b7re", ",", "T\u00fcr\u00b7me", ",", "Glo\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jede, wenn sie h\u00f6rt, \u203aer naht\u2039,", "tokens": ["Und", "je\u00b7de", ",", "wenn", "sie", "h\u00f6rt", ",", "\u203a", "er", "naht", "\u2039", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "$,", "KOUS", "PPER", "VVFIN", "$,", "$(", "PPER", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird in Leide frohlocken.", "tokens": ["Wird", "in", "Lei\u00b7de", "froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.11": {"line.1": {"text": "Eine jede ladet ihn zu sich ein", "tokens": ["Ei\u00b7ne", "je\u00b7de", "la\u00b7det", "ihn", "zu", "sich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "VVFIN", "PPER", "APPR", "PRF", "ART"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "In ihrer Pfeiler Schatten.\u00ab", "tokens": ["In", "ih\u00b7rer", "Pfei\u00b7ler", "Schat\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sprach seine Witwe: \u00bb", "tokens": ["Da", "sprach", "sei\u00b7ne", "Wit\u00b7we", ":", "\u00bb"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.12": {"line.1": {"text": "Wohl hat er hier keine Kirche gebaut", "tokens": ["Wohl", "hat", "er", "hier", "kei\u00b7ne", "Kir\u00b7che", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013 Die stand schon hundert Jahre \u2013", "tokens": ["\u2013", "Die", "stand", "schon", "hun\u00b7dert", "Jah\u00b7re", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier aber, als Herluf Trolles Braut", "tokens": ["Hier", "a\u00b7ber", ",", "als", "Her\u00b7luf", "Trol\u00b7les", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "NN", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stand ich mit ihm vorm Altare.", "tokens": ["Stand", "ich", "mit", "ihm", "vorm", "Al\u00b7ta\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Vor demselben Altar, auf selbem Stein", "tokens": ["Vor", "dem\u00b7sel\u00b7ben", "Al\u00b7tar", ",", "auf", "sel\u00b7bem", "Stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Steh' er wieder in aller Stille,", "tokens": ["Steh'", "er", "wie\u00b7der", "in", "al\u00b7ler", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nichts soll dabei gesprochen sein", "tokens": ["Nichts", "soll", "da\u00b7bei", "ge\u00b7spro\u00b7chen", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PAV", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als: Herr, es geschehe dein Wille.", "tokens": ["Als", ":", "Herr", ",", "es", "ge\u00b7sche\u00b7he", "dein", "Wil\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.14": {"line.1": {"text": "Morgen aber, eh' noch der Tag erstand,", "tokens": ["Mor\u00b7gen", "a\u00b7ber", ",", "eh'", "noch", "der", "Tag", "er\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "me