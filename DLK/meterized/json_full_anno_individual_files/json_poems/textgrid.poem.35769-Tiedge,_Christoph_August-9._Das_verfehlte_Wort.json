{"textgrid.poem.35769": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "9. Das verfehlte Wort", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ging zum Sonntagstanze!", "tokens": ["Sie", "ging", "zum", "Sonn\u00b7tags\u00b7tan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon klang Musikget\u00f6n!", "tokens": ["Schon", "klang", "Mu\u00b7sik\u00b7ge\u00b7t\u00f6n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sie, im gr\u00fcnen Kranze,", "tokens": ["Und", "sie", ",", "im", "gr\u00fc\u00b7nen", "Kran\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie war so wundersch\u00f6n!", "tokens": ["Sie", "war", "so", "wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Heut, dacht' ich, kannst du's wagen!", "tokens": ["Heut", ",", "dacht'", "ich", ",", "kannst", "du's", "wa\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du kannst ja mit ihr geh'n,", "tokens": ["Du", "kannst", "ja", "mit", "ihr", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr dies und jenes sagen,", "tokens": ["Ihr", "dies", "und", "je\u00b7nes", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "KON", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihr dein Herz gesteh'n.", "tokens": ["Und", "ihr", "dein", "Herz", "ge\u00b7steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich ging ihr nach; sie eilte", "tokens": ["Ich", "ging", "ihr", "nach", ";", "sie", "eil\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dahin am Lerchenhain;", "tokens": ["Da\u00b7hin", "am", "Ler\u00b7chen\u00b7hain", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wo der Weg sich teilte,", "tokens": ["Und", "wo", "der", "Weg", "sich", "teil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da holt' ich sie erst ein.", "tokens": ["Da", "holt'", "ich", "sie", "erst", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie fragte, was ich wollte;", "tokens": ["Sie", "frag\u00b7te", ",", "was", "ich", "woll\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ach, ich wu\u00dfte nicht,", "tokens": ["Und", "ach", ",", "ich", "wu\u00df\u00b7te", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich ihr sagen sollte!", "tokens": ["Was", "ich", "ihr", "sa\u00b7gen", "soll\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir brannte das Gesicht.", "tokens": ["Mir", "brann\u00b7te", "das", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und was ich endlich sagte \u2013", "tokens": ["Und", "was", "ich", "end\u00b7lich", "sag\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir war nicht wohl dabei \u2013", "tokens": ["Mir", "war", "nicht", "wohl", "da\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sagte nichts, und fragte,", "tokens": ["Ich", "sag\u00b7te", "nichts", ",", "und", "frag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob heute Sonntag sei!", "tokens": ["Ob", "heu\u00b7te", "Sonn\u00b7tag", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr f\u00e4rbten sich die Wangen;", "tokens": ["Ihr", "f\u00e4rb\u00b7ten", "sich", "die", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kaum wagt' ich, aufzuseh'n!", "tokens": ["Kaum", "wagt'", "ich", ",", "auf\u00b7zu\u00b7seh'n", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So blieb ich, ganz befangen,", "tokens": ["So", "blieb", "ich", ",", "ganz", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor ihren Blicken steh'n.", "tokens": ["Vor", "ih\u00b7ren", "Bli\u00b7cken", "steh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die h\u00e4tt' ich fliehen m\u00f6gen;", "tokens": ["Die", "h\u00e4tt'", "ich", "flie\u00b7hen", "m\u00f6\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn trieben sie mir nicht,", "tokens": ["Denn", "trie\u00b7ben", "sie", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob sie Wasser z\u00f6gen,", "tokens": ["Als", "ob", "sie", "Was\u00b7ser", "z\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Thr\u00e4nen ins Gesicht?", "tokens": ["Die", "Thr\u00e4\u00b7nen", "ins", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Kaum h\u00f6rt' ich, was ich h\u00f6rte.", "tokens": ["Kaum", "h\u00f6rt'", "ich", ",", "was", "ich", "h\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nein! Robert hat kein Gl\u00fcck!", "tokens": ["Nein", "!", "Ro\u00b7bert", "hat", "kein", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich nahm ein Herz, und kehrte", "tokens": ["Ich", "nahm", "ein", "Herz", ",", "und", "kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Besch\u00e4mt und still zur\u00fcck.", "tokens": ["Be\u00b7sch\u00e4mt", "und", "still", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Was ich ihr sagen wollte,", "tokens": ["Was", "ich", "ihr", "sa\u00b7gen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War wohl ein sch\u00f6nes Wort;", "tokens": ["War", "wohl", "ein", "sch\u00f6\u00b7nes", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als es gelten sollte,", "tokens": ["Und", "als", "es", "gel\u00b7ten", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da war's auf einmal fort.", "tokens": ["Da", "wa\u00b7r's", "auf", "ein\u00b7mal", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Wenn das so mit mir bliebe,", "tokens": ["Wenn", "das", "so", "mit", "mir", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dann w\u00fcrd' ich noch zum Tropf.", "tokens": ["Dann", "w\u00fcrd'", "ich", "noch", "zum", "Tropf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, glaubt es nur! die Liebe", "tokens": ["Ach", ",", "glaubt", "es", "nur", "!", "die", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verwirrt den kl\u00fcgsten Kopf.", "tokens": ["Ver\u00b7wirrt", "den", "kl\u00fcgs\u00b7ten", "Kopf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie ging zum Sonntagstanze!", "tokens": ["Sie", "ging", "zum", "Sonn\u00b7tags\u00b7tan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon klang Musikget\u00f6n!", "tokens": ["Schon", "klang", "Mu\u00b7sik\u00b7ge\u00b7t\u00f6n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sie, im gr\u00fcnen Kranze,", "tokens": ["Und", "sie", ",", "im", "gr\u00fc\u00b7nen", "Kran\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie war so wundersch\u00f6n!", "tokens": ["Sie", "war", "so", "wun\u00b7der\u00b7sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Heut, dacht' ich, kannst du's wagen!", "tokens": ["Heut", ",", "dacht'", "ich", ",", "kannst", "du's", "wa\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du kannst ja mit ihr geh'n,", "tokens": ["Du", "kannst", "ja", "mit", "ihr", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr dies und jenes sagen,", "tokens": ["Ihr", "dies", "und", "je\u00b7nes", "sa\u00b7gen