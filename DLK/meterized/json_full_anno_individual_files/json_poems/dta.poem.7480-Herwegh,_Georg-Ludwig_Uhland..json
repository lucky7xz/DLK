{"dta.poem.7480": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ludwig Uhland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur selten noch, fast graut's mir, es zu sagen,", "tokens": ["Nur", "sel\u00b7ten", "noch", ",", "fast", "graut's", "mir", ",", "es", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nehm' ich der Freiheit Evangelium,", "tokens": ["Nehm'", "ich", "der", "Frei\u00b7heit", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Schatz von Minne und von Ritterthum", "tokens": ["Den", "Schatz", "von", "Min\u00b7ne", "und", "von", "Rit\u00b7ter\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur Hand in unsern hartbedr\u00e4ngten Tagen.", "tokens": ["Zur", "Hand", "in", "un\u00b7sern", "hart\u00b7be\u00b7dr\u00e4ng\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie hab' ich einst so hei\u00df daf\u00fcr geschlagen!", "tokens": ["Wie", "hab'", "ich", "einst", "so", "hei\u00df", "da\u00b7f\u00fcr", "ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hastig dreht' ich Blatt um Blatt herum!", "tokens": ["Wie", "has\u00b7tig", "dreht'", "ich", "Blatt", "um", "Blatt", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann nicht mehr \u2014 ich kann nicht \u2014 sei es drum!", "tokens": ["Ich", "kann", "nicht", "mehr", "ich", "kann", "nicht", "sei", "es", "drum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$(", "PPER", "VMFIN", "PTKNEG", "$(", "VAFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Es soll doch Niemand mich zu schelten wagen.", "tokens": ["Es", "soll", "doch", "Nie\u00b7mand", "mich", "zu", "schel\u00b7ten", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein ander Hassen und ein ander Lieben", "tokens": ["Ein", "an\u00b7der", "Has\u00b7sen", "und", "ein", "an\u00b7der", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist in die Welt gekommen, und von allen", "tokens": ["Ist", "in", "die", "Welt", "ge\u00b7kom\u00b7men", ",", "und", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,", "KON", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind wenig Herzen nur sich ", "tokens": ["Sind", "we\u00b7nig", "Her\u00b7zen", "nur", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "PRF"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "So sind auch ", "tokens": ["So", "sind", "auch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ein einziges steht fest in mir geschrieben;", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", "steht", "fest", "in", "mir", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Kennst Du das Lied: \u201eWeh Euch, Ihr stolzen Hallen!\u201c", "tokens": ["Kennst", "Du", "das", "Lied", ":", "\u201e", "Weh", "Euch", ",", "Ihr", "stol\u00b7zen", "Hal\u00b7len", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$(", "NN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}