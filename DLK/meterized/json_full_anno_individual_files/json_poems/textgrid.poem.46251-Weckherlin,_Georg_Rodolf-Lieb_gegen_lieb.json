{"textgrid.poem.46251": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Lieb gegen lieb", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Demnach mich Amor selbs numehr ein lange zeit", "tokens": ["Dem\u00b7nach", "mich", "A\u00b7mor", "selbs", "nu\u00b7mehr", "ein", "lan\u00b7ge", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PRF", "NE", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gez\u00fcchtiget und recht zu kriegen underrichtet,", "tokens": ["ge\u00b7z\u00fcch\u00b7ti\u00b7get", "und", "recht", "zu", "krie\u00b7gen", "un\u00b7der\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hat endlich sich mein mut, mein lang erw\u00fcnschte beut", "tokens": ["hat", "end\u00b7lich", "sich", "mein", "mut", ",", "mein", "lang", "er\u00b7w\u00fcnschte", "beut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PRF", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "VVFIN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "oder den sch\u00f6nsten tod zu erwerben, verpflichtet.", "tokens": ["o\u00b7der", "den", "sch\u00f6ns\u00b7ten", "tod", "zu", "er\u00b7wer\u00b7ben", ",", "ver\u00b7pflich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Darum als in dem feld 