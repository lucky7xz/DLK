{"textgrid.poem.33728": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Obwohl erdr\u00fcckt beinah vom Seelenschmerze,", "tokens": ["Ob\u00b7wohl", "er\u00b7dr\u00fcckt", "bei\u00b7nah", "vom", "See\u00b7len\u00b7schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Obwohl allein und auf dem schlimmsten Pfade,", "tokens": ["Ob\u00b7wohl", "al\u00b7lein", "und", "auf", "dem", "schlimms\u00b7ten", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sucht' ich nicht bei Menschenherzen Gnade,", "tokens": ["Doch", "sucht'", "ich", "nicht", "bei", "Men\u00b7schen\u00b7her\u00b7zen", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich wu\u00dfte wohl, ich schl\u00fcge nur an Erze.", "tokens": ["Ich", "wu\u00df\u00b7te", "wohl", ",", "ich", "schl\u00fc\u00b7ge", "nur", "an", "Er\u00b7ze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6hnte meines Grams, ich schwang im Scherze", "tokens": ["Ich", "h\u00f6hn\u00b7te", "mei\u00b7nes", "Grams", ",", "ich", "schwang", "im", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das volle Glas und pries auch alles Fade,", "tokens": ["Das", "vol\u00b7le", "Glas", "und", "pries", "auch", "al\u00b7les", "Fa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Mancher sprach: \u00bbDies Licht brennt sch\u00f6n gerade,\u00ab", "tokens": ["Und", "Man\u00b7cher", "sprach", ":", "\u00bb", "Dies", "Licht", "brennt", "sch\u00f6n", "ge\u00b7ra\u00b7de", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "$(", "PDS", "NN", "VVFIN", "ADJD", "ADV", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch war's nur das Licht der Leichenkerze. \u2013", "tokens": ["Und", "doch", "wa\u00b7r's", "nur", "das", "Licht", "der", "Lei\u00b7chen\u00b7ker\u00b7ze", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Das Wort des Lebens schlie\u00dft mit tr\u00fcber Endung;", "tokens": ["Das", "Wort", "des", "Le\u00b7bens", "schlie\u00dft", "mit", "tr\u00fc\u00b7ber", "En\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum schwarzen Stein inmitten e