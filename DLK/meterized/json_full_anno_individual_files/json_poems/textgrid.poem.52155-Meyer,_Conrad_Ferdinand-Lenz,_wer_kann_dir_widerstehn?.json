, "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder ein verlornes Singen \u2013", "tokens": ["O\u00b7der", "ein", "ver\u00b7lor\u00b7nes", "Sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lenz, wer kann dir widerstehn?", "tokens": ["Lenz", ",", "wer", "kann", "dir", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch das Wiesengr\u00fcn, das linde,", "tokens": ["Durch", "das", "Wie\u00b7sen\u00b7gr\u00fcn", ",", "das", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandr ich mit dem eignen Kinde", "tokens": ["Wandr", "ich", "mit", "dem", "eig\u00b7nen", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es kann an Murmelb\u00e4chen", "tokens": ["Und", "es", "kann", "an", "Mur\u00b7mel\u00b7b\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht mit stummen Lippen gehn \u2013", "tokens": ["Nicht", "mit", "stum\u00b7men", "Lip\u00b7pen", "gehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die Knospen alle brechen,", "tokens": ["Wann", "die", "Knos\u00b7pen", "al\u00b7le", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollen Lippen sich entfalten,", "tokens": ["Wol\u00b7len", "Lip\u00b7pen", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf den jungen, auf den alten,", "tokens": ["Auf", "den", "jun\u00b7gen", ",", "auf", "den", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ein kleines Lied entstehn.", "tokens": ["Will", "ein", "klei\u00b7nes", "Lied", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieb und Lust und Leben saugen", "tokens": ["Lieb", "und", "Lust", "und", "Le\u00b7ben", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich aus den Kinderaugen,", "tokens": ["Will", "ich", "aus", "den", "Kin\u00b7der\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Blicke meiner Kleinen", "tokens": ["In", "dem", "Bli\u00b7cke", "mei\u00b7ner", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich nach dem Himmel sp\u00e4hn,", "tokens": ["Will", "ich", "nach", "dem", "Him\u00b7mel", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, es ist das gleiche Scheinen,", "tokens": ["Ja", ",", "es", "ist", "das", "glei\u00b7che", "Schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier im Blauen, dort im Blauen,", "tokens": ["Hier", "im", "Blau\u00b7en", ",", "dort", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und das selbige Vertrauen \u2013", "tokens": ["Und", "das", "sel\u00b7bi\u00b7ge", "Ver\u00b7trau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lenz, wer kann dir widerstehn?", "tokens": ["Lenz", ",", "wer", "kann", "dir", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kuckuck ruft! willst du erfahren", "tokens": ["Ku\u00b7ckuck", "ruft", "!", "willst", "du", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Jahre, gl\u00e4ub'ge Seele?", "tokens": ["Dei\u00b7ne", "Jah\u00b7re", ",", "gl\u00e4ub'\u00b7ge", "See\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kuckuck ruft im Walde, z\u00e4hle!", "tokens": ["Ku\u00b7ckuck", "ruft", "im", "Wal\u00b7de", ",", "z\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neun und zehn und mehr als zehn...", "tokens": ["Neun", "und", "zehn", "und", "mehr", "als", "zehn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "KON", "PIAT", "KOKOM", "CARD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, das will ja gar nicht enden,", "tokens": ["Ei", ",", "das", "will", "ja", "gar", "nicht", "en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fchling schenkt aus vollen H\u00e4nden \u2013", "tokens": ["Fr\u00fch\u00b7ling", "schenkt", "aus", "vol\u00b7len", "H\u00e4n\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll auf diesen blonden Haaren", "tokens": ["Soll", "auf", "die\u00b7sen", "blon\u00b7den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch den Myrtenkranz ich sehn?...", "tokens": ["Noch", "den"