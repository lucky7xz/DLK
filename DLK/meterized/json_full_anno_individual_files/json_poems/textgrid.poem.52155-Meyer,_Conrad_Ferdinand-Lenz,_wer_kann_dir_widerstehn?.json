, ",", "gl\u00e4ub'\u00b7ge", "See\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kuckuck ruft im Walde, z\u00e4hle!", "tokens": ["Ku\u00b7ckuck", "ruft", "im", "Wal\u00b7de", ",", "z\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neun und zehn und mehr als zehn...", "tokens": ["Neun", "und", "zehn", "und", "mehr", "als", "zehn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "KON", "PIAT", "KOKOM", "CARD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, das will ja gar nicht enden,", "tokens": ["Ei", ",", "das", "will", "ja", "gar", "nicht", "en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fchling schenkt aus vollen H\u00e4nden \u2013", "tokens": ["Fr\u00fch\u00b7ling", "schenkt", "aus", "vol\u00b7len", "H\u00e4n\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll auf diesen blonden Haaren", "tokens": ["Soll", "auf", "die\u00b7sen", "blon\u00b7den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch den Myrtenkranz ich sehn?...", "tokens": ["Noch", "den", "Myr\u00b7ten\u00b7kranz", "ich", "sehn", "?", "..."], "tok