."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Glentzt sie gleich nicht mit theuren Sachen,", "tokens": ["Glentzt", "sie", "gleich", "nicht", "mit", "theu\u00b7ren", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gl\u00e4ntzt doch ihrer Augen Liecht:", "tokens": ["So", "gl\u00e4ntzt", "doch", "ih\u00b7rer", "Au\u00b7gen", "Liecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar viel mu\u00df Hoffart sch\u00f6ne machen,", "tokens": ["Gar", "viel", "mu\u00df", "Hof\u00b7fart", "sch\u00f6\u00b7ne", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr schlechter Schein betreugt micht nicht.", "tokens": ["Ihr", "schlech\u00b7ter", "Schein", "be\u00b7treugt", "micht", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Jeder lobe seinen Sinn,", "tokens": ["Ein", "Je\u00b7der", "lo\u00b7be", "sei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe meine Sch\u00e4fferinn.", "tokens": ["Ich", "lie\u00b7be", "mei\u00b7ne", "Sch\u00e4f\u00b7fe\u00b7rinn", "."], "