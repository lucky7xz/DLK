 "tokens": ["Ich", "lie\u00b7be", "mei\u00b7ne", "Sch\u00e4f\u00b7fe\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein hohes Schlo\u00df wird von den Schl\u00e4gen", "tokens": ["Ein", "ho\u00b7hes", "Schlo\u00df", "wird", "von", "den", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De\u00df starcken Donners mehr ber\u00fchrt;", "tokens": ["De\u00df", "star\u00b7cken", "Don\u00b7ners", "mehr", "be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer weit wil, fellt offt au\u00df den Wegen", "tokens": ["Wer", "weit", "wil", ",", "fellt", "offt", "au\u00df", "den", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VMFIN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird durch seinen Stoltz verf\u00fchrt.", "tokens": ["Und", "wird", "durch", "sei\u00b7nen", "Stoltz", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Jeder lobe seinen Sinn,", "tokens": ["Ein", "Je\u00b7der", "lo\u00b7be", "sei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe meine Sch\u00e4fferinn.", "tokens": ["Ich", "lie\u00b7be", "mei\u00b7ne", "Sch\u00e4f\u00b7fe\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auff grosser See sind grosse Wellen,", "tokens": ["Auff", "gros\u00b7ser", "See", "sind", "gros\u00b7se", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Klippen, Sturm und harter Wind;", "tokens": ["Viel", "Klip\u00b7pen", ",", "Sturm", "und", "har\u00b7ter", "Wind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer klug ist, bleibet bey den Quellen,", "tokens": ["Wer", "klug", "ist", ",", "blei\u00b7bet", "bey", "den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die in den gr\u00fcnen W\u00e4ldern sind.", "tokens": ["Die", "in", "den", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7dern", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Jeder lobe seinen Sinn,", "tokens": ["Ein", "Je\u00b7der", "lo\u00b7be", "sei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe meine Sch\u00e4fferinn.", "tokens": ["Ich", "lie\u00b7be",