{"dta.poem.16080": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "134.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt versprach dir nichts, mach' ihrs nicht zum Verbrechen,", "tokens": ["Die", "Welt", "ver\u00b7sprach", "dir", "nichts", ",", "mach'", "ihrs", "nicht", "zum", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$,", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du mu\u00dft dir selber nicht zuviel von ihr versprechen.", "tokens": ["Du", "mu\u00dft", "dir", "sel\u00b7ber", "nicht", "zu\u00b7viel", "von", "ihr", "ver\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum bel\u00fcgst du dich, sie habe dich belogen?", "tokens": ["Wa\u00b7rum", "be\u00b7l\u00fcgst", "du", "dich", ",", "sie", "ha\u00b7be", "dich", "be\u00b7lo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An ihr betrogst du dich, sie hat dich nicht betrogen.", "tokens": ["An", "ihr", "be\u00b7trogst", "du", "dich", ",", "sie", "hat", "dich", "nicht", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word",