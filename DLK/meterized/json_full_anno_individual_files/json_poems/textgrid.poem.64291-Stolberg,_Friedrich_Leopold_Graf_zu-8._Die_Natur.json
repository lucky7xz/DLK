u", "Va\u00b7ter", "al\u00b7ler", "!", "...", "Wir\u00b7bel", "und", "Stolz", "er\u00b7griff"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PIAT", "$.", "$(", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Mann von Staub, da\u00df er des Staubes", "tokens": ["Den", "Mann", "von", "Staub", ",", "da\u00df", "er", "des", "Stau\u00b7bes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spottete, den er beweinen sollte.", "tokens": ["Spot\u00b7te\u00b7te", ",", "den", "er", "be\u00b7wei\u00b7nen", "soll\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "O, sei gesegnet, Thr\u00e4ne der Neue, mir!", "tokens": ["O", ",", "sei", "ge\u00b7seg\u00b7net", ",", "Thr\u00e4\u00b7ne", "der", "Neu\u00b7e", ",", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "VVPP", "$,", "NN", "ART", "ADJA", "$,", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mehr noch, des Mitleids Thr\u00e4ne, gesegnet du!", "tokens": ["Mehr", "noch", ",", "des", "Mit\u00b7leids", "Thr\u00e4\u00b7ne", ",", "ge\u00b7seg\u00b7net", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun werden, wie nach Fr\u00fchlingsregen,", "tokens": ["Nun", "wer\u00b7den", ",", "wie", "nach", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Traulich die Blumen der Au mir l\u00e4cheln!", "tokens": ["Trau\u00b7lich", "die", "Blu\u00b7men", "der", "Au", "mir", "l\u00e4\u00b7cheln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Nur reinen Herzen duftet der Abendtau", "tokens": ["Nur", "rei\u00b7nen", "Her\u00b7zen", "duf\u00b7tet", "der", "A\u00b7bend\u00b7tau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der bunten Lenzflur! Heilig nur ihnen sind", "tokens": ["Der", "bun\u00b7ten", "Lenz\u00b7flur", "!", "Hei\u00b7lig", "nur", "ih\u00b7nen", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJA", "ADV", "PPER", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Eiche Schatten! Deine Segen,", "tokens": ["Der", "Ei\u00b7che", "Schat\u00b7ten", "!", "Dei\u00b7ne", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einsamkeit, k\u00f6nnen nur sie ertragen!", "tokens": ["Ein\u00b7sam\u00b7keit", ",", "k\u00f6n\u00b7nen", "nur", "sie", "er\u00b7tra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Woll'st oft, o sanfte Mutter der Weisheit, mich", "tokens": ["Woll'st", "oft", ",", "o", "sanf\u00b7te", "Mut\u00b7ter", "der", "Weis\u00b7heit", ",", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf ernste Pfade leiten im Mondenschein,", "tokens": ["Auf", "erns\u00b7te", "Pfa\u00b7de", "lei\u00b7ten", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo nur der Denker tiefe Wahrheit", "tokens": ["Wo", "nur", "der", "Den\u00b7ker", "tie\u00b7fe", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "+ Sch\u00f6pfet, und, gl\u00fchender Stirne, wallet!", "tokens": ["+", "Sch\u00f6p\u00b7fet", ",", "und", ",", "gl\u00fc\u00b7hen\u00b7der", "Stir\u00b7ne", ",", "wal\u00b7let", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Dann werden oft sich hohe Betrachtungen", "tokens": ["Dann", "wer\u00b7den", "oft", "sich", "ho\u00b7he", "Be\u00b7trach\u00b7tun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PRF", "ADJA", "NN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Harmonien wandeln! Begeisterung", "tokens": ["In", 