{"textgrid.poem.64291": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "8. Die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er sei mein Freund nicht, welcher die g\u00f6ttliche", "tokens": ["Er", "sei", "mein", "Freund", "nicht", ",", "wel\u00b7cher", "die", "g\u00f6tt\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Natur nicht liebet! Engelgef\u00fchle sind", "tokens": ["Na\u00b7tur", "nicht", "lie\u00b7bet", "!", "En\u00b7gel\u00b7ge\u00b7f\u00fch\u00b7le", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "VVFIN", "$.", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihm nicht bekannt! Er kann mit Inbrunst", "tokens": ["Ihm", "nicht", "be\u00b7kannt", "!", "Er", "kann", "mit", "In\u00b7brunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ADJD", "$.", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Freunde nicht! Kinder nicht! Weib nicht lieben!", "tokens": ["Freun\u00b7de", "nicht", "!", "Kin\u00b7der", "nicht", "!", "Weib", "nicht", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "NN", "PTKNEG", "$.", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ihm bebte nie von trunkner Begeisterung", "tokens": ["Ihm", "beb\u00b7te", "nie", "von", "trunk\u00b7ner", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die stumme Lippe! Schauer begegneten,", "tokens": ["Die", "stum\u00b7me", "Lip\u00b7pe", "!", "Schau\u00b7er", "be\u00b7ge\u00b7gne\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In hoher Wallung, seiner Seele", "tokens": ["In", "ho\u00b7her", "Wal\u00b7lung", ",", "sei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie mit der steigenden Morgensonne!", "tokens": ["Nie", "mit", "der", "stei\u00b7gen\u00b7den", "Mor\u00b7gen\u00b7son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "In deinen Wonnebecher, Allg\u00fctiger,", "tokens": ["In", "dei\u00b7nen", "Won\u00b7ne\u00b7be\u00b7cher", ",", "All\u00b7g\u00fc\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entfielen niemals Thr\u00e4nen dem Dankenden!", "tokens": ["Ent\u00b7fie\u00b7len", "nie\u00b7mals", "Thr\u00e4\u00b7nen", "dem", "Dan\u00b7ken\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sein Erb' ist Taumel, oder Schlafsucht;", "tokens": ["Sein", "Erb'", "ist", "Tau\u00b7mel", ",", "o\u00b7der", "Schlaf\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wehmut und Wonne des Weisen Erbe!", "tokens": ["Weh\u00b7mut", "und", "Won\u00b7ne", "des", "Wei\u00b7sen", "Er\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Er ist kein Sohn der Freiheit! Das Vaterland", "tokens": ["Er", "ist", "kein", "Sohn", "der", "Frei\u00b7heit", "!", "Das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist Spreu dem Feigen! \u2013 Sklave! Dich freite nicht", "tokens": ["Ist", "Spreu", "dem", "Fei\u00b7gen", "!", "\u2013", "Skla\u00b7ve", "!", "Dich", "frei\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "ART", "NN", "$.", "$(", "NN", "$.", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die R\u00f6merschlacht! \u2013 Zu meinen F\u00fc\u00dfen", "tokens": ["Die", "R\u00f6\u00b7mer\u00b7schlacht", "!", "\u2013", "Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kr\u00fcmme dich, Raupe, da\u00df dein ich spotte! ...", "tokens": ["Kr\u00fcm\u00b7me", "dich", ",", "Rau\u00b7pe", ",", "da\u00df", "dein", "ich", "spot\u00b7te", "!", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "KOUS", "PPOSAT", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Ich seiner spotten? \u2013 Weh mir! O, z\u00fcrne nicht,", "tokens": ["Ich", "sei\u00b7ner", "spot\u00b7ten", "?", "\u2013", "Weh", "mir", "!", "O", ",", "z\u00fcr\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "VVFIN", "$.", "$(", "NN", "PPER", "$.", "NE", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du Vater aller! ... Wirbel und Stolz ergriff", "tokens": ["Du", "Va\u00b7ter", "al\u00b7ler", "!", "...", "Wir\u00b7bel", "und", "Stolz", "er\u00b7griff"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PIAT", "$.", "$(", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Mann von Staub, da\u00df er des Staubes", "tokens": ["Den", "Mann", "von", "Staub", ",", "da\u00df", "er", "des", "Stau\u00b7bes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spottete, den er beweinen sollte.", "tokens": ["Spot\u00b7te\u00b7te", ",", "den", "er", "be\u00b7wei\u00b7nen", "soll\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "O, sei gesegnet, Thr\u00e4ne der Neue, mir!", "tokens": ["O", ",", "sei", "ge\u00b7seg\u00b7net", ",", "Thr\u00e4\u00b7ne", "der", "Neu\u00b7e", ",", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "VVPP", "$,", "NN", "ART", "ADJA", "$,", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mehr noch, des Mitleids Thr\u00e4ne, gesegnet du!", "tokens": ["Mehr", "noch", ",", "des", "Mit\u00b7leids", "Thr\u00e4\u00b7ne", ",", "ge\u00b7seg\u00b7net", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun werden, wie nach Fr\u00fchlingsregen,", "tokens": ["Nun", "wer\u00b7den", ",", "wie", "nach", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Traulich die Blumen der Au mir l\u00e4cheln!", "tokens": ["Trau\u00b7lich", "die", "Blu\u00b7men", "der", "Au", "mir", "l\u00e4\u00b7cheln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Nur reinen Herzen duftet der Abendtau", "tokens": ["Nur", "rei\u00b7nen", "Her\u00b7zen", "duf\u00b7tet", "der", "A\u00b7bend\u00b7tau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der bunten Lenzflur! Heilig nur ihnen sind", "tokens": ["Der", "bun\u00b7ten", "Lenz\u00b7flur", "!", "Hei\u00b7lig", "nur", "ih\u00b7nen", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJA", "ADV", "PPER", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Eiche Schatten! Deine Segen,", "tokens": ["Der", "Ei\u00b7che", "Schat\u00b7ten", "!", "Dei\u00b7ne", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einsamkeit, k\u00f6nnen nur sie ertragen!", "tokens": ["Ein\u00b7sam\u00b7keit", ",", "k\u00f6n\u00b7nen", "nur", "sie", "er\u00b7tra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Woll'st oft, o sanfte Mutter der Weisheit, mich", "tokens": ["Woll'st", "oft", ",", "o", "sanf\u00b7te", "Mut\u00b7ter", "der", "Weis\u00b7heit", ",", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf ernste Pfade leiten im Mondenschein,", "tokens": ["Auf", "erns\u00b7te", "Pfa\u00b7de", "lei\u00b7ten", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo nur der Denker tiefe Wahrheit", "tokens": ["Wo", "nur", "der", "Den\u00b7ker", "tie\u00b7fe", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "+ Sch\u00f6pfet, und, gl\u00fchender Stirne, wallet!", "tokens": ["+", "Sch\u00f6p\u00b7fet", ",", "und", ",", "gl\u00fc\u00b7hen\u00b7der", "Stir\u00b7ne", ",", "wal\u00b7let", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Dann werden oft sich hohe Betrachtungen", "tokens": ["Dann", "wer\u00b7den", "oft", "sich", "ho\u00b7he", "Be\u00b7trach\u00b7tun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PRF", "ADJA", "NN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Harmonien wandeln! Begeisterung", "tokens": ["In", "Har\u00b7mo\u00b7ni\u00b7en", "wan\u00b7deln", "!", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "VVFIN", "$.", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wird mich erf\u00fcllen, da\u00df die Thale", "tokens": ["Wird", "mich", "er\u00b7f\u00fcl\u00b7len", ",", "da\u00df", "die", "Tha\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hallen mein Lied und die Felseng\u00e4nge!", "tokens": ["Hal\u00b7len", "mein", "Lied", "und", "die", "Fel\u00b7sen\u00b7g\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Wenn du mich f\u00fcrder leitest, Natur, so soll", "tokens": ["Wenn", "du", "mich", "f\u00fcr\u00b7der", "lei\u00b7test", ",", "Na\u00b7tur", ",", "so", "soll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,", "NN", "$,", "ADV", "VMFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Lied dir jauchzen, weil ich ein J\u00fcngling bin;", "tokens": ["Mein", "Lied", "dir", "jauch\u00b7zen", ",", "weil", "ich", "ein", "J\u00fcng\u00b7ling", "bin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es soll dich feiern, wenn mit Silber", "tokens": ["Es", "soll", "dich", "fei\u00b7ern", ",", "wenn", "mit", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fcrzere Locken die Scheitel schm\u00fccken!", "tokens": ["K\u00fcr\u00b7ze\u00b7re", "Lo\u00b7cken", "die", "Schei\u00b7tel", "schm\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Er sei mein Freund nicht, welcher die g\u00f6ttliche", "tokens": ["Er", "sei", "mein", "Freund", "nicht", ",", "wel\u00b7cher", "die", "g\u00f6tt\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Natur nicht liebet! Engelgef\u00fchle sind", "tokens": ["Na\u00b7tur", "nicht", "lie\u00b7bet", "!", "En\u00b7gel\u00b7ge\u00b7f\u00fch\u00b7le", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "VVFIN", "$.", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihm nicht bekannt! Er kann mit Inbrunst", "tokens": ["Ihm", "nicht", "be\u00b7kannt", "!", "Er", "kann", "mit", "In\u00b7brunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ADJD", "$.", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Freunde nicht! Kinder nicht! Weib nicht lieben!", "tokens": ["Freun\u00b7de", "nicht", "!", "Kin\u00b7der", "nicht", "!", "Weib", "nicht", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "NN", "PTKNEG", "$.", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Ihm bebte nie von trunkner Begeisterung", "tokens": ["Ihm", "beb\u00b7te", "nie", "von", "trunk\u00b7ner", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die stumme Lippe! Schauer begegneten,", "tokens": ["Die", "stum\u00b7me", "Lip\u00b7pe", "!", "Schau\u00b7er", "be\u00b7ge\u00b7gne\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In hoher Wallung, seiner Seele", "tokens": ["In", "ho\u00b7her", "Wal\u00b7lung", ",", "sei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie mit der steigenden Morgensonne!", "tokens": ["Nie", "mit", "der", "stei\u00b7gen\u00b7den", "Mor\u00b7gen\u00b7son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "In deinen Wonnebecher, Allg\u00fctiger,", "tokens": ["In", "dei\u00b7nen", "Won\u00b7ne\u00b7be\u00b7cher", ",", "All\u00b7g\u00fc\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entfielen niemals Thr\u00e4nen dem Dankenden!", "tokens": ["Ent\u00b7fie\u00b7len", "nie\u00b7mals", "Thr\u00e4\u00b7nen", "dem", "Dan\u00b7ken\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sein Erb' ist Taumel, oder Schlafsucht;", "tokens": ["Sein", "Erb'", "ist", "Tau\u00b7mel", ",", "o\u00b7der", "Schlaf\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wehmut und Wonne des Weisen Erbe!", "tokens": ["Weh\u00b7mut", "und", "Won\u00b7ne", "des", "Wei\u00b7sen", "Er\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Er ist kein Sohn der Freiheit! Das Vaterland", "tokens": ["Er", "ist", "kein", "Sohn", "der", "Frei\u00b7heit", "!", "Das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "po