{"textgrid.poem.24803": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Zu einer Totenfeier f\u00fcr Arnold B\u00f6cklin", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun schweig, Musik! Nun ist die Szene mein,", "tokens": ["Nun", "schweig", ",", "Mu\u00b7sik", "!", "Nun", "ist", "die", "Sze\u00b7ne", "mein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ich will klagen, denn mir steht es zu!", "tokens": ["Und", "ich", "will", "kla\u00b7gen", ",", "denn", "mir", "steht", "es", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von dieser Zeiten Jugend flie\u00dft der Saft", "tokens": ["Von", "die\u00b7ser", "Zei\u00b7ten", "Ju\u00b7gend", "flie\u00dft", "der", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In mir; und er, des Standbild auf mich blickt,", "tokens": ["In", "mir", ";", "und", "er", ",", "des", "Stand\u00b7bild", "auf", "mich", "blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "KON", "PPER", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "War meiner Seele so geliebter Freund!", "tokens": ["War", "mei\u00b7ner", "See\u00b7le", "so", "ge\u00b7lieb\u00b7ter", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und dieses Guten hab ich sehr bedurft,", "tokens": ["Und", "die\u00b7ses", "Gu\u00b7ten", "hab", "ich", "sehr", "be\u00b7durft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn Finsternis ist viel in dieser Zeit,", "tokens": ["Denn", "Fins\u00b7ter\u00b7nis", "ist", "viel", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wie der Schwan, ein selig schwimmend Tier,", "tokens": ["Und", "wie", "der", "Schwan", ",", "ein", "se\u00b7lig", "schwim\u00b7mend", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aus der Najade triefend wei\u00dfen H\u00e4nden", "tokens": ["Aus", "der", "Na\u00b7ja\u00b7de", "trie\u00b7fend", "wei\u00b7\u00dfen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sich seine Nahrung k\u00fc\u00dft, so bog ich mich", "tokens": ["Sich", "sei\u00b7ne", "Nah\u00b7rung", "k\u00fc\u00dft", ",", "so", "bog", "ich", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In dunklen Stunden \u00fcber seine H\u00e4nde", "tokens": ["In", "dunk\u00b7len", "Stun\u00b7den", "\u00fc\u00b7ber", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Um meiner Seele Nahrung: tiefen Traum.", "tokens": ["Um", "mei\u00b7ner", "See\u00b7le", "Nah\u00b7rung", ":", "tie\u00b7fen", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Schm\u00fcck ich dein Bild mit Zweig und Bl\u00fcten nur?", "tokens": ["Schm\u00fcck", "ich", "dein", "Bild", "mit", "Zweig", "und", "Bl\u00fc\u00b7ten", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und du hast mir das Bild der Welt geschm\u00fcckt", "tokens": ["Und", "du", "hast", "mir", "das", "Bild", "der", "Welt", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und aller Bl\u00fctenzweige Lieblichkeit", "tokens": ["Und", "al\u00b7ler", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7ge", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mit einem solchen Glanze \u00fcberh\u00f6ht,", "tokens": ["Mit", "ei\u00b7nem", "sol\u00b7chen", "Glan\u00b7ze", "\u00fc\u00b7berh\u00b7\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df ich mich trunken an den Boden warf", "tokens": ["Da\u00df", "ich", "mich", "trun\u00b7ken", "an", "den", "Bo\u00b7den", "warf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und jauchzend f\u00fchlte, wie sie ihr Gewand", "tokens": ["Und", "jauch\u00b7zend", "f\u00fchl\u00b7te", ",", "wie", "sie", "ihr", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mir sinken lie\u00df, die leuchtende Natur!", "tokens": ["Mir", "sin\u00b7ken", "lie\u00df", ",", "die", "leuch\u00b7ten\u00b7de", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "H\u00f6r mich, mein Freund! Ich will nicht Herolde", "tokens": ["H\u00f6r", "mich", ",", "mein", "Freund", "!", "Ich", "will", "nicht", "He\u00b7rol\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Aussenden, da\u00df sie deinen Namen schrein", "tokens": ["Aus\u00b7sen\u00b7den", ",", "da\u00df", "sie", "dei\u00b7nen", "Na\u00b7men", "schrein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADJD"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.22": {"text": "In die vier Winde, wie wenn K\u00f6nige sterben:", "tokens": ["In", "die", "vier", "Win\u00b7de", ",", "wie", "wenn", "K\u00f6\u00b7ni\u00b7ge", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "$,", "KOKOM", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Ein K\u00f6nig l\u00e4\u00dft dem Erben seinen Reif", "tokens": ["Ein", "K\u00f6\u00b7nig", "l\u00e4\u00dft", "dem", "Er\u00b7ben", "sei\u00b7nen", "Reif"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und einem Grabstein seines Namens Schall.", "tokens": ["Und", "ei\u00b7nem", "Grab\u00b7stein", "sei\u00b7nes", "Na\u00b7mens", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Doch du warst solch ein gro\u00dfer Zauberer,", "tokens": ["Doch", "du", "warst", "solch", "ein", "gro\u00b7\u00dfer", "Zau\u00b7be\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.26": {"text": "Dein Sichtbares ging fort, doch wei\u00df ich nicht,", "tokens": ["Dein", "Sicht\u00b7ba\u00b7res", "ging", "fort", ",", "doch", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Was da und dort nicht alles von dir bleibt,", "tokens": ["Was", "da", "und", "dort", "nicht", "al\u00b7les", "von", "dir", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "PTKNEG", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Mit heimlicher fortlebender Gewalt", "tokens": ["Mit", "heim\u00b7li\u00b7cher", "fort\u00b7le\u00b7ben\u00b7der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Sich dunklen Auges aus der n\u00e4chtigen Flut", "tokens": ["Sich", "dunk\u00b7len", "Au\u00b7ges", "aus", "der", "n\u00e4ch\u00b7ti\u00b7gen", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.30": {"text": "Zum Ufer hebt \u2013 oder sein haarig Ohr", "tokens": ["Zum", "U\u00b7fer", "hebt", "\u2013", "o\u00b7der", "sein", "haa\u00b7rig", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$(", "KON", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Hinter dem Efeu horchend reckt,", "tokens": ["Hin\u00b7ter", "dem", "E\u00b7feu", "hor\u00b7chend", "reckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "drum will ich", "tokens": ["drum", "will", "ich"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Nie glauben, da\u00df ich irgendwo allein bin,", "tokens": ["Nie", "glau\u00b7ben", ",", "da\u00df", "ich", "ir\u00b7gend\u00b7wo", "al\u00b7lein", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Wo B\u00e4ume oder Blumen sind, ja selbst", "tokens": ["Wo", "B\u00e4u\u00b7me", "o\u00b7der", "Blu\u00b7men", "sind", ",", "ja", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VAFIN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Nur schweigendes Gestein und kleine W\u00f6lkchen", "tokens": ["Nur", "schwei\u00b7gen\u00b7des", "Ge\u00b7stein", "und", "klei\u00b7ne", "W\u00f6lk\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Unter dem Himmel sind: leicht da\u00df ein Etwas,", "tokens": ["Un\u00b7ter", "dem", "Him\u00b7mel", "sind", ":", "leicht", "da\u00df", "ein", "Et\u00b7was", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "$.", "ADJD", "KOUS", "ART", "ADV", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.37": {"text": "Durchsichtiger als Ariel, mir im R\u00fccken", "tokens": ["Durch\u00b7sich\u00b7ti\u00b7ger", "als", "A\u00b7riel", ",", "mir", "im", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "NE", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Hingaukelt, denn ich wei\u00df: geheimnisvoll", "tokens": ["Hin\u00b7gau\u00b7kelt", ",", "denn", "ich", "wei\u00df", ":", "ge\u00b7heim\u00b7nis\u00b7voll"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "KON", "PPER", "VVFIN", "$.", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "War zwischen dir und mancher Kreatur", "tokens": ["War", "zwi\u00b7schen", "dir", "und", "man\u00b7cher", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Ein Bund gekn\u00fcpft, ja! und des Fr\u00fchlings Au,", "tokens": ["Ein", "Bund", "ge\u00b7kn\u00fcpft", ",", "ja", "!", "und", "des", "Fr\u00fch\u00b7lings", "Au", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "$.", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Siehe, sie lachte dir so wie ein Weib", "tokens": ["Sie\u00b7he", ",", "sie", "lach\u00b7te", "dir", "so", "wie", "ein", "Weib"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Den anlacht, dem sie in der Nacht sich gab!", "tokens": ["Den", "an\u00b7lacht", ",", "dem", "sie", "in", "der", "Nacht", "sich", "gab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich meint um dich zu klagen, und mein Mund", "tokens": ["Ich", "meint", "um", "dich", "zu", "kla\u00b7gen", ",", "und", "mein", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schwillt an von trunkenem und freudigem Wort:", "tokens": ["Schwillt", "an", "von", "trun\u00b7ke\u00b7nem", "und", "freu\u00b7di\u00b7gem", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Drum ziemt mir nun nicht l\u00e4nger hier zu stehen.", "tokens": ["Drum", "ziemt", "mir", "nun", "nicht", "l\u00e4n\u00b7ger", "hier", "zu", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich will den Stab dreimal zu Boden sto\u00dfen", "tokens": ["Ich", "will", "den", "Stab", "drei\u00b7mal", "zu", "Bo\u00b7den", "sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dies Gezelt mit Traumgestalten f\u00fcllen.", "tokens": ["Und", "dies", "Ge\u00b7zelt", "mit", "Traum\u00b7ge\u00b7stal\u00b7ten", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die will ich mit der Last der Traurigkeit", "tokens": ["Die", "will", "ich", "mit", "der", "Last", "der", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], 