": {"text": "Aufgel\u00f6st in W\u00fcnsche, sanft wie den Augen der Doris", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "W\u00fcn\u00b7sche", ",", "sanft", "wie", "den", "Au\u00b7gen", "der", "Do\u00b7ris"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "ART", "NE"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Zitternde Tr\u00e4nen, vermischt", "tokens": ["Zit\u00b7tern\u00b7de", "Tr\u00e4\u00b7nen", ",", "ver\u00b7mischt"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Mit den meinen, entflossen, die Kinder der edelsten Freuden,", "tokens": ["Mit", "den", "mei\u00b7nen", ",", "ent\u00b7flos\u00b7sen", ",", "die", "Kin\u00b7der", "der", "e\u00b7dels\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "$,", "VVPP", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Traumgott, so zeige Sie mir!", "tokens": ["Traum\u00b7gott", ",", "so", "zei\u00b7ge", "Sie", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Doris, so komm mit umfassenden Armen, mit k\u00fcssenden Lippen,", "tokens": ["Do\u00b7ris", ",", "so", "komm", "mit", "um\u00b7fas\u00b7sen\u00b7den", "Ar\u00b7men", ",", "mit", "k\u00fcs\u00b7sen\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$