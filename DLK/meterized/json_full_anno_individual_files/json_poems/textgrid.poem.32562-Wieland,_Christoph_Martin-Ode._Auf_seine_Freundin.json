{"textgrid.poem.32562": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode. Auf seine Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm aus den Armen der Nacht, o Traumgott, vom scherzenden Schwarme", "tokens": ["Komm", "aus", "den", "Ar\u00b7men", "der", "Nacht", ",", "o", "Traum\u00b7gott", ",", "vom", "scher\u00b7zen\u00b7den", "Schwar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "FM", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Holder Gesichte umringt,", "tokens": ["Hol\u00b7der", "Ge\u00b7sich\u00b7te", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Komm, die schlummernde Seele, zu deiner Begeistrung ge\u00f6ffnet,", "tokens": ["Komm", ",", "die", "schlum\u00b7mern\u00b7de", "See\u00b7le", ",", "zu", "dei\u00b7ner", "Be\u00b7geis\u00b7trung", "ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Liegt und erwartet dich hier.", "tokens": ["Liegt", "und", "er\u00b7war\u00b7tet", "dich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Tr\u00fcge dies liebende Herz, zeig ihm die himmlische Freundin,", "tokens": ["Tr\u00fc\u00b7ge", "dies", "lie\u00b7ben\u00b7de", "Herz", ",", "zeig", "ihm", "die", "himm\u00b7li\u00b7sche", "Freun\u00b7din", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Zeig ihm das z\u00e4rtlichste Kind,", "tokens": ["Zeig", "ihm", "das", "z\u00e4rt\u00b7lichs\u00b7te", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Mit den Geistvollen Augen, voll sanfter liebender Blicke,", "tokens": ["Mit", "den", "Geist\u00b7vol\u00b7len", "Au\u00b7gen", ",", "voll", "sanf\u00b7ter", "lie\u00b7ben\u00b7der", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ADJD", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Mit dem l\u00e4chelnden Mund;", "tokens": ["Mit", "dem", "l\u00e4\u00b7cheln\u00b7den", "Mund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "So wie Sie war, so sch\u00f6n, so voll unbesiegbarer Anmut,", "tokens": ["So", "wie", "Sie", "war", ",", "so", "sch\u00f6n", ",", "so", "voll", "un\u00b7be\u00b7sieg\u00b7ba\u00b7rer", "An\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und Unsterblicher Pracht,", "tokens": ["Und", "U\u00b7nsterb\u00b7li\u00b7cher", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.11": {"text": "Wie die G\u00f6ttliche war, wenn unter zephyrischen Schatten", "tokens": ["Wie", "die", "G\u00f6tt\u00b7li\u00b7che", "war", ",", "wenn", "un\u00b7ter", "ze\u00b7phy\u00b7ri\u00b7schen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Uns der Abend umfing;", "tokens": ["Uns", "der", "A\u00b7bend", "um\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Wenn die Natur in Schlummer schon sank, und die einsame D\u00e4mmrung", "tokens": ["Wenn", "die", "Na\u00b7tur", "in", "Schlum\u00b7mer", "schon", "sank", ",", "und", "die", "ein\u00b7sa\u00b7me", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "ADV", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Uns zu Betrachtungen lud;", "tokens": ["Uns", "zu", "Be\u00b7trach\u00b7tun\u00b7gen", "lud", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Wenn wir, voll neuer Gedanken, uns in die Zukunft entfernten,", "tokens": ["Wenn", "wir", ",", "voll", "neu\u00b7er", "Ge\u00b7dan\u00b7ken", ",", "uns", "in", "die", "Zu\u00b7kunft", "ent\u00b7fern\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "ADJA", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Und die Lieb um uns her", "tokens": ["Und", "die", "Lieb", "um", "uns", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "APZR"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Paradiese von Freuden erschuf, und in reizender Aussicht", "tokens": ["Pa\u00b7ra\u00b7die\u00b7se", "von", "Freu\u00b7den", "er\u00b7schuf", ",", "und", "in", "rei\u00b7zen\u00b7der", "Aus\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+--++", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Unser Blick sich verlor.", "tokens": ["Un\u00b7ser", "Blick", "sich", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Ihres Gl\u00fcckes versichert und deiner Liebe, o Sch\u00f6pfer!", "tokens": ["Ih\u00b7res", "Gl\u00fc\u00b7ckes", "ver\u00b7si\u00b7chert", "und", "dei\u00b7ner", "Lie\u00b7be", ",", "o", "Sch\u00f6p\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Flossen die Seelen zu dir,", "tokens": ["Flos\u00b7sen", "die", "See\u00b7len", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Aufgel\u00f6st in W\u00fcnsche, sanft wie den Augen der Doris", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "W\u00fcn\u00b7sche", ",", "sanft", "wie", "den", "Au\u00b7gen", "der", "Do\u00b7ris"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "ART", "NE"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Zitternde Tr\u00e4nen, vermischt", "tokens": ["Zit\u00b7tern\u00b7de", "Tr\u00e4\u00b7nen", ",", "ver\u00b7mischt"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Mit den meinen, entflossen, die Kinder der edelsten Freuden,", "tokens": ["Mit", "den", "mei\u00b7nen", ",", "ent\u00b7flos\u00b7sen", ",", "die", "Kin\u00b7der", "der", "e\u00b7dels\u00b7ten", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "$,", "VVPP", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Traumgott, so zeige Sie mir!", "tokens": ["Traum\u00b7gott", ",", "so", "zei\u00b7ge", "Sie", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Doris, so komm mit umfassenden Armen, mit k\u00fcssenden Lippen,", "tokens": ["Do\u00b7ris", ",", "so", "komm", "mit", "um\u00b7fas\u00b7sen\u00b7den", "Ar\u00b7men", ",", "mit", "k\u00fcs\u00b7sen\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Mit entz\u00fcckendem Blick.", "tokens": ["Mit", "ent\u00b7z\u00fc\u00b7cken\u00b7dem", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Aber wenn ich Sie seh, wenn Sie mich liebreich umhalset,", "tokens": ["A\u00b7ber", "wenn", "ich", "Sie", "seh", ",", "wenn", "Sie", "mich", "lieb\u00b7reich", "um\u00b7hal\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Traumgott, denn eil auch zu ihr,", "tokens": ["Traum\u00b7gott", ",", "denn", "eil", "auch", "zu", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Dort wo in den Armen der Tugend, die himmlische schlummert,", "tokens": ["Dort", "wo", "in", "den", "Ar\u00b7men", "der", "Tu\u00b7gend", ",", "die", "himm\u00b7li\u00b7sche", "schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.30": {"text": "Oft vom Seraph gek\u00fc\u00dft,", "tokens": ["Oft", "vom", "Se\u00b7raph", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Gleich dem Fr\u00fchling, wenn er in Abendwolken geh\u00fcllet", "tokens": ["Gleich", "dem", "Fr\u00fch\u00b7ling", ",", "wenn", "er", "in", "Ab\u00b7end\u00b7wol\u00b7ken", "ge\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Auf der d\u00e4mmernden Flur", "tokens": ["Auf", "der", "d\u00e4m\u00b7mern\u00b7den", "Flur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Schlummert; denn eile zu ihr und zeig ihr in gleichen Gesichten", "tokens": ["Schlum\u00b7mert", ";", "denn", "ei\u00b7le", "zu", "ihr", "und", "zeig", "ihr", "in", "glei\u00b7chen", "Ge\u00b7sich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Ihren liebenden Freund,", "tokens": ["Ih\u00b7ren", "lie\u00b7ben\u00b7den", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Mit den Mienen voll Ruh, voll hoher wallender Wonne", "tokens": ["Mit", "den", "Mie\u00b7nen", "voll", "Ruh", ",", "voll", "ho\u00b7her", "wal\u00b7len\u00b7der", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,", "ADJD", "ADJA", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Die ihr Anblick erschafft;", "tokens": ["Die", "ihr", "An\u00b7blick", "er\u00b7schafft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Mit dem Auge das dankend hinauf zum Ewigen siehet", "tokens": ["Mit", "dem", "Au\u00b7ge", "das", "dan\u00b7kend", "hin\u00b7auf", "zum", "E\u00b7wi\u00b7gen", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "ADV", "APPRART", "ADJA", "VVFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.38": {"text": "Und denn wieder auf Sie,", "tokens": ["Und", "denn", "wie\u00b7der", "auf", "Sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.39": {"text": "Mit der z\u00e4rtlichsten Seele, die ihrer Begeistrung zu enge,", "tokens": ["Mit", "der", "z\u00e4rt\u00b7lichs\u00b7ten", "See\u00b7le", ",", "die", "ih\u00b7rer", "Be\u00b7geis\u00b7trung", "zu", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.40": {"text": "Voll wehm\u00fctiger Lust", "tokens": ["Voll", "weh\u00b7m\u00fc\u00b7ti\u00b7ger", "Lust"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Kaum noch sich f\u00fchlt und in deinen K\u00fcssen, o Doris, ges\u00e4ttigt,", "tokens": ["Kaum", "noch", "sich", "f\u00fchlt", "und", "in", "dei\u00b7nen", "K\u00fcs\u00b7sen", ",", "o", "Do\u00b7ris", ",", "ge\u00b7s\u00e4t\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "VVFIN", "KON", "APPR", "PPOSAT", "NN", "$,", "FM", "NE", "$,", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.42": {"text": "Sich und die Sch\u00f6pfung vergi\u00dft.", "tokens": ["Sich", "und", "die", "Sch\u00f6p\u00b7fung", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Komm aus den Armen der Nacht, o Traumgott, vom scherzenden Schwarme", "tokens": ["Komm", "aus", "den", "Ar\u00b7men", "der", "Nacht", ",", "o", "Traum\u00b7gott", ",", "vom", "scher\u00b7zen\u00b7den", "Schwar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "FM", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Holder Gesichte umringt,", "tokens": ["Hol\u00b7der", "Ge\u00b7sich\u00b7te", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Komm, die schlummernde Seele, zu deiner Begeistrung ge\u00f6ffnet,", "tokens": ["Komm", ",", "die", "schlum\u00b7mern\u00b7de", "See\u00b7le", ",", "zu", "dei\u00b7ner", "Be\u00b7geis\u00b7trung", "ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Liegt und erwartet dich hier.", "tokens": ["Liegt", "und", "er\u00b7war\u00b7tet", "dich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Tr\u00fcge dies liebende Herz, zeig ihm die himmlische F