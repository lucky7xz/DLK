{"textgrid.poem.53324": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[ihr Seelen, die jhr durch den Todt]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Seelen, die jhr durch den Todt", "tokens": ["Ihr", "See\u00b7len", ",", "die", "jhr", "durch", "den", "Todt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die selig' Endschafft aller Noht", "tokens": ["Die", "se\u00b7lig'", "End\u00b7schafft", "al\u00b7ler", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd alles Drangsals \u00fcberkommet,", "tokens": ["Vnd", "al\u00b7les", "Drangs\u00b7als", "\u00fc\u00b7ber\u00b7kom\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr wisset, da\u00df es einem frommet", "tokens": ["Ihr", "wis\u00b7set", ",", "da\u00df", "es", "ei\u00b7nem", "from\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hie niemals fast ohn Leiden seyn,", "tokens": ["Hie", "nie\u00b7mals", "fast", "ohn", "Lei\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd da\u00df ein Christ sich seiner Pein,", "tokens": ["Vnd", "da\u00df", "ein", "Christ", "sich", "sei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die f\u00fcr dem Himmel nichts zu sch\u00e4tzen,", "tokens": ["Die", "f\u00fcr", "dem", "Him\u00b7mel", "nichts", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vielmehr als sonst dort werd' ergetzen.", "tokens": ["Viel\u00b7mehr", "als", "sonst", "dort", "werd'", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer auff den schweren Arbeits-Tag", "tokens": ["Wer", "auff", "den", "schwe\u00b7ren", "Ar\u00b7beits\u00b7Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich umb die Nacht erholen mag,", "tokens": ["Sich", "umb", "die", "Nacht", "er\u00b7ho\u00b7len", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem thut der Schlaff noch eins so eben.", "tokens": ["Dem", "thut", "der", "Schlaff", "noch", "eins", "so", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Mensch, der hie sein gantzes Leben", "tokens": ["Ein", "Mensch", ",", "der", "hie", "sein", "gant\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In steter Wollust zugebracht,", "tokens": ["In", "ste\u00b7ter", "Wol\u00b7lust", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bef\u00e4llt jhn dann die Todes-Nacht", "tokens": ["Be\u00b7f\u00e4llt", "jhn", "dann", "die", "To\u00b7des\u00b7Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd er zu Grabe sich sol finden,", "tokens": ["Vnd", "er", "zu", "Gra\u00b7be", "sich", "sol", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Ruh wird jhn so sanfft nicht binden.", "tokens": ["Die", "Ruh", "wird", "jhn", "so", "sanfft", "nicht", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort wird die Krone dessen H\u00e4upt,", "tokens": ["Dort", "wird", "die", "Kro\u00b7ne", "des\u00b7sen", "H\u00e4upt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den GOTT allhie f\u00fcr andern st\u00e4upt,", "tokens": ["Den", "GoTT", "all\u00b7hie", "f\u00fcr", "an\u00b7dern", "st\u00e4upt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr andern auffgesetzet werden;", "tokens": ["F\u00fcr", "an\u00b7dern", "auff\u00b7ge\u00b7set\u00b7zet", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer seinen Samen hie auff Erden", "tokens": ["Wer", "sei\u00b7nen", "Sa\u00b7men", "hie", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Thr\u00e4nen in den Acker strewt,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "in", "den", "A\u00b7cker", "strewt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem wird dort werden abgemeyt", "tokens": ["Dem", "wird", "dort", "wer\u00b7den", "ab\u00b7ge\u00b7meyt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Frucht in h\u00f6chsten Himmel-Frewden:", "tokens": ["Die", "Frucht", "in", "h\u00f6chs\u00b7ten", "Him\u00b7mel\u00b7Fre\u00b7wden", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wer wolte nun nicht Tr\u00fcbsal leiden?", "tokens": ["Wer", "wol\u00b7te", "nun", "nicht", "Tr\u00fcb\u00b7sal", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ist sie? Eine Meisterin", "tokens": ["Was", "ist", "sie", "?", "Ei\u00b7ne", "Meis\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der L\u00fcste, welchen unser Sinn", "tokens": ["Der", "L\u00fcs\u00b7te", ",", "wel\u00b7chen", "un\u00b7ser", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn sie sich gar zu leicht ergiebet.", "tokens": ["Ohn", "sie", "sich", "gar", "zu", "leicht", "er\u00b7gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PRF", "ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wol dem, den GOTT hie stets betr\u00fcbet,", "tokens": ["Wol", "dem", ",", "den", "GoTT", "hie", "stets", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Vnd an dem Creutze ziehen l\u00e4sst,", "tokens": ["Vnd", "an", "dem", "Creut\u00b7ze", "zie\u00b7hen", "l\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4lt er an seinem GOTT nur fest,", "tokens": ["H\u00e4lt", "er", "an", "sei\u00b7nem", "GoTT", "nur", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihm wird sein unbefleckt Gewissen", "tokens": ["Ihm", "wird", "sein", "un\u00b7be\u00b7fleckt", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zuletzt von keiner Angst gebissen!", "tokens": ["Zu\u00b7letzt", "von", "kei\u00b7ner", "Angst", "ge\u00b7bis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Schiffman, der in Sorgen lebt,", "tokens": ["Ein", "Schiff\u00b7man", ",", "der", "in", "Sor\u00b7gen", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn wo ein Sturm das Meer erhebt,", "tokens": ["Wenn", "wo", "ein", "Sturm", "das", "Meer", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird in Bereitschafft stets gefunden,", "tokens": ["Wird", "in", "Be\u00b7reit\u00b7schafft", "stets", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kriegsman, welcher seine Stunden", "tokens": ["Ein", "Kriegs\u00b7man", ",", "wel\u00b7cher", "sei\u00b7ne", "Stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auff seiner Wache sorglich h\u00e4lt,", "tokens": ["Auff", "sei\u00b7ner", "Wa\u00b7che", "sorg\u00b7lich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird leichtlich nicht durch List gef\u00e4llt;", "tokens": ["Wird", "leicht\u00b7lich", "nicht", "durch", "List", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer aber hie in Wollust lieget,", "tokens": ["Wer", "a\u00b7ber", "hie", "in", "Wol\u00b7lust", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem hat der Feind schon angesieget.", "tokens": ["Dem", "hat", "der", "Feind", "schon", "an\u00b7ge\u00b7sie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr Seelen, die jhr durch den Todt", "tokens": ["Ihr", "See\u00b7len", ",", "die", "jhr", "durch", "den", "Todt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die sehlig' Endschafft aller Noht", "tokens": ["Die", "seh\u00b7lig'", "End\u00b7schafft", "al\u00b7ler", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "lin