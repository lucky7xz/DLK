{"dta.poem.8265": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "erster  A ufftritt.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin verf\u00fchrt; mein Hoffen trift nicht", "tokens": ["Ich", "bin", "ver\u00b7f\u00fchrt", ";", "mein", "Hof\u00b7fen", "trift", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "ein:", "tokens": ["ein", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ich suchte Lust/ und finde nichts als", "tokens": ["Ich", "such\u00b7te", "Lust", "/", "und", "fin\u00b7de", "nichts", "als"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$(", "KON", "VVFIN", "PIS", "KOKOM"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pein/", "tokens": ["Pein", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wie \u00e4ndert doch die Zeit so bald die Sinnen:", "tokens": ["Wie", "\u00e4n\u00b7dert", "doch", "die", "Zeit", "so", "bald", "die", "Sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Augen Licht/ das sonst mein Gl\u00fccks-Stern", "tokens": ["Der", "Au\u00b7gen", "Licht", "/", "das", "sonst", "mein", "Gl\u00fccks\u00b7S\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "PDS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "schien/", "tokens": ["schien", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Will mir nunmehr der Strahlen Gunst entziehn/", "tokens": ["Will", "mir", "nun\u00b7mehr", "der", "Strah\u00b7len", "Gunst", "ent\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Was soll ich wohl bey diesen Spiel beginnen.", "tokens": ["Was", "soll", "ich", "wohl", "bey", "die\u00b7sen", "Spiel", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}