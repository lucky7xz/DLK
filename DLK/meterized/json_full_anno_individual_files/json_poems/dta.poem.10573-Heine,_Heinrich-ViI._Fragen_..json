{"dta.poem.10573": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Fragen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Am Meer, am w\u00fcsten, n\u00e4chtlichen Meer", "tokens": ["Am", "Meer", ",", "am", "w\u00fcs\u00b7ten", ",", "n\u00e4cht\u00b7li\u00b7chen", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Steht ein J\u00fcngling-Mann,", "tokens": ["Steht", "ein", "J\u00fcng\u00b7ling\u00b7Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die Brust voll Wehmuth, das Haupt voll Zweifel,", "tokens": ["Die", "Brust", "voll", "Weh\u00b7muth", ",", "das", "Haupt", "voll", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und mit d\u00fcstern Lippen fragt er die Wogen:", "tokens": ["Und", "mit", "d\u00fcs\u00b7tern", "Lip\u00b7pen", "fragt", "er", "die", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eo l\u00f6s't mir das R\u00e4thsel des Lebens,", "tokens": ["\u201e", "o", "l\u00f6s't", "mir", "das", "R\u00e4th\u00b7sel", "des", "Le\u00b7bens", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das qualvoll uralte R\u00e4thsel,", "tokens": ["Das", "qual\u00b7voll", "ur\u00b7al\u00b7te", "R\u00e4th\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wor\u00fcber schon manche H\u00e4upter gegr\u00fcbelt,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "schon", "man\u00b7che", "H\u00e4up\u00b7ter", "ge\u00b7gr\u00fc\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00e4upter in Hieroglyphenm\u00fctzen,", "tokens": ["H\u00e4up\u00b7ter", "in", "Hie\u00b7ro\u00b7gly\u00b7phen\u00b7m\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "H\u00e4upter in Turban und schwarzem Barett,", "tokens": ["H\u00e4up\u00b7ter", "in", "Tur\u00b7ban", "und", "schwar\u00b7zem", "Ba\u00b7rett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Per\u00fcckenh\u00e4upter und tausend andre", "tokens": ["Pe\u00b7r\u00fc\u00b7cken\u00b7h\u00e4up\u00b7ter", "und", "tau\u00b7send", "and\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "CARD", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Arme, schwitzende Menschenh\u00e4upter \u2014", "tokens": ["Ar\u00b7me", ",", "schwit\u00b7zen\u00b7de", "Men\u00b7schen\u00b7h\u00e4up\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sagt mir, was bedeutet der Mensch?", "tokens": ["Sagt", "mir", ",", "was", "be\u00b7deu\u00b7tet", "der", "Mensch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Woher ist er kommen? Wo geht er hin?", "tokens": ["Wo\u00b7her", "ist", "er", "kom\u00b7men", "?", "Wo", "geht", "er", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Wer wohnt dort oben auf goldenen Sternen?", "tokens": ["Wer", "wohnt", "dort", "o\u00b7ben", "auf", "gol\u00b7de\u00b7nen", "Ster\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es murmeln die Wogen ihr ew'ges Gemurmel,", "tokens": ["Es", "mur\u00b7meln", "die", "Wo\u00b7gen", "ihr", "ew'\u00b7ges", "Ge\u00b7mur\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es wehet der Wind, es fliehen die Wolken,", "tokens": ["Es", "we\u00b7het", "der", "Wind", ",", "es", "flie\u00b7hen", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es blinken die Sterne, gleichg\u00fcltig und kalt,", "tokens": ["Es", "blin\u00b7ken", "die", "Ster\u00b7ne", ",", "gleich\u00b7g\u00fcl\u00b7tig", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und ein Narr wartet auf Antwort.", "tokens": ["Und", "ein", "Narr", "war\u00b7tet", "auf", "Ant\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}