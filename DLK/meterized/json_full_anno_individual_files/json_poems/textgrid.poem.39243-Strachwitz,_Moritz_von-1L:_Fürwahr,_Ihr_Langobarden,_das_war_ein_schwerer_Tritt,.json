sure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie lagest du, o Mailand, du, sonst so hoch und frei,", "tokens": ["Wie", "la\u00b7gest", "du", ",", "o", "Mai\u00b7land", ",", "du", ",", "sonst", "so", "hoch", "und", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "FM", "NE", "$,", "PPER", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zertreten im blutigen Staube, du Perle der Lombardei!", "tokens": ["Zer\u00b7tre\u00b7ten", "im", "blu\u00b7ti\u00b7gen", "Stau\u00b7be", ",", "du", "Per\u00b7le", "der", "Lom\u00b7bar\u00b7dei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Der Schutt im Winde wirbelte, wo S\u00e4ulen geragt unl\u00e4ngst,", "tokens": ["Der", "Schutt", "im", "Win\u00b7de", "wir\u00b7bel\u00b7te", ",", "wo", "S\u00e4u\u00b7len", "ge\u00b7ragt", "un\u00b7l\u00e4ngst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "PWAV", "NN", "VVPP", "ADJD", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und \u00fcber den Marmor stampfte der schwerhufige Friesenhengst.", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Mar\u00b7mor", "stampf\u00b7te", "der", "schwer\u00b7hu\u00b7fi\u00b7ge", "Frie\u00b7sen\u00b7hengst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Und Stille \u00fcber den Tr\u00fcmmern und Stille in dem Tro\u00df,", "tokens": ["Und", "Stil\u00b7le", "\u00fc\u00b7ber", "den", "Tr\u00fcm\u00b7mern", "und", "Stil\u00b7le", "in", "dem", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da z\u00fcgelte der R\u00e4cher sein kaiserliches Ro\u00df.", "tokens": ["Da", "z\u00fc\u00b7gel\u00b7te", "der", "R\u00e4\u00b7cher", "sein", "kai\u00b7ser\u00b7li\u00b7ches", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und tiefer ward die Stille, denn alles stand zur Stell',", "tokens": ["Und", "tie\u00b7fer", "ward", "die", "Stil\u00b7le", ",", "denn", "al\u00b7les", "stand", "zur", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "KON", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Quer auf des Siegers Wege lag ein sterbender Rebell.", "tokens": ["Quer", "auf", "des", "Sie\u00b7gers", "We\u00b7ge", "lag", "ein", "ster\u00b7ben\u00b7der", "Re\u00b7bell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Der b\u00e4umte sich gewaltig mit halbem Leib hochauf", "tokens": ["Der", "b\u00e4um\u00b7te", "sich", "ge\u00b7wal\u00b7tig", "mit", "hal\u