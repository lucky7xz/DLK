{"dta.poem.133": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die gebundene Freyheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "G\u00fcldne Freyheit/ mein Vergn\u00fcgen!", "tokens": ["G\u00fcld\u00b7ne", "Frey\u00b7heit", "/", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Fesseln sind entzwey/", "tokens": ["Mei\u00b7ne", "Fes\u00b7seln", "sind", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Liebe l\u00e4st mich siegen/", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "l\u00e4st", "mich", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Hertze ist nun frey.", "tokens": ["Und", "mein", "Hert\u00b7ze", "ist", "nun", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solche Freyheit kan man haben", "tokens": ["Sol\u00b7che", "Frey\u00b7heit", "kan", "man", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PIS", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn auf einer sch\u00f6nen Brust/", "tokens": ["Wenn", "auf", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich die Seele wei\u00df zu laben", "tokens": ["Sich", "die", "See\u00b7le", "wei\u00df", "zu", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In vergn\u00fcgter Himmels-Lust.", "tokens": ["In", "ver\u00b7gn\u00fcg\u00b7ter", "Him\u00b7mels\u00b7Lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "So bindt mich auch deine G\u00fcte/", "tokens": ["So", "bindt", "mich", "auch", "dei\u00b7ne", "G\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Sch\u00f6nster Engel jetzo lo\u00df/", "tokens": ["Sch\u00f6ns\u00b7ter", "En\u00b7gel", "jet\u00b7zo", "lo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und dein gn\u00e4diges Gem\u00fchte", "tokens": ["Und", "dein", "gn\u00e4\u00b7di\u00b7ges", "Ge\u00b7m\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Oeffnet mir die sch\u00f6ne Schoo\u00df.", "tokens": ["Oeff\u00b7net", "mir", "die", "sch\u00f6\u00b7ne", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ob die Ketten gleich entbunden/", "tokens": ["Ob", "die", "Ket\u00b7ten", "gleich", "ent\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dennoch legst du Fesseln an/", "tokens": ["Den\u00b7noch", "legst", "du", "Fes\u00b7seln", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die Freyheit wird gefunden/", "tokens": ["Und", "die", "Frey\u00b7heit", "wird", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wenn man nicht entgehen kan.", "tokens": ["Wenn", "man", "nicht", "ent\u00b7ge\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Diese Ketten mu\u00df ich k\u00fcssen/", "tokens": ["Die\u00b7se", "Ket\u00b7ten", "mu\u00df", "ich", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Weil sie allzu lieblich seyn;", "tokens": ["Weil", "sie", "all\u00b7zu", "lieb\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Was mich vordem binden m\u00fcssen", "tokens": ["Was", "mich", "vor\u00b7dem", "bin\u00b7den", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVINF", "VMINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Kleidet sich in Freyheit ein.", "tokens": ["Klei\u00b7det", "sich", "in", "Frey\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Weil mich diese Banden binden", "tokens": ["Weil", "mich", "die\u00b7se", "Ban\u00b7den", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wei\u00df ich/ da\u00df du meine bist/", "tokens": ["Wei\u00df", "ich", "/", "da\u00df", "du", "mei\u00b7ne", "bist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "PPOSAT", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und die Freyheit kan ich finden", "tokens": ["Und", "die", "Frey\u00b7heit", "kan", "ich", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wo sie eine Sclavin ist.", "tokens": ["Wo", "sie", "ei\u00b7ne", "Scla\u00b7vin", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Bey den zarten Schwaanen-Br\u00fcsten", "tokens": ["Bey", "den", "zar\u00b7ten", "Schwaa\u00b7nen\u00b7Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Meine Hand gefangen liegt", "tokens": ["Mei\u00b7ne", "Hand", "ge\u00b7fan\u00b7gen", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und die Anmuht von den L\u00fcsten", "tokens": ["Und", "die", "An\u00b7muht", "von", "den", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Den erl\u00f6\u00dften Geist vergn\u00fcgt.", "tokens": ["Den", "er\u00b7l\u00f6\u00df\u00b7ten", "Geist", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Durch die Fesseln deiner H\u00e4nde", "tokens": ["Durch", "die", "Fes\u00b7seln", "dei\u00b7ner", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Werden meine frey gemacht/", "tokens": ["Wer\u00b7den", "mei\u00b7ne", "frey", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wo ich meinen Geist hinwende", "tokens": ["Wo", "ich", "mei\u00b7nen", "Geist", "hin\u00b7wen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Alles in der Freyheit lacht.", "tokens": ["Al\u00b7les", "in", "der", "Frey\u00b7heit", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Bey dem Marmor deiner Glieder", "tokens": ["Bey", "dem", "Mar\u00b7mor", "dei\u00b7ner", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Bey der Brust und bey dem Hals", "tokens": ["Bey", "der", "Brust", "und", "bey", "dem", "Hals"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Find ich meine Freyheit wieder", "tokens": ["Find", "ich", "mei\u00b7ne", "Frey\u00b7heit", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und verliehr sie ebenfals.", "tokens": ["Und", "ver\u00b7liehr", "sie", "e\u00b7ben\u00b7fals", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Von dem angeschlossnen F\u00fcssen", "tokens": ["Von", "dem", "an\u00b7ge\u00b7schloss\u00b7nen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "L\u00f6sest du die Fesseln ab/", "tokens": ["L\u00f6\u00b7sest", "du", "die", "Fes\u00b7seln", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Was den Sclaven mag verdriessen", "tokens": ["Was", "den", "Scla\u00b7ven", "mag", "ver\u00b7dries\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Mir die g\u00fcldne Freyheit gab.", "tokens": ["Mir", "die", "g\u00fcld\u00b7ne", "Frey\u00b7heit", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Wenn mein Geist bey Mund und Wangen", "tokens": ["Wenn", "mein", "Geist", "bey", "Mund", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "In der s\u00fcssen Sclaverey", "tokens": ["In", "der", "s\u00fcs\u00b7sen", "Scla\u00b7ve\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Kan mit solchen Fesseln prangen/", "tokens": ["Kan", "mit", "sol\u00b7chen", "Fes\u00b7seln", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Ist er in den Banden frey.", "tokens": ["Ist", "er", "in", "den", "Ban\u00b7den", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Soll er stets gebunden liegen", "tokens": ["Soll", "er", "stets", "ge\u00b7bun\u00b7den", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "An der Brust/ und deiner Schoo\u00df?", "tokens": ["An", "der", "Brust", "/", "und", "dei\u00b7ner", "Schoo\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Glaub/ es wird ihn recht vergn\u00fcgen/", "tokens": ["Glaub", "/", "es", "wird", "ihn", "recht", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Keine Last ist da zu gro\u00df.", "tokens": ["Kei\u00b7ne", "Last", "ist", "da", "zu", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Weil ich dich nun kan umfassen/", "tokens": ["Weil", "ich", "dich", "nun", "kan", "um\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Will ich gern dein Sclave seyn/", "tokens": ["Will", "ich", "gern", "dein", "Scla\u00b7ve", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Ich will alle Frey heit hassen", "tokens": ["Ich", "will", "al\u00b7le", "Frey", "heit", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Schlie\u00df mich nur in Banden ein.", "tokens": ["Schlie\u00df", "mich", "nur", "in", "Ban\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Meine Freyheit ist verhanden", "tokens": ["Mei\u00b7ne", "Frey\u00b7heit", "ist", "ver\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Wenn dein holdes Auge lacht", "tokens": ["Wenn", "dein", "hol\u00b7des", "Au\u00b7ge", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Und die Last der leichten Banden", "tokens": ["Und", "die", "Last", "der", "leich\u00b7ten", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Wird der Freyheit gleich geacht.", "tokens": ["Wird", "der", "Frey\u00b7heit", "gleich", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}