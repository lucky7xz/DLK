{"dta.poem.11230": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der ungl\u00fcckliche freyer.  \n G. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schickte mich neulich zum ehlichen stande,", "tokens": ["Ich", "schick\u00b7te", "mich", "neu\u00b7lich", "zum", "eh\u00b7li\u00b7chen", "stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Trug aber das ja-wort mit k\u00f6rben davon,", "tokens": ["Trug", "a\u00b7ber", "das", "ja\u00b7wort", "mit", "k\u00f6r\u00b7ben", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "VVFIN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was macht es? Die antwort bekam ich zum lohn;", "tokens": ["Was", "macht", "es", "?", "Die", "ant\u00b7wort", "be\u00b7kam", "ich", "zum", "lohn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich h\u00e4tte nicht liegende g\u00fcter im lande.", "tokens": ["Ich", "h\u00e4t\u00b7te", "nicht", "lie\u00b7gen\u00b7de", "g\u00fc\u00b7ter", "im", "lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "APPRART", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ich dachte, du n\u00e4rrgen! ich freye de\u00dfwegen,", "tokens": ["Ich", "dach\u00b7te", ",", "du", "n\u00e4rr\u00b7gen", "!", "ich", "frey\u00b7e", "de\u00df\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$.", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df ich mich auf liegende gr\u00fcnde will legen.", "tokens": ["Da\u00df", "ich", "mich", "auf", "lie\u00b7gen\u00b7de", "gr\u00fcn\u00b7de", "will", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}