{"dta.poem.12765": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die hochzeit  \n  Herrn Theodor Steudners,   Ph. &  \n M. D.  u.  Physici  zu Greiffenberg,  \n  Mit Jungfer Anna Barbara  \n K\u00e4tzlerin, 1674.  \n J. F. K.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr im lazareth der eitlen liebe liegt,", "tokens": ["Die", "ihr", "im", "la\u00b7za\u00b7reth", "der", "eit\u00b7len", "lie\u00b7be", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "VVFIN", "ART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die l\u00fcsternde begier euch last zu selaven machen,", "tokens": ["Die", "l\u00fcs\u00b7tern\u00b7de", "be\u00b7gier", "euch", "last", "zu", "se\u00b7la\u00b7ven", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verf\u00fchrte sterblichen! was ists, das euch vergn\u00fcgt?", "tokens": ["Ver\u00b7f\u00fchr\u00b7te", "sterb\u00b7li\u00b7chen", "!", "was", "ists", ",", "das", "euch", "ver\u00b7gn\u00fcgt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "PWS", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein schatz, wo ach und weh mit tausend drachen wachen;", "tokens": ["Ein", "schatz", ",", "wo", "ach", "und", "weh", "mit", "tau\u00b7send", "dra\u00b7chen", "wa\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "XY", "KON", "ADV", "APPR", "CARD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein marckt, wo man mehr gifft als mareipan verkaufft;", "tokens": ["Ein", "marckt", ",", "wo", "man", "mehr", "gifft", "als", "ma\u00b7rei\u00b7pan", "ver\u00b7kaufft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "PWAV", "PIS", "ADV", "VVFIN", "KOKOM", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein meer, wo unser schiff offt mann und guth verlohren,", "tokens": ["Ein", "meer", ",", "wo", "un\u00b7ser", "schiff", "offt", "mann", "und", "guth", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "ADV", "NN", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo das bestrittne bot an klipp und felsen laufft,", "tokens": ["Wo", "das", "be\u00b7stritt\u00b7ne", "bot", "an", "klipp", "und", "fel\u00b7sen", "laufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "VVFIN", "APPR", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn sich die strenge fluth auf unser haupt verschworen.", "tokens": ["Wenn", "sich", "die", "stren\u00b7ge", "fluth", "auf", "un\u00b7ser", "haupt", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie mancher hat die bahr, den dolch und strang erkiest,", "tokens": ["Wie", "man\u00b7cher", "hat", "die", "bahr", ",", "den", "dolch", "und", "strang", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Cypris und ihr kind mit ihrer macht bestritten!", "tokens": ["Den", "Cyp\u00b7ris", "und", "ihr", "kind", "mit", "ih\u00b7rer", "macht", "be\u00b7strit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man wei\u00df, wie f\u00fcrst Anton zuletzt gefallen ist;", "tokens": ["Man", "wei\u00df", ",", "wie", "f\u00fcrst", "An\u00b7ton", "zu\u00b7letzt", "ge\u00b7fal\u00b7len", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "ADV", "NE", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie Jphis vor der zeit des lebens garn zerschnitten.", "tokens": ["Wie", "Jphis", "vor", "der", "zeit", "des", "le\u00b7bens", "garn", "zer\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "Die achse Jlions klagt noch den rauhen fall,", "tokens": ["Die", "ac\u00b7hse", "Jli\u00b7ons", "klagt", "noch", "den", "rau\u00b7hen", "fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Sie flucht der Helenen den zunder ihrer flammen:", "tokens": ["Sie", "flucht", "der", "He\u00b7le\u00b7nen", "den", "zun\u00b7der", "ih\u00b7rer", "flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Man h\u00f6rt um Jda noch den leichten widerschall", "tokens": ["Man", "h\u00f6rt", "um", "Jda", "noch", "den", "leich\u00b7ten", "wi\u00b7der\u00b7schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Des Paris richt-urtheil und tolle brunst verdammen.", "tokens": ["Des", "Pa\u00b7ris", "richt\u00b7urt\u00b7heil", "und", "tol\u00b7le", "brunst", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So ists: wo man der lieb alt\u00e4r und tempel baut,", "tokens": ["So", "ists", ":", "wo", "man", "der", "lieb", "al\u00b7t\u00e4r", "und", "tem\u00b7pel", "baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$.", "PWAV", "PIS", "ART", "ADJD", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.18": {"text": "Mu\u00df klugheit und verstand den seepter niederlegen;", "tokens": ["Mu\u00df", "klug\u00b7heit", "und", "ver\u00b7stand", "den", "seep\u00b7ter", "nie\u00b7der\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "VVFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das fern-glas der vernunfft wird schwehrlich augeschaut,", "tokens": ["Das", "fern\u00b7glas", "der", "ver\u00b7nunfft", "wird", "schwehr\u00b7lich", "au\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo in befteckter brust sich geile flammen regen.", "tokens": ["Wo", "in", "bef\u00b7teck\u00b7ter", "brust", "sich", "gei\u00b7le", "flam\u00b7men", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "VVFIN", "PRF", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die motte sieht das licht und slieget dennoch drein;", "tokens": ["Die", "mot\u00b7te", "sieht", "das", "licht", "und", "slie\u00b7get", "den\u00b7noch", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der botsmann kennt die fluth der ungetreuen wellen;", "tokens": ["Der", "bots\u00b7mann", "kennt", "die", "fluth", "der", "un\u00b7ge\u00b7treu\u00b7en", "wel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der vogel schaut den leim, und will gefangen seyn;", "tokens": ["Der", "vo\u00b7gel", "schaut", "den", "leim", ",", "und", "will", "ge\u00b7fan\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das schnelle reh das garn, so ihm die j\u00e4ger stellen:", "tokens": ["Das", "schnel\u00b7le", "reh", "das", "garn", ",", "so", "ihm", "die", "j\u00e4\u00b7ger", "stel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,", "ADV", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So st\u00fcrtzt auch, der da liebt, mit willen in die noth,", "tokens": ["So", "st\u00fcrtzt", "auch", ",", "der", "da", "liebt", ",", "mit", "wil\u00b7len", "in", "die", "noth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PRELS", "ADV", "VVFIN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und will das gifft ihm selbst zu seinem tode kochen.", "tokens": ["Und", "will", "das", "gifft", "ihm", "selbst", "zu", "sei\u00b7nem", "to\u00b7de", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PDS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Armseelger aberwitz! der perlen in dem koth,", "tokens": ["Arm\u00b7seel\u00b7ger", "a\u00b7ber\u00b7witz", "!", "der", "per\u00b7len", "in", "dem", "koth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und lebens-blumen sucht bey d\u00fcrren todten-knochen!", "tokens": ["Und", "le\u00b7bens\u00b7blu\u00b7men", "sucht", "bey", "d\u00fcr\u00b7ren", "tod\u00b7ten\u00b7kno\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Furcht und verzweiffelung sind seine folterbanck,", "tokens": ["Furcht", "und", "ver\u00b7zweif\u00b7fe\u00b7lung", "sind", "sei\u00b7ne", "fol\u00b7ter\u00b7banck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der fels, woran offt ehr und tugend scheitern m\u00fcssen,", "tokens": ["Der", "fels", ",", "wo\u00b7ran", "offt", "ehr", "und", "tu\u00b7gend", "schei\u00b7tern", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "NN", "KON", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn das verh\u00e4ngni\u00df ihm itzt reicht den gallen-tranck,", "tokens": ["Wenn", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "ihm", "itzt", "reicht", "den", "gal\u00b7len\u00b7tranck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die sterne seiner lust vor ihm die augen schliessen.", "tokens": ["Die", "ster\u00b7ne", "sei\u00b7ner", "lust", "vor", "ihm", "die", "au\u00b7gen", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein schwacher kiel erbebt, die hand wird ei\u00df und stein;", "tokens": ["Mein", "schwa\u00b7cher", "kiel", "er\u00b7bebt", ",", "die", "hand", "wird", "ei\u00df", "und", "stein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der starre mund steht an die mittel zu ber\u00fchren,", "tokens": ["Der", "star\u00b7re", "mund", "steht", "an", "die", "mit\u00b7tel", "zu", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wodurch Jdalia, die ihr gewidmet seyn,", "tokens": ["Wo\u00b7durch", "Jda\u00b7lia", ",", "die", "ihr", "ge\u00b7wid\u00b7met", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PRELS", "PPER", "VVPP", "VAINF", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Nach zweiffelhafften sturm will in den hafen f\u00fchren.", "tokens": ["Nach", "zweif\u00b7fel\u00b7haff\u00b7ten", "sturm", "will", "in", "den", "ha\u00b7fen", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Man steigt zum grausen Styx durch fremder w\u00f6rter macht,", "tokens": ["Man", "steigt", "zum", "grau\u00b7sen", "Styx", "durch", "frem\u00b7der", "w\u00f6r\u00b7ter", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und nimmt den Radamanth zum artzt der liebes-wunden,", "tokens": ["Und", "nimmt", "den", "Ra\u00b7da\u00b7man\u00b7th", "zum", "artzt", "der", "lie\u00b7bes\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Der seelen bezoar wird leyder! schlecht geacht,", "tokens": ["Der", "see\u00b7len", "be\u00b7zoar", "wird", "ley\u00b7der", "!", "schlecht", "ge\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$.", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Wenn nur ein ", "tokens": ["Wenn", "nur", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Das schwartze zauber-spiel sucht bey den geistsrn rath,", "tokens": ["Das", "schwart\u00b7ze", "zau\u00b7ber\u00b7spiel", "sucht", "bey", "den", "geistsrn", "rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.42": {"text": "Wo ein bekanter weg sich in viel stra\u00dfen theilet.", "tokens": ["Wo", "ein", "be\u00b7kan\u00b7ter", "weg", "sich", "in", "viel", "stra\u00b7\u00dfen", "thei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADV", "PRF", "APPR", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Es wei\u00df das gro\u00dfe Rom, was dort Palumbus that,", "tokens": ["Es", "wei\u00df", "das", "gro\u00b7\u00dfe", "Rom", ",", "was", "dort", "Pa\u00b7lum\u00b7bus", "that", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "$,", "PRELS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wenn sein verfluchter brieff verliebter pein geheilet.", "tokens": ["Wenn", "sein", "ver\u00b7fluch\u00b7ter", "brieff", "ver\u00b7lieb\u00b7ter", "pein", "ge\u00b7hei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Was mehr? der tolle grimm f\u00e4llt auch die todten an,", "tokens": ["Was", "mehr", "?", "der", "tol\u00b7le", "grimm", "f\u00e4llt", "auch", "die", "tod\u00b7ten", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "ART", "ADJA", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Bricht grufft und gr\u00e4ber auf, und r