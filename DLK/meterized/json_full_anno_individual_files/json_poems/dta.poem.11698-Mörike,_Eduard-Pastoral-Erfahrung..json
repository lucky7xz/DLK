{"dta.poem.11698": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Pastoral-Erfahrung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Meine guten Bauern freuen mich sehr;", "tokens": ["Mei\u00b7ne", "gu\u00b7ten", "Bau\u00b7ern", "freu\u00b7en", "mich", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Eine \u201escharfe Predigt\u201c ist ihr Begehr.", "tokens": ["Ei\u00b7ne", "\u201e", "schar\u00b7fe", "Pre\u00b7digt", "\u201c", "ist", "ihr", "Be\u00b7gehr", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADJA", "NN", "$(", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und wenn man mir es nicht verdenkt,", "tokens": ["Und", "wenn", "man", "mir", "es", "nicht", "ver\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sag' ich, wie das zusammenh\u00e4ngt.", "tokens": ["Sag'", "ich", ",", "wie", "das", "zu\u00b7sam\u00b7men\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonnabend, wohl nach Elfe spat,", "tokens": ["Sonn\u00b7a\u00b7bend", ",", "wohl", "nach", "El\u00b7fe", "spat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Garten stehlen sie mir den Salat;", "tokens": ["Im", "Gar\u00b7ten", "steh\u00b7len", "sie", "mir", "den", "Sa\u00b7lat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In der Morgenkirch' mit guter Ruh'", "tokens": ["In", "der", "Mor\u00b7gen\u00b7kirch'", "mit", "gu\u00b7ter", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Erwarten sie den Essig dazu;", "tokens": ["Er\u00b7war\u00b7ten", "sie", "den", "Es\u00b7sig", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Der Predigt Schlu\u00df fein linde sey:", "tokens": ["Der", "Pre\u00b7digt", "Schlu\u00df", "fein", "lin\u00b7de", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie wollen gern auch Oel dabei.", "tokens": ["Sie", "wol\u00b7len", "gern", "auch", "O\u00b7el", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}}}}