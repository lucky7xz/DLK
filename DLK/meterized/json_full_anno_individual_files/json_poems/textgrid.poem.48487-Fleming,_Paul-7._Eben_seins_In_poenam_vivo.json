{"textgrid.poem.48487": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Eben seins In poenam vivo", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, Jungfrau, es ist satt! Der Pfeil von deinen Augen,", "tokens": ["Ach", ",", "Jung\u00b7frau", ",", "es", "ist", "satt", "!", "Der", "Pfeil", "von", "dei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der sich in mich verkroch, der wegert mir den Tod.", "tokens": ["der", "sich", "in", "mich", "ver\u00b7kroch", ",", "der", "we\u00b7gert", "mir", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir w\u00e4re Sterben Lust; das will ja ganz nicht taugen;", "tokens": ["Mir", "w\u00e4\u00b7re", "Ster\u00b7ben", "Lust", ";", "das", "will", "ja", "ganz", "nicht", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NN", "$.", "PDS", "VMFIN", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil ich im Leben bin, so bin ich in der Not.", "tokens": ["weil", "ich", "im", "Le\u00b7ben", "bin", ",", "so", "bin", "ich", "in", "der", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Antlitz ist die Bank, darauf ich bin gestrecket,", "tokens": ["Dein", "Ant\u00b7litz", "ist", "die", "Bank", ",", "da\u00b7rauf", "ich", "bin", "ge\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da werd' ich aufgedehnt. Dein, was man himlisch nennt,", "tokens": ["da", "werd'", "ich", "auf\u00b7ge\u00b7dehnt", ".", "Dein", ",", "was", "man", "him\u00b7lisch", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$.", "PPOSAT", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "hat einmal in mein Herz' ein Feuer angestecket,", "tokens": ["hat", "ein\u00b7mal", "in", "mein", "Her\u00b7z'", "ein", "Feu\u00b7er", "an\u00b7ge\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "das mich entz\u00fcndet stets und nimmermehr verbrennt.", "tokens": ["das", "mich", "ent\u00b7z\u00fcn\u00b7det", "stets", "und", "nim\u00b7mer\u00b7mehr", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine