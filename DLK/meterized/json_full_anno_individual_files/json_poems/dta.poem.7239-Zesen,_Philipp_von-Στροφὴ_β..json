{"dta.poem.7239": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "\u03a3\u03c4\u03c1\u03bf\u03c6\u1f74 \u03b2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O du zier der sch\u00f6nen Jugend/", "tokens": ["O", "du", "zier", "der", "sch\u00f6\u00b7nen", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die von vns gewichen nun/", "tokens": ["die", "von", "vns", "ge\u00b7wi\u00b7chen", "nun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "vnd gesegnet vnser Thun", "tokens": ["vnd", "ge\u00b7seg\u00b7net", "vn\u00b7ser", "Thun"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wiltu nun der sch\u00f6nen Tugend/", "tokens": ["wil\u00b7tu", "nun", "der", "sch\u00f6\u00b7nen", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die der Himmel giebet dir/", "tokens": ["die", "der", "Him\u00b7mel", "gie\u00b7bet", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "dich befleissen f\u00fcr vnd f\u00fcr?", "tokens": ["dich", "be\u00b7fleis\u00b7sen", "f\u00fcr", "vnd", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ey so wil ich noch erschwingen", "tokens": ["Ey", "so", "wil", "ich", "noch", "er\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Stim\u0303\u2019 vnd letzlich singen", "tokens": ["Mei\u00b7ne", "Stim\u0303'", "vnd", "letz\u00b7lich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dir ein traurigs Grabelied/", "tokens": ["Dir", "ein", "trau\u00b7rigs", "Gra\u00b7be\u00b7lied", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "vnd den letzten dienst beweisen/", "tokens": ["vnd", "den", "letz\u00b7ten", "dienst", "be\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "weil von vns Du wollen reisen/", "tokens": ["weil", "von", "vns", "Du", "wol\u00b7len", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "O du Seeliges gem\u00fcth.", "tokens": ["O", "du", "See\u00b7li\u00b7ges", "ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}