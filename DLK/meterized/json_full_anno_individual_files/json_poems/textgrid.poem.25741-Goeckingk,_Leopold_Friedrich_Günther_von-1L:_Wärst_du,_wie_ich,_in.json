{"textgrid.poem.25741": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4rst du, wie ich, in ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4rst du, wie ich, in ", "tokens": ["W\u00e4rst", "du", ",", "wie", "ich", ",", "in"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "PPER", "$,", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Du h\u00e4ttest keine Messe noch geh\u00f6rt,", "tokens": ["Du", "h\u00e4t\u00b7test", "kei\u00b7ne", "Mes\u00b7se", "noch", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Wachslicht keinen Kreuzer noch verloren,", "tokens": ["F\u00fcr", "Wachs\u00b7licht", "kei\u00b7nen", "Kreu\u00b7zer", "noch", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Als das von Sanct Anton geehrt.", "tokens": ["Als", "das", "von", "Sanct", "An\u00b7ton", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NE", "NE", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Und h\u00e4tt' in ", "tokens": ["Und", "h\u00e4tt'", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ich h\u00e4tte ", "tokens": ["Ich", "h\u00e4t\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Geknieet vor dem Hochaltare,", "tokens": ["Ge\u00b7kni\u00b7e\u00b7et", "vor", "dem", "Hoc\u00b7hal\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "In meiner Hand den Rosenkranz.", "tokens": ["In", "mei\u00b7ner", "Hand", "den", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "W\u00e4r' aber auch, o ", "tokens": ["W\u00e4r'", "a\u00b7ber", "auch", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "ADV", "$,", "FM"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "An jedem Ort' im weiten Reich'", "tokens": ["An", "je\u00b7dem", "Ort'", "im", "wei\u00b7ten", "Reich'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Europens, aufgekeimet: unser Streben,", "tokens": ["Eu\u00b7ro\u00b7pens", ",", "auf\u00b7ge\u00b7kei\u00b7met", ":", "un\u00b7ser", "Stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den Kern im Inneren der Schaale", "tokens": ["Den", "Kern", "im", "In\u00b7ne\u00b7ren", "der", "Schaa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zu suchen, blieb sich immer gleich.", "tokens": ["Zu", "su\u00b7chen", ",", "blieb", "sich", "im\u00b7mer", "gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der eine Apfel spielt mit hundert Farben,", "tokens": ["Der", "ei\u00b7ne", "Ap\u00b7fel", "spielt", "mit", "hun\u00b7dert", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der andre hat ein Streifchen kaum,", "tokens": ["Der", "and\u00b7re", "hat", "ein", "Streif\u00b7chen", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und recht besehn, hat jeder seine Narben,", "tokens": ["Und", "recht", "be\u00b7sehn", ",", "hat", "je\u00b7der", "sei\u00b7ne", "Nar\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Doch alle gleichen Kern; denn alle", "tokens": ["Doch", "al\u00b7le", "glei\u00b7chen", "Kern", ";", "denn", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$.", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Trug Eine Wurzel und Ein Baum.", "tokens": ["Trug", "Ei\u00b7ne", "Wur\u00b7zel", "und", "Ein", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wir streiten uns nicht um die \u00e4u\u00dfre Schaale,", "tokens": ["Wir", "strei\u00b7ten", "uns", "nicht", "um", "die", "\u00e4u\u00df\u00b7re", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Wie Knaben sich um bunte Aepfel; frei", "tokens": ["Wie", "Kna\u00b7ben", "sich", "um", "bun\u00b7te", "A\u00b7e\u00b7pfel", ";", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "NN", "PRF", "APPR", "ADJA", "NN", "$.", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Steht dir und mir, da\u00df jeder sie sich male,", "tokens": ["Steht", "dir", "und", "mir", ",", "da\u00df", "je\u00b7der", "sie", "sich", "ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "$,", "KOUS", "PIS", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wie ihn es gut d\u00fcnkt, ja sich schmeichle,", "tokens": ["Wie", "ihn", "es", "gut", "d\u00fcnkt", ",", "ja", "sich", "schmeich\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$,", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df er der beste Maler sey.", "tokens": ["Da\u00df", "er", "der", "bes\u00b7te", "Ma\u00b7ler", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Gern wollen wir die Narben oder Flecken", "tokens": ["Gern", "wol\u00b7len", "wir", "die", "Nar\u00b7ben", "o\u00b7der", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und kleinen Warzen schweigend \u00fcbersehn,", "tokens": ["Und", "klei\u00b7nen", "War\u00b7zen", "schwei\u00b7gend", "\u00fc\u00b7ber\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Vor fremdem Blick' sorgf\u00e4ltig sie bedecken;", "tokens": ["Vor", "frem\u00b7dem", "Blick'", "sorg\u00b7f\u00e4l\u00b7tig", "sie", "be\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Denn, ohne Duldung, la\u00df zu Heiden", "tokens": ["Denn", ",", "oh\u00b7ne", "Dul\u00b7dung", ",", "la\u00df", "zu", "Hei\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUI", "NN", "$,", "VVIMP", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Uns weg von Afterchristen gehn!", "tokens": ["Uns", "weg", "von", "Af\u00b7ter\u00b7chris\u00b7ten", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Sie glei\u00dfen zwar von au\u00dfen, trotz dem Stiche", "tokens": ["Sie", "glei\u00b7\u00dfen", "zwar", "von", "au\u00b7\u00dfen", ",", "trotz", "dem", "Sti\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Des Wurms; allein zu Staub ward l\u00e4ngst ihr Kern,", "tokens": ["Des", "Wurms", ";", "al\u00b7lein", "zu", "Staub", "ward", "l\u00e4ngst", "ihr", "Kern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "APPR", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und wie der Ros' erquickende Ger\u00fcche", "tokens": ["Und", "wie", "der", "Ros'", "er\u00b7qui\u00b7cken\u00b7de", "Ge\u00b7r\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.34": {"text": "Lockt keiner so wie du, mein Lieber!", "tokens": ["Lockt", "kei\u00b7ner", "so", "wie", "du", ",", "mein", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "KOKOM", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Den matten Wanderer von fern.", "tokens": ["Den", "mat\u00b7ten", "Wan\u00b7de\u00b7rer", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Du w\u00fcrdest Vater deiner tausend Armen", "tokens": ["Du", "w\u00fcr\u00b7dest", "Va\u00b7ter", "dei\u00b7ner", "tau\u00b7send", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Gewi\u00df so gut in meinem ", "tokens": ["Ge\u00b7wi\u00df", "so", "gut", "in", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Als du zu ", "tokens": ["Als", "du", "zu"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Quillt wahrlich nicht aus Catechismen,", "tokens": ["Quillt", "wahr\u00b7lich", "nicht", "aus", "Cat\u00b7e\u00b7chis\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Quillt aus dem Herzen ganz allein.", "tokens": ["Quillt", "aus", "dem", "Her\u00b7zen", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Ich liebe dich vor tausend meiner Br\u00fcder,", "tokens": ["Ich", "lie\u00b7be", "dich", "vor", "tau\u00b7send", "mei\u00b7ner", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "CARD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Denn sind die ", "tokens": ["Denn", "sind", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "Der wahren unsichtbaren Kirche Glieder,", "tokens": ["Der", "wah\u00b7ren", "un\u00b7sicht\u00b7ba\u00b7ren", "Kir\u00b7che", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "So ist die unsichtbare Kirche", "tokens": ["So", "ist", "die", "un\u00b7sicht\u00b7ba\u00b7re", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Nur ein ersonnenes Gedicht.", "tokens": ["Nur", "ein", "er\u00b7son\u00b7ne\u00b7nes", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4rst du, wie ich, in ", "tokens": ["W\u00e4rst", "du", ",", "wie", "ich", ",", "in"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "PPER", "$,", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Du h\u00e4ttest keine Messe noch geh\u00f6rt,", "tokens": ["Du", "h\u00e4t\u00b7test", "kei\u00b7ne", "Mes\u00b7se", "noch", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Wachslicht keinen Kreuzer noch verloren,", "tokens": ["F\u00fcr", "Wachs\u00b7licht", "kei\u00b7nen", "Kreu\u00b7zer", "noch", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Als das von Sanct Anton geehrt.", "tokens": ["Als", "das", "von", "Sanct", "An\u00b7ton", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NE", 