{"dta.poem.227": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er z\u00fcrnet mit sich/ da\u00df er so wanckelm\u00fctig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wo denckt ihr hin/ ihr fl\u00fcchtigen Gedancken?", "tokens": ["Wo", "denckt", "ihr", "hin", "/", "ihr", "fl\u00fcch\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch Unbestand nimmt euch/ ihr Sinnen/ ein?", "tokens": ["Welch", "Un\u00b7be\u00b7stand", "nimmt", "euch", "/", "ihr", "Sin\u00b7nen", "/", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heist dieses wol treu und best\u00e4ndig seyn?", "tokens": ["Heist", "die\u00b7ses", "wol", "treu", "und", "be\u00b7st\u00e4n\u00b7dig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald hie/ und denn bald dort hinaus zu wancken.", "tokens": ["Bald", "hie", "/", "und", "denn", "bald", "dort", "hin\u00b7aus", "zu", "wan\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KON", "ADV", "ADV", "ADV", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was n\u00fctzet euch die falsche Heucheley?", "tokens": ["Was", "n\u00fct\u00b7zet", "euch", "die", "fal\u00b7sche", "Heu\u00b7che\u00b7ley", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie! wenn ihr lieben wolt/ so liebet treu.", "tokens": ["Wie", "!", "wenn", "ihr", "lie\u00b7ben", "wolt", "/", "so", "lie\u00b7bet", "treu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So liebet treu/ mein Gl\u00fcck ist noch verhanden/", "tokens": ["So", "lie\u00b7bet", "treu", "/", "mein", "Gl\u00fcck", "ist", "noch", "ver\u00b7han\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wetter/ so ihr Antlitz \u00fcberzieht", "tokens": ["Das", "Wet\u00b7ter", "/", "so", "ihr", "Ant\u00b7litz", "\u00fc\u00b7berz\u00b7ieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird nicht best\u00e4ndig seyn/ ihr Zorn entflieht", "tokens": ["Wird", "nicht", "be\u00b7st\u00e4n\u00b7dig", "seyn", "/", "ihr", "Zorn", "ent\u00b7flieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADJD", "VAINF", "$(", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So schleunig als er bey dir ist entstanden/", "tokens": ["So", "schleu\u00b7nig", "als", "er", "bey", "dir", "ist", "ent\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "APPR", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geduldet euch nur eine kleine Zeit/", "tokens": ["Ge\u00b7dul\u00b7det", "euch", "nur", "ei\u00b7ne", "klei\u00b7ne", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wird zum Ende seyn euer hartes Leid.", "tokens": ["So", "wird", "zum", "En\u00b7de", "seyn", "eu\u00b7er", "har\u00b7tes", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "VAINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Eur hartes Leid wird sich in Lust verkehren", "tokens": ["Eur", "har\u00b7tes", "Leid", "wird", "sich", "in", "Lust", "ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey Rosen lassen sich die Dornen sehn", "tokens": ["Bey", "Ro\u00b7sen", "las\u00b7sen", "sich", "die", "Dor\u00b7nen", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So kan man auch nicht stets auf Sammet gehn", "tokens": ["So", "kan", "man", "auch", "nicht", "stets", "auf", "Sam\u00b7met", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "PTKNEG", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe will nicht immer Lust verehren/", "tokens": ["Die", "Lie\u00b7be", "will", "nicht", "im\u00b7mer", "Lust", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zuweilen sie auch scharffe Salsen giebt/", "tokens": ["Zu\u00b7wei\u00b7len", "sie", "auch", "scharf\u00b7fe", "Sal\u00b7sen", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und pr\u00fcfet uns/ ob man best\u00e4ndig lieb\u2019t", "tokens": ["Und", "pr\u00fc\u00b7fet", "uns", "/", "ob", "man", "be\u00b7st\u00e4n\u00b7dig", "lie\u00b7b't"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Best\u00e4ndig liebt ein recht verliebtes Hertze/", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "liebt", "ein", "recht", "ver\u00b7lieb\u00b7tes", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJD", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es wancket nicht ob schon ein Sturm entsteht/", "tokens": ["Es", "wan\u00b7cket", "nicht", "ob", "schon", "ein", "Sturm", "ent\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn alles neben ihm zu Grunde geht.", "tokens": ["Wenn", "al\u00b7les", "ne\u00b7ben", "ihm", "zu", "Grun\u00b7de", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "So brennet doch in ihm die Liebes-Kertze", "tokens": ["So", "bren\u00b7net", "doch", "in", "ihm", "die", "Lie\u00b7bes\u00b7Kert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die keine Noht/ wie gro\u00df sie ist ausbl\u00e4\u00dft/", "tokens": ["Die", "kei\u00b7ne", "Noht", "/", "wie", "gro\u00df", "sie", "ist", "aus\u00b7bl\u00e4\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis da\u00df den matten Leib der Geist verl\u00e4st.", "tokens": ["Bis", "da\u00df", "den", "mat\u00b7ten", "Leib", "der", "Geist", "ver\u00b7l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Geist verl\u00e4st/ offt eh des Leibes-H\u00f6le", "tokens": ["Der", "Geist", "ver\u00b7l\u00e4st", "/", "offt", "eh", "des", "Lei\u00b7bes\u00b7H\u00f6le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als da\u00df er seine treue Liebe bricht", "tokens": ["Als", "da\u00df", "er", "sei\u00b7ne", "treu\u00b7e", "Lie\u00b7be", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohin man einst den steiffen Sinn gericht", "tokens": ["Wo\u00b7hin", "man", "einst", "den", "steif\u00b7fen", "Sinn", "ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dahin verlanget immerfort die Seele", "tokens": ["Da\u00b7hin", "ver\u00b7lan\u00b7get", "im\u00b7mer\u00b7fort", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich f\u00fchle auch das angenehme Strick", "tokens": ["Ich", "f\u00fch\u00b7le", "auch", "das", "an\u00b7ge\u00b7neh\u00b7me", "Strick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das nach ", "tokens": ["Das", "nach"], "token_info": ["word", "word"], "pos": ["PDS", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Nun zieht zur\u00fcck/ bleibt immer fort best\u00e4ndig", "tokens": ["Nun", "zieht", "zu\u00b7r\u00fcck", "/", "bleibt", "im\u00b7mer", "fort", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$(", "VVFIN", "ADV", "PTKVZ", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sieger wird erst nach der Schlacht gekr\u00f6hnt", "tokens": ["Der", "Sie\u00b7ger", "wird", "erst", "nach", "der", "Schlacht", "ge\u00b7kr\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer sich recht nach dem Sieges-Krantze sehnt", "tokens": ["Wer", "sich", "recht", "nach", "dem", "Sie\u00b7ges\u00b7Krant\u00b7ze", "sehnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den machet nichts von seinem Zweck abwendig.", "tokens": ["Den", "ma\u00b7chet", "nichts", "von", "sei\u00b7nem", "Zweck", "ab\u00b7wen\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weil ich nun auch ein ", "tokens": ["Weil", "ich", "nun", "auch", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "So flieht die Meuterey/ wo denckt ihr hin?", "tokens": ["So", "flieht", "die", "Meu\u00b7te\u00b7rey", "/", "wo", "denckt", "ihr", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}