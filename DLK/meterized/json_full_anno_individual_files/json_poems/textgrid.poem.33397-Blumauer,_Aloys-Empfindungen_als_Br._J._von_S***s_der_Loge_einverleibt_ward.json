{"textgrid.poem.33397": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Empfindungen als Br. J. von S***s der Loge einverleibt ward", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr, Br\u00fcder, einen \u00e4chten Maurer kennen,", "tokens": ["Wollt", "ihr", ",", "Br\u00fc\u00b7der", ",", "ei\u00b7nen", "\u00e4ch\u00b7ten", "Mau\u00b7rer", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "H\u00f6ret, was er spricht, und sehet, wie er baut;", "tokens": ["H\u00f6\u00b7ret", ",", "was", "er", "spricht", ",", "und", "se\u00b7het", ",", "wie", "er", "baut", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Wahrheit wird er seine Schwester nennen.", "tokens": ["Wahr\u00b7heit", "wird", "er", "sei\u00b7ne", "Schwes\u00b7ter", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Menschenliebe seine Braut.", "tokens": ["Und", "die", "Men\u00b7schen\u00b7lie\u00b7be", "sei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Vor die Wahrheit hin wird er sich stellen", "tokens": ["Vor", "die", "Wahr\u00b7heit", "hin", "wird", "er", "sich", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Mit dem Schilde der Entschlossenheit:", "tokens": ["Mit", "dem", "Schil\u00b7de", "der", "Ent\u00b7schlos\u00b7sen\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aberglaube, Dummheit, List und Neid", "tokens": ["A\u00b7berg\u00b7lau\u00b7be", ",", "Dumm\u00b7heit", ",", "List", "und", "Neid"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "M\u00f6gen sich daran ihr Haupt zerschellen,", "tokens": ["M\u00f6\u00b7gen", "sich", "da\u00b7ran", "ihr", "Haupt", "zer\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Doch sein Fu\u00df weicht nicht ein H\u00e4rchen breit.", "tokens": ["Doch", "sein", "Fu\u00df", "weicht", "nicht", "ein", "H\u00e4r\u00b7chen", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Laut auf, Br\u00fcder, wird er rufen", "tokens": ["Laut", "auf", ",", "Br\u00fc\u00b7der", ",", "wird", "er", "ru\u00b7fen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PTKVZ", "$,", "NN", "$,", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "F\u00fcr die Menschheit, laut bis hin zum Thron,", "tokens": ["F\u00fcr", "die", "Menschheit", ",", "laut", "bis", "hin", "zum", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "APPR", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Und verhallt sein Wort gleich an des Thrones Stufen,", "tokens": ["Und", "ver\u00b7hallt", "sein", "Wort", "gleich", "an", "des", "Thro\u00b7nes", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Nein, er schweigt nicht, er verdoppelt seinen Ton.", "tokens": ["Nein", ",", "er", "schweigt", "nicht", ",", "er", "ver\u00b7dop\u00b7pelt", "sei\u00b7nen", "Ton", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Den Belohner und den R\u00e4cher", "tokens": ["Den", "Be\u00b7loh\u00b7ner", "und", "den", "R\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Lehrt er, wie den B\u00fcrger, seine Pflicht,", "tokens": ["Lehrt", "er", ",", "wie", "den", "B\u00fcr\u00b7ger", ",", "sei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Doch vergi\u00dft er selbst in dem Verbrecher,", "tokens": ["Doch", "ver\u00b7gi\u00dft", "er", "selbst", "in", "dem", "Ver\u00b7bre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Den die Strafe trifft \u2013 den Menschen nicht!", "tokens": ["Den", "die", "Stra\u00b7fe", "trifft", "\u2013", "den", "Men\u00b7schen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Vorurtheile sucht er zu zerstreuen,", "tokens": ["Vor\u00b7urt\u00b7hei\u00b7le", "sucht", "er", "zu", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Bricht der Wahrheit eb'ne Bahn,", "tokens": ["Bricht", "der", "Wahr\u00b7heit", "eb'\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "L\u00e4chelt, wenn der Finsternisse Kinder schreien,", "tokens": ["L\u00e4\u00b7chelt", ",", "wenn", "der", "Fins\u00b7ter\u00b7nis\u00b7se", "Kin\u00b7der", "schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Und ihn schimpfend \u2013 doch dem Licht sich nah'n. \u2013", "tokens": ["Und", "ihn", "schimp\u00b7fend", "\u2013", "doch", "dem", "Licht", "sich", "nah'", "n.", "\u2013"], "token_info": ["wo