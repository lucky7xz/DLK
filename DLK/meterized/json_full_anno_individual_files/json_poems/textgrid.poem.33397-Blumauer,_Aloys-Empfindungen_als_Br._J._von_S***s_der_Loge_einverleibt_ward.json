 ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "L\u00e4chelt, wenn der Finsternisse Kinder schreien,", "tokens": ["L\u00e4\u00b7chelt", ",", "wenn", "der", "Fins\u00b7ter\u00b7nis\u00b7se", "Kin\u00b7der", "schrei\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Und ihn schimpfend \u2013 doch dem Licht sich nah'n. \u2013", "tokens": ["Und", "ihn", "schimp\u00b7fend", "\u2013", "doch", "dem", "Licht", "sich", "nah'", "n.", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["KON", "PPER", "ADJD", "$(", "ADV", "ART", "NN", "PRF", "APPR", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Diesen Kampf der Menschlichkeit und Wahrheit", "tokens": ["Die\u00b7sen", "Kampf", "der", "Menschlich\u00b7keit", "und", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "K\u00e4mpfte der, der nun f\u00fcr uns auch lebt,", "tokens": ["K\u00e4mpf\u00b7te", "der", ",", "der", "nun", "f\u00fcr", "uns", "auch", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Und bis zu des ew'gen Lichtes Klarheit", "tokens": ["Und", "bis", "zu", "des", "ew'\u00b7gen", "Lich\u00b7tes", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Hand in Hand mit uns zu dringen strebt.", "tokens": ["Hand", "in", "Hand", "mit", "uns", "zu", "drin\u00b7gen", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Menschenlieb' und Wahrheit bringen", "tokens": ["Men\u00b7schen\u00b7lieb'", "und", "Wahr\u00b7heit", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Diesen Bruder in den Arm uns heut,", "tokens": ["Die\u00b7sen", "Bru\u00b7der", "in", "den", "Arm", "uns", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Und der Knoten, den zwei solche Bande schlingen,", "tokens": ["Und", "der", "Kno\u00b7ten", ",", "den", "zwei", "sol\u00b7che", "Ban\u00b7de", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "CARD", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.29": {"text": "Sichert uns auch steter Ewigkeit.", "tokens": ["Si\u00b7chert", "uns", "auch", "ste\u00b7ter", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}