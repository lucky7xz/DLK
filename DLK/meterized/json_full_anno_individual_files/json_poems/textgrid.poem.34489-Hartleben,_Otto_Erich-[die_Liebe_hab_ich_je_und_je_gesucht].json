{"textgrid.poem.34489": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[die Liebe hab ich je und je gesucht]", "genre": "verse", "period": "N.A.", "pub_year": 1888, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe hab ich je und je gesucht,", "tokens": ["Die", "Lie\u00b7be", "hab", "ich", "je", "und", "je", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in der ich ruhen darf und mich verlieren", "tokens": ["in", "der", "ich", "ru\u00b7hen", "darf", "und", "mich", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und w\u00e4hnen, niemals mehr allein zu sein.", "tokens": ["und", "w\u00e4h\u00b7nen", ",", "nie\u00b7mals", "mehr", "al\u00b7lein", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADV", "ADV", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 So war es deine Liebe, die ich suchte?", "tokens": ["\u2013", "So", "war", "es", "dei\u00b7ne", "Lie\u00b7be", ",", "die", "ich", "such\u00b7te", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja \u2013 lege deine weiche, kleine Hand", "tokens": ["Ja", "\u2013", "le\u00b7ge", "dei\u00b7ne",