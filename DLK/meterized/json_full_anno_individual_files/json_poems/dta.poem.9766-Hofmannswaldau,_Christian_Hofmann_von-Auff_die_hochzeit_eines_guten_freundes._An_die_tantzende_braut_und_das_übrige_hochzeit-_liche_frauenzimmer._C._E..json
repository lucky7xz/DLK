{"dta.poem.9766": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff die hochzeit eines guten freundes.  \n An die tantzende braut/ und das \u00fcbrige hochzeit-  \n liche frauenzimmer.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "hu:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tantzt! g\u00fcldne Nymphen/ tantzt! hat alles seine zeit/", "tokens": ["Tantzt", "!", "g\u00fcld\u00b7ne", "Nym\u00b7phen", "/", "tantzt", "!", "hat", "al\u00b7les", "sei\u00b7ne", "zeit", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "NN", "$(", "VVFIN", "$.", "VAFIN", "PIS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist nun tantzens-zeit/ da sich die sterne neigen/", "tokens": ["So", "ist", "nun", "tant\u00b7zens\u00b7zeit", "/", "da", "sich", "die", "ster\u00b7ne", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$(", "KOUS", "PRF", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zur gew\u00fcnschten nacht euch nichts denn fr\u00f6ligkeit", "tokens": ["Und", "zur", "ge\u00b7w\u00fcnschten", "nacht", "euch", "nichts", "denn", "fr\u00f6\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "PPER", "PIS", "KON", "NN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Bey dieser hochzeit-lust aus offnem himmel zeigen.", "tokens": ["Bey", "die\u00b7ser", "hoch\u00b7zeit\u00b7lust", "aus", "off\u00b7nem", "him\u00b7mel", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr sonnen die ihr steht/ wenn mond und sonne f\u00e4llt/", "tokens": ["Ihr", "son\u00b7nen", "die", "ihr", "steht", "/", "wenn", "mond", "und", "son\u00b7ne", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PRELS", "PPER", "VVFIN", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und noch die strahlen zeigt/ wenn alles licht verschwindet.", "tokens": ["Und", "noch", "die", "strah\u00b7len", "zeigt", "/", "wenn", "al\u00b7les", "licht", "ver\u00b7schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$(", "KOUS", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zerschmeltzet itzt den schnee der annoch freyen welt/", "tokens": ["Zer\u00b7schmelt\u00b7zet", "itzt", "den", "schnee", "der", "an\u00b7noch", "frey\u00b7en", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die sich schon halb ger\u00fchrt in euren strrcken findet.", "tokens": ["Die", "sich", "schon", "halb", "ge\u00b7r\u00fchrt", "in", "eu\u00b7ren", "strr\u00b7cken", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bewaffnet eur gesicht mit stiller freundligkeit/", "tokens": ["Be\u00b7waff\u00b7net", "eur", "ge\u00b7sicht", "mit", "stil\u00b7ler", "freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00dft die verdeckte glut durch mund und augen spielen/", "tokens": ["La\u00dft", "die", "ver\u00b7deck\u00b7te", "glut", "durch", "mund", "und", "au\u00b7gen", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ieden/ der sich euch zu widersetzen dr\u00e4ut/", "tokens": ["Und", "ie\u00b7den", "/", "der", "sich", "euch", "zu", "wi\u00b7der\u00b7set\u00b7zen", "dr\u00e4ut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$(", "PRELS", "PRF", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den s\u00fcssen wunder-brandt in blut und adern f\u00fchlen.", "tokens": ["Den", "s\u00fcs\u00b7sen", "wun\u00b7der\u00b7brandt", "in", "blut", "und", "a\u00b7dern", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Tantzt! tantzt! Jhr Gratien/ la\u00dft eurer glieder pracht/", "tokens": ["Tantzt", "!", "tantzt", "!", "Ihr", "Gra\u00b7ti\u00b7en", "/", "la\u00dft", "eu\u00b7rer", "glie\u00b7der", "pracht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$.", "PPER", "NE", "$(", "VVIMP", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und den geschickten leib bi\u00df zur verwundrung schauen?", "tokens": ["Und", "den", "ge\u00b7schick\u00b7ten", "leib", "bi\u00df", "zur", "ver\u00b7wund\u00b7rung", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach! aber gebt zugleich auf eure freyheit acht;", "tokens": ["Ach", "!", "a\u00b7ber", "gebt", "zu\u00b7gleich", "auf", "eu\u00b7re", "frey\u00b7heit", "acht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem guten Wetter darff man nicht zu sicher trauen.", "tokens": ["Dem", "gu\u00b7ten", "Wet\u00b7ter", "darff", "man", "nicht", "zu", "si\u00b7cher", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "PTKNEG", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jhr tantzet nicht allein; Cupido sieht euch zu/", "tokens": ["Ihr", "tant\u00b7zet", "nicht", "al\u00b7lein", ";", "Cu\u00b7pi\u00b7do", "sieht", "euch", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$.", "NE", "VVFIN", "PPER", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und Venus selber ist in euren reihen kommen:", "tokens": ["Und", "Ve\u00b7nus", "sel\u00b7ber", "ist", "in", "eu\u00b7ren", "rei\u00b7hen", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie hat wol ehermals den Nymphen ihre ruh/", "tokens": ["Sie", "hat", "wol", "e\u00b7her\u00b7mals", "den", "Nym\u00b7phen", "ih\u00b7re", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Cupido aber gar die hertzen mit genommen.", "tokens": ["Cu\u00b7pi\u00b7do", "a\u00b7ber", "gar", "die", "hert\u00b7zen", "mit", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jetzt wirfft er angeln aus. Hier legt er netz und strick;", "tokens": ["Jetzt", "wirfft", "er", "an\u00b7geln", "aus", ".", "Hier", "legt", "er", "netz", "und", "strick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "NE", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dort aber sucht er euch verschmitzt ins garn zu bringen.", "tokens": ["Dort", "a\u00b7ber", "sucht", "er", "euch", "ver\u00b7schmitzt", "ins", "garn", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es ist um euch gethan/ wofern ihm dieser t\u00fcck/", "tokens": ["Es", "ist", "um", "euch", "ge\u00b7than", "/", "wo\u00b7fern", "ihm", "die\u00b7ser", "t\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$(", "KOUS", "PPER", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und solch ein meister-streich nach wunsche wird gelingen.", "tokens": ["Und", "solch", "ein", "meis\u00b7ter\u00b7streich", "nach", "wun\u00b7sche", "wird", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJD", "APPR", "ADJA", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was aber sorgt ihr viel? ach! sch\u00f6nste/ gebt euch doch!", "tokens": ["Was", "a\u00b7ber", "sorgt", "ihr", "viel", "?", "ach", "!", "sch\u00f6ns\u00b7te", "/", "gebt", "euch", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "$.", "XY", "$.", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr seyd die ersten nicht/ die diese regung kennen;", "tokens": ["Ihr", "seyd", "die", "ers\u00b7ten", "nicht", "/", "die", "die\u00b7se", "re\u00b7gung", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PTKNEG", "$(", "ART", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die braut zieht selber mit am s\u00fcssen liebes-joch/", "tokens": ["Die", "braut", "zieht", "sel\u00b7ber", "mit", "am", "s\u00fcs\u00b7sen", "lie\u00b7bes\u00b7joch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "ADV", "APPR", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der zur ergetzung heut die hochzeit-fackeln brennen;", "tokens": ["Der", "zur", "er\u00b7get\u00b7zung", "heut", "die", "hoch\u00b7zeit\u00b7fa\u00b7ckeln", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die flamme keuscher brunst/ die ihre seele r\u00fchrt/", "tokens": ["Die", "flam\u00b7me", "keu\u00b7scher", "brunst", "/", "die", "ih\u00b7re", "see\u00b7le", "r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wird sie/ ihr werdt es sehn/ noch diesen abend b\u00fcssen;", "tokens": ["Wird", "sie", "/", "ihr", "werdt", "es", "sehn", "/", "noch", "die\u00b7sen", "a\u00b7bend", "b\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "PPER", "VAFIN", "PPER", "VVINF", "$(", "ADV", "PDAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jhr/ und nicht sie allein/ werd itzt mit auffgef\u00fchrt/", "tokens": ["Ihr", "/", "und", "nicht", "sie", "al\u00b7lein", "/", "werd", "itzt", "mit", "auff\u00b7ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KON", "PTKNEG", "PPER", "ADV", "$(", "VAFIN", "ADV", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie tantzet nur voran/ ihr werdet folgen m\u00fcssen.", "tokens": ["Sie", "tant\u00b7zet", "nur", "vo\u00b7ran", "/", "ihr", "wer\u00b7det", "fol\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$(", "PPER", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mich deucht/ ich sp\u00fcr auch schon/ da\u00df Venus endlich siegt/", "tokens": ["Mich", "deucht", "/", "ich", "sp\u00fcr", "auch", "schon", "/", "da\u00df", "Ve\u00b7nus", "end\u00b7lich", "siegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "ADV", "$(", "KOUS", "NE", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und da\u00df ihr kleiner sohn nicht alle macht verlohren.", "tokens": ["Und", "da\u00df", "ihr", "klei\u00b7ner", "sohn", "nicht", "al\u00b7le", "macht", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "PIS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Seht! seht! wie eiffrig man sich zur Melinden f\u00fcgt;", "tokens": ["Seht", "!", "seht", "!", "wie", "eif\u00b7frig", "man", "sich", "zur", "Me\u00b7lin\u00b7den", "f\u00fcgt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$.", "PWAV", "ADJD", "PIS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Wie hier wird lieb und gunst/ dort treu und huld geschworen.", "tokens": ["Wie", "hier", "wird", "lieb", "und", "gunst", "/", "dort", "treu", "und", "huld", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ADJD", "KON", "NN", "$(", "ADV", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie man die Dorilis um ihre neigung bitt/", "tokens": ["Wie", "man", "die", "Do\u00b7ri\u00b7lis", "um", "ih\u00b7re", "nei\u00b7gung", "bitt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und bey Auroren sich l\u00e4st keine m\u00fch verdriessen;", "tokens": ["Und", "bey", "Au\u00b7ro\u00b7ren", "sich", "l\u00e4st", "kei\u00b7ne", "m\u00fch", "ver\u00b7dries\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "PRF", "VVFIN", "PIAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie man dort Sylvien verfolgt auff ieden schritt/", "tokens": ["Wie", "man", "dort", "Syl\u00b7vi\u00b7en", "ver\u00b7folgt", "auff", "ie\u00b7den", "schritt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "NE", "VVFIN", "APPR", "PIS", "VVFIN", "$("], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Und ietzt die h\u00e4nde dr\u00fcckt/ ietzt sucht den mund zu k\u00fcssen.", "tokens": ["Und", "ietzt", "die", "h\u00e4n\u00b7de", "dr\u00fcckt", "/", "ietzt", "sucht", "den", "mund", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "VVFIN", "$(", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die glut bemeistert schon der augen s\u00fcsses licht/", "tokens": ["Die", "glut", "be\u00b7meis\u00b7tert", "schon", "der", "au\u00b7gen", "s\u00fcs\u00b7ses", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die funcken \u00e4ussern sich/ die in der seelen stecken.", "tokens": ["Die", "fun\u00b7cken", "\u00e4us\u00b7sern", "sich", "/", "die", "in", "der", "see\u00b7len", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Orante thut verliebt/ sie lacht und scheut sich nicht", "tokens": ["O\u00b7ran\u00b7te", "thut", "ver\u00b7liebt", "/", "sie", "lacht", "und", "scheut", "sich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "VVPP", "$(", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Den gantz geheimen brand durch blicke zu entdecken.", "tokens": ["Den", "gantz", "ge\u00b7hei\