{"textgrid.poem.33964": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen Nachts vor'm Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor'm Spiegel steht sie, die sch\u00f6ne Maid,", "tokens": ["Vor'm", "Spie\u00b7gel", "steht", "sie", ",", "die", "sch\u00f6\u00b7ne", "Maid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei n\u00e4chtlicher Zeit,", "tokens": ["Bei", "n\u00e4cht\u00b7li\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und spricht in magdlichem Scherze,", "tokens": ["Und", "spricht", "in", "magd\u00b7li\u00b7chem", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Indem sie den eigenen Reiz beschaut:", "tokens": ["In\u00b7dem", "sie", "den", "ei\u00b7ge\u00b7nen", "Reiz", "be\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wann werd' ich Braut? \u2013", "tokens": ["Wann", "werd'", "ich", "Braut", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf einmal erlischt da die Kerze.", "tokens": ["Auf", "ein\u00b7mal", "er\u00b7lischt", "da", "die", "Ker\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und als nun die Nacht ihr Bild verschluckt,", "tokens": ["Und", "als", "nun", "die", "Nacht", "ihr", "Bild", "ver\u00b7schluckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Da wird sie durchzuckt", "tokens": ["Da", "wird", "sie", "durch\u00b7zuckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Von einem ahnenden Schmerze,", "tokens": ["Von", "ei\u00b7nem", "ah\u00b7nen\u00b7den", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr ist, als ob ihr der finstre Tod", "tokens": ["Ihr", "ist", ",", "als", "ob", "ihr", "der", "finst\u00b7re", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Arm jetzt bot", "tokens": ["Den", "Arm", "jetzt", "bot"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Gott befiehlt sich ihr Herze.", "tokens": ["Und", "Gott", "be\u00b7fiehlt", "sich", "ihr", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "PPER", "VVFIN", "$."], "meter":