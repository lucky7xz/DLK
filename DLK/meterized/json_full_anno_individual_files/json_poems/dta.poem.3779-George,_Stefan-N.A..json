{"dta.poem.3779": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Neidisch der schlummer mich mied", "tokens": ["Nei\u00b7disch", "der", "schlum\u00b7mer", "mich", "mied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "So bringt keine wundersager", "tokens": ["So", "bringt", "kei\u00b7ne", "wun\u00b7der\u00b7sa\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So will ich kein lullendes lied", "tokens": ["So", "will", "ich", "kein", "lul\u00b7len\u00b7des", "lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der m\u00e4dchen attischer lande", "tokens": ["Der", "m\u00e4d\u00b7chen", "at\u00b7ti\u00b7scher", "lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Was mir vor monden gefiel \u00b7", "tokens": ["Was", "mir", "vor", "mon\u00b7den", "ge\u00b7fiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "XY"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Nun schlingt mich in eure bande", "tokens": ["Nun", "schlingt", "mich", "in", "eu\u00b7re", "ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Fl\u00f6tenspieler vom Nil", "tokens": ["Fl\u00f6\u00b7ten\u00b7spie\u00b7ler", "vom", "Nil"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich lag in \u00e4thergezelten", "tokens": ["Ich", "lag", "in", "\u00e4t\u00b7her\u00b7ge\u00b7zel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ich ass von himmlischem brod", "tokens": ["Ich", "ass", "von", "himm\u00b7li\u00b7schem", "brod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihr sanget die flucht aus den welten", "tokens": ["Ihr", "san\u00b7get", "die", "flucht", "aus", "den", "wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ihr sanget vom glorreichen tod", "tokens": ["Ihr", "san\u00b7get", "vom", "glor\u00b7rei\u00b7chen", "tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bevor die brennenden lider", "tokens": ["Be\u00b7vor", "die", "bren\u00b7nen\u00b7den", "li\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Endlicher schlummer befiel \u00b7", "tokens": ["End\u00b7li\u00b7cher", "schlum\u00b7mer", "be\u00b7fiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "XY"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Entr\u00fcckt und t\u00f6tet mich wieder", "tokens": ["Ent\u00b7r\u00fcckt", "und", "t\u00f6\u00b7tet", "mich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Fl\u00f6tenspieler vom Nil", "tokens": ["Fl\u00f6\u00b7ten\u00b7spie\u00b7ler", "vom", "Nil"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}