, ",", "halt", "aus", "und", "denck", "an", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "KON", "VVIMP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gedencke der vergangnen Tage,", "tokens": ["Ge\u00b7den\u00b7cke", "der", "ver\u00b7gang\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie manches Creuz, wie manche List,", "tokens": ["Wie", "man\u00b7ches", "Creuz", ",", "wie", "man\u00b7che", "List", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie manche Lust, wie manche Plage", "tokens": ["Wie", "man\u00b7che", "Lust", ",", "wie", "man\u00b7che", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bereits damit vergangen ist;", "tokens": ["Be\u00b7reits", "da\u00b7mit", "ver\u00b7gan\u00b7gen", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gedenck an Altan, Hof und Herd,", "tokens": ["Ge\u00b7denck", "an", "Al\u00b7tan", ",", "Hof", "und", "Herd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wobey sich dir mein Herz erkl\u00e4rt.", "tokens": ["Wo\u00b7bey", "sich", "dir", "mein", "Herz", "er\u00b7k