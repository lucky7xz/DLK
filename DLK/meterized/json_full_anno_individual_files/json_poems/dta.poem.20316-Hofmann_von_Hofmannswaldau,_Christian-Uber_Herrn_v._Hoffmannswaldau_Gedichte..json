{"dta.poem.20316": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Uber Herrn v. Hoffmannswaldau  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich gestorben bin/ so merckt den letzten willen/", "tokens": ["Wenn", "ich", "ge\u00b7stor\u00b7ben", "bin", "/", "so", "merckt", "den", "letz\u00b7ten", "wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scharrt mich/ wie ihr mich findt/ in Hoffmanns schrifften ein/", "tokens": ["Scharrt", "mich", "/", "wie", "ihr", "mich", "findt", "/", "in", "Hoff\u00b7manns", "schriff\u00b7ten", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWAV", "PPER", "PRF", "VVFIN", "$(", "APPR", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn dadurch werdet ihr den eintzgen wunsch erf\u00fcllen:", "tokens": ["Denn", "da\u00b7durch", "wer\u00b7det", "ihr", "den", "eintz\u00b7gen", "wunsch", "er\u00b7f\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich werde aufferweckt und nicht begraben seyn.", "tokens": ["Ich", "wer\u00b7de", "auf\u00b7fer\u00b7weckt", "und", "nicht", "be\u00b7gra\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht wirds ziemlich lang bi\u00df jener tag erscheinet/", "tokens": ["Viel\u00b7leicht", "wirds", "ziem\u00b7lich", "lang", "bi\u00df", "je\u00b7ner", "tag", "er\u00b7schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bleibt mir dieses buch der beste zeitvertreib/", "tokens": ["So", "bleibt", "mir", "die\u00b7ses", "buch", "der", "bes\u00b7te", "zeit\u00b7ver\u00b7treib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der wird mir unrecht thun/ der meinen tod beweinet/", "tokens": ["Der", "wird", "mir", "un\u00b7recht", "thun", "/", "der", "mei\u00b7nen", "tod", "be\u00b7wei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "VVINF", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wisst: Hoffmanns hoher geist beseelt den kalten leib.", "tokens": ["Wisst", ":", "Hoff\u00b7manns", "ho\u00b7her", "geist", "be\u00b7seelt", "den", "kal\u00b7ten", "leib", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NE", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}