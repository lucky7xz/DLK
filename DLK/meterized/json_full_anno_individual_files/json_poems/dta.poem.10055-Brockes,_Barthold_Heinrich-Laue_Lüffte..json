{"dta.poem.10055": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Laue L\u00fcffte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich mit Bedachtsamkeit, billig bey mir \u00fcberlege,", "tokens": ["Da", "ich", "mit", "Be\u00b7dacht\u00b7sam\u00b7keit", ",", "bil\u00b7lig", "bey", "mir", "\u00fc\u00b7berl\u00b7e\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie die Lufft anietzt so sanft, lieblich, angenehm und", "tokens": ["Wie", "die", "Lufft", "an\u00b7ietzt", "so", "sanft", ",", "lieb\u00b7lich", ",", "an\u00b7ge\u00b7nehm", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON"], "meter": "-++-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Und, indem ich es bedenck\u2019, auch zugleich dabey erwege", "tokens": ["Und", ",", "in\u00b7dem", "ich", "es", "be\u00b7denck", ",", "auch", "zu\u00b7gleich", "da\u00b7bey", "er\u00b7we\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "PTKVZ", "$,", "ADV", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Jhr so scharff- und rauhes schneiden, welches ich im Winter", "tokens": ["Ihr", "so", "scha\u00b7rff", "und", "rau\u00b7hes", "schnei\u00b7den", ",", "wel\u00b7ches", "ich", "im", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "TRUNC", "KON", "ADJA", "ADJA", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Zeigt der Unterschied mit Recht, welch ein Gl\u00fcck ich ietzt", "tokens": ["Zeigt", "der", "Un\u00b7ter\u00b7schied", "mit", "Recht", ",", "welch", "ein", "Gl\u00fcck", "ich", "ietzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "PWAT", "ART", "NN", "PPER", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Da kein gar zu strenger Frost, keine gar zu matte Hitze", "tokens": ["Da", "kein", "gar", "zu", "stren\u00b7ger", "Frost", ",", "kei\u00b7ne", "gar", "zu", "mat\u00b7te", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADV", "APPR", "ADJA", "NN", "$,", "PIAT", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Mir die Lung\u2019 und Brust beschweren,", "tokens": ["Mir", "die", "Lung'", "und", "Brust", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Roch mit einem scharffen Druck peinlich Haut und Fleisch", "tokens": ["Roch", "mit", "ei\u00b7nem", "scharf\u00b7fen", "Druck", "pein\u00b7lich", "Haut", "und", "Fleisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.9": {"text": "Sondern da in solchem Grad ietzt die Lufft so s\u00fc\u00df gemischt,", "tokens": ["Son\u00b7dern", "da", "in", "sol\u00b7chem", "Grad", "ietzt", "die", "Lufft", "so", "s\u00fc\u00df", "ge\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADV", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Da\u00df ein iedes Athem-holen Nase, Zunge, Lung\u2019 und Blut,", "tokens": ["Da\u00df", "ein", "ie\u00b7des", "A\u00b7them\u00b7ho\u00b7len", "Na\u00b7se", ",", "Zun\u00b7ge", ",", "Lung'", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "ADJA", "NN", "$,", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Durch ihr ausgedehntes Wesen, nehrt, erquicket und er-", "tokens": ["Durch", "ihr", "aus\u00b7ge\u00b7dehn\u00b7tes", "We\u00b7sen", ",", "nehrt", ",", "er\u00b7quic\u00b7ket", "und", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Ja, indem sie Haut und Fleisch mit so sanftem Wallen", "tokens": ["Ja", ",", "in\u00b7dem", "sie", "Haut", "und", "Fleisch", "mit", "so", "sanf\u00b7tem", "Wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "NN", "KON", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und durch spielendes Ges\u00e4usel \u00fcberall uns r\u00fchrt und schmei-", "tokens": ["Und", "durch", "spie\u00b7len\u00b7des", "Ge\u00b7s\u00e4u\u00b7sel", "\u00fc\u00b7be\u00b7rall", "uns", "r\u00fchrt", "und", "schmei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "PPER", "VVFIN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Innerlich und \u00e4usserlich unserm C\u00f6rper sanfte thut.", "tokens": ["In\u00b7ner\u00b7lich", "und", "\u00e4us\u00b7ser\u00b7lich", "un\u00b7serm", "C\u00f6r\u00b7per", "sanf\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn man das durch laue W\u00e4rme, und durchs nahe", "tokens": ["Wenn", "man", "das", "durch", "lau\u00b7e", "W\u00e4r\u00b7me", ",", "und", "durchs", "na\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "APPR", "ADJA", "NN", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "In der ausgedehnten Lufft blos gewirckte Gleich-Gewicht,", "tokens": ["In", "der", "aus\u00b7ge\u00b7dehn\u00b7ten", "Lufft", "blos", "ge\u00b7wirck\u00b7te", "Gleich\u00b7Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Das uns \u00fcberall durchdringt, das uns \u00fcberall ber\u00fchret,", "tokens": ["Das", "uns", "\u00fc\u00b7be\u00b7rall", "durch\u00b7dringt", ",", "das", "uns", "\u00fc\u00b7be\u00b7rall", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und worin wir gleichsam schwimmen, inn- und \u00e4usserlich", "tokens": ["Und", "wo\u00b7rin", "wir", "gleich\u00b7sam", "schwim\u00b7men", ",", "in\u00b7n", "und", "\u00e4us\u00b7ser\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVINF", "$,", "TRUNC", "KON", "ADJD"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sollte man ja dieses billig \u00fcberlegen, es erkennen,", "tokens": ["Soll\u00b7te", "man", "ja", "die\u00b7ses", "bil\u00b7lig", "\u00fc\u00b7berl\u00b7e\u00b7gen", ",", "es", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "PDAT", "ADJD", "VVINF", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und, bey unsrer C\u00f6rper Lust, auch der Seel\u2019 ein Labsal", "tokens": ["Und", ",", "bey", "uns\u00b7rer", "C\u00f6r\u00b7per", "Lust", ",", "auch", "der", "Seel'", "ein", "Lab\u00b7sal"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "NN", "$,", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da\u00df sie ein entferntes Ubel, und die gegenw\u00e4rtge Lust,", "tokens": ["Da\u00df", "sie", "ein", "ent\u00b7fern\u00b7tes", "U\u00b7bel", ",", "und", "die", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ge", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Durch vern\u00fcnftigs Uberlegen, als ein doppelt Gl\u00fcck be-", "tokens": ["Durch", "ver\u00b7n\u00fcnf\u00b7tigs", "U\u00b7ber\u00b7le\u00b7gen", ",", "als", "ein", "dop\u00b7pelt", "Gl\u00fcck", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "ADJD", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Und die Wunder unsers Sch\u00f6pfers denck- und danckens-", "tokens": ["Und", "die", "Wun\u00b7der", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "den\u00b7ck", "und", "dan\u00b7ckens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "TRUNC", "KON", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Sollte sie, wenn sanfte L\u00fcffte so gesch\u00e4fftig \u00fcm sie fliegen,", "tokens": ["Soll\u00b7te", "sie", ",", "wenn", "sanf\u00b7te", "L\u00fcff\u00b7te", "so", "ge\u00b7sch\u00e4ff\u00b7tig", "\u00fcm", "sie", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jhr daraus entspriessendes zugeeignetes Vergn\u00fcgen,", "tokens": ["Ihr", "da\u00b7raus", "ent\u00b7spries\u00b7sen\u00b7des", "zu\u00b7ge\u00b7eig\u00b7ne\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem, der alles ihr geschencket, nicht in einem frohen dencken,", "tokens": ["Dem", ",", "der", "al\u00b7les", "ihr", "ge\u00b7schen\u00b7cket", ",", "nicht", "in", "ei\u00b7nem", "fro\u00b7hen", "den\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,", "PTKNEG", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und in Danck-erf\u00fcllter Brust ihm zum Lob\u2019 und Dienst zu", "tokens": ["Und", "in", "Dan\u00b7ck\u00b7er\u00b7f\u00fcll\u00b7ter", "Brust", "ihm", "zum", "Lob'", "und", "Dienst", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "KON", "NN", "PTKZU"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Bey ged\u00e4mpften Leidenschaften, sich nach M\u00f6glichkeit be-", "tokens": ["Bey", "ge\u00b7d\u00e4mpf\u00b7ten", "Lei\u00b7den\u00b7schaf\u00b7ten", ",", "sich", "nach", "M\u00f6g\u00b7lich\u00b7keit", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRF", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Billig solten wir mit Freuden, fast bey iedem Athem-ziehn,", "tokens": ["Bil\u00b7lig", "sol\u00b7ten", "wir", "mit", "Freu\u00b7den", ",", "fast", "bey", "ie\u00b7dem", "A\u00b7them\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "NN", "$,", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Den, der solche Lust uns g\u00f6nnet, zu erheben uns bem\u00fchn.", "tokens": ["Den", ",", "der", "sol\u00b7che", "Lust", "uns", "g\u00f6n\u00b7net", ",", "zu", "er\u00b7he\u00b7ben", "uns", "be\u00b7m\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIAT", "NN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}