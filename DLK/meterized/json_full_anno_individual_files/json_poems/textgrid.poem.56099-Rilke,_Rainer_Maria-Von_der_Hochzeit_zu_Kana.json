{"textgrid.poem.56099": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hochzeit zu Kana", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Konnte sie denn anders, als auf ihn", "tokens": ["Konn\u00b7te", "sie", "denn", "an\u00b7ders", ",", "als", "auf", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "APPR", "PPER"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "stolz sein, der ihr Schlichtestes versch\u00f6nte?", "tokens": ["stolz", "sein", ",", "der", "ihr", "Schlich\u00b7tes\u00b7tes", "ver\u00b7sch\u00f6n\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "War nicht selbst die hohe, gro\u00dfgew\u00f6hnte", "tokens": ["War", "nicht", "selbst", "die", "ho\u00b7he", ",", "gro\u00df\u00b7ge\u00b7w\u00f6hn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nacht wie au\u00dfer sich, da er erschien?", "tokens": ["Nacht", "wie", "au\u00b7\u00dfer", "sich", ",", "da", "er", "er\u00b7schien", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ging nicht auch, da\u00df er sich einst verloren,", "tokens": ["Ging", "nicht", "auch", ",", "da\u00df", "er", "sich", "einst", "ver\u00b7lo\u00b7ren", ","], "to