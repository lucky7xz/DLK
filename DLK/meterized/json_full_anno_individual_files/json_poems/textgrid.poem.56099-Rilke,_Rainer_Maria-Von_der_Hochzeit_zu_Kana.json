asure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und dann tat er's. Sie verstand es sp\u00e4ter,", "tokens": ["Und", "dann", "tat", "er'", "s.", "Sie", "ver\u00b7stand", "es", "sp\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "VVIMP", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wie sie ihn in seinen Weg gedr\u00e4ngt:", "tokens": ["wie", "sie", "ihn", "in", "sei\u00b7nen", "Weg", "ge\u00b7dr\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "denn jetzt war er wirklich Wundert\u00e4ter,", "tokens": ["denn", "jetzt", "war", "er", "wirk\u00b7lich", "Wun\u00b7der\u00b7t\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und das ganze Opfer war verh\u00e4ngt,", "tokens": ["und", "das", "gan\u00b7ze", "Op\u00b7fer", "war", "ver\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "unaufhaltsam. Ja, es stand geschrieben.", "tokens": ["un\u00b7auf\u00b7halt\u00b7sam", ".", "Ja", ",", "es", "stand", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PTKANT", "$,", "PPER", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber war es damals schon bereit?", "tokens": ["A\u00b7ber", "war", "es", "da\u