{"textgrid.poem.41672": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann du vorbei mir gehst, gleichg\u00fcltig-stolze Sch\u00f6ne,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann du vorbei mir gehst, gleichg\u00fcltig-stolze Sch\u00f6ne,", "tokens": ["Wann", "du", "vor\u00b7bei", "mir", "gehst", ",", "gleich\u00b7g\u00fcl\u00b7tig\u00b7stol\u00b7ze", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim Sange der Musik, der am Gew\u00f6lb zerflie\u00dft,", "tokens": ["Beim", "San\u00b7ge", "der", "Mu\u00b7sik", ",", "der", "am", "Ge\u00b7w\u00f6lb", "zer\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie du dich sacht bewegst, harmonisch wie die T\u00f6ne,", "tokens": ["Wie", "du", "dich", "sacht", "be\u00b7wegst", ",", "har\u00b7mo\u00b7nisch", "wie", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Langeweil im Blick tiefm\u00fcde um dich siehst;", "tokens": ["Und", "Lan\u00b7ge\u00b7weil", "im", "Blick", "tief\u00b7m\u00fc\u00b7de", "um", "dich", "siehst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erblicke ich belebt vom wehnden Gasgeflimmer", "tokens": ["Er\u00b7bli\u00b7cke", "ich", "be\u00b7lebt", "vom", "wehn\u00b7den", "Gas\u00b7ge\u00b7flim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die krankhaft-bleiche Stirn, wo wundersam der Brand", "tokens": ["Die", "krank\u00b7haft\u00b7blei\u00b7che", "Stirn", ",", "wo", "wun\u00b7der\u00b7sam", "der", "Brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Abendfackeln spielt, wie neuer Morgenschimmer,", "tokens": ["Der", "A\u00b7bend\u00b7fa\u00b7ckeln", "spielt", ",", "wie", "neu\u00b7er", "Mor\u00b7gen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Auge, das mich wie der Blick von Bildern bannt \u2013", "tokens": ["Dein", "Au\u00b7ge", ",", "das", "mich", "wie", "der", "Blick", "von", "Bil\u00b7dern", "bannt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "KOKOM", "ART", "NN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denk ich: wie ist sie sch\u00f6n, von frischem Reiz umflutet!", "tokens": ["Denk", "ich", ":", "wie", "ist", "sie", "sch\u00f6n", ",", "von", "fri\u00b7schem", "Reiz", "um\u00b7flu\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "PWAV", "VAFIN", "PPER", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erinnrung, wie ein Turm, der schwer und k\u00f6niglich,", "tokens": ["E\u00b7rinn\u00b7rung", ",", "wie", "ein", "Turm", ",", "der", "schwer", "und", "k\u00f6\u00b7nig\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bekr\u00f6nt sie, und ihr Herz, das wie ein Pfirsich blutet,", "tokens": ["Be\u00b7kr\u00f6nt", "sie", ",", "und", "ihr", "Herz", ",", "das", "wie", "ein", "Pfir\u00b7sich", "blu\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPOSAT", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beut reif, gleich ihrem Leib, der kundgen Liebe sich.", "tokens": ["Beut", "reif", ",", "gleich", "ih\u00b7rem", "Leib", ",", "der", "kund\u00b7gen", "Lie\u00b7be", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADV", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bist du des Herbstes Frucht, von auserlesner Milde?", "tokens": ["Bist", "du", "des", "Herbs\u00b7tes", "Frucht", ",", "von", "au\u00b7ser\u00b7les\u00b7ner", "Mil\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist eine Urne du, die sich nach Tr\u00e4nen bangt,", "tokens": ["Bist", "ei\u00b7ne", "Ur\u00b7ne", "du", ",", "die", "sich", "nach", "Tr\u00e4\u00b7nen", "bangt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Duft, der tr\u00e4umen macht von seligem Gefilde,", "tokens": ["Ein", "Duft", ",", "der", "tr\u00e4u\u00b7men", "macht", "von", "se\u00b7li\u00b7gem", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein schmeichlerischer Pf\u00fchl, ein Korb, der Bl\u00fcten prangt?", "tokens": ["Ein", "schmeich\u00b7le\u00b7ri\u00b7scher", "Pf\u00fchl", ",", "ein", "Korb", ",", "der", "Bl\u00fc\u00b7ten", "prangt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df es: Augen sind, voll trauervoller Reine,", "tokens": ["Ich", "wei\u00df", "es", ":", "Au\u00b7gen", "sind", ",", "voll", "trau\u00b7er\u00b7vol\u00b7ler", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "VAFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sich kein R\u00e4tsel birgt, das k\u00f6stlich zu erschaun,", "tokens": ["Wo", "sich", "kein", "R\u00e4t\u00b7sel", "birgt", ",", "das", "k\u00f6st\u00b7lich", "zu", "er\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "VVFIN", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie leere Medaillons, kleinodienarme Schreine,", "tokens": ["Wie", "lee\u00b7re", "Me\u00b7dail\u00b7lons", ",", "klein\u00b7o\u00b7dien\u00b7ar\u00b7me", "Schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und tiefer, \u00f6der noch als selbst der Himmel Blaun.", "tokens": ["Und", "tie\u00b7fer", ",", "\u00f6\u00b7der", "noch", "als", "selbst", "der", "Him\u00b7mel", "Blaun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "ADV", "KOKOM", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch die Erscheinung ists, die zagendem Gef\u00fchle,", "tokens": ["Doch", "die", "Er\u00b7schei\u00b7nung", "ists", ",", "die", "za\u00b7gen\u00b7dem", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das vor der Wahrheit flieht, das Sein vers\u00fc\u00dfen kann.", "tokens": ["Das", "vor", "der", "Wahr\u00b7heit", "flieht", ",", "das", "Sein", "ver\u00b7s\u00fc\u00b7\u00dfen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was k\u00fcmmern Torheit mich und seelenlose K\u00fchle?", "tokens": ["Was", "k\u00fcm\u00b7mern", "Tor\u00b7heit", "mich", "und", "see\u00b7len\u00b7lo\u00b7se", "K\u00fch\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "PPER", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob Maske oder Zier \u2013 dich, Sch\u00f6nheit, bet ich an!", "tokens": ["Ob", "Mas\u00b7ke", "o\u00b7der", "Zier", "\u2013", "dich", ",", "Sch\u00f6n\u00b7heit", ",", "bet", "ich", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$(", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wann du vorbei mir gehst, gleichg\u00fcltig-stolze Sch\u00f6ne,", "tokens": ["Wann", "du", "vor\u00b7bei", "mir", "gehst", ",", "gleich\u00b7g\u00fcl\u00b7tig\u00b7stol\u00b7ze", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim Sange der Musik, der am Gew\u00f6lb zerflie\u00dft,", "tokens": ["Beim", "San\u00b7ge", "der", "Mu\u00b7sik", ",", "der", "am", "Ge\u00b7w\u00f6lb", "zer\u00b7flie\u00dft", ","], "token_info": ["word", "