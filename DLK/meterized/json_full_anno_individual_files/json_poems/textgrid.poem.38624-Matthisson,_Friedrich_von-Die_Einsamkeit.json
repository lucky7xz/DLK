"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Vom Vollmond begl\u00e4nzt.", "tokens": ["Vom", "Voll\u00b7mond", "be\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Der Welt zu vergessen,", "tokens": ["Der", "Welt", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Empfangt mich, ihr H\u00f6hn,", "tokens": ["Emp\u00b7fangt", "mich", ",", "ihr", "H\u00f6hn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wo dunkle Zipressen", "tokens": ["Wo", "dunk\u00b7le", "Zip\u00b7res\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Ein Grabmal umwehn;", "tokens": ["Ein", "Grab\u00b7mal", "um\u00b7wehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wo, tief zwischen Ranken", "tokens": ["Wo", ",", "tief", "zwi\u00b7schen", "Ran\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "ADJD", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Wildni\u00df versteckt,", "tokens": ["Der", "Wild\u00b7ni\u00df", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Kein menschliches Wanken", "tokens": ["Kein", "menschli\u00b7ches", "Wan\u00b7ken"], "token_info": ["word", "word", "word"],