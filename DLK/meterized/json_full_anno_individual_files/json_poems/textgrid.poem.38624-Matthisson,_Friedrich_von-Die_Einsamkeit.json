{"textgrid.poem.38624": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie blinkt mir der Himmel", "tokens": ["Wie", "blinkt", "mir", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Gr\u00fcnen so hehr.", "tokens": ["Im", "Gr\u00fc\u00b7nen", "so", "hehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der St\u00e4dte Get\u00fcmmel", "tokens": ["Der", "St\u00e4d\u00b7te", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ist rauschend und leer.", "tokens": ["Ist", "rau\u00b7schend", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Drum sei meiner Thr\u00e4nen", "tokens": ["Drum", "sei", "mei\u00b7ner", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Vertraute die Flur,", "tokens": ["Ver\u00b7trau\u00b7te", "die", "Flur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Drum h\u00f6re mein Sehnen", "tokens": ["Drum", "h\u00f6\u00b7re", "mein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Einsamkeit nur.", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "nur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ihr liebt' ich, im Lenze", "tokens": ["Ihr", "liebt'", "ich", ",", "im", "Len\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Lebens, im Hain", "tokens": ["Des", "Le\u00b7bens", ",", "im", "Hain"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schon Veilchen in Kr\u00e4nze", "tokens": ["Schon", "Veil\u00b7chen", "in", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zum Opfer zu reihn.", "tokens": ["Zum", "Op\u00b7fer", "zu", "reihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ihr sp\u00e4ht' ich, beim Hauche", "tokens": ["Ihr", "sp\u00e4ht'", "ich", ",", "beim", "Hau\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Mailuft, am Bach", "tokens": ["Der", "Mai\u00b7luft", ",", "am", "Bach"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im Nachtigallstrauche", "tokens": ["Im", "Nach\u00b7ti\u00b7gall\u00b7strau\u00b7che"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wohl Stundenlang nach.", "tokens": ["Wohl", "Stun\u00b7den\u00b7lang", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr seufzt' ich, vom Spiele", "tokens": ["Ihr", "seufzt'", "ich", ",", "vom", "Spie\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der J\u00fcnglinge fern,", "tokens": ["Der", "J\u00fcng\u00b7lin\u00b7ge", "fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Erstlingsgef\u00fchle", "tokens": ["Die", "Erst\u00b7lings\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Liebe so gern!", "tokens": ["Der", "Lie\u00b7be", "so", "gern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ihr war, beim Geflimmer", "tokens": ["Ihr", "war", ",", "beim", "Ge\u00b7flim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Sterne, mein Leid", "tokens": ["Der", "Ster\u00b7ne", ",", "mein", "Leid"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und jeglicher Schimmer", "tokens": ["Und", "jeg\u00b7li\u00b7cher", "Schim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Freude geweiht.", "tokens": ["Der", "Freu\u00b7de", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Mir sei bis zum Grabe", "tokens": ["Mir", "sei", "bis", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gef\u00e4hrtin und Braut", "tokens": ["Ge\u00b7f\u00e4hr\u00b7tin", "und", "Braut"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die, der ich als Knabe", "tokens": ["Die", ",", "der", "ich", "als", "Kna\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "PPER", "KOUS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mein Innres vertraut.", "tokens": ["Mein", "Inn\u00b7res", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nur sie hat die Z\u00e4hren", "tokens": ["Nur", "sie", "hat", "die", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der Trennung gestillt,", "tokens": ["Der", "Tren\u00b7nung", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und himmlische Sph\u00e4ren", "tokens": ["Und", "himm\u00b7li\u00b7sche", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Voll Glanz mir enth\u00fcllt.", "tokens": ["Voll", "Glanz", "mir", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Sie meidet die Pfade,", "tokens": ["Sie", "mei\u00b7det", "die", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Flieht Park und Alleen,", "tokens": ["Flieht", "Park", "und", "Al\u00b7leen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und weilt am Gestade", "tokens": ["Und", "weilt", "am", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Romantischer Seen,", "tokens": ["Ro\u00b7man\u00b7ti\u00b7scher", "Seen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wo V\u00f6gel nur schmettern,", "tokens": ["Wo", "V\u00f6\u00b7gel", "nur", "schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Eichhorn nur lauscht,", "tokens": ["Das", "Eich\u00b7horn", "nur", "lauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und etwa den Bl\u00e4ttern", "tokens": ["Und", "et\u00b7wa", "den", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ein T\u00e4ubchen entrauscht.", "tokens": ["Ein", "T\u00e4ub\u00b7chen", "ent\u00b7rauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Nur ihr sind, vom wilden", "tokens": ["Nur", "ihr", "sind", ",", "vom", "wil\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "$,", "APPRART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Granitfels umdr\u00e4ut,", "tokens": ["Gra\u00b7nit\u00b7fels", "um\u00b7dr\u00e4ut", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "An Gletschergefilden", "tokens": ["An", "Glet\u00b7scher\u00b7ge\u00b7fil\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Th\u00e4ler geweiht,", "tokens": ["Die", "Th\u00e4\u00b7ler", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wo Adler nur streifen", "tokens": ["Wo", "Ad\u00b7ler", "nur", "strei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Am Lerchenbaumwald,", "tokens": ["Am", "Ler\u00b7chen\u00b7baum\u00b7wald", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und fernher das Pfeifen", "tokens": ["Und", "fern\u00b7her", "das", "Pfei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Gemsen erschallt.", "tokens": ["Der", "Gem\u00b7sen", "er\u00b7schallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Sie freut sich der Schl\u00fcnde", "tokens": ["Sie", "freut", "sich", "der", "Schl\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Volkanischer Gluth,", "tokens": ["Vol\u00b7ka\u00b7ni\u00b7scher", "Gluth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Des Sausens der Winde,", "tokens": ["Des", "Sau\u00b7sens", "der", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der z\u00fcrnenden Fluth.", "tokens": ["Der", "z\u00fcr\u00b7nen\u00b7den", "Fluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie wohnt unter Spalten,", "tokens": ["Sie", "wohnt", "un\u00b7ter", "Spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nur mondlich erhellt,", "tokens": ["Nur", "mond\u00b7lich", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "In Gr\u00e4bern der alten", "tokens": ["In", "Gr\u00e4\u00b7bern", "der", "al\u00b7ten"], "token_info": ["word", "word", 