{"textgrid.poem.52472": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "54. Die stillende Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo das Geb\u00fcsch geweihte Schatten streut,", "tokens": ["Wo", "das", "Ge\u00b7b\u00fcsch", "ge\u00b7weih\u00b7te", "Schat\u00b7ten", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Rasensitz, von Weiden \u00fcberh\u00fcllet,", "tokens": ["Im", "Ra\u00b7sen\u00b7sitz", ",", "von", "Wei\u00b7den", "\u00fc\u00b7berh\u00b7\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ruht sie im Schmucke holder Weiblichkeit,", "tokens": ["Ruht", "sie", "im", "Schmu\u00b7cke", "hol\u00b7der", "Weib\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Mutter, die geheim den S\u00e4ugling stillet.", "tokens": ["Die", "Mut\u00b7ter", ",", "die", "ge\u00b7heim", "den", "S\u00e4ug\u00b7ling", "stil\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gesenkten Blicks, gleich einer Caritas;", "tokens": ["Ge\u00b7senk\u00b7ten", "Blicks", ",", "gleich", "ei\u00b7ner", "Ca\u00b7ri\u00b7tas", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Demut hehr, wie die Gebenedeite;", "tokens": ["Durch", "De\u00b7mut", "hehr", ",", "wie", "die", "Ge\u00b7be\u00b7ne\u00b7dei\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sieh, wie sie sich im Wohlthun s\u00fc\u00df verga\u00df,", "tokens": ["Sieh", ",", "wie", "sie", "sich", "im", "Wohl\u00b7thun", "s\u00fc\u00df", "ver\u00b7ga\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PRF", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ganz sich der Pflicht \u2013 ein Bl\u00fctenopfer \u2013 weihte.", "tokens": ["Ganz", "sich", "der", "Pflicht", "\u2013", "ein", "Bl\u00fc\u00b7ten\u00b7op\u00b7fer", "\u2013", "weih\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sieh, frommen Ernst mit Z\u00e4rtlichkeit gemischt;", "tokens": ["Sieh", ",", "from\u00b7men", "Ernst", "mit", "Z\u00e4rt\u00b7lich\u00b7keit", "ge\u00b7mischt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Jungfrau Reinheit bei der Gattin Treue;", "tokens": ["Der", "Jung\u00b7frau", "Rein\u00b7heit", "bei", "der", "Gat\u00b7tin", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Frohsinns Glanz durch Leiden halb verwischt;", "tokens": ["Des", "Froh\u00b7sinns", "Glanz", "durch", "Lei\u00b7den", "halb", "ver\u00b7wischt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf heitrer Stirn der Schmerzen erste Weihe.", "tokens": ["Auf", "hei\u00b7trer", "Stirn", "der", "Schmer\u00b7zen", "ers\u00b7te", "Wei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie sie das Kind an ihren Busen dr\u00fcckt,", "tokens": ["Wie", "sie", "das", "Kind", "an", "ih\u00b7ren", "Bu\u00b7sen", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit holder Sorge zu ihm hingebogen,", "tokens": ["Mit", "hol\u00b7der", "Sor\u00b7ge", "zu", "ihm", "hin\u00b7ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wonnel\u00e4chelnd auf den Spr\u00f6\u00dfling blickt,", "tokens": ["Und", "won\u00b7ne\u00b7l\u00e4\u00b7chelnd", "auf", "den", "Spr\u00f6\u00df\u00b7ling", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den s\u00fc\u00dfe M\u00fch' und zarte Angst erzogen.", "tokens": ["Den", "s\u00fc\u00b7\u00dfe", "M\u00fch'", "und", "zar\u00b7te", "Angst", "er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gleich einer Knospe, die ihr Dorn verletzt,", "tokens": ["Gleich", "ei\u00b7ner", "Knos\u00b7pe", ",", "die", "ihr", "Dorn", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Z\u00fcrnt nimmer sie der Ursach' ihrer Schmerzen;", "tokens": ["Z\u00fcrnt", "nim\u00b7mer", "sie", "der", "Ur\u00b7sach'", "ih\u00b7rer", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der stumme Dank im Blick des Kinds ersetzt", "tokens": ["Der", "stum\u00b7me", "Dank", "im", "Blick", "des", "Kinds", "er\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die herbsten Leiden einem Mutterherzen.", "tokens": ["Die", "herbs\u00b7ten", "Lei\u00b7den", "ei\u00b7nem", "Mut\u00b7ter\u00b7her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der ersten Freude mattes Morgenlicht,", "tokens": ["Der", "ers\u00b7ten", "Freu\u00b7de", "mat\u00b7tes", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das sich auf ihres Kleinen Wangen zeigte,", "tokens": ["Das", "sich", "auf", "ih\u00b7res", "Klei\u00b7nen", "Wan\u00b7gen", "zeig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkl\u00e4rt im Wiederschein ihr Angesicht,", "tokens": ["Ver\u00b7kl\u00e4rt", "im", "Wie\u00b7der\u00b7schein", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn es auch thr\u00e4nenfeucht sich zu ihm neigte;", "tokens": ["Wenn", "es", "auch", "thr\u00e4\u00b7nen\u00b7feucht", "sich", "zu", "ihm", "neig\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie Philomele rein und leiser lockt,", "tokens": ["Wie", "Phi\u00b7lo\u00b7me\u00b7le", "rein", "und", "lei\u00b7ser", "lockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Fittig w\u00e4rmend um ihr Nest verspreitet,", "tokens": ["Den", "Fit\u00b7tig", "w\u00e4r\u00b7mend", "um", "ihr", "Nest", "ver\u00b7sprei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo sie, von Wei\u00dfdornbl\u00fcten \u00fcberflockt,", "tokens": ["Wo", "sie", ",", "von", "Wei\u00df\u00b7dorn\u00b7bl\u00fc\u00b7ten", "\u00fc\u00b7ber\u00b7flockt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Mutter s\u00fc\u00dfes Wiegenlied begleitet;", "tokens": ["Der", "Mut\u00b7ter", "s\u00fc\u00b7\u00dfes", "Wie\u00b7gen\u00b7lied", "be\u00b7glei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wo h\u00e4uslich eingekleidet, schlicht und rein,", "tokens": ["Wo", "h\u00e4us\u00b7lich", "ein\u00b7ge\u00b7klei\u00b7det", ",", "schlicht", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie die geschlo\u00dfne Lilie verschleiert,", "tokens": ["Wie", "die", "ge\u00b7schlo\u00df\u00b7ne", "Li\u00b7lie", "ver\u00b7schlei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In leiser D\u00e4mmerung Verkl\u00e4rungsschein", "tokens": ["In", "lei\u00b7ser", "D\u00e4m\u00b7me\u00b7rung", "Ver\u00b7kl\u00e4\u00b7rungs\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie nach des Hauses treuen Sorgen feiert.", "tokens": ["Sie", "nach", "des", "Hau\u00b7ses", "treu\u00b7en", "Sor\u00b7gen", "fei\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Abendstern ergeu\u00dft sein mildes Licht", "tokens": ["Der", "A\u00b7bends\u00b7tern", "er\u00b7geu\u00dft", "sein", "mil\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mit Wohlgefallen durch betaute Zweige;", "tokens": ["Mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "durch", "be\u00b7tau\u00b7te", "Zwei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch milder, klarer leuchtet Hesper nicht,", "tokens": ["Doch", "mil\u00b7der", ",", "kla\u00b7rer", "leuch\u00b7tet", "Hes\u00b7per", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJA", "VVFIN", "NE", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als jener Blick, der Mutterw\u00fcrde Zeuge.", "tokens": ["Als", "je\u00b7ner", "Blick", ",", "der", "Mut\u00b7ter\u00b7w\u00fcr\u00b7de", "Zeu\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die reine Grazie der Mutterhuld,", "tokens": ["Die", "rei\u00b7ne", "Gra\u00b7zie", "der", "Mut\u00b7ter\u00b7huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die ernste Schwester j\u00fcngrer Charitinnen,", "tokens": ["Die", "erns\u00b7te", "Schwes\u00b7ter", "j\u00fcng\u00b7rer", "Cha\u00b7ri\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zart wie die Liebe, fest wie die Geduld,", "tokens": ["Zart", "wie", "die", "Lie\u00b7be", ",", "fest", "wie", "die", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Treu, wie der heil'gen Flamme H\u00fcterinnen,", "tokens": ["Treu", ",", "wie", "der", "heil'\u00b7gen", "Flam\u00b7me", "H\u00fc\u00b7te\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.11": {"line.1": {"text": "Steht, als ihr Engel, schirmend hinter ihr,", "tokens": ["Steht", ",", "als", "ihr", "En\u00b7gel", ",", "schir\u00b7mend", "hin\u00b7ter", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und, von der Unsichtbaren Glanz umleuchtet,", "tokens": ["Und", ",", "von", "der", "Un\u00b7sicht\u00b7ba\u00b7ren", "Glanz", "um\u00b7leuch\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Flieht eitler Wunsch und sinnliche Begier,", "tokens": ["Flieht", "eit\u00b7ler", "Wunsch", "und", "sinn\u00b7li\u00b7che", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo fromme Sehnsucht nur ihr Auge feuchtet.", "tokens": ["Wo", "from\u00b7me", "Sehn\u00b7sucht", "nur", "ihr", "Au\u00b7ge", "feuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Durch Liebe stark, vermag ein Mutterherz", "tokens": ["Durch", "Lie\u00b7be", "stark", ",", "ver\u00b7mag", "ein", "Mut\u00b7ter\u00b7herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den sch\u00f6nen Kranz von ihren Jugendtagen,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Kranz", "von", "ih\u00b7ren", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verl\u00e4chelnd des Verbl\u00fchens leisen Schmerz,", "tokens": ["Ver\u00b7l\u00e4\u00b7chelnd", "des", "Ver\u00b7bl\u00fc\u00b7hens", "lei\u00b7sen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf den Altar der Treue froh zu tragen.", "tokens": ["Auf", "den", "Al\u00b7tar", "der", "Treu\u00b7e", "froh", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["