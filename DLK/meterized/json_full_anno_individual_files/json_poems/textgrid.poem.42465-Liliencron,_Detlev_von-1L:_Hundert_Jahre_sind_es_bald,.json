{"textgrid.poem.42465": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hundert Jahre sind es bald,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hundert Jahre sind es bald,", "tokens": ["Hun\u00b7dert", "Jah\u00b7re", "sind", "es", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Despot Napoleon,", "tokens": ["Als", "Des\u00b7pot", "Na\u00b7po\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weggehaun und weggeknallt,", "tokens": ["Weg\u00b7ge\u00b7haun", "und", "weg\u00b7ge\u00b7knallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lief auf Leipzigs Feld davon.", "tokens": ["Lief", "auf", "Leip\u00b7zigs", "Feld", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Guten Schlu\u00df gemacht", "tokens": ["Gu\u00b7ten", "Schlu\u00df", "ge\u00b7macht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hat die V\u00f6lkerschlacht,", "tokens": ["Hat", "die", "V\u00f6l\u00b7ker\u00b7schlacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und er hatte seinen Lohn.", "tokens": ["Und", "er", "hat\u00b7te", "sei\u00b7nen", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einmal noch, nach manchem Jahr,", "tokens": ["Ein\u00b7mal", "noch", ",", "nach", "man\u00b7chem", "Jahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will der Franzmann unsern Rhein;", "tokens": ["Will", "der", "Franz\u00b7mann", "un\u00b7sern", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPOSAT", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der teutonische Barbar", "tokens": ["Der", "teu\u00b7to\u00b7ni\u00b7sche", "Bar\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jagt ihn \u00fcber Stock und Stein.", "tokens": ["Jagt", "ihn", "\u00fc\u00b7ber", "Stock", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sedan, hoch! Hurra!", "tokens": ["Se\u00b7dan", ",", "hoch", "!", "Hur\u00b7ra", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$.", "NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und mit Gloria", "tokens": ["Und", "mit", "Glo\u00b7ria"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Drangen wir in Welschland ein.", "tokens": ["Dran\u00b7gen", "wir", "in", "Wel\u00b7schland", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deutschland einig! Nord und S\u00fcd!", "tokens": ["Deutschland", "ei\u00b7nig", "!", "Nord", "und", "S\u00fcd", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hand in Hand und Brust an Brust!", "tokens": ["Hand", "in", "Hand", "und", "Brust", "an", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaiser Wilhelm, niemals m\u00fcd,", "tokens": ["Kai\u00b7ser", "Wil\u00b7helm", ",", "nie\u00b7mals", "m\u00fcd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Tode pflichtbewu\u00dft.", "tokens": ["Bis", "zum", "To\u00b7de", "pflicht\u00b7be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des Kanzlers Kraft", "tokens": ["Und", "des", "Kanz\u00b7lers", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mit dem Eisenschaft,", "tokens": ["Mit", "dem", "Ei\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Steht breitbeinig wie Granit.", "tokens": ["Steht", "breit\u00b7bei\u00b7nig", "wie", "Gra\u00b7nit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "NE", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Komm, wer will, nur jetzt heran;", "tokens": ["Komm", ",", "wer", "will", ",", "nur", "jetzt", "he\u00b7ran", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VMFIN", "$,", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Welt uns auch umgraust,", "tokens": ["Wenn", "die", "Welt", "uns", "auch", "um\u00b7graust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Kaiser obenan", "tokens": ["Un\u00b7ser", "Kai\u00b7ser", "o\u00b7be\u00b7nan"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt dem Teufel seine Faust.", "tokens": ["Zeigt", "dem", "Teu\u00b7fel", "sei\u00b7ne", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Friede soll es sein!", "tokens": ["Frie\u00b7de", "soll", "es", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bricht der Feind herein,", "tokens": ["Bricht", "der", "Feind", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wird gepackt er und zerzaust.", "tokens": ["Wird", "ge\u00b7packt", "er", "und", "zer\u00b7zaust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ruh nicht aus, mein Vaterland!", "tokens": ["Ruh", "nicht", "aus", ",", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark zu Lande, stark zu Meer!", "tokens": ["Stark", "zu", "Lan\u00b7de", ",", "stark", "zu", "Meer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Duck dich nie! Pa\u00df auf am Strand!", "tokens": ["Duck", "dich", "nie", "!", "Pa\u00df", "auf", "am", "Strand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df den Finger am Gewehr!", "tokens": ["La\u00df", "den", "Fin\u00b7ger", "am", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Flotte Hut", "tokens": ["Dei\u00b7ner", "Flot\u00b7te", "Hut"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sch\u00fctzt die K\u00fcste gut,", "tokens": ["Sch\u00fctzt", "die", "K\u00fcs\u00b7te", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sch\u00fctzt den ruhigen Verkehr.", "tokens": ["Sch\u00fctzt", "den", "ru\u00b7hi\u00b7gen", "Ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "M\u00e4chtig mu\u00df die Flotte sein,", "tokens": ["M\u00e4ch\u00b7tig", "mu\u00df", "die", "Flot\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings gesehn im Ozean.", "tokens": ["Rings", "ge\u00b7sehn", "im", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgenrot und Mittagsschein", "tokens": ["Mor\u00b7gen\u00b7rot", "und", "Mit\u00b7tags\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fchn auf ihrer Flaggenbahn.", "tokens": ["Gl\u00fchn", "auf", "ih\u00b7rer", "Flag\u00b7gen\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorw\u00e4rts! Auf! Es gilt!", "tokens": ["Vor\u00b7w\u00e4rts", "!", "Auf", "!", "Es", "gilt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "APPR", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Halten wir den Schild", "tokens": ["Hal\u00b7ten", "wir", "den", "Schild"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "\u00dcber Deutschlands fl\u00fcggen Schwan.", "tokens": ["\u00dc\u00b7ber", "Deutschlands", "fl\u00fcg\u00b7gen", "Schwan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Hundert Jahre sind es bald,", "tokens": ["Hun\u00b7dert", "Jah\u00b7re", "sind", "es", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Despot Napoleon,", "tokens": ["Als", "Des\u00b7pot", "Na\u00b7po\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weggehaun und weggeknallt,", "tokens": ["Weg\u00b7ge\u00b7haun", "und", "weg\u00b7ge\u00b7knallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lief auf Leipzigs Feld davon.", "tokens": ["Lief", "auf", "Leip\u00b7zigs", "Feld", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Guten Schlu\u00df gemacht", "tokens": ["Gu\u00b7ten", "Schlu\u00df", "ge\u00b7macht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hat die V\u00f6lkerschlacht,", "tokens": ["Hat", "die", "V\u00f6l\u00b7ker\u00b7schlacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und er hatte seinen Lohn.", "tokens": ["Und", "er", "hat\u00b7te", "sei\u00b7nen", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text