", ",", "nur", "jetzt", "he\u00b7ran", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VMFIN", "$,", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Welt uns auch umgraust,", "tokens": ["Wenn", "die", "Welt", "uns", "auch", "um\u00b7graust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Kaiser obenan", "tokens": ["Un\u00b7ser", "Kai\u00b7ser", "o\u00b7be\u00b7nan"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt dem Teufel seine Faust.", "tokens": ["Zeigt", "dem", "Teu\u00b7fel", "sei\u00b7ne", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Friede soll es sein!", "tokens": ["Frie\u00b7de", "soll", "es", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bricht der Feind herein,", "tokens": ["Bricht", "der", "Feind", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wird gepackt er und zerzaust.", "tokens": ["Wird", "ge\u00b7packt", "er", "und", "zer\u00b7zaust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPER", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ruh nicht aus, mein Vaterland!", "tokens": ["Ruh", "nicht", "aus", ",", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark zu Lande, stark zu Meer!", "tokens": ["Stark", "zu", "Lan\u00b7de", ",", "stark", "zu", "Meer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Duck dich nie! Pa\u00df auf am Strand!", "tokens": ["Duck", "dich", "nie", "!", "Pa\u00df", "auf", "am", "Strand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df den Finger am Gewehr!", "tokens": ["La\u00df", "den", "Fin\u00b7ger", "am", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Flotte Hut", "tokens": ["Dei\u00b7ner", "Flot\u00b7te", "Hut"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sch\u00fctzt die K\u00fcste gut,", "tokens": ["Sch\u00fctzt", "die", "K\u00fcs\u00b7te", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sch\u00fctzt den ruhigen Verkehr.", "tokens": ["Sch\u00fctzt", "den", "ru\u00b7hi\u00b7gen", "Ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "M\u00e4chtig mu\u00df die Flotte sein,", "tokens": ["M\u00e4ch\u00b7tig", "mu\u00df", "die", "Flot\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings gesehn im Ozean.", "tokens": ["Rings", "ge\u00b7sehn", "im", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgenrot und Mittagsschein", "tokens": ["Mor\u00b7gen\u00b7rot", "und", "Mit\u00b7tags\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fchn auf ihrer Flaggenbahn.", "tokens": ["Gl\u00fchn", "auf", "ih\u00b7rer", "Flag\u00b7gen\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorw\u00e4rts! Auf! Es gilt!", "tokens": ["Vor\u00b7w\u00e4rts", "!", "Auf", "!", "Es", "gilt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "APPR", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Halten wir den Schild", "tokens": ["Hal\u00b7ten", "wir", "den", "Schild"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "\u00dcber Deutschlands fl\u00fcggen Schwan.", "tokens": ["\u00dc\u00b7ber", "Deutschlands", "fl\u00fcg\u00b7gen", "Schwan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}