{"dta.poem.2140": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Erkenntni\u00df Gottes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Thiere wissen nichts von Gott, der Mensch, von", "tokens": ["Die", "Thie\u00b7re", "wis\u00b7sen", "nichts", "von", "Gott", ",", "der", "Mensch", ",", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "NN", "$,", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "allen Kreaturen", "tokens": ["al\u00b7len", "Kre\u00b7a\u00b7tu\u00b7ren"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Erblickt allein, in Gottes Werken, von einem Gott ge-", "tokens": ["Er\u00b7blickt", "al\u00b7lein", ",", "in", "Got\u00b7tes", "Wer\u00b7ken", ",", "von", "ei\u00b7nem", "Gott", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "NN", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "wisse Spuren.", "tokens": ["wis\u00b7se", "Spu\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Doch, damit nicht vergn\u00fcgt, spricht er: Ich weis von", "tokens": ["Doch", ",", "da\u00b7mit", "nicht", "ver\u00b7gn\u00fcgt", ",", "spricht", "er", ":", "Ich", "weis", "von"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PTKNEG", "VVPP", "$,", "VVFIN", "PPER", "$.", "PPER", "PTKVZ", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gott noch mancherley.", "tokens": ["Gott", "noch", "man\u00b7cher\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wir sollen, da\u00df ein Gott sey, wissen, wir wollen wis-", "tokens": ["Wir", "sol\u00b7len", ",", "da\u00df", "ein", "Gott", "sey", ",", "wis\u00b7sen", ",", "wir", "wol\u00b7len", "wis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "NN", "VAFIN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sen, was er sey.", "tokens": ["sen", ",", "was", "er", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da die\u00df doch eine Wissenschaft, die uns in unserm Stand", "tokens": ["Da", "die\u00df", "doch", "ei\u00b7ne", "Wis\u00b7sen\u00b7schaft", ",", "die", "uns", "in", "un\u00b7serm", "Stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "hienieden", "tokens": ["hien\u00b7ie\u00b7den"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Nicht: sondern, sollen wir sie haben, vermuthlich k\u00fcnftig", "tokens": ["Nicht", ":", "son\u00b7dern", ",", "sol\u00b7len", "wir", "sie", "ha\u00b7ben", ",", "ver\u00b7muth\u00b7lich", "k\u00fcnf\u00b7tig"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "$.", "KON", "$,", "VMFIN", "PPER", "PPER", "VAFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "erst beschieden.", "tokens": ["erst", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}