{"textgrid.poem.40943": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie hen mer gseit im Dorf, i sott's nit tue.", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hen mer gseit im Dorf, i sott's nit tue.", "tokens": ["Sie", "hen", "mer", "gseit", "im", "Dorf", ",", "i", "sott's", "nit", "tue", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie hen mer gseit: \u00bbEs schickt si nit. Sie z\u00fcrnt's.", "tokens": ["Sie", "hen", "mer", "gseit", ":", "\u00bb", "Es", "schickt", "si", "nit", ".", "Sie", "z\u00fcrnt'", "s."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "NE"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Du weisch nit, wie me mit der F\u00fcrstin redt", "tokens": ["Du", "weisch", "nit", ",", "wie", "me", "mit", "der", "F\u00fcrs\u00b7tin", "redt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKNEG", "$,", "PWAV", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in diner guten Einfalt. Blib deheim!\u00ab", "tokens": ["in", "di\u00b7ner", "gu\u00b7ten", "Ein\u00b7falt", ".", "Blib", "de\u00b7heim", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O nei, 's isch nit so, edli F\u00fcrstefrau!", "tokens": ["O", "nei", ",", "'s", "isch", "nit", "so", ",", "ed\u00b7li", "F\u00fcrs\u00b7te\u00b7frau", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "PPER", "ADJD", "PTKNEG", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Nei, d'Liebi het e gueti fini Red,", "tokens": ["Nei", ",", "d'\u00b7Lie\u00b7bi", "het", "e", "gue\u00b7ti", "fi\u00b7ni", "Red", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "und so ne Gm\u00fcet wie eueres z\u00fcrnt 's nit.", "tokens": ["und", "so", "ne", "Gm\u00fcet", "wie", "eu\u00b7e\u00b7res", "z\u00fcrnt", "'s", "nit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "KOKOM", "PPOSAT", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der sind jo in ganz F\u00fcrsteberg so lieb", "tokens": ["Der", "sind", "jo", "in", "ganz", "F\u00fcrs\u00b7te\u00b7berg", "so", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NE", "APPR", "ADV", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der w\u00fc\u00dfet's nit, der glaubet's nit, wie lieb.", "tokens": ["Der", "w\u00fc\u00b7\u00dfet's", "nit", ",", "der", "glau\u00b7bet's", "nit", ",", "wie", "lieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$,", "PRELS", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Drum hani denkt, i gang und sageres", "tokens": ["Drum", "ha\u00b7ni", "denkt", ",", "i", "gang", "und", "sa\u00b7ge\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "NE", "VVFIN", "$,", "NE", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und bringere mi frumme Segenswunsch", "tokens": ["und", "brin\u00b7ge\u00b7re", "mi", "frum\u00b7me", "Se\u00b7gens\u00b7wunsch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und bringere mi treu und dankbar Herz.", "tokens": ["und", "brin\u00b7ge\u00b7re", "mi", "treu", "und", "dank\u00b7bar", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "O F\u00fcrstin, euer sch\u00f6ne Name wohnt", "tokens": ["O", "F\u00fcrs\u00b7tin", ",", "eu\u00b7er", "sch\u00f6\u00b7ne", "Na\u00b7me", "wohnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "in alle Herzen, und im Himmel stoht", "tokens": ["in", "al\u00b7le", "Her\u00b7zen", ",", "und", "im", "Him\u00b7mel", "stoht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "er, denkwohl, au, und isch den Engle lieb.", "tokens": ["er", ",", "denk\u00b7wohl", ",", "au", ",", "und", "isch", "den", "Eng\u00b7le", "lieb", "."], "token