{"textgrid.poem.57845": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Mai", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcn ist der Wald, rot ist der Bock,", "tokens": ["Gr\u00fcn", "ist", "der", "Wald", ",", "rot", "ist", "der", "Bock", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Drum zieh' ich an den gr\u00fcnen Rock,", "tokens": ["Drum", "zieh'", "ich", "an", "den", "gr\u00fc\u00b7nen", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Setz' auf den gr\u00fcnen Hut;", "tokens": ["Setz'", "auf", "den", "gr\u00fc\u00b7nen", "Hut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df einen Bock im Holze stehn,", "tokens": ["Ich", "wei\u00df", "ei\u00b7nen", "Bock", "im", "Hol\u00b7ze", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf den will ich heute pirschen gehn.", "tokens": ["Auf", "den", "will", "ich", "heu\u00b7te", "pir\u00b7schen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.