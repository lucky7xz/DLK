{"dta.poem.11818": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zween Br\u00fcder waren einst, der gro\u00df und jener klein,", "tokens": ["Zween", "Br\u00fc\u00b7der", "wa\u00b7ren", "einst", ",", "der", "gro\u00df", "und", "je\u00b7ner", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "$,", "PRELS", "ADJD", "KON", "PDS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eine war zu grob, der andre war zu fein.", "tokens": ["Der", "ei\u00b7ne", "war", "zu", "grob", ",", "der", "and\u00b7re", "war", "zu", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PTKA", "ADJD", "$,", "PRELS", "PIS", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und zwischen beiden stand ein dritter in der Mitte,", "tokens": ["Und", "zwi\u00b7schen", "bei\u00b7den", "stand", "ein", "drit\u00b7ter", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wie ein Fremdling war zu sehn an Wuchs und Sitte.", "tokens": ["Der", "wie", "ein", "Fremd\u00b7ling", "war", "zu", "sehn", "an", "Wuchs", "und", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Wagenmacher hie\u00df all dieser dreie Vater,", "tokens": ["Ein", "Wa\u00b7gen\u00b7ma\u00b7cher", "hie\u00df", "all", "die\u00b7ser", "drei\u00b7e", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "PDAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie alle seine Kunst zu lehren alles that er.", "tokens": ["Sie", "al\u00b7le", "sei\u00b7ne", "Kunst", "zu", "leh\u00b7ren", "al\u00b7les", "that", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PPOSAT", "NN", "PTKZU", "VVINF", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und mit des Himmels Gunst, da keine M\u00fch er spart,", "tokens": ["Und", "mit", "des", "Him\u00b7mels", "Gunst", ",", "da", "kei\u00b7ne", "M\u00fch", "er", "spart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lernen sie all die Kunst, jeder nach seiner Art.", "tokens": ["Ler\u00b7nen", "sie", "all", "die", "Kunst", ",", "je\u00b7der", "nach", "sei\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "NN", "$,", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+---+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Der gro\u00dfe grobe macht den Wagen gro\u00df und grob;", "tokens": ["Der", "gro\u00b7\u00dfe", "gro\u00b7be", "macht", "den", "Wa\u00b7gen", "gro\u00df", "und", "grob", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er nur t\u00fcchtig ist, verdient er auch sein Lob.", "tokens": ["Wenn", "er", "nur", "t\u00fcch\u00b7tig", "ist", ",", "ver\u00b7dient", "er", "auch", "sein", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Wagen ist nicht sch\u00f6n, doch derb und fest, ihn soll", "tokens": ["Der", "Wa\u00b7gen", "ist", "nicht", "sch\u00f6n", ",", "doch", "derb", "und", "fest", ",", "ihn", "soll"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "ADV", "ADJD", "KON", "PTKVZ", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zugochsenvorspann ziehn der schwersten Garben voll.", "tokens": ["Zu\u00b7goch\u00b7sen\u00b7vor\u00b7spann", "ziehn", "der", "schwers\u00b7ten", "Gar\u00b7ben", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Der kleine feine macht den Wagen klein und fein,", "tokens": ["Der", "klei\u00b7ne", "fei\u00b7ne", "macht", "den", "Wa\u00b7gen", "klein", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Arbeit taugt er nicht, zum Spielwerk nur allein.", "tokens": ["Zur", "Ar\u00b7beit", "taugt", "er", "nicht", ",", "zum", "Spiel\u00b7werk", "nur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Arbeit ist so fein, da\u00df sie nicht ganz erschien", "tokens": ["Die", "Ar\u00b7beit", "ist", "so", "fein", ",", "da\u00df", "sie", "nicht", "ganz", "er\u00b7schien"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem blo\u00dfen Aug', ihn soll ein Joch von M\u00fccken ziehn.", "tokens": ["Dem", "blo\u00b7\u00dfen", "Aug'", ",", "ihn", "soll", "ein", "Joch", "von", "M\u00fc\u00b7cken", "ziehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Schon fertig sind die zwei, noch ist zur\u00fcck der dritte,", "tokens": ["Schon", "fer\u00b7tig", "sind", "die", "zwei", ",", "noch", "ist", "zu\u00b7r\u00fcck", "der", "drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "CARD", "$,", "ADV", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er steht in ihrer Mitt' und hielte gern die Mitte.", "tokens": ["Er", "steht", "in", "ih\u00b7rer", "Mitt'", "und", "hiel\u00b7te", "gern", "die", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das Beste von den zwein nimmt er zusammen blo\u00df,", "tokens": ["Das", "Bes\u00b7te", "von", "den", "zwein", "nimmt", "er", "zu\u00b7sam\u00b7men", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er macht den Wagen fein und macht den Wagen gro\u00df.", "tokens": ["Er", "macht", "den", "Wa\u00b7gen", "fein", "und", "macht", "den", "Wa\u00b7gen", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Vollendet ist die Kunst, und auf dem Wagen sann", "tokens": ["Voll\u00b7en\u00b7det", "ist", "die", "Kunst", ",", "und", "auf", "dem", "Wa\u00b7gen", "sann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er stehend, was davor sich zieme zum Gespann.", "tokens": ["Er", "ste\u00b7hend", ",", "was", "da\u00b7vor", "sich", "zie\u00b7me", "zum", "Ge\u00b7spann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PRELS", "PAV", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Da kamen aus der Luft herunter Fl\u00fcgelpferde,", "tokens": ["Da", "ka\u00b7men", "aus", "der", "Luft", "her\u00b7un\u00b7ter", "Fl\u00fc\u00b7gel\u00b7pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ziehn den Wagen an zum Himmel von der Erde.", "tokens": ["Und", "ziehn", "den", "Wa\u00b7gen", "an", "zum", "Him\u00b7mel", "von", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}