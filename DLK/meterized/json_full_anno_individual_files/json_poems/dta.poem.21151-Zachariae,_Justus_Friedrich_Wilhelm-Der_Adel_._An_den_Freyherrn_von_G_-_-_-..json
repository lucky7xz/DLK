{"dta.poem.21151": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Adel .  \n An den Freyherrn von G - - -.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, der Adel, der dich unterscheidet,", "tokens": ["Freund", ",", "der", "A\u00b7del", ",", "der", "dich", "un\u00b7ter\u00b7schei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den der B\u00fcrger spottend oft beneidet,", "tokens": ["Den", "der", "B\u00fcr\u00b7ger", "spot\u00b7tend", "oft", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dieser Vorwurf in so viel Satyren", "tokens": ["Die\u00b7ser", "Vor\u00b7wurf", "in", "so", "viel", "Sa\u00b7ty\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wird dich stets zieren.", "tokens": ["Wird", "dich", "stets", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wer gewohnt ist, so, wie du, zu denken,", "tokens": ["Wer", "ge\u00b7wohnt", "ist", ",", "so", ",", "wie", "du", ",", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$,", "ADV", "$,", "PWAV", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und zur Weisheit seinen Trieb zu lenken,", "tokens": ["Und", "zur", "Weis\u00b7heit", "sei\u00b7nen", "Trieb", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der stolziert nicht auf zerri\u00dfne Fahnen", "tokens": ["Der", "stol\u00b7ziert", "nicht", "auf", "zer\u00b7ri\u00df\u00b7ne", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ruhmwerther Ahnen.", "tokens": ["Ruhm\u00b7wert\u00b7her", "Ah\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Er gebraucht nur, leichter sich zu heben,", "tokens": ["Er", "ge\u00b7braucht", "nur", ",", "leich\u00b7ter", "sich", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was ein Zufall ihm umsonst gegeben;", "tokens": ["Was", "ein", "Zu\u00b7fall", "ihm", "um\u00b7sonst", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da der Ruhm und Glanz von Wapenschilden", "tokens": ["Da", "der", "Ruhm", "und", "Glanz", "von", "Wa\u00b7pen\u00b7schil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht Helden bilden.", "tokens": ["Nicht", "Hel\u00b7den", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Stand und Adel von dem Muth gebohren", "tokens": ["Stand", "und", "A\u00b7del", "von", "dem", "Muth", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wird zur Thorheit bey den stolzen Thoren.", "tokens": ["Wird", "zur", "Thor\u00b7heit", "bey", "den", "stol\u00b7zen", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und wie \u00f6fters bl\u00e4ht die hohe Dame", "tokens": ["Und", "wie", "\u00f6f\u00b7ters", "bl\u00e4ht", "die", "ho\u00b7he", "Da\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nichts, als ihr Name.", "tokens": ["Nichts", ",", "als", "ihr", "Na\u00b7me", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Hat sie etwa angenehmre Wangen?", "tokens": ["Hat", "sie", "et\u00b7wa", "an\u00b7ge\u00b7nehm\u00b7re", "Wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lacht ihr Auge z\u00e4rtlicher Verlangen?", "tokens": ["Lacht", "ihr", "Au\u00b7ge", "z\u00e4rt\u00b7li\u00b7cher", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und zeigt sie uns etwa h\u00f6hre Sinnen", "tokens": ["Und", "zeigt", "sie", "uns", "et\u00b7wa", "h\u00f6h\u00b7re", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als B\u00fcrgerinnen?", "tokens": ["Als", "B\u00fcr\u00b7ge\u00b7rin\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ist der Junker zum Soldaten besser?", "tokens": ["Ist", "der", "Jun\u00b7ker", "zum", "Sol\u00b7da\u00b7ten", "bes\u00b7ser", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist sein Fortgang in der Wei\u00dfheit gr\u00f6\u00dfer?", "tokens": ["Ist", "sein", "Fort\u00b7gang", "in", "der", "Wei\u00df\u00b7heit", "gr\u00f6\u00b7\u00dfer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oder ist er, wenn Parteyen sprechen,", "tokens": ["O\u00b7der", "ist", "er", ",", "wenn", "Par\u00b7te\u00b7yen", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nicht zu bestechen?", "tokens": ["Nicht", "zu", "be\u00b7ste\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "++-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Freund, du wei\u00dft es, einen wahren Weisen", "tokens": ["Freund", ",", "du", "wei\u00dft", "es", ",", "ei\u00b7nen", "wah\u00b7ren", "Wei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mu\u00df die Nachwelt, ohne von, auch preisen;", "tokens": ["Mu\u00df", "die", "Nach\u00b7welt", ",", "oh\u00b7ne", "von", ",", "auch", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "KOUI", "APPR", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da der Ritter, der den Fuchs bekrieget,", "tokens": ["Da", "der", "Rit\u00b7ter", ",", "der", "den", "Fuchs", "be\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vergessen lieget.", "tokens": ["Ver\u00b7ges\u00b7sen", "lie\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Dich G \u2014, braucht kein Stand zu heben;", "tokens": ["Dich", "G", ",", "braucht", "kein", "Stand", "zu", "he\u00b7ben", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$(", "$,", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst ewig durch dich selber leben.", "tokens": ["Du", "wirst", "e\u00b7wig", "durch", "dich", "sel\u00b7ber", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auch als B\u00fcrger m\u00fc\u00dft es dir gelingen,", "tokens": ["Auch", "als", "B\u00fcr\u00b7ger", "m\u00fc\u00dft", "es", "dir", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich hoch zu schwingen.", "tokens": ["Dich", "hoch", "zu", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}