{"textgrid.poem.59322": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Hochzeits\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fernher ziehn wir durch die Gassen,", "tokens": ["Fern\u00b7her", "ziehn", "wir", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn im Regen und im Wind,", "tokens": ["Stehn", "im", "Re\u00b7gen", "und", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl von aller Welt verlassen", "tokens": ["Wohl", "von", "al\u00b7ler", "Welt", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme Musikanten sind.", "tokens": ["Ar\u00b7me", "Mu\u00b7si\u00b7kan\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den Fenstern Geigen klingen,", "tokens": ["Aus", "den", "Fens\u00b7tern", "Gei\u00b7gen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleift und dreht sich's bunt und laut,", "tokens": ["Schleift", "und", "dreht", "sich's", "bunt", "und", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir Musikanten singen", "tokens": ["Und", "wir", "Mu\u00b7si\u00b7kan\u00b7ten", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drau\u00dfen da der reichen Braut.", "tokens": ["Drau\u00b7\u00dfen", "da", "der", "rei\u00b7chen", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollt sie doch keinen andern haben,", "tokens": ["Wollt", "sie", "doch", "kei\u00b7nen", "an\u00b7dern", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ging mit mir durch Wald und Feld,", "tokens": ["Ging", "mit", "mir", "durch", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pr\u00e4chtig in den blauen Tagen", "tokens": ["Pr\u00e4ch\u00b7tig", "in", "den", "blau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien die Sonne auf die Welt.", "tokens": ["Schien", "die", "Son\u00b7ne", "auf", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heisa: lustig Drehn und Ringen,", "tokens": ["Hei\u00b7sa", ":", "lus\u00b7tig", "Drehn", "und", "Rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "CARD", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder h\u00e4lt sein Liebchen warm,", "tokens": ["Je\u00b7der", "h\u00e4lt", "sein", "Lieb\u00b7chen", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wir Musikanten singen", "tokens": ["Und", "wir", "Mu\u00b7si\u00b7kan\u00b7ten", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lustig so, da\u00df Gott erbarm.", "tokens": ["Lus\u00b7tig", "so", ",", "da\u00df", "Gott", "er\u00b7barm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lachend reicht man uns die Neigen,", "tokens": ["La\u00b7chend", "reicht", "man", "uns", "die", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf ihr Wohlsein trinken wir;", "tokens": ["Auf", "ihr", "Wohl\u00b7sein", "trin\u00b7ken", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt sie sich am Fenster zeigen,", "tokens": ["Wollt", "sie", "sich", "am", "Fens\u00b7ter", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "'s w\u00e4re doch recht fein von ihr.", "tokens": ["'s", "w\u00e4\u00b7re", "doch", "recht", "fein", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wir fiedeln und wir singen", "tokens": ["Und", "wir", "fie\u00b7deln", "und", "wir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche sch\u00f6ne Melodei,", "tokens": ["Man\u00b7che", "sch\u00f6\u00b7ne", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die besten Saiten springen,", "tokens": ["Da\u00df", "die", "bes\u00b7ten", "Sai\u00b7ten", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "'s war, als spr\u00e4ng mir's Herz entzwei.", "tokens": ["'s", "war", ",", "als", "spr\u00e4ng", "mir's", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NE", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt ist Schmaus und Tanz zerstoben,", "tokens": ["Jetzt", "ist", "Schmaus", "und", "Tanz", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer stiller wird's im Haus,", "tokens": ["Im\u00b7mer", "stil\u00b7ler", "wird's", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die M\u00e4gde putzen oben", "tokens": ["Und", "die", "M\u00e4g\u00b7de", "put\u00b7zen", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle lust'gen Kerzen aus.", "tokens": ["Al\u00b7le", "lust'\u00b7gen", "Ker\u00b7zen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wir blasen recht mit Rasen", "tokens": ["Doch", "wir", "bla\u00b7sen", "recht", "mit", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder in sein Instrument,", "tokens": ["Je\u00b7der", "in", "sein", "Inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6cht in meinem Grimm ausblasen", "tokens": ["M\u00f6cht", "in", "mei\u00b7nem", "Grimm", "aus\u00b7bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Stern am Firmament!", "tokens": ["Al\u00b7le", "Stern", "am", "Fir\u00b7ma\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und am Hause seine Runde", "tokens": ["Und", "am", "Hau\u00b7se", "sei\u00b7ne", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt der W\u00e4chter g\u00e4hnend an,", "tokens": ["Tritt", "der", "W\u00e4ch\u00b7ter", "g\u00e4h\u00b7nend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rufet aus die Schlafensstunde,", "tokens": ["Ru\u00b7fet", "aus", "die", "Schla\u00b7fens\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sieht ganz erbost uns an.", "tokens": ["Und", "sieht", "ganz", "er\u00b7bost", "uns", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch nach ihrem Kabinette", "tokens": ["Doch", "nach", "ih\u00b7rem", "Ka\u00b7bi\u00b7net\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwing ich noch mein Tamburin,", "tokens": ["Schwing", "ich", "noch", "mein", "Tam\u00b7bu\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fahr wohl in dein Himmelbette,", "tokens": ["Fahr", "wohl", "in", "dein", "Him\u00b7mel\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil wir m\u00fcssen weiterziehn!", "tokens": ["Weil", "wir", "m\u00fcs\u00b7sen", "wei\u00b7ter\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fernher ziehn wir durch die Gassen,", "tokens": ["Fern\u00b7her", "ziehn", "wir", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn im Regen und im Wind,", "tokens": ["Stehn", "im", "Re\u00b7gen", "und", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl von aller Welt verlassen", "tokens": ["Wohl", "von", "al\u00b7ler", "Welt", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme Musikanten sind.", "tokens": ["Ar\u00b7me", "Mu\u00b7si\u00b7kan\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den Fenstern Geigen klingen,", "tokens": ["Aus", "den", "Fens\u00b7tern", "Gei\u00b7gen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleift und dreht sich's bunt und laut,", "tokens": ["Schleift", "und", "dreht", "sich's", "bunt", "und", "laut", ","], "token_info