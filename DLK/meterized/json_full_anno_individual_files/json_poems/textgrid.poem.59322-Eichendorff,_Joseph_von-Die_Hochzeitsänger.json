 ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "'s w\u00e4re doch recht fein von ihr.", "tokens": ["'s", "w\u00e4\u00b7re", "doch", "recht", "fein", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wir fiedeln und wir singen", "tokens": ["Und", "wir", "fie\u00b7deln", "und", "wir", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche sch\u00f6ne Melodei,", "tokens": ["Man\u00b7che", "sch\u00f6\u00b7ne", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die besten Saiten springen,", "tokens": ["Da\u00df", "die", "bes\u00b7ten", "Sai\u00b7ten", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "'s war, als spr\u00e4ng mir's Herz entzwei.", "tokens": ["'s", "war", ",", "als", "spr\u00e4ng", "mir's", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NE", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Jetzt ist Schmaus und Tanz zerstoben,", "tokens": ["Jetzt", "ist", "Schmaus", "und", "Tanz", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer stiller wird's im Haus,", "tokens": ["Im\u00b7mer", "stil\u00b7ler", "wird's", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die M\u00e4gde putzen oben", "tokens": ["Und", "die", "M\u00e4g\u00b7de", "put\u00b7zen", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle lust'gen Kerzen aus.", "tokens": ["Al\u00b7le", "lust'\u00b7gen", "Ker\u00b7zen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wir blasen recht mit Rasen", "tokens": ["Doch", "wir", "bla\u00b7sen", "recht", "mit", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder in sein Instrument,", "tokens": ["Je\u00b7der", "in", "sein", "Inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6cht in meinem Grimm ausblasen", "tokens": ["M\u00f6cht", "in", "mei\u00b7nem", "Grimm", "aus\u00b7bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Stern am Firmament!", "tokens": ["Al\u00b7le", "Stern", "am", "Fir\u00b7ma\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und am Hause seine Runde", "tokens": ["Und", "am", "Hau\u00b7se", "sei\u00b7ne", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt der W\u00e4chter g\u00e4hnend an,", "tokens": ["Tritt", "der", "W\u00e4ch\u00b7ter", "g\u00e4h\u00b7nend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rufet aus die Schlafensstunde,", "tokens": ["Ru\u00b7fet", "aus", "die", "Schla\u00b7fens\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sieht ganz erbost uns an.", "tokens": ["Und", "sieht", "ganz", "er\u00b7bost", "uns", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch nach ihrem Kabinette", "tokens": ["Doch", "nach", "ih\u00b7rem", "Ka\u00b7bi\u00b7net\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwing ich noch mein Tamburin,", "tokens": ["Schwing", "ich", "noch", "mein", "Tam\u00b7bu\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fahr wohl in dein Himmelbette,", "tokens": ["Fahr", "wohl", "in", "dein", "Him\u00b7mel\u00b7bet\u00b7te", ","], "token_info":