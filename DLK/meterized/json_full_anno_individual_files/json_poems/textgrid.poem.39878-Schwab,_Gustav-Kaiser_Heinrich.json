{"textgrid.poem.39878": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Heinrich", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzog Heinrich war's von Baiern,", "tokens": ["Her\u00b7zog", "Hein\u00b7rich", "wa\u00b7r's", "von", "Bai\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich in der Mitternacht,", "tokens": ["Der", "sich", "in", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die fr\u00f6mmsten Br\u00fcder feiern,", "tokens": ["Wo", "die", "fr\u00f6mms\u00b7ten", "Br\u00fc\u00b7der", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin zur Kirchen aufgemacht.", "tokens": ["Hin", "zur", "Kir\u00b7chen", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ernste Bilder nach ihm fassen,", "tokens": ["Erns\u00b7te", "Bil\u00b7der", "nach", "ihm", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treiben ihn zum Beten an,", "tokens": ["Trei\u00b7ben", "ihn", "zum", "Be\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die Regenspurger Gassen", "tokens": ["Durch", "die", "Re\u00b7gen\u00b7spur\u00b7ger", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geht er nach Sankt Heimeran.", "tokens": ["Geht", "er", "nach", "Sankt", "Hei\u00b7me\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Junges Heldenantlitz betend", "tokens": ["Jun\u00b7ges", "Hel\u00b7den\u00b7ant\u00b7litz", "be\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ein sch\u00f6ner Anblick sein,", "tokens": ["M\u00f6cht'", "ein", "sch\u00f6\u00b7ner", "An\u00b7blick", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser zum Altare tretend", "tokens": ["Die\u00b7ser", "zum", "Al\u00b7ta\u00b7re", "tre\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kniet umnachtet und allein.", "tokens": ["Kniet", "um\u00b7nach\u00b7tet", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor den Augen gar die H\u00e4nde,", "tokens": ["Vor", "den", "Au\u00b7gen", "gar", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcckend jedes Bild zur\u00fcck,", "tokens": ["Dr\u00fc\u00b7ckend", "je\u00b7des", "Bild", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fleht er um ein sel'ges Ende,", "tokens": ["Fleht", "er", "um", "ein", "sel'\u00b7ges", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht um irdisch Heil und Gl\u00fcck.", "tokens": ["Nicht", "um", "ir\u00b7disch", "Heil", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als er aufstand, schien's vom R\u00fccken", "tokens": ["Als", "er", "auf\u00b7stand", ",", "schien's", "vom", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber ihn, als wie ein Licht,", "tokens": ["Ue\u00b7ber", "ihn", ",", "als", "wie", "ein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Staunend th\u00e4t er um sich blicken,", "tokens": ["Stau\u00b7nend", "th\u00e4t", "er", "um", "sich", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht ein heil'ges Angesicht.", "tokens": ["Sieht", "ein", "heil'\u00b7ges", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hochaltar und Kreuz verkl\u00e4rend", "tokens": ["Hoc\u00b7hal\u00b7tar", "und", "Kreuz", "ver\u00b7kl\u00e4\u00b7rend"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dort ein lichter Bischof stand,", "tokens": ["Dort", "ein", "lich\u00b7ter", "Bi\u00b7schof", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der mit hoher Hand, wie schw\u00f6rend,", "tokens": ["Der", "mit", "ho\u00b7her", "Hand", ",", "wie", "schw\u00f6\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeigte nach der Kirchenwand.", "tokens": ["Zeig\u00b7te", "nach", "der", "Kir\u00b7chen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit den Fingern, wie mit Kerzen,", "tokens": ["Mit", "den", "Fin\u00b7gern", ",", "wie", "mit", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtet er auf eine Schrift,", "tokens": ["Leuch\u00b7tet", "er", "auf", "ei\u00b7ne", "Schrift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der F\u00fcrst mit bangem Herzen", "tokens": ["Wo", "der", "F\u00fcrst", "mit", "ban\u00b7gem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf ein r\u00f6misch Sechse trifft.", "tokens": ["Auf", "ein", "r\u00f6\u00b7misch", "Sech\u00b7se", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbwill mich Gott so bald erh\u00f6ren?", "tokens": ["\u00bb", "will", "mich", "Gott", "so", "bald", "er\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herr, ich glaub's auf Eure Hand,", "tokens": ["Herr", ",", "ich", "glaub's", "auf", "Eu\u00b7re", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hebt sie nicht so ernst zum Schw\u00f6ren!\u00ab", "tokens": ["Hebt", "sie", "nicht", "so", "ernst", "zum", "Schw\u00f6\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprach der Held, und Alles schwand.", "tokens": ["Sprach", "der", "Held", ",", "und", "Al\u00b7les", "schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie sechs Stunden sind vergangen,", "tokens": ["Wie", "sechs", "Stun\u00b7den", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrt' er fromm auf seinen Tod,", "tokens": ["Harrt'", "er", "fromm", "auf", "sei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es schien ihm auf die Wangen", "tokens": ["Doch", "es", "schien", "ihm", "auf", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebenshell das Morgenrot.", "tokens": ["Le\u00b7bens\u00b7hell", "das", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der sechste Tag gekommen,", "tokens": ["Wie", "der", "sechs\u00b7te", "Tag", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er bereit und fertig ist,", "tokens": ["Er", "be\u00b7reit", "und", "fer\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch es giebt der Herr dem Frommen", "tokens": ["Doch", "es", "giebt", "der", "Herr", "dem", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neue heitre Lebensfrist.", "tokens": ["Neu\u00b7e", "heit\u00b7re", "Le\u00b7bens\u00b7frist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Darum h\u00e4lt er an mit Beten,", "tokens": ["Da\u00b7rum", "h\u00e4lt", "er", "an", "mit", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis der sechste Mond erscheint,", "tokens": ["Bis", "der", "sechs\u00b7te", "Mond", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd'ger stets vor Gott zu treten,", "tokens": ["W\u00fcrd'\u00b7ger", "stets", "vor", "Gott", "zu", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es war nicht so gemeint.", "tokens": ["Doch", "es", "war", "nicht", "so", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ernste Todsgedanken", "tokens": ["A\u00b7ber", "erns\u00b7te", "Tods\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandeln mit ihm immerdar,", "tokens": ["Wan\u00b7deln", "mit", "ihm", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so lebt er sonder Wanken", "tokens": ["Und", "so", "lebt", "er", "son\u00b7der", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heilig bis in's sechste Jahr.", "tokens": ["Hei\u00b7lig", "bis", "in's", "sechs\u00b7te", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und in hoher Kirche stand er", "tokens": ["Und", "in", "ho\u00b7her", "Kir\u00b7che", "stand", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtend um das sechste Jahr,", "tokens": ["Leuch\u00b7tend", "um", "das", "sechs\u00b7te", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf seinem Haupte fand er", "tokens": ["Und", "auf", "sei\u00b7nem", "Haup\u00b7te", "fand", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00f6m'sche K\u00f6nigskrone gar.", "tokens": ["R\u00f6m'\u00b7sche", "K\u00f6\u00b7nigs\u00b7kro\u00b7ne", "gar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nig Heinrich war's der Zweite,", "tokens": ["K\u00f6\u00b7nig", "Hein\u00b7rich", "wa\u00b7r's", "der", "Zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Herr von allem deutschen Land,", "tokens": ["Herr", "von", "al\u00b7lem", "deut\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der von dort an ward bis heute", "tokens": ["Der", "von", "dort", "an", "ward", "bis", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "APZR", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Stets der Heilige genannt.", "tokens": ["Stets", "der", "Hei\u00b7li\u00b7ge", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Zwei und zwanzig Jahre heilig", "tokens": ["Zwei", "und", "zwan\u00b7zig", "Jah\u00b7re", "hei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "KON", "CARD", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrscht' er ohne Fluch und Spott,", "tokens": ["Herrscht'", "er", "oh\u00b7ne", "Fluch", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die r\u00f6m'sche Sechse treulich", "tokens": ["An", "die", "r\u00f6m'\u00b7sche", "Sech\u00b7se", "treu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dacht' er, und an Tod und Gott.", "tokens": ["Dacht'", "er", ",", "und", "an", "Tod", "und", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er fertig war zum Sterben", "tokens": ["Weil", "er", "fer\u00b7tig", "war", "zum", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hielt ihn Gott des Lebens wert,", "tokens": ["Hielt", "ihn", "Gott", "des", "Le\u00b7bens", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil den Himmel er konnt' erben,", "tokens": ["Weil", "den", "Him\u00b7mel", "er", "konnt'", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Ward ihm auch das Reich bescheert.", "tokens": ["Ward", "ihm", "auch", "das", "Reich", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Herzog Heinrich war's von Baiern,", "tokens": ["Her\u00b7zog", "Hein\u00b7rich", "wa\u00b7r's", "von", "Bai\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich in der Mitternacht,", "tokens": ["Der", "sich", "in", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die fr\u00f6mmsten Br\u00fcder feiern,", "tokens": ["Wo", "die", "fr\u00f6mms\u00b7ten", "Br\u00fc\u00b7der", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin zur Kirchen aufgemacht.", "tokens": ["Hin", "zur", "Kir\u00b7chen", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ernste Bilder nach ihm fassen,", "tokens": ["Erns\u00b7te", "Bil\u00b7der", "nach", "ihm", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treiben ihn zum Beten an,", "tokens": ["Trei\u00b7ben", "ihn", "zum", "Be\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die Regenspurger Gassen", "tokens": ["Durch", "die", "Re\u00b7gen\u00b7spur\u00b7ger", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geht er nach Sankt Heimeran.", "tokens": ["Geht", "er", "nach", "Sankt", "Hei\u00b7me\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Junges Heldenantlitz betend", "tokens": ["Jun\u00b7ges", "Hel\u00b7den\u00b7ant\u00b7litz", "be\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ein sch\u00f6ner Anblick sein,", "tokens": ["M\u00f6cht'", "ein", "sch\u00f6\u00b7ner", "An\u00b7blick", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser zum Altare tretend", "tokens": ["Die\u00b7ser", "zum", "Al\u00b7ta\u00b7re", "tre\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kniet umnachtet und allein.", "tokens": ["Kniet", "um\u00b7nach\u00b7tet", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor den Augen gar die H\u00e4nde,", "tokens": ["Vor", "den", "Au\u00b7gen", "gar", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcckend jedes Bild zur\u00fcck,", "tokens": ["Dr\u00fc\u00b7ckend", "je\u00b7des", "Bild", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fleht er um ein sel'ges Ende,", "tokens": ["Fleht", "er", "um", "ein", "sel'\u00b7ges", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht um irdisch Heil und Gl\u00fcck.", "tokens": ["Nicht", "um", "ir\u00b7disch", "Heil", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Als er aufstand, schien's vom R\u00fccken", "tokens": ["Als", "er", "auf\u00b7stand", ",", "schien's", "vom", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber ihn, als wie ein Licht,", "tokens": ["Ue\u00b7ber", "ihn", ",", "als", "wie", "ein", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Staunend th\u00e4t er um sich blicken,", "tokens": ["Stau\u00b7nend", "th\u00e4t", "er", "um", "sich", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht ein heil'ges Angesicht.", "tokens": ["Sieht", "ein", "heil'\u00b7ges", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hochaltar und Kreuz verkl\u00e4rend", "tokens": ["Hoc\u00b7hal\u00b7tar", "und", "Kreuz", "ver\u00b7kl\u00e4\u00b7rend"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dort ein lichter Bischof stand,", "tokens": ["Dort", "ein", "lich\u00b7ter", "Bi\u00b7schof", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der mit hoher Hand, wie schw\u00f6rend,", "tokens": ["Der", "mit", "ho\u00b7her", "Hand", ",", "wie", "schw\u00f6\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeigte nach der Kirchenwand.", "tokens": ["Zeig\u00b7te", "nach", "der", "Kir\u00b7chen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Mit den Fingern, wie mit Kerzen,", "tokens": ["Mit", "den", "Fin\u00b7gern", ",", "wie", "mit", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtet er auf eine Schrift,", "tokens": ["Leuch\u00b7tet", "er", "auf", "ei\u00b7ne", "Schrift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der F\u00fcrst mit bangem Herzen", "tokens": ["Wo", "der", "F\u00fcrst", "mit", "ban\u00b7gem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf ein r\u00f6misch Sechse trifft.", "tokens": ["Auf", "ein", "r\u00f6\u00b7misch", "Sech\u00b7se", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbwill mich Gott so bald erh\u00f6ren?", "tokens": ["\u00bb", "will", "mich", "Gott", "so", "bald", "er\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herr, ich glaub's auf Eure Hand,", "tokens": ["Herr", ",", "ich", "glaub's", "auf", "Eu\u00b7re", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hebt sie nicht so ernst zum Schw\u00f6ren!\u00ab", "tokens": ["Hebt", "sie", "nicht", "so", "ernst", "zum", "Schw\u00f6\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sprach der Held, und Alles schwand.", "tokens": ["Sprach", "der", "Held", ",", "und", "Al\u00b7les", "schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie sechs Stunden sind vergangen,", "tokens": ["Wie", "sechs", "Stun\u00b7den", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrt' er fromm auf seinen Tod,", "tokens": ["Harrt'", "er", "fromm", "auf", "sei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es schien ihm auf die Wangen", "tokens": ["Doch", "es", "schien", "ihm", "auf", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebenshell das Morgenrot.", "tokens": ["Le\u00b7bens\u00b7hell", "das", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der sechste Tag gekommen,", "tokens": ["Wie", "der", "sechs\u00b7te", "Tag", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er bereit und fertig ist,", "tokens": ["Er", "be\u00b7reit", "und", "fer\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch es giebt der Herr dem Frommen", "tokens": ["Doch", "es", "giebt", "der", "Herr", "dem", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neue heitre Lebensfrist.", "tokens": ["Neu\u00b7e", "heit\u00b7re", "Le\u00b7bens\u00b7frist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Darum h\u00e4lt er an mit Beten,", "tokens": ["Da\u00b7rum", "h\u00e4lt", "er", "an", "mit", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis der sechste Mond erscheint,", "tokens": ["Bis", "der", "sechs\u00b7te", "Mond", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd'ger stets vor Gott zu treten,", "tokens": ["W\u00fcrd'\u00b7ger", "stets", "vor", "Gott", "zu", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es war nicht so gemeint.", "tokens": ["Doch", "es", "war", "nicht", "so", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ernste Todsgedanken", "tokens": ["A\u00b7ber", "erns\u00b7te", "Tods\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandeln mit ihm immerdar,", "tokens": ["Wan\u00b7deln", "mit", "ihm", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so lebt er sonder Wanken", "tokens": ["Und", "so", "lebt", "er", "son\u00b7der", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heilig bis in's sechste Jahr.", "tokens": ["Hei\u00b7lig", "bis", "in's", "sechs\u00b7te", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und in hoher Kirche stand er", "tokens": ["Und", "in", "ho\u00b7her", "Kir\u00b7che", "stand", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtend um das sechste Jahr,", "tokens": ["Leuch\u00b7tend", "um", "das", "sechs\u00b7te", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf seinem Haupte fand er", "tokens": ["Und", "auf", "sei\u00b7nem", "Haup\u00b7te", "fand", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00f6m'sche K\u00f6nigskrone gar.", "tokens": ["R\u00f6m'\u00b7sche", "K\u00f6\u00b7nigs\u00b7kro\u00b7ne", "gar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nig Heinrich war's der Zweite,", "tokens": ["K\u00f6\u00b7nig", "Hein\u00b7rich", "wa\u00b7r's", "der", "Zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Herr von allem deutschen Land,", "tokens": ["Herr", "von", "al\u00b7lem", "deut\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der von dort an ward bis heute", "tokens": ["Der", "von", "dort", "an", "ward", "bis", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "APZR", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Stets der Heilige genannt.", "tokens": ["Stets", "der", "Hei\u00b7li\u00b7ge", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Zwei und zwanzig Jahre heilig", "tokens": ["Zwei", "und", "zwan\u00b7zig", "Jah\u00b7re", "hei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "KON", "CARD", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrscht' er ohne Fluch und Spott,", "tokens": ["Herrscht'", "er", "oh\u00b7ne", "Fluch", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die r\u00f6m'sche Sechse treulich", "tokens": ["An", "die", "r\u00f6m'\u00b7sche", "Sech\u00b7se", "treu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dacht' er, und an Tod und Gott.", "tokens": ["Dacht'", "er", ",", "und", "an", "Tod", "und", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er fertig war zum Sterben", "tokens": ["Weil", "er", "fer\u00b7tig", "war", "zum", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hielt ihn Gott des Lebens wert,", "tokens": ["Hielt", "ihn", "Gott", "des", "Le\u00b7bens", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil den Himmel er konnt' erben,", "tokens": ["Weil", "den", "Him\u00b7mel", "er", "konnt'", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Ward ihm auch das Reich bescheert.", "tokens": ["Ward", "ihm", "auch", "das", "Reich", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}