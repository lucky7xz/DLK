{"textgrid.poem.53539": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Christian Wagner", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist dies nicht ein frevles Schicksalswalten,", "tokens": ["Ist", "dies", "nicht", "ein", "frev\u00b7les", "Schick\u00b7sals\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Menschtum in zwei Teile zu zerspalten?", "tokens": ["Menschtum", "in", "zwei", "Tei\u00b7le", "zu", "zer\u00b7spal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In zwei blutige H\u00e4lften zu zerrei\u00dfen,", "tokens": ["In", "zwei", "blu\u00b7ti\u00b7ge", "H\u00e4lf\u00b7ten", "zu", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eine Mann, die andre Weib zu hei\u00dfen?", "tokens": ["Ei\u00b7ne", "Mann", ",", "die", "and\u00b7re", "Weib", "zu", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Beide voll von hei\u00dfem Sehnsuchtsdrange,", "tokens": ["Bei\u00b7de", "voll", "von", "hei\u00b7\u00dfem", "Sehn\u00b7suchts\u00b7dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich zu finden auf des Lebens Gange,", "tokens": ["Sich", "zu", "fin\u00b7den", "auf", "des", "Le\u00b7bens", "Gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ich dem Ich zur Opfergab zu bringen?", "tokens": ["Ich", "dem", "Ich", "zur", "Op\u00b7fer\u00b7gab", "zu", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach wie wenigen, wenigen mags gelingen,", "tokens": ["Ach", "wie", "we\u00b7ni\u00b7gen", ",", "we\u00b7ni\u00b7gen", "mags", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "$,", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Ohne Losung, F\u00e4hrten oder Spuren", "tokens": ["Oh\u00b7ne", "Lo\u00b7sung", ",", "F\u00e4hr\u00b7ten", "o\u00b7der", "Spu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich zu finden auf des Lebens Fluren!", "tokens": ["Sich", "zu", "fin\u00b7den", "auf", "des", "Le\u00b7bens", "Flu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Selige Kindheit, die nicht kennt die Wirre