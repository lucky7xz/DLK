word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Deck auf, o Ruhm, die Narben,", "tokens": ["Deck", "auf", ",", "o", "Ruhm", ",", "die", "Nar\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mach unsre Namen klar!", "tokens": ["Mach", "uns\u00b7re", "Na\u00b7men", "klar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du machst den Schmerz gesetzter,", "tokens": ["Du", "machst", "den", "Schmerz", "ge\u00b7setz\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn unsres Volkes letzter", "tokens": ["Denn", "uns\u00b7res", "Vol\u00b7kes", "letz\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist gr\u00f6\u00dfer als der Zar.", "tokens": ["Ist", "gr\u00f6\u00b7\u00dfer", "als", "der", "Zar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Uns bleibt nur Ein Verm\u00e4chtnis:", "tokens": ["Uns", "bleibt", "nur", "Ein", "Ver\u00b7m\u00e4cht\u00b7nis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des edlen Kampfs Ged\u00e4chtnis,", "tokens": ["Des", "ed\u00b7len", "Kampfs", "Ge\u00b7d\u00e4cht\u00b7nis", ","], 