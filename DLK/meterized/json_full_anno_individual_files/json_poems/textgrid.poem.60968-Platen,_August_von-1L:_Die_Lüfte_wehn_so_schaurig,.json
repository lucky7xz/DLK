{"textgrid.poem.60968": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die L\u00fcfte wehn so schaurig,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00fcfte wehn so schaurig,", "tokens": ["Die", "L\u00fcf\u00b7te", "wehn", "so", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir ziehn dahin so traurig", "tokens": ["Wir", "ziehn", "da\u00b7hin", "so", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach ungewissem Ziel.", "tokens": ["Nach", "un\u00b7ge\u00b7wis\u00b7sem", "Ziel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kaum leuchten uns die Sterne:", "tokens": ["Kaum", "leuch\u00b7ten", "uns", "die", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Europa sieht von ferne", "tokens": ["Eu\u00b7ro\u00b7pa", "sieht", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das gro\u00dfe Trauerspiel.", "tokens": ["Das", "gro\u00b7\u00dfe", "Trau\u00b7er\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Uns wendend oft zur\u00fccke,", "tokens": ["Uns", "wen\u00b7dend", "oft", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Betreten wir die Br\u00fccke,", "tokens": ["Be\u00b7tre\u00b7ten", "wir", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die uns von Polen trennt.", "tokens": ["Die", "uns", "von", "Po\u00b7len", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bei tr\u00fcbem Fackelbrande", "tokens": ["Bei", "tr\u00fc\u00b7bem", "Fa\u00b7ckel\u00b7bran\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gr\u00fc\u00dft uns das Volk am Strande,", "tokens": ["Gr\u00fc\u00dft", "uns", "das", "Volk", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das unsre Leiden kennt.", "tokens": ["Das", "uns\u00b7re", "Lei\u00b7den", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verkauft, besiegt, verraten \u2013", "tokens": ["Ver\u00b7kauft", ",", "be\u00b7siegt", ",", "ver\u00b7ra\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind unsre besten Taten", "tokens": ["Sind", "uns\u00b7re", "bes\u00b7ten", "Ta\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Tr\u00e4ume leer und hohl", "tokens": ["Wie", "Tr\u00e4u\u00b7me", "leer", "und", "hohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "KON", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und lassen keine Spuren;", "tokens": ["Und", "las\u00b7sen", "kei\u00b7ne", "Spu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So nehmt, geliebte Fluren,", "tokens": ["So", "nehmt", ",", "ge\u00b7lieb\u00b7te", "Flu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das letzte Lebewohl!", "tokens": ["Das", "letz\u00b7te", "Le\u00b7be\u00b7wohl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lebt ewig wohl, ihr Br\u00fcder!", "tokens": ["Lebt", "e\u00b7wig", "wohl", ",", "ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Haufe Lebensm\u00fcder", "tokens": ["Ein", "Hau\u00b7fe", "Le\u00b7bens\u00b7m\u00fc\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Trifft \u00fcberall ein Grab.", "tokens": ["Trifft", "\u00fc\u00b7be\u00b7rall", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht uns vom Tod zu retten,", "tokens": ["Nicht", "uns", "vom", "Tod", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nein, nur zu fliehn die Ketten,", "tokens": ["Nein", ",", "nur", "zu", "fliehn", "die", "Ket\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ergreifen wir den Stab.", "tokens": ["Er\u00b7grei\u00b7fen", "wir", "den", "Stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir ziehn von Weib und Kindern,", "tokens": ["Wir", "ziehn", "von", "Weib", "und", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verm\u00f6gen nicht zu hindern", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "nicht", "zu", "hin\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Vaterlands Ruin.", "tokens": ["Des", "Va\u00b7ter\u00b7lands", "Ru\u00b7in", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schon lechzt nach unserm Blute", "tokens": ["Schon", "lechzt", "nach", "un\u00b7serm", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Petersburger Knute,", "tokens": ["Die", "Pe\u00b7ters\u00b7bur\u00b7ger", "Knu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Fuchtel von Berlin.", "tokens": ["Die", "Fuch\u00b7tel", "von", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein tr\u00e4nenloses Wesen", "tokens": ["Ein", "tr\u00e4\u00b7nen\u00b7lo\u00b7ses", "We\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward uns zum Herrn erlesen,", "tokens": ["Ward", "uns", "zum", "Herrn", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Versteint und ungebeugt:", "tokens": ["Ver\u00b7steint", "und", "un\u00b7ge\u00b7beugt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus m\u00f6rderischem Stamme", "tokens": ["Aus", "m\u00f6r\u00b7de\u00b7ri\u00b7schem", "Stam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tr\u00e4gt seine Stirn die Schramme,", "tokens": ["Tr\u00e4gt", "sei\u00b7ne", "Stirn", "die", "Schram\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die sein Geschlecht bezeugt.", "tokens": ["Die", "sein", "Ge\u00b7schlecht", "be\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die wir jedoch erwarben,", "tokens": ["Die", "wir", "je\u00b7doch", "er\u00b7war\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Deck auf, o Ruhm, die Narben,", "tokens": ["Deck", "auf", ",", "o", "Ruhm", ",", "die", "Nar\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mach unsre Namen klar!", "tokens": ["Mach", "uns\u00b7re", "Na\u00b7men", "klar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du machst den Schmerz gesetzter,", "tokens": ["Du", "machst", "den", "Schmerz", "ge\u00b7setz\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn unsres Volkes letzter", "tokens": ["Denn", "uns\u00b7res", "Vol\u00b7kes", "letz\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist gr\u00f6\u00dfer als der Zar.", "tokens": ["Ist", "gr\u00f6\u00b7\u00dfer", "als", "der", "Zar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Uns bleibt nur Ein Verm\u00e4chtnis:", "tokens": ["Uns", "bleibt", "nur", "Ein", "Ver\u00b7m\u00e4cht\u00b7nis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des edlen Kampfs Ged\u00e4chtnis,", "tokens": ["Des", "ed\u00b7len", "Kampfs", "Ge\u00b7d\u00e4cht\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Polen neu verband,", "tokens": ["Der", "Po\u00b7len", "neu", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des langen Kriegs Beschwerde", "tokens": ["Des", "lan\u00b7gen", "Kriegs", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und eine Handvoll Erde", "tokens": ["Und", "ei\u00b7ne", "Hand\u00b7voll", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus unserm Vaterland.", "tokens": ["Aus", "un\u00b7serm", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O selig Jene, welche", "tokens": ["O", "se\u00b7lig", "Je\u00b7ne", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ADJD", "PDS", "$,", "PRELS"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Berauscht vom Todeskelche,", "tokens": ["Be\u00b7rauscht", "vom", "To\u00b7des\u00b7kel\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesunken sind im Streit.", "tokens": ["Ge\u00b7sun\u00b7ken", "sind", "im", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ihr, Volhyniens S\u00f6hne,", "tokens": ["Und", "ihr", ",", "Vol\u00b7hy\u00b7ni\u00b7ens", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die aus dem Angstgest\u00f6hne", "tokens": ["Die", "aus", "dem", "Angst\u00b7ge\u00b7st\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die feuchte Gruft befreit!", "tokens": ["Die", "feuch\u00b7te", "Gruft", "be\u00b7freit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie drangen auf den Rossen,", "tokens": ["Sie", "dran\u00b7gen", "auf", "den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Feinden fast umschlossen,", "tokens": ["Von", "Fein\u00b7den", "fast", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Weichselufer vor,", "tokens": ["Zum", "Weich\u00b7se\u00b7lu\u00b7fer", "vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An fremden Strand zu schiffen:", "tokens": ["An", "frem\u00b7den", "Strand", "zu", "schif\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da schwoll von Schmerz ergriffen", "tokens": ["Da", "schwoll", "von", "Schmerz", "er\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr gro\u00df Gem\u00fct empor.", "tokens": ["Ihr", "gro\u00df", "Ge\u00b7m\u00fct", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie konnten's nicht ertragen,", "tokens": ["Sie", "konn\u00b7ten's", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Heimat abzusagen,", "tokens": ["Der", "Hei\u00b7mat", "ab\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die jeden Wunsch umschlo\u00df.", "tokens": ["Die", "je\u00b7den", "Wunsch", "um\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da st\u00fcrzten sich die Guten", "tokens": ["Da", "st\u00fcrz\u00b7ten", "sich", "die", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hinunter in die Fluten", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "me