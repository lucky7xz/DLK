{"textgrid.poem.37793": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Jesaias Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesaia dem Propheten dies geschah,", "tokens": ["Je\u00b7sai\u00b7a", "dem", "Pro\u00b7phe\u00b7ten", "dies", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Da\u00df er im Geist den Herren sitzen sah", "tokens": ["Da\u00df", "er", "im", "Geist", "den", "Her\u00b7ren", "sit\u00b7zen", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf einem hohen Thron und hellen Glanz,", "tokens": ["Auf", "ei\u00b7nem", "ho\u00b7hen", "Thron", "und", "hel\u00b7len", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Seines Kleides Saum den Chor f\u00fcllet ganz,", "tokens": ["Sei\u00b7nes", "Klei\u00b7des", "Saum", "den", "Chor", "f\u00fcl\u00b7let", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Es stunden zween Seraph bey ihm dran,", "tokens": ["Es", "stun\u00b7den", "zween", "Se\u00b7raph", "bey", "ihm", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "NN", "APPR", "PPER", "PAV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sechs Fl\u00fcgel sah er einen jeden han,", "tokens": ["Sechs", "Fl\u00fc\u00b7gel", "sah", "er", "ei\u00b7nen", "je\u00b7den", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ART", "PIAT", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit zween verbargen sie ihr Antlitz klar,", "tokens": ["Mit", "zween", "ver\u00b7bar\u00b7gen", "sie", "ihr", "Ant\u00b7litz", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit zween bedeckten sie ihre F\u00fc\u00dfe gar,", "tokens": ["Mit", "zween", "be\u00b7deck\u00b7ten", "sie", "ih\u00b7re", "F\u00fc\u00b7\u00dfe", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADJA", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und mit den andern zween sie flogen frey,", "tokens": ["Und", "mit", "den", "an\u00b7dern", "zween", "sie", "flo\u00b7gen", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Gegenander ruften sie mit gro\u00dfem Schrey:", "tokens": ["Ge\u00b7gen\u00b7an\u00b7der", "ruf\u00b7ten", "sie", "mit", "gro\u00b7\u00dfem", "Schrey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Heilig ist Gott der Herr Zebaoth,", "tokens