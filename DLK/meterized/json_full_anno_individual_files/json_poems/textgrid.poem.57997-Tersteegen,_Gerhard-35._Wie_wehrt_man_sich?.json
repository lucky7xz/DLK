{"textgrid.poem.57997": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "35. Wie wehrt man sich?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein armes Kind, von Witz und Kr\u00e4ften blo\u00df,", "tokens": ["Ich", "bin", "ein", "ar\u00b7mes", "Kind", ",", "von", "Witz", "und", "Kr\u00e4f\u00b7ten", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da tausend Feinde mir nach meiner Seele stehen;", "tokens": ["Da", "tau\u00b7send", "Fein\u00b7de", "mir", "nach", "mei\u00b7ner", "See\u00b7le", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Jesu, Mutterherz, ich kriech' in deinen Scho\u00df", "tokens": ["O", "Je\u00b7su", ",", "Mut\u00b7ter\u00b7herz", ",", "ich", "kriech'", "in", "dei\u00b7nen", "Scho\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will mich regen nicht, sollt' gleich die Welt vergehen.", "tokens": ["Und", "will", "mich", "re\u00b7gen", "nicht", ",", "sollt'", "gleich", "die", "Welt", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA",