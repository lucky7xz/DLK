{"textgrid.poem.37947": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das hei\u00dfe Afrika", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf! ihr Br\u00fcder und seyd stark!", "tokens": ["Auf", ",", "auf", "!", "ihr", "Br\u00fc\u00b7der", "und", "seyd", "stark", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$.", "PPOSAT", "NN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Abschiedstag ist da,", "tokens": ["Der", "Ab\u00b7schieds\u00b7tag", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwer liegt er auf der Seele, schwer!", "tokens": ["Schwer", "liegt", "er", "auf", "der", "See\u00b7le", ",", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sollen \u00fcber Land und Meer,", "tokens": ["Wir", "sol\u00b7len", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ins hei\u00dfe Afrika.", "tokens": ["Ins", "hei\u00b7\u00dfe", "Af\u00b7ri\u00b7ka", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein dichter Kreis von Lieben steht,", "tokens": ["Ein", "dich\u00b7ter", "Kreis", "von", "Lie\u00b7ben", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O, Br\u00fcder! um uns her.", "tokens": ["O", ",", "Br\u00fc\u00b7der", "!", "um", "uns", "her", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns kn\u00fcpft so manches theure Band,", "tokens": ["Uns", "kn\u00fcpft", "so", "man\u00b7ches", "theu\u00b7re", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An unser teutsches Vaterland,", "tokens": ["An", "un\u00b7ser", "teut\u00b7sches", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum f\u00e4llt der Abschied schwer.", "tokens": ["Drum", "f\u00e4llt", "der", "Ab\u00b7schied", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem bieten graue Eltern noch,", "tokens": ["Dem", "bie\u00b7ten", "grau\u00b7e", "El\u00b7tern", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum leztenmal die Hand,", "tokens": ["Zum", "lez\u00b7ten\u00b7mal", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den kosen Bruder, Schwester, Freund,", "tokens": ["Den", "ko\u00b7sen", "Bru\u00b7der", ",", "Schwes\u00b7ter", ",", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles schweigt, und alles weint,", "tokens": ["Und", "al\u00b7les", "schweigt", ",", "und", "al\u00b7les", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Todtbla\u00df von uns gewandt.", "tokens": ["Todt\u00b7bla\u00df", "von", "uns", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie ein Geist schlingt um den Hals,", "tokens": ["Und", "wie", "ein", "Geist", "schlingt", "um", "den", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Liebchen sich herum,", "tokens": ["Das", "Lieb\u00b7chen", "sich", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst mich verlassen liebes Herz,", "tokens": ["Willst", "mich", "ver\u00b7las\u00b7sen", "lie\u00b7bes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ewig, und der bittre Schmerz,", "tokens": ["Auf", "e\u00b7wig", ",", "und", "der", "bitt\u00b7re", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Machts arme Liebchen stumm.", "tokens": ["Machts", "ar\u00b7me", "Lieb\u00b7chen", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ist hart! Drum wirble du Tambur,", "tokens": ["Ist", "hart", "!", "Drum", "wirb\u00b7le", "du", "Tam\u00b7bur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PAV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Generalmarsch drein;", "tokens": ["Den", "Ge\u00b7ne\u00b7ral\u00b7marsch", "drein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abschied macht uns sonst zu weich!", "tokens": ["Der", "Ab\u00b7schied", "macht", "uns", "sonst", "zu", "weich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir weinen kleinen Kindern gleich,", "tokens": ["Wir", "wei\u00b7nen", "klei\u00b7nen", "Kin\u00b7dern", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es mu\u00df geschieden seyn.", "tokens": ["Es", "mu\u00df", "ge\u00b7schie\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Lebt wohl! Ihr Freunde, sehn wir uns", "tokens": ["Lebt", "wohl", "!", "Ihr", "Freun\u00b7de", ",", "sehn", "wir", "uns"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht zum leztenmal,", "tokens": ["Viel\u00b7leicht", "zum", "lez\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denkt: Nicht f\u00fcr die kurze Zeit;", "tokens": ["So", "denkt", ":", "Nicht", "f\u00fcr", "die", "kur\u00b7ze", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Freundschaft ist f\u00fcr die Ewigkeit,", "tokens": ["Freund\u00b7schaft", "ist", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und Gott ist \u00fcberall.", "tokens": ["Und", "Gott", "ist", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "An Teutschlands Grenzen f\u00fcllen wir", "tokens": ["An", "Teutschlands", "Gren\u00b7zen", "f\u00fcl\u00b7len", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Erden unsere Hand,", "tokens": ["Mit", "Er\u00b7den", "un\u00b7se\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und k\u00fcssen sie, das sey der Dank", "tokens": ["Und", "k\u00fcs\u00b7sen", "sie", ",", "das", "sey", "der", "Dank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr deine Pflege, Spei\u00df und Trank,", "tokens": ["F\u00fcr", "dei\u00b7ne", "Pfle\u00b7ge", ",", "Spei\u00df", "und", "Trank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du liebes Vaterland.", "tokens": ["Du", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wann denn des Meeres Woge sich,", "tokens": ["Wann", "denn", "des", "Mee\u00b7res", "Wo\u00b7ge", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An unserm Schiff zerbricht,", "tokens": ["An", "un\u00b7serm", "Schiff", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So segeln wir gelassen fort,", "tokens": ["So", "se\u00b7geln", "wir", "ge\u00b7las\u00b7sen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann Gott ist hier, und Gott ist dort,", "tokens": ["Dann", "Gott", "ist", "hier", ",", "und", "Gott", "ist", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "$,", "KON", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der verl\u00e4st uns nicht.", "tokens": ["Und", "der", "ver\u00b7l\u00e4st", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und ha, wenn sich der Tafelberg,", "tokens": ["Und", "ha", ",", "wenn", "sich", "der", "Ta\u00b7fel\u00b7berg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "KOUS", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus blauen D\u00fcften hebt,", "tokens": ["Aus", "blau\u00b7en", "D\u00fcf\u00b7ten", "hebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So strecken wir empor die Hand,", "tokens": ["So", "stre\u00b7cken", "wir", "em\u00b7por", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jauchzen: Land, ihr Br\u00fcder, Land!", "tokens": ["Und", "jauch\u00b7zen", ":", "Land", ",", "ihr", "Br\u00fc\u00b7der", ",", "Land", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df unser Schiff erbebt.", "tokens": ["Da\u00df", "un\u00b7ser", "Schiff", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wenn Soldat und Offizier,", "tokens": ["Und", "wenn", "Sol\u00b7dat", "und", "Of\u00b7fi\u00b7zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesund ans Ufer springt,", "tokens": ["Ge\u00b7sund", "ans", "U\u00b7fer", "springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn jubeln wir: Hurra! Hurra!", "tokens": ["Denn", "ju\u00b7beln", "wir", ":", "Hur\u00b7ra", "!", "Hur\u00b7ra", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "NE", "$.", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nun sind wir ja in Afrika,", "tokens": ["Nun", "sind", "wir", "ja", "in", "Af\u00b7ri\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alles dankt und singt.", "tokens": ["Und", "al\u00b7les", "dankt", "und", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wir leben drauf in fernem Land,", "tokens": ["Wir", "le\u00b7ben", "drauf", "in", "fer\u00b7nem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Teutsche brav und gut:", "tokens": ["Als", "Teut\u00b7sche", "brav", "und", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sagen soll man weit und breit,", "tokens": ["Und", "sa\u00b7gen", "soll", "man", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Teutschen sind doch brave Leut:", "tokens": ["Die", "Teut\u00b7schen", "sind", "doch", "bra\u00b7ve", "Leut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie haben Geist und Muth.", "tokens": ["Sie", "ha\u00b7ben", "Geist", "und", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und trinken auf dem Hoffnungs-Kap,", "tokens": ["Und", "trin\u00b7ken", "auf", "dem", "Hoff\u00b7nungs\u00b7Kap", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir seinen G\u00f6tter-Wein!", "tokens": ["Wir", "sei\u00b7nen", "G\u00f6t\u00b7ter\u00b7Wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denken wir von Sehnsucht weich,", "tokens": ["So", "den\u00b7ken", "wir", "von", "Sehn\u00b7sucht", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr fernen Freunde, dann an euch:", "tokens": ["Ihr", "fer\u00b7nen", "Freun\u00b7de", ",", "dann", "an", "euch", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Thr\u00e4nen flie\u00dfen drein.", "tokens": ["Und", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Auf, auf! ihr Br\u00fcder und seyd stark!", "tokens": ["Auf", ",", "auf", "!", "ihr", "Br\u00fc\u00b7der", "und", "seyd", "stark", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$.", "PPOSAT", "NN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Abschiedstag ist da,", "tokens": ["Der", "Ab\u00b7schieds\u00b7tag", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwer liegt er auf der Seele, schwer!", "tokens": ["Schwer", "liegt", "er", "auf", "der", "See\u00b7le", ",", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sollen \u00fcber Land und Meer,", "tokens": ["Wir", "sol\u00b7len", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "wo