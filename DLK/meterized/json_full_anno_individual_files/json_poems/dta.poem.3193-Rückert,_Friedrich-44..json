{"dta.poem.3193": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem bist du mit dir uneins fort und fort,", "tokens": ["In", "ei\u00b7nem", "bist", "du", "mit", "dir", "un\u00b7eins", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PPER", "APPR", "PPER", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df bald die Sache mehr dir gilt und bald das Wort.", "tokens": ["Da\u00df", "bald", "die", "Sa\u00b7che", "mehr", "dir", "gilt", "und", "bald", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PPER", "VVFIN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bald scheint das Wort dir leer, die Sache nur vorhanden,", "tokens": ["Bald", "scheint", "das", "Wort", "dir", "leer", ",", "die", "Sa\u00b7che", "nur", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bald die Sache todt, das Wort allein verstanden.", "tokens": ["Und", "bald", "die", "Sa\u00b7che", "todt", ",", "das", "Wort", "al\u00b7lein", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und nur wo Poesie ihr sch\u00f6nes Bild des Scheins", "tokens": ["Und", "nur", "wo", "Poe\u00b7sie", "ihr", "sch\u00f6\u00b7nes", "Bild", "des", "Scheins"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PWAV", "NN", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir vorh\u00e4lt, f\u00fchlest du, wie Wort und Sach' ist eins.", "tokens": ["Dir", "vor\u00b7h\u00e4lt", ",", "f\u00fch\u00b7lest", "du", ",", "wie", "Wort", "und", "Sach'", "ist", "eins", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PWAV", "NN", "KON", "NN", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So, wort- und sachgelehrt, ein Dichtersprachgelehrter,", "tokens": ["So", ",", "wor\u00b7t", "und", "sach\u00b7ge\u00b7lehrt", ",", "ein", "Dich\u00b7ter\u00b7sprach\u00b7ge\u00b7lehr\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "TRUNC", "KON", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sei du vielfach gelehrt, und nicht ein Fachgelehrter.", "tokens": ["Sei", "du", "viel\u00b7fach", "ge\u00b7lehrt", ",", "und", "nicht", "ein", "Fach\u00b7ge\u00b7lehr\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}}}}