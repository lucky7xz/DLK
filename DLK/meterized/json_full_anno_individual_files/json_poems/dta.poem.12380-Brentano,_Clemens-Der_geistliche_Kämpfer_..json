{"dta.poem.12380": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der geistliche K\u00e4mpfer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df Lieb thut mich bezwingen,               ", "tokens": ["Gro\u00df", "Lieb", "thut", "mich", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich mu\u00df heben an,", "tokens": ["Da\u00df", "ich", "mu\u00df", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von einem K\u00e4mpfer singen,", "tokens": ["Von", "ei\u00b7nem", "K\u00e4mp\u00b7fer", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der war so wohlgethan.", "tokens": ["Der", "war", "so", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den K\u00e4mpfer will ich nennen,", "tokens": ["Den", "K\u00e4mp\u00b7fer", "will", "ich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ihr k\u00f6nnt merken wie,", "tokens": ["Da\u00df", "ihr", "k\u00f6nnt", "mer\u00b7ken", "wie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "KOKOM", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eigentlich erkennen,", "tokens": ["Und", "ei\u00b7gent\u00b7lich", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Christ Gottes Sohn allhie.", "tokens": ["Christ", "Got\u00b7tes", "Sohn", "all\u00b7hie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der K\u00e4mpfer tugendreiche,", "tokens": ["Der", "K\u00e4mp\u00b7fer", "tu\u00b7gen\u00b7drei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nahm sich vor einen Sinn,", "tokens": ["Nahm", "sich", "vor", "ei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus seines Vaters Reiche,", "tokens": ["Aus", "sei\u00b7nes", "Va\u00b7ters", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schickt er seinen Boten hin.", "tokens": ["Schickt", "er", "sei\u00b7nen", "Bo\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu einer sch\u00f6n Jungfrauen,", "tokens": ["Zu", "ei\u00b7ner", "sch\u00f6n", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in dem Morgenland,", "tokens": ["Wohl", "in", "dem", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wollt er gerne schauen,", "tokens": ["Die", "wollt", "er", "ger\u00b7ne", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da er sein Boten sandt.", "tokens": ["Da", "er", "sein", "Bo\u00b7ten", "sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wollet ihr sie auch kennen,", "tokens": ["Wol\u00b7let", "ihr", "sie", "auch", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Jungfrau minniglich,", "tokens": ["Die", "Jung\u00b7frau", "min\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gabriel thut sie nennen,", "tokens": ["Ga\u00b7bri\u00b7el", "thut", "sie", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und spricht gar tugendlich,", "tokens": ["Und", "spricht", "gar", "tu\u00b7gend\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da er sie gr\u00fc\u00dft geschwinde,", "tokens": ["Da", "er", "sie", "gr\u00fc\u00dft", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach Ave Maria,", "tokens": ["Sprach", "A\u00b7ve", "Ma\u00b7ria", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit Worten also linde,", "tokens": ["Mit", "Wor\u00b7ten", "al\u00b7so", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er pflag auch s\u00fc\u00dfer Worte,", "tokens": ["Er", "pflag", "auch", "s\u00fc\u00b7\u00dfer", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey der Jungfrauen rein,", "tokens": ["Bey", "der", "Jung\u00b7frau\u00b7en", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bis sie aufschlu\u00df die Pforte,", "tokens": ["Bis", "sie", "auf\u00b7schlu\u00df", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und lie\u00df ihn zu sich ein.", "tokens": ["Und", "lie\u00df", "ihn", "zu", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Jungfrau ber\u00fchrt ihr Herze,", "tokens": ["Die", "Jung\u00b7frau", "be\u00b7r\u00fchrt", "ihr", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und sprach: \u201eAch wer ist der,", "tokens": ["Und", "sprach", ":", "\u201e", "Ach", "wer", "ist", "der", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ITJ", "PWS", "VAFIN", "ART", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u201eder in fr\u00f6hlichem Scherze,", "tokens": ["\u201e", "der", "in", "fr\u00f6h\u00b7li\u00b7chem", "Scher\u00b7ze", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u201ebegehrt zu mir her.\u201c", "tokens": ["\u201e", "be\u00b7gehrt", "zu", "mir", "her", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Der Bot der antwortt schiere:", "tokens": ["Der", "Bot", "der", "ant\u00b7wortt", "schie\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eer ist gewaltiglich,", "tokens": ["\u201e", "er", "ist", "ge\u00b7wal\u00b7tig\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eer kommt herab zu dire,", "tokens": ["\u201e", "er", "kommt", "her\u00b7ab", "zu", "di\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eer macht euch alle reich.\u201c", "tokens": ["\u201e", "er", "macht", "euch", "al\u00b7le", "reich", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIS", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Maria sprach mit Z\u00fcchten:", "tokens": ["Ma\u00b7ria", "sprach", "mit", "Z\u00fcch\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u201eich thu keins Manns Begehren!\u201c", "tokens": ["\u201e", "ich", "thu", "keins", "Manns", "Be\u00b7geh\u00b7ren", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201esollst mit m\u00e4gdlichen Fr\u00fcchten,", "tokens": ["\u201e", "sollst", "mit", "m\u00e4gd\u00b7li\u00b7chen", "Fr\u00fcch\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u201eein Kind ohn Mann geb\u00e4ren.", "tokens": ["\u201e", "ein", "Kind", "ohn", "Mann", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u201egott Sohn von Ewigkeite,", "tokens": ["\u201e", "gott", "Sohn", "von", "E\u00b7wig\u00b7kei\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eder kommt herab zu dir,\u201c", "tokens": ["\u201e", "der", "kommt", "her\u00b7ab", "zu", "dir", ",", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "APPR", "PPER", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sprach: \u201eIch bin bereite,", "tokens": ["Sie", "sprach", ":", "\u201e", "Ich", "bin", "be\u00b7rei\u00b7te", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201enach deinem Wort geschehe mir.\u201c", "tokens": ["\u201e", "nach", "dei\u00b7nem", "Wort", "ge\u00b7sche\u00b7he", "mir", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Welt die stand in Sorgen", "tokens": ["Die", "Welt", "die", "stand", "in", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mehr dann f\u00fcnf tausend Jahr,", "tokens": ["Mehr", "dann", "f\u00fcnf", "tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In H\u00f6llengrund verborgen,", "tokens": ["In", "H\u00f6l\u00b7len\u00b7grund", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis kam der K\u00e4mpfer klar.", "tokens": ["Bis", "kam", "der", "K\u00e4mp\u00b7fer", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Das wollt er wieder kehren (wenden),", "tokens": ["Das", "wollt", "er", "wie\u00b7der", "keh\u00b7ren", "(", "wen\u00b7den", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$(", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der edel K\u00e4mpfer werth,", "tokens": ["Der", "e\u00b7del", "K\u00e4mp\u00b7fer", "werth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Blut um uns verehren,", "tokens": ["Sein", "Blut", "um", "uns", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kam herab auf Erd.", "tokens": ["Und", "kam", "her\u00b7ab", "auf", "Erd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}