{"dta.poem.9598": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie erkl\u00e4ret sich seine schwester zu seyn.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des himmels ausspruch hat mein gl\u00fccke weit getrieben/", "tokens": ["Des", "him\u00b7mels", "aus\u00b7spruch", "hat", "mein", "gl\u00fc\u00b7cke", "weit", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPOSAT", "VVFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hei\u00dft mich Phillis nun als eine schwester lieben/", "tokens": ["Er", "hei\u00dft", "mich", "Phil\u00b7lis", "nun", "als", "ei\u00b7ne", "schwes\u00b7ter", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "ADV", "KOUS", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und will/ ich soll ihr bruder seyn:", "tokens": ["Und", "will", "/", "ich", "soll", "ihr", "bru\u00b7der", "seyn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn weil ja meiner treu ein gleiches recht geb\u00fchret/", "tokens": ["Denn", "weil", "ja", "mei\u00b7ner", "treu", "ein", "glei\u00b7ches", "recht", "ge\u00b7b\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So will er eben auch mich durch ein wort erfreun/", "tokens": ["So", "will", "er", "e\u00b7ben", "auch", "mich", "durch", "ein", "wort", "er\u00b7freun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das nichts/ denn lieb\u2019 und treu/ in seinem munde f\u00fchret.", "tokens": ["Das", "nichts", "/", "denn", "lieb'", "und", "treu", "/", "in", "sei\u00b7nem", "mun\u00b7de", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "KON", "VVFIN", "KON", "ADJD", "$(", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der nahme Phillis ist nur schatten-werck gewesen/", "tokens": ["Der", "nah\u00b7me", "Phil\u00b7lis", "ist", "nur", "schat\u00b7ten\u00b7\u00b7werck", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich lie\u00df sie blo\u00df daraus ihr steinern hertze lesen", "tokens": ["Ich", "lie\u00df", "sie", "blo\u00df", "da\u00b7raus", "ihr", "stei\u00b7nern", "hert\u00b7ze", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihre strenge grausamkeit/", "tokens": ["Und", "ih\u00b7re", "stren\u00b7ge", "grau\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sie ihr dazumal zur tugend hat erkohren;", "tokens": ["Die", "sie", "ihr", "da\u00b7zu\u00b7mal", "zur", "tu\u00b7gend", "hat", "er\u00b7koh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jtzt aber soll er so/ wie jene tr\u00fcbe zeit/", "tokens": ["Jtzt", "a\u00b7ber", "soll", "er", "so", "/", "wie", "je\u00b7ne", "tr\u00fc\u00b7be", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "$(", "KOKOM", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch diesen neuen bund auff ewig seyn verlohren.", "tokens": ["Durch", "die\u00b7sen", "neu\u00b7en", "bund", "auff", "e\u00b7wig", "seyn", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPR", "ADJD", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verh\u00e4ngnis/ f\u00f6rdre dann die freundschafft beyder hertzen/", "tokens": ["Ver\u00b7h\u00e4ng\u00b7nis", "/", "f\u00f6rd\u00b7re", "dann", "die", "freund\u00b7schafft", "bey\u00b7der", "hert\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "ADV", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und g\u00f6nne/ da\u00df ich darff/ als Bruder/ mit ihr schertzen/", "tokens": ["Und", "g\u00f6n\u00b7ne", "/", "da\u00df", "ich", "darff", "/", "als", "Bru\u00b7der", "/", "mit", "ihr", "schert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "$(", "KOUS", "NN", "$(", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie thu/ was eine schwester soll/", "tokens": ["Sie", "thu", "/", "was", "ei\u00b7ne", "schwes\u00b7ter", "soll", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "ART", "ADJA", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich erfreut/ das stoltze gl\u00fcck genie\u00dfen/", "tokens": ["Und", "mich", "er\u00b7freut", "/", "das", "stolt\u00b7ze", "gl\u00fcck", "ge\u00b7nie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich als engel sie/ zwar aller ehren voll;", "tokens": ["Da\u00df", "ich", "als", "en\u00b7gel", "sie", "/", "zwar", "al\u00b7ler", "eh\u00b7ren", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PPER", "$(", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch aber auch dabey als schwester m\u00f6ge k\u00fc\u00dfen.", "tokens": ["Doch", "a\u00b7ber", "auch", "da\u00b7bey", "als", "schwes\u00b7ter", "m\u00f6\u00b7ge", "k\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PAV", "KOUS", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}