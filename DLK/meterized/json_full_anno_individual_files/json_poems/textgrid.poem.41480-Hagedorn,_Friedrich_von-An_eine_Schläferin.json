{"textgrid.poem.41480": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An eine Schl\u00e4ferin", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwache, sch\u00f6ne Schl\u00e4ferin,", "tokens": ["Er\u00b7wa\u00b7che", ",", "sch\u00f6\u00b7ne", "Schl\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Falls dieser Ku\u00df nicht zu bestrafen:", "tokens": ["Falls", "die\u00b7ser", "Ku\u00df", "nicht", "zu", "be\u00b7stra\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wenn ich dir zu z\u00e4rtlich bin,", "tokens": ["Doch", "wenn", "ich", "dir", "zu", "z\u00e4rt\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlaf, oder scheine mir zu schlafen.", "tokens": ["Schlaf", ",", "o\u00b7der", "schei\u00b7ne", "mir", "zu", "schla\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Unschuld, die nur halb erwacht,", "tokens": ["Die", "Un\u00b7schuld", ",", "die", "nur", "halb", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "ia