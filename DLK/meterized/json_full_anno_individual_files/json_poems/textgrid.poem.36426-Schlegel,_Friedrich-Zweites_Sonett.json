{"textgrid.poem.36426": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zweites Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du meine Hand bist mehr als alle Weiber,", "tokens": ["Du", "mei\u00b7ne", "Hand", "bist", "mehr", "als", "al\u00b7le", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VAFIN", "PIAT", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist stets da, wie keine Frau erprobt,", "tokens": ["Du", "bist", "stets", "da", ",", "wie", "kei\u00b7ne", "Frau", "er\u00b7probt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PWAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du hast noch nie in Eifersucht getobt,", "tokens": ["Du", "hast", "noch", "nie", "in", "Ei\u00b7fer\u00b7sucht", "ge\u00b7tobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bist auch nie zu weit, du enger Reiber.", "tokens": ["Und", "bist", "auch", "nie", "zu", "weit", ",", "du", "en\u00b7ger", "Rei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ovid, mein Lehrer weiland, hat dich recht gelobt,", "tokens": ["O\u00b7vid", ",", "mein", "Leh\u00b7rer", "wei\u00b7land", ",", "hat", "dich", "recht", "ge\u00b7lobt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn du verbirgst in dir ja alle Leiber,", "tokens": ["Denn", "du", "ver\u00b7birgst", "in", "dir", "ja", "al\u00b7le", "Lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ich mir w\u00fcnsche. K\u00fchler Glutvertreiber,", "tokens": ["Die", "ich", "mir", "w\u00fcn\u00b7sche", ".", "K\u00fch\u00b7ler", "Glut\u00b7ver\u00b7trei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$.", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir hab ich mich f\u00fcr immer anverlobt.", "tokens": ["Dir", "hab", "ich", "mich", "f\u00fcr", "im\u00b7mer", "an\u00b7ver\u00b7lobt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich stehe stolz allein mir dir im Raume", "tokens": ["Ich", "ste\u00b7he", "stolz", "al\u00b7lein", "mir", "dir", "im", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und streichle meine bl\u00e4ulichrote Glans.", "tokens": ["Und", "streich\u00b7le", "mei\u00b7ne", "bl\u00e4u\u00b7lich\u00b7ro\u00b7te", "Glans", "."],