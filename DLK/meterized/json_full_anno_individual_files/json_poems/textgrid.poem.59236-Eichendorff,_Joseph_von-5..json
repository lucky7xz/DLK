{"textgrid.poem.59236": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh ich des Tages wirrendes Beginnen,", "tokens": ["Seh", "ich", "des", "Ta\u00b7ges", "wir\u00b7ren\u00b7des", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Die bunten Bilder fliehn und sich vereinen,", "tokens": ["Die", "bun\u00b7ten", "Bil\u00b7der", "fliehn", "und", "sich", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6cht ich das sch\u00f6ne Schattenspiel beweinen,", "tokens": ["M\u00f6cht", "ich", "das", "sch\u00f6\u00b7ne", "Schat\u00b7ten\u00b7spiel", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn eitel ist, was jeder will gewinnen.", "tokens": ["Denn", "ei\u00b7tel", "ist", ",", "was", "je\u00b7der", "will", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wenn die Stra\u00dfen leer, einsam die Zinnen", "tokens": ["Doch", "wenn", "die", "Stra\u00b7\u00dfen", "leer", ",", "ein\u00b7sam", "die", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "$,", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Morgenglanze wie Kometen scheinen,", "tokens": ["Im", "Mor\u00b7gen\u00b7glan\u00b7ze", "wie", "Ko\u00b7me\u00b7ten", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein stiller Geist steht auf den dunklen Steinen,", "tokens": ["Ein", "stil\u00b7ler", "Geist", "steht", "auf", "den", "dunk\u00b7len", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als wollt er sich auf alte Zeit besinnen:", "tokens": ["Als", "wollt", "er", "sich", "auf", "al\u00b7te", "Zeit", "be\u00b7sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da nimmt die Seele r\u00fcstig sich zusammen,", "tokens": ["Da", "nimmt", "die", "See\u00b7le", "r\u00fcs\u00b7tig", "sich", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Gott gedenkend und an alles Hohe,", "tokens": ["An", "Gott", "ge\u00b7den\u00b7kend", "und", "an", "al\u00b7les", "Ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was rings gedeihet auf der Erden Runde.", "tokens": ["Was", "rings", "ge\u00b7dei\u00b7het", "auf", "der", "Er\u00b7den", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und aus dem Herzen lang verhaltne Flammen,", "tokens": ["Und", "aus", "dem", "Her\u00b7zen", "lang", "ver\u00b7halt\u00b7ne", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie brechen fr\u00f6hlich in des Morgens Lohe,", "tokens": ["Sie", "bre\u00b7chen", "fr\u00f6h\u00b7lich", "in", "des", "Mor\u00b7gens", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da gr\u00fc\u00df ich, S\u00e4nger, dich aus Herzensgrunde!", "tokens": ["Da", "gr\u00fc\u00df", "ich", ",", "S\u00e4n\u00b7ger", ",", "dich", "aus", "Her\u00b7zens\u00b7grun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von Seen und W\u00e4ldern eine n\u00e4cht'ge Runde", "tokens": ["Von", "Seen", "und", "W\u00e4l\u00b7dern", "ei\u00b7ne", "n\u00e4cht'\u00b7ge", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich, und Drachen ziehn mit gl\u00fchnden Schweifen,", "tokens": ["Sah", "ich", ",", "und", "Dra\u00b7chen", "ziehn", "mit", "gl\u00fchn\u00b7den", "Schwei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Eicheswipfeln einen Horst von Greifen,", "tokens": ["In", "Ei\u00b7ches\u00b7wip\u00b7feln", "ei\u00b7nen", "Horst", "von", "Grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Nordlicht schr\u00e4ge leuchtend \u00fcberm Grunde.", "tokens": ["Das", "Nord\u00b7licht", "schr\u00e4\u00b7ge", "leuch\u00b7tend", "\u00fc\u00b7berm", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Durch Qualm dann klingend brach die Morgenstunde,", "tokens": ["Durch", "Qualm", "dann", "klin\u00b7gend", "brach", "die", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da schweiften Ritter blank durch Nebelstreifen,", "tokens": ["Da", "schweif\u00b7ten", "Rit\u00b7ter", "blank", "durch", "Ne\u00b7bel\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Winde scharf, die auf der Heide pfeifen,", "tokens": ["Durch", "Win\u00b7de", "scharf", ",", "die", "auf", "der", "Hei\u00b7de", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Harfner sang, lobt' Gott aus Herzensgrunde.", "tokens": ["Ein", "Harf\u00b7ner", "sang", ",", "lobt'", "Gott", "aus", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Tiefatmend stand ich \u00fcber diesen Kl\u00fcften,", "tokens": ["Tie\u00b7fat\u00b7mend", "stand", "ich", "\u00fc\u00b7ber", "die\u00b7sen", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des Lebens Mark r\u00fchrt' schauernd an das meine,", "tokens": ["Des", "Le\u00b7bens", "Mark", "r\u00fchrt'", "schau\u00b7ernd", "an", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "APPR", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie ein geharn'schter Riese da erhoben.", "tokens": ["Wie", "ein", "ge\u00b7harn'schter", "Rie\u00b7se", "da", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Kein ird'scher Laut mehr reichte durch die L\u00fcfte,", "tokens": ["Kein", "ird'\u00b7scher", "Laut", "mehr", "reich\u00b7te", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "APPR", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir war's, als st\u00e4nde ich mit Gott alleine,", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "st\u00e4n\u00b7de", "ich", "mit", "Gott", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "So einsam, weit und sternhell war's da oben.", "tokens": ["So", "ein\u00b7sam", ",", "weit", "und", "stern\u00b7hell", "wa\u00b7r's", "da", "o\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "In Stein gehaun, zwei L\u00f6wen stehen drau\u00dfen,", "tokens": ["In", "Stein", "ge\u00b7haun", ",", "zwei", "L\u00f6\u00b7wen", "ste\u00b7hen", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "CARD", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewachen ewig stumm die heil'ge Pforte.", "tokens": ["Be\u00b7wa\u00b7chen", "e\u00b7wig", "stumm", "die", "heil'\u00b7ge", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sich, die Brust voll Weltlust, naht dem Orte,", "tokens": ["Wer", "sich", ",", "die", "Brust", "voll", "Welt\u00b7lust", ",", "naht", "dem", "Or\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "$,", "ART", "NN", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den f\u00fcllt ihr steinern Blicken bald mit Grausen.", "tokens": ["Den", "f\u00fcllt", "ihr", "stei\u00b7nern", "Bli\u00b7cken", "bald", "mit", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Dir w\u00e4chst dein Herz noch bei der W\u00e4lder Sausen,", "tokens": ["Dir", "w\u00e4chst", "dein", "Herz", "noch", "bei", "der", "W\u00e4l\u00b7der", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich r\u00fchren noch die wilden Riesenworte,", "tokens": ["Dich", "r\u00fch\u00b7ren", "noch", "die", "wil\u00b7den", "Rie\u00b7sen\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Gott vertraund, dem h\u00f6chsten Schirm und Horte \u2013", "tokens": ["Nur", "Gott", "ver\u00b7traund", ",", "dem", "h\u00f6chs\u00b7ten", "Schirm", "und", "Hor\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So magst du bei den alten Wundern hausen.", "tokens": ["So", "magst", "du", "bei", "den", "al\u00b7ten", "Wun\u00b7dern", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ob auch die andern deines Lieds nicht achten,", "tokens": ["Ob", "auch", "die", "an\u00b7dern", "dei\u00b7nes", "Lieds", "nicht", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Heldenlust und zarten Liebesbl\u00fcte,", "tokens": ["Der", "Hel\u00b7den\u00b7lust", "und", "zar\u00b7ten", "Lie\u00b7bes\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedanken treulos wechselnd mit der Mode:", "tokens": ["Ge\u00b7dan\u00b7ken", "treu\u00b7los", "wech\u00b7selnd", "mit", "der", "Mo\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "So felsenfester sei dein gro\u00dfes Trachten,", "tokens": ["So", "fel\u00b7sen\u00b7fes\u00b7ter", "sei", "dein", "gro\u00b7\u00dfes", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hau klingend Luft dir, ritterlich Gem\u00fcte!", "tokens": ["Hau", "klin\u00b7gend", "Luft", "dir", ",", "rit\u00b7ter\u00b7lich", "Ge\u00b7m\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "NN", "PPER", "$,", "ADJD", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.13": {"line.1": {"text": "Seh ich des Tages wirrendes Beginnen,", "tokens": ["Seh", "ich", "des", "Ta\u00b7ges", "wir\u00b7ren\u00b7des", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Die bunten Bilder fliehn und sich vereinen,", "tokens": ["Die", "bun\u00b7ten", "Bil\u00b7der", "fliehn", "und", "sich", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6cht ich das sch\u00f6ne Schattenspiel beweinen,", "tokens": ["M\u00f6cht", "ich", "das", "sch\u00f6\u00b7ne", "Schat\u00b7ten\u00b7spiel", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn eitel ist, was jeder will gewinnen.", "tokens": ["Denn", "ei\u00b7tel", "ist", ",", "was", "je\u00b7der", "will", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Doch wenn die Stra\u00dfen leer, einsam die Zinnen", "tokens": ["Doch", "wenn", "die", "Stra\u00b7\u00dfen", "leer", ",", "ein\u00b7sam", "die", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word",