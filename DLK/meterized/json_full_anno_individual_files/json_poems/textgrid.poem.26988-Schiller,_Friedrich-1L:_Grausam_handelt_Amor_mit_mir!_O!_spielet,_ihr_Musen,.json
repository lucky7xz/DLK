word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.36": {"text": "Alle gingen sie weg, weinend, von Jupiters Thron.", "tokens": ["Al\u00b7le", "gin\u00b7gen", "sie", "weg", ",", "wei\u00b7nend", ",", "von", "Ju\u00b7pi\u00b7ters", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.37": {"text": "Leben mu\u00df man und lieben! Es endet Leben und Liebe!", "tokens": ["Le\u00b7ben", "mu\u00df", "man", "und", "lie\u00b7ben", "!", "Es", "en\u00b7det", "Le\u00b7ben", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "KON", "VVINF", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Schnittest du, Parze, doch nur beide die F\u00e4den zugleich.", "tokens": ["Schnit\u00b7test", "du", ",", "Par\u00b7ze", ",", "doch", "nur", "bei\u00b7de", "die", "F\u00e4\u00b7den", "zu\u00b7gleich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ADV", "PIS", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}}}}