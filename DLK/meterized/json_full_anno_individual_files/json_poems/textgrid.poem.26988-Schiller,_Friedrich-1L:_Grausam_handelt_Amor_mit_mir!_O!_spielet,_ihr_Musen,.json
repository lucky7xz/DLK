{"textgrid.poem.26988": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Grausam handelt Amor mit mir! O! spielet, ihr Musen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grausam handelt Amor mit mir! O! spielet, ihr Musen,", "tokens": ["Grau\u00b7sam", "han\u00b7delt", "A\u00b7mor", "mit", "mir", "!", "O", "!", "spie\u00b7let", ",", "ihr", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "APPR", "PPER", "$.", "NE", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Mit den Schmerzen, die er, spielend, im Busen erregt.", "tokens": ["Mit", "den", "Schmer\u00b7zen", ",", "die", "er", ",", "spie\u00b7lend", ",", "im", "Bu\u00b7sen", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "$,", "ADJD", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Manuskripte besitz ich wie kein Gelehrter noch K\u00f6nig,", "tokens": ["Ma\u00b7nus\u00b7krip\u00b7te", "be\u00b7sitz", "ich", "wie", "kein", "Ge\u00b7lehr\u00b7ter", "noch", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "KOKOM", "PIAT", "NN", "ADV", "NN", "$,"], "meter": "+-+--+-++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Denn mein Liebchen, sie schreibt, was ich ihr dichtete, mir.", "tokens": ["Denn", "mein", "Lieb\u00b7chen", ",", "sie", "schreibt", ",", "was", "ich", "ihr", "dich\u00b7te\u00b7te", ",", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,", "PPER", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wie im Winter die Saat nur langsam keimet, im Fr\u00fchling", "tokens": ["Wie", "im", "Win\u00b7ter", "die", "Saat", "nur", "lang\u00b7sam", "kei\u00b7met", ",", "im", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,", "APPRART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Lebhaft treibet und scho\u00dft, so war die Neigung zu dir.", "tokens": ["Leb\u00b7haft", "trei\u00b7bet", "und", "scho\u00dft", ",", "so", "war", "die", "Nei\u00b7gung", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Immer war mir das Feld und der Wald und der Fels und die G\u00e4rten", "tokens": ["Im\u00b7mer", "war", "mir", "das", "Feld", "und", "der", "Wald", "und", "der", "Fels", "und", "die", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Nur ein Raum, und du machst sie, Geliebte, zum Ort.", "tokens": ["Nur", "ein", "Raum", ",", "und", "du", "machst", "sie", ",", "Ge\u00b7lieb\u00b7te", ",", "zum", "Ort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Raum und Zeit, ich empfind es, sind blo\u00dfe Formen des Denkens,", "tokens": ["Raum", "und", "Zeit", ",", "ich", "emp\u00b7find", "es", ",", "sind", "blo\u00b7\u00dfe", "For\u00b7men", "des", "Den\u00b7kens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VAFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Da das Eckchen mit dir, Liebchen, unendlich mir scheint.", "tokens": ["Da", "das", "Eck\u00b7chen", "mit", "dir", ",", "Lieb\u00b7chen", ",", "un\u00b7end\u00b7lich", "mir", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "$,", "NN", "$,", "ADJD", "PPER", "VVFIN", "$."], "meter": "--+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "Sorge! sie steiget mit dir zu Pferde, sie steiget zu Schiffe,", "tokens": ["Sor\u00b7ge", "!", "sie", "stei\u00b7get", "mit", "dir", "zu", "Pfer\u00b7de", ",", "sie", "stei\u00b7get", "zu", "Schif\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Viel zudringlicher noch packet sich Amor mir auf.", "tokens": ["Viel", "zu\u00b7dring\u00b7li\u00b7cher", "noch", "pa\u00b7cket", "sich", "A\u00b7mor", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PRF", "NE", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Schwer zu besiegen ist schon die Neigung, gesellet sich aber", "tokens": ["Schwer", "zu", "be\u00b7sie\u00b7gen", "ist", "schon", "die", "Nei\u00b7gung", ",", "ge\u00b7sel\u00b7let", "sich", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "VAFIN", "ADV", "ART", "NN", "$,", "VVFIN", "PRF", "ADV"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Gar die Gewohnheit zu ihr, un\u00fcberwindlich ist sie.", "tokens": ["Gar", "die", "Ge\u00b7wohn\u00b7heit", "zu", "ihr", ",", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "ist", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "$,", "ADJD", "VAFIN", "PPER", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Welche Schrift ich zweimal, ja dreimal hintereinander", "tokens": ["Wel\u00b7che", "Schrift", "ich", "zwei\u00b7mal", ",", "ja", "drei\u00b7mal", "hin\u00b7ter\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "NN", "PPER", "ADV", "$,", "ADV", "ADV", "ADV"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Lese? Das herzliche Blatt, das die Geliebte mir schreibt.", "tokens": ["Le\u00b7se", "?", "Das", "herz\u00b7li\u00b7che", "Blatt", ",", "das", "die", "Ge\u00b7lieb\u00b7te", "mir", "schreibt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Wer mich entz\u00fcckt, vermag mich zu t\u00e4uschen. Oh! Dichter und S\u00e4nger,", "tokens": ["Wer", "mich", "ent\u00b7z\u00fcckt", ",", "ver\u00b7mag", "mich", "zu", "t\u00e4u\u00b7schen", ".", "Oh", "!", "Dich\u00b7ter", "und", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "ITJ", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Mimen! lerntet ihr doch meiner Geliebten was ab.", "tokens": ["Mi\u00b7men", "!", "lern\u00b7tet", "ihr", "doch", "mei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "was", "ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PWS", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Alle Freude des Dichters, ein gutes Gedicht zu erschaffen,", "tokens": ["Al\u00b7le", "Freu\u00b7de", "des", "Dich\u00b7ters", ",", "ein", "gu\u00b7tes", "Ge\u00b7dicht", "zu", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.20": {"text": "F\u00fchle das liebliche Kind, das ihn begeisterte, mit.", "tokens": ["F\u00fch\u00b7le", "das", "lieb\u00b7li\u00b7che", "Kind", ",", "das", "ihn", "be\u00b7geis\u00b7ter\u00b7te", ",", "mit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Ein Epigramm sei zu kurz, mir etwas Herzlichs zu sagen?", "tokens": ["Ein", "E\u00b7pig\u00b7ramm", "sei", "zu", "kurz", ",", "mir", "et\u00b7was", "Herz\u00b7lichs", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Wie, mein Geliebter, ist denn nicht noch viel k\u00fcrzer der Ku\u00df?", "tokens": ["Wie", ",", "mein", "Ge\u00b7lieb\u00b7ter", ",", "ist", "denn", "nicht", "noch", "viel", "k\u00fcr\u00b7zer", "der", "Ku\u00df", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Kennst du den herrlichen Gift der unbefriedigten Liebe?", "tokens": ["Kennst", "du", "den", "herr\u00b7li\u00b7chen", "Gift", "der", "un\u00b7be\u00b7frie\u00b7dig\u00b7ten", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Er versengt und erquickt, zehret am Mark und erneuts.", "tokens": ["Er", "ver\u00b7sengt", "und", "er\u00b7quickt", ",", "zeh\u00b7ret", "am", "Mark", "und", "er\u00b7neuts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVPP", "$,", "VVFIN", "APPRART", "NN", "KON", "NE", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Kennst du die herrliche Wirkung der endlich befriedigten Liebe?", "tokens": ["Kennst", "du", "die", "herr\u00b7li\u00b7che", "Wir\u00b7kung", "der", "end\u00b7lich", "be\u00b7frie\u00b7dig\u00b7ten", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+---+-+--+--+-", "measure": "dactylic.di.plus"}, "line.26": {"text": "K\u00f6rper verbindet sie sch\u00f6n, wenn sie die Geister befreit.", "tokens": ["K\u00f6r\u00b7per", "ver\u00b7bin\u00b7det", "sie", "sch\u00f6n", ",", "wenn", "sie", "die", "Geis\u00b7ter", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Das ist die wahre Liebe, die immer und immer sich gleich bleibt,", "tokens": ["Das", "ist", "die", "wah\u00b7re", "Lie\u00b7be", ",", "die", "im\u00b7mer", "und", "im\u00b7mer", "sich", "gleich", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "KON", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.28": {"text": "Wenn man ihr alles gew\u00e4hrt, wenn man ihr alles versagt.", "tokens": ["Wenn", "man", "ihr", "al\u00b7les", "ge\u00b7w\u00e4hrt", ",", "wenn", "man", "ihr", "al\u00b7les", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PIS", "VVPP", "$,", "KOUS", "PIS", "PPER", "PIS", "VVPP", "$."], "meter": "---+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Alles w\u00fcnscht ich zu haben, um mit ihr alles zu teilen,", "tokens": ["Al\u00b7les", "w\u00fcnscht", "ich", "zu", "ha\u00b7ben", ",", "um", "mit", "ihr", "al\u00b7les", "zu", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKZU", "VAINF", "$,", "KOUI", "APPR", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Alles g\u00e4b ich dahin, w\u00e4r sie, die Einzige, mein.", "tokens": ["Al\u00b7les", "g\u00e4b", "ich", "da\u00b7hin", ",", "w\u00e4r", "sie", ",", "die", "Ein\u00b7zi\u00b7ge", ",", "mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PAV", "$,", "VAFIN", "PPER", "$,", "ART", "ADJA", "$,", "PPOSAT", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Kr\u00e4nken ein liebendes Herz und schweigen m\u00fcssen! Gesch\u00e4rfter", "tokens": ["Kr\u00e4n\u00b7ken", "ein", "lie\u00b7ben\u00b7des", "Herz", "und", "schwei\u00b7gen", "m\u00fcs\u00b7sen", "!", "Ge\u00b7sch\u00e4rf\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "VVINF", "VMINF", "$.", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "K\u00f6nnen die Qualen nicht sein, die Rhadamant sich ersinnt.", "tokens": ["K\u00f6n\u00b7nen", "die", "Qua\u00b7len", "nicht", "sein", ",", "die", "Rha\u00b7da\u00b7mant", "sich", "er\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VAINF", "$,", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+--+-++-+-+--+", "measure": "iambic.septa.invert"}, "line.33": {"text": "Warum bin ich verg\u00e4nglich? o Zeus! so fragte die Sch\u00f6nheit.", "tokens": ["Wa\u00b7rum", "bin", "ich", "ver\u00b7g\u00e4ng\u00b7lich", "?", "o", "Zeus", "!", "so", "frag\u00b7te", "die", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "FM", "NE", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.34": {"text": "Macht dich doch, sagte der Gott, nur das Verg\u00e4ngliche sch\u00f6n.", "tokens": ["Macht", "dich", "doch", ",", "sag\u00b7te", "der", "Gott", ",", "nur", "das", "Ver\u00b7g\u00e4ng\u00b7li\u00b7che", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Und die Liebe, die Blumen, der Tau und die Jugend vernahmens,", "tokens": ["Und", "die", "Lie\u00b7be", ",", "die", "Blu\u00b7men", ",", "der", "Tau", "und", "die", "Ju\u00b7gend", "ver\u00b7nah\u00b7mens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.36": {"text": "Alle gingen sie weg, weinend, von Jupiters Thron.", "tokens": ["Al\u00b7le", "gin\u00b7gen", "sie", "weg", ",", "wei\u00b7nend", ",", "von", "Ju\u00b7pi\u00b7ters", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.37": {"text": "Leben mu\u00df man und lieben! Es endet Leben und Liebe!", "tokens": ["Le\u00b7ben", "mu\u00df", "man", "und", "lie\u00b7ben", "!", "Es", "en\u00b7det", "Le\u00b7ben", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "KON", "VVINF", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Schnittest du, Parze, doch nur beide die F\u00e4den zugleich.", "tokens": ["Schnit\u00b7test", "du", ",", "Par\u00b7ze", ",", "doch", "nur", "bei\u00b7de", "die", "F\u00e4\u00b7den", "zu\u00b7gleich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ADV", "PIS", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Grausam handelt Amor mit mir! O! spielet, ihr Musen,", "tokens": ["Grau\u00b7sam", "han\u00b7delt", "A\u00b7mor", "mit", "mir", "!", "O", "!", "spie\u00b7let", ",", "ihr", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "APPR", "PPER", "$.", "NE", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Mit den Schmerzen, die er, spielend, im Busen erregt.", "tokens": ["Mit", "den", "Schmer\u00b7zen", ",", "die", "er", ",", "spie\u00b7lend", ",", "im", "Bu\u00b7sen", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "$,", "ADJD", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Manuskripte besitz ich wie kein Gelehrter noch K\u00f6nig,", "tokens": ["Ma\u00b7nus\u00b7krip\u00b7te", "be\u00b7sitz", "ich", "wie", "kein", "Ge\u00b7lehr\u00b7ter", "noch", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "KOKOM", "PIAT", "NN", "ADV", "NN", "$,"], "meter": "+-+--+-++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Denn mein Liebchen, sie schreibt, was ich ihr dichtete, mir.", "tokens": ["Denn", "mein", "Lieb\u00b7chen", ",", "sie", "schreibt", ",", "was", "ich", "ihr", "dich\u00b7te\u00b7te", ",", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,", "PPER", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wie im Winter die Saat nur langsam keimet, im Fr\u00fchling", "tokens": ["Wie", "im", "Win\u00b7ter", "die", "Saat", "nur", "lang\u00b7sam", "kei\u00b7met", ",", "im", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,", "APPRART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Lebhaft treibet und scho\u00dft, so war die Neigung zu dir.", "tokens": ["Leb\u00b7haft", "trei\u00b7bet", "und", "scho\u00dft", ",", "so", "war", "die", "Nei\u00b7gung", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Immer war mir das Feld und der Wald und der Fels und die G\u00e4rten", "tokens": ["Im\u00b7mer", "war", "mir", "das", "Feld", "und", "der", "Wald", "und", "der", "Fels", "und", "die", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Nur ein Raum, und du machst sie, Geliebte, zum Ort.", "tokens": ["Nur", "ein", "Raum", ",", "und", "du", "machst", "sie", ",", "Ge\u00b7lieb\u00b7te", ",", "zum", "Ort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Raum und Zeit, ich empfind es, sind blo\u00dfe Formen des Denkens,", "tokens": ["Raum", "und", "Zeit", ",", "ich", "emp\u00b7find", "es", ",", "sind", "blo\u00b7\u00dfe", "For\u00b7men", "des", "Den\u00b7kens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VAFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Da das Eckchen mit dir, Liebchen, unendlich mir scheint.", "tokens": ["Da", "das", "Eck\u00b7chen", "mit", "dir", ",", "Lieb\u00b7chen", ",", "un\u00b7end\u00b7lich", "mir", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "$,", "NN", "$,", "ADJD", "PPER", "VVFIN", "$."], "meter": "--+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "Sorge! sie steiget mit dir zu Pferde, sie steiget zu Schiffe,", "tokens": ["Sor\u00b7ge", "!", "sie", "stei\u00b7get", "mit", "dir", "zu", "Pfer\u00b7de", ",", "sie", "stei\u00b7get", "zu", "Schif\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Viel zudringlicher noch packet sich Amor mir auf.", "tokens": ["Viel", "zu\u00b7dring\u00b7li\u00b7cher", "noch", "pa\u00b7cket", "sich", "A\u00b7mor", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PRF", "NE", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Schwer zu besiegen ist schon die Neigung, gesellet sich aber", "tokens": ["Schwer", "zu", "be\u00b7sie\u00b7gen", "ist", "schon", "die", "Nei\u00b7gung", ",", "ge\u00b7sel\u00b7let", "sich", "a\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "VAFIN", "ADV", "ART", "NN", "$,", "VVFIN", "PRF", "ADV"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Gar die Gewohnheit zu ihr, un\u00fcberwindlich ist sie.", "tokens": ["Gar", "die", "Ge\u00b7wohn\u00b7heit", "zu", "ihr", ",", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "ist", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "$,", "ADJD", "VAFIN", "PPER", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Welche Schrift ich zweimal, ja dreimal hintereinander", "tokens": ["Wel\u00b7che", "Schrift", "ich", "zwei\u00b7mal", ",", "ja", "drei\u00b7mal", "hin\u00b7ter\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "NN", "PPER", "ADV", "$,", "ADV", "ADV", "ADV"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Lese? Das herzliche Blatt, das die Geliebte mir schreibt.", "tokens": ["Le\u00b7se", "?", "Das", "herz\u00b7li\u00b7che", "Blatt", ",", "das", "die", "Ge\u00b7lieb\u00b7te", "mir", "schreibt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Wer mich entz\u00fcckt, vermag mich zu t\u00e4uschen. Oh! Dichter und S\u00e4nger,", "tokens": ["Wer", "mich", "ent\u00b7z\u00fcckt", ",", "ver\u00b7mag", "mich", "zu", "t\u00e4u\u00b7schen", ".", "Oh", "!", "Dich\u00b7ter", "und", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$.", "ITJ", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Mimen! lerntet ihr doch meiner Geliebten was ab.", "tokens": ["Mi\u00b7men", "!", "lern\u00b7tet", "ihr", "doch", "mei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "was", "ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PWS", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Alle Freude des Dichters, ein gutes Gedicht zu erschaffen,", "tokens": ["Al\u00b7le", "Freu\u00b7de", "des", "Dich\u00b7ters", ",", "ein", "gu\u00b7tes", "Ge\u00b7dicht", "zu", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.20": {"text": "F\u00fchle das liebliche Kind, das ihn begeisterte, mit.", "tokens": ["F\u00fch\u00b7le", "das", "lieb\u00b7li\u00b7che", "Kind", ",", "das", "ihn", "be\u00b7geis\u00b7ter\u00b7te", ",", "mit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Ein Epigramm sei zu kurz, mir etwas Herzlichs zu sagen?", "tokens": ["Ein", "E\u00b7pig\u00b7ramm", "sei", "zu", "kurz", ",", "mir", "et\u00b7was", "Herz\u00b7lichs", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Wie, mein Geliebter, ist denn nicht noch viel k\u00fcrzer der Ku\u00df?", "tokens": ["Wie", ",", "mein", "Ge\u00b7lieb\u00b7ter", ",", "ist", "denn", "nicht", "noch", "viel", "k\u00fcr\u00b7zer", "der", "Ku\u00df", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Kennst du den herrlichen Gift der unbefriedigten Liebe?", "tokens": ["Kennst", "du", "den", "herr\u00b7li\u00b7chen", "Gift", "der", "un\u00b7be\u00b7frie\u00b7dig\u00b7ten", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Er versengt und erquickt, zehret am Mark und erneuts.", "tokens": ["Er", "ver\u00b7sengt", "und", "er\u00b7quickt", ",", "zeh\u00b7ret", "am", "Mark", "und", "er\u00b7neuts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVPP", "$,", "VVFIN", "APPRART", "NN", "KON", "NE", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Kennst du die herrliche Wirkung der endlich befriedigten Liebe?", "tokens": ["Kennst", "du", "die", "herr\u00b7li\u00b7che", "Wir\u00b7kung", "der", "end\u00b7lich", "be\u00b7frie\u00b7dig\u00b7ten", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+---+-+--+--+-", "measure": "dactylic.di.plus"}, "line.26": {"text": "K\u00f6rper verbindet sie sch\u00f6n, wenn sie die Geister befreit.", "tokens": ["K\u00f6r\u00b7per", "ver\u00b7bin\u00b7det", "sie", "sch\u00f6n", ",", "wenn", "sie", "die", "Geis\u00b7ter", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Das ist die wahre Liebe, die immer und immer sich gleich bleibt,", "tokens": ["Das", "ist", "die", "wah\u00b7re", "Lie\u00b7be", ",", "die", "im\u00b7mer", "und", "im\u00b7mer", "sich", "gleich", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "w