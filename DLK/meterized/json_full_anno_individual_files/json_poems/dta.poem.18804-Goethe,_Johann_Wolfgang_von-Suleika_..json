{"dta.poem.18804": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Was bedeutet die Bewegung?", "tokens": ["Was", "be\u00b7deu\u00b7tet", "die", "Be\u00b7we\u00b7gung", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt der Ost mir frohe Kunde?", "tokens": ["Bringt", "der", "Ost", "mir", "fro\u00b7he", "Kun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Schwingen frische Regung", "tokens": ["Sei\u00b7ner", "Schwin\u00b7gen", "fri\u00b7sche", "Re\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchlt des Herzens tiefe Wunde.", "tokens": ["K\u00fchlt", "des", "Her\u00b7zens", "tie\u00b7fe", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kosend spielt er mit dem Staube,", "tokens": ["Ko\u00b7send", "spielt", "er", "mit", "dem", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagt ihn auf in leichten W\u00f6lkchen,", "tokens": ["Jagt", "ihn", "auf", "in", "leich\u00b7ten", "W\u00f6lk\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt zur sichern Rebenlaube", "tokens": ["Treibt", "zur", "si\u00b7chern", "Re\u00b7ben\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Insecten frohes V\u00f6lkchen.", "tokens": ["Der", "In\u00b7sec\u00b7ten", "fro\u00b7hes", "V\u00f6lk\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lindert sanft der Sonne Gl\u00fchen,", "tokens": ["Lin\u00b7dert", "sanft", "der", "Son\u00b7ne", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchlt auch mir die heissen Wangen,", "tokens": ["K\u00fchlt", "auch", "mir", "die", "heis\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcsst die Reben noch im Fliehen,", "tokens": ["K\u00fcsst", "die", "Re\u00b7ben", "noch", "im", "Flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die auf Feld und H\u00fcgel prangen.", "tokens": ["Die", "auf", "Feld", "und", "H\u00fc\u00b7gel", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mir bringt sein leises Fl\u00fcstern", "tokens": ["Und", "mir", "bringt", "sein", "lei\u00b7ses", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Freunde tausend Gr\u00fcsse;", "tokens": ["Von", "dem", "Freun\u00b7de", "tau\u00b7send", "Gr\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh noch diese H\u00fcgel d\u00fcstern", "tokens": ["Eh", "noch", "die\u00b7se", "H\u00fc\u00b7gel", "d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fcssen mich wohl tausend K\u00fcsse.", "tokens": ["Gr\u00fcs\u00b7sen", "mich", "wohl", "tau\u00b7send", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so kannst du weiter ziehen!", "tokens": ["Und", "so", "kannst", "du", "wei\u00b7ter", "zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diene Freunden und Betr\u00fcbten.", "tokens": ["Die\u00b7ne", "Freun\u00b7den", "und", "Be\u00b7tr\u00fcb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort wo hohe Mauern gl\u00fchen", "tokens": ["Dort", "wo", "ho\u00b7he", "Mau\u00b7ern", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Find\u2019 ich bald den Vielgeliebten.", "tokens": ["Find'", "ich", "bald", "den", "Viel\u00b7ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! die wahre Herzenskunde,", "tokens": ["Ach", "!", "die", "wah\u00b7re", "Her\u00b7zens\u00b7kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebeshauch, erfrischtes Leben", "tokens": ["Lie\u00b7bes\u00b7hauch", ",", "er\u00b7frischtes", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wird mir nur aus seinem Munde,", "tokens": ["Wird", "mir", "nur", "aus", "sei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann mir nur sein Athem geben.", "tokens": ["Kann", "mir", "nur", "sein", "A\u00b7them", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}