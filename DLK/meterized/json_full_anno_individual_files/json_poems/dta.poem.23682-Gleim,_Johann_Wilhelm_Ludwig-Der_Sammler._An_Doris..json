{"dta.poem.23682": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Sammler.  \n An  \n  Doris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In den W&#228;ldern, voller Linden,", "tokens": ["In", "den", "W", "&#228;", "ldern", ",", "vol\u00b7ler", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "XY", "$(", "VVINF", "$,", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sammlen Bienen Wachs und Honig;", "tokens": ["Samm\u00b7len", "Bie\u00b7nen", "Wachs", "und", "Ho\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Fluren, voll Getreide,", "tokens": ["Auf", "den", "Flu\u00b7ren", ",", "voll", "Ge\u00b7trei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sammlen Hamster Weitzenk\u00f6rner,", "tokens": ["Samm\u00b7len", "Hams\u00b7ter", "Weit\u00b7zen\u00b7k\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Ameis in dem Garten,", "tokens": ["Und", "die", "Am\u00b7eis", "in", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Sammlet Nahrung f\u00fcr den Winter.", "tokens": ["Samm\u00b7let", "Nah\u00b7rung", "f\u00fcr", "den", "Win\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wi\u00dft ihr wol, was ich mir sammle?", "tokens": ["Wi\u00dft", "ihr", "wol", ",", "was", "ich", "mir", "samm\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meint ihr etwa goldne M\u00fcnzen?", "tokens": ["Meint", "ihr", "et\u00b7wa", "gold\u00b7ne", "M\u00fcn\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mir vom Pabst und seines gleichen", "tokens": ["Mir", "vom", "Pabst", "und", "sei\u00b7nes", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "KON", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Einen Himmel zu erhandeln.", "tokens": ["Ei\u00b7nen", "Him\u00b7mel", "zu", "er\u00b7han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Oder meint ihr Silberlinge?", "tokens": ["O\u00b7der", "meint", "ihr", "Sil\u00b7ber\u00b7lin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mir beim Richter Recht zu schaffen;", "tokens": ["Mir", "beim", "Rich\u00b7ter", "Recht", "zu", "schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Nein, dis hat Nikandor n\u00f6thig,", "tokens": ["Nein", ",", "dis", "hat", "Ni\u00b7kan\u00b7dor", "n\u00f6\u00b7thig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Nein, ich sammle mir nur K\u00fcsse.", "tokens": ["Nein", ",", "ich", "samm\u00b7le", "mir", "nur", "K\u00fcs\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Gold zu sammlen macht nur M\u00fche,", "tokens": ["Gold", "zu", "samm\u00b7len", "macht", "nur", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "K\u00fcsse kann ich leichter sammlen.", "tokens": ["K\u00fcs\u00b7se", "kann", "ich", "leich\u00b7ter", "samm\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Neulich hab ich erst in Sachsen,", "tokens": ["Neu\u00b7lich", "hab", "ich", "erst", "in", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Zwanzig tausend eingesammlet.", "tokens": ["Zwan\u00b7zig", "tau\u00b7send", "ein\u00b7ge\u00b7samm\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "CARD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}