{"dta.poem.10871": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erbauliche Gedanken  \n bey den Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser Blumen holden Bl\u00fchte,", "tokens": ["In", "die\u00b7ser", "Blu\u00b7men", "hol\u00b7den", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer Form und Farben Pracht,", "tokens": ["In", "ih\u00b7rer", "Form", "und", "Far\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erkenn ich dessen Wunder-Macht,", "tokens": ["Er\u00b7kenn", "ich", "des\u00b7sen", "Wun\u00b7der\u00b7Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besing ich dessen weise G\u00fcte,", "tokens": ["Be\u00b7sing", "ich", "des\u00b7sen", "wei\u00b7se", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der sie, f\u00fcr mich, hervorgebracht.", "tokens": ["Der", "sie", ",", "f\u00fcr", "mich", ",", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach! nimm in ihnen, mein Gem\u00fcthe!", "tokens": ["Ach", "!", "nimm", "in", "ih\u00b7nen", ",", "mein", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Lust und Dank und mit Bedacht,", "tokens": ["Mit", "Lust", "und", "Dank", "und", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Sch\u00f6pfers Weisheit, Macht und G\u00fcte,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die sie uns zeigen, oft in Acht.", "tokens": ["Die", "sie", "uns", "zei\u00b7gen", ",", "oft", "in", "Acht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVINF", "$,", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}