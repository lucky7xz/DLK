{"textgrid.poem.28667": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ey, so la\u00df ich den nicht bleiben,", "tokens": ["Ey", ",", "so", "la\u00df", "ich", "den", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVIMP", "PPER", "ART", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich schrieb, noch mehr zu schreiben?", "tokens": ["Was", "ich", "schrieb", ",", "noch", "mehr", "zu", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erbarkeit hats bald verdrossen,", "tokens": ["Er\u00b7bar\u00b7keit", "hats", "bald", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wann sie um soll gehn mit Possen.", "tokens": ["Wann", "sie", "um", "soll", "gehn", "mit", "Pos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "VMFIN", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist mir recht, Verdru\u00df zu mindern,", "tokens": ["Ist", "mir", "recht", ",", "Ver\u00b7dru\u00df", "zu", "min\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kindeln M\u00e4nner offt mit Kindern;", "tokens": ["Kin\u00b7deln", "M\u00e4n\u00b7ner", "offt", "mit", "Kin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch so bringt man ernste Sachen", "tokens": ["Auch", "so", "bringt", "man", "erns\u00b7te", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcglich an und ein durch lachen.", "tokens": ["F\u00fcg\u00b7lich", "an", "und", "ein", "durch", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ART", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ey, so la\u00df ich den nicht bleiben,", "tokens": ["Ey", ",", "so", "la\u00df", "ich", "den", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVIMP", "PPER", "ART", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich schrieb, noch mehr zu schreiben?", "tokens": ["Was", "ich", "schrieb", ",", "noch", "mehr", "zu", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erbarkeit hats bald verdrossen,", "tokens": ["Er\u00b7bar\u00b7keit", "hats", "bald", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {