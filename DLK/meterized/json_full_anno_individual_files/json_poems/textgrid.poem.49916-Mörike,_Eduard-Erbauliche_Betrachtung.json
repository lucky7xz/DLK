ct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "ADJD", "NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zwar wei\u00df ich's nicht, den G\u00f6ttern sei es heimgestellt.", "tokens": ["Zwar", "wei\u00df", "ich's", "nicht", ",", "den", "G\u00f6t\u00b7tern", "sei", "es", "heim\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$,", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch wie es falle, la\u00dft euch nichts mit mir gereun.", "tokens": ["Doch", "wie", "es", "fal\u00b7le", ",", "la\u00dft", "euch", "nichts", "mit", "mir", "ge\u00b7reun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Auf meinem Grabstein soll man ein Paar Schuhe sehn,", "tokens": ["Auf", "mei\u00b7nem", "Grab\u00b7stein", "soll", "man", "ein", "Paar", "Schu\u00b7he", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den Stab dar\u00fcber und den Reisehut gelegt,", "tokens": ["Den", "Stab", "da\u00b7r\u00fc\u00b7ber", "und", "den", "Rei\u00b7se\u00b7hut", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measur