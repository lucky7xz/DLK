{"textgrid.poem.49916": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Erbauliche Betrachtung", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wie im Forst ein J\u00e4ger, der, am hei\u00dfen Tag", "tokens": ["Als", "wie", "im", "Forst", "ein", "J\u00e4\u00b7ger", ",", "der", ",", "am", "hei\u00b7\u00dfen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Eichenschatten ruhend, mit zufriednem Blick", "tokens": ["Im", "Ei\u00b7chen\u00b7schat\u00b7ten", "ru\u00b7hend", ",", "mit", "zu\u00b7fried\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf seine Hunde niederschaut, das treue Paar,", "tokens": ["Auf", "sei\u00b7ne", "Hun\u00b7de", "nie\u00b7der\u00b7schaut", ",", "das", "treu\u00b7e", "Paar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das, Hals um Hals geschlungen, br\u00fcderlich den Schlaf,", "tokens": ["Das", ",", "Hals", "um", "Hals", "ge\u00b7schlun\u00b7gen", ",", "br\u00fc\u00b7der\u00b7lich", "den", "Schlaf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "APPR", "NN", "VVPP", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und schlafend noch des Jagens Lust und M\u00fche teilt:", "tokens": ["Und", "schla\u00b7fend", "noch", "des", "Ja\u00b7gens", "Lust", "und", "M\u00fc\u00b7he", "teilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So schau ich hier an des Geh\u00f6lzes Schattenrand", "tokens": ["So", "schau", "ich", "hier", "an", "des", "Ge\u00b7h\u00f6l\u00b7zes", "Schat\u00b7ten\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bei kurzer Rast auf meiner eignen F\u00fc\u00dfe Paar", "tokens": ["Bei", "kur\u00b7zer", "Rast", "auf", "mei\u00b7ner", "eig\u00b7nen", "F\u00fc\u00b7\u00dfe", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hinab, nicht ohne R\u00fchrung; in gewissem Sinn", "tokens": ["Hin\u00b7ab", ",", "nicht", "oh\u00b7ne", "R\u00fch\u00b7rung", ";", "in", "ge\u00b7wis\u00b7sem", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PTKNEG", "APPR", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zum erstenmal, so alt ich bin, betracht ich sie,", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", ",", "so", "alt", "ich", "bin", ",", "be\u00b7tracht", "ich", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und bin f\u00fcrwahr von ihrem Dasein \u00fcberrascht,", "tokens": ["Und", "bin", "f\u00fcr\u00b7wahr", "von", "ih\u00b7rem", "Da\u00b7sein", "\u00fc\u00b7berr\u00b7ascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie sie, in Schuhn bis \u00fcbern Kn\u00f6chel eingeschn\u00fcrt,", "tokens": ["Wie", "sie", ",", "in", "Schuhn", "bis", "\u00fc\u00b7bern", "Kn\u00f6\u00b7chel", "ein\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Best\u00e4ubt da vor mir liegen im verlechzten Gras.", "tokens": ["Be\u00b7st\u00e4ubt", "da", "vor", "mir", "lie\u00b7gen", "im", "ver\u00b7lechz\u00b7ten", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie manches Lustrum, ehrliche Gesellen, schleppt", "tokens": ["Wie", "man\u00b7ches", "Lust\u00b7rum", ",", "ehr\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len", ",", "schleppt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PIS", "NE", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr mich auf dieser buckeligen Welt umher,", "tokens": ["Ihr", "mich", "auf", "die\u00b7ser", "bu\u00b7cke\u00b7li\u00b7gen", "Welt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gehorsam eurem Herren jeden Augenblick,", "tokens": ["Ge\u00b7hor\u00b7sam", "eu\u00b7rem", "Her\u00b7ren", "je\u00b7den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tag oder Nacht, wohin er nur mit euch begehrt.", "tokens": ["Tag", "o\u00b7der", "Nacht", ",", "wo\u00b7hin", "er", "nur", "mit", "euch", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Sein Wandel mochte t\u00f6richt oder weislich sein,", "tokens": ["Sein", "Wan\u00b7del", "moch\u00b7te", "t\u00f6\u00b7richt", "o\u00b7der", "weis\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den besten Herrn, wenn man euch h\u00f6rte, trugt ihr stets.", "tokens": ["Den", "bes\u00b7ten", "Herrn", ",", "wenn", "man", "euch", "h\u00f6r\u00b7te", ",", "trugt", "ihr", "stets", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr seid bereit, den Unglimpf, der ihm widerfuhr,", "tokens": ["Ihr", "seid", "be\u00b7reit", ",", "den", "Un\u00b7glimpf", ",", "der", "ihm", "wi\u00b7der\u00b7fuhr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u2013 Und w\u00e4re sein Beleidiger ein Reichsbaron \u2013", "tokens": ["\u2013", "Und", "w\u00e4\u00b7re", "sein", "Be\u00b7lei\u00b7di\u00b7ger", "ein", "Reichs\u00b7ba\u00b7ron", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Alsbald zu strafen mit ergrimmtem Hundetritt", "tokens": ["Als\u00b7bald", "zu", "stra\u00b7fen", "mit", "er\u00b7grimm\u00b7tem", "Hun\u00b7de\u00b7tritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "(doch hief\u00fcr hat er selber zu viel Lebensart).", "tokens": ["(", "doch", "hie\u00b7f\u00fcr", "hat", "er", "sel\u00b7ber", "zu", "viel", "Le\u00b7ben\u00b7sart", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PAV", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo war ein Berg zu steil f\u00fcr euch, zu j\u00e4h ein Fels?", "tokens": ["Wo", "war", "ein", "Berg", "zu", "steil", "f\u00fcr", "euch", ",", "zu", "j\u00e4h", "ein", "Fels", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "NN", "APPR", "PPER", "$,", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und gl\u00fccklich immer habt ihr mich nach Haus gebracht;", "tokens": ["Und", "gl\u00fcck\u00b7lich", "im\u00b7mer", "habt", "ihr", "mich", "nach", "Haus", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gleichwohl noch nie mit einem W\u00f6rtchen dankt ich euch,", "tokens": ["Gleich\u00b7wohl", "noch", "nie", "mit", "ei\u00b7nem", "W\u00f6rt\u00b7chen", "dankt", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vom Sch\u00f6nsten was mein Herz geno\u00df erfuhrt ihr nichts!", "tokens": ["Vom", "Sch\u00f6ns\u00b7ten", "was", "mein", "Herz", "ge\u00b7no\u00df", "er\u00b7fuhrt", "ihr", "nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRELS", "PPOSAT", "NN", "VVFIN", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn, von der blausten Fr\u00fchlingsmitternacht entz\u00fcckt,", "tokens": ["Wenn", ",", "von", "der", "blaus\u00b7ten", "Fr\u00fch\u00b7lings\u00b7mit\u00b7ter\u00b7nacht", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oft aus der Gartenlaube weg vom Zechgelag", "tokens": ["Oft", "aus", "der", "Gar\u00b7ten\u00b7lau\u00b7be", "weg", "vom", "Zech\u00b7ge\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein hochgestimmter Freund mich noch hinausgelockt,", "tokens": ["Mein", "hoch\u00b7ges\u00b7timm\u00b7ter", "Freund", "mich", "noch", "hin\u00b7aus\u00b7ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die offne Stra\u00dfe hinzuschw\u00e4rmen raschen Gangs,", "tokens": ["Die", "off\u00b7ne", "Stra\u00b7\u00dfe", "hin\u00b7zu\u00b7schw\u00e4r\u00b7men", "ra\u00b7schen", "Gangs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir J\u00fcnglinge, des Jugendgl\u00fcckes \u00dcberma\u00df", "tokens": ["Wir", "J\u00fcng\u00b7lin\u00b7ge", ",", "des", "Ju\u00b7gend\u00b7gl\u00fc\u00b7ckes", "\u00dc\u00b7berm\u00b7a\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "ART", "NN", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Als baren Schmerz empfindend, ins Unendliche", "tokens": ["Als", "ba\u00b7ren", "Schmerz", "emp\u00b7fin\u00b7dend", ",", "ins", "Un\u00b7end\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVPP", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Geister hetzten, und die Rede wie Feuer troff,", "tokens": ["Die", "Geis\u00b7ter", "hetz\u00b7ten", ",", "und", "die", "Re\u00b7de", "wie", "Feu\u00b7er", "troff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Bis wir zuletzt an K\u00fchnheit mit dem sichern Mann", "tokens": ["Bis", "wir", "zu\u00b7letzt", "an", "K\u00fchn\u00b7heit", "mit", "dem", "si\u00b7chern", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wetteiferten, da dieser Urwelts-G\u00f6ttersohn", "tokens": ["Wett\u00b7ei\u00b7fer\u00b7ten", ",", "da", "die\u00b7ser", "Ur\u00b7welts\u00b7G\u00f6t\u00b7ter\u00b7sohn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PDAT", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "In Fl\u00f6\u00dferstiefeln vom Gebirg zum Himmel sich", "tokens": ["In", "Fl\u00f6\u00b7\u00dfer\u00b7stie\u00b7feln", "vom", "Ge\u00b7birg", "zum", "Him\u00b7mel", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "APPRART", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verstieg und mit der breiten Hand der Sterne Heer", "tokens": ["Ver\u00b7stieg", "und", "mit", "der", "brei\u00b7ten", "Hand", "der", "Ster\u00b7ne", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zusammenstrich in einen Habersack und den", "tokens": ["Zu\u00b7sam\u00b7men\u00b7strich", "in", "ei\u00b7nen", "Ha\u00b7ber\u00b7sack", "und", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit gro\u00dfem Schnaufen bis zum Rand der Sch\u00f6pfung trug,", "tokens": ["Mit", "gro\u00b7\u00dfem", "Schnau\u00b7fen", "bis", "zum", "Rand", "der", "Sch\u00f6p\u00b7fung", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Plunder auszusch\u00fctteln vor das Weltentor \u2013", "tokens": ["Den", "Plun\u00b7der", "aus\u00b7zu\u00b7sch\u00fct\u00b7teln", "vor", "das", "Wel\u00b7ten\u00b7tor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach, gute Bursche, damals wart ihr auch dabei,", "tokens": ["Ach", ",", "gu\u00b7te", "Bur\u00b7sche", ",", "da\u00b7mals", "wart", "ihr", "auch", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wo nicht sonst, davon ich jetzo schweigen will!", "tokens": ["Und", "wo", "nicht", "sonst", ",", "da\u00b7von", "ich", "jet\u00b7zo", "schwei\u00b7gen", "will", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "ADV", "$,", "PAV", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bleibt mir getreu, und altert schneller nicht als ich!", "tokens": ["Bleibt", "mir", "ge\u00b7treu", ",", "und", "al\u00b7tert", "schnel\u00b7ler", "nicht", "als", "ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "ADJD", "PTKNEG", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wir haben, hoff ich, noch ein sch\u00f6n St\u00fcck Wegs vor uns;", "tokens": ["Wir", "ha\u00b7ben", ",", "hoff", "ich", ",", "noch", "ein", "sch\u00f6n", "St\u00fcck", "Wegs", "vor", "uns", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "ADJD", "NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zwar wei\u00df ich's nicht, den G\u00f6ttern sei es heimgestellt.", "tokens": ["Zwar", "wei\u00df", "ich's", "nicht", ",", "den", "G\u00f6t\u00b7tern", "sei", "es", "heim\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$,", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch wie es falle, la\u00dft euch nichts mit mir gereun.", "tokens": ["Doch", "wie", "es", "fal\u00b7le", ",", "la\u00dft", "euch", "nichts", "mit", "mir", "ge\u00b7reun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Auf meinem Grabstein soll man ein Paar Schuhe sehn,", "tokens": ["Auf", "mei\u00b7nem", "Grab\u00b7stein", "soll", "man", "ein", "Paar", "Schu\u00b7he", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den Stab dar\u00fcber und den Reisehut gelegt,", "tokens": ["Den", "Stab", "da\u00b7r\u00fc\u00b7ber", "und", "den", "Rei\u00b7se\u00b7hut", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das beste Sinnbild eines ruhenden Wandersmanns.", "tokens": ["Das", "bes\u00b7te", "Sinn\u00b7bild", "ei\u00b7nes", "ru\u00b7hen\u00b7den", "Wan\u00b7ders\u00b7manns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Wer dann mich segnet, der vergi\u00dft auch eurer nicht.", "tokens": ["Wer", "dann", "mich", "seg\u00b7net", ",", "der", "ver\u00b7gi\u00dft", "auch", "eu\u00b7rer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Genug f\u00fcr jetzt! denn dort seh ich's gewitterschwer", "tokens": ["Ge\u00b7nug", "f\u00fcr", "jetzt", "!", "denn", "dort", "seh", "ich's", "ge\u00b7wit\u00b7ter\u00b7schwer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "$.", "KON", "ADV", "VVFIN", "PIS", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Von Mittag kommen, und mich deucht, es donnert schon.", "tokens": ["Von", "Mit\u00b7tag", "kom\u00b7men", ",", "und", "mich", "deucht", ",", "es", "don\u00b7nert", "schon", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Eh uns der Regen \u00fcbereilt, ihr Knaben, auf!", "tokens": ["Eh", "uns", "der", "Re\u00b7gen", "\u00fc\u00b7be\u00b7reilt", ",", "ihr", "Kna\u00b7ben", ",", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Steig hinab! zum St\u00e4dtchen langt sich's eben noch.", "tokens": ["Die", "Steig", "hin\u00b7ab", "!", "zum", "St\u00e4dt\u00b7chen", "langt", "sich's", "e\u00b7ben", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als wie im Forst ein J\u00e4ger, der, am hei\u00dfen Tag", "tokens": ["Als", "wie", "im", "Forst", "ein", "J\u00e4\u00b7ger", ",", "der", ",", "am", "hei\u00b7\u00dfen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Eichenschatten ruhend, mit zufriednem Blick", "tokens": ["Im", "Ei\u00b7chen\u00b7schat\u00b7ten", "ru\u00b7hend", ",", "mit", "zu\u00b7fried\u00b7nem", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf seine Hunde niederschaut, das treue Paar,", "tokens": ["Auf", "sei\u00b7ne", "Hun\u00b7de", "nie\u00b7der\u00b7schaut", ",", "das", "treu\u00b7e", "Paar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das, Hals um Hals geschlungen, br\u00fcderlich den Schlaf,", "tokens": ["Das", ",", "Hals", "um", "Hals", "ge\u00b7schlun\u00b7gen", ",", "br\u00fc\u00b7der\u00b7lich", "den", "Schlaf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "APPR", "NN", "VVPP", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und schlafend noch des Jagens Lust und M\u00fche teilt:", "tokens": ["Und", "schla\u00b7fend", "noch", "des", "Ja\u00b7gens", "Lust", "und", "M\u00fc\u00b7he", "teilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So schau ich hier an des Geh\u00f6lzes Schattenrand", "tokens": ["So", "schau", "ich", "hier", "an", "des", "Ge\u00b7h\u00f6l\u00b7zes", "Schat\u00b7ten\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bei kurzer Rast auf meiner eignen F\u00fc\u00dfe Paar", "tokens": ["Bei", "kur\u00b7zer", "Rast", "auf", "mei\u00b7ner", "eig\u00b7nen", "F\u00fc\u00b7\u00dfe", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hinab, nicht ohne R\u00fchrung; in gewissem Sinn", "tokens": ["Hin\u00b7ab", ",", "nicht", "oh\u00b7ne", "R\u00fch\u00b7rung", ";", "in", "ge\u00b7wis\u00b7sem", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PTKNEG", "APPR", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zum erstenmal, so alt ich bin, betracht ich sie,", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", ",", "so", "alt", "ich", "bin", ",", "be\u00b7tracht", "ich", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und bin f\u00fcrwahr von ihrem Dasein \u00fcberrascht,", "tokens": ["Und", "bin", "f\u00fcr\u00b7wahr", "von", "ih\u00b7rem", "Da\u00b7sein", "\u00fc\u00b7berr\u00b7ascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie sie, in Schuhn bis \u00fcbern Kn\u00f6chel eingeschn\u00fcrt,", "tokens": ["Wie", "sie", ",", "in", "Schuhn", "bis", "\u00fc\u00b7bern", "Kn\u00f6\u00b7chel", "ein\u00b7ge\u00b7