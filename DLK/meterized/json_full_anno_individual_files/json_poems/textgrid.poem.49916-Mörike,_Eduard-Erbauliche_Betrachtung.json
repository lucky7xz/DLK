}, "line.9": {"text": "Wetteiferten, da dieser Urwelts-G\u00f6ttersohn", "tokens": ["Wett\u00b7ei\u00b7fer\u00b7ten", ",", "da", "die\u00b7ser", "Ur\u00b7welts\u00b7G\u00f6t\u00b7ter\u00b7sohn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PDAT", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "In Fl\u00f6\u00dferstiefeln vom Gebirg zum Himmel sich", "tokens": ["In", "Fl\u00f6\u00b7\u00dfer\u00b7stie\u00b7feln", "vom", "Ge\u00b7birg", "zum", "Him\u00b7mel", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "APPRART", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verstieg und mit der breiten Hand der Sterne Heer", "tokens": ["Ver\u00b7stieg", "und", "mit", "der", "brei\u00b7ten", "Hand", "der", "Ster\u00b7ne", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zusammenstrich in einen Habersack und den", "tokens": ["Zu\u00b7sam\u00b7men\u00b7strich", "in", "ei\u00b7nen", "Ha\u00b7ber\u00b7sack", "und", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit gro\u00dfem Schnaufen bis zum Rand der Sch\u00f6pfung trug,", "tokens": ["Mit", "gro\u00b7\u00dfem", "Schnau\u00b7fen", "bis", "zum", "Rand", "der", "Sch\u00f6p\u00b7fung", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Plunder auszusch\u