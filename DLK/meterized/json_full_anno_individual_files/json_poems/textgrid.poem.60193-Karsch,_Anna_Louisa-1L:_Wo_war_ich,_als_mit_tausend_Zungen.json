{"textgrid.poem.60193": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo war ich, als mit tausend Zungen", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo war ich, als mit tausend Zungen", "tokens": ["Wo", "war", "ich", ",", "als", "mit", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KOUS", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die G\u00f6ttin Harmonie im Fl\u00fcgel mir gesungen!", "tokens": ["Die", "G\u00f6t\u00b7tin", "Har\u00b7mo\u00b7nie", "im", "Fl\u00fc\u00b7gel", "mir", "ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Ohr vernahm, mein Herz zerschmolz.", "tokens": ["Mein", "Ohr", "ver\u00b7nahm", ",", "mein", "Herz", "zer\u00b7schmolz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Musen! mit Apollens ganzer St\u00e4rke,", "tokens": ["Ihr", "Mu\u00b7sen", "!", "mit", "A\u00b7pol\u00b7lens", "gan\u00b7zer", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Thut eure Schwester Wunderwerke", "tokens": ["Thut", "eu\u00b7re", "Schwes\u00b7ter", "Wun\u00b7der\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In diesem ausgeh\u00f6hlten Holz!", "tokens": ["In", "die\u00b7sem", "aus\u00b7ge\u00b7h\u00f6hl\u00b7ten", "Holz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr singt mit unbelebtem Thone", "tokens": ["Ihr", "singt", "mit", "un\u00b7be\u00b7leb\u00b7tem", "Tho\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Helden in dem Streit, vom K\u00f6nig auf dem Throne,", "tokens": ["Von", "Hel\u00b7den", "in", "dem", "Streit", ",", "vom", "K\u00f6\u00b7nig", "auf", "dem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Freundschaft, Liebe, Ku\u00df und Wein.", "tokens": ["Von", "Freund\u00b7schaft", ",", "Lie\u00b7be", ",", "Ku\u00df", "und", "Wein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ohr, der Witz bewundern eure Scherze;", "tokens": ["Das", "Ohr", ",", "der", "Witz", "be\u00b7wun\u00b7dern", "eu\u00b7re", "Scher\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie aber nimmt des Menschen Herze,", "tokens": ["Sie", "a\u00b7ber", "nimmt", "des", "Men\u00b7schen", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die ganze Seele nimmt sie ein", "tokens": ["Die", "gan\u00b7ze", "See\u00b7le", "nimmt", "sie", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt th\u00f6nt sie langsam; sanfte Trauer", "tokens": ["Jetzt", "th\u00f6nt", "sie", "lang\u00b7sam", ";", "sanf\u00b7te", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dringt schmeichelnd in die Brust, und mich ergreift ein Schauer,", "tokens": ["Dringt", "schmei\u00b7chelnd", "in", "die", "Brust", ",", "und", "mich", "er\u00b7greift", "ein", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Gram, der Wollust bey sich f\u00fchrt.", "tokens": ["Ein", "Gram", ",", "der", "Wol\u00b7lust", "bey", "sich", "f\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt hebt sie sich. O! welch ein himmlisch Feuer", "tokens": ["Jetzt", "hebt", "sie", "sich", ".", "O", "!", "welch", "ein", "himm\u00b7lisch", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "NE", "$.", "PWAT", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Empfind ich! So hat Orpheus Leyer", "tokens": ["Emp\u00b7find", "ich", "!", "So", "hat", "Or\u00b7pheus", "Le\u00b7yer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ADV", "VAFIN", "NE", "NE"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Mit zauberischer Kraft ger\u00fchrt.", "tokens": ["Mit", "zau\u00b7be\u00b7ri\u00b7scher", "Kraft", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch eine Majest\u00e4t! Wie pr\u00e4chtig", "tokens": ["Welch", "ei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "!", "Wie", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "PWAV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ihr Ger\u00e4usch! So wie entfernter Donner m\u00e4chtig,", "tokens": ["Ist", "ihr", "Ge\u00b7r\u00e4usch", "!", "So", "wie", "ent\u00b7fern\u00b7ter", "Don\u00b7ner", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "ADV", "KOKOM", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dennoch uns nicht furchtbar rollt.", "tokens": ["Und", "den\u00b7noch", "uns", "nicht", "furcht\u00b7bar", "rollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun singt sie lieblich, wie ihr Nachtigallen", "tokens": ["Nun", "singt", "sie", "lieb\u00b7lich", ",", "wie", "ihr", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ihr durch hohen Thon gefallen,", "tokens": ["Wenn", "ihr", "durch", "ho\u00b7hen", "Thon", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Seufzer uns entz\u00fccken wollt!", "tokens": ["Durch", "Seuf\u00b7zer", "uns", "ent\u00b7z\u00fc\u00b7cken", "wollt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Thonkunst! schwesterliche Sch\u00f6ne", "tokens": ["O", "Thon\u00b7kunst", "!", "schwes\u00b7ter\u00b7li\u00b7che", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Muse! welch ein Gott gab in die Erden-S\u00f6hne", "tokens": ["Der", "Mu\u00b7se", "!", "welch", "ein", "Gott", "gab", "in", "die", "Er\u00b7den\u00b7S\u00f6h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PWAT", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich zu erfinden, den Verstand?", "tokens": ["Dich", "zu", "er\u00b7fin\u00b7den", ",", "den", "Ver\u00b7stand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein dich hat nicht der Menschen Witz gebohren,", "tokens": ["Nein", "dich", "hat", "nicht", "der", "Men\u00b7schen", "Witz", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du bist (f\u00fcr Weise, nicht f\u00fcr Thoren:)", "tokens": ["Du", "bist", "(", "f\u00fcr", "Wei\u00b7se", ",", "nicht", "f\u00fcr", "Tho\u00b7ren", ":)"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "emoticon"], "pos": ["PPER", "VAFIN", "$(", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von dem Olymp herabgesandt!", "tokens": ["Von", "dem", "O\u00b7lymp", "her\u00b7ab\u00b7ge\u00b7sandt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und du, fein ausgeh\u00f6hlte Ceder,", "tokens": ["Und", "du", ",", "fein", "aus\u00b7ge\u00b7h\u00f6hl\u00b7te", "Ce\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Fl\u00fcgel! zaubere, wann nah an dir ein Spr\u00f6der", "tokens": ["Du", "Fl\u00fc\u00b7gel", "!", "zau\u00b7be\u00b7re", ",", "wann", "nah", "an", "dir", "ein", "Spr\u00f6\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ein zu stolzes M\u00e4dchen, stehn.", "tokens": ["Und", "ein", "zu", "stol\u00b7zes", "M\u00e4d\u00b7chen", ",", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "PTKZU", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann sollst du die verschm\u00e4hte Liebe r\u00e4chen.", "tokens": ["Dann", "sollst", "du", "die", "ver\u00b7schm\u00e4h\u00b7te", "Lie\u00b7be", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der J\u00fcngling soll durch Seufzer sprechen,", "tokens": ["Der", "J\u00fcng\u00b7ling", "soll", "durch", "Seuf\u00b7zer", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das M\u00e4dchen fort zu weinen gehn!", "tokens": ["Das", "M\u00e4d\u00b7chen", "fort", "zu", "wei\u00b7nen", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo war ich, als mit tausend Zungen", "tokens": ["Wo", "war", "ich", ",", "als", "mit", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KOUS", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die G\u00f6ttin Harmonie im Fl\u00fcgel mir gesungen!", "tokens": ["Die", "G\u00f6t\u00b7tin", "Har\u00b7mo\u00b7nie", "im", "Fl\u00fc\u00b7gel", "mir", "ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "wor