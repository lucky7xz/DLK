{"dta.poem.20126": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Einigkeit .  \n 1842.  \n (Bei Gelegenheit des Hamburger Brandes.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Jubel mehr! die Freude sei bemeistert", "tokens": ["Kein", "Ju\u00b7bel", "mehr", "!", "die", "Freu\u00b7de", "sei", "be\u00b7meis\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "$.", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob deutschen Sinn\u2019s und deutscher Einigkeit;", "tokens": ["Ob", "deut\u00b7schen", "Sinn's", "und", "deut\u00b7scher", "Ei\u00b7nig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es gilt nicht viel, wenn sich ein Volk begeistert", "tokens": ["Es", "gilt", "nicht", "viel", ",", "wenn", "sich", "ein", "Volk", "be\u00b7geis\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "PRF", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In unsrer krankhaft-\u00fcberreizten Zeit.", "tokens": ["In", "uns\u00b7rer", "krank\u00b7haft\u00b7\u00fcber\u00b7reiz\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was Ihr gesehn \u2014 des Mitleids frommes Walten", "tokens": ["Was", "Ihr", "ge\u00b7sehn", "des", "Mit\u00b7leids", "from\u00b7mes", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erl\u00f6st noch lang vom alten Fluch uns nicht,", "tokens": ["Er\u00b7l\u00f6st", "noch", "lang", "vom", "al\u00b7ten", "Fluch", "uns", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und unsre Heimath ist und bleibt zerspalten,", "tokens": ["Und", "uns\u00b7re", "Hei\u00b7math", "ist", "und", "bleibt", "zer\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bevor uns nicht ein ", "tokens": ["Be\u00b7vor", "uns", "nicht", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Begeistrung! ja, bei Gott, auf allen Gassen", "tokens": ["Be\u00b7geis\u00b7trung", "!", "ja", ",", "bei", "Gott", ",", "auf", "al\u00b7len", "Gas\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PTKANT", "$,", "APPR", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und aller Orten macht sie jetzt sich breit,", "tokens": ["Und", "al\u00b7ler", "Or\u00b7ten", "macht", "sie", "jetzt", "sich", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADV", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wessen Herz sich will begeistern lassen,", "tokens": ["Und", "wes\u00b7sen", "Herz", "sich", "will", "be\u00b7geis\u00b7tern", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PRF", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der eile sich \u2014 jetzt ist die rechte Zeit,", "tokens": ["Der", "ei\u00b7le", "sich", "jetzt", "ist", "die", "rech\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es ist die Zeit, wo sich die deutsche Jugend,", "tokens": ["Es", "ist", "die", "Zeit", ",", "wo", "sich", "die", "deut\u00b7sche", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Unw\u00fcrdig, vor den K\u00fcnstlerwagen spannt; ", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", ",", "vor", "den", "K\u00fcnst\u00b7ler\u00b7wa\u00b7gen", "spannt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie stempelt auch die Mode \u2019mal zur Tugend,", "tokens": ["Sie", "stem\u00b7pelt", "auch", "die", "Mo\u00b7de", "'mal", "zur", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und schw\u00e4rmt f\u00fcr Einigkeit im Vaterland.", "tokens": ["Und", "schw\u00e4rmt", "f\u00fcr", "Ei\u00b7nig\u00b7keit", "im", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, Einigkeit! die Liebe kann sich regen", "tokens": ["Ach", ",", "Ei\u00b7nig\u00b7keit", "!", "die", "Lie\u00b7be", "kann", "sich", "re\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$.", "ART", "NN", "VMFIN", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In einem Herzen, das der Ha\u00df verzehrt,", "tokens": ["In", "ei\u00b7nem", "Her\u00b7zen", ",", "das", "der", "Ha\u00df", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schl\u00e4gt dem Feinde zornentbrannt entgegen,", "tokens": ["Es", "schl\u00e4gt", "dem", "Fein\u00b7de", "zor\u00b7nent\u00b7brannt", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00e4lt ihn dennoch seines Mitleids werth.", "tokens": ["Und", "h\u00e4lt", "ihn", "den\u00b7noch", "sei\u00b7nes", "Mit\u00b7leids", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer hat von Euch die namenlosen Schmerzen", "tokens": ["Wer", "hat", "von", "Euch", "die", "na\u00b7men\u00b7lo\u00b7sen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zerschossner Feinde frohen Muth\u2019s erblickt?", "tokens": ["Zer\u00b7schoss\u00b7ner", "Fein\u00b7de", "fro\u00b7hen", "Muth's", "er\u00b7blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So hassenswerth lebt nie der Ha\u00df im Herzen,", "tokens": ["So", "has\u00b7sens\u00b7werth", "lebt", "nie", "der", "Ha\u00df", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df er des Mitleids Stimme selbst erstickt.", "tokens": ["Da\u00df", "er", "des", "Mit\u00b7leids", "Stim\u00b7me", "selbst", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nein, soll die Zukunft uns ", "tokens": ["Nein", ",", "soll", "die", "Zu\u00b7kunft", "uns"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VMFIN", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da gilt es mehr als eine milde Hand,", "tokens": ["Da", "gilt", "es", "mehr", "als", "ei\u00b7ne", "mil\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da gilt\u2019s ein muthig Ringen und Bezwingen,", "tokens": ["Da", "gilt's", "ein", "mut\u00b7hig", "Rin\u00b7gen", "und", "Be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Fr\u00fchlingswehn durch\u2019s ganze deutsche Land.", "tokens": ["Ein", "Fr\u00fch\u00b7lings\u00b7wehn", "durch's", "gan\u00b7ze", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn \u00fcberall der ", "tokens": ["Wenn", "\u00fc\u00b7be\u00b7rall", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und kein bedr\u00fccktes Volk um Rettung schreit,", "tokens": ["Und", "kein", "be\u00b7dr\u00fcck\u00b7tes", "Volk", "um", "Ret\u00b7tung", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann will auch ich die Zweifel froh vertauschen,", "tokens": ["Dann", "will", "auch", "ich", "die", "Zwei\u00b7fel", "froh", "ver\u00b7tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und gl\u00e4ubig baun auf Deutschlands Einigkeit.", "tokens": ["Und", "gl\u00e4u\u00b7big", "baun", "auf", "Deutschlands", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}