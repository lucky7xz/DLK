ch", "s\u00fc\u00b7\u00dfe", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem treuen Arm.", "tokens": ["In", "ih\u00b7rem", "treu\u00b7en", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und lacht mir, einem Engel gleich,", "tokens": ["Und", "lacht", "mir", ",", "ei\u00b7nem", "En\u00b7gel", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kind an ihrer Brust,", "tokens": ["Mein", "Kind", "an", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann n\u00e4hm' ich nicht ein K\u00f6nigreich", "tokens": ["Dann", "n\u00e4hm'", "ich", "nicht", "ein", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr diese Herzenslust!", "tokens": ["F\u00fcr", "die\u00b7se", "Her\u00b7zens\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}