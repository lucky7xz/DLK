{"textgrid.poem.32756": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun nenn' ich schon ein ganzes Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun nenn' ich schon ein ganzes Jahr", "tokens": ["Nun", "nenn'", "ich", "schon", "ein", "gan\u00b7zes", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein liebes Weibchen mein;", "tokens": ["Mein", "lie\u00b7bes", "Weib\u00b7chen", "mein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, denk' ich nach, so scheint's f\u00fcrwahr", "tokens": ["Und", ",", "denk'", "ich", "nach", ",", "so", "scheint's", "f\u00fcr\u00b7wahr"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum Wochen her zu sein.", "tokens": ["Kaum", "Wo\u00b7chen", "her", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APZR", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So hurtig streicht kein Bach dahin,", "tokens": ["So", "hur\u00b7tig", "streicht", "kein", "Bach", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als diese Zeit verstrich;", "tokens": ["Als", "die\u00b7se", "Zeit", "ver\u00b7strich", ";"